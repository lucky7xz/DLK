{"textgrid.poem.50829": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan, ihr neunmal Weisen!", "tokens": ["Wo\u00b7hlan", ",", "ihr", "neun\u00b7mal", "Wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich fordre euch heraus!", "tokens": ["Ich", "ford\u00b7re", "euch", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Baut ihr aus Stahl und Eisen", "tokens": ["Baut", "ihr", "aus", "Stahl", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Marmor euer Haus:", "tokens": ["Und", "Mar\u00b7mor", "eu\u00b7er", "Haus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bau ich aus Rosend\u00fcften", "tokens": ["Bau", "ich", "aus", "Ro\u00b7sen\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und Mondschein mir ein Schlo\u00df;", "tokens": ["Und", "Mond\u00b7schein", "mir", "ein", "Schlo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drin biete ich euch allen Hohn", "tokens": ["Drin", "bie\u00b7te", "ich", "euch", "al\u00b7len", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und eurem Sch\u00fclertro\u00df!", "tokens": ["Und", "eu\u00b7rem", "Sch\u00fc\u00b7ler\u00b7tro\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die goldnen Sonnenstrahlen", "tokens": ["Die", "gold\u00b7nen", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind meine Lanzen scharf;", "tokens": ["Sind", "mei\u00b7ne", "Lan\u00b7zen", "scharf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen in den Talen", "tokens": ["Die", "Blu\u00b7men", "in", "den", "Ta\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind all mein Schie\u00dfbedarf!", "tokens": ["Sind", "all", "mein", "Schie\u00df\u00b7be\u00b7darf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Tannen auf den Bergen", "tokens": ["Die", "Tan\u00b7nen", "auf", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind meine W\u00e4chtersleut,", "tokens": ["Sind", "mei\u00b7ne", "W\u00e4ch\u00b7ters\u00b7leut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Himmels Sterne allzumal", "tokens": ["Des", "Him\u00b7mels", "Ster\u00b7ne", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein gl\u00e4nzend Heer zum Streit.", "tokens": ["Mein", "gl\u00e4n\u00b7zend", "Heer", "zum", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Oberstfeldzeugmeister", "tokens": ["Mein", "O\u00b7berst\u00b7feld\u00b7zeug\u00b7meis\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist meine Phantasie,", "tokens": ["Ist", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ihre guten Geister", "tokens": ["Denn", "ih\u00b7re", "gu\u00b7ten", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlie\u00dfen mich noch nie;", "tokens": ["Ver\u00b7lie\u00b7\u00dfen", "mich", "noch", "nie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und meine Kriegeskasse", "tokens": ["Und", "mei\u00b7ne", "Krie\u00b7ges\u00b7kas\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Quellen Silberschaum,", "tokens": ["Der", "Quel\u00b7len", "Sil\u00b7ber\u00b7schaum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein lustig s\u00e4uselnd Lagerzelt", "tokens": ["Mein", "lus\u00b7tig", "s\u00e4u\u00b7selnd", "La\u00b7ger\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Waldes gr\u00fcner Baum!", "tokens": ["Des", "Wal\u00b7des", "gr\u00fc\u00b7ner", "Baum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und meine Siegsstandarte,", "tokens": ["Und", "mei\u00b7ne", "Siegs\u00b7stand\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ist das Morgenrot!", "tokens": ["Die", "ist", "das", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Feldherrnkarte,", "tokens": ["Und", "mei\u00b7ne", "Feld\u00b7herrn\u00b7kar\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ist das Abendrot!", "tokens": ["Die", "ist", "das", "A\u00b7ben\u00b7drot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Tambour ist der Donner,", "tokens": ["Mein", "Tam\u00b7bour", "ist", "der", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der in den Bergen rollt,", "tokens": ["Der", "in", "den", "Ber\u00b7gen", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Trompeter ist der wilde Sturm,", "tokens": ["Trom\u00b7pe\u00b7ter", "ist", "der", "wil\u00b7de", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der auf den Meeren grollt.", "tokens": ["Der", "auf", "den", "Mee\u00b7ren", "grollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Wolken sind Trabanten,", "tokens": ["Die", "Wol\u00b7ken", "sind", "Tra\u00b7ban\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meine Stimme ruft,", "tokens": ["Die", "mei\u00b7ne", "Stim\u00b7me", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Adjutanten", "tokens": ["Und", "mei\u00b7ne", "Ad\u00b7ju\u00b7tan\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Adler in der Luft.", "tokens": ["Die", "Ad\u00b7ler", "in", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die fliegen und die sp\u00e4hen", "tokens": ["Die", "flie\u00b7gen", "und", "die", "sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinaus in alle Welt.", "tokens": ["Hin\u00b7aus", "in", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Dichterherz ist Feldmarschall,", "tokens": ["Mein", "Dich\u00b7ter\u00b7herz", "ist", "Feld\u00b7mar\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist ein guter Held.", "tokens": ["Das", "ist", "ein", "gu\u00b7ter", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich schicke dir entgegen,", "tokens": ["Ich", "schi\u00b7cke", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Feind! die Nachtigall:", "tokens": ["O", "Feind", "!", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bringt mit ihren Schl\u00e4gen", "tokens": ["Die", "bringt", "mit", "ih\u00b7ren", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich alsogleich zu Fall.", "tokens": ["Dich", "al\u00b7so\u00b7gleich", "zu", "Fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich werde lassen spielen", "tokens": ["Ich", "wer\u00b7de", "las\u00b7sen", "spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein duftendes Gesch\u00fctz,", "tokens": ["Mein", "duf\u00b7ten\u00b7des", "Ge\u00b7sch\u00fctz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und euer Eis vergehen soll", "tokens": ["Und", "eu\u00b7er", "Eis", "ver\u00b7ge\u00b7hen", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "An meiner Waffen Blitz.", "tokens": ["An", "mei\u00b7ner", "Waf\u00b7fen", "Blitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gott hat zu seinem Zeugen", "tokens": ["Gott", "hat", "zu", "sei\u00b7nem", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschaffen den Gesang,", "tokens": ["Er\u00b7schaf\u00b7fen", "den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wird nun nimmer schweigen", "tokens": ["Der", "wird", "nun", "nim\u00b7mer", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Ewigkeit entlang.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In seinen Zauberwellen", "tokens": ["In", "sei\u00b7nen", "Zau\u00b7ber\u00b7wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Versinkt der letzte Spott!", "tokens": ["Ver\u00b7sinkt", "der", "letz\u00b7te", "Spott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Solange noch ein Dichter singt,", "tokens": ["So\u00b7lan\u00b7ge", "noch", "ein", "Dich\u00b7ter", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So lange lebt auch Gott!", "tokens": ["So", "lan\u00b7ge", "lebt", "auch", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wohlan, ihr neunmal Weisen!", "tokens": ["Wo\u00b7hlan", ",", "ihr", "neun\u00b7mal", "Wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich fordre euch heraus!", "tokens": ["Ich", "ford\u00b7re", "euch", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Baut ihr aus Stahl und Eisen", "tokens": ["Baut", "ihr", "aus", "Stahl", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Marmor e