{"textgrid.poem.45998": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am 17. Mai 1817", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pilger! all der Blumenschein", "tokens": ["Pil\u00b7ger", "!", "all", "der", "Blu\u00b7men\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PIAT", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All die Fr\u00fcchte hier sind mein,", "tokens": ["All", "die", "Fr\u00fcch\u00b7te", "hier", "sind", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch kein Bl\u00e4ttchen will ich missen", "tokens": ["Auch", "kein", "Bl\u00e4tt\u00b7chen", "will", "ich", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer mir nur ein Keimchen knickt", "tokens": ["Wer", "mir", "nur", "ein", "Keim\u00b7chen", "knickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ich liebvoll angeblickt", "tokens": ["Das", "ich", "lieb\u00b7voll", "an\u00b7ge\u00b7blickt"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Trage Dornen im Gewissen.", "tokens": ["Tra\u00b7ge", "Dor\u00b7nen", "im", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr! ach ist dies alles dein,", "tokens": ["Herr", "!", "ach", "ist", "dies", "al\u00b7les", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "VAFIN", "PDS", "PIS", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "O so la\u00df mich dein auch sein!", "tokens": ["O", "so", "la\u00df", "mich", "dein", "auch", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPER", "PPOSAT", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pilger! was hier bl\u00fchend steht,", "tokens": ["Pil\u00b7ger", "!", "was", "hier", "bl\u00fc\u00b7hend", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Saat, die ich ges\u00e4et,", "tokens": ["Ist", "die", "Saat", ",", "die", "ich", "ge\u00b7s\u00e4et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn ich ein Unkraut f\u00e4nde,", "tokens": ["Und", "wenn", "ich", "ein", "Un\u00b7kraut", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen meiner Blumenzier,", "tokens": ["Zwi\u00b7schen", "mei\u00b7ner", "Blu\u00b7men\u00b7zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4hm' ich Rechenschaft von dir,", "tokens": ["N\u00e4hm'", "ich", "Re\u00b7chen\u00b7schaft", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Darum falte deine H\u00e4nde!", "tokens": ["Da\u00b7rum", "fal\u00b7te", "dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr ach s\u00e4e mein Gebet", "tokens": ["Herr", "ach", "s\u00e4e", "mein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Hier ins d\u00fcrrste Gartenbeet.", "tokens": ["Hier", "ins", "d\u00fcrrs\u00b7te", "Gar\u00b7ten\u00b7beet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pilger! du bist hergef\u00fchrt,", "tokens": ["Pil\u00b7ger", "!", "du", "bist", "her\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df dein b\u00f6ser Sinn es sp\u00fcrt,", "tokens": ["Da\u00df", "dein", "b\u00f6\u00b7ser", "Sinn", "es", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich k\u00f6nnt' in Blumen wallen,", "tokens": ["Wie", "ich", "k\u00f6nnt'", "in", "Blu\u00b7men", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nun Dornen tr\u00e4gt mein Haupt,", "tokens": ["Und", "nun", "Dor\u00b7nen", "tr\u00e4gt", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil du mir den Kranz geraubt,", "tokens": ["Weil", "du", "mir", "den", "Kranz", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Der an dir mir wohlgefallen.", "tokens": ["Der", "an", "dir", "mir", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr dein Dornenkranz mich r\u00fchrt,", "tokens": ["Herr", "dein", "Dor\u00b7nen\u00b7kranz", "mich", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gieb ihn mir, dem er geb\u00fchrt.", "tokens": ["Gieb", "ihn", "mir", ",", "dem", "er", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Pilger, auf der Maienau", "tokens": ["Pil\u00b7ger", ",", "auf", "der", "Mai\u00b7e\u00b7nau"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Blumen, in dem Tau", "tokens": ["In", "den", "Blu\u00b7men", ",", "in", "dem", "Tau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh die Spur, die ich gegangen", "tokens": ["Sieh", "die", "Spur", ",", "die", "ich", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du sollst des Weges nur", "tokens": ["Und", "du", "sollst", "des", "We\u00b7ges", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht des trunknen Schmucks der Flur", "tokens": ["Nicht", "des", "trunk\u00b7nen", "Schmucks", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollst allein nach mir verlangen.", "tokens": ["Sollst", "al\u00b7lein", "nach", "mir", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr! auf meine Tritte schau,", "tokens": ["Herr", "!", "auf", "mei\u00b7ne", "Trit\u00b7te", "schau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir am Ziel die H\u00fctte bau!", "tokens": ["Mir", "am", "Ziel", "die", "H\u00fct\u00b7te", "bau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Pilger wenn die Sehnsucht singt", "tokens": ["Pil\u00b7ger", "wenn", "die", "Sehn\u00b7sucht", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Kind die \u00c4rmchen schwingt", "tokens": ["Und", "ein", "Kind", "die", "\u00c4rm\u00b7chen", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fliegen m\u00f6cht', ein Nestchen bauen", "tokens": ["Flie\u00b7gen", "m\u00f6cht'", ",", "ein", "Nest\u00b7chen", "bau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcten in des Kalmus Rohr", "tokens": ["Br\u00fc\u00b7ten", "in", "des", "Kal\u00b7mus", "Rohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau du so zum Kreuz empor", "tokens": ["Schau", "du", "so", "zum", "Kreuz", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich dir ins Herz kann schauen.", "tokens": ["Da\u00df", "ich", "dir", "ins", "Herz", "kann", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr und wenn es mir gelingt", "tokens": ["Herr", "und", "wenn", "es", "mir", "ge\u00b7lingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schau es an, da\u00df es zerspringt.", "tokens": ["Schau", "es", "an", ",", "da\u00df", "es", "zer\u00b7springt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Pilger! wenn ein Blumenstern", "tokens": ["Pil\u00b7ger", "!", "wenn", "ein", "Blu\u00b7mens\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dir ins Aug' blickt, er ist fern,", "tokens": ["Dir", "ins", "Aug'", "blickt", ",", "er", "ist", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Himmelfern, in meinem Garten", "tokens": ["Him\u00b7mel\u00b7fern", ",", "in", "mei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat die Liebe dann geweht,", "tokens": ["Hat", "die", "Lie\u00b7be", "dann", "ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Stern nach dir gedreht,", "tokens": ["Und", "den", "Stern", "nach", "dir", "ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du sollst auf die Ernde warten,", "tokens": ["Du", "sollst", "auf", "die", "Ern\u00b7de", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Herr, trag' ich nur einen Kern,", "tokens": ["Herr", ",", "trag'", "ich", "nur", "ei\u00b7nen", "Kern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Find' ich Gnade vor dem Herrn.", "tokens": ["Find'", "ich", "Gna\u00b7de", "vor", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Pilger! wenn vom Bl\u00fctenzelt", "tokens": ["Pil\u00b7ger", "!", "wenn", "vom", "Bl\u00fc\u00b7ten\u00b7zelt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir aufs Herz ein Bl\u00e4ttchen f\u00e4llt", "tokens": ["Dir", "aufs", "Herz", "ein", "Bl\u00e4tt\u00b7chen", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst du nicht, was mein begehren,", "tokens": ["Sollst", "du", "nicht", ",", "was", "mein", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dein Herz ist mein Altar,", "tokens": ["Denn", "dein", "Herz", "ist", "mein", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo, so rein die Bl\u00fcte war,", "tokens": ["Wo", ",", "so", "rein", "die", "Bl\u00fc\u00b7te", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie mein Feuer will verzehren.", "tokens": ["Sie", "mein", "Feu\u00b7er", "will", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr, verzehr' die ganze Welt", "tokens": ["Herr", ",", "ver\u00b7zehr'", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da, so dir mein Herz gef\u00e4llt.", "tokens": ["Da", ",", "so", "dir", "mein", "Herz", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Pilger, wenn ein bl\u00fchend Reis,", "tokens": ["Pil\u00b7ger", ",", "wenn", "ein", "bl\u00fc\u00b7hend", "Reis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindisch froh um sich nicht wei\u00df,", "tokens": ["Kin\u00b7disch", "froh", "um", "sich", "nic