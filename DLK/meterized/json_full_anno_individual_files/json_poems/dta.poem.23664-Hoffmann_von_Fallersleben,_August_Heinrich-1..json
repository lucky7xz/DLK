{"dta.poem.23664": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst die Philister immer schrei'n:", "tokens": ["Lasst", "die", "Phi\u00b7lis\u00b7ter", "im\u00b7mer", "schrei'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar keine Zeit wird bald mehr sein!", "tokens": ["Gar", "kei\u00b7ne", "Zeit", "wird", "bald", "mehr", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir nur soviel Zeit noch haben", "tokens": ["Wenn", "wir", "nur", "so\u00b7viel", "Zeit", "noch", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Jugendlust voll Fr\u00f6hlichkeit", "tokens": ["In", "Ju\u00b7gend\u00b7lust", "voll", "Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns zu erfreun an Gottes Gaben,", "tokens": ["Uns", "zu", "er\u00b7freun", "an", "Got\u00b7tes", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was k\u00fcmmert uns dann noch die Zeit!", "tokens": ["Was", "k\u00fcm\u00b7mert", "uns", "dann", "noch", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob leer ist oder voll die Tasche,", "tokens": ["Ob", "leer", "ist", "o\u00b7der", "voll", "die", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nur immer voll die Flasche,", "tokens": ["Ist", "nur", "im\u00b7mer", "voll", "die", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und Herz, Geist und \u2014", "tokens": ["Und", "Herz", ",", "Geist", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "$("], "meter": "-++-", "measure": "unknown.measure.di"}, "line.10": {"text": "Der Magen gesund,", "tokens": ["Der", "Ma\u00b7gen", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Dann kann man sich in unsern Tagen", "tokens": ["Dann", "kann", "man", "sich", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Auch mit der papiernen Zeit vertragen;", "tokens": ["Auch", "mit", "der", "pa\u00b7pier\u00b7nen", "Zeit", "ver\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und wir lassen ein", "tokens": ["Und", "wir", "las\u00b7sen", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Jeden Cassenschein,", "tokens": ["Je\u00b7den", "Cas\u00b7sen\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Und mit Geduld ein", "tokens": ["Und", "mit", "Ge\u00b7duld", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Jeden Staatsschuldschein,", "tokens": ["Je\u00b7den", "Staats\u00b7schuld\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Und ohne weitere Deliberation", "tokens": ["Und", "oh\u00b7ne", "wei\u00b7te\u00b7re", "De\u00b7li\u00b7be\u00b7ra\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Jede heitere Obligation,", "tokens": ["Je\u00b7de", "hei\u00b7te\u00b7re", "Ob\u00b7li\u00b7ga\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Und wir halten nicht die Hand schief,", "tokens": ["Und", "wir", "hal\u00b7ten", "nicht", "die", "Hand", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wenn uns kommet ein Pfandbrief,", "tokens": ["Wenn", "uns", "kom\u00b7met", "ein", "Pfand\u00b7brief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "Und w\u00fcnschen, da\u00df immer heckten", "tokens": ["Und", "w\u00fcn\u00b7schen", ",", "da\u00df", "im\u00b7mer", "heck\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "In unseren Kisten und Kasten die Staatseffecten,", "tokens": ["In", "un\u00b7se\u00b7ren", "Kis\u00b7ten", "und", "Kas\u00b7ten", "die", "Staats\u00b7ef\u00b7fec\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.23": {"text": "Und freuen uns \u00fcber jedes Lumpenpapier,", "tokens": ["Und", "freu\u00b7en", "uns", "\u00fc\u00b7ber", "je\u00b7des", "Lum\u00b7pen\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Wovon man leben kann bei dem Humpen dahier.", "tokens": ["Wo\u00b7von", "man", "le\u00b7ben", "kann", "bei", "dem", "Hum\u00b7pen", "da\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}