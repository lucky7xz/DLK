{"textgrid.poem.36784": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Grotte der Egeria", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Numa Pompil, noch w\u00f6lbt sich die heilige Grotte der Nymphe,", "tokens": ["Nu\u00b7ma", "Pom\u00b7pil", ",", "noch", "w\u00f6lbt", "sich", "die", "hei\u00b7li\u00b7ge", "Grot\u00b7te", "der", "Nym\u00b7phe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und der lebendige Quell sprudelt noch immer in ihr,", "tokens": ["Und", "der", "le\u00b7ben\u00b7di\u00b7ge", "Quell", "spru\u00b7delt", "noch", "im\u00b7mer", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "---+--+---+-++", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo mit Unsterblichem einst der Sterbliche traulich verkehrte,", "tokens": ["Wo", "mit", "U\u00b7nsterb\u00b7li\u00b7chem", "einst", "der", "Sterb\u00b7li\u00b7che", "trau\u00b7lich", "ver\u00b7kehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und die Weisheit die Frucht solcher Umarmungen war.", "tokens": ["Und", "die", "Weis\u00b7heit", "die", "Frucht", "sol\u00b7cher", "Um\u00b7ar\u00b7mun\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Jetzt besucht sie der Britte daf\u00fcr, doch die Nymph' ist verschwunden,", "tokens": ["Jetzt", "be\u00b7sucht", "sie", "der", "Brit\u00b7te", "da\u00b7f\u00fcr", ",", "doch", "die", "Nym\u00b7ph'", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "$,", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und die Weisheit wird nun besser von Nibby docirt.", "tokens": ["Und", "die", "Weis\u00b7heit", "wird", "nun", "bes\u00b7ser", "von", "Nib\u00b7by", "do\u00b7cirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NE", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Numa Pompil, noch w\u00f6lbt sich die heilige Grotte der Nymphe,", "tokens": ["Nu\u00b7ma", "Pom\u00b7pil", ",", "noch", "w\u00f6lbt", "sich", "die", "hei\u00b7li\u00b7ge", "Grot\u00b7te", "der", "Nym\u00b7phe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und der lebendige Quell sprudelt noch immer in ihr,", "tokens": ["Und", "der", "le\u00b7ben\u00b7di\u00b7ge", "Quell", "spru\u00b7delt", "noch", "im\u00b7mer", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,"