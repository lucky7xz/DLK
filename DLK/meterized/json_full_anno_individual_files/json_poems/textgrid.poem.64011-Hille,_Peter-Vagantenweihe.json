{"textgrid.poem.64011": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Vagantenweihe", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zugv\u00f6gel ziehn in grauem Ernst,", "tokens": ["Zug\u00b7v\u00f6\u00b7gel", "ziehn", "in", "grau\u00b7em", "Ernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stehst du Walter nun und lernst,", "tokens": ["Da", "stehst", "du", "Wal\u00b7ter", "nun", "und", "lernst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jahre welken 's greise Haupt.", "tokens": ["Die", "Jah\u00b7re", "wel\u00b7ken", "'s", "grei\u00b7se", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fast steht der Hain schon blattberaubt \u2013", "tokens": ["Fast", "steht", "der", "Hain", "schon", "blatt\u00b7be\u00b7raubt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie kalt des Regens d\u00fcnnes Na\u00df!", "tokens": ["Wie", "kalt", "des", "Re\u00b7gens", "d\u00fcn\u00b7nes", "Na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und doch Kopf oben! unverzagt,", "tokens": ["Und", "doch", "Kopf", "o\u00b7ben", "!", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "NN", "ADV", "$.", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend Rosen unbenagt,", "tokens": ["Der", "Ju\u00b7gend", "Ro\u00b7sen", "un\u00b7be\u00b7nagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trotz ", "tokens": ["Trotz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sie regen sich voll dunklem Duft", "tokens": ["Sie", "re\u00b7gen", "sich", "voll", "dunk\u00b7lem", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In ewig blauer Feierluft:", "tokens": ["In", "e\u00b7wig", "blau\u00b7er", "Fei\u00b7er\u00b7luft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der tiefe rote Ku\u00df macht das.", "tokens": ["Der", "tie\u00b7fe", "ro\u00b7te", "Ku\u00df", "macht", "das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich hab' viel Marterbilder hier,", "tokens": ["Ich", "hab'", "viel", "Mar\u00b7ter\u00b7bil\u00b7der", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind gar geringe Kirchenzier!", "tokens": ["Sind", "gar", "ge\u00b7rin\u00b7ge", "Kir\u00b7chen\u00b7zier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und voll von Pein und ", "tokens": ["Und", "voll", "von", "Pein", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So mager, leer und dintenvoll,", "tokens": ["So", "ma\u00b7ger", ",", "leer", "und", "din\u00b7ten\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Saal, darin Latein erscholl,", "tokens": ["Der", "Saal", ",", "da\u00b7rin", "La\u00b7tein", "er\u00b7scholl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein M\u00e4nnlein da, das Leder ganz.", "tokens": ["Ein", "M\u00e4nn\u00b7lein", "da", ",", "das", "Le\u00b7der", "ganz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Sonne leuchtet treu und warm,", "tokens": ["Die", "Son\u00b7ne", "leuch\u00b7tet", "treu", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da leuchtet Lieb' mir schon im Arm,", "tokens": ["Da", "leuch\u00b7tet", "Lieb'", "mir", "schon", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wieder weichen Lippen los", "tokens": ["Die", "wie\u00b7der", "wei\u00b7chen", "Lip\u00b7pen", "los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Elfenbein, die Hand im Scho\u00df;", "tokens": ["Wie", "El\u00b7fen\u00b7bein", ",", "die", "Hand", "im", "Scho\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von blauem Glanz die Augen na\u00df.", "tokens": ["Von", "blau\u00b7em", "Glanz", "die", "Au\u00b7gen", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dann ein Blick aus warmem Lid,", "tokens": ["Und", "dann", "ein", "Blick", "aus", "war\u00b7mem", "Lid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wieder tief ins Traumland flieht,", "tokens": ["Der", "wie\u00b7der", "tief", "ins", "Traum\u00b7land", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Des Odems Duft durchgraust mein Mark,", "tokens": ["Des", "O\u00b7dems", "Duft", "durch\u00b7graust", "mein", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das weiht den Mann, das macht ihn stark,", "tokens": ["Das", "weiht", "den", "Mann", ",", "das", "macht", "ihn", "stark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja bis zum Gotte hebt ihn das.", "tokens": ["Ja", "bis", "zum", "Got\u00b7te", "hebt", "ihn", "das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KON", "APPRART", "NN", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und meidet mich die Klerisei,", "tokens": ["Und", "mei\u00b7det", "mich", "die", "Kle\u00b7ri\u00b7sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil meinen Wirbel floh die Weih' \u2013", "tokens": ["Weil", "mei\u00b7nen", "Wir\u00b7bel", "floh", "die", "Weih'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das ist ja nur der pure Neid,", "tokens": ["Das", "ist", "ja", "nur", "der", "pu\u00b7re", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der h\u00fcllt sich dann in Kreuz und Leid", "tokens": ["Der", "h\u00fcllt", "sich", "dann", "in", "Kreuz", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und donnert los im L\u00fcgenba\u00df.", "tokens": ["Und", "don\u00b7nert", "los", "im", "L\u00fc\u00b7gen\u00b7ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Altarbild gar lieb und hold,", "tokens": ["Das", "Al\u00b7tar\u00b7bild", "gar", "lieb", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erhellt von zartem Lichtergold,", "tokens": ["Er\u00b7hellt", "von", "zar\u00b7tem", "Lich\u00b7ter\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, Himmel, ist nicht ", "tokens": ["Das", ",", "Him\u00b7mel", ",", "ist", "nicht"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "NN", "$,", "VAFIN", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Das ist ein Tag, der ewig steht,", "tokens": ["Das", "ist", "ein", "Tag", ",", "der", "e\u00b7wig", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir niemals aus dem Sinne geht,", "tokens": ["Mir", "nie\u00b7mals", "aus", "dem", "Sin\u00b7ne", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Tag im Wald im weichen Gras.", "tokens": ["Ein", "Tag", "im", "Wald", "im", "wei\u00b7chen", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das alles war so ernst, so tief,", "tokens": ["Das", "al\u00b7les", "war", "so", "ernst", ",", "so", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie so himmlisch lag und schlief,", "tokens": ["Wie", "sie", "so", "himm\u00b7lisch", "lag", "und", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trotz ", "tokens": ["Trotz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und Blumen frisch und Amselschlag,", "tokens": ["Und", "Blu\u00b7men", "frisch", "und", "Am\u00b7sel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der weihen Ruh' ich denken mag,", "tokens": ["Der", "wei\u00b7hen", "Ruh'", "ich", "den\u00b7ken", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des weichen Golds im gr\u00fcnen Gras.", "tokens": ["Des", "wei\u00b7chen", "Golds", "im", "gr\u00fc\u00b7nen", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ein Ruf, von wo, der sich verlor,", "tokens": ["Ein", "Ruf", ",", "von", "wo", ",", "der", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAV", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da f\u00e4hrt sie scheu vom Grund empor:", "tokens": ["Da", "f\u00e4hrt", "sie", "scheu", "vom", "Grund", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Schrecken, Kind, ist ", "tokens": ["Dein", "Schre\u00b7cken", ",", "Kind", ",", "ist"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Locken fahren wild herum,", "tokens": ["Die", "Lo\u00b7cken", "fah\u00b7ren", "wild", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Gott im Himmel, war das dumm, \u2013", "tokens": ["O", "Gott", "im", "Him\u00b7mel", ",", "war", "das", "dumm", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,", "VAFIN", "ART", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich nenne meine Weihe das.", "tokens": ["Ich", "nen\u00b7ne", "mei\u00b7ne", "Wei\u00b7he", "das", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Zugv\u00f6gel ziehn in grauem Ernst,", "tokens": ["Zug\u00b7v\u00f6\u00b7gel", "ziehn", "in", "grau\u00b7em", "Ernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stehst du Walter nun und lernst,", "tokens": ["Da", "stehst", "du", "Wal\u00b7ter", "nun", "und", "lernst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jahre welken 's greise Haupt.", "tokens": ["Die", "Jah\u00b7re", "wel\u00b7ken", "'s", "grei\u00b7se", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fast steht der Hain schon blattberaubt \u2013", "tokens": ["Fast", "steht", "der", "Hain", "schon", "blatt\u00b7be\u00b7raubt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie kalt des Regens d\u00fcnnes Na\u00df!", "tokens": ["Wie", "kalt", "des", "Re\u00b7gens", "d\u00fcn\u00b7nes", "Na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und doch Kopf oben! unverzagt,", "tokens": ["Und", "doch", "Kopf", "o\u00b7