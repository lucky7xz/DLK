{"dta.poem.3622": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "11. Erziehung durch Ahndung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denkst du nicht, Maria, mehr an die ausge-               ", "tokens": ["Denkst", "du", "nicht", ",", "Ma\u00b7ria", ",", "mehr", "an", "die", "aus\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "NE", "$,", "ADV", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "standnen Schmerzen,", "tokens": ["stand\u00b7nen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Als das kleine Jesulein in dir ein Gestalt gewann?", "tokens": ["Als", "das", "klei\u00b7ne", "Je\u00b7su\u00b7lein", "in", "dir", "ein", "Ge\u00b7stalt", "ge\u00b7wann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "O wie sollt ich ihn nicht drum tausendmal im Glauben", "tokens": ["O", "wie", "sollt", "ich", "ihn", "nicht", "drum", "tau\u00b7send\u00b7mal", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "PPER", "PTKNEG", "PAV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "herzen,", "tokens": ["her\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Da er nun zusehens w\u00e4chst, mir zum Br\u00e4utgam und", "tokens": ["Da", "er", "nun", "zu\u00b7se\u00b7hens", "w\u00e4chst", ",", "mir", "zum", "Br\u00e4ut\u00b7gam", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PPER", "APPRART", "NE", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "zum Mann.", "tokens": ["zum", "Mann", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Hat Johannes nicht vor Freud, schon im Mutter-", "tokens": ["Hat", "Jo\u00b7han\u00b7nes", "nicht", "vor", "Freud", ",", "schon", "im", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NE", "PTKNEG", "APPR", "NN", "$,", "ADV", "APPRART", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "leib gesprungen,", "tokens": ["leib", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Spielt er nicht zum voraus schon, eh er noch kam an", "tokens": ["Spielt", "er", "nicht", "zum", "vo\u00b7raus", "schon", ",", "eh", "er", "noch", "kam", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das Licht;", "tokens": ["das", "Licht", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Haben wir nicht seine Freund oft sein Hochzeitslied ge-", "tokens": ["Ha\u00b7ben", "wir", "nicht", "sei\u00b7ne", "Freund", "oft", "sein", "Hoch\u00b7zeits\u00b7lied", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "sungen,", "tokens": ["sun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Hat man mir mit Fingern da dieses Kind gezeiget nicht.", "tokens": ["Hat", "man", "mir", "mit", "Fin\u00b7gern", "da", "die\u00b7ses", "Kind", "ge\u00b7zei\u00b7get", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "APPR", "NN", "ADV", "PDAT", "NN", "VVPP", "PTKNEG", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun liegt mir dies Kind im Schoo\u00df! Nun hab", "tokens": ["Nun", "liegt", "mir", "dies", "Kind", "im", "Schoo\u00df", "!", "Nun", "hab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "NN", "APPRART", "NN", "$.", "ADV", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ich das Lamm vor Augen,", "tokens": ["ich", "das", "Lamm", "vor", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schaue, wie es mir zur Lust treibt so manches s\u00fc\u00dfe", "tokens": ["Schau\u00b7e", ",", "wie", "es", "mir", "zur", "Lust", "treibt", "so", "man\u00b7ches", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "ADV", "PIAT", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Spiel;", "tokens": ["Spiel", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ist dies nicht mein Freund, der pflegt meiner Mutter", "tokens": ["Ist", "dies", "nicht", "mein", "Freund", ",", "der", "pflegt", "mei\u00b7ner", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Brust zu saugen,", "tokens": ["Brust", "zu", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ist er nicht mein Salomon, den ich niemals k\u00fc\u00df zu", "tokens": ["Ist", "er", "nicht", "mein", "Sa\u00b7lo\u00b7mon", ",", "den", "ich", "nie\u00b7mals", "k\u00fc\u00df", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "PTKZU"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "viel.", "tokens": ["viel", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Ja er ists, und was ich will, kann ich in dem", "tokens": ["Ja", "er", "ists", ",", "und", "was", "ich", "will", ",", "kann", "ich", "in", "dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "$,", "KON", "PWS", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Kindlein finden,", "tokens": ["Kin\u00b7dlein", "fin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Kind und Br\u00e4utigam zugleich hei\u00dft und ist er in der", "tokens": ["Kind", "und", "Br\u00e4u\u00b7ti\u00b7gam", "zu\u00b7gleich", "hei\u00dft", "und", "ist", "er", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "ADV", "VVFIN", "KON", "VAFIN", "PPER", "APPR", "ART"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "That;", "tokens": ["That", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Denn die zarte Liebe kann auch wohl Kinder ehlich", "tokens": ["Denn", "die", "zar\u00b7te", "Lie\u00b7be", "kann", "auch", "wohl", "Kin\u00b7der", "eh\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "ADV", "ADV", "NN", "ADJD"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "binden,", "tokens": ["bin\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Da\u00df in Unschuld als sich selbst, eins das andre lie-", "tokens": ["Da\u00df", "in", "Un\u00b7schuld", "als", "sich", "selbst", ",", "eins", "das", "and\u00b7re", "lie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KOUS", "PRF", "ADV", "$,", "PIS", "ART", "ADJA", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "ber hat.", "tokens": ["ber", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}