{"dta.poem.3477": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Nicht Wiedersehn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ade mein allerherzliebster Schaz,               ", "tokens": ["Nun", "a\u00b7de", "mein", "al\u00b7ler\u00b7herz\u00b7liebs\u00b7ter", "Schaz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jezt mu\u00df ich wohl scheiden von dir,", "tokens": ["Jezt", "mu\u00df", "ich", "wohl", "schei\u00b7den", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bis auf den andern Sommer,", "tokens": ["Bis", "auf", "den", "an\u00b7dern", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann komm ich wieder zu dir.", "tokens": ["Dann", "komm", "ich", "wie\u00b7der", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und als der junge Knab heimkam,", "tokens": ["Und", "als", "der", "jun\u00b7ge", "Knab", "heim\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von seiner Liebsten fing er an,", "tokens": ["Von", "sei\u00b7ner", "Liebs\u00b7ten", "fing", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ist meine Herzallerliebste,", "tokens": ["Wo", "ist", "mei\u00b7ne", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die ich verlassen hab?", "tokens": ["Die", "ich", "ver\u00b7las\u00b7sen", "hab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf dem Kirchhof liegt sie begraben,", "tokens": ["Auf", "dem", "Kirch\u00b7hof", "liegt", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Heut ists der dritte Tag,", "tokens": ["Heut", "ists", "der", "drit\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Trauren und das Weinen", "tokens": ["Das", "Trau\u00b7ren", "und", "das", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat sie zum Tod gebracht.", "tokens": ["Hat", "sie", "zum", "Tod", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt will ich auf den Kirchhof gehen,", "tokens": ["Jetzt", "will", "ich", "auf", "den", "Kirch\u00b7hof", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will suchen meiner Liebsten Grab,", "tokens": ["Will", "su\u00b7chen", "mei\u00b7ner", "Liebs\u00b7ten", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ihr alleweil rufen,", "tokens": ["Will", "ihr", "al\u00b7le\u00b7weil", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bis da\u00df sie mir Antwort giebt.", "tokens": ["Bis", "da\u00df", "sie", "mir", "Ant\u00b7wort", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ey du mein allerherzliebster Schaz,", "tokens": ["Ey", "du", "mein", "al\u00b7ler\u00b7herz\u00b7liebs\u00b7ter", "Schaz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-++-++-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Mach auf dein tiefes Grab,", "tokens": ["Mach", "auf", "dein", "tie\u00b7fes", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du h\u00f6rst kein Gl\u00f6cklein l\u00e4uten,", "tokens": ["Du", "h\u00f6rst", "kein", "Gl\u00f6c\u00b7klein", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du h\u00f6rst kein V\u00f6glein pfeifen,", "tokens": ["Du", "h\u00f6rst", "kein", "V\u00f6\u00b7glein", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du siehst weder Sonn noch Mond!", "tokens": ["Du", "siehst", "we\u00b7der", "Sonn", "noch", "Mond", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}