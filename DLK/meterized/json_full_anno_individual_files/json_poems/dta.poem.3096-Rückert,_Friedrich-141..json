{"dta.poem.3096": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "141.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht triftig schienen mir von Gottes G\u00fct' und Macht", "tokens": ["Nicht", "trif\u00b7tig", "schie\u00b7nen", "mir", "von", "Got\u00b7tes", "G\u00fct'", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beweise nur aus Endursachen vorgebracht;", "tokens": ["Be\u00b7wei\u00b7se", "nur", "aus", "En\u00b7dur\u00b7sa\u00b7chen", "vor\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum ein Angesicht der Augen habe zwei,", "tokens": ["Wa\u00b7rum", "ein", "An\u00b7ge\u00b7sicht", "der", "Au\u00b7gen", "ha\u00b7be", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VAFIN", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da alles doch zu sehn gnug eins der Sonne sei.", "tokens": ["Da", "al\u00b7les", "doch", "zu", "sehn", "gnug", "eins", "der", "Son\u00b7ne", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKZU", "VVINF", "ADV", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nheit und Ebenma\u00df lie\u00df ich als Grund mir gn\u00fcgen,", "tokens": ["Sch\u00f6n\u00b7heit", "und", "E\u00b7ben\u00b7ma\u00df", "lie\u00df", "ich", "als", "Grund", "mir", "gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sie aber wollten noch dazu den andern f\u00fcgen,", "tokens": ["Sie", "a\u00b7ber", "woll\u00b7ten", "noch", "da\u00b7zu", "den", "an\u00b7dern", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "PAV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df dieser edelste und himmeln\u00e4chste Sinn", "tokens": ["Da\u00df", "die\u00b7ser", "e\u00b7dels\u00b7te", "und", "him\u00b7mel\u00b7n\u00e4chs\u00b7te", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei doppelt angelegt dazu von Anbeginn,", "tokens": ["Sei", "dop\u00b7pelt", "an\u00b7ge\u00b7legt", "da\u00b7zu", "von", "An\u00b7be\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVPP", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Damit ein Auge doch, wann eines ward gekr\u00e4nkt,", "tokens": ["Da\u00b7mit", "ein", "Au\u00b7ge", "doch", ",", "wann", "ei\u00b7nes", "ward", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "$,", "PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch blieb', in welches nun die ganze Kraft sich senkt.", "tokens": ["Noch", "blieb'", ",", "in", "wel\u00b7ches", "nun", "die", "gan\u00b7ze", "Kraft", "sich", "senkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PRELS", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Des Grundes Richtigkeit vermocht' ich nicht zu fassen,", "tokens": ["Des", "Grun\u00b7des", "Rich\u00b7tig\u00b7keit", "ver\u00b7mocht'", "ich", "nicht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun aber will ich ihn und mu\u00df ihn gelten lassen,", "tokens": ["Nun", "a\u00b7ber", "will", "ich", "ihn", "und", "mu\u00df", "ihn", "gel\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "KON", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Seitdem ein Auge, mir nicht minder lieb als meines,", "tokens": ["Seit\u00b7dem", "ein", "Au\u00b7ge", ",", "mir", "nicht", "min\u00b7der", "lieb", "als", "mei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "PPER", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An einem theuern Haupt zu Schaden kam, nur eines.", "tokens": ["An", "ei\u00b7nem", "theu\u00b7ern", "Haupt", "zu", "Scha\u00b7den", "kam", ",", "nur", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nun dank' ich Gott, da\u00df ihm noch eines blieb geschenkt,", "tokens": ["Nun", "dank'", "ich", "Gott", ",", "da\u00df", "ihm", "noch", "ei\u00b7nes", "blieb", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bete, da\u00df darein sei Doppelkraft gesenkt,", "tokens": ["Und", "be\u00b7te", ",", "da\u00df", "da\u00b7rein", "sei", "Dop\u00b7pel\u00b7kraft", "ge\u00b7senkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PAV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Gedoppelt Himmelslicht, gedoppelt Seelenlust,", "tokens": ["Ge\u00b7dop\u00b7pelt", "Him\u00b7mels\u00b7licht", ",", "ge\u00b7dop\u00b7pelt", "See\u00b7len\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df innen zum Gewinn werd' au\u00dfen der Verlust.", "tokens": ["Da\u00df", "in\u00b7nen", "zum", "Ge\u00b7winn", "werd'", "au\u00b7\u00dfen", "der", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Endursache mag im Dinge selbst nicht seyn,", "tokens": ["Die", "En\u00b7dur\u00b7sa\u00b7che", "mag", "im", "Din\u00b7ge", "selbst", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Recht tr\u00e4gt sie der Mensch zu seinem Trost hinein.", "tokens": ["Mit", "Recht", "tr\u00e4gt", "sie", "der", "Mensch", "zu", "sei\u00b7nem", "Trost", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}