{"textgrid.poem.67260": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum guten neuen Jahr, wo nehm' ich her", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum guten neuen Jahr, wo nehm' ich her", "tokens": ["Zum", "gu\u00b7ten", "neu\u00b7en", "Jahr", ",", "wo", "nehm'", "ich", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Blumenkranz, den ich, mit frommen Wunsch,", "tokens": ["Den", "Blu\u00b7men\u00b7kranz", ",", "den", "ich", ",", "mit", "from\u00b7men", "Wunsch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Weihgeschenk den Eltern wind'? Es starrt", "tokens": ["Zum", "Weih\u00b7ge\u00b7schenk", "den", "El\u00b7tern", "wind'", "?", "Es", "starrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Winter, jedes Spr\u00f6\u00dflings Haupt ist welk,", "tokens": ["Der", "Win\u00b7ter", ",", "je\u00b7des", "Spr\u00f6\u00df\u00b7lings", "Haupt", "ist", "welk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und eisern ist der Erde Mutterschoo\u00df \u2013", "tokens": ["Und", "ei\u00b7sern", "ist", "der", "Er\u00b7de", "Mut\u00b7ter\u00b7schoo\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch warm gl\u00fcht dieses Herz! ach, ihm entsprie\u00dft", "tokens": ["Doch", "warm", "gl\u00fcht", "die\u00b7ses", "Herz", "!", "ach", ",", "ihm", "ent\u00b7sprie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PDAT", "NN", "$.", "XY", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Bl\u00fcmlein manches, doch verborgen bl\u00fcht's", "tokens": ["Der", "Bl\u00fcm\u00b7lein", "man\u00b7ches", ",", "doch", "ver\u00b7bor\u00b7gen", "bl\u00fcht's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "$,", "ADV", "VVPP", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und duftet's hier \u2013 ich ging und sann und sann,", "tokens": ["Und", "duf\u00b7tet's", "hier", "\u2013", "ich", "ging", "und", "sann", "und", "sann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da \u00f6ffnete mein Blick sich und ich sah", "tokens": ["Da", "\u00f6ff\u00b7ne\u00b7te", "mein", "Blick", "sich", "und", "ich", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Musen G\u00e4rten, reich im sch\u00f6nsten Schmuck", "tokens": ["Der", "Mu\u00b7sen", "G\u00e4r\u00b7ten", ",", "reich", "im", "sch\u00f6ns\u00b7ten", "Schmuck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Des unverbl\u00fchten Lenzes \u2013 Freundlich ist", "tokens": ["Des", "un\u00b7ver\u00b7bl\u00fch\u00b7ten", "Len\u00b7zes", "\u2013", "Freund\u00b7lich", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und leicht gesch\u00fcrzt Thalia, eilend trat", "tokens": ["Und", "leicht", "ge\u00b7sch\u00fcrzt", "Tha\u00b7lia", ",", "ei\u00b7lend", "trat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "NE", "$,", "ADJD", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Sie vor, ergriff die Hand mir, lispelte", "tokens": ["Sie", "vor", ",", "er\u00b7griff", "die", "Hand", "mir", ",", "lis\u00b7pel\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mir leise zu: \u00bbDu gutes M\u00e4dchen schweig',", "tokens": ["Mir", "lei\u00b7se", "zu", ":", "\u00bb", "Du", "gu\u00b7tes", "M\u00e4d\u00b7chen", "schweig'", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$.", "$(", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich wei\u00df schon dein Begehren; sehn sie dich,", "tokens": ["Ich", "wei\u00df", "schon", "dein", "Be\u00b7geh\u00b7ren", ";", "sehn", "sie", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Schwestern, o dann dr\u00e4nget sich hervor", "tokens": ["Die", "Schwes\u00b7tern", ",", "o", "dann", "dr\u00e4n\u00b7get", "sich", "her\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "FM", "ADV", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Calliope, ja Melpomene zuckt", "tokens": ["Cal\u00b7li\u00b7o\u00b7pe", ",", "ja", "Mel\u00b7po\u00b7me\u00b7ne", "zuckt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "NE", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Wol gar auf mich den Dolch, Urania", "tokens": ["Wol", "gar", "auf", "mich", "den", "Dolch", ",", "U\u00b7ra\u00b7nia"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "ART", "NN", "$,", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Schwebt dann herzu, und geltend ist ihr Recht,", "tokens": ["Schwebt", "dann", "her\u00b7zu", ",", "und", "gel\u00b7tend", "ist", "ihr", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sie, die verschwisterte mit Ihr, mit Ihr,", "tokens": ["Sie", ",", "die", "ver\u00b7schwis\u00b7ter\u00b7te", "mit", "Ihr", ",", "mit", "Ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "APPR", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Auf der in Lieb' und W\u00fcnschen immer ruht", "tokens": ["Auf", "der", "in", "Lieb'", "und", "W\u00fcn\u00b7schen", "im\u00b7mer", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN", "KON", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ach, unser Aller Auge \u2013 Ina nimm", "tokens": ["Ach", ",", "un\u00b7ser", "Al\u00b7ler", "Au\u00b7ge", "\u2013", "I\u00b7na", "nimm"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "$(", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Des ganzen K\u00f6rbchens F\u00fclle, winde Strau\u00df", "tokens": ["Des", "gan\u00b7zen", "K\u00f6rb\u00b7chens", "F\u00fcl\u00b7le", ",", "win\u00b7de", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und flechte Kr\u00e4nze; jedes Bl\u00fcmchen bl\u00fch'", "tokens": ["Und", "flech\u00b7te", "Kr\u00e4n\u00b7ze", ";", "je\u00b7des", "Bl\u00fcm\u00b7chen", "bl\u00fch'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$.", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ein sch\u00f6ner Segen! Deiner Eltern Heerd", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Se\u00b7gen", "!", "Dei\u00b7ner", "El\u00b7tern", "Heerd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ist unser Heiligthum. Geh' \u2013 doch noch Eins:", "tokens": ["Ist", "un\u00b7ser", "Hei\u00b7lig\u00b7thum", ".", "Geh'", "\u2013", "doch", "noch", "Eins", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "NE", "$(", "ADV", "ADV", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Verkennt mich dort nicht! Meine Larve birgt", "tokens": ["Ver\u00b7kennt", "mich", "dort", "nicht", "!", "Mei\u00b7ne", "Lar\u00b7ve", "birgt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der R\u00fchrung Z\u00e4hren oft und im Gewand", "tokens": ["Der", "R\u00fch\u00b7rung", "Z\u00e4h\u00b7ren", "oft", "und", "im", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Des L\u00e4chelns bebt, der wahren Freude Freund,", "tokens": ["Des", "L\u00e4\u00b7chelns", "bebt", ",", "der", "wah\u00b7ren", "Freu\u00b7de", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Der Ernst, auf meiner Lippe. \u2013 Schallet nicht", "tokens": ["Der", "Ernst", ",", "auf", "mei\u00b7ner", "Lip\u00b7pe", ".", "\u2013", "Schal\u00b7let", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "$(", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Mit heller Saite, wie mit tiefem Ton", "tokens": ["Mit", "hel\u00b7ler", "Sai\u00b7te", ",", "wie", "mit", "tie\u00b7fem", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Apollons Leyer? Einer, Einer nur", "tokens": ["A\u00b7pol\u00b7lons", "Le\u00b7yer", "?", "Ei\u00b7ner", ",", "Ei\u00b7ner", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PIS", "$,", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ist unser Musenreigen! \u2013 H\u00fcpfe du", "tokens": ["Ist", "un\u00b7ser", "Mu\u00b7sen\u00b7rei\u00b7gen", "!", "\u2013", "H\u00fcp\u00b7fe", "du"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "$(", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Nun heim, und opfre t\u00f6chterlich Gel\u00fcbd'", "tokens": ["Nun", "heim", ",", "und", "opf\u00b7re", "t\u00f6ch\u00b7ter\u00b7lich", "Ge\u00b7l\u00fcbd'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und W\u00fcnsche; w\u00e4re nicht den Sterblichen", "tokens": ["Und", "W\u00fcn\u00b7sche", ";", "w\u00e4\u00b7re", "nicht", "den", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "VAFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Das Ohr verschlossen, o so h\u00f6rtet ihr", "tokens": ["Das", "Ohr", "ver\u00b7schlos\u00b7sen", ",", "o", "so", "h\u00f6r\u00b7tet", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "FM", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Auch unsers Chores Stimme sich in Wunsch", "tokens": ["Auch", "un\u00b7sers", "Cho\u00