{"dta.poem.195": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er entschuldiget sich bey dem Frauen-Zim-  \n mer wegen seiner Unbest\u00e4ndigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch werthes Jungfern-Volck ihr Lust-Geb\u00e4hrerinnen/", "tokens": ["Hoch", "wert\u00b7hes", "Jung\u00b7fern\u00b7Volck", "ihr", "Lust\u00b7Ge\u00b7b\u00e4h\u00b7re\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr Kinder des Gel\u00fccks/ ihr Schmuck der gantzen", "tokens": ["Ihr", "Kin\u00b7der", "des", "Ge\u00b7l\u00fccks", "/", "ihr", "Schmuck", "der", "gant\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welt/", "tokens": ["Welt", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der/ welcher eure Pracht nicht recht kan Lieb-gewinnen/", "tokens": ["Der", "/", "wel\u00b7cher", "eu\u00b7re", "Pracht", "nicht", "recht", "kan", "Lie\u00b7bge\u00b7win\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAT", "PPOSAT", "NN", "PTKNEG", "ADV", "VMFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor eurem Sieges-Thron in h\u00f6chster Demuth f\u00e4llt.", "tokens": ["Vor", "eu\u00b7rem", "Sie\u00b7ges\u00b7Thron", "in", "h\u00f6chs\u00b7ter", "De\u00b7muth", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er beichtet seine Schuld/ er schilt auf sein Verbrechen/", "tokens": ["Er", "beich\u00b7tet", "sei\u00b7ne", "Schuld", "/", "er", "schilt", "auf", "sein", "Ver\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein b\u00f6ses Wesen ist ihm \u00fcberm\u00e4ssig leid/", "tokens": ["Sein", "b\u00f6\u00b7ses", "We\u00b7sen", "ist", "ihm", "\u00fc\u00b7berm\u00b7\u00e4s\u00b7sig", "leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch kan er euch dabey nicht Besserung versprechen/", "tokens": ["Doch", "kan", "er", "euch", "da\u00b7bey", "nicht", "Bes\u00b7se\u00b7rung", "ver\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PAV", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil ihm in Wege steht die Unbest\u00e4ndigkeit.", "tokens": ["Weil", "ihm", "in", "We\u00b7ge", "steht", "die", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein Hertze wird zwar wol von euren Pracht ger\u00fchret/", "tokens": ["Mein", "Hert\u00b7ze", "wird", "zwar", "wol", "von", "eu\u00b7ren", "Pracht", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es scheint als w\u00e4re ich in euren Schmuck verliebt/", "tokens": ["Es", "scheint", "als", "w\u00e4\u00b7re", "ich", "in", "eu\u00b7ren", "Schmuck", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch wenn man mich alsdann zur lichten Sonnen f\u00fchren", "tokens": ["Doch", "wenn", "man", "mich", "als\u00b7dann", "zur", "lich\u00b7ten", "Son\u00b7nen", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PRF", "ADV", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So zeigt sich/ da\u00df mein Hertz nur falsche Stellung giebt.", "tokens": ["So", "zeigt", "sich", "/", "da\u00df", "mein", "Hertz", "nur", "fal\u00b7sche", "Stel\u00b7lung", "giebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$(", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich bin mir selbsten gramm ob diesen b\u00f6sen Wesen/", "tokens": ["Ich", "bin", "mir", "selbs\u00b7ten", "gramm", "ob", "die\u00b7sen", "b\u00f6\u00b7sen", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "KOUS", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich fluche aufs Geschick/ das mich nicht lieben l\u00e4st.", "tokens": ["Ich", "flu\u00b7che", "aufs", "Ge\u00b7schick", "/", "das", "mich", "nicht", "lie\u00b7ben", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "PRELS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hat heute gleich mein Hertz was Liebes auserlesen/", "tokens": ["Hat", "heu\u00b7te", "gleich", "mein", "Hertz", "was", "Lie\u00b7bes", "au\u00b7ser\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "PWS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das flieh ich morgen schon als w\u00e4r es Gifft und Pest.", "tokens": ["Das", "flieh", "ich", "mor\u00b7gen", "schon", "als", "w\u00e4r", "es", "Gifft", "und", "Pest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vertumnus kan sich nicht so bald und viel verkehren.", "tokens": ["Ver\u00b7tum\u00b7nus", "kan", "sich", "nicht", "so", "bald", "und", "viel", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PTKNEG", "ADV", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als mir mein Wanckelmuth was frisches lieben heist/", "tokens": ["Als", "mir", "mein", "Wan\u00b7ckel\u00b7muth", "was", "fri\u00b7sches", "lie\u00b7ben", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PWS", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dieselbe/ die ich fr\u00fch mu\u00df \u00fcber g\u00f6ttlich ehren/", "tokens": ["Die\u00b7sel\u00b7be", "/", "die", "ich", "fr\u00fch", "mu\u00df", "\u00fc\u00b7ber", "g\u00f6tt\u00b7lich", "eh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "PRELS", "PPER", "ADJD", "VMFIN", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vor Abend offt mein Geist von ihrem Throne schmeist.", "tokens": ["Vor", "A\u00b7bend", "offt", "mein", "Geist", "von", "ih\u00b7rem", "Thro\u00b7ne", "schmeist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Meer/ so stille ist/ und spielt mit sanfften Wellen/", "tokens": ["Das", "Meer", "/", "so", "stil\u00b7le", "ist", "/", "und", "spielt", "mit", "sanff\u00b7ten", "Wel\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJA", "VAFIN", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Erhebet nicht so bald die ungest\u00fchme Fluth/", "tokens": ["Er\u00b7he\u00b7bet", "nicht", "so", "bald", "die", "un\u00b7ge\u00b7st\u00fch\u00b7me", "Fluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als wie mein Wanckelmuth mein Lieben kan verstellen/", "tokens": ["Als", "wie", "mein", "Wan\u00b7ckel\u00b7muth", "mein", "Lie\u00b7ben", "kan", "ver\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "PPOSAT", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und setzen das in Pein/ was in der Lust geruht.", "tokens": ["Und", "set\u00b7zen", "das", "in", "Pein", "/", "was", "in", "der", "Lust", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPR", "NN", "$(", "PWS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Thier ", "tokens": ["Das", "Thier"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Ich sch\u00e4me mich darum/ da\u00df ich so schwermisch bin/", "tokens": ["Ich", "sch\u00e4\u00b7me", "mich", "da\u00b7rum", "/", "da\u00df", "ich", "so", "schwer\u00b7misch", "bin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$(", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der Unbestand des Monds kan meiner Art nicht gleichen/", "tokens": ["Der", "Un\u00b7be\u00b7stand", "des", "Monds", "kan", "mei\u00b7ner", "Art", "nicht", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es \u00fcberwieget ihn mein unbest\u00e4ndger Sinn.", "tokens": ["Es", "\u00fc\u00b7berw\u00b7ie\u00b7get", "ihn", "mein", "un\u00b7be\u00b7st\u00e4nd\u00b7ger", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Auch meinem Wanckelmuth nicht im geringsten gleicht/", "tokens": ["Auch", "mei\u00b7nem", "Wan\u00b7ckel\u00b7muth", "nicht", "im", "ge\u00b7rings\u00b7ten", "gleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der bin ich jetzo hold/ und jene la\u00df ich schlendern/", "tokens": ["Der", "bin", "ich", "jet\u00b7zo", "hold", "/", "und", "je\u00b7ne", "la\u00df", "ich", "schlen\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$(", "KON", "PDS", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dem Augenblick schon dort mein Hertz die Seegel streicht.", "tokens": ["Dem", "Au\u00b7gen\u00b7blick", "schon", "dort", "mein", "Hertz", "die", "See\u00b7gel", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dis ist Hochwertheste was meine Sinnen qu\u00e4hlet/", "tokens": ["Dis", "ist", "Hoch\u00b7wert\u00b7hes\u00b7te", "was", "mei\u00b7ne", "Sin\u00b7nen", "qu\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "PRELS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Was mich in Kummer setzt/ was meine Brust veracht.", "tokens": ["Was", "mich", "in", "Kum\u00b7mer", "setzt", "/", "was", "mei\u00b7ne", "Brust", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Macht doch Annehmlichste/ da\u00df mich ein Stern beseelet", "tokens": ["Macht", "doch", "An\u00b7nehm\u00b7lichs\u00b7te", "/", "da\u00df", "mich", "ein", "Stern", "be\u00b7see\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Der gegen euren Schmuck mein Hertze brennen macht!", "tokens": ["Der", "ge\u00b7gen", "eu\u00b7ren", "Schmuck", "mein", "Hert\u00b7ze", "bren\u00b7nen", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPOSAT", "VVFIN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich w\u00fcnsche mir gar offt ein hei\u00df verliebtes Hertze/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "gar", "offt", "ein", "hei\u00df", "ver\u00b7lieb\u00b7tes", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJD", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn eine sch\u00f6ne Hand mir meine Finger dr\u00fcckt/", "tokens": ["Wenn", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Hand", "mir", "mei\u00b7ne", "Fin\u00b7ger", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch dieses ist alsdann mein allergr\u00f6\u00dfter Schmertze/", "tokens": ["Doch", "die\u00b7ses", "ist", "als\u00b7dann", "mein", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ter", "Schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da\u00df sich mein Stellen nicht zu ihren Flammen schickt.", "tokens": ["Da\u00df", "sich", "mein", "Stel\u00b7len", "nicht", "zu", "ih\u00b7ren", "Flam\u00b7men", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ach Engels z\u00fcrnet nicht/ verspahret eur Verdammen/", "tokens": ["Ach", "En\u00b7gels", "z\u00fcr\u00b7net", "nicht", "/", "ver\u00b7spah\u00b7ret", "eur", "Ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], 