{"textgrid.poem.59592": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann beginnt das Heil zu tagen?", "tokens": ["Wann", "be\u00b7ginnt", "das", "Heil", "zu", "ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es braust mit Rossen und mit Wagen", "tokens": ["Es", "braust", "mit", "Ros\u00b7sen", "und", "mit", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wild durch die weite Welt der Krieg,", "tokens": ["Wild", "durch", "die", "wei\u00b7te", "Welt", "der", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Brandgemalte Teufel scherzen", "tokens": ["Brand\u00b7ge\u00b7mal\u00b7te", "Teu\u00b7fel", "scher\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Menschenrechte Menschenherzen,", "tokens": ["Mit", "Men\u00b7schen\u00b7rech\u00b7te", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die schwarze H\u00f6lle hat den Sieg.", "tokens": ["Die", "schwar\u00b7ze", "H\u00f6l\u00b7le", "hat", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie rufen trotzig aus", "tokens": ["Sie", "ru\u00b7fen", "trot\u00b7zig", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In alle Welt hinaus:", "tokens": ["In", "al\u00b7le", "Welt", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch die Wahrheit steht und schweiget,", "tokens": ["Doch", "die", "Wahr\u00b7heit", "steht", "und", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die stolze Freiheit traurt und zeuget", "tokens": ["Die", "stol\u00b7ze", "Frei\u00b7heit", "traurt", "und", "zeu\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Satans glatten Worten nicht,", "tokens": ["Des", "Sa\u00b7tans", "glat\u00b7ten", "Wor\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ehre fliehet vor der Schande,", "tokens": ["Die", "Eh\u00b7re", "flie\u00b7het", "vor", "der", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Treue r\u00e4umet flugs die Lande,", "tokens": ["Die", "Treu\u00b7e", "r\u00e4u\u00b7met", "flugs", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wohnet nur mit Recht und Pflicht.", "tokens": ["Sie", "woh\u00b7net", "nur", "mit", "Recht", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die hohen Zeugen all", "tokens": ["Die", "ho\u00b7hen", "Zeu\u00b7gen", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Erklingen lauten Schall:", "tokens": ["Er\u00b7klin\u00b7gen", "lau\u00b7ten", "Schall", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nimmer, nimmer war Gottes Reich", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "war", "Got\u00b7tes", "Reich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Der H\u00f6lle gleich,", "tokens": ["Der", "H\u00f6l\u00b7le", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ihr Bund hei\u00dft Elend, Trug ihr Steig.", "tokens": ["Ihr", "Bund", "hei\u00dft", "E\u00b7lend", ",", "Trug", "ihr", "Steig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seid gegr\u00fc\u00dft, ihr edlen Zeugen!", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "ed\u00b7len", "Zeu\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der h\u00f6chste Richter wird nicht schweigen,", "tokens": ["Der", "h\u00f6chs\u00b7te", "Rich\u00b7ter", "wird", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der waltend hoch auf Sternen geht,", "tokens": ["Der", "wal\u00b7tend", "hoch", "auf", "Ster\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der die lichten Himmelskerzen", "tokens": ["Der", "die", "lich\u00b7ten", "Him\u00b7mels\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Entz\u00fcndet und die Menschenherzen", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "und", "die", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seines Odems Kraft durchweht.", "tokens": ["Mit", "sei\u00b7nes", "O\u00b7dems", "Kraft", "durch\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er ist der rechte Mann,", "tokens": ["Er", "ist", "der", "rech\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der einzig helfen kann:", "tokens": ["Der", "ein\u00b7zig", "hel\u00b7fen", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Preis dem M\u00e4chtigen! Preis dem Hort!", "tokens": ["Preis", "dem", "M\u00e4ch\u00b7ti\u00b7gen", "!", "Preis", "dem", "Hort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Es steht sein Wort:", "tokens": ["Es", "steht", "sein", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Tobe, Satan! Sei verwegen!", "tokens": ["To\u00b7be", ",", "Sa\u00b7tan", "!", "Sei", "ver\u00b7we\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VAIMP", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dieser Macht zersplittern Degen,", "tokens": ["Vor", "die\u00b7ser", "Macht", "zer\u00b7split\u00b7tern", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zerspringet diamantner Stahl!", "tokens": ["Zer\u00b7sprin\u00b7get", "di\u00b7a\u00b7mant\u00b7ner", "Stahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott will Recht und Ehre sch\u00fctzen", "tokens": ["Gott", "will", "Recht", "und", "Eh\u00b7re", "sch\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Trug und Bosheit niederblitzen", "tokens": ["Und", "Trug", "und", "Bos\u00b7heit", "nie\u00b7der\u00b7blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seiner Rache Donnerstrahl:", "tokens": ["Mit", "sei\u00b7ner", "Ra\u00b7che", "Don\u00b7ner\u00b7strahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der starke Siegesheld,", "tokens": ["Der", "star\u00b7ke", "Sie\u00b7ge\u00b7sheld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Erd' und Himmel h\u00e4lt,", "tokens": ["Der", "Erd'", "und", "Him\u00b7mel", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Schmettert Schande hinab ins Nichts,", "tokens": ["Schmet\u00b7tert", "Schan\u00b7de", "hin\u00b7ab", "ins", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPRART", "PIS", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Der Gott des Lichts", "tokens": ["Der", "Gott", "des", "Lichts"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ist nicht ein Gott des B\u00f6sewichts.", "tokens": ["Ist", "nicht", "ein", "Gott", "des", "B\u00f6\u00b7se\u00b7wichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum himmelauf, Gedanken!", "tokens": ["Da\u00b7rum", "him\u00b7me\u00b7lauf", ",", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Gott dem Helfer in die Schranken", "tokens": ["Mit", "Gott", "dem", "Hel\u00b7fer", "in", "die", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Freiheit, Recht und Vaterland!", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "Recht", "und", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ihr's meint mit rechten Treuen,", "tokens": ["So", "ih\u00b7r's", "meint", "mit", "rech\u00b7ten", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bl\u00e4st Gott euch an mit Mut der Leuen", "tokens": ["Bl\u00e4st", "Gott", "euch", "an", "mit", "Mut", "der", "Leu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PPER", "APPR", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und st\u00e4rkt mit Kraft die schw\u00e4chste Hand!", "tokens": ["Und", "st\u00e4rkt", "mit", "Kraft", "die", "schw\u00e4chs\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der gute fromme Gott,", "tokens": ["Der", "gu\u00b7te", "from\u00b7me", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Er bleibt in Not und Tod.", "tokens": ["Er", "bleibt", "in", "Not", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Fallet nieder und betet an!", "tokens": ["Fal\u00b7let", "nie\u00b7der", "und", "be\u00b7tet", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Der helfen kann,", "tokens": ["Der", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Er ficht als Streiter euch voran.", "tokens": ["Er", "ficht", "als", "Strei\u00b7ter", "euch", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wann beginnt das Heil zu tagen?", "tokens": ["Wann", "be\u00b7ginnt", "das", "Heil", "zu", "ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es braust mit Rossen und mit Wagen", "tokens": ["Es", "braust", "mit", "Ros\u00b7sen", "und", "mit", "Wa\u00b7gen"], "token_info": ["word", "word", "word",