{"dta.poem.3824": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00f6stendes Schreiben,  \n Bey Absterben eines alten wohlverdienten Geistlichen, auf be-  \n sondere Umst\u00e4nde, unter eines andern Nahmen 1743.  \n verfertiget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "O Freunde! deren Hertz in Blut und Thr\u00e4-", "tokens": ["O", "Freun\u00b7de", "!", "de\u00b7ren", "Hertz", "in", "Blut", "und", "Thr\u00e4"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PRELAT", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nen schwimmt,", "tokens": ["nen", "schwimmt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da euch des H\u00f6chsten Hand den frommen", "tokens": ["Da", "euch", "des", "H\u00f6chs\u00b7ten", "Hand", "den", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nathan nimmt.", "tokens": ["Na\u00b7than", "nimmt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Da der getreue Hirt erbla\u00dft, erstarrt, erblichen,", "tokens": ["Da", "der", "ge\u00b7treu\u00b7e", "Hirt", "er\u00b7bla\u00dft", ",", "er\u00b7starrt", ",", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch das Urims Schild von seiner Brust ge-", "tokens": ["Wo\u00b7durch", "das", "U\u00b7rims", "Schild", "von", "sei\u00b7ner", "Brust", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wichen,", "tokens": ["wi\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Von dem beliebten Krei\u00df; ach euer Simeon", "tokens": ["Von", "dem", "be\u00b7lieb\u00b7ten", "Krei\u00df", ";", "ach", "eu\u00b7er", "Si\u00b7me\u00b7on"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "XY", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sieht nun das Heil der Welt f\u00fcr GOttes Glantz", "tokens": ["Sieht", "nun", "das", "Heil", "der", "Welt", "f\u00fcr", "Got\u00b7tes", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und Thron.", "tokens": ["und", "Thron", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Euch schmertzet sein Verlust, und di\u00df gerecht u. billig,", "tokens": ["Euch", "schmert\u00b7zet", "sein", "Ver\u00b7lust", ",", "und", "di\u00df", "ge\u00b7recht", "u.", "bil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PDS", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Warum? Euch kr\u00e4nckt sein Amt, er war getrost", "tokens": ["Wa\u00b7rum", "?", "Euch", "kr\u00e4nckt", "sein", "Amt", ",", "er", "war", "ge\u00b7trost"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "und willig,", "tokens": ["und", "wil\u00b7lig", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Und eurer Seelen Wohl hat er mit Flei\u00df gesucht,", "tokens": ["Und", "eu\u00b7rer", "See\u00b7len", "Wohl", "hat", "er", "mit", "Flei\u00df", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Laster war sein Feind, die S\u00fcnde ihm verflucht.", "tokens": ["Das", "Las\u00b7ter", "war", "sein", "Feind", ",", "die", "S\u00fcn\u00b7de", "ihm", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Worte, so der HErr in Tafeln einge\u00e4tzet,", "tokens": ["Die", "Wor\u00b7te", ",", "so", "der", "Herr", "in", "Ta\u00b7feln", "ein\u00b7ge\u00b7\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hat er aus Schwachheit zwar, doch nie mit Flei\u00df", "tokens": ["Hat", "er", "aus", "Schwach\u00b7heit", "zwar", ",", "doch", "nie", "mit", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "$,", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "verletzet.", "tokens": ["ver\u00b7let\u00b7zet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Nun l\u00e4st er euch, sein Volck, er flieht di\u00df Jam\u0303erthal,", "tokens": ["Nun", "l\u00e4st", "er", "euch", ",", "sein", "Volck", ",", "er", "flieht", "di\u00df", "Jam\u0303er\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "O h\u00f6chst begl\u00fcckter Tausch! o auserlesne Wahl!", "tokens": ["O", "h\u00f6chst", "be\u00b7gl\u00fcck\u00b7ter", "Tausch", "!", "o", "au\u00b7ser\u00b7les\u00b7ne", "Wahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er streckt den Hirten-Stab, er \u00fcbergiebt die Heerde,", "tokens": ["Er", "streckt", "den", "Hir\u00b7ten\u00b7Stab", ",", "er", "\u00fc\u00b7berg\u00b7iebt", "die", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df er des Lammes Knecht vor seinem Throne", "tokens": ["Da\u00df", "er", "des", "Lam\u00b7mes", "Knecht", "vor", "sei\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "werde.", "tokens": ["wer\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Die Vorsicht dr\u00fcckt ihm selbst die m\u00fcden Augen zu,", "tokens": ["Die", "Vor\u00b7sicht", "dr\u00fcckt", "ihm", "selbst", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wohlan so g\u00f6nnet ihm die angenehmste Ruh.", "tokens": ["Wo\u00b7hlan", "so", "g\u00f6n\u00b7net", "ihm", "die", "an\u00b7ge\u00b7nehms\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Streut Buchsbaum, streut Napell, streut gr\u00fcne", "tokens": ["Streut", "Buchs\u00b7baum", ",", "streut", "Na\u00b7pell", ",", "streut", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NE", "$,", "VVFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Leid-Cypressen,", "tokens": ["Lei\u00b7dCy\u00b7pres\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Zum Zeichen, sein Verdienst sey ewig unvergessen.", "tokens": ["Zum", "Zei\u00b7chen", ",", "sein", "Ver\u00b7dienst", "sey", "e\u00b7wig", "un\u00b7ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Betr\u00fcbte! bauet ihm den besten Leichen-Stein,", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "!", "bau\u00b7et", "ihm", "den", "bes\u00b7ten", "Lei\u00b7chen\u00b7Stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Grabt Nahmen, Danck und Ruhm in eure Her-", "tokens": ["Grabt", "Nah\u00b7men", ",", "Danck", "und", "Ruhm", "in", "eu\u00b7re", "Her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "tzen ein.", "tokens": ["tzen", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Legt euer Aaron so Amt als Ephod nieder,", "tokens": ["Legt", "eu\u00b7er", "Aa\u00b7ron", "so", "Amt", "als", "E\u00b7phod", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Vielleicht ergreifft es nun sein Eleasar wieder;", "tokens": ["Viel\u00b7leicht", "er\u00b7greifft", "es", "nun", "sein", "E\u00b7lea\u00b7sar", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Sucht er die stoltze Ruh in Elims Palmen-Stadt,", "tokens": ["Sucht", "er", "die", "stolt\u00b7ze", "Ruh", "in", "E\u00b7lims", "Pal\u00b7men\u00b7Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erweget, da\u00df der HErr noch andre Hirten hat.", "tokens": ["Er\u00b7we\u00b7get", ",", "da\u00df", "der", "Herr", "noch", "and\u00b7re", "Hir\u00b7ten", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}