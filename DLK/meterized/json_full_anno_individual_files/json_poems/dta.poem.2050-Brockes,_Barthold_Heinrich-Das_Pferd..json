{"dta.poem.2050": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Pferd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als der Glieder Symmetrie, ernstlich und bedachtsam", "tokens": ["Als", "der", "Glie\u00b7der", "Sym\u00b7me\u00b7trie", ",", "ernst\u00b7lich", "und", "be\u00b7dacht\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sehn,", "tokens": ["sehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wenn wir, da\u00df sein frecher Geist doch sich z\u00e4hmen l\u00e4\u00dft,", "tokens": ["Wenn", "wir", ",", "da\u00df", "sein", "fre\u00b7cher", "Geist", "doch", "sich", "z\u00e4h\u00b7men", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "betrachten,", "tokens": ["be\u00b7trach\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wenn wir auf den vielen Nutzen, den die\u00df Thier uns", "tokens": ["Wenn", "wir", "auf", "den", "vie\u00b7len", "Nut\u00b7zen", ",", "den", "die\u00df", "Thier", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "PIAT", "NN", "$,", "PRELS", "PDS", "NN", "PPER"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "bringet, achten,", "tokens": ["brin\u00b7get", ",", "ach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sollten wir denn nicht den Sch\u00f6pfer zu bewundern Ursach", "tokens": ["Soll\u00b7ten", "wir", "denn", "nicht", "den", "Sch\u00f6p\u00b7fer", "zu", "be\u00b7wun\u00b7dern", "Ur\u00b7sach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "NN"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "finden?", "tokens": ["fin\u00b7den", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Sollten wir nicht, auch bey Pferden, Denken, Lust und", "tokens": ["Soll\u00b7ten", "wir", "nicht", ",", "auch", "bey", "Pfer\u00b7den", ",", "Den\u00b7ken", ",", "Lust", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Dank verbinden?", "tokens": ["Dank", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Sollten wir darinn nicht Weisheit, Macht, und Liebes-", "tokens": ["Soll\u00b7ten", "wir", "da\u00b7rinn", "nicht", "Weis\u00b7heit", ",", "Macht", ",", "und", "Lie\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PAV", "PTKNEG", "NN", "$,", "NN", "$,", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "spuren sehn,", "tokens": ["spu\u00b7ren", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und die Weisheit, Macht und Liebe durch Bewundrung", "tokens": ["Und", "die", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", "durch", "Be\u00b7wund\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "nicht erh\u00f6hn?", "tokens": ["nicht", "er\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}