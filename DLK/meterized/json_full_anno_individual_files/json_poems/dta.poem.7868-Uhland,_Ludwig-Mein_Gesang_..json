{"dta.poem.7868": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mein Gesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ich die Freude nie empfunden?", "tokens": ["Ob", "ich", "die", "Freu\u00b7de", "nie", "emp\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob stets mein Lied so traurig klang?", "tokens": ["Ob", "stets", "mein", "Lied", "so", "trau\u00b7rig", "klang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O nein! ich lebte frohe Stunden,", "tokens": ["O", "nein", "!", "ich", "leb\u00b7te", "fro\u00b7he", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da war mein Leben Lustgesang.", "tokens": ["Da", "war", "mein", "Le\u00b7ben", "Lust\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die milde Gegenwart der S\u00fc\u00dfen", "tokens": ["Die", "mil\u00b7de", "Ge\u00b7gen\u00b7wart", "der", "S\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verkl\u00e4rte mir das Blumenjahr.", "tokens": ["Ver\u00b7kl\u00e4r\u00b7te", "mir", "das", "Blu\u00b7men\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Morgentr\u00e4ume mir verhie\u00dfen,", "tokens": ["Was", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7me", "mir", "ver\u00b7hie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das machte stets der Abend wahr.", "tokens": ["Das", "mach\u00b7te", "stets", "der", "A\u00b7bend", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O k\u00f6nnten meiner Wonne zeugen", "tokens": ["O", "k\u00f6nn\u00b7ten", "mei\u00b7ner", "Won\u00b7ne", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels und der B\u00e4che Blau,", "tokens": ["Des", "Him\u00b7mels", "und", "der", "B\u00e4\u00b7che", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Haine mit den Bl\u00fcthenzweigen,", "tokens": ["Die", "Hai\u00b7ne", "mit", "den", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Garten und die lichte Au!", "tokens": ["Der", "Gar\u00b7ten", "und", "die", "lich\u00b7te", "Au", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die haben Alles einst gesehen,", "tokens": ["Die", "ha\u00b7ben", "Al\u00b7les", "einst", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und haben Alles einst geh\u00f6rt.", "tokens": ["Und", "ha\u00b7ben", "Al\u00b7les", "einst", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ach! sie m\u00fcssen traurig stehen,", "tokens": ["Doch", "ach", "!", "sie", "m\u00fcs\u00b7sen", "trau\u00b7rig", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch ihre Zier ist nun zerst\u00f6rt.", "tokens": ["Auch", "ih\u00b7re", "Zier", "ist", "nun", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du aber zeuge, meine Traute!", "tokens": ["Du", "a\u00b7ber", "zeu\u00b7ge", ",", "mei\u00b7ne", "Trau\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Ferne mir, du Nahe doch!", "tokens": ["Du", "Fer\u00b7ne", "mir", ",", "du", "Na\u00b7he", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$,", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du denkst der kindlich frohen Laute,", "tokens": ["Du", "denkst", "der", "kind\u00b7lich", "fro\u00b7hen", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du denkst der sel\u2019gen Blicke noch.", "tokens": ["Du", "denkst", "der", "sel'\u00b7gen", "Bli\u00b7cke", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir hatten uns so ganz empfunden,", "tokens": ["Wir", "hat\u00b7ten", "uns", "so", "ganz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir suchten nicht das enge Wort;", "tokens": ["Wir", "such\u00b7ten", "nicht", "das", "en\u00b7ge", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns flo\u00df der rasche Strom der Stunden", "tokens": ["Uns", "flo\u00df", "der", "ra\u00b7sche", "Strom", "der", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In freien Melodieen fort.", "tokens": ["In", "frei\u00b7en", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Du schiedest hin, die Welt ward \u00f6de,", "tokens": ["Du", "schie\u00b7dest", "hin", ",", "die", "Welt", "ward", "\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich stieg hinab in meine Brust;", "tokens": ["Ich", "stieg", "hin\u00b7ab", "in", "mei\u00b7ne", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lieder sanfte Klagerede", "tokens": ["Der", "Lie\u00b7der", "sanf\u00b7te", "Kla\u00b7ge\u00b7re\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist all mein Trost und meine Lust.", "tokens": ["Ist", "all", "mein", "Trost", "und", "mei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was bleibt mir, als in Trauert\u00f6nen", "tokens": ["Was", "bleibt", "mir", ",", "als", "in", "Trau\u00b7er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu singen die Vergangenheit?", "tokens": ["Zu", "sin\u00b7gen", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und als mich schmerzlich hinzusehnen", "tokens": ["Und", "als", "mich", "schmerz\u00b7lich", "hin\u00b7zu\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In neue goldne Liebeszeit?", "tokens": ["In", "neu\u00b7e", "gold\u00b7ne", "Lie\u00b7bes\u00b7zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}