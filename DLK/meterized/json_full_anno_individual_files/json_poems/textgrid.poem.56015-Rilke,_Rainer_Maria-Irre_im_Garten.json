{"textgrid.poem.56015": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Irre im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch schlie\u00dft die aufgegebene Kartause", "tokens": ["Noch", "schlie\u00dft", "die", "auf\u00b7ge\u00b7ge\u00b7be\u00b7ne", "Kar\u00b7tau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "sich um den Hof, als w\u00fcrde etwas heil.", "tokens": ["sich", "um", "den", "Hof", ",", "als", "w\u00fcr\u00b7de", "et\u00b7was", "heil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "KOKOM", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch die sie jetzt bewohnen, haben Pause", "tokens": ["Auch", "die", "sie", "jetzt", "be\u00b7woh\u00b7nen", ",", "ha\u00b7ben", "Pau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "PPER", "ADV", "VVINF", "$,", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und nehmen nicht am Leben drau\u00dfen teil.", "tokens": ["und", "neh\u00b7men", "nicht", "am", "Le\u00b7ben", "drau\u00b7\u00dfen", "teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was irgend kommen konnte, das verlief.", "tokens": ["Was", "ir\u00b7gend", "kom\u00b7men", "konn\u00b7te", ",", "das", "ver\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "punct", "word