{"textgrid.poem.36208": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich traf, ich wei\u00df nicht wie, ein s\u00fc\u00df Verlangen,", "tokens": ["Mich", "traf", ",", "ich", "wei\u00df", "nicht", "wie", ",", "ein", "s\u00fc\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sogleich mit dir zu sterben.", "tokens": ["Sog\u00b7leich", "mit", "dir", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es d\u00fcnkte mich, wir gingen", "tokens": ["Es", "d\u00fcnk\u00b7te", "mich", ",", "wir", "gin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Gr\u00fcn, die Stirn vom Morgenstrahl getroffen,", "tokens": ["Im", "Gr\u00fcn", ",", "die", "Stirn", "vom", "Mor\u00b7gen\u00b7strahl", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weit, weit von Menschenspuren.", "tokens": ["Weit", ",", "weit", "von", "Men\u00b7schen\u00b7spu\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Steigen h\u00f6ren wir die Lerche singen,", "tokens": ["Im", "Stei\u00b7gen", "h\u00f6\u00b7ren", "wir", "die", "Ler\u00b7che", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Auge l\u00e4chelt auf die stillen Fluren,", "tokens": ["Das", "Au\u00b7ge", "l\u00e4\u00b7chelt", "auf", "die", "stil\u00b7len", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Berges H\u00f6h' schon nah, wo froh wir wagen,", "tokens": ["Des", "Ber\u00b7ges", "H\u00f6h'", "schon", "nah", ",", "wo", "froh", "wir", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Freiheit zu erwerben.", "tokens": ["Die", "Frei\u00b7heit", "zu", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nun wirst den Freund du, Freudenreiche fragen:", "tokens": ["Nun", "wirst", "den", "Freund", "du", ",", "Freu\u00b7den\u00b7rei\u00b7che", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbwarum willst du verderben?\u00ab \u2013", "tokens": ["\u00bb", "wa\u00b7rum", "willst", "du", "ver\u00b7der\u00b7ben", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ich sah mich selbst, mein Wesen klar und offen;", "tokens": ["Ich", "sah", "mich", "selbst", ",", "mein", "We\u00b7sen", "klar", "und", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Erreicht das Ziel, das alle W\u00fcnsche hoffen,", "tokens": ["Er\u00b7reicht", "das", "Ziel", ",", "das", "al\u00b7le", "W\u00fcn\u00b7sche", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn sie sich nicht beschr\u00e4nken.", "tokens": ["Wenn", "sie", "sich", "nicht", "be\u00b7schr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Wie wenig alles, mu\u00df ich seitdem denken.", "tokens": ["Wie", "we\u00b7nig", "al\u00b7les", ",", "mu\u00df", "ich", "seit\u00b7dem", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "$,", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich traf, ich wei\u00df nicht wie, ein s\u00fc\u00df Verlangen,", "tokens": ["Mich", "traf", ",", "ich", "wei\u00df", "nicht", "wie", "