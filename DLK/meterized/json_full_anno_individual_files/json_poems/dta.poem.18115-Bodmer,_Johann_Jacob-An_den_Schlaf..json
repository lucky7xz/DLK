{"dta.poem.18115": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wach allhie mit sehnlichem Verlangen,", "tokens": ["Ich", "wach", "all\u00b7hie", "mit", "sehn\u00b7li\u00b7chem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sanfter Schlaf hast gantz mein Lieb umfangen,", "tokens": ["Du", "sanf\u00b7ter", "Schlaf", "hast", "gantz", "mein", "Lieb", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erblickt sie dich mit einem Aeugelein,", "tokens": ["Er\u00b7blickt", "sie", "dich", "mit", "ei\u00b7nem", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirst du bald von ihr vertrieben seyn.", "tokens": ["So", "wirst", "du", "bald", "von", "ihr", "ver\u00b7trie\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}