{"dta.poem.13281": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So machte sie den Schlu\u00df; zugleich erwies der Saal", "tokens": ["So", "mach\u00b7te", "sie", "den", "Schlu\u00df", ";", "zu\u00b7gleich", "er\u00b7wies", "der", "Saal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie, zu was er schon bereitet war, befahl.", "tokens": ["Da\u00df", "sie", ",", "zu", "was", "er", "schon", "be\u00b7rei\u00b7tet", "war", ",", "be\u00b7fahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "615Der F\u00fcrst der Ehgemahl bezeigte sich indessen,", "tokens": ["F\u00fcrst", "der", "Eh\u00b7ge\u00b7mahl", "be\u00b7zeig\u00b7te", "sich", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Als h\u00e4tt er ihren Wunsch und den Entschlu\u00df ermessen.", "tokens": ["Als", "h\u00e4tt", "er", "ih\u00b7ren", "Wunsch", "und", "den", "Ent\u00b7schlu\u00df", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drauf sprach er: \u201eK\u00f6niginn! verordne! du bist Frau;", "tokens": ["Drauf", "sprach", "er", ":", "\u201e", "K\u00f6\u00b7ni\u00b7ginn", "!", "ver\u00b7ord\u00b7ne", "!", "du", "bist", "Frau", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "ADJA", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edas Kreuz und dein Befehl ist das, worauf ich bau.", "tokens": ["\u201e", "das", "Kreuz", "und", "dein", "Be\u00b7fehl", "ist", "das", ",", "wo\u00b7rauf", "ich", "bau", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "PPOSAT", "NN", "VAFIN", "PDS", "$,", "PWAV", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ees weichen Feind und Furcht! derselben Stell\u2019 erf\u00fclle", "tokens": ["\u201e", "es", "wei\u00b7chen", "Feind", "und", "Furcht", "!", "der\u00b7sel\u00b7ben", "Stell'", "er\u00b7f\u00fcl\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "ADJA", "NN", "KON", "NN", "$.", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "620\u201dDas einzige Gesaz, Theresia! dein Wille.", "tokens": ["\"", "Das", "ein\u00b7zi\u00b7ge", "Ge\u00b7saz", ",", "The\u00b7re\u00b7sia", "!", "dein", "Wil\u00b7le", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "NE", "$.", "PPOSAT", "NN", "$."], "meter": "--+-----+-+-", "measure": "anapaest.init"}}}}}