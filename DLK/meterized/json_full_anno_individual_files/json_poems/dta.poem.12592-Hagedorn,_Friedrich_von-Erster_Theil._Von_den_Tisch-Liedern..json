{"dta.poem.12592": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du Admets Geschichte weisst,", "tokens": ["Der", "du", "Ad\u00b7mets", "Ge\u00b7schich\u00b7te", "weisst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Freund, liebe stets nur brave M\u00e4nner,", "tokens": ["Freund", ",", "lie\u00b7be", "stets", "nur", "bra\u00b7ve", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meide die verzagten Selen;", "tokens": ["Und", "mei\u00b7de", "die", "ver\u00b7zag\u00b7ten", "Se\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn die Gesellschaft dieser Leute", "tokens": ["Denn", "die", "Ge\u00b7sell\u00b7schaft", "die\u00b7ser", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird dir gewi\u00df verdrie\u00dflich seyn.", "tokens": ["Wird", "dir", "ge\u00b7wi\u00df", "ver\u00b7drie\u00df\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}