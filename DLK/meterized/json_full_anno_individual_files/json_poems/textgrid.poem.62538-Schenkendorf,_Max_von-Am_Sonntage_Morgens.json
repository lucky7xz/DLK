{"textgrid.poem.62538": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Sonntage Morgens", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, mein Herz! la\u00df deine Stimme h\u00f6ren!", "tokens": ["Auf", ",", "mein", "Herz", "!", "la\u00df", "dei\u00b7ne", "Stim\u00b7me", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Singe laut mit allen Engelch\u00f6ren:", "tokens": ["Sin\u00b7ge", "laut", "mit", "al\u00b7len", "En\u00b7gelc\u00b7h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heilig, heilig ist", "tokens": ["Hei\u00b7lig", ",", "hei\u00b7lig", "ist"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJD", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gottes Sohn, der Christ.", "tokens": ["Got\u00b7tes", "Sohn", ",", "der", "Christ", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Seinen Namen will ich stets verehren.", "tokens": ["Sei\u00b7nen", "Na\u00b7men", "will", "ich", "stets", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ihn, in dem sich alle Kniee beugen,", "tokens": ["Ihn", ",", "in", "dem", "sich", "al\u00b7le", "Kni\u00b7ee", "beu\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELS", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sollen meine Lippen laut bezeugen,", "tokens": ["Sol\u00b7len", "mei\u00b7ne", "Lip\u00b7pen", "laut", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weil mein Herz noch schl\u00e4gt,", "tokens": ["Weil", "mein", "Herz", "noch", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und die Zung' sich regt,", "tokens": ["Und", "die", "Zung'", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Will ich nicht von seinem Lobe schweigen.", "tokens": ["Will", "ich", "nicht", "von", "sei\u00b7nem", "Lo\u00b7be", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Bis der ew'ge Sabbath wird erscheinen,", "tokens": ["Bis", "der", "ew'\u00b7ge", "Sab\u00b7ba\u00b7th", "wird", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo kein S\u00fcnd'gen sein wird, und kein Weinen,", "tokens": ["Wo", "kein", "S\u00fcn\u00b7d'\u00b7gen", "sein", "wird", ",", "und", "kein", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAINF", "VAFIN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo mein Lobgesang", "tokens": ["Wo", "mein", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mit dem Harfenklang", "tokens": ["Mit", "dem", "Har\u00b7fen\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aller Heiligen sich wird vereinen.", "tokens": ["Al\u00b7ler", "Hei\u00b7li\u00b7gen", "sich", "wird", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Auf, mein Herz! la\u00df deine Stimme h\u00f6ren!", "tokens": ["Auf", ",", "mein", "Herz", "!", "la\u00df", "dei\u00b7ne", "Stim\u00b7me", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Singe laut mit allen Engelch\u00f6ren:", "tokens": ["Sin\u00b7ge", "laut", "mit", "al\u00b7len", "En\u00b7gelc\u00b7h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heilig, heilig ist", "tokens": ["Hei\u00b7lig", ",", "hei\u00b7lig", "ist"], "token_info": ["word", "punct", "word", "word"], "pos"