{"dta.poem.19306": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur wer ein K\u00f6nig war, kann den Verlust empfinden,", "tokens": ["Nur", "wer", "ein", "K\u00f6\u00b7nig", "war", ",", "kann", "den", "Ver\u00b7lust", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VAFIN", "$,", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er ein Reich verlor, und kann's nicht wieder finden.", "tokens": ["Da\u00df", "er", "ein", "Reich", "ver\u00b7lor", ",", "und", "kann's", "nicht", "wie\u00b7der", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und so empfindet wol der Mensch, da\u00df er verlor", "tokens": ["Und", "so", "emp\u00b7fin\u00b7det", "wol", "der", "Mensch", ",", "da\u00df", "er", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Herrschaft der Natur, die er besa\u00df zuvor.", "tokens": ["Die", "Herr\u00b7schaft", "der", "Na\u00b7tur", ",", "die", "er", "be\u00b7sa\u00df", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wodurch lie\u00df er den Stab der Herrschaft sich entwinden?", "tokens": ["Wo\u00b7durch", "lie\u00df", "er", "den", "Stab", "der", "Herr\u00b7schaft", "sich", "ent\u00b7win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was soll er nun thun um wieder ihn zu finden!", "tokens": ["Und", "was", "soll", "er", "nun", "thun", "um", "wie\u00b7der", "ihn", "zu", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "KOUI", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}