{"dta.poem.14489": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "sv:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun rauschen schon st\u00e4rker die W\u00e4lder,", "tokens": ["Nun", "rau\u00b7schen", "schon", "st\u00e4r\u00b7ker", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Morgenlicht funkelt herauf,", "tokens": ["Mor\u00b7gen\u00b7licht", "fun\u00b7kelt", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Lerche singt \u00fcber den Feldern,", "tokens": ["Die", "Ler\u00b7che", "singt", "\u00fc\u00b7ber", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sch\u00f6ne Erde nun wache auf!", "tokens": ["Sch\u00f6\u00b7ne", "Er\u00b7de", "nun", "wa\u00b7che", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJA", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}