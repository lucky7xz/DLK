{"textgrid.poem.48056": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Mainacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So geh ich einsam wieder meine Bahnen", "tokens": ["So", "geh", "ich", "ein\u00b7sam", "wie\u00b7der", "mei\u00b7ne", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im gleichen Schritt im kalten D\u00e4mmerlicht,", "tokens": ["im", "glei\u00b7chen", "Schritt", "im", "kal\u00b7ten", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und selten treibt ein stummes Liebesahnen", "tokens": ["und", "sel\u00b7ten", "treibt", "ein", "stum\u00b7mes", "Lie\u00b7be\u00b7sah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Blut mir noch ins bleiche Angesicht.", "tokens": ["das", "Blut", "mir", "noch", "ins", "blei\u00b7che", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist, wenn traumesselig in der holden", "tokens": ["Das", "ist", ",", "wenn", "trau\u00b7mes\u00b7se\u00b7lig", "in", "der", "hol\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "ADJD