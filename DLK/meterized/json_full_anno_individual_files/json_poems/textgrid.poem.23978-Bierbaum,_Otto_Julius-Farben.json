{"textgrid.poem.23978": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Farben", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Moose mein Kopf,", "tokens": ["Auf", "dem", "Moo\u00b7se", "mein", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In den Himmel mein Blick,", "tokens": ["In", "den", "Him\u00b7mel", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "In die Himmelsbl\u00e4ue durch Bl\u00e4ttergr\u00fcn,", "tokens": ["In", "die", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", "durch", "Bl\u00e4t\u00b7ter\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In die klare, stille, unendliche Welt", "tokens": ["In", "die", "kla\u00b7re", ",", "stil\u00b7le", ",", "un\u00b7end\u00b7li\u00b7che", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Der leuchtenden Luft.", "tokens": ["Der", "leuch\u00b7ten\u00b7den", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie im M\u00e4rchen, gebannt", "tokens": ["Wie", "im", "M\u00e4r\u00b7chen", ",", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPRART", "NN", "$,", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Zu schweigendem Schlaf,", "tokens": ["Zu", "schwei\u00b7gen\u00b7dem", "Schlaf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Starr stehen die B\u00e4ume.", "tokens": ["Starr", "ste\u00b7hen", "die", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Kein Wipfel rauscht,", "tokens": ["Kein", "Wip\u00b7fel", "rauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Es schaukelt kein Blatt,", "tokens": ["Es", "schau\u00b7kelt", "kein", "Blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Kein Vogel h\u00fcpft", "tokens": ["Kein", "Vo\u00b7gel", "h\u00fcpft"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Von Zweig zu Zweig,", "tokens": ["Von", "Zweig", "zu", "Zweig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Von keinem Zweige", "tokens": ["Von", "kei\u00b7nem", "Zwei\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Klingt Vogelgesang.", "tokens": ["Klingt", "Vo\u00b7gel\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Dem sch\u00f6nheitsoffenen Auge allein", "tokens": ["Dem", "sch\u00f6n\u00b7heits\u00b7of\u00b7fe\u00b7nen", "Au\u00b7ge", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geh\u00f6rt diese stumme, lebendige Welt.", "tokens": ["Ge\u00b7h\u00f6rt", "die\u00b7se", "stum\u00b7me", ",", "le\u00b7ben\u00b7di\u00b7ge", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Himmels Blau,", "tokens": ["Des", "Him\u00b7mels", "Blau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Bl\u00e4tter Gr\u00fcn,", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der St\u00e4mme und Aeste Schwarz-Grau-Braun:", "tokens": ["Der", "St\u00e4m\u00b7me", "und", "A\u00b7es\u00b7te", "Schwa\u00b7rz\u00b7Grau\u00b7Braun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-++", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Sie leuchten ein Lied in den lauschenden Blick,", "tokens": ["Sie", "leuch\u00b7ten", "ein", "Lied", "in", "den", "lau\u00b7schen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wohl lautlos, still, doch voll Harmonie", "tokens": ["Wohl", "laut\u00b7los", ",", "still", ",", "doch", "voll", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und lebenden Gl\u00fcckes voll, das fest", "tokens": ["Und", "le\u00b7ben\u00b7den", "Gl\u00fc\u00b7ckes", "voll", ",", "das", "fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD", "$,", "PRELS", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Im Herzen haftet, wie ein Gesang,", "tokens": ["Im", "Her\u00b7zen", "haf\u00b7tet", ",", "wie", "ein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Der leise sp\u00e4ter aus Herzensgrund", "tokens": ["Der", "lei\u00b7se", "sp\u00e4\u00b7ter", "aus", "Her\u00b7zens\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Erinnerungsmelodien herauf", "tokens": ["E\u00b7rin\u00b7ne\u00b7rungs\u00b7me\u00b7lo\u00b7di\u00b7en", "her\u00b7auf"], "token_info": ["word", "word"], "pos": ["NE", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In flatterndem Schwellen erklingen l\u00e4\u00dft.", "tokens": ["In", "flat\u00b7tern\u00b7dem", "Schwel\u00b7len", "er\u00b7klin\u00b7gen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Du sinnst und fragst: Wo kamen sie her?", "tokens": ["Du", "sinnst", "und", "fragst", ":", "Wo", "ka\u00b7men", "sie", "her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wo klangen sie einst sich", "tokens": ["Wo", "klan\u00b7gen", "sie", "einst", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ins Herz mir ein?", "tokens": ["Ins", "Herz", "mir", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und lauschst dem Lied aus der eigenen Brust,", "tokens": ["Und", "lauschst", "dem", "Lied", "aus", "der", "ei\u00b7ge\u00b7nen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und tauchst hinab in des Gl\u00fcckes Tiefen,", "tokens": ["Und", "tauchst", "hin\u00b7ab", "in", "des", "Gl\u00fc\u00b7ckes", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aus denen geheimnisd\u00e4mmerweich", "tokens": ["Aus", "de\u00b7nen", "ge\u00b7heim\u00b7nis\u00b7d\u00e4m\u00b7mer\u00b7weich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der s\u00fc\u00dfen T\u00f6ne Erinnerung quillt ...", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7ne", "E\u00b7rin\u00b7ne\u00b7rung", "quillt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wo klang so voll und zart in Eins", "tokens": ["Wo", "klang", "so", "voll", "und", "zart", "in", "Eins"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Himmelsblau,", "tokens": ["Das", "Him\u00b7mels\u00b7blau", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das Bl\u00e4ttergr\u00fcn,", "tokens": ["Das", "Bl\u00e4t\u00b7ter\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Von wechselndem Grau dumpf untert\u00f6nt?", "tokens": ["Von", "wech\u00b7seln\u00b7dem", "Grau", "dumpf", "un\u00b7ter\u00b7t\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die stumme, leuchtende Melodie", "tokens": ["Die", "stum\u00b7me", ",", "leuch\u00b7ten\u00b7de", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dr\u00e4ngt tief ins Herz:", "tokens": ["Dr\u00e4ngt", "tief", "ins", "Herz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich f\u00fchle, einst", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "einst"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Klingt sie herauf", "tokens": ["Klingt", "sie", "her\u00b7auf"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In farbenleerer, dunkler Zeit.", "tokens": ["In", "far\u00b7ben\u00b7lee\u00b7rer", ",", "dunk\u00b7ler", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Auge, trinke, trinke die t\u00f6nende, leuchtende Flut,", "tokens": ["Mein", "Au\u00b7ge", ",", "trin\u00b7ke", ",", "trin\u00b7ke", "die", "t\u00f6\u00b7nen\u00b7de", ",", "leuch\u00b7ten\u00b7de", "Flut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sauge, sauge sie ein, oh Herz,", "tokens": ["Sau\u00b7ge", ",", "sau\u00b7ge", "sie", "ein", ",", "oh", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Waffne, r\u00fcste mit Sch\u00f6nheit dich", "tokens": ["Waff\u00b7ne", ",", "r\u00fcs\u00b7te", "mit", "Sch\u00f6n\u00b7heit", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Gegen die Finsternis!", "tokens": ["Ge\u00b7gen", "die", "Fins\u00b7ter\u00b7nis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {