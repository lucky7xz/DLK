{"dta.poem.11327": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Hochzeit-Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beld, Mars und Venus sind die helden dieser erden,", "tokens": ["Beld", ",", "Mars", "und", "Ve\u00b7nus", "sind", "die", "hel\u00b7den", "die\u00b7ser", "er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen jeder leicht kan \u00fcberwunden werden:", "tokens": ["Von", "de\u00b7nen", "je\u00b7der", "leicht", "kan", "\u00fc\u00b7berw\u00b7un\u00b7den", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADJD", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur ist der unterscheid, da\u00df geld uns offt betriegt,", "tokens": ["Nur", "ist", "der", "un\u00b7ter\u00b7scheid", ",", "da\u00df", "geld", "uns", "offt", "be\u00b7triegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mars ohne zorn nicht k\u00e4mpfft, und Venus k\u00fcssend siegt.", "tokens": ["Mars", "oh\u00b7ne", "zorn", "nicht", "k\u00e4mpfft", ",", "und", "Ve\u00b7nus", "k\u00fcs\u00b7send", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKNEG", "VVFIN", "$,", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber thut die welt? Man sieht den meisten hauffen", "tokens": ["Was", "a\u00b7ber", "thut", "die", "welt", "?", "Man", "sieht", "den", "meis\u00b7ten", "hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "$.", "PIS", "VVFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus geldsucht in den krieg, durch krieg nach gelde lauffen.", "tokens": ["Aus", "geld\u00b7sucht", "in", "den", "krieg", ",", "durch", "krieg", "nach", "gel\u00b7de", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir steigen auf ein holtz, durchschwimmen fluth und meer,", "tokens": ["Wir", "stei\u00b7gen", "auf", "ein", "holtz", ",", "durch\u00b7schwim\u00b7men", "fluth", "und", "meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und holen fremdes gut aus weiten l\u00e4ndern her.", "tokens": ["Und", "ho\u00b7len", "frem\u00b7des", "gut", "aus", "wei\u00b7ten", "l\u00e4n\u00b7dern", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "ADJD", "APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Warum? Es bringet geld. Geld, das den nachbar treibet,", "tokens": ["Wa\u00b7rum", "?", "Es", "brin\u00b7get", "geld", ".", "Geld", ",", "das", "den", "nach\u00b7bar", "trei\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "NN", "$.", "NN", "$,", "PRELS", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df er, eh man es denckt, an unsre haut sich reibet,", "tokens": ["Da\u00df", "er", ",", "eh", "man", "es", "denckt", ",", "an", "uns\u00b7re", "haut", "sich", "rei\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und unser gut besucht. Denn geht das streiten an,", "tokens": ["Und", "un\u00b7ser", "gut", "be\u00b7sucht", ".", "Denn", "geht", "das", "strei\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "VVPP", "$.", "KON", "VVFIN", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein jeder denckt und meint, er habe recht gethan.", "tokens": ["Ein", "je\u00b7der", "denckt", "und", "meint", ",", "er", "ha\u00b7be", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch wenn der lange zanck den reichthum aufgefressen,", "tokens": ["Doch", "wenn", "der", "lan\u00b7ge", "zanck", "den", "reicht\u00b7hum", "auf\u00b7ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So sieht man allererst, wie weit man sich vergessen,", "tokens": ["So", "sieht", "man", "al\u00b7le\u00b7rerst", ",", "wie", "weit", "man", "sich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PWAV", "ADJD", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und von der bahn verirrt. Hingegen lebt ein mann", "tokens": ["Und", "von", "der", "bahn", "ver\u00b7irrt", ".", "Hin\u00b7ge\u00b7gen", "lebt", "ein", "mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Recht gl\u00fccklich, der sich nur im geitze fassen kan,", "tokens": ["Recht", "gl\u00fcck\u00b7lich", ",", "der", "sich", "nur", "im", "geit\u00b7ze", "fas\u00b7sen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PRF", "ADV", "APPRART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und in der wuth begreifft. Er nimmt, was GOtt bescheret,", "tokens": ["Und", "in", "der", "wuth", "be\u00b7greifft", ".", "Er", "nimmt", ",", "was", "Gott", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und sieht, wie ehermahls sein vater sich gen\u00e4hret.", "tokens": ["Und", "sieht", ",", "wie", "e\u00b7her\u00b7mahls", "sein", "va\u00b7ter", "sich", "ge\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das ist: Er l\u00e4st den krieg f\u00fcr grosse herren seyn,", "tokens": ["Das", "ist", ":", "Er", "l\u00e4st", "den", "krieg", "f\u00fcr", "gros\u00b7se", "her\u00b7ren", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Schrenckt seine geld-begier in feste grentzen ein,", "tokens": ["Schrenckt", "sei\u00b7ne", "geld\u00b7be\u00b7gier", "in", "fes\u00b7te", "grent\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und h\u00e4ngt der liebe nach. Der liebe, will ich sagen,", "tokens": ["Und", "h\u00e4ngt", "der", "lie\u00b7be", "nach", ".", "Der", "lie\u00b7be", ",", "will", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Adam, als er schlief, schon in der brust getragen,", "tokens": ["Die", "A\u00b7dam", ",", "als", "er", "schlief", ",", "schon", "in", "der", "brust", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wachend fortgepflantzt. Der liebe, die nichts acht,", "tokens": ["Und", "wa\u00b7chend", "fort\u00b7ge\u00b7pflantzt", ".", "Der", "lie\u00b7be", ",", "die", "nichts", "acht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "ART", "ADJA", "$,", "PRELS", "PIS", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und durch verborgne gluth aus zweyen eines macht.", "tokens": ["Und", "durch", "ver\u00b7borg\u00b7ne", "gluth", "aus", "zwe\u00b7yen", "ei\u00b7nes", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "VVFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein solcher zittert nicht, wenn sich das gl\u00fccke lencket,", "tokens": ["Ein", "sol\u00b7cher", "zit\u00b7tert", "nicht", ",", "wenn", "sich", "das", "gl\u00fc\u00b7cke", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PTKNEG", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ihn durch feind und feur an seiner nahrung kr\u00e4ncket.", "tokens": ["Und", "ihn", "durch", "feind", "und", "feur", "an", "sei\u00b7ner", "nah\u00b7rung", "kr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er kennet keinen schatz, als seiner liebsten hertz;", "tokens": ["Er", "ken\u00b7net", "kei\u00b7nen", "schatz", ",", "als", "sei\u00b7ner", "liebs\u00b7ten", "hertz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Beh\u00e4lt er dieses nur, so giebt er ohne schmertz", "tokens": ["Be\u00b7h\u00e4lt", "er", "die\u00b7ses", "nur", ",", "so", "giebt", "er", "oh\u00b7ne", "schmertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den gantzen reichthum hin. Was ich so hoch gepriesen,", "tokens": ["Den", "gant\u00b7zen", "reicht\u00b7hum", "hin", ".", "Was", "ich", "so", "hoch", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PWS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Hat er, herr br\u00e4utigam! uns allen heut erwiesen,", "tokens": ["Hat", "er", ",", "herr", "br\u00e4u\u00b7ti\u00b7gam", "!", "uns", "al\u00b7len", "heut", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "NE", "$.", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da er bey dieser zeit, die nichts als kriege zeigt,", "tokens": ["Da", "er", "bey", "die\u00b7ser", "zeit", ",", "die", "nichts", "als", "krie\u00b7ge", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "$,", "PRELS", "PIS", "KOKOM", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da jeder wucher treibt, ins friedens-lager steigt,", "tokens": ["Da", "je\u00b7der", "wu\u00b7cher", "treibt", ",", "ins", "frie\u00b7dens\u00b7la\u00b7ger", "steigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "VVFIN", "$,", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und seine g\u00fcter theilt. Nun lehrt er, da\u00df er wisse,", "tokens": ["Und", "sei\u00b7ne", "g\u00fc\u00b7ter", "theilt", ".", "Nun", "lehrt", "er", ",", "da\u00df", "er", "wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was rechter handel sey. Durch tausend \u00e4rgernisse,", "tokens": ["Was", "rech\u00b7ter", "han\u00b7del", "sey", ".", "Durch", "tau\u00b7send", "\u00e4r\u00b7ger\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$.", "APPR", "CARD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Durch vortheil und betrug den n\u00e4chsten hintergehn,", "tokens": ["Durch", "vor\u00b7theil", "und", "be\u00b7trug", "den", "n\u00e4chs\u00b7ten", "hin\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sind dinge, die nunmehr auch kinder schon verstehn.", "tokens": ["Sind", "din\u00b7ge", ",", "die", "nun\u00b7mehr", "auch", "kin\u00b7der", "schon", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADV", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Vor alters wuste man von solchem kunst-verkehren", "tokens": ["Vor", "al\u00b7ters", "wus\u00b7te", "man", "von", "sol\u00b7chem", "kunst\u00b7ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So wenig, als ein baur, wie sich die schlangen n\u00e4hren,", "tokens": ["So", "we\u00b7nig", ",", "als", "ein", "baur", ",", "wie", "sich", "die", "schlan\u00b7gen", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KOUS", "ART", "NN", "$,", "PWAV", "PRF", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die man in Java sieht. Recht hie\u00df damahls gewinn,", "tokens": ["Die", "man", "in", "Ja\u00b7va", "sieht", ".", "Recht", "hie\u00df", "da\u00b7mahls", "ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "$.", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Man gab ein st\u00fccke brod f\u00fcr einen braten hin,", "tokens": ["Man", "gab", "ein", "st\u00fc\u00b7cke", "brod", "f\u00fcr", "ei\u00b7nen", "bra\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und nahm ein fettes huhn f\u00fcr garn zu einem kleide.", "tokens": ["Und", "nahm", "ein", "fet\u00b7tes", "huhn", "f\u00fcr", "garn", "zu", "ei\u00b7nem", "klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein weib war dazumahl, zu vieler m\u00e4nner freude,", "tokens": ["Ein", "weib", "war", "da\u00b7zu\u00b7mahl", ",", "zu", "vie\u00b7ler", "m\u00e4n\u00b7ner", "freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nicht gegen geld erkaufft, nicht wegen geld erfreyt;", "tokens": ["Nicht", "ge\u00b7gen", "geld", "er\u00b7kaufft", ",", "nicht", "we\u00b7gen", "geld", "er\u00b7freyt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "$,", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man zahlte hertz mit hertz, und treu mit redlichkeit,", "tokens": ["Man", "zahl\u00b7te", "hertz", "mit", "hertz", ",", "und", "treu", "mit", "red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "APPR", "NN", "$,", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und kriegte dennoch wohl mehr, als man offt vermeinte.", "tokens": ["Und", "krieg\u00b7te", "den\u00b7noch", "wohl", "mehr", ",", "als", "man", "offt", "ver\u00b7mein\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ach w\u00e4r es heute noch! Jedoch es ist ja heunte,", "tokens": ["Ach", "w\u00e4r", "es", "heu\u00b7te", "noch", "!", "Je\u00b7doch", "es", "ist", "ja", "heun\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ADV", "$.", "ADV", "PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Da er, herr br\u00e4utigam! den nutz bey seite stellt,", "tokens": ["Da", "er", ",", "herr", "br\u00e4u\u00b7ti\u00b7gam", "!", "den", "nutz", "bey", "sei\u00b7te", "stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "NE", "$.", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Gut gegen gut ertauscht, und ohne list und geld", "tokens": ["Gut", "ge\u00b7gen", "gut", "er\u00b7tauscht", ",", "und", "oh\u00b7ne", "list", "und", "geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJD", "VVPP", "$,", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Den gr\u00f6sten handel thut; l\u00e4\u00dft andre sich bem\u00fchen,", "tokens": ["Den", "gr\u00f6s\u00b7ten", "han\u00b7del", "thut", ";", "l\u00e4\u00dft", "and\u00b7re", "sich", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VVFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und um ein st\u00fccke gold bi\u00df an den Ganges ziehen.", "tokens": ["Und", "um", "ein", "st\u00fc\u00b7cke", "gold", "bi\u00df", "an", "den", "Gan\u00b7ges", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Er rei\u00dft auf einmal sich von allen sorgen los,", "tokens": ["Er", "rei\u00dft", "auf", "ein\u00b7mal", "sich", "von", "al\u00b7len", "sor\u00b7gen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und findet, was er will, in seiner liebsten schos.", "tokens": ["Und", "fin\u00b7det", ",", "was", "er", "will", ",", "in", "sei\u00b7ner", "liebs\u00b7ten", "schos", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Sie bringt ihm sch\u00f6nheit zu, er zahlet sie mit lieben:", "tokens": ["Sie", "bringt", "ihm", "sch\u00f6n\u00b7heit", "zu", ",", "er", "zah\u00b7let", "sie", "mit", "lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ach himmel! la\u00df doch nichts di\u00df edle paar betr\u00fcben!", "tokens": ["Ach", "him\u00b7mel", "!", "la\u00df", "doch", "nichts", "di\u00df", "ed\u00b7le", "paar", "be\u00b7tr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVIMP", "ADV", "PIS", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ach, Amor! bringe doch die schmertzen und die pein,", "tokens": ["Ach", ",", "A\u00b7mor", "!", "brin\u00b7ge", "doch", "die", "schmert\u00b7zen", "und", "die", "pein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "VVFIN", "ADV", "ART", "ADJA", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die du bi\u00dfher erweckt, mit s\u00fcssem wucher ein!", "tokens": ["Die", "du", "bi\u00df\u00b7her", "er\u00b7weckt", ",", "mit", "s\u00fcs\u00b7sem", "wu\u00b7cher", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}