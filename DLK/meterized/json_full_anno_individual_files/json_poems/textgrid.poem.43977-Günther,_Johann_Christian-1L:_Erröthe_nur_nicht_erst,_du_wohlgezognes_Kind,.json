{"textgrid.poem.43977": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Err\u00f6the nur nicht erst, du wohlgezognes Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Err\u00f6the nur nicht erst, du wohlgezognes Kind,", "tokens": ["Er\u00b7r\u00f6\u00b7the", "nur", "nicht", "erst", ",", "du", "wohl\u00b7ge\u00b7zog\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn jezo Mund und Kiel aus Liebe k\u00fchner sind", "tokens": ["Wenn", "je\u00b7zo", "Mund", "und", "Kiel", "aus", "Lie\u00b7be", "k\u00fch\u00b7ner", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NE", "APPR", "NN", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, da dein Wesen mir bereits das Herz genommen,", "tokens": ["Und", ",", "da", "dein", "We\u00b7sen", "mir", "be\u00b7reits", "das", "Herz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Ernst und Redligkeit nach deinem Herzen kommen.", "tokens": ["Mit", "Ernst", "und", "Red\u00b7lig\u00b7keit", "nach", "dei\u00b7nem", "Her\u00b7zen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist kein blinder Schlu\u00df noch leichter Eigensinn;", "tokens": ["Es", "ist", "kein", "blin\u00b7der", "Schlu\u00df", "noch", "leich\u00b7ter", "Ei\u00b7gen\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Himmel f\u00fchrt mich selbst zu deiner Tugend hin", "tokens": ["Der", "Him\u00b7mel", "f\u00fchrt", "mich", "selbst", "zu", "dei\u00b7ner", "Tu\u00b7gend", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und bringt uns auf der Welt kaum einmahl recht zusammen,", "tokens": ["Und", "bringt", "uns", "auf", "der", "Welt", "kaum", "ein\u00b7mahl", "recht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So f\u00fchl ich alsobald die rein- und edlen Flammen", "tokens": ["So", "f\u00fchl", "ich", "al\u00b7so\u00b7bald", "die", "rein", "und", "ed\u00b7len", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Liebe gegen dich, die ohne Falschheit brennt", "tokens": ["Der", "Lie\u00b7be", "ge\u00b7gen", "dich", ",", "die", "oh\u00b7ne", "Falschheit", "brennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und jedem auf der Welt das gr\u00f6ste Gl\u00fccke g\u00f6nnt,", "tokens": ["Und", "je\u00b7dem", "auf", "der", "Welt", "das", "gr\u00f6s\u00b7te", "Gl\u00fc\u00b7cke", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn mich nur Gott und Zeit bald so geneigt bedencken", "tokens": ["Wenn", "mich", "nur", "Gott", "und", "Zeit", "bald", "so", "ge\u00b7neigt", "be\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und meiner treuen Brust dein Herz zum Lohne schencken,", "tokens": ["Und", "mei\u00b7ner", "treu\u00b7en", "Brust", "dein", "Herz", "zum", "Loh\u00b7ne", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zum Lohne vor den Flei\u00df und vor so manche Nacht,", "tokens": ["Zum", "Loh\u00b7ne", "vor", "den", "Flei\u00df", "und", "vor", "so", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "KON", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die mein Studiren oft mit Wachen zugebracht,", "tokens": ["Die", "mein", "Stu\u00b7di\u00b7ren", "oft", "mit", "Wa\u00b7chen", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Um dermahleins an Kunst und Wi\u00dfenschaft zu gr\u00fcnen", "tokens": ["Um", "der\u00b7mah\u00b7leins", "an", "Kunst", "und", "Wi\u00b7\u00dfen\u00b7schaft", "zu", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und als ein n\u00fczlich Glied der Republic zu dienen.", "tokens": ["Und", "als", "ein", "n\u00fcz\u00b7lich", "Glied", "der", "Re\u00b7pub\u00b7lic", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der, so im Himmel wohnt und ins Verborgne sieht,", "tokens": ["Der", ",", "so", "im", "Him\u00b7mel", "wohnt", "und", "ins", "Ver\u00b7borg\u00b7ne", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "VVFIN", "KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mag selber Zeuge seyn, wie starck mein Eifer gl\u00fcht,", "tokens": ["Mag", "sel\u00b7ber", "Zeu\u00b7ge", "seyn", ",", "wie", "starck", "mein", "Ei\u00b7fer", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VAINF", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein gleichgesinntes Herz und treues Weib zu finden,", "tokens": ["Ein", "gleich\u00b7ge\u00b7sinn\u00b7tes", "Herz", "und", "treu\u00b7es", "Weib", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey der sich Tugend, Wiz und Z\u00e4rtligkeit verbinden.", "tokens": ["Bey", "der", "sich", "Tu\u00b7gend", ",", "Wiz", "und", "Z\u00e4rt\u00b7lig\u00b7keit", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So weit nun mein Verstand Gem\u00fcther pr\u00fcfen kan,", "tokens": ["So", "weit", "nun", "mein", "Ver\u00b7stand", "Ge\u00b7m\u00fc\u00b7ther", "pr\u00fc\u00b7fen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So freudig seh ich dich vor meines gleichen an", "tokens": ["So", "freu\u00b7dig", "seh", "ich", "dich", "vor", "mei\u00b7nes", "glei\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und finde, wie mich d\u00fcnckt, an deinen edlen Gaben", "tokens": ["Und", "fin\u00b7de", ",", "wie", "mich", "d\u00fcnckt", ",", "an", "dei\u00b7nen", "ed\u00b7len", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was mehr als insgemein des Landes T\u00f6chter haben,", "tokens": ["Was", "mehr", "als", "ins\u00b7ge\u00b7mein", "des", "Lan\u00b7des", "T\u00f6ch\u00b7ter", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "KOKOM", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und darum hoft mein Geist, wofern er dich erh\u00e4lt,", "tokens": ["Und", "da\u00b7rum", "hoft", "mein", "Geist", ",", "wo\u00b7fern", "er", "dich", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In recht vergn\u00fcgter Eh den Himmel auf der Welt.", "tokens": ["In", "recht", "ver\u00b7gn\u00fcg\u00b7ter", "Eh", "den", "Him\u00b7mel", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Eintracht soll bey uns in Bett und Tische lachen", "tokens": ["Die", "Ein\u00b7tracht", "soll", "bey", "uns", "in", "Bett", "und", "Ti\u00b7sche", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und unsern Lebenslauf voll g\u00fcldner Stunden machen.", "tokens": ["Und", "un\u00b7sern", "Le\u00b7bens\u00b7lauf", "voll", "g\u00fcld\u00b7ner", "Stun\u00b7den", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich r\u00fchme nichts von mir als unverf\u00e4lschte Treu", "tokens": ["Ich", "r\u00fch\u00b7me", "nichts", "von", "mir", "als", "un\u00b7ver\u00b7f\u00e4lschte", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Und stelle dir hiermit die Wahl in Demuth frey:", "tokens": ["Und", "stel\u00b7le", "dir", "hier\u00b7mit", "die", "Wahl", "in", "De\u00b7muth", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Getraustu dich, mit mir vergn\u00fcgt und wohl zu leben,", "tokens": ["Ge\u00b7traus\u00b7tu", "dich", ",", "mit", "mir", "ver\u00b7gn\u00fcgt", "und", "wohl", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PPER", "VVPP", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So s\u00e4ume l\u00e4nger nicht, dein Herz an Tag zu geben.", "tokens": ["So", "s\u00e4u\u00b7me", "l\u00e4n\u00b7ger", "nicht", ",", "dein", "Herz", "an", "Tag", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PTKNEG", "$,", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein Vater, de\u00dfen Geist und Klugheit und Verstand", "tokens": ["Dein", "Va\u00b7ter", ",", "de\u00b7\u00dfen", "Geist", "und", "Klug\u00b7heit", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich nur die kurze Zeit zur Gn\u00fcge schon erkand,", "tokens": ["Ich", "nur", "die", "kur\u00b7ze", "Zeit", "zur", "Gn\u00fc\u00b7ge", "schon", "er\u00b7kand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wird schon so g\u00fctig seyn und unter Wuntsch und Seegen", "tokens": ["Wird", "schon", "so", "g\u00fc\u00b7tig", "seyn", "und", "un\u00b7ter", "Wunt\u00b7sch", "und", "See\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VAINF", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Sein Jawort nebst der Hand auf unser B\u00fcndn\u00fc\u00df legen,", "tokens": ["Sein", "Ja\u00b7wort", "nebst", "der", "Hand", "auf", "un\u00b7ser", "B\u00fcnd\u00b7n\u00fc\u00df", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das blos vom Himmel kommt. Ich nenne dich schon mein,", "tokens": ["Das", "blos", "vom", "Him\u00b7mel", "kommt", ".", "Ich", "nen\u00b7ne", "dich", "schon", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und du kanst gegentheils gewis versichert seyn,", "tokens": ["Und", "du", "kanst", "ge\u00b7gen\u00b7theils", "ge\u00b7wis", "ver\u00b7si\u00b7chert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Da\u00df, ob ich mich gleich nicht mit Blute hoch verschw\u00f6re,", "tokens": ["Da\u00df", ",", "ob", "ich", "mich", "gleich", "nicht", "mit", "Blu\u00b7te", "hoch", "ver\u00b7schw\u00f6\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich dennoch mit Vernunft mich blos vor dein erkl\u00e4re.", "tokens": ["Ich", "den\u00b7noch", "mit", "Ver\u00b7nunft", "mich", "blos", "vor", "dein", "er\u00b7kl\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PPER", "ADV", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Allmacht seegne dich in deines Vaters Haus", "tokens": ["Die", "All\u00b7macht", "seeg\u00b7ne", "dich", "in", "dei\u00b7nes", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und f\u00fchre dich zu mir mit Wuntsch und Heil heraus", "tokens": ["Und", "f\u00fch\u00b7re", "dich", "zu", "mir", "mit", "Wunt\u00b7sch", "und", "Heil", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+--+---+-+-+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Und cr\u00f6ne den Beruf, worin er mich gesezet,", "tokens": ["Und", "cr\u00f6\u00b7ne", "den", "Be\u00b7ruf", ",", "wo\u00b7rin", "er", "mich", "ge\u00b7se\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mit allem, was ein Mensch vor gut und gl\u00fccklich sch\u00e4zet.", "tokens": ["Mit", "al\u00b7lem", ",", "was", "ein", "Mensch", "vor", "gut", "und", "gl\u00fcck\u00b7lich", "sch\u00e4\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du aber, werthes Kind, sey immer unbetr\u00fcbt", "tokens": ["Du", "a\u00b7ber", ",", "wert\u00b7hes", "Kind", ",", "sey", "im\u00b7mer", "un\u00b7be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und glaube, da\u00df der Herr, der fromme Seelen liebt,", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "der", "Herr", ",", "der", "from\u00b7me", "See\u00b7len", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Uns als ein treues Paar auch hier noch auf der Erde", "tokens": ["Uns", "als", "ein", "treu\u00b7es", "Paar", "auch", "hier", "noch", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KOUS", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Den Neidern zum Verdru\u00df mit Wollust tr\u00e4ncken werde.", "tokens": ["Den", "Nei\u00b7dern", "zum", "Ver\u00b7dru\u00df", "mit", "Wol\u00b7lust", "tr\u00e4n\u00b7cken", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Was wiltu doch wohl mehr in dieser eitlen Welt,", "tokens": ["Was", "wil\u00b7tu", "doch", "wohl", "mehr", "in", "die\u00b7ser", "eit\u00b7len", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wo Creuz und Unbestand das B\u00fcrgerrecht beh\u00e4lt,", "tokens": ["Wo", "Creuz", "und", "Un\u00b7be\u00b7stand", "das", "B\u00fcr\u00b7ger\u00b7recht", "be\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Was wiltu, sag ich, mehr in dieser Welt erwerben,", "tokens": ["Was", "wil\u00b7tu", ",", "sag", "ich", ",", "mehr", "in", "die\u00b7ser", "Welt", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Als blos mit mir vergn\u00fcgt zu leben und zu sterben?", "tokens": ["Als", "blos", "mit", "mir", "ver\u00b7gn\u00fcgt", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "VVPP", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Err\u00f6the nur nicht erst, du wohlgezognes Kind,", "tokens": ["Er\u00b7r\u00f6\u00b7the", "nur", "nicht", "erst", ",", "du", "wohl\u00b7ge\u00b7zog\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn jezo Mund und Kiel aus Liebe k\u00fchner sind", "tokens": ["Wenn", "je\u00b7zo", "Mund", "und", "Kiel", "aus", "Lie\u00b7be", "k\u00fch\u00b7ner", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NE", "APPR", "NN", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, da dein Wesen mir bereits das Herz genommen,", "tokens": ["Und", ",", "da", "dein", "We\u00b7sen", "mir", "be\u00b7reits", "das", "Herz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Ernst und Redligkeit nach deinem Herzen kommen.", "tokens": ["Mit", "Ernst", "und", "Red\u00b7lig\u00b7keit", "nach", "dei\u00b7nem", "Her\u00b7zen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist kein blinder Schlu\u00df noch leichter Eigensinn;", "tokens": ["Es", "ist", "kein", "blin\u00b7der", "Schlu\u00df", "noch", "leich\u00b7ter", "Ei\u00b7gen\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Himmel f\u00fchrt mich selbst zu deiner Tugend hin", "tokens": ["Der", "Him\u00b7mel", "f\u00fchrt", "mich", "selbst", "zu", "dei\u00b7ner", "Tu\u00b7gend", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und bringt uns auf der Welt kaum einmahl recht zusammen,", "tokens": ["Und", "bringt", "uns", "auf", "der", "Welt", "kaum", "ein\u00b7mahl", "recht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So f\u00fchl ich alsobald die rein- und edlen Flammen", "tokens": ["So", "f\u00fchl", "ich", "al\u00b7so\u00b7bald", "die", "rein", "und", "ed\u00b7len", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Liebe gegen dich, die ohne Falschheit brennt", "tokens": ["Der", "Lie\u00b7be", "ge\u00b7gen", "dich", ",", "die", "oh\u00b7ne", "Falschheit", "brennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und jedem auf der Welt das gr\u00f6ste Gl\u00fccke g\u00f6nnt,", "tokens": ["Und", "je\u00b7dem", "auf", "der", "Welt", "das", "gr\u00f6s\u00b7te", "Gl\u00fc\u00b7cke", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn mich nur Gott und Zeit bald so geneigt bedencken", "tokens": ["Wenn", "mich", "nur", "Gott", "und", "Zeit", "bald", "so", "ge\u00b7neigt", "be\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und meiner treuen Brust dein Herz zum Lohne schencken,", "tokens": ["Und", "mei\u00b7ner", "treu\u00b7en", "Brust", "dein", "Herz", "zum", "Loh\u00b7ne", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zum Lohne vor den Flei\u00df und vor so manche Nacht,", "tokens": ["Zum", "Loh\u00b7ne", "vor", "den", "Flei\u00df", "und", "vor", "so", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "KON", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die mein Studiren oft mit Wachen zugebracht,", "tokens": ["Die", "mein", "Stu\u00b7di\u00b7ren", "oft", "mit", "Wa\u00b7chen", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Um dermahleins an Kunst und Wi\u00dfenschaft zu gr\u00fcnen", "tokens": ["Um", "der\u00b7mah\u00b7leins", "an", "Kunst", "und", "Wi\u00b7\u00dfen\u00b7schaft", "zu", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und als ein n\u00fczlich Glied der Republic zu dienen.", "tokens": ["Und", "als", "ein", "n\u00fcz\u00b7lich", "Glied", "der", "Re\u00b7pub\u00b7lic", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der, so im Himmel wohnt und ins Verborgne sieht,", "tokens": ["Der", ",", "so", "im", "Him\u00b7mel", "wohnt", "und", "ins", "Ver\u00b7borg\u00b7ne", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "VVFIN", "KON", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mag selber Zeuge seyn, wie starck mein Eifer gl\u00fcht,", "tokens": ["Mag", "sel\u00b7ber", "Zeu\u00b7ge", "seyn", ",", "wie", "starck", "mein", "Ei\u00b7fer", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VAINF", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein gleichgesinntes Herz und treues Weib zu finden,", "tokens": ["Ein", "gleich\u00b7ge\u00b7sinn\u00b7tes", "Herz", "und", "treu\u00b7es", "Weib", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey der sich Tugend, Wiz und Z\u00e4rtligkeit verbinden.", "tokens": ["Bey", "der", "sich", "Tu\u00b7gend", ",", "Wiz", "und", "Z\u00e4rt\u00b7lig\u00b7keit", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So weit nun mein Verstand Gem\u00fcther pr\u00fcfen kan,", "tokens": ["So", "weit", "nun", "mein", "Ver\u00b7stand", "Ge\u00b7m\u00fc\u00b7ther", "pr\u00fc\u00b7fen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So freudig seh ich dich vor meines gleichen an", "tokens": ["So", "freu\u00b7dig", "seh", "ich", "dich", "vor", "mei\u00b7nes", "glei\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und finde, wie mich d\u00fcnckt, an deinen edlen Gaben", "tokens": ["Und", "fin\u00b7de", ",", "wie", "mich", "d\u00fcnckt", ",", "an", "dei\u00b7nen", "ed\u00b7len", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was mehr als insgemein des Landes T\u00f6chter haben,", "tokens": ["Was", "mehr", "als", "ins\u00b7ge\u00b7mein", "des", "Lan\u00b7des", "T\u00f6ch\u00b7ter", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "KOKOM", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und darum hoft mein Geist, wofern er dich erh\u00e4lt,", "tokens": ["Und", "da\u00b7rum", "hoft", "mein", "Geist", ",", "wo\u00b7fern", "er", "dich", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In recht vergn\u00fcgter Eh den Himmel auf der Welt.", "tokens": ["In", "recht", "ver\u00b7gn\u00fcg\u00b7ter", "Eh", "den", "Him\u00b7mel", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Eintracht soll bey uns in Bett und Tische lachen", "tokens": ["Die", "Ein\u00b7tracht", "soll", "bey", "uns", "in", "Bett", "und", "Ti\u00b7sche", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und unsern Lebenslauf voll g\u00fcldner Stunden machen.", "tokens": ["Und", "un\u00b7sern", "Le\u00b7bens\u00b7lauf", "voll", "g\u00fcld\u00b7ner", "Stun\u00b7den", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich r\u00fchme nichts von mir als unverf\u00e4lschte Treu", "tokens": ["Ich", "r\u00fch\u00b7me", "nichts", "von", "mir", "als", "un\u00b7ver\u00b7f\u00e4lschte", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Und stelle dir hiermit die Wahl in Demuth frey:", "tokens": ["Und", "stel\u00b7le", "dir", "hier\u00b7mit", "die", "Wahl", "in", "De\u00b7muth", "frey", ":"], "token_info": ["word", "word", "word", "word", "word