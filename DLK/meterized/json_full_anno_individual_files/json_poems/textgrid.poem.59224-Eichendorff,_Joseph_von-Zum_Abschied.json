{"textgrid.poem.59224": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zum Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbstwind sch\u00fcttelt die Linde,", "tokens": ["Der", "Herbst\u00b7wind", "sch\u00fct\u00b7telt", "die", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie geht die Welt so geschwinde!", "tokens": ["Wie", "geht", "die", "Welt", "so", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Halte dein Kindlein warm.", "tokens": ["Hal\u00b7te", "dein", "Kin\u00b7dlein", "warm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Sommer ist hingefahren,", "tokens": ["Der", "Som\u00b7mer", "ist", "hin\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da wir zusammen waren \u2013", "tokens": ["Da", "wir", "zu\u00b7sam\u00b7men", "wa\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, die sich lieben, wie arm!", "tokens": ["Ach", ",", "die", "sich", "lie\u00b7ben", ",", "wie", "arm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PRF", "VVINF", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wie arm, die sich lieben und scheiden!", "tokens": ["Wie", "arm", ",", "die", "sich", "lie\u00b7ben", "und", "schei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PRELS", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das haben erfahren wir beiden,", "tokens": ["Das", "ha\u00b7ben", "er\u00b7fah\u00b7ren", "wir", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "PPER", "PIAT", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mir graut vor dem stillen Haus.", "tokens": ["Mir", "graut", "vor", "dem", "stil\u00b7len", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein T\u00fcchlein noch l\u00e4\u00dft du wehen,", "tokens": ["Dein", "T\u00fcch\u00b7lein", "noch", "l\u00e4\u00dft", "du", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Ich kann's vor Tr\u00e4nen kaum sehen,", "tokens": ["Ich", "kann's", "vor", "Tr\u00e4\u00b7nen", "kaum", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schau still in die Gasse hinaus.", "tokens": ["Schau", "still", "in", "die", "Gas\u00b7se", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Gassen schauen noch n\u00e4chtig,", "tokens": ["Die", "Gas\u00b7sen", "schau\u00b7en", "noch", "n\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es rasselt der Wagen bed\u00e4chtig \u2013", "tokens": ["Es", "ras\u00b7selt", "der", "Wa\u00b7gen", "be\u00b7d\u00e4ch\u00b7tig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nun pl\u00f6tzlich rascher der Trott", "tokens": ["Nun", "pl\u00f6tz\u00b7lich", "ra\u00b7scher", "der", "Trott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Durchs Tor in die Stille der Felder", "tokens": ["Durchs", "Tor", "in", "die", "Stil\u00b7le", "der", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da gr\u00fc\u00dfen so mutig die W\u00e4lder,", "tokens": ["Da", "gr\u00fc\u00b7\u00dfen", "so", "mu\u00b7tig", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Lieb T\u00f6chterlein, fahre mit Gott!", "tokens": ["Lieb", "T\u00f6ch\u00b7ter\u00b7lein", ",", "fah\u00b7re", "mit", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["N