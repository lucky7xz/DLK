{"textgrid.poem.43129": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Kamin lag grau die Asche,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Kamin lag grau die Asche,", "tokens": ["Im", "Ka\u00b7min", "lag", "grau", "die", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ich sa\u00df, nachdenklich sch\u00fcrend,", "tokens": ["Und", "ich", "sa\u00df", ",", "nach\u00b7denk\u00b7lich", "sch\u00fc\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem letzten tauben Reste", "tokens": ["In", "dem", "letz\u00b7ten", "tau\u00b7ben", "Res\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach verborg'nen Gluthen sp\u00fcrend.", "tokens": ["Nach", "ver\u00b7bor\u00b7g'\u00b7nen", "Glu\u00b7then", "sp\u00fc\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und es flammte aus der Asche,", "tokens": ["Und", "es", "flamm\u00b7te", "aus", "der", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder helle Funken spr\u00fchend,", "tokens": ["Wie\u00b7der", "hel\u00b7le", "Fun\u00b7ken", "spr\u00