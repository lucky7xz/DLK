{"dta.poem.12769": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das bey eben derselben erwogne  \n gl\u00fccke des Herrn Br\u00e4utigams.  \n G. Stolle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund! Mein ander Ich! dein wachsendes gel\u00fccke", "tokens": ["Mein", "Freund", "!", "Mein", "an\u00b7der", "Ich", "!", "dein", "wach\u00b7sen\u00b7des", "ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "ADJD", "PPER", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht, da\u00df ich meinen flor halb von den augen zieh:", "tokens": ["Macht", ",", "da\u00df", "ich", "mei\u00b7nen", "flor", "halb", "von", "den", "au\u00b7gen", "zieh", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "VVFIN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Da\u00df ich mein tr\u00fcbes ach auf eine zeit verdr\u00fccke,", "tokens": ["Da\u00df", "ich", "mein", "tr\u00fc\u00b7bes", "ach", "auf", "ei\u00b7ne", "zeit", "ver\u00b7dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mich bey deiner lust um einen vers bem\u00fch\u2019.", "tokens": ["Und", "mich", "bey", "dei\u00b7ner", "lust", "um", "ei\u00b7nen", "vers", "be\u00b7m\u00fch'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wird zwar nicht so rein und nett, als deiner, fliessen:", "tokens": ["Er", "wird", "zwar", "nicht", "so", "rein", "und", "nett", ",", "als", "dei\u00b7ner", ",", "flies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPOSAT", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein unstern hat in mir den alten trieb ersteckt;", "tokens": ["Mein", "uns\u00b7tern", "hat", "in", "mir", "den", "al\u00b7ten", "trieb", "er\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ART", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch weil sich Ephraim und Dorothea k\u00fcssen,", "tokens": ["Doch", "weil", "sich", "Eph\u00b7raim", "und", "Do\u00b7ro\u00b7thea", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "NE", "KON", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So wird der matte geist doch etwas aufgeweckt.", "tokens": ["So", "wird", "der", "mat\u00b7te", "geist", "doch", "et\u00b7was", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wag es dann getrost. Vielleicht fl\u00f6\u00dft deine liebe", "tokens": ["Ich", "wag", "es", "dann", "ge\u00b7trost", ".", "Viel\u00b7leicht", "fl\u00f6\u00dft", "dei\u00b7ne", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bey meiner herben noth mir noch was s\u00fc\u00dfes ein.", "tokens": ["Bey", "mei\u00b7ner", "her\u00b7ben", "noth", "mir", "noch", "was", "s\u00fc\u00b7\u00dfes", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "PWS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und rinnt die Hypoeren in meinen versen tr\u00fcbe,", "tokens": ["Und", "rinnt", "die", "Hy\u00b7po\u00b7e\u00b7ren", "in", "mei\u00b7nen", "ver\u00b7sen", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "So wird mein wille doch ohn allen tadel seyn.", "tokens": ["So", "wird", "mein", "wil\u00b7le", "doch", "ohn", "al\u00b7len", "ta\u00b7del", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du bist vor langer zeit der Weisheit nachgegangen,", "tokens": ["Du", "bist", "vor", "lan\u00b7ger", "zeit", "der", "Weis\u00b7heit", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die weder Socrates noch Antoninus zeigt.", "tokens": ["Die", "we\u00b7der", "So\u00b7cra\u00b7tes", "noch", "An\u00b7to\u00b7ni\u00b7nus", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NE", "ADV", "NE", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Der himmel lie\u00df dich auch den edlen zweck erlangen,", "tokens": ["Der", "him\u00b7mel", "lie\u00df", "dich", "auch", "den", "ed\u00b7len", "zweck", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und hat dir ihre gunst vollkommen zugeneigt.", "tokens": ["Und", "hat", "dir", "ih\u00b7re", "gunst", "voll\u00b7kom\u00b7men", "zu\u00b7gen\u00b7eigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie hat dein hertze dann in ihren port gezogen,", "tokens": ["Sie", "hat", "dein", "hert\u00b7ze", "dann", "in", "ih\u00b7ren", "port", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das wie ein schwehres schiff im meere dieser welt", "tokens": ["Das", "wie", "ein", "schweh\u00b7res", "schiff", "im", "mee\u00b7re", "die\u00b7ser", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "ADJA", "VVFIN", "APPRART", "ADJA", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bey ungest\u00fcmer lufft immittelst well und wogen,", "tokens": ["Bey", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "lufft", "im\u00b7mit\u00b7telst", "well", "und", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den vorgesteckten lauf, trotz nacht und donner! h\u00e4lt.", "tokens": ["Den", "vor\u00b7ge\u00b7steck\u00b7ten", "lauf", ",", "trotz", "nacht", "und", "don\u00b7ner", "!", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du hast das g\u00f6ldne Flie\u00df, so Jason nie erblicket,", "tokens": ["Du", "hast", "das", "g\u00f6ld\u00b7ne", "Flie\u00df", ",", "so", "Ja\u00b7son", "nie", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die ruhe des gem\u00fcths durch ihren trieb erreicht;", "tokens": ["Die", "ru\u00b7he", "des", "ge\u00b7m\u00fcths", "durch", "ih\u00b7ren", "trieb", "er\u00b7reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So fern es in der welt den sterblichen gel\u00fccket,", "tokens": ["So", "fern", "es", "in", "der", "welt", "den", "sterb\u00b7li\u00b7chen", "ge\u00b7l\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Allwo der wetter-sturm auch offt im hafen streicht.", "tokens": ["All\u00b7wo", "der", "wet\u00b7ter\u00b7sturm", "auch", "offt", "im", "ha\u00b7fen", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drum hat Saline dir der weisheit erantz geschencket:", "tokens": ["Drum", "hat", "Sa\u00b7li\u00b7ne", "dir", "der", "weis\u00b7heit", "e\u00b7rantz", "ge\u00b7schen\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "PPER", "ART", "NN", "NE", "VVPP", "$."], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Drum hast du auch bisher nicht ohne krafft gelehrt.", "tokens": ["Drum", "hast", "du", "auch", "bis\u00b7her", "nicht", "oh\u00b7ne", "krafft", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jhr, die ihr euren sinn auf faule l\u00fcste lencket,", "tokens": ["Ihr", ",", "die", "ihr", "eu\u00b7ren", "sinn", "auf", "fau\u00b7le", "l\u00fcs\u00b7te", "len\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Seht, ob ihr auch dereinst dergleichen lob-spruch h\u00f6rt.", "tokens": ["Seht", ",", "ob", "ihr", "auch", "de\u00b7reinst", "derg\u00b7lei\u00b7chen", "lob\u00b7spruch", "h\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die, welche du bisher vern\u00fcnfftig unterrichtet:", "tokens": ["Die", ",", "wel\u00b7che", "du", "bis\u00b7her", "ver\u00b7n\u00fcnff\u00b7tig", "un\u00b7ter\u00b7rich\u00b7tet", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die, welchen nichts so lieb, als deine freundschafft ist,", "tokens": ["Die", ",", "wel\u00b7chen", "nichts", "so", "lieb", ",", "als", "dei\u00b7ne", "freund\u00b7schafft", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAT", "PIS", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sind zeugen, da\u00df ich dir nichts fremdes angedichtet:", "tokens": ["Sind", "zeu\u00b7gen", ",", "da\u00df", "ich", "dir", "nichts", "frem\u00b7des", "an\u00b7ge\u00b7dich\u00b7tet", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "PIS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sind zeugen, da\u00df man hier die klare wahrheit liest.", "tokens": ["Sind", "zeu\u00b7gen", ",", "da\u00df", "man", "hier", "die", "kla\u00b7re", "wahr\u00b7heit", "liest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So steigt, Gelehrter Mann! der bau von deinem gl\u00fccke,", "tokens": ["So", "steigt", ",", "Ge\u00b7lehr\u00b7ter", "Mann", "!", "der", "bau", "von", "dei\u00b7nem", "gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Besonders, da du dich der Themis anvertraut.", "tokens": ["Be\u00b7son\u00b7ders", ",", "da", "du", "dich", "der", "The\u00b7mis", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie mancher steiget zwar, f\u00e4llt aber bald zur\u00fccke,", "tokens": ["Wie", "man\u00b7cher", "stei\u00b7get", "zwar", ",", "f\u00e4llt", "a\u00b7ber", "bald", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dieweil er nicht genung auf seine wege schaut.", "tokens": ["Die\u00b7weil", "er", "nicht", "ge\u00b7nung", "auf", "sei\u00b7ne", "we\u00b7ge", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du aber, den der stern der klugheit stets begleitet,", "tokens": ["Du", "a\u00b7ber", ",", "den", "der", "stern", "der", "klug\u00b7heit", "stets", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "K\u00f6mmst auf der engen bahn der ehren gl\u00fccklich fort.", "tokens": ["K\u00f6mmst", "auf", "der", "en\u00b7gen", "bahn", "der", "eh\u00b7ren", "gl\u00fcck\u00b7lich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Drum hat die Themis dir itzt einen crantz bereitet:", "tokens": ["Drum", "hat", "die", "The\u00b7mis", "dir", "itzt", "ei\u00b7nen", "crantz", "be\u00b7rei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "De\u00dfwegen zog sie dich an einen h\u00f6hern ort.", "tokens": ["De\u00df\u00b7we\u00b7gen", "zog", "sie", "dich", "an", "ei\u00b7nen", "h\u00f6\u00b7hern", "ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": 