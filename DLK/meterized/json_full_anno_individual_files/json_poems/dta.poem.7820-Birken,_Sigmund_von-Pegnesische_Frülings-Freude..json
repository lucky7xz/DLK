{"dta.poem.7820": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.85", "fr:0.14"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Nur di\u00df mal sprich nit wahr/ du plaudrendes", "tokens": ["Nur", "di\u00df", "mal", "sprich", "nit", "wahr", "/", "du", "plaud\u00b7ren\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDS", "ADV", "ADV", "PTKNEG", "ADJD", "$(", "PPER", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "du B\u00f6ses Ding! wie recht gibt Maro dir den", "tokens": ["du", "B\u00f6\u00b7ses", "Ding", "!", "wie", "recht", "gibt", "Ma\u00b7ro", "dir", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAV", "ADJD", "VVFIN", "NE", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "du streust itzt aus/", "tokens": ["du", "streust", "itzt", "aus", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "nichts als nur b\u00f6sen Zeitungs-samen:", "tokens": ["nichts", "als", "nur", "b\u00f6\u00b7sen", "Zei\u00b7tungs\u00b7sa\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "mir/ mir voraus/ in die erstaunte Ohren.", "tokens": ["mir", "/", "mir", "vo\u00b7raus", "/", "in", "die", "er\u00b7staun\u00b7te", "Oh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "PTKVZ", "$(", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ach! w\u00e4r es ein Gedicht/", "tokens": ["ach", "!", "w\u00e4r", "es", "ein", "Ge\u00b7dicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "was Fama spricht!", "tokens": ["was", "Fa\u00b7ma", "spricht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ach nicht!", "tokens": ["Ach", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Nur allzuwahr hat einen Stern verlohren/", "tokens": ["Nur", "all\u00b7zu\u00b7wahr", "hat", "ei\u00b7nen", "Stern", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "der Erde G\u00f6tter-Haus.", "tokens": ["der", "Er\u00b7de", "G\u00f6t\u00b7ter\u00b7Haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Was/ Stern? ach! eine Sonne.", "tokens": ["Was", "/", "Stern", "?", "ach", "!", "ei\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$(", "NN", "$.", "XY", "$.", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Gab sich ", "tokens": ["Gab", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "das Wunder der Natur/", "tokens": ["das", "Wun\u00b7der", "der", "Na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "des Sch\u00f6pfers Meisterspur/", "tokens": ["des", "Sch\u00f6p\u00b7fers", "Meis\u00b7ter\u00b7spur", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "das Tugend-Zelt/", "tokens": ["das", "Tu\u00b7gen\u00b7dZelt", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "die Augen-Wonne/", "tokens": ["die", "Au\u00b7gen\u00b7Won\u00b7ne", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "der Trefflichkeiten Meer/", "tokens": ["der", "Treff\u00b7lich\u00b7kei\u00b7ten", "Meer", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "die Zier der Welt/", "tokens": ["die", "Zier", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "des Ostlands Ehr/", "tokens": ["des", "Ost\u00b7lands", "Ehr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "dem Tode?", "tokens": ["dem", "To\u00b7de", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "O Tod! Feind aller unsrer Lust!", "tokens": ["O", "Tod", "!", "Feind", "al\u00b7ler", "uns\u00b7rer", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "sahst du/ in nidrem Gras/ nit tausend Bl\u00fcm-", "tokens": ["sahst", "du", "/", "in", "nid\u00b7rem", "Gras", "/", "nit", "tau\u00b7send", "Bl\u00fcm"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "APPR", "ADJA", "NN", "$(", "PTKNEG", "CARD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "auf denen man ohndas mit F\u00fcssen pflegt zu-", "tokens": ["auf", "de\u00b7nen", "man", "ohn\u00b7das", "mit", "F\u00fcs\u00b7sen", "pflegt", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "NE", "APPR", "NN", "VVFIN", "TRUNC"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.24": {"text": "hast unsre Rose du zuf\u00e4llen nur gewust?", "tokens": ["hast", "uns\u00b7re", "Ro\u00b7se", "du", "zu\u00b7f\u00e4l\u00b7len", "nur", "ge\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "ha! G\u00f6ttlichs Angesicht!", "tokens": ["ha", "!", "G\u00f6tt\u00b7lichs", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "ha! Augen! und ach! Blicke!", "tokens": ["ha", "!", "Au\u00b7gen", "!", "und", "ach", "!", "Bli\u00b7cke", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "KON", "XY", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "ists Nacht mit euch?", "tokens": ["ists", "Nacht", "mit", "euch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "ha! Lippen! sprecht ihr nicht?", "tokens": ["ha", "!", "Lip\u00b7pen", "!", "sprecht", "ihr", "nicht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "die ihr so manches Herz wund habet reden", "tokens": ["die", "ihr", "so", "man\u00b7ches", "Herz", "wund", "ha\u00b7bet", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "ha! zarter Glieder Schnee!", "tokens": ["ha", "!", "zar\u00b7ter", "Glie\u00b7der", "Schnee", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "ists billig/ da\u00df man dich so fr\u00fch zu Grab hin-", "tokens": ["ists", "bil\u00b7lig", "/", "da\u00df", "man", "dich", "so", "fr\u00fch", "zu", "Grab", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$(", "KOUS", "PIS", "PRF", "ADV", "ADJD", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "ha! grosser Geist! wie war die Wohnung dir", "tokens": ["ha", "!", "gros\u00b7ser", "Geist", "!", "wie", "war", "die", "Woh\u00b7nung", "dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADJA", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "und dannoch hast du nun von ihr dich lassen", "tokens": ["und", "dan\u00b7noch", "hast", "du", "nun", "von", "ihr", "dich", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Sag/ Fama! ich vergeh/ vor Weh!", "tokens": ["Sag", "/", "Fa\u00b7ma", "!", "ich", "ver\u00b7geh", "/", "vor", "Weh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$.", "PPER", "VVFIN", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "lebt mein Mec\u00e4nas noch? Sein Leben ist ja", "tokens": ["lebt", "mein", "Me\u00b7c\u00e4\u00b7nas", "noch", "?", "Sein", "Le\u00b7ben", "ist", "ja"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "Ach! da\u00df das Leben Ihr mein Tod k\u00f6nd wie-", "tokens": ["Ach", "!", "da\u00df", "das", "Le\u00b7ben", "Ihr", "mein", "Tod", "k\u00f6nd", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "PPER", "PPOSAT", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "wie gerne st\u00fcrb ich doch! ", "tokens": ["wie", "ger\u00b7ne", "st\u00fcrb", "ich", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "da\u00df/ Ihm zu Trost/ Sie l\u00e4nger m\u00f6chte leben.", "tokens": ["da\u00df", "/", "Ihm", "zu", "Trost", "/", "Sie", "l\u00e4n\u00b7ger", "m\u00f6ch\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PPER", "APPR", "NN", "$(", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Lebt\u2019 Orfeus noch:", "tokens": ["Lebt'", "Or\u00b7feus", "noch", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "es h\u00e4tt mit dieser Noht nit noht;", "tokens": ["es", "h\u00e4tt", "mit", "die\u00b7ser", "Noht", "nit", "noht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "ein Gr\u00e4flichs Herz an Ihr noch lang sich sol-", "tokens": ["ein", "Gr\u00e4f\u00b7lichs", "Herz", "an", "Ihr", "noch", "lang", "sich", "sol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ADV", "ADJD", "PRF", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Zwar Orfeus lebt: ", "tokens": ["Zwar", "Or\u00b7feus", "lebt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Mec\u00e4nas selbst es ist.", "tokens": ["Me\u00b7c\u00e4\u00b7nas", "selbst", "es", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Vielleicht hat Er die Laute zuger\u00fcst:", "tokens": ["Viel\u00b7leicht", "hat", "Er", "die", "Lau\u00b7te", "zu\u00b7ge\u00b7r\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Sie durch den Saitenklang/ der Tellus ab-", "tokens": ["Sie", "durch", "den", "Sai\u00b7ten\u00b7klang", "/", "der", "Tel\u00b7lus", "ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$(", "ART", "NE", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Was hilfts? die Erd hat nicht den Geist:", "tokens": ["Was", "hilfts", "?", "die", "Erd", "hat", "nicht", "den", "Geist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "er ist zu wohl verreist", "tokens": ["er", "ist", "zu", "wohl", "ver\u00b7reist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "und aufgehebt/", "tokens": ["und", "auf\u00b7ge\u00b7hebt", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "mit seinen Himmelsgaben.", "tokens": ["mit", "sei\u00b7nen", "Him\u00b7mels\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Ach! was der Himmel hat/ mag nicht auf", "tokens": ["Ach", "!", "was", "der", "Him\u00b7mel", "hat", "/", "mag", "nicht", "auf"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "ART", "NN", "VAFIN", "$(", "VMFIN", "PTKNEG", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.51": {"text": "er kom\u0304t nit wieder her:", "tokens": ["er", "kom\u0304t", "nit", "wie\u00b7der", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.52": {"text": "der Leib zu ihme mu\u00df/ und noch viel sch\u00f6ner/", "tokens": ["der", "Leib", "zu", "ih\u00b7me", "mu\u00df", "/", "und", "noch", "viel", "sch\u00f6\u00b7ner", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VMFIN", "$(", "KON", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Sie ist/ Sie bleibt/ genommen.", "tokens": ["Sie", "ist", "/", "Sie", "bleibt", "/", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Ach Opfer! die ich Ihr zu weihen war be-", "tokens": ["Ach", "Op\u00b7fer", "!", "die", "ich", "Ihr", "zu", "wei\u00b7hen", "war", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "PRELS", "PPER", "PPER", "PTKZU", "VVINF", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "das Grab ist nun Altar. ", "tokens": ["das", "Grab", "ist", "nun", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.56": {"text": "Kr\u00f6nt ihr itzund den Dichter/ ihr Zypressen!", "tokens": ["Kr\u00f6nt", "ihr", "it\u00b7zund", "den", "Dich\u00b7ter", "/", "ihr", "Zyp\u00b7res\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+--+--+--",