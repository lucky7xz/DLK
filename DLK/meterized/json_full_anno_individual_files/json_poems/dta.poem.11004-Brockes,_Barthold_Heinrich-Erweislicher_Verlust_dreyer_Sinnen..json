{"dta.poem.11004": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erweislicher Verlust  \n dreyer Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir, zu des Sch\u00f6pfers Ehren, der die Sinnen uns\ngegeben,", "tokens": ["Wenn", "wir", ",", "zu", "des", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ",", "der", "die", "Sin\u00b7nen", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sie, als g\u00f6ttliche Geschenke, anzusehn, uns nicht bestrebeu,", "tokens": ["Sie", ",", "als", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7schen\u00b7ke", ",", "an\u00b7zu\u00b7sehn", ",", "uns", "nicht", "be\u00b7stre\u00b7beu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ADJA", "NN", "$,", "VVIZU", "$,", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Noch die Welt voll Wunder finden: So sind, ausser Ang-und", "tokens": ["Noch", "die", "Welt", "voll", "Wun\u00b7der", "fin\u00b7den", ":", "So", "sind", ",", "aus\u00b7ser", "Ang\u00b7\u00b7und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "NN", "VVINF", "$.", "ADV", "VAFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "(als durch deren Mittel man", "tokens": ["(", "als", "durch", "de\u00b7ren", "Mit\u00b7tel", "man"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "APPR", "PRELAT", "NN", "PIS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gute Lehren endlich h\u00f6ren, und die Bibel lesen kann,)", "tokens": ["Gu\u00b7te", "Leh\u00b7ren", "end\u00b7lich", "h\u00f6\u00b7ren", ",", "und", "die", "Bi\u00b7bel", "le\u00b7sen", "kann", ",", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,", "KON", "ART", "NN", "VVINF", "VMFIN", "$,", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Zu des gro\u00dfen Sch\u00f6pfers Ehren, drey derselben ganz ver-", "tokens": ["Zu", "des", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ",", "drey", "der\u00b7sel\u00b7ben", "ganz", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "CARD", "PDAT", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Da im ", "tokens": ["Da", "im"], "token_info": ["word", "word"], "pos": ["ADV", "APPRART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Doch so gro\u00dfe Wunder stecken;", "tokens": ["Doch", "so", "gro\u00b7\u00dfe", "Wun\u00b7der", "ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und sie uns, auf manche Weise,", "tokens": ["Und", "sie", "uns", ",", "auf", "man\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Uns zum Nutz, und Gott zum Preise,", "tokens": ["Uns", "zum", "Nutz", ",", "und", "Gott", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gottes Lieb und Macht entdecken.", "tokens": ["Got\u00b7tes", "Lieb", "und", "Macht", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}