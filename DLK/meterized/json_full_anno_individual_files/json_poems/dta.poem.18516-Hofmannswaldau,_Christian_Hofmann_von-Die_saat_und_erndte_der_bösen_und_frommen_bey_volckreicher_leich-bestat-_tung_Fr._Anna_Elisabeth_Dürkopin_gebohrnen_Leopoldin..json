{"dta.poem.18516": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die saat und erndte der b\u00f6sen und  \n frommen/ bey volckreicher leich-bestat-  \n tung Fr. Anna Elisabeth D\u00fcrkopin/  \n gebohrnen Leopoldin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geehrter/ la\u00df mich doch in diesen zeilen weinen/", "tokens": ["Ge\u00b7ehr\u00b7ter", "/", "la\u00df", "mich", "doch", "in", "die\u00b7sen", "zei\u00b7len", "wei\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil hertz und auge sich hiezu am besten schickt;", "tokens": ["Weil", "hertz", "und", "au\u00b7ge", "sich", "hie\u00b7zu", "am", "bes\u00b7ten", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "PAV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit trost mag dir und mir ein ander freund erscheinen/", "tokens": ["Mit", "trost", "mag", "dir", "und", "mir", "ein", "an\u00b7der", "freund", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den keine centner-last geh\u00e4uffter plagen dr\u00fcckt.", "tokens": ["Den", "kei\u00b7ne", "cent\u00b7ner\u00b7last", "ge\u00b7h\u00e4uff\u00b7ter", "pla\u00b7gen", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich zehle leider nichts als frisch-erbla\u00dfte leichen/", "tokens": ["Ich", "zeh\u00b7le", "lei\u00b7der", "nichts", "als", "frischer\u00b7bla\u00df\u00b7te", "lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "KOKOM", "ADJA", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wobey die treue schaar mit nassen wangen steht;", "tokens": ["Wo\u00b7bey", "die", "treu\u00b7e", "schaar", "mit", "nas\u00b7sen", "wan\u00b7gen", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was wunder/ wenn mein hertz bey solchen trauer-zeichen", "tokens": ["Was", "wun\u00b7der", "/", "wenn", "mein", "hertz", "bey", "sol\u00b7chen", "trau\u00b7e\u00b7rzei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$(", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von thr\u00e4nen sonder zahl aus wehmuth \u00fcbergeht?", "tokens": ["Von", "thr\u00e4\u00b7nen", "son\u00b7der", "zahl", "aus", "weh\u00b7muth", "\u00fc\u00b7ber\u00b7geht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Betr\u00fcbtes Jubel-jahr/ wie schwartz sind deine tage!", "tokens": ["Be\u00b7tr\u00fcb\u00b7tes", "Ju\u00b7bel\u00b7jahr", "/", "wie", "schwartz", "sind", "dei\u00b7ne", "ta\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der anfang meldet sich mit marter-wochen an/", "tokens": ["Der", "an\u00b7fang", "mel\u00b7det", "sich", "mit", "mar\u00b7ter\u00b7wo\u00b7chen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die neuen stunden sind die boten neuer plage;", "tokens": ["Die", "neu\u00b7en", "stun\u00b7den", "sind", "die", "bo\u00b7ten", "neu\u00b7er", "pla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wohl dem der zeitiger dort Ostern feyren kan!", "tokens": ["Wohl", "dem", "der", "zei\u00b7ti\u00b7ger", "dort", "Os\u00b7tern", "fey\u00b7ren", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jedoch was soll ich viel den lauff des jahres schelten?", "tokens": ["Je\u00b7doch", "was", "soll", "ich", "viel", "den", "lauff", "des", "jah\u00b7res", "schel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das gantze lebens-seld gleicht einer thr\u00e4nen-saat:", "tokens": ["Das", "gant\u00b7ze", "le\u00b7bens\u00b7seld", "gleicht", "ei\u00b7ner", "thr\u00e4\u00b7nen\u00b7saat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der s\u00e4mann ist der mensch/ der s\u00e4et wunderselten/", "tokens": ["Der", "s\u00e4\u00b7mann", "ist", "der", "mensch", "/", "der", "s\u00e4et", "wun\u00b7der\u00b7sel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "VVFIN", "ADJA", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da\u00df er ein quentlein lust gewi\u00df zu erndten hat.", "tokens": ["Da\u00df", "er", "ein", "quent\u00b7lein", "lust", "ge\u00b7wi\u00df", "zu", "ernd\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Den eintritt in die welt begr\u00fcssen wir mit z\u00e4hren/", "tokens": ["Den", "ein\u00b7tritt", "in", "die", "welt", "be\u00b7gr\u00fcs\u00b7sen", "wir", "mit", "z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das winseln ohne ziel ist Heerold vieler pein/", "tokens": ["Das", "win\u00b7seln", "oh\u00b7ne", "ziel", "ist", "Hee\u00b7rold", "vie\u00b7ler", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VAFIN", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So das verh\u00e4ngni\u00df uns als mutter will geb\u00e4hren/", "tokens": ["So", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "uns", "als", "mut\u00b7ter", "will", "ge\u00b7b\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "KOUS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn wir an jahren reich und auch an kr\u00e4fften seyn.", "tokens": ["Wenn", "wir", "an", "jah\u00b7ren", "reich", "und", "auch", "an", "kr\u00e4ff\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "KON", "ADV", "APPR", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die jugend kennet nichts/ als taumeln/ fallen/ fehlen/", "tokens": ["Die", "ju\u00b7gend", "ken\u00b7net", "nichts", "/", "als", "tau\u00b7meln", "/", "fal\u00b7len", "/", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "KOKOM", "VVFIN", "$(", "VVINF", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auff taumeln/ fall und fehl erfolget weh und ach;", "tokens": ["Auff", "tau\u00b7meln", "/", "fall", "und", "fehl", "er\u00b7fol\u00b7get", "weh", "und", "ach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$(", "NN", "KON", "ADJD", "VVFIN", "PTKVZ", "KON", "XY", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die ruthe strenger zucht ist ein verha\u00dftes qu\u00e4len/", "tokens": ["Die", "ru\u00b7the", "stren\u00b7ger", "zucht", "ist", "ein", "ver\u00b7ha\u00df\u00b7tes", "qu\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Jhr anblick wandelt uns in eine thr\u00e4nen-bach.", "tokens": ["Ihr", "an\u00b7blick", "wan\u00b7delt", "uns", "in", "ei\u00b7ne", "thr\u00e4\u00b7nen\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zuweilen erndten wir/ doch aber kurtzes lachen/", "tokens": ["Zu\u00b7wei\u00b7len", "ernd\u00b7ten", "wir", "/", "doch", "a\u00b7ber", "kurt\u00b7zes", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADV", "ADV", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn welt und wollust uns von ihrer taffel speist;", "tokens": ["Wenn", "welt", "und", "wol\u00b7lust", "uns", "von", "ih\u00b7rer", "taf\u00b7fel", "speist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dann pflegt man ohne wirth die rechnung offt zu machen/", "tokens": ["Dann", "pflegt", "man", "oh\u00b7ne", "wirth", "die", "rech\u00b7nung", "offt", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJD", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was wunder/ wenn betrug das wahre facit heist?", "tokens": ["Was", "wun\u00b7der", "/", "wenn", "be\u00b7trug", "das", "wah\u00b7re", "fa\u00b7cit", "heist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "VVFIN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der reiffen jahre frucht ist \u00e4pffeln gleich zu sch\u00e4tzen/", "tokens": ["Der", "reif\u00b7fen", "jah\u00b7re", "frucht", "ist", "\u00e4pf\u00b7feln", "gleich", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Sodoms w\u00fcster rest in seinen feldern tr\u00e4gt:", "tokens": ["Die", "So\u00b7doms", "w\u00fcs\u00b7ter", "rest", "in", "sei\u00b7nen", "fel\u00b7dern", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Versucht ein hungriger den zahn darein zu setzen/", "tokens": ["Ver\u00b7sucht", "ein", "hung\u00b7ri\u00b7ger", "den", "zahn", "da\u00b7rein", "zu", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "NN", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So f\u00fchlt er wie der staub ihm in die augen schl\u00e4gt.", "tokens": ["So", "f\u00fchlt", "er", "wie", "der", "staub", "ihm", "in", "die", "au\u00b7gen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Man sucht durch schweres gold den gipffel zu erreichen/", "tokens": ["Man", "sucht", "durch", "schwe\u00b7res", "gold", "den", "gipf\u00b7fel", "zu", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wo das ber\u00fchmte schlo\u00df der stoltzen ehre prangt/", "tokens": ["Wo", "das", "be\u00b7r\u00fchm\u00b7te", "schlo\u00df", "der", "stolt\u00b7zen", "eh\u00b7re", "prangt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da heisst es offtmals: Freund/ du must vor diesem weichen!", "tokens": ["Da", "heisst", "es", "offt\u00b7mals", ":", "Freund", "/", "du", "must", "vor", "die\u00b7sem", "wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "NN", "$(", "PPER", "VMFIN", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "An thr\u00e4nen fehlt es nicht/ bi\u00df man den zweg erlangt.", "tokens": ["An", "thr\u00e4\u00b7nen", "fehlt", "es", "nicht", "/", "bi\u00df", "man", "den", "zweg", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PIS", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "L\u00e4st denn der ehren-stand auch an den ehstand dencken/", "tokens": ["L\u00e4st", "denn", "der", "eh\u00b7ren\u00b7stand", "auch", "an", "den", "eh\u00b7stand", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So l\u00e4ufft ein wilder hengst zu seinen noth-stall ein;", "tokens": ["So", "l\u00e4ufft", "ein", "wil\u00b7der", "hengst", "zu", "sei\u00b7nen", "noth\u00b7stall", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er l\u00e4sset nicht vernunfft noch klugen raht sich lencken/", "tokens": ["Er", "l\u00e4s\u00b7set", "nicht", "ver\u00b7nunfft", "noch", "klu\u00b7gen", "raht", "sich", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVFIN", "ADV", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und will bey Delila mit macht ein Samson seyn.", "tokens": ["Und", "will", "bey", "De\u00b7li\u00b7la", "mit", "macht", "ein", "Sam\u00b7son", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NE", "APPR", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das demant-feste band ist mehrmals kaum gebunden/", "tokens": ["Das", "de\u00b7mant\u00b7fes\u00b7te", "band", "ist", "mehr\u00b7mals", "kaum", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So fehlt es schon an wein/ iedoch an weinen nicht/", "tokens": ["So", "fehlt", "es", "schon", "an", "wein", "/", "ie\u00b7doch", "an", "wei\u00b7nen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$(", "ADV", "APPR", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn zehlt der arme tropff die sorgen-reichen stunden/", "tokens": ["Denn", "zehlt", "der", "ar\u00b7me", "tropff", "die", "sor\u00b7gen\u00b7rei\u00b7chen", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Bevor der tod den hal\u00df dem lieben weibe bricht.", "tokens": ["Be\u00b7vor", "der", "tod", "den", "hal\u00df", "dem", "lie\u00b7ben", "wei\u00b7be", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Will dieser nicht so bald den heissen wunsch gew\u00e4hren/", "tokens": ["Will", "die\u00b7ser", "nicht", "so", "bald", "den", "heis\u00b7sen", "wunsch", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "PTKNEG", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So merckt man wie der groll sich in das hertze stellt;", "tokens": ["So", "merckt", "man", "wie", "der", "groll", "sich", "in", "das", "hert\u00b7ze", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ART", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Der groll pflegt streit und krieg im hause zu geb\u00e4hren/", "tokens": ["Der", "groll", "pflegt", "streit", "und", "krieg", "im", "hau\u00b7se", "zu", "ge\u00b7b\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Des krieges endschafft ist ein thr\u00e4nend wangen-feld.", "tokens": ["Des", "krie\u00b7ges", "end\u00b7schafft", "ist", "ein", "thr\u00e4\u00b7nend", "wan\u00b7gen\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "So sieht der wehstand aus! wird dann ein paar geschieden/", "tokens": ["So", "sieht", "der", "weh\u00b7stand", "aus", "!", "wird", "dann", "ein", "paar", "ge\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "VAFIN", "ADV", "ART", "PIAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und durch des todes pfeil das faule band zertrennt/", "tokens": ["Und", "durch", "des", "to\u00b7des", "pfeil", "das", "fau\u00b7le", "band", "zer\u00b7trennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "So gibt man sich fein bald ob dem verlust zu frieden/", "tokens": ["So", "gibt", "man", "sich", "fein", "bald", "ob", "dem", "ver\u00b7lust", "zu", "frie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADJD", "ADV", "KOUS", "ART", "NN", "APPR", "NN", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Und kennt die glut nicht mehr/ die vor so starck gebrennt.", "tokens": ["Und", "kennt", "die", "glut", "nicht", "mehr", "/", "die", "vor", "so", "starck", "ge\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$(", "ART", "APPR", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Inzwischen pflegt der leib/ doch nicht das hertz zu trauren/", "tokens": ["I\u00b7nzwi\u00b7schen", "pflegt", "der", "leib", "/", "doch", "nicht", "das", "hertz", "zu", "trau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Das auge weinet offt wenn geist und seele lacht;", "tokens": ["Das", "au\u00b7ge", "wei\u00b7net", "offt", "wenn", "geist", "und", "see\u00b7le", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOUS", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Man denckt: so sind numehr zerbrochen diese mauren/", "tokens": ["Man", "denckt", ":", "so", "sind", "nu\u00b7mehr", "zer\u00b7bro\u00b7chen", "die\u00b7se", "mau\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "ADV", "VAFIN", "ADV", "VVPP", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "So eine scheidewand der geilen lust gemacht.", "tokens": ["So", "ei\u00b7ne", "schei\u00b7de\u00b7wand", "der", "gei\u00b7len", "lust", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Der frische Wittwer geht auff stoltzen freyers-f\u00fcssen/", "tokens": ["Der", "fri\u00b7sche", "Witt\u00b7wer", "geht", "auff", "stolt\u00b7zen", "freyer\u00b7sf\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.58": {"text": "Und streicht den pogel-leim bey zeiten auf den heerd;", "tokens": ["Und", "streicht", "den", "po\u00b7gel\u00b7leim", "bey", "zei\u00b7ten", "auf", "den", "heerd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Hat eine kr\u00e4he sich gefangen geben m\u00fcssen/", "tokens": ["Hat", "ei\u00b7ne", "kr\u00e4\u00b7he", "sich", "ge\u00b7fan\u00b7gen", "ge\u00b7ben", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "VVFIN", "PRF", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "So lacht er da\u00df das gl\u00fcck ihm solchen fang bescheert.", "tokens": ["So", "lacht", "er", "da\u00df", "das", "gl\u00fcck", "ihm", "sol\u00b7chen", "fang", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Jedoch so pflegt es nur der schaum von menschen machen/", "tokens": ["Je\u00b7doch", "so", "pflegt", "es", "nur", "der", "schaum", "von", "men\u00b7schen", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Der reich an unverstand/ und arm an witz erscheint:", "tokens": ["Der", "reich", "an", "un\u00b7ver\u00b7stand", "/", "und", "arm", "an", "witz", "er\u00b7scheint", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "$(", "KON", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Hier gilt der wahre spruch: Wenn kinder zeitig lachen/", "tokens": ["Hier", "gilt", "der", "wah\u00b7re", "spruch", ":", "