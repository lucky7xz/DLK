{"dta.poem.1372": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eitelkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr bejahrten Eich- und Tannen/ deren dick-umlaubtes", "tokens": ["Ihr", "be\u00b7jahr\u00b7ten", "Eich", "und", "Tan\u00b7nen", "/", "de\u00b7ren", "dick\u00b7um\u00b7laub\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "TRUNC", "KON", "NN", "$(", "PRELAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Haubt", "tokens": ["Haubt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Diesem Bache Schatten giebet/ ihre Macht der Sonnen", "tokens": ["Die\u00b7sem", "Ba\u00b7che", "Schat\u00b7ten", "gie\u00b7bet", "/", "ih\u00b7re", "Macht", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "raubt/", "tokens": ["raubt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wie vergleicht sich euer Stand also wenig mit dem meinen!", "tokens": ["Wie", "ver\u00b7gleicht", "sich", "eu\u00b7er", "Stand", "al\u00b7so", "we\u00b7nig", "mit", "dem", "mei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "PIS", "APPR", "ART", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Wie so wenig kan der Mensch eurem Wesen \u00e4hnlich schei-", "tokens": ["Wie", "so", "we\u00b7nig", "kan", "der", "Mensch", "eu\u00b7rem", "We\u00b7sen", "\u00e4hn\u00b7lich", "schei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "VMFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "nen!", "tokens": ["nen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Hundert Jahre sind verstrichen/ und ihr seyd noch frisch und", "tokens": ["Hun\u00b7dert", "Jah\u00b7re", "sind", "ver\u00b7stri\u00b7chen", "/", "und", "ihr", "seyd", "noch", "frisch", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$(", "KON", "PPER", "VAFIN", "ADV", "ADJD", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "gantz/", "tokens": ["gantz", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Eure Rind\u2019 und Bl\u00e4tter haben noch vollkommen ihren", "tokens": ["Eu\u00b7re", "Rind'", "und", "Bl\u00e4t\u00b7ter", "ha\u00b7ben", "noch", "voll\u00b7kom\u00b7men", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Glantz.", "tokens": ["Glantz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Ich/ bey Leben mehr als todt/ mu\u00df dem Rest der Jahre wei-", "tokens": ["Ich", "/", "bey", "Le\u00b7ben", "mehr", "als", "todt", "/", "mu\u00df", "dem", "Rest", "der", "Jah\u00b7re", "wei"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "APPR", "NN", "PIAT", "KOKOM", "ADJD", "$(", "VMFIN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Eh ich noch das halbe Theil eures Alters kan erreichen.", "tokens": ["Eh", "ich", "noch", "das", "hal\u00b7be", "Theil", "eu\u00b7res", "Al\u00b7ters", "kan", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Euch mu\u00df nutzen/ mehr als schaden der beflammte Sonnen-", "tokens": ["Euch", "mu\u00df", "nut\u00b7zen", "/", "mehr", "als", "scha\u00b7den", "der", "be\u00b7flamm\u00b7te", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "PIAT", "KOKOM", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Schein/", "tokens": ["Schein", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Was der kalte Winter raubet/ bringt der warme Sommer", "tokens": ["Was", "der", "kal\u00b7te", "Win\u00b7ter", "rau\u00b7bet", "/", "bringt", "der", "war\u00b7me", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "ein;", "tokens": ["ein", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Ich erfriere/ wenn es kalt; ich verbrenne/ wenn es hitzet/", "tokens": ["Ich", "er\u00b7frie\u00b7re", "/", "wenn", "es", "kalt", ";", "ich", "ver\u00b7bren\u00b7ne", "/", "wenn", "es", "hit\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADJD", "$.", "PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Weder Lentz noch Winter ists/ welcher mich vorm Tode sch\u00fctzet", "tokens": ["We\u00b7der", "Lentz", "noch", "Win\u00b7ter", "ists", "/", "wel\u00b7cher", "mich", "vorm", "To\u00b7de", "sch\u00fct\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "NN", "VAFIN", "$(", "PWS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Ihr erhebet eure Wipffel fast bi\u00df an das Sternen-Dach/", "tokens": ["Ihr", "er\u00b7he\u00b7bet", "eu\u00b7re", "Wipf\u00b7fel", "fast", "bi\u00df", "an", "das", "Ster\u00b7nen\u00b7Dach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Ihr umarmet Lufft und Wolcken/ gebet keinem Sturme nach;", "tokens": ["Ihr", "um\u00b7ar\u00b7met", "Lufft", "und", "Wol\u00b7cken", "/", "ge\u00b7bet", "kei\u00b7nem", "Stur\u00b7me", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$(", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Ich vor Blitz und Donner scheu/ mu\u00df das Haubt zur Erden", "tokens": ["Ich", "vor", "Blitz", "und", "Don\u00b7ner", "scheu", "/", "mu\u00df", "das", "Haubt", "zur", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "ADJD", "$(", "VMFIN", "ART", "NN", "APPRART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "biegen/", "tokens": ["bie\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Deren offne Scho\u00df f\u00fcr mich machet Raum und Platz zu lie-", "tokens": ["De\u00b7ren", "off\u00b7ne", "Scho\u00df", "f\u00fcr", "mich", "ma\u00b7chet", "Raum", "und", "Platz", "zu", "lie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "APPR", "PPER", "VVFIN", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Sind nun mehr als wir die B\u00e4ume/ was erhebt sich unser", "tokens": ["Sind", "nun", "mehr", "als", "wir", "die", "B\u00e4u\u00b7me", "/", "was", "er\u00b7hebt", "sich", "un\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "PPER", "ART", "NN", "$(", "PWS", "VVFIN", "PRF", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.28": {"text": "Geist?", "tokens": ["Geist", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Was ists/ da\u00df man in Gedancken \u00fcber Mond und Sternen", "tokens": ["Was", "ists", "/", "da\u00df", "man", "in", "Ge\u00b7dan\u00b7cken", "\u00fc\u00b7ber", "Mond", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$(", "KOUS", "PIS", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "reist?", "tokens": ["reist", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Last uns iede Stund und Tag/ ieden Morgen also leben/", "tokens": ["Last", "uns", "ie\u00b7de", "Stund", "und", "Tag", "/", "ie\u00b7den", "Mor\u00b7gen", "al\u00b7so", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$(", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Als wenn auff den Abend wir m\u00fcsten Gutt und Blutt be-", "tokens": ["Als", "wenn", "auff", "den", "A\u00b7bend", "wir", "m\u00fcs\u00b7ten", "Gutt", "und", "Blutt", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ART", "NN", "PPER", "VMFIN", "ADJD", "KON", "ADJD", "TRUNC"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "geben.", "tokens": ["ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}