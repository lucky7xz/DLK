{"textgrid.poem.33264": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Geist wohnt in den H\u00f6hn,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Geist wohnt in den H\u00f6hn,", "tokens": ["Ein", "Geist", "wohnt", "in", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdessen Herz einer Laute gleicht\u00ab;", "tokens": ["\u00bb", "des\u00b7sen", "Herz", "ei\u00b7ner", "Lau\u00b7te", "gleicht", "\u00ab", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PRELAT", "NN", "ART", "NN", "VVFIN", "$(", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wie Israfel so sch\u00f6n", "tokens": ["Wie", "Is\u00b7ra\u00b7fel", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Singt keiner in den H\u00f6hn;", "tokens": ["Singt", "kei\u00b7ner", "in", "den", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Sterne, die sich kreisend drehn,", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "sich", "krei\u00b7send", "drehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verstummen im Vor\u00fcbergehn,", "tokens": ["Ver\u00b7stum\u00b7men", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn der Klang sie erreicht.", "tokens": ["Wenn", "der", "Klang", "sie", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und wenn im Weltgetriebe", "tokens": ["Und", "wenn", "im", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der wechselnde Mond", "tokens": ["Der", "wech\u00b7seln\u00b7de", "Mond"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Am h\u00f6chsten thront,", "tokens": ["Am", "h\u00f6chs\u00b7ten", "thront", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ergl\u00fcht er von Liebe;", "tokens": ["Er\u00b7gl\u00fcht", "er", "von", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und horchend verharren der rote Blitz", "tokens": ["Und", "hor\u00b7chend", "ver\u00b7har\u00b7ren", "der", "ro\u00b7te", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und die sieben Plejaden stockenden Schritts", "tokens": ["Und", "die", "sie\u00b7ben", "Ple\u00b7ja\u00b7den", "sto\u00b7cken\u00b7den", "Schritts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "CARD", "NN", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auf ihrem Himmelssitz.", "tokens": ["Auf", "ih\u00b7rem", "Him\u00b7mels\u00b7sitz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sie sagen (der sternige Rat", "tokens": ["Und", "sie", "sa\u00b7gen", "(", "der", "ster\u00b7ni\u00b7ge", "Rat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$(", "ART", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und alle Lauscher in seinem Geleite),", "tokens": ["Und", "al\u00b7le", "Lau\u00b7scher", "in", "sei\u00b7nem", "Ge\u00b7lei\u00b7te", ")", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$(", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df Israfel sein Feuer", "tokens": ["Da\u00df", "Is\u00b7ra\u00b7fel", "sein", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verdanke jener Leier,", "tokens": ["Ver\u00b7dan\u00b7ke", "je\u00b7ner", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die seine Stimme weihte \u2013", "tokens": ["Die", "sei\u00b7ne", "Stim\u00b7me", "weih\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem bebenden lebenden Draht", "tokens": ["Dem", "be\u00b7ben\u00b7den", "le\u00b7ben\u00b7den", "Draht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Jener ungew\u00f6hnlichen Saite.", "tokens": ["Je\u00b7ner", "un\u00b7ge\u00b7w\u00f6hn\u00b7li\u00b7chen", "Sai\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch die H\u00f6hn, wo der Engel wohnt,", "tokens": ["Doch", "die", "H\u00f6hn", ",", "wo", "der", "En\u00b7gel", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo hohe Gedanken, Pflicht und Zoll,", "tokens": ["Wo", "ho\u00b7he", "Ge\u00b7dan\u00b7ken", ",", "Pflicht", "und", "Zoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo, erwachsene Gottheit, die Liebe thront,", "tokens": ["Wo", ",", "er\u00b7wach\u00b7se\u00b7ne", "Got\u00b7theit", ",", "die", "Lie\u00b7be", "thront", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wo die Huri blickt, sind nah und fern", "tokens": ["Wo", "die", "Hu\u00b7ri", "blickt", ",", "sind", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Von all der Sch\u00f6nheit voll,", "tokens": ["Von", "all", "der", "Sch\u00f6n\u00b7heit", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die wir sch\u00e4tzen an einem Stern.", "tokens": ["Die", "wir", "sch\u00e4t\u00b7zen", "an", "ei\u00b7nem", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Drum gehst du recht in deinem Drang,", "tokens": ["Drum", "gehst", "du", "recht", "in", "dei\u00b7nem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Israfel, du weiser Barde!", "tokens": ["O", "Is\u00b7ra\u00b7fel", ",", "du", "wei\u00b7ser", "Bar\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verachtend glutenlosen Sang", "tokens": ["Ver\u00b7ach\u00b7tend", "glu\u00b7ten\u00b7lo\u00b7sen", "Sang"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gab dir der Ruhm den h\u00f6chsten Rang,", "tokens": ["Gab", "dir", "der", "Ruhm", "den", "h\u00f6chs\u00b7ten", "Rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein ist der Lorbeer, bester Barde!", "tokens": ["Dein", "ist", "der", "Lor\u00b7beer", ",", "bes\u00b7ter", "Bar\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Heiter lebe und lang!", "tokens": ["Hei\u00b7ter", "le\u00b7be", "und", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und die Verz\u00fcckungen dr\u00fcben,", "tokens": ["Und", "die", "Ver\u00b7z\u00fc\u00b7ckun\u00b7gen", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie passen zu deinem feurigen Reigen,", "tokens": ["Sie", "pas\u00b7sen", "zu", "dei\u00b7nem", "feu\u00b7ri\u00b7gen", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deinem Gram, deiner Lust, deinem Ha\u00df, deinem Lieben,", "tokens": ["Dei\u00b7nem", "Gram", ",", "dei\u00b7ner", "Lust", ",", "dei\u00b7nem", "Ha\u00df", ",", "dei\u00b7nem", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sind ganz deiner Inbrunst zu eigen \u2013", "tokens": ["Sind", "ganz", "dei\u00b7ner", "In\u00b7brunst", "zu", "ei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKA", "ADJD", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wohl m\u00f6gen die Sterne schweigen!", "tokens": ["Wohl", "m\u00f6\u00b7gen", "die", "Ster\u00b7ne", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ja, der Himmel ist dein! Doch dieser Welt", "tokens": ["Ja", ",", "der", "Him\u00b7mel", "ist", "dein", "!", "Doch", "die\u00b7ser", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$.", "KON", "PDAT", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ist S\u00fc\u00df und Sauer gemein;", "tokens": ["Ist", "S\u00fc\u00df", "und", "Sau\u00b7er", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unsre Blumen k\u00f6nnen nur \u2013 Blumen sein;", "tokens": ["Uns\u00b7re", "Blu\u00b7men", "k\u00f6n\u00b7nen", "nur", "\u2013", "Blu\u00b7men", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "$(", "NN", "VAINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Schatten deiner Wonne f\u00e4llt", "tokens": ["Der", "Schat\u00b7ten", "dei\u00b7ner", "Won\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf uns als Sonnenschein.", "tokens": ["Auf", "uns", "als", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O w\u00e4r ich schnell,", "tokens": ["O", "w\u00e4r", "ich", "schnell", ","], "token_info": ["word", "word", "word", "