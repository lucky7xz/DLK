{"dta.poem.2742": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Absterben eines zarten S\u00f6hnleins  \n C. A. den 16. Novembr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du zartes Kind/ du Anmuths-voller Knabe/", "tokens": ["Du", "zar\u00b7tes", "Kind", "/", "du", "An\u00b7muths\u00b7vol\u00b7ler", "Kna\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Eltern Trost und Hoffnung ihrer Zeit/", "tokens": ["Der", "El\u00b7tern", "Trost", "und", "Hoff\u00b7nung", "ih\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eilst du doch so bald zu deinem Grabe:", "tokens": ["Wie", "eilst", "du", "doch", "so", "bald", "zu", "dei\u00b7nem", "Gra\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerschleust so fr\u00fch der Leib/ der Seele Kleid?", "tokens": ["Zer\u00b7schleust", "so", "fr\u00fch", "der", "Leib", "/", "der", "See\u00b7le", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer treibt dich an? den du bi\u00dfher getragen", "tokens": ["Wer", "treibt", "dich", "an", "?", "den", "du", "bi\u00df\u00b7her", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "PPER", "ADV", "VVPP"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Jm Hertzen hast/ der Welt und Himmel tr\u00e4gt/", "tokens": ["Jm", "Hert\u00b7zen", "hast", "/", "der", "Welt", "und", "Him\u00b7mel", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein C Hristus wil dir Christoph dieses sagen:", "tokens": ["Dein", "C", "Hris\u00b7tus", "wil", "dir", "Chris\u00b7toph", "die\u00b7ses", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "NE", "VMFIN", "PPER", "NN", "PDAT", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Fleuch/ fleuch/ mein Freund wie ein Rehb\u00f6cklein pflegt.", "tokens": ["Fleuch", "/", "fleuch", "/", "mein", "Freund", "wie", "ein", "Reh\u00b7b\u00f6c\u00b7klein", "pflegt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Rei\u00df\u2019 als ein Hirsch durch auffgestellte Netze/", "tokens": ["Rei\u00df'", "als", "ein", "Hirsch", "durch", "auff\u00b7ge\u00b7stell\u00b7te", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh Satan dich der J\u00e4ger noch beharrt.", "tokens": ["Eh", "Sa\u00b7tan", "dich", "der", "J\u00e4\u00b7ger", "noch", "be\u00b7harrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh dich die Welt mit ihren Winden hetze/", "tokens": ["Eh", "dich", "die", "Welt", "mit", "ih\u00b7ren", "Win\u00b7den", "het\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sich dein Aug an Eitelkeit vernarrt.", "tokens": ["Und", "sich", "dein", "Aug", "an", "Ei\u00b7tel\u00b7keit", "ver\u00b7narrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verschleu\u00df dein Ohr so bald die Hifft geblasen/", "tokens": ["Ver\u00b7schleu\u00df", "dein", "Ohr", "so", "bald", "die", "Hifft", "ge\u00b7bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie setzt dir f\u00fcr zu hindern Spur und Gang.", "tokens": ["Sie", "setzt", "dir", "f\u00fcr", "zu", "hin\u00b7dern", "Spur", "und", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht trau zu viel dem sch\u00f6n\u2019 und gr\u00fcnen Rasen/", "tokens": ["Nicht", "trau", "zu", "viel", "dem", "sch\u00f6n'", "und", "gr\u00fc\u00b7nen", "Ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PIS", "ART", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Eh du es meynst/ so hast dn einen Fang.", "tokens": ["Eh", "du", "es", "meynst", "/", "so", "hast", "dn", "ei\u00b7nen", "Fang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du folgst der Stimm und hast den Fu\u00df entzogen", "tokens": ["Du", "folgst", "der", "Stimm", "und", "hast", "den", "Fu\u00df", "ent\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem S\u00fcnden Garn/ die Stricke sind entzwey.", "tokens": ["Dem", "S\u00fcn\u00b7den", "Garn", "/", "die", "Stri\u00b7cke", "sind", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich hat die Welt/ die Circe nie betrogen/", "tokens": ["Dich", "hat", "die", "Welt", "/", "die", "Cir\u00b7ce", "nie", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch eingeschl\u00e4fft durch ihre Zauberey.", "tokens": ["Noch", "ein\u00b7ge\u00b7schl\u00e4fft", "durch", "ih\u00b7re", "Zau\u00b7be\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du als ein Kind bist klugf\u00fcr uns zu nennen/", "tokens": ["Du", "als", "ein", "Kind", "bist", "klug\u00b7f\u00fcr", "uns", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das zeitlich hat erkant des Lebens Traum:", "tokens": ["Das", "zeit\u00b7lich", "hat", "er\u00b7kant", "des", "Le\u00b7bens", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Indem wir uns bem\u00fchen/ lauffen/ rennen", "tokens": ["In\u00b7dem", "wir", "uns", "be\u00b7m\u00fc\u00b7hen", "/", "lauf\u00b7fen", "/", "ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$(", "VVINF", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Umb lauter Nichts und umb drey Ellen Raum.", "tokens": ["Umb", "lau\u00b7ter", "Nichts", "und", "umb", "drey", "El\u00b7len", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "KON", "APPR", "CARD", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir kriechen stets wie Kinder auff der Erden/", "tokens": ["Wir", "krie\u00b7chen", "stets", "wie", "Kin\u00b7der", "auff", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn schwingt sich wol die Seele Himmel an?", "tokens": ["Wenn", "schwingt", "sich", "wol", "die", "See\u00b7le", "Him\u00b7mel", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ein Kind z\u00fcrnt/ wenn es beraubt mu\u00df werden", "tokens": ["Wie", "ein", "Kind", "z\u00fcrnt", "/", "wenn", "es", "be\u00b7raubt", "mu\u00df", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$(", "KOUS", "PPER", "VVPP", "VMFIN", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Puppen-Werck/ und weinet was es kan.", "tokens": ["Vom", "Pup\u00b7pen\u00b7\u00b7Werck", "/", "und", "wei\u00b7net", "was", "es", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KON", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So auch wenn uns entnommen was ergetzet/", "tokens": ["So", "auch", "wenn", "uns", "ent\u00b7nom\u00b7men", "was", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VVFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und zeitlich Gut/ der Amei\u00df Hauff\u2019 entgeht.", "tokens": ["Und", "zeit\u00b7lich", "Gut", "/", "der", "Am\u00b7ei\u00df", "Hauff'", "ent\u00b7geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da f\u00fchlen wir so t\u00f6dtlich uns verletzet/", "tokens": ["Da", "f\u00fch\u00b7len", "wir", "so", "t\u00f6dt\u00b7lich", "uns", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df Blut umb Hertz/ im Auge Wasser steht.", "tokens": ["Da\u00df", "Blut", "umb", "Hertz", "/", "im", "Au\u00b7ge", "Was\u00b7ser", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$(", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo aber hin heist dich dein Heyland fliehen/", "tokens": ["Wo", "a\u00b7ber", "hin", "heist", "dich", "dein", "Hey\u00b7land", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kleiner Christ doch grosser Glaubens-Held?", "tokens": ["Du", "klei\u00b7ner", "Christ", "doch", "gros\u00b7ser", "Glau\u00b7bens\u00b7Held", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sollstu gleich weg/ so nah Weynachten/ ziehen/", "tokens": ["Soll\u00b7stu", "gleich", "weg", "/", "so", "nah", "Wey\u00b7nach\u00b7ten", "/", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKVZ", "$(", "ADV", "ADJD", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da er sich sonst ein zu bescheren stellt.", "tokens": ["Da", "er", "sich", "sonst", "ein", "zu", "be\u00b7sche\u00b7ren", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So kriegstu hier kein herrlich Christ-Geschencke/", "tokens": ["So", "kriegs\u00b7tu", "hier", "kein", "herr\u00b7lich", "Christ\u00b7Ge\u00b7schen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man tr\u00e4gt dir nicht die Gab in B\u00fcrden zu?", "tokens": ["Man", "tr\u00e4gt", "dir", "nicht", "die", "Gab", "in", "B\u00fcr\u00b7den", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach nein! man setzt dir itzt die Leichen-B\u00e4ncke", "tokens": ["Ach", "nein", "!", "man", "setzt", "dir", "itzt", "die", "Lei\u00b7chen\u00b7B\u00e4n\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKANT", "$.", "PIS", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und legt den Rest der Glieder zu der Ruh.", "tokens": ["Und", "legt", "den", "Rest", "der", "Glie\u00b7der", "zu", "der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So gehet nun dein sehnliches Verlangen/", "tokens": ["So", "ge\u00b7het", "nun", "dein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach den Gew\u00fcrtz- und Freuden-Bergen hin.", "tokens": ["Nach", "den", "Ge\u00b7w\u00fcrtz", "und", "Freu\u00b7den\u00b7Ber\u00b7gen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Es mag dein Mund kein\u2019 Artzney mehr empfangen/", "tokens": ["Es", "mag", "dein", "Mund", "kein'", "Artz\u00b7ney", "mehr", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PIAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du nennst die Flucht den sch\u00e4tzbarsten Gewinn.", "tokens": ["Du", "nennst", "die", "Flucht", "den", "sch\u00e4tz\u00b7bars\u00b7ten", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gleichwie ein Hirsch/ der sicher auff den H\u00f6hen", "tokens": ["Gleich\u00b7wie", "ein", "Hirsch", "/", "der", "si\u00b7cher", "auff", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fccke siht/ wie man ihm nachgestellt.