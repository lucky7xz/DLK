{"dta.poem.12627": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  \n  Der Ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie unvergleichlich ist", "tokens": ["Wie", "un\u00b7ver\u00b7gleich\u00b7lich", "ist"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Sch\u00f6ne, die recht k\u00fc\u00dft!", "tokens": ["Die", "Sch\u00f6\u00b7ne", ",", "die", "recht", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ihren K\u00fcssen steckt", "tokens": ["In", "ih\u00b7ren", "K\u00fcs\u00b7sen", "steckt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was tausend Lust erweckt.", "tokens": ["Was", "tau\u00b7send", "Lust", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Mund gab die Natur", "tokens": ["Den", "Mund", "gab", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Uns nicht zur Sprache nur:", "tokens": ["Uns", "nicht", "zur", "Spra\u00b7che", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, was ihn s\u00fcsser macht,", "tokens": ["Das", ",", "was", "ihn", "s\u00fcs\u00b7ser", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist, da\u00df er k\u00fc\u00dft und lacht.", "tokens": ["Ist", ",", "da\u00df", "er", "k\u00fc\u00dft", "und", "lacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach \u00fcberzeuge dich", "tokens": ["Ach", "\u00fc\u00b7berz\u00b7eu\u00b7ge", "dich"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Davon, mein Kind! durch mich", "tokens": ["Da\u00b7von", ",", "mein", "Kind", "!", "durch", "mich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$.", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimm und gib im Ku\u00df", "tokens": ["Und", "nimm", "und", "gib", "im", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "KON", "VVIMP", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Freuden Ueberflu\u00df.", "tokens": ["Der", "Freu\u00b7den", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}