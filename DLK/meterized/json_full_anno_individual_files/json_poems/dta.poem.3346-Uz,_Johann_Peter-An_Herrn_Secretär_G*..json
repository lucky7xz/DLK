{"dta.poem.3346": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "O bleibt, ihr staubichten Pedanten!", "tokens": ["O", "bleibt", ",", "ihr", "stau\u00b7bich\u00b7ten", "Pe\u00b7dan\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Jhr unertr\u00e4glichen Citanten!", "tokens": ["Ihr", "un\u00b7er\u00b7tr\u00e4g\u00b7li\u00b7chen", "Ci\u00b7tan\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey euern lieben Folianten:", "tokens": ["Bey", "eu\u00b7ern", "lie\u00b7ben", "Fo\u00b7li\u00b7an\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was brauch ich den gelehrten Mist?", "tokens": ["Was", "brauch", "ich", "den", "ge\u00b7lehr\u00b7ten", "Mist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "D\u00fcrft ihr bey allen Lumpen-Dingen", "tokens": ["D\u00fcrft", "ihr", "bey", "al\u00b7len", "Lum\u00b7pen\u00b7Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Rom und Griechenland mich bey den Haaren", "tokens": ["Nach", "Rom", "und", "Grie\u00b7chen\u00b7land", "mich", "bey", "den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zwingen,", "tokens": ["zwin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Da, was ihr sucht, in Deutschland ist?", "tokens": ["Da", ",", "was", "ihr", "sucht", ",", "in", "Deutschland", "ist", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie? k\u00f6nnt ihr mich nicht \u00fcberf\u00fchren,", "tokens": ["Wie", "?", "k\u00f6nnt", "ihr", "mich", "nicht", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df viele Hahnrey sind, als wenn ihr griechisch flucht,", "tokens": ["Da\u00df", "vie\u00b7le", "Hahn\u00b7rey", "sind", ",", "als", "wenn", "ihr", "grie\u00b7chisch", "flucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und eure F\u00e4uste Rom citiren?", "tokens": ["Und", "eu\u00b7re", "F\u00e4us\u00b7te", "Rom", "ci\u00b7ti\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kehrt immer erst vor euern Th\u00fcren:", "tokens": ["Kehrt", "im\u00b7mer", "erst", "vor", "eu\u00b7ern", "Th\u00fc\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Jhr findet hier vielleicht, was ihr so ferne sucht.", "tokens": ["Ihr", "fin\u00b7det", "hier", "viel\u00b7leicht", ",", "was", "ihr", "so", "fer\u00b7ne", "sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}