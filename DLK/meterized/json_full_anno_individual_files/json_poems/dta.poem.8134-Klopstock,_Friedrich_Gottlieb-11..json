{"dta.poem.8134": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begleit\u2019 Jhn zum Thron auf, o Lichtheer,", "tokens": ["Be\u00b7gleit'", "Jhn", "zum", "Thron", "auf", ",", "o", "Licht\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der Harf\u2019 Jhn, der Posaun\u2019hall, und dem Chorpsalm,", "tokens": ["Mit", "der", "Ha\u00b7rf'", "Jhn", ",", "der", "Po\u00b7saun'\u00b7hall", ",", "und", "dem", "Chor\u00b7psalm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jesus, Gottes Sohn! Menschlich ist Er!", "tokens": ["Je\u00b7sus", ",", "Got\u00b7tes", "Sohn", "!", "Menschlich", "ist", "Er", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NN", "$.", "NE", "VAFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gn\u00e4dig! das rufest du laut, blutiger Altar!", "tokens": ["Gn\u00e4\u00b7dig", "!", "das", "ru\u00b7fest", "du", "laut", ",", "blu\u00b7ti\u00b7ger", "Al\u00b7tar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "PDS", "VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}