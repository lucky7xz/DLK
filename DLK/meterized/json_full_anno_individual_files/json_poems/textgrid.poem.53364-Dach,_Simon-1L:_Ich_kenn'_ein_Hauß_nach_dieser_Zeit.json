{"textgrid.poem.53364": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kenn' ein Hau\u00df nach dieser Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn' ein Hau\u00df nach dieser Zeit", "tokens": ["Ich", "kenn'", "ein", "Hau\u00df", "nach", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das heist die selig' Ewigheit,", "tokens": ["Das", "heist", "die", "se\u00b7lig'", "E\u00b7wig\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Vater-Stadt der Frommen,", "tokens": ["Die", "Va\u00b7ter\u00b7Stadt", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In welche sie", "tokens": ["In", "wel\u00b7che", "sie"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Aus aller M\u00fch", "tokens": ["Aus", "al\u00b7ler", "M\u00fch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nach diesem Leben kommen.", "tokens": ["Nach", "die\u00b7sem", "Le\u00b7ben", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie darff des Sonnen-Scheines nicht,", "tokens": ["Sie", "darff", "des", "Son\u00b7nen\u00b7Sch