 "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sie von Hertzen liebet", "tokens": ["Der", "sie", "von", "Hert\u00b7zen", "lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als seine Zier", "tokens": ["Als", "sei\u00b7ne", "Zier"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vnd gantz sich jhr", "tokens": ["Vnd", "gantz", "sich", "jhr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PRF", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Selbs zu geniessen giebet.", "tokens": ["Selbs", "zu", "ge\u00b7nies\u00b7sen", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr allerbestes Ehren-Kleid", "tokens": ["Ihr", "al\u00b7ler\u00b7bes\u00b7tes", "Eh\u00b7ren\u00b7Kleid"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Vnschuld und Gerechtigheit,", "tokens": ["Ist", "Vn\u00b7schuld", "und", "Ge\u00b7rech\u00b7tig\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur hiemit wil sie prangen,", "tokens": ["Nur", "hie\u00b7mit", "wil", "sie", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies ist ihr gnug,", "tokens": ["Dies", "ist", "ihr", "gnug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Kein ander Schmuck", "tokens": ["Kein", "an\u00b7der", "Schmuck"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Kann jhren Liebsten fangen.", "tokens": ["Kann", "jhren", "Liebs\u00b7ten", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Vnd also wird das Hochzeit-mal", "tokens": ["Vnd", "al\u00b7so", "wird", "das", "Hoch\u00b7zeit\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch in der Ewigkeiten-Sal", "tokens": ["Hoch", "in", "der", "E\u00b7wig\u00b7kei\u00b7ten\u00b7Sal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit h\u00f6chster Pracht gehalten,", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "Pracht", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da Spiel, Gesang", "tokens": ["Da", "Spiel", ",", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "NN", "$,", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vnd Seiten-Klang", "tokens": ["Vnd", "Sei\u00b7ten\u00b7Klang"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vnd Frewden ewig walten.", "tokens": ["Vnd", "Frew\u00b7den", "e\u00b7wig", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Engel selbs sind hoch erfrewt", "tokens": ["Die", "En\u00b7gel", "selbs", "sind", "hoch", "er\u00b7frewt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von wegen unsrer Seligkeit,", "tokens": ["Von", "we\u00b7gen", "uns\u00b7rer", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wiederf\u00e4hrt den Frommen", "tokens": ["Da", "wie\u00b7der\u00b7f\u00e4hrt", "den", "From\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was nimmermehr", "tokens": ["Was", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word"], "pos": ["PWS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vns zu Geh\u00f6r", "tokens": ["Vns", "zu", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Noch in das Hertz ist kommen.", "tokens": ["Noch", "in", "das", "Hertz", "ist", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da sehn' ich hertzlich mich hinauff,", "tokens": ["Da", "sehn'", "ich", "hertz\u00b7lich", "mich", "hin\u00b7auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer endet mir den Lebens-Lauff?", "tokens": ["Wer", "en\u00b7det", "mir", "den", "Le\u00b7bens\u00b7Lauff", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wil befreyet werden:", "tokens": ["Ich", "wil", "be\u00b7fre\u00b7yet", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, Jesu, bald,", "tokens": ["Komm", ",", "Je\u00b7su", ",", "bald", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Auffenthalt,", "tokens": ["Mein", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vnd nimm mich von der Erden!", "tokens": ["Vnd", "nimm", "mich", "von", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch mach mich von der S\u00fcnd erst rein,", "tokens": ["Doch", "mach", "mich", "von", "der", "S\u00fcnd", "erst", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur diese k\u00f6mpt bey dir nicht ein:", "tokens": ["Nur", "die\u00b7se", "k\u00f6mpt", "bey", "dir", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "APPR", "PPE