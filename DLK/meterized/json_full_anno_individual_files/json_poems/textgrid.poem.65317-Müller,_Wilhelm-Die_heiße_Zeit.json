{"textgrid.poem.65317": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die hei\u00dfe Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ach, nun sind vertrocknet alle Quellen!", "tokens": ["Ach", ",", "ach", ",", "nun", "sind", "ver\u00b7trock\u00b7net", "al\u00b7le", "Quel\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "ADV", "VAFIN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo soll mein L\u00e4mmchen seinen Durst denn stillen,", "tokens": ["Wo", "soll", "mein", "L\u00e4mm\u00b7chen", "sei\u00b7nen", "Durst", "denn", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ihm am Gaum die hei\u00dfen Gr\u00e4ser brennen?", "tokens": ["Wenn", "ihm", "am", "Gaum", "die", "hei\u00b7\u00dfen", "Gr\u00e4\u00b7ser", "bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, ach, nun sitzt mein M\u00e4dchen in der Kammer!", "tokens": ["Ach", ",", "ach", ",", "nun", "sitzt", "mein", "M\u00e4d\u00b7chen", "in", "der", "Kam\u00b7mer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.