{"textgrid.poem.24135": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh ja, die Liebe und ein treues Herz,", "tokens": ["Oh", "ja", ",", "die", "Lie\u00b7be", "und", "ein", "treu\u00b7es", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und alles, was wir Seele nennen, ist viel wert.", "tokens": ["Und", "al\u00b7les", ",", "was", "wir", "See\u00b7le", "nen\u00b7nen", ",", "ist", "viel", "wert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "NN", "VVINF", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch davon wollen wir nicht reden, Kind,", "tokens": ["Doch", "da\u00b7von", "wol\u00b7len", "wir", "nicht", "re\u00b7den", ",", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wollen keine Fesseln hin und her", "tokens": ["Und", "wol\u00b7len", "kei\u00b7ne", "Fes\u00b7seln", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIAT", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Herz zu Herzen binden, und das Wort:", "tokens": ["Von", "Herz", "zu", "Her\u00b7zen", "bin\u00b7den", ",", "und", "das", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbauf immer\u00ab spanne seine F\u00e4den nicht", "tokens": ["\u00bb", "auf", "im\u00b7mer", "\u00ab", "span\u00b7ne", "sei\u00b7ne", "F\u00e4\u00b7den", "nicht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ADV", "$(", "VVFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von dir zu mir. Uns sei der Tag genug,", "tokens": ["Von", "dir", "zu", "mir", ".", "Uns", "sei", "der", "Tag", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die stille Stunde, die uns gl\u00fccklich macht.", "tokens": ["Die", "stil\u00b7le", "Stun\u00b7de", ",", "die", "uns", "gl\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dann wird, wenn einst auch das vergangen ist,", "tokens": ["Dann", "wird", ",", "wenn", "einst", "auch", "das", "ver\u00b7gan\u00b7gen", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ADV", "ADV", "PDS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Uns keine L\u00fcge die Erinnerung", "tokens": ["Uns", "kei\u00b7ne", "L\u00fc\u00b7ge", "die", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "wo