{"textgrid.poem.27478": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Apollos Bilds\u00e4ule in einem gewissen Gartentempel", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Linken regiert er die Leier; wen nimmt es noch wunder,", "tokens": ["Mit", "der", "Lin\u00b7ken", "re\u00b7giert", "er", "die", "Lei\u00b7er", ";", "wen", "nimmt", "es", "noch", "wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Da\u00df er in diesem Revier immer so linkisch gespielt?", "tokens": ["Da\u00df", "er", "in", "die\u00b7sem", "Re\u00b7vier", "im\u00b7mer", "so", "lin\u00b7kisch", "ge\u00b7spielt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Was mit gl\u00fchendem Ernst die liebende Seele gebildet,", "tokens": ["Was", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Ernst", "die", "lie\u00b7ben\u00b7de", "See\u00b7le", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Reizte dich nicht; dich reizt, Leser, mein Kobold allein.", "tokens": ["Reiz\u00b7te", "dich", "nicht", ";", "dich", "reizt", ",", "Le\u00b7ser", ",", "mein", "Ko\u00b7bold", "al\u00b7lein", "."], "token_info": ["wor