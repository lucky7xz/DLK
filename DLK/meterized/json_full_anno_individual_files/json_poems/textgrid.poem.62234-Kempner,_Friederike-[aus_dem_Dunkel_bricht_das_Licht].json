{"textgrid.poem.62234": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[aus dem Dunkel bricht das Licht]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Dunkel bricht das Licht,", "tokens": ["Aus", "dem", "Dun\u00b7kel", "bricht", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu erstrahlet mir die Welt,", "tokens": ["Neu", "er\u00b7strah\u00b7let", "mir", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und versto\u00dfen bin ich nicht,", "tokens": ["Und", "ver\u00b7sto\u00b7\u00dfen", "bin", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott, aus Deines Lichtes Zelt!", "tokens": ["Gott", ",", "aus", "Dei\u00b7nes", "Lich\u00b7tes", "Zelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welche Wonne, welches Gl\u00fcck,", "tokens": ["Wel\u00b7che", "Won\u00b7ne", ",", "wel\u00b7ches", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher Jubel kehrt zur\u00fcck!", "tokens": ["Wel\u00b7cher", "Ju\u00b7bel", "kehrt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einzig Gl\u00fcck wohnt nur im Licht,", "tokens": ["Ein\u00b7zig", "Gl\u00fcck", "wohnt", "nur", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott, ich lese ein Gedicht.", "tokens": ["Gott", ",", "ich", "le\u00b7se", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus dem Dunkel bricht das Licht,", "tokens": ["Aus", "dem", "Dun\u00b7kel", "bricht", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu erstrahlet mir die Welt,", "tokens": ["Neu", "er\u00b7strah\u00b7let", "mir", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und versto\u00dfen bin ich nicht,", "tokens": ["Und", "ver\u00b7sto\u00b7\u00dfen", "bin", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott, aus Deines Lichtes Zelt!", "tokens": ["Gott", ",", "aus", "Dei\u00b7nes", "Lich\u00b7tes", "Zelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "st