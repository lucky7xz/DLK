{"textgrid.poem.54218": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie hart ist nicht der Schlag, der unsre Hertzen trifft,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hart ist nicht der Schlag, der unsre Hertzen trifft,", "tokens": ["Wie", "hart", "ist", "nicht", "der", "Schlag", ",", "der", "uns\u00b7re", "Hert\u00b7zen", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist wohl in der Welt, das uns mehr Kummer stifft?", "tokens": ["Was", "ist", "wohl", "in", "der", "Welt", ",", "das", "uns", "mehr", "Kum\u00b7mer", "stifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als di\u00df, da\u00df man Dich soll, entflohner Printz, vermissen,", "tokens": ["Als", "di\u00df", ",", "da\u00df", "man", "Dich", "soll", ",", "ent\u00b7floh\u00b7ner", "Printz", ",", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "KOUS", "PIS", "PPER", "VMFIN", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wir nicht mehr den Saum von deinen Purpur k\u00fcssen;", "tokens": ["Und", "wir", "nicht", "mehr", "den", "Saum", "von", "dei\u00b7nen", "Pur\u00b7pur", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr B\u00e4ume! zeigt mit uns zugleich auch euer Leid,", "tokens": ["Ihr", "B\u00e4u\u00b7me", "!", "zeigt", "mit", "uns", "zu\u00b7gleich", "auch", "eu\u00b7er", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "APPR", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was nutzet euch nunmehr das gr\u00fcne Feyer-Kleid,", "tokens": ["Was", "nut\u00b7zet", "euch", "nun\u00b7mehr", "das", "gr\u00fc\u00b7ne", "Feyer\u00b7Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Worinnen man bi\u00dfher mit Lust auch sahe prangen,", "tokens": ["Wo\u00b7rin\u00b7nen", "man", "bi\u00df\u00b7her", "mit", "Lust", "auch", "sa\u00b7he", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Es schickt sich selbiges nicht zu bethr\u00e4nten Wangen.", "tokens": ["Es", "schickt", "sich", "sel\u00b7bi\u00b7ges", "nicht", "zu", "be\u00b7thr\u00e4n\u00b7ten", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bebl\u00fchmte Wiesen! legt das Schmeltzwerck wieder ab,", "tokens": ["Be\u00b7bl\u00fchm\u00b7te", "Wie\u00b7sen", "!", "legt", "das", "Sch\u00b7melt\u00b7zwerck", "wie\u00b7der", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "Das euch der Floren Hand aus ihren Schatz-Hau\u00df gab,", "tokens": ["Das", "euch", "der", "Flo\u00b7ren", "Hand", "aus", "ih\u00b7ren", "Schatz\u00b7Hau\u00df", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "La\u00df, fahler Pleissen-Strand, durch mattes Rauschen h\u00f6ren,", "tokens": ["La\u00df", ",", "fah\u00b7ler", "Pleis\u00b7sen\u00b7Strand", ",", "durch", "mat\u00b7tes", "Rau\u00b7schen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df deine Wellen sich in Thr\u00e4nen-Saltz verkehren;", "tokens": ["Da\u00df", "dei\u00b7ne", "Wel\u00b7len", "sich", "in", "Thr\u00e4\u00b7nen\u00b7Saltz", "ver\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Musen, die ihr ihm, so bald er angelangt,", "tokens": ["Ihr", "Mu\u00b7sen", ",", "die", "ihr", "ihm", ",", "so", "bald", "er", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "$,", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein frohes Jubel-Lied auf euren H\u00e4ynen sangt,", "tokens": ["Ein", "fro\u00b7hes", "Ju\u00b7bel\u00b7Lied", "auf", "eu\u00b7ren", "H\u00e4y\u00b7nen", "sangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "H\u00e4ngt Harff und Zitter auf, werfft eure Fl\u00f6ten nieder,", "tokens": ["H\u00e4ngt", "Harff", "und", "Zit\u00b7ter", "auf", ",", "werfft", "eu\u00b7re", "Fl\u00f6\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und singt bey seiner Flucht nur eitel Klage-Lieder.", "tokens": ["Und", "singt", "bey", "sei\u00b7ner", "Flucht", "nur", "ei\u00b7tel", "Kla\u00b7ge\u00b7Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Seitdem, verrei\u00dfter Printz, hier unsre Linden-Stadt,", "tokens": ["Seit\u00b7dem", ",", "ver\u00b7rei\u00df\u00b7ter", "Printz", ",", "hier", "uns\u00b7re", "Lin\u00b7den\u00b7Stadt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von Deiner Herrlichkeit den Strahl vermisset hat,", "tokens": ["Von", "Dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "den", "Strahl", "ver\u00b7mis\u00b7set", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sieht alles dunckel aus, der Mauern \u00f6des Wesen,", "tokens": ["Sieht", "al\u00b7les", "dun\u00b7ckel", "aus", ",", "der", "Mau\u00b7ern", "\u00f6\u00b7des", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "L\u00e4st uns ein Ebenbild von W\u00fcsteneyen lesen;", "tokens": ["L\u00e4st", "uns", "ein", "E\u00b7ben\u00b7bild", "von", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Sonne h\u00fcllet sich in tr\u00fcbe Wolcken ein,", "tokens": ["Die", "Son\u00b7ne", "h\u00fcl\u00b7let", "sich", "in", "tr\u00fc\u00b7be", "Wol\u00b7cken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Deiner Nachbarschafft hier soll beraubet seyn,", "tokens": ["Die", "Dei\u00b7ner", "Nach\u00b7bar\u00b7schafft", "hier", "soll", "be\u00b7rau\u00b7bet", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VMFIN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und Luna, die den Glantz von Deinen lichten Orden", "tokens": ["Und", "Lu\u00b7na", ",", "die", "den", "Glantz", "von", "Dei\u00b7nen", "lich\u00b7ten", "Or\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Verliehrt, ist bla\u00df und bleich vor K\u00fcmmerni\u00df geworden.", "tokens": ["Ver\u00b7liehrt", ",", "ist", "bla\u00df", "und", "bleich", "vor", "K\u00fcm\u00b7mer\u00b7ni\u00df", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wir stellen uns mit Schmertz die Treflichkeit von Dir,", "tokens": ["Wir", "stel\u00b7len", "uns", "mit", "Schmertz", "die", "Tref\u00b7lich\u00b7keit", "von", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und Deine Seltenheit noch augenblicklich f\u00fcr;", "tokens": ["Und", "Dei\u00b7ne", "Sel\u00b7ten\u00b7heit", "noch", "au\u00b7gen\u00b7blick\u00b7lich", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein jeder, welcher Dich und Deinen Geist erblicket,", "tokens": ["Ein", "je\u00b7der", ",", "wel\u00b7cher", "Dich", "und", "Dei\u00b7nen", "Geist", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hat sich Dein Bild recht tieff in Seel und Hertz gedr\u00fccket,", "tokens": ["Hat", "sich", "Dein", "Bild", "recht", "tieff", "in", "Seel", "und", "Hertz", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bewundre solches nicht, warum gleich aller Welt", "tokens": ["Be\u00b7wund\u00b7re", "sol\u00b7ches", "nicht", ",", "wa\u00b7rum", "gleich", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "PWAV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dein mehr als pr\u00e4chtger Strahl in die Gedancken f\u00e4llt,", "tokens": ["Dein", "mehr", "als", "pr\u00e4cht\u00b7ger", "Strahl", "in", "die", "Ge\u00b7dan\u00b7cken", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "KOKOM", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es reitzen uns darzu die G\u00f6tter gleichen Gaben,", "tokens": ["Es", "reit\u00b7zen", "uns", "dar\u00b7zu", "die", "G\u00f6t\u00b7ter", "glei\u00b7chen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vor die man, Grosser Printz, mu\u00df Lieb und Ehrfurcht haben;", "tokens": ["Vor", "die", "man", ",", "Gros\u00b7ser", "Printz", ",", "mu\u00df", "Lieb", "und", "Ehr\u00b7furcht", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "$,", "ADJA", "NN", "$,", "VMFIN", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jedoch was schildern wir zu desto gr\u00f6\u00dfrer Pein", "tokens": ["Je\u00b7doch", "was", "schil\u00b7dern", "wir", "zu", "des\u00b7to", "gr\u00f6\u00df\u00b7rer", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Uns Deine Sch\u00f6hnheit ab? Da Deines Purpurs Schein", "tokens": ["Uns", "Dei\u00b7ne", "Sch\u00f6hn\u00b7heit", "ab", "?", "Da", "Dei\u00b7nes", "Pur\u00b7purs", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$.", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Nach andern V\u00f6lckern sich bedaurens-werth will lencken,", "tokens": ["Nach", "an\u00b7dern", "V\u00f6l\u00b7ckern", "sich", "be\u00b7dau\u00b7rens\u00b7werth", "will", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Woran wir wahrlich recht mit Eifersucht gedencken,", "tokens": ["Wo\u00b7ran", "wir", "wahr\u00b7lich", "recht", "mit", "Ei\u00b7fer\u00b7sucht", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ihr Pierinnen kommt, hier ist der Linden-Plan,", "tokens": ["Ihr", "Pie\u00b7rin\u00b7nen", "kommt", ",", "hier", "ist", "der", "Lin\u00b7den\u00b7Plan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.38": {"text": "Erw\u00e4hlt den sch\u00f6nsten Baum, schneidt seinen Nahmen an,", "tokens": ["Er\u00b7w\u00e4hlt", "den", "sch\u00f6ns\u00b7ten", "Baum", ",", "schneidt", "sei\u00b7nen", "Nah\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Damit noch dermahleinst die sp\u00e4the Welt mag lesen,", "tokens": ["Da\u00b7mit", "noch", "der\u00b7mah\u00b7le\u00b7inst", "die", "sp\u00e4\u00b7the", "Welt", "mag", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "Wie schmertzlich, leider uns! sein Abschied sey gewesen.", "tokens": ["Wie", "schmertz\u00b7lich", ",", "lei\u00b7der", "uns", "!", "sein", "Ab\u00b7schied", "sey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADV", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie hart ist nicht der Schlag, der unsre Hertzen trifft,", "tokens": ["Wie", "hart", "ist", "nicht", "der", "Schlag", ",", "der", "uns\u00b7re", "Hert\u00b7zen", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist wohl in der Welt, das uns mehr Kummer stifft?", "tokens": ["Was", "ist", "wohl", "in", "der", "Welt", ",", "das", "uns", "mehr", "Kum\u00b7mer", "stifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als di\u00df, da\u00df man Dich soll, entflohner Printz, vermissen,", "tokens": ["Als", "di\u00df", ",", "da\u00df", "man", "Dich", "soll", ",", "ent\u00b7floh\u00b7ner", "Printz", ",", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "KOUS", "PIS", "PPER", "VMFIN", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wir nicht mehr den Saum von deinen Purpur k\u00fcssen;", "tokens": ["Und", "wir", "nicht", "mehr", "den", "Saum", "von", "dei\u00b7nen", "Pur\u00b7pur", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+