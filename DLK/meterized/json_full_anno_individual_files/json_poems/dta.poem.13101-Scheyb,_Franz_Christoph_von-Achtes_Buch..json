{"dta.poem.13101": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edas Volck wies mit der Hand, und gaffte mit dem Mund,", "tokens": ["\u201e", "das", "Volck", "wies", "mit", "der", "Hand", ",", "und", "gaff\u00b7te", "mit", "dem", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edas Wunder war zu gro\u00df, das ihm vor Augen stund,", "tokens": ["\u201e", "das", "Wun\u00b7der", "war", "zu", "gro\u00df", ",", "das", "ihm", "vor", "Au\u00b7gen", "stund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eals es des Himmels Krei\u00df, das weite Land gesehen,", "tokens": ["\u201e", "als", "es", "des", "Him\u00b7mels", "Krei\u00df", ",", "das", "wei\u00b7te", "Land", "ge\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "360\u201dWie sich der Sonne Licht, der Mond, die Sterne drehen.", "tokens": ["\"", "Wie", "sich", "der", "Son\u00b7ne", "Licht", ",", "der", "Mond", ",", "die", "Ster\u00b7ne", "dre\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PRF", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewer bracht ihm dazumahl den Grund der Wahrheit bey?", "tokens": ["\u201e", "wer", "bracht", "ihm", "da\u00b7zu\u00b7mahl", "den", "Grund", "der", "Wahr\u00b7heit", "bey", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewer lehrt\u2019 es, was die Welt, der Mensch, der Himmel sey?", "tokens": ["\u201e", "wer", "lehrt'", "es", ",", "was", "die", "Welt", ",", "der", "Mensch", ",", "der", "Him\u00b7mel", "sey", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie Gabe meiner Kunst erkl\u00e4rte solche Fragen:", "tokens": ["\u201e", "die", "Ga\u00b7be", "mei\u00b7ner", "Kunst", "er\u00b7kl\u00e4r\u00b7te", "sol\u00b7che", "Fra\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eich wu\u00dfte dem Geschlecht den Spiegel vorzutragen,", "tokens": ["\u201e", "ich", "wu\u00df\u00b7te", "dem", "Ge\u00b7schlecht", "den", "Spie\u00b7gel", "vor\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "365\u201dIn welchem es ersah, wie sich der Mond bewegt;", "tokens": ["\"", "In", "wel\u00b7chem", "es", "er\u00b7sah", ",", "wie", "sich", "der", "Mond", "be\u00b7wegt", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewie durch der Sonne Lauf die Zeit zu wechseln pflegt;", "tokens": ["\u201e", "wie", "durch", "der", "Son\u00b7ne", "Lauf", "die", "Zeit", "zu", "wech\u00b7seln", "pflegt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewie sich Natur und Kunst zu seinem Wohl bem\u00fchen;", "tokens": ["\u201e", "wie", "sich", "Na\u00b7tur", "und", "Kunst", "zu", "sei\u00b7nem", "Wohl", "be\u00b7m\u00fc\u00b7hen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PRF", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewie Menschen aus dem Grund die Lebens-Nahrung ziehen.", "tokens": ["\u201e", "wie", "Men\u00b7schen", "aus", "dem", "Grund", "die", "Le\u00b7bens\u00b7Nah\u00b7rung", "zie\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}