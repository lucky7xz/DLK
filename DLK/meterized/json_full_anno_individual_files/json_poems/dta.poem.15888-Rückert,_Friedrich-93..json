{"dta.poem.15888": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Einsicht schadet nur Gelehrsamkeit zu gro\u00dfe,", "tokens": ["Der", "Ein\u00b7sicht", "scha\u00b7det", "nur", "Ge\u00b7lehr\u00b7sam\u00b7keit", "zu", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besser als Brillen sieht gesunder Sinn der blo\u00dfe.", "tokens": ["Bes\u00b7ser", "als", "Bril\u00b7len", "sieht", "ge\u00b7sun\u00b7der", "Sinn", "der", "blo\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVFIN", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Hast du erst nachgesehn, wie die Ausleger es", "tokens": ["Hast", "du", "erst", "nach\u00b7ge\u00b7sehn", ",", "wie", "die", "Aus\u00b7le\u00b7ger", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,", "PWAV", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verstehn, so bist du blind; sieh selber und verstehs!", "tokens": ["Ver\u00b7stehn", ",", "so", "bist", "du", "blind", ";", "sieh", "sel\u00b7ber", "und", "ver\u00b7stehs", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$.", "VVIMP", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "meas