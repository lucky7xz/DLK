{"textgrid.poem.49169": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00dflied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster Jesu, Trost der Herzen,", "tokens": ["Liebs\u00b7ter", "Je\u00b7su", ",", "Trost", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denen ihre S\u00fcnden schmertzen,", "tokens": ["De\u00b7nen", "ih\u00b7re", "S\u00fcn\u00b7den", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einigs Heil der gantzen Welt!", "tokens": ["Ei\u00b7nigs", "Heil", "der", "gant\u00b7zen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich bei mir \u00fcberschlage", "tokens": ["Wenn", "ich", "bei", "mir", "\u00fc\u00b7bersc\u00b7hla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was f\u00fcr Schuld ich auf mir trage,", "tokens": ["Was", "f\u00fcr", "Schuld", "ich", "auf", "mir", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dein Buch verzeichnet h\u00e4lt,", "tokens": ["Die", "dein", "Buch", "ver\u00b7zeich\u00b7net", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, so kann ich nicht ersehen,", "tokens": ["Ach", ",", "so", "kann", "ich", "nicht", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ich k\u00fcnftig soll bestehen.", "tokens": ["Wie", "ich", "k\u00fcnf\u00b7tig", "soll", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Geburt bin ich ein S\u00fcnder,", "tokens": ["Von", "Ge\u00b7burt", "bin", "ich", "ein", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wegen meines Thuns nicht minder,", "tokens": ["We\u00b7gen", "mei\u00b7nes", "Thuns", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg ist meiner Seelen Zier,", "tokens": ["Weg", "ist", "mei\u00b7ner", "See\u00b7len", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Bild hab' ich ganz verloren,", "tokens": ["Dein", "Bild", "hab'", "ich", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und des Teufels Bild erkoren,", "tokens": ["Und", "des", "Teu\u00b7fels", "Bild", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Nichtes ist mehr gut an mir,", "tokens": ["Nich\u00b7tes", "ist", "mehr", "gut", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch noch alle Tag' und Stunden", "tokens": ["Auch", "noch", "al\u00b7le", "Tag'", "und", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mach' ich gr\u00f6sser meine Wunden.", "tokens": ["Mach'", "ich", "gr\u00f6s\u00b7ser", "mei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herr, wo soll ich mich hinwenden?", "tokens": ["Herr", ",", "wo", "soll", "ich", "mich", "hin\u00b7wen\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Zorn dr\u00fcckt mich aller Enden,", "tokens": ["Dein", "Zorn", "dr\u00fcckt", "mich", "al\u00b7ler", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In mir hab' ich keine Ruh;", "tokens": ["In", "mir", "hab'", "ich", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mich ist der Welt Get\u00fcmmel,", "tokens": ["Um", "mich", "ist", "der", "Welt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oben schrecket mich der Himmel,", "tokens": ["O\u00b7ben", "schre\u00b7cket", "mich", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Satan spricht den Fluch dazu,", "tokens": ["Sa\u00b7tan", "spricht", "den", "Fluch", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mich d\u00fcnkt, da\u00df auch die Erde", "tokens": ["Und", "mich", "d\u00fcnkt", ",", "da\u00df", "auch", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber mich schon Kl\u00e4ger werde.", "tokens": ["\u00dc\u00b7ber", "mich", "schon", "Kl\u00e4\u00b7ger", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, du frommer S\u00fcnden-B\u00fcsser,", "tokens": ["Ach", ",", "du", "from\u00b7mer", "S\u00fcn\u00b7den\u00b7B\u00fcs\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du getreuster Blutvergiesser,", "tokens": ["Du", "ge\u00b7treus\u00b7ter", "Blut\u00b7ver\u00b7gies\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jesu, komm' mir hie zu statt,", "tokens": ["Je\u00b7su", ",", "komm'", "mir", "hie", "zu", "statt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei mein' H\u00fclf' und mein Berather,", "tokens": ["Sei", "mein'", "H\u00fcl\u00b7f'", "und", "mein", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sprich mich los bey deinem Vater", "tokens": ["Sprich", "mich", "los", "bey", "dei\u00b7nem", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und tilg' aus mein' \u00dcbelthat;", "tokens": ["Und", "til\u00b7g'", "aus", "mein'", "\u00dc\u00b7belt\u00b7hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hast du vor mich wollen b\u00fcssen,", "tokens": ["Hast", "du", "vor", "mich", "wol\u00b7len", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So lass mich auch de\u00df geniessen.", "tokens": ["So", "lass", "mich", "auch", "de\u00df", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Zwar ich muss es gern gestehen,", "tokens": ["Zwar", "ich", "muss", "es", "gern", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich gr\u00f6blich mich versehen", "tokens": ["Da\u00df", "ich", "gr\u00f6b\u00b7lich", "mich", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verdient der H\u00f6llen Lohn.", "tokens": ["Und", "ver\u00b7dient", "der", "H\u00f6l\u00b7len", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber sieh', ich komme wieder,", "tokens": ["A\u00b7ber", "sieh'", ",", "ich", "kom\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fall', o Jesu, vor dir nieder,", "tokens": ["Fall'", ",", "o", "Je\u00b7su", ",", "vor", "dir", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NE", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ich dein l\u00e4ngst verlorner Sohn,", "tokens": ["Ich", "dein", "l\u00e4ngst", "ver\u00b7lor\u00b7ner", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ersuche dich mit Thr\u00e4nen,", "tokens": ["Und", "er\u00b7su\u00b7che", "dich", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df dich wiederum vers\u00f6hnen.", "tokens": ["La\u00df", "dich", "wie\u00b7de\u00b7rum", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist im Himmel gr\u00f6sser Freude,", "tokens": ["Ist", "im", "Him\u00b7mel", "gr\u00f6s\u00b7ser", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein S\u00fcnder kehrt mit Leide", "tokens": ["Wenn", "ein", "S\u00fcn\u00b7der", "kehrt", "mit", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder zu der rechten Bahn,", "tokens": ["Wie\u00b7der", "zu", "der", "rech\u00b7ten", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als um neun und neunzig Fromme,", "tokens": ["Als", "um", "neun", "und", "neun\u00b7zig", "From\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, so nimm mich, da ich komme,", "tokens": ["Herr", ",", "so", "nimm", "mich", ",", "da", "ich", "kom\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiederum zu Gnaden an;", "tokens": ["Wie\u00b7de\u00b7rum", "zu", "Gna\u00b7den", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ich bis hieher begangen,", "tokens": ["Was", "ich", "bis", "hie\u00b7her", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich an zu bessern fangen.", "tokens": ["Will", "ich", "an", "zu", "bes\u00b7sern", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weicht hinfort, ihr S\u00fcnden-Geister,", "tokens": ["Weicht", "hin\u00b7fort", ",", "ihr", "S\u00fcn\u00b7den\u00b7Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn mein einz'ger Seelen-Meister,", "tokens": ["Denn", "mein", "einz'\u00b7ger", "See\u00b7len\u00b7Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jesus, wohnt allein bei mir;", "tokens": ["Je\u00b7sus", ",", "wohnt", "al\u00b7lein", "bei", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohn' ihn w\u00e4r' ich ganz verloren,", "tokens": ["Ohn'", "ihn", "w\u00e4r'", "ich", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ihn bin ich neu geboren,", "tokens": ["Durch", "ihn", "bin", "ich", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ihm leb' ich f\u00fcr und f\u00fcr,", "tokens": ["In", "ihm", "leb'", "ich", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit ihm geh' ich aus der Erden", "tokens": ["Mit", "ihm", "geh'", "ich", "aus", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und werd' ewig selig werden.", "tokens": ["Und", "werd'", "e\u00b7wig", "se\u00b7lig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebster Jesu, Trost der Herzen,", "tokens": ["Liebs\u00b7ter", "Je\u00b7su", ",", "Trost", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denen ihre S\u00fcnden schmertzen,", "tokens": ["De\u00b7nen", "ih\u00b7re", "S\u00fcn\u00b7den", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einigs Heil der gantzen Welt!", "tokens": ["Ei\u00b7nigs", "Heil", "der", "gant\u00b7zen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich bei mir \u00fcberschlage", "tokens": ["Wenn", "ich", "bei", "mir", "\u00fc\u00b7bersc\u00b7hla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was f\u00fcr Schuld ich auf mir trage,", "tokens": ["Was", "f\u00fcr", "Schuld", "ich", "auf", "mir", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dein Buch verzeichnet h\u00e4lt,", "tokens": ["Die", "dein", "Buch", "ver\u00b7zeich\u00b7net", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, so kann ich nicht ersehen,", "tokens": ["Ach", ",", "so", "kann", "ich", "nicht", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ich k\u00fcnftig soll bestehen.", "tokens": ["Wie", "ich", "k\u00fcnf\u00b7tig", "soll", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Von Geburt bin ich ein S\u00fcnder,", "tokens": ["Von", "Ge\u00b7burt", "bin", "ich", "ein", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wegen meines Thuns nicht minder,", "tokens": ["We\u00b7gen", "mei\u00b7nes", "Thuns", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg ist meiner Seelen Zier,", "tokens": ["Weg", "ist", "mei\u00b7ner", "See\u00b7len", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Bild hab' ich ganz verloren,", "tokens": ["Dein", "Bild", "hab'", "ich", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und des Teufels Bild erkoren,", "tokens": ["Und", "des", "Teu\u00b7fels", "Bild", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Nichtes ist mehr gut an mir,", "tokens": ["Nich\u00b7tes", "ist", "mehr", "gut", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch noch alle Tag' und Stunden", "tokens": ["Auch", "noch", "al\u00b7le", "Tag'", "und", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mach' ich gr\u00f6sser meine Wunden.", "tokens": ["Mach'", "ich", "gr\u00f6s\u00b7ser", "mei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Herr, wo soll ich mich hinwenden?", "tokens": ["Herr", ",", "wo", "soll", "ich", "mich", "hin\u00b7wen\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Zorn dr\u00fcckt mich aller Enden,", "tokens": ["Dein", "Zorn", "dr\u00fcckt", "mich", "al\u00b7ler", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In mir hab' ich keine Ruh;", "tokens": ["In", "mir", "hab'", "ich", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mich ist der Welt Get\u00fcmmel,", "tokens": ["Um", "mich", "ist", "der", "Welt", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oben schrecket mich der Himmel,", "tokens": ["O\u00b7ben", "schre\u00b7cket", "mich", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Satan spricht den Fluch dazu,", "tokens": ["Sa\u00b7tan", "spricht", "den", "Fluch", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mich d\u00fcnkt, da\u00df auch die Erde", "tokens": ["Und", "mich", "d\u00fcnkt", ",", "da\u00df", "auch", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber mich schon Kl\u00e4ger werde.", "tokens": ["\u00dc\u00b7ber", "mich", "schon", "Kl\u00e4\u00b7ger", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, du frommer S\u00fcnden-B\u00fcsser,", "tokens": ["Ach", ",", "du", "from\u00b7mer", "S\u00fcn\u00b7den\u00b7B\u00fcs\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du getreuster Blutvergiesser,", "tokens": ["Du", "ge\u00b7treus\u00b7ter", "Blut\u00b7ver\u00b7gies\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jesu, komm' mir hie zu statt,", "tokens": ["Je\u00b7su", ",", "komm'", "mir", "hie", "zu", "statt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei mein' H\u00fclf' und mein Berather,", "tokens": ["Sei", "mein'", "H\u00fcl\u00b7f'", "und", "mein", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sprich mich los bey deinem Vater", "tokens": ["Sprich", "mich", "los", "bey", "dei\u00b7nem", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und tilg' aus mein' \u00dcbelthat;", "tokens": ["Und", "til\u00b7g'", "aus", "mein'", "\u00dc\u00b7belt\u00b7hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hast du vor mich wollen b\u00fcssen,", "tokens": ["Hast", "du", "vor", "mich", "wol\u00b7len", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So lass mich auch de\u00df geniessen.", "tokens": ["So", "lass", "mich", "auch", "de\u00df", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Zwar ich muss es gern gestehen,", "tokens": ["Zwar", "ich", "muss", "es", "gern", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich gr\u00f6blich mich versehen", "tokens": ["Da\u00df", "ich", "gr\u00f6b\u00b7lich", "mich", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verdient der H\u00f6llen Lohn.", "tokens": ["Und", "ver\u00b7dient", "der", "H\u00f6l\u00b7len", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber sieh', ich komme wieder,", "tokens": ["A\u00b7ber", "sieh'", ",", "ich", "kom\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fall', o Jesu, vor dir nieder,", "tokens": ["Fall'", ",", "o", "Je\u00b7su", ",", "vor", "dir", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NE", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ich dein l\u00e4ngst verlorner Sohn,", "tokens": ["Ich", "dein", "l\u00e4ngst", "ver\u00b7lor\u00b7ner", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ersuche dich mit Thr\u00e4nen,", "tokens": ["Und", "er\u00b7su\u00b7che", "dich", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df dich wiederum vers\u00f6hnen.", "tokens": ["La\u00df", "dich", "wie\u00b7de\u00b7rum", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ist im Himmel gr\u00f6sser Freude,", "tokens": ["Ist", "im", "Him\u00b7mel", "gr\u00f6s\u00b7ser", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein S\u00fcnder kehrt mit Leide", "tokens": ["Wenn", "ein", "S\u00fcn\u00b7der", "kehrt", "mit", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder zu der rechten Bahn,", "tokens": ["Wie\u00b7der", "zu", "der", "rech\u00b7ten", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als um neun und neunzig Fromme,", "tokens": ["Als", "um", "neun", "und", "neun\u00b7zig", "From\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, so nimm mich, da ich komme,", "tokens": ["Herr", ",", "so", "nimm", "mich", ",", "da", "ich", "kom\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiederum zu Gnaden an;", "tokens": ["Wie\u00b7de\u00b7rum", "zu", "Gna\u00b7den", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ich bis hieher begangen,", "tokens": ["Was", "ich", "bis", "hie\u00b7her", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich an zu bessern fangen.", "tokens": ["Will", "ich", "an", "zu", "bes\u00b7sern", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Weicht hinfort, ihr S\u00fcnden-Geister,", "tokens": ["Weicht", "hin\u00b7fort", ",", "ihr", "S\u00fcn\u00b7den\u00b7Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn mein einz'ger Seelen-Meister,", "tokens": ["Denn", "mein", "einz'\u00b7ger", "See\u00b7len\u00b7Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jesus, wohnt allein bei mir;", "tokens": ["Je\u00b7sus", ",", "wohnt", "al\u00b7lein", "bei", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohn' ihn w\u00e4r' ich ganz verloren,", "tokens": ["Ohn'", "ihn", "w\u00e4r'", "ich", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ihn bin ich neu geboren,", "tokens": ["Durch", "ihn", "bin", "ich", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ihm leb' ich f\u00fcr und f\u00fcr,", "tokens": ["In", "ihm", "leb'", "ich", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit ihm geh' ich aus der Erden", "tokens": ["Mit", "ihm", "geh'", "ich", "aus", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und werd' ewig selig werden.", "tokens": ["Und", "werd'", "e\u00b7wig", "se\u00b7lig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}