{"dta.poem.18719": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Aergert\u2019s jemand dass es Gott gefallen", "tokens": ["A\u00b7er\u00b7gert's", "je\u00b7mand", "dass", "es", "Gott", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "KOUS", "PPER", "NN", "VVPP"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Mahomed zu g\u00f6nnen Schutz und Gl\u00fcck,", "tokens": ["Ma\u00b7ho\u00b7med", "zu", "g\u00f6n\u00b7nen", "Schutz", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um den st\u00e4rksten Balken seiner Hallen", "tokens": ["Um", "den", "st\u00e4rks\u00b7ten", "Bal\u00b7ken", "sei\u00b7ner", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da befestig\u2019 er den derben Strick,", "tokens": ["Da", "be\u00b7fes\u00b7tig'", "er", "den", "der\u00b7ben", "Strick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kn\u00fcpfe sich daran! das h\u00e4lt und tr\u00e4gt,", "tokens": ["Kn\u00fcp\u00b7fe", "sich", "da\u00b7ran", "!", "das", "h\u00e4lt", "und", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PAV", "$.", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Er wird f\u00fchlen dass sein Zorn sich legt.", "tokens": ["Er", "wird", "f\u00fch\u00b7len", "dass", "sein", "Zorn", "sich", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}