{"dta.poem.15720": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, ob jeder Mensch denn nicht zur h\u00f6chsten Stufe", "tokens": ["Du", "fragst", ",", "ob", "je\u00b7der", "Mensch", "denn", "nicht", "zur", "h\u00f6chs\u00b7ten", "Stu\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "ADV", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Berufen sei, zu der ich selbst empor dich rufe?", "tokens": ["Be\u00b7ru\u00b7fen", "sei", ",", "zu", "der", "ich", "selbst", "em\u00b7por", "dich", "ru\u00b7fe", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erkenntnis Gottes, Weltverst\u00e4ndnis, Harmonie", "tokens": ["Er\u00b7kennt\u00b7nis", "Got\u00b7tes", ",", "Welt\u00b7ver\u00b7st\u00e4nd\u00b7nis", ",", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sf\u00e4ren alles Seyns, gilt das nicht allen hie?", "tokens": ["Der", "Sf\u00e4\u00b7ren", "al\u00b7les", "Seyns", ",", "gilt", "das", "nicht", "al\u00b7len", "hie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "VVFIN", "PDS", "PTKNEG", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was aber soll ich dann zu jenem Schmiede sagen,", "tokens": ["Was", "a\u00b7ber", "soll", "ich", "dann", "zu", "je\u00b7nem", "Schmie\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den auf den Ambo\u00df ich h\u00f6r' unharmonisch schlagen?", "tokens": ["Den", "auf", "den", "Am\u00b7bo\u00df", "ich", "h\u00f6r'", "un\u00b7har\u00b7mo\u00b7nisch", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er wirkt nicht f\u00fcr die Kunst, er schafft f\u00fcr seinen Magen.", "tokens": ["Er", "wirkt", "nicht", "f\u00fcr", "die", "Kunst", ",", "er", "schafft", "f\u00fcr", "sei\u00b7nen", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er schmiedet Pflug und Schwert f\u00fcr Ackermann und Krieger;", "tokens": ["Er", "schmie\u00b7det", "Pflug", "und", "Schwert", "f\u00fcr", "A\u00b7cker\u00b7mann", "und", "Krie\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die beiden sind der Welt Ern\u00e4hrer und Besieger.", "tokens": ["Die", "bei\u00b7den", "sind", "der", "Welt", "Er\u00b7n\u00e4h\u00b7rer", "und", "Be\u00b7sie\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die F\u00fclle schaffen sie und schaffen dir den Frieden,", "tokens": ["Die", "F\u00fcl\u00b7le", "schaf\u00b7fen", "sie", "und", "schaf\u00b7fen", "dir", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin zu denken dir, zu dichten ist beschieden.", "tokens": ["Da\u00b7rin", "zu", "den\u00b7ken", "dir", ",", "zu", "dich\u00b7ten", "ist", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "PPER", "$,", "APPR", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So dicht' und denk' und dank', und la\u00df den Schmied nur schmieden!", "tokens": ["So", "dicht'", "und", "denk'", "und", "dank'", ",", "und", "la\u00df", "den", "Schmied", "nur", "schmie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVIMP", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}