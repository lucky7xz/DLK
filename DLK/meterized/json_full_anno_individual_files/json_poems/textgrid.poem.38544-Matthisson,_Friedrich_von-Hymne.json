d", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "$.", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und doch trennt von der Gruft mich nur ein kleiner Raum;", "tokens": ["Und", "doch", "trennt", "von", "der", "Gruft", "mich", "nur", "ein", "klei\u00b7ner", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nur Traum und D\u00e4mmrung bleibt im Erdenthal mein Wissen,", "tokens": ["Nur", "Traum", "und", "D\u00e4mm\u00b7rung", "bleibt", "im", "Er\u00b7dent\u00b7hal", "mein", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Leben fleucht dahin umringt von Finsternissen!", "tokens": ["Mein", "Le\u00b7ben", "fleucht", "da\u00b7hin", "um\u00b7ringt", "von", "Fins\u00b7ter\u00b7nis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O du, den oft zu Gott der Andacht Fl\u00fcgel trug,", "tokens": ["O", "du", ",", "den", "oft", "zu", "Gott", "der", "An\u00b7dacht", "Fl\u00fc\u00b7gel", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"