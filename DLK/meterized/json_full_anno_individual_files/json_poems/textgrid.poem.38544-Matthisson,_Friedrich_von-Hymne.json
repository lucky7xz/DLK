{"textgrid.poem.38544": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Hymne", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! es verk\u00fcndigt dich der Wandelsterne Gang,", "tokens": ["Herr", "!", "es", "ver\u00b7k\u00fcn\u00b7digt", "dich", "der", "Wan\u00b7dels\u00b7ter\u00b7ne", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Durch alle Himmel t\u00f6nt seraphischer Gesang,", "tokens": ["Durch", "al\u00b7le", "Him\u00b7mel", "t\u00f6nt", "se\u00b7ra\u00b7phi\u00b7scher", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ganze Sch\u00f6pfung schwebt in ewgen Harmonieen,", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "schwebt", "in", "ew\u00b7gen", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So weit sich Welten drehn und Sonnenheere gl\u00fchen!", "tokens": ["So", "weit", "sich", "Wel\u00b7ten", "drehn", "und", "Son\u00b7nen\u00b7hee\u00b7re", "gl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Tempel, die N