{"textgrid.poem.47215": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich mir einst eine H\u00fctte will bauen,", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mir einst eine H\u00fctte will bauen,", "tokens": ["Wenn", "ich", "mir", "einst", "ei\u00b7ne", "H\u00fct\u00b7te", "will", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will ich nach einem Stamm umschauen,", "tokens": ["Will", "ich", "nach", "ei\u00b7nem", "Stamm", "um\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo in der Mitte Bienen wohnen,", "tokens": ["Wo", "in", "der", "Mit\u00b7te", "Bie\u00b7nen", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Fu\u00df Ameisen und Tauben auf den Kronen;", "tokens": ["Am", "Fu\u00df", "A\u00b7mei\u00b7sen", "und", "Tau\u00b7ben", "auf", "den", "Kro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Damit, wenn ich draus die H\u00fctte gebauet,", "tokens": ["Da\u00b7mit", ",", "wenn", "ich", "draus", "die", "H\u00fct\u00b7te", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "punct", "w