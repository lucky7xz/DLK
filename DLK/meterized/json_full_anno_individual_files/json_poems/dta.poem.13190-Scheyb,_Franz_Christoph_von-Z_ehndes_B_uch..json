{"dta.poem.13190": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So stellte dieses Throns Staat, W\u00fcrde, Pracht und Flor,", "tokens": ["So", "stell\u00b7te", "die\u00b7ses", "Throns", "Staat", ",", "W\u00fcr\u00b7de", ",", "Pracht", "und", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "NN", "$,", "VAFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glanz, Ansehn, Majest\u00e4t, sich unsern Augen vor.", "tokens": ["Glanz", ",", "An\u00b7sehn", ",", "Ma\u00b7jes\u00b7t\u00e4t", ",", "sich", "un\u00b7sern", "Au\u00b7gen", "vor", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man liesse keinen Blick, nur achtlo\u00df, von sich schiessen,", "tokens": ["Man", "lies\u00b7se", "kei\u00b7nen", "Blick", ",", "nur", "acht\u00b7lo\u00df", ",", "von", "sich", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,", "ADV", "ADV", "$,", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der nicht f\u00fcr Lust bestrickt ihn hat bewundern m\u00fcssen.", "tokens": ["Der", "nicht", "f\u00fcr", "Lust", "be\u00b7strickt", "ihn", "hat", "be\u00b7wun\u00b7dern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "VVFIN", "PPER", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Augs Vergn\u00fcgen blieb indessen nicht allein,", "tokens": ["Des", "Augs", "Ver\u00b7gn\u00fc\u00b7gen", "blieb", "in\u00b7des\u00b7sen", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "90Ein unvermercktes Was nahm alle Sinnen ein.", "tokens": ["un\u00b7ver\u00b7merck\u00b7tes", "Was", "nahm", "al\u00b7le", "Sin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "PWS", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Der Saal vertiefte sich in eine solche Stille,", "tokens": ["Der", "Saal", "ver\u00b7tief\u00b7te", "sich", "in", "ei\u00b7ne", "sol\u00b7che", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ob die Einsamkeit allein den Ort erf\u00fclle;", "tokens": ["Als", "ob", "die", "Ein\u00b7sam\u00b7keit", "al\u00b7lein", "den", "Ort", "er\u00b7f\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man sahe nirgendswo den Finger an dem Mund,", "tokens": ["Man", "sa\u00b7he", "nir\u00b7gends\u00b7wo", "den", "Fin\u00b7ger", "an", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Nur durch den Ehrfurchts-Trieb ward solches Schweigen kund.", "tokens": ["Nur", "durch", "den", "Ehr\u00b7furchts\u00b7Trieb", "ward", "sol\u00b7ches", "Schwei\u00b7gen", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "95Die durch der Winde Schlaf gestillte Meeres-Wellen", "tokens": ["durch", "der", "Win\u00b7de", "Schlaf", "ge\u00b7still\u00b7te", "Mee\u00b7res\u00b7Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Seynd nicht so still, als hier die Tugenden sich stellen.", "tokens": ["Seynd", "nicht", "so", "still", ",", "als", "hier", "die", "Tu\u00b7gen\u00b7den", "sich", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir nahmen auch in acht, was die Verwundrung kann,", "tokens": ["Wir", "nah\u00b7men", "auch", "in", "acht", ",", "was", "die", "Ver\u00b7wund\u00b7rung", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "$,", "PRELS", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da manches Aug in Trost- und Freuden-Thr\u00e4nen rann.", "tokens": ["Da", "man\u00b7ches", "Aug", "in", "Trost", "und", "Freu\u00b7den\u00b7Thr\u00e4\u00b7nen", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein schmeichelndes Gef\u00fchl durchwallte das Gebl\u00fcthe,", "tokens": ["Ein", "schmei\u00b7cheln\u00b7des", "Ge\u00b7f\u00fchl", "durch\u00b7wall\u00b7te", "das", "Ge\u00b7bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "100Und die Empfindlichkeit war nur in dem Gem\u00fcthe.", "tokens": ["die", "Emp\u00b7find\u00b7lich\u00b7keit", "war", "nur", "in", "dem", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Erstaunung und Begier, Reiz, Anmuth, Furcht und Lust", "tokens": ["Er\u00b7stau\u00b7nung", "und", "Be\u00b7gier", ",", "Reiz", ",", "An\u00b7muth", ",", "Furcht", "und", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vermischt- und sch\u00e4rfften sich im Blick und in der Brust.", "tokens": ["Ver\u00b7mischt", "und", "sch\u00e4rff\u00b7ten", "sich", "im", "Blick", "und", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "VVFIN", "PRF", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Verlangen, Z\u00e4rtlichkeit, Entz\u00fcckung, Gunst und Liebe", "tokens": ["Ver\u00b7lan\u00b7gen", ",", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "Ent\u00b7z\u00fc\u00b7ckung", ",", "Gunst", "und", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bemeisterten das Herz, den Geist durch ihre Triebe,", "tokens": ["Be\u00b7meis\u00b7ter\u00b7ten", "das", "Herz", ",", "den", "Geist", "durch", "ih\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "105Bi\u00df unser guter Rath das Schweigen unterbrach,", "tokens": ["un\u00b7ser", "gu\u00b7ter", "Rath", "das", "Schwei\u00b7gen", "un\u00b7ter\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Sich vor den Thron begab, und diese Worte sprach:", "tokens": ["Sich", "vor", "den", "Thron", "be\u00b7gab", ",", "und", "die\u00b7se", "Wor\u00b7te", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}