{"dta.poem.8754": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Verborgen vor der ganzen Welt,", "tokens": ["Ver\u00b7bor\u00b7gen", "vor", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf stillen Fl\u00fcssen m\u00f6cht' ich fahren,", "tokens": ["Auf", "stil\u00b7len", "Fl\u00fcs\u00b7sen", "m\u00f6cht'", "ich", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bedeckt vom schatt'gen Wolkenzelt.", "tokens": ["Be\u00b7deckt", "vom", "schatt'\u00b7gen", "Wol\u00b7ken\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Sommerv\u00f6geln \u00fcbergaukelt,", "tokens": ["Von", "Som\u00b7mer\u00b7v\u00f6\u00b7geln", "\u00fc\u00b7berg\u00b7au\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ird'schen Schwere mich entziehn,", "tokens": ["Der", "ird'\u00b7schen", "Schwe\u00b7re", "mich", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom reinen Element geschaukelt", "tokens": ["Vom", "rei\u00b7nen", "E\u00b7le\u00b7ment", "ge\u00b7schau\u00b7kelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die schuldbefleckten Menschen fliehn.", "tokens": ["Die", "schuld\u00b7be\u00b7fleck\u00b7ten", "Men\u00b7schen", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur selten an das Ufer streifen,", "tokens": ["Nur", "sel\u00b7ten", "an", "das", "U\u00b7fer", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nie entsteigen meinem Kahn,", "tokens": ["Doch", "nie", "ent\u00b7stei\u00b7gen", "mei\u00b7nem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach einer Rosenknospe greifen,", "tokens": ["Nach", "ei\u00b7ner", "Ro\u00b7sen\u00b7knos\u00b7pe", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder ziehn die feuchte Bahn.", "tokens": ["Und", "wie\u00b7der", "ziehn", "die", "feuch\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von ferne sehn, wie Heerden weiden,", "tokens": ["Von", "fer\u00b7ne", "sehn", ",", "wie", "Heer\u00b7den", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVINF", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Blumen wachsen immer neu,", "tokens": ["Wie", "Blu\u00b7men", "wach\u00b7sen", "im\u00b7mer", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Winzerinnen Trauben schneiden,", "tokens": ["Wie", "Win\u00b7ze\u00b7rin\u00b7nen", "Trau\u00b7ben", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Schnitter m\u00e4h'n das duft'ge Heu.", "tokens": ["Wie", "Schnit\u00b7ter", "m\u00e4h'n", "das", "duft'\u00b7ge", "Heu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nichts genie\u00dfen, als die Helle", "tokens": ["Und", "nichts", "ge\u00b7nie\u00b7\u00dfen", ",", "als", "die", "Hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lichts, das ewig lauter bleibt,", "tokens": ["Des", "Lichts", ",", "das", "e\u00b7wig", "lau\u00b7ter", "bleibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einen Trunk der frischen Welle,", "tokens": ["Und", "ei\u00b7nen", "Trunk", "der", "fri\u00b7schen", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nie das Blut geschwinder treibt.", "tokens": ["Der", "nie", "das", "Blut", "ge\u00b7schwin\u00b7der", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}