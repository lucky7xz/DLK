2": {"text": "Selig wie Kinder,", "tokens": ["Se\u00b7lig", "wie", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die, halb noch an n\u00e4hrender Mutterbrust,", "tokens": ["Die", ",", "halb", "noch", "an", "n\u00e4h\u00b7ren\u00b7der", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Halb schon die ersten Schritte thun,", "tokens": ["Halb", "schon", "die", "ers\u00b7ten", "Schrit\u00b7te", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Blumen und Faltern gelenkt", "tokens": ["Von", "Blu\u00b7men", "und", "Fal\u00b7tern", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und vom Zwitschern des Vogels;", "tokens": ["Und", "vom", "Zwit\u00b7schern", "des", "Vo\u00b7gels", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Aber versch\u00fcchtert sogleich", "tokens": ["A\u00b7ber", "ver\u00b7sch\u00fcch\u00b7tert", "sog\u00b7leich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Vor jedem rauschenden Lufthauch", "tokens": ["Vor", "je\u00b7dem", "rau\u00b7schen\u00b7den", "Luft\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Zur\u00fcck sich fl\u00fcchten in die sch\u00fctzende Hut.", "tokens": ["Zu\u00b7r\u00fcck", "sich", "fl\u00fcch\u00b7ten", "in", "die", "sch\u00fct\u00b7zen\u00b7de", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Nur N\u00e4chstes im Auge,", "tokens": ["Nur", "N\u00e4chs\u00b7tes", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Greifen sie nach dem N\u00e4chsten nur \u2013", "tokens": ["Grei\u00b7fen", "sie", "nach", "dem", "N\u00e4chs\u00b7ten", "nur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Und so leben sie hin", "tokens": ["Und", "so", "le\u00b7ben", "sie", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Gute und b\u00f6se Tage,", "tokens": ["Gu\u00b7te", "und", "b\u00f6\u00b7se", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.14": {"text": "Harmlos, als m\u00fc\u00dft' es so sein,", "tokens": ["Harm\u00b7los", ",", "als", "m\u00fc\u00dft'", "es", "so", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Nur das eigene Wohl und Weh' bedenkend.", "tokens": ["Nur", "das", "ei\u00b7ge\u00b7ne", "Wohl", "und", "Weh'", "be\u00b7den\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Inzwischen schreitet an ihnen vor\u00fcber die Zeit", "tokens": ["I\u00b7nzwi\u00b7schen", "schrei\u00b7tet", "an", "ih\u00b7nen", "vor\u00b7\u00fc\u00b7ber", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.17": {"text": "Und rei\u00dft die Ahnungslosen", "tokens": ["Und", "rei\u00dft", "die", "Ah\u00b7nungs\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Wie im Traum mit sich fort.", "tokens": ["Wie", "im", "Traum", "mit", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Und wenn sie dann pl\u00f6tzlich", "tokens": ["Und", "wenn", "sie", "dann", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Erwachen bei unsanftem Ruck,", "tokens": ["Er\u00b7wa\u00b7chen", "bei", "un\u00b7sanf\u00b7tem", "Ruck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Blicken sie auf und fragen in r\u00fchrender Unschuld: was ist? \u2013", "tokens": ["Bli\u00b7cken", "sie", "auf", "und", "fra\u00b7gen", "in", "r\u00fch\u00b7ren\u00b7der", "Un\u00b7schuld", ":", "was", "ist", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "PWS", "VAFIN", "$.", "$("], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ja, was ist!? Ihr And'ren", "tokens": ["Ja", ",", "was", "ist", "!?", "Ihr", "An\u00b7d'\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PWS", "VAFIN", "$.", "PPOSAT", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "K\u00f6nnt es ihnen sagen:", "tokens": ["K\u00f6nnt", "es", "ih\u00b7nen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn ihr wi\u00dft es.", "tokens": ["Denn", "ihr", "wi\u00dft", "es", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Dann horchen sie auf", "tokens": ["Dann", "hor\u00b7chen", "sie", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und stehen besch\u00e4mt \u2013", "tokens": ["Und", "ste\u00b7hen", "be\u00b7sch\u00e4mt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und klug wie zuvor.", "tokens": ["Und", "klug", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie begreifen nichts,", "tokens": ["Sie", "be\u00b7grei\u00b7fen", "nichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sie lernen nichts", "tokens": ["Sie", "ler\u00b7nen", "nichts"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und fremd bleibt ihnen Alles,", "tokens": ["Und", "fremd", "bleibt", "ih\u00b7nen", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Was ihr preist als die h\u00f6chsten Triumphe der Menschheit.", "tokens": ["Was", "ihr", "preist", "als", "die", "h\u00f6chs\u00b7ten", "Tri\u00b7um\u00b7phe", "der", "Menschheit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Aber daf\u00fcr auch", "tokens": ["A\u00b7ber", "da\u00b7f\u00fcr", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Bleibt ihnen erspart die letzte Erkenntni\u00df:", "tokens": ["Bleibt", "ih\u00b7nen", "er\u00b7spart", "die", "letz\u00b7te", "Er\u00b7kennt\u00b7ni\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die Erkenntni\u00df der eigenen Nichtigkeit", "tokens": ["Die", "Er\u00b7kennt\u00b7ni\u00df", "der", "ei\u00b7ge\u00b7nen", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+", "me