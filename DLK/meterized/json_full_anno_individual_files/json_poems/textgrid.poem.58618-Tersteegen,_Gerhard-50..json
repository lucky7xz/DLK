{"textgrid.poem.58618": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen kleinen Augenblick, nur ein wenig will Gott lassen", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Au\u00b7gen\u00b7blick", ",", "nur", "ein", "we\u00b7nig", "will", "Gott", "las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ART", "PIS", "VMFIN", "NN", "VVINF"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und alsdann mit gro\u00dfer Gnad' uns versammeln und umfassen;", "tokens": ["Und", "als\u00b7dann", "mit", "gro\u00b7\u00dfer", "Gnad'", "uns", "ver\u00b7sam\u00b7meln", "und", "um\u00b7fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch der kleine Augenblick, dieses Wenig, scheint so lang,", "tokens": ["Doch", "der", "klei\u00b7ne", "Au\u00b7gen\u00b7blick", ",", "die\u00b7ses", "We\u00b7nig", ",", "scheint", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PDAT", "NN", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weil mein Glaube noch so schwach und mein bl\u00f6des Herz so bang.", "tokens": ["Weil", "mein", "Glau\u00b7be", "noch", "so", "schwach", "und", "mein", "bl\u00f6\u00b7des", "Herz", "so", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "KON", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Ja, Herr, wenn du mich verl\u00e4\u00dft, geht dein Sch\u00e4flein in der D\u00fcrre;", "tokens": ["Ja", ",", "Herr", ",", "wenn", "du", "mich", "ver\u00b7l\u00e4\u00dft", ",", "geht", "dein", "Sch\u00e4f\u00b7lein", "in", "der", "D\u00fcr\u00b7re", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Dein Erbarmen sammle mich, da\u00df ich doch nicht gar verirre,", "tokens": ["Dein", "Er\u00b7bar\u00b7men", "samm\u00b7le", "mich", ",", "da\u00df", "ich", "doch", "nicht", "gar", "ver\u00b7ir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Zeig dein holdes Angesicht, das sich mir so oft verdeckt,", "tokens": ["Zeig", "dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht", ",", "das", "sich", "mir", "so", "oft", "ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PRF", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Ew'ge Gnad', erbarme dich, ach, dein Zorn so bitter schmeckt!", "tokens": ["Ew'\u00b7ge", "Gnad'", ",", "er\u00b7bar\u00b7me", "dich", ",", "ach",