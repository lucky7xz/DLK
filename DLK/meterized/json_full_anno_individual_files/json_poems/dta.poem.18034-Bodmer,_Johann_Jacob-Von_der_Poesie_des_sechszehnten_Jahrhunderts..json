{"dta.poem.18034": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Poesie des sechszehnten  \n Jahrhunderts.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu der Seelen drey Strassen gahn,", "tokens": ["Zu", "der", "See\u00b7len", "drey", "Stras\u00b7sen", "gahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die dem Tod allzeit offen stahn.", "tokens": ["Die", "dem", "Tod", "all\u00b7zeit", "of\u00b7fen", "stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wer in S&#252;nden erlieget todt,", "tokens": ["Wer", "in", "S", "&#252;", "nden", "er\u00b7lie\u00b7get", "todt", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "$(", "VVFIN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Seel wird leiden ewig Not.", "tokens": ["Des", "Seel", "wird", "lei\u00b7den", "e\u00b7wig", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ander ist, wer &#252;bel thut", "tokens": ["Die", "an\u00b7der", "ist", ",", "wer", "&#252;", "bel", "thut"], "token_info": ["word", "word", "word", "punct", "word", "XML_entity", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PWS", "$(", "ADJD", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und sich bed&#252;nkt dennoch syn gut.", "tokens": ["Und", "sich", "bed", "&#252;", "nkt", "den\u00b7noch", "syn", "gut", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "$(", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die dritt Strass ist so breit und weit", "tokens": ["Die", "dritt", "Strass", "ist", "so", "breit", "und", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dass all Welt darauf geht allzeit.", "tokens": ["Dass", "all", "Welt", "da\u00b7rauf", "geht", "all\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PAV", "VVFIN", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}