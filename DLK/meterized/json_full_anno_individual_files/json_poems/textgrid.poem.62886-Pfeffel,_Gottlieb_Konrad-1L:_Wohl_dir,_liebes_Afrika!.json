s": ["Er", "nur", "ist", "der", "Frey\u00b7heit", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fcder, wenn wir f\u00fcr ihn siegen,", "tokens": ["Br\u00fc\u00b7der", ",", "wenn", "wir", "f\u00fcr", "ihn", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen wir mit Howens Schwert", "tokens": ["Wol\u00b7len", "wir", "mit", "Ho\u00b7wens", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Penns geweihte Felder pfl\u00fcgen.", "tokens": ["Penns", "ge\u00b7weih\u00b7te", "Fel\u00b7der", "pfl\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Aber wird die Tyranney", "tokens": ["A\u00b7ber", "wird", "die", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Br\u00fcderstadt verderben;", "tokens": ["Auch", "die", "Br\u00fc\u00b7der\u00b7stadt", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freunde, ha! so la\u00dft uns frey", "tokens": ["Freun\u00b7de", ",", "ha", "!", "so", "la\u00dft", "uns", "frey"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$.", "ADV", "VVIMP", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den letzten Menschen sterben!", "tokens": ["Mit", "den", "letz\u00b7ten", "Men\u00b7schen", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}