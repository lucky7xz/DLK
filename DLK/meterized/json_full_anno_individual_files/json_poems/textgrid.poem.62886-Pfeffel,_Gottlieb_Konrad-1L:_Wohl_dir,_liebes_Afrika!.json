{"textgrid.poem.62886": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl dir, liebes Afrika!", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dir, liebes Afrika!", "tokens": ["Wohl", "dir", ",", "lie\u00b7bes", "Af\u00b7ri\u00b7ka", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun beh\u00e4lst du deine Kinder,", "tokens": ["Nun", "be\u00b7h\u00e4lst", "du", "dei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon verkauft Germania", "tokens": ["Schon", "ver\u00b7kauft", "Ger\u00b7ma\u00b7nia"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Seine S\u00f6hne wie die Rinder.", "tokens": ["Sei\u00b7ne", "S\u00f6h\u00b7ne", "wie", "die", "Rin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit stiefm\u00fctterlicher Hand", "tokens": ["Mit", "stief\u00b7m\u00fct\u00b7ter\u00b7li\u00b7cher", "Hand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00dft es sie von seinen Br\u00fcsten,", "tokens": ["Rei\u00dft", "es", "sie", "von", "sei\u00b7nen", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um durch sie das neue Land,", "tokens": ["Um", "durch", "sie", "das", "neu\u00b7e", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das wir d\u00fcngen, zu verw\u00fcsten.", "tokens": ["Das", "wir", "d\u00fcn\u00b7gen", ",", "zu", "ver\u00b7w\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dreymal selig mu\u00df ich, traun,", "tokens": ["Drey\u00b7mal", "se\u00b7lig", "mu\u00df", "ich", ",", "traun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "$,", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich vor deutschen Sklaven achten;", "tokens": ["Mich", "vor", "deut\u00b7schen", "Skla\u00b7ven", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich zwingt man Toback zu baun;", "tokens": ["Mich", "zwingt", "man", "To\u00b7back", "zu", "baun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jene m\u00fc\u00dfen Menschen schlachten.", "tokens": ["Je\u00b7ne", "m\u00fc\u00b7\u00dfen", "Men\u00b7schen", "schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Halb so theuer ist das Blut", "tokens": ["Halb", "so", "theu\u00b7er", "ist", "das", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Hessen angesetzet,", "tokens": ["Ei\u00b7nes", "Hes\u00b7sen", "an\u00b7ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als man in Konnektikut", "tokens": ["Als", "man", "in", "Kon\u00b7nek\u00b7ti\u00b7kut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Stirne Schwei\u00df gesch\u00e4tzet.", "tokens": ["Mei\u00b7ner", "Stir\u00b7ne", "Schwei\u00df", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr, die Feind und Britte ha\u00dft,", "tokens": ["Ihr", ",", "die", "Feind", "und", "Brit\u00b7te", "ha\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsche, la\u00dft die Welt im Frieden!", "tokens": ["Deut\u00b7sche", ",", "la\u00dft", "die", "Welt", "im", "Frie\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt ihr Ketten, deren Last", "tokens": ["Wollt", "ihr", "Ket\u00b7ten", ",", "de\u00b7ren", "Last"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr verflucht, f\u00fcr Br\u00fcder schmieden?", "tokens": ["Ihr", "ver\u00b7flucht", ",", "f\u00fcr", "Br\u00fc\u00b7der", "schmie\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch ihr f\u00fchlts! Mit frommer Scheu", "tokens": ["Doch", "ihr", "f\u00fchlts", "!", "Mit", "from\u00b7mer", "Scheu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werfen halbe Legionen", "tokens": ["Wer\u00b7fen", "hal\u00b7be", "Le\u00b7gi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Waffen weg, um frey", "tokens": ["Ih\u00b7re", "Waf\u00b7fen", "weg", ",", "um", "frey"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "KOUI", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Amerika zu wohnen.", "tokens": ["In", "A\u00b7me\u00b7ri\u00b7ka", "zu", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Und mit einem solchen Heer", "tokens": ["Und", "mit", "ei\u00b7nem", "sol\u00b7chen", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollt ihr dieses Land bezwingen?", "tokens": ["Wollt", "ihr", "die\u00b7ses", "Land", "be\u00b7zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eitle Britten, nimmermehr", "tokens": ["Eit\u00b7le", "Brit\u00b7ten", ",", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Anschlag euch gelingen.", "tokens": ["Wird", "der", "An\u00b7schlag", "euch", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Trotzig wirft das Sklavenjoch", "tokens": ["Trot\u00b7zig", "wirft", "das", "Skla\u00b7ven\u00b7joch"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Washington vom L\u00f6wennacken,", "tokens": ["Was\u00b7hing\u00b7ton", "vom", "L\u00f6\u00b7wen\u00b7na\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und der Heuchler h\u00e4lt sich doch", "tokens": ["Und", "der", "Heuch\u00b7ler", "h\u00e4lt", "sich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Sklaven, die ihm hacken?", "tokens": ["Tau\u00b7send", "Skla\u00b7ven", ",", "die", "ihm", "ha\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "K\u00fchne Pflanzer, h\u00e4ttet ihr", "tokens": ["K\u00fch\u00b7ne", "Pflan\u00b7zer", ",", "h\u00e4t\u00b7tet", "ihr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns mit euch f\u00fcr frey erkl\u00e4ret;", "tokens": ["Uns", "mit", "euch", "f\u00fcr", "frey", "er\u00b7kl\u00e4\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "APPR", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Howe trotzte nicht mehr hier,", "tokens": ["Ho\u00b7we", "trotz\u00b7te", "nicht", "mehr", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Percy w\u00e4re heimgekehret.", "tokens": ["Per\u00b7cy", "w\u00e4\u00b7re", "heim\u00b7ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lange m\u00fc\u00dften, Hunden gleich,", "tokens": ["Lan\u00b7ge", "m\u00fc\u00df\u00b7ten", ",", "Hun\u00b7den", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Britten eure F\u00fc\u00dfe lecken,", "tokens": ["Brit\u00b7ten", "eu\u00b7re", "F\u00fc\u00b7\u00dfe", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir w\u00fcrden stolz mit euch", "tokens": ["Und", "wir", "w\u00fcr\u00b7den", "stolz", "mit", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsrer Siege Fr\u00fcchte schmecken.", "tokens": ["Uns\u00b7rer", "Sie\u00b7ge", "Fr\u00fcch\u00b7te", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun verlach ich euern Streit;", "tokens": ["Nun", "ver\u00b7lach", "ich", "eu\u00b7ern", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was kann ich dabey verlieren?", "tokens": ["Was", "kann", "ich", "da\u00b7bey", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird das Erbtheil Penns bedr\u00e4ut,", "tokens": ["Wird", "das", "E\u00b7rbtheil", "Penns", "be\u00b7dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Negern, dann m\u00fc\u00dft ihr euch r\u00fchren.", "tokens": ["Ne\u00b7gern", ",", "dann", "m\u00fc\u00dft", "ihr", "euch", "r\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Dann schlie\u00dft einen ehrnen Kreis", "tokens": ["Dann", "schlie\u00dft", "ei\u00b7nen", "ehr\u00b7nen", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um des Quakers fette Saaten,", "tokens": ["Um", "des", "Qua\u00b7kers", "fet\u00b7te", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher nichts von Sklaven wei\u00df,", "tokens": ["Wel\u00b7cher", "nichts", "von", "Skla\u00b7ven", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts von Pfaffen und Soldaten.", "tokens": ["Nichts", "von", "Pfaf\u00b7fen", "und", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Er nur ist der Freyheit werth;", "tokens": ["Er", "nur", "ist", "der", "Frey\u00b7heit", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Br\u00fcder, wenn wir f\u00fcr ihn siegen,", "tokens": ["Br\u00fc\u00b7der", ",", "wenn", "wir", "f\u00fcr", "ihn", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen wir mit Howens Schwert", "tokens": ["Wol\u00b7len", "wir", "mit", "Ho\u00b7wens", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Penns geweihte Felder pfl\u00fcgen.", "tokens": ["Penns", "ge\u00b7weih\u00b7te", "Fel\u00b7der", "pfl\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Aber wird die Tyranney", "tokens": ["A\u00b7ber", "wird", "die", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Br\u00fcderstadt verderben;", "tokens": ["Auch", "die", "Br\u00fc\u00b7der\u00b7stadt", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freunde, ha! so la\u00dft uns frey", "tokens": ["Freun\u00b7de", ",", "ha", "!", "so", "la\u00dft", "uns", "frey"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$.", "ADV", "VVIMP", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den letzten Menschen sterben!", "tokens": ["Mit", "den", "letz\u00b7ten", "Men\u00b7schen", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wohl dir, liebes Afrika!", "tokens": ["Wohl", "dir", ",", "lie\u00b7bes", "Af\u00b7ri\u00b7ka", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun beh\u00e4lst du deine Kinder,", "tokens": ["Nun", "be\u00b7h\u00e4lst", "du", "dei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon verkauft Germania", 