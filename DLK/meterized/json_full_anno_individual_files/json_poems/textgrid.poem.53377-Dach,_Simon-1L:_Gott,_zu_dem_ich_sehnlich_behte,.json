{"textgrid.poem.53377": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, zu dem ich sehnlich behte,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, zu dem ich sehnlich behte,", "tokens": ["Gott", ",", "zu", "dem", "ich", "sehn\u00b7lich", "beh\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann es jetzt dein Wort nicht th\u00e4te,", "tokens": ["Wann", "es", "jetzt", "dein", "Wort", "nicht", "th\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das mir einig Hoffnung gibt", "tokens": ["Das", "mir", "ei\u00b7nig", "Hoff\u00b7nung", "gibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Angst, so mich betr\u00fcbt,", "tokens": ["In", "der", "Angst", ",", "so", "mich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie w\u00fcst' ich es au\u00df zustehen?", "tokens": ["Wie", "w\u00fcst'", "ich", "es", "au\u00df", "zu\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warlich ich m\u00fcst' untergehen,", "tokens": ["War\u00b7lich", "ich", "m\u00fcst'", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses Elend w\u00fcrde mein", "tokens": ["Die\u00b7ses", "E\u00b7lend", "w\u00fcr\u00b7de", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr als \u00fcbrig m\u00e4chtig seyn.", "tokens": ["Mehr", "als", "\u00fcb\u00b7rig", "m\u00e4ch\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie hast du dich mir verwandelt!", "tokens": ["Wie", "hast", "du", "dich", "mir", "ver\u00b7wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "PPER", "VVPP", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Wie man sonst mit Feinden handelt,", "tokens": ["Wie", "man", "sonst", "mit", "Fein\u00b7den", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die beraubt sind aller Zier,", "tokens": ["Die", "be\u00b7raubt", "sind", "al\u00b7ler", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als handelst du mit mir.", "tokens": ["Als", "han\u00b7delst", "du", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hast du mir nicht \u00fcber hoffen", "tokens": ["Hast", "du", "mir", "nicht", "\u00fc\u00b7ber", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da recht in das Hertz getroffen,", "tokens": ["Da", "recht", "in", "das", "Hertz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mich verw\u00fcstet gantz und gar,", "tokens": ["Mich", "ver\u00b7w\u00fcs\u00b7tet", "gantz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da mein Sinn und Leben war?", "tokens": ["Da", "mein", "Sinn", "und", "Le\u00b7ben", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer wird mir nun Trost erwecken?", "tokens": ["Wer", "wird", "mir", "nun", "Trost", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo ist meines Alters Stecken?", "tokens": ["Wo", "ist", "mei\u00b7nes", "Al\u00b7ters", "Ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Hoffnung, mein Gewinn,", "tokens": ["Mei\u00b7ne", "Hoff\u00b7nung", ",", "mein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruh und Absehn ist dahin:", "tokens": ["Ruh", "und", "Ab\u00b7sehn", "ist", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lasst den falschen Schein der Zeiten", "tokens": ["Lasst", "den", "fal\u00b7schen", "Schein", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch, ihr Menschen, nicht verleiten,", "tokens": ["Euch", ",", "ihr", "Men\u00b7schen", ",", "nicht", "ver\u00b7lei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann sich leichtes Gl\u00fcck er\u00e4ugt,", "tokens": ["Wann", "sich", "leich\u00b7tes", "Gl\u00fcck", "er\u00b7\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht gl\u00e4ubt seiner Gunst, es tr\u00e4ugt.", "tokens": ["Nicht", "gl\u00e4ubt", "sei\u00b7ner", "Gunst", ",", "es", "tr\u00e4ugt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wessen soll ich mich nun freuen?", "tokens": ["Wes\u00b7sen", "soll", "ich", "mich", "nun", "freu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer bleibt mir mit festen Treuen", "tokens": ["Wer", "bleibt", "mir", "mit", "fes\u00b7ten", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zugethan in aller Noht?", "tokens": ["Zu\u00b7ge\u00b7than", "in", "al\u00b7ler", "Noht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du O meines Lebens Gott.", "tokens": ["Du", "O", "mei\u00b7nes", "Le\u00b7bens", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Berg' und Klippen m\u00fcssen weichen,", "tokens": ["Ber\u00b7g'", "und", "Klip\u00b7pen", "m\u00fcs\u00b7sen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Welt sammt allen K\u00f6nigreichen", "tokens": ["Welt", "sammt", "al\u00b7len", "K\u00f6\u00b7nig\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fcssen endlich untergehn,", "tokens": ["M\u00fcs\u00b7sen", "end\u00b7lich", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Warheit bleibt bestehn.", "tokens": ["Dei\u00b7ne", "War\u00b7heit", "bleibt", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vnd betheurt es hin und wieder,", "tokens": ["Vnd", "be\u00b7theurt", "es", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df von deinem Volck' ein jder,", "tokens": ["Da\u00df", "von", "dei\u00b7nem", "Volck'", "ein", "jder", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den du liebst, de\u00df Creutzes Pein,", "tokens": ["Den", "du", "liebst", ",", "de\u00df", "Creut\u00b7zes", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterw\u00fcrffig m\u00fcsse seyn,", "tokens": ["Un\u00b7ter\u00b7w\u00fcrf\u00b7fig", "m\u00fcs\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und da\u00df du an Vater-Sinnen", "tokens": ["Und", "da\u00df", "du", "an", "Va\u00b7ter\u00b7Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reich seyst allezeit von innen,", "tokens": ["Reich", "seyst", "al\u00b7le\u00b7zeit", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob es gleich von aussen scheint,", "tokens": ["Ob", "es", "gleich", "von", "aus\u00b7sen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Du seyst unser \u00e4rgster Feind.", "tokens": ["Du", "seyst", "un\u00b7ser", "\u00e4rgs\u00b7ter", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzt la\u00df deinen Trost mich mercken,", "tokens": ["Jetzt", "la\u00df", "dei\u00b7nen", "Trost", "mich", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd mein kranckes Hertz sich st\u00e4rcken,", "tokens": ["Vnd", "mein", "kran\u00b7ckes", "Hertz", "sich", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das der Vnmut fast ohn Raht", "tokens": ["Das", "der", "Vn\u00b7mut", "fast", "ohn", "Raht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnter seiner Herrschafft hat.", "tokens": ["Vn\u00b7ter", "sei\u00b7ner", "Herr\u00b7schafft", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd weil nichts allhie zu schauen,", "tokens": ["