{"dta.poem.10786": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet   Vff  \n Herr D. J\u00f6rg Hartman Haagens  \n Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was sein soll/ schickt sich baldt: was Gott zusammen f\u00fcget/", "tokens": ["Was", "sein", "soll", "/", "schickt", "sich", "baldt", ":", "was", "Gott", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAINF", "VMFIN", "$(", "VVFIN", "PRF", "ADV", "$.", "PWS", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dasselb einander wohl/ auch eh\u2019 es suchet/ findt;", "tokens": ["Das\u00b7selb", "ein\u00b7an\u00b7der", "wohl", "/", "auch", "eh'", "es", "su\u00b7chet", "/", "findt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "$(", "ADV", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Mensch es finden kan/ was man hieniden bindt/", "tokens": ["Kein", "Mensch", "es", "fin\u00b7den", "kan", "/", "was", "man", "hie\u00b7ni\u00b7den", "bindt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "VMFIN", "$(", "PWS", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wans droben nicht zuvor versehen/ offt betrieget/", "tokens": ["Wans", "dro\u00b7ben", "nicht", "zu\u00b7vor", "ver\u00b7se\u00b7hen", "/", "offt", "be\u00b7trie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "ADV", "VVINF", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hafftet nimmer recht: ob manchmahl schon obsieget", "tokens": ["Es", "haff\u00b7tet", "nim\u00b7mer", "recht", ":", "ob", "manch\u00b7mahl", "schon", "ob\u00b7sie\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "KOUS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gewalt vnd Menschen list. Wie Stuppelfewr verschwindt/", "tokens": ["Ge\u00b7walt", "vnd", "Men\u00b7schen", "list", ".", "Wie", "Stup\u00b7pel\u00b7fewr", "ver\u00b7schwindt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$.", "PWAV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd wohlgeredte wort zerfahren in den Wind:", "tokens": ["Vnd", "wohl\u00b7ge\u00b7red\u00b7te", "wort", "zer\u00b7fah\u00b7ren", "in", "den", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So auch die Lieb die man durch practiciren bieget.", "tokens": ["So", "auch", "die", "Lieb", "die", "man", "durch", "prac\u00b7ti\u00b7ci\u00b7ren", "bie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "PIS", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Herr Haagen in sich selbst ist gangen/ vnd zu raht", "tokens": ["Herr", "Haa\u00b7gen", "in", "sich", "selbst", "ist", "gan\u00b7gen", "/", "vnd", "zu", "raht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "PRF", "ADV", "VAFIN", "VVPP", "$(", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Genommen hat sein Hertz/ gefolget Gottes Pfaat/", "tokens": ["Ge\u00b7nom\u00b7men", "hat", "sein", "Hertz", "/", "ge\u00b7fol\u00b7get", "Got\u00b7tes", "Pfaat", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$(", "VVPP", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der jhm dasselb regirt/ vnd thut jhn auch jetzt segnen", "tokens": ["Der", "jhm", "das\u00b7selb", "re\u00b7girt", "/", "vnd", "thut", "jhn", "auch", "jetzt", "seg\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PDAT", "VVPP", "$(", "KON", "VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df er nach seinem wunsch gefunden vngezeigt", "tokens": ["Da\u00df", "er", "nach", "sei\u00b7nem", "wunsch", "ge\u00b7fun\u00b7den", "vn\u00b7ge\u00b7zeigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJD", "VVPP", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein solche/ deren Hertz zugleich vff jhn geneigt:", "tokens": ["Ein", "sol\u00b7che", "/", "de\u00b7ren", "Hertz", "zu\u00b7gleich", "vff", "jhn", "ge\u00b7neigt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$(", "PRELAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die kommen recht zu hauff die jhnen selbst begegnen.", "tokens": ["Die", "kom\u00b7men", "recht", "zu", "hauff", "die", "jh\u00b7nen", "selbst", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "ART", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}