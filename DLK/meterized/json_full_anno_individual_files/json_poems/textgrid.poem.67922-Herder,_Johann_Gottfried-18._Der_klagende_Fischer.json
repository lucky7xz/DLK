e.1": {"text": "\u00bbwie lange, s\u00fcsse Feindin,", "tokens": ["\u00bb", "wie", "lan\u00b7ge", ",", "s\u00fcs\u00b7se", "Fein\u00b7din", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie lange willt du fliehen?", "tokens": ["Wie", "lan\u00b7ge", "willt", "du", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Willt h\u00e4rter, als der Fels seyn,", "tokens": ["Willt", "h\u00e4r\u00b7ter", ",", "als", "der", "Fels", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und leichter als die Winde! \u2013\u00ab", "tokens": ["Und", "leich\u00b7ter", "als", "die", "Win\u00b7de", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbo wie er traurig klagte!", "tokens": ["\u00bb", "o", "wie", "er", "trau\u00b7rig", "klag\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KON", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbein Jahr ists, Undankbare,", "tokens": ["\u00bb", "ein", "Jahr", "ists", ",", "Un\u00b7dank\u00b7ba\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seit du dies Ufer flohest,", "tokens": ["S