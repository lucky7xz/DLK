{"dta.poem.16227": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "281.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum vertragen sich verschiedne Menschen selten?", "tokens": ["Wa\u00b7rum", "ver\u00b7tra\u00b7gen", "sich", "ver\u00b7schied\u00b7ne", "Men\u00b7schen", "sel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil jeder gelten will, und keinen lassen gelten.", "tokens": ["Weil", "je\u00b7der", "gel\u00b7ten", "will", ",", "und", "kei\u00b7nen", "las\u00b7sen", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "KON", "PIAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und doch verschieden ist nur darum Mann von Mann,", "tokens": ["Und", "doch", "ver\u00b7schie\u00b7den", "ist", "nur", "da\u00b7rum", "Mann", "von", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ADV", "PAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder, jedem unbeschadet, gelten kann.", "tokens": ["Da\u00df", "je\u00b7der", ",", "je\u00b7dem", "un\u00b7be\u00b7scha\u00b7det", ",", "gel\u00b7ten", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PIAT", "ADJD", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In der Verschiedenheit der Stellung und der Meinung", "tokens": ["In", "der", "Ver\u00b7schie\u00b7den\u00b7heit", "der", "Stel\u00b7lung", "und", "der", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist wol der Spaltung Grund, doch der auch der Vereinung.", "tokens": ["Ist", "wol", "der", "Spal\u00b7tung", "Grund", ",", "doch", "der", "auch", "der", "Ver\u00b7ei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,", "ADV", "ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}