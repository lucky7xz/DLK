{"textgrid.poem.53946": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sie schl\u00e4ft", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgens, vom letzten Schlaf ein St\u00fcck,", "tokens": ["Mor\u00b7gens", ",", "vom", "letz\u00b7ten", "Schlaf", "ein", "St\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "nimm mich ein bi\u00dfchen mit \u2013", "tokens": ["nimm", "mich", "ein", "bi\u00df\u00b7chen", "mit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "PIS", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "auf deinem Traumboot zu gleiten ist Gl\u00fcck \u2013", "tokens": ["auf", "dei\u00b7nem", "Traum\u00b7boot", "zu", "glei\u00b7ten", "ist", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VAFIN", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Zeituhr geht ihren harten Schritt . . .", "tokens": ["Die", "Zei\u00b7tuhr", "geht", "ih\u00b7ren", "har\u00b7ten", "Schritt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "pick-pack . . .", "tokens": ["pick\u00b7pack", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NE", "$.", "$.", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "\u00bbsie schl\u00e4ft 