{"dta.poem.2771": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem   Scaliger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Honig-Macherinnen/", "tokens": ["Ihr", "Ho\u00b7nig\u00b7Ma\u00b7che\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Blumen Samlerinnen/", "tokens": ["Und", "Blu\u00b7men", "Sam\u00b7le\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Thau Einleserinnen/", "tokens": ["Und", "Thau", "Ein\u00b7le\u00b7se\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihr euch last gel\u00fcsten/", "tokens": ["Die", "ihr", "euch", "last", "ge\u00b7l\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Hyblens s\u00fcssen Br\u00fcsten/", "tokens": ["Von", "Hyb\u00b7lens", "s\u00fcs\u00b7sen", "Br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Balsam-Kraut zu rauben/", "tokens": ["Das", "Bal\u00b7sam\u00b7Kraut", "zu", "rau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die ihr ", "tokens": ["Die", "ihr"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Und gelbe Saffran Gaben/", "tokens": ["Und", "gel\u00b7be", "Saf\u00b7fran", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Jm Fr\u00fchling denckt zu haben/", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "denckt", "zu", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sucht ihr denn bunte Rosen/", "tokens": ["Sucht", "ihr", "denn", "bun\u00b7te", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Violen und Zeitlosen?", "tokens": ["Vi\u00b7o\u00b7len", "und", "Zeit\u00b7lo\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Es lachet euch zum Ruhme/", "tokens": ["Es", "la\u00b7chet", "euch", "zum", "Ruh\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Lilg\u2019 und Ringelblume/", "tokens": ["Die", "Lil\u00b7g'", "und", "Rin\u00b7gel\u00b7blu\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Und andre Lentzen Bilder/", "tokens": ["Und", "and\u00b7re", "Lent\u00b7zen", "Bil\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die Venus im Gefilder/", "tokens": ["Die", "Ve\u00b7nus", "im", "Ge\u00b7fil\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "\nin einem Krei\u00df geschlossen/", "tokens": ["in", "ei\u00b7nem", "Krei\u00df", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und fruchtbar hat begossen/", "tokens": ["Und", "frucht\u00b7bar", "hat", "be\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Was wolt ihr lange summen/", "tokens": ["Was", "wolt", "ihr", "lan\u00b7ge", "sum\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und in Ein\u00f6den brummen?", "tokens": ["Und", "in", "Ein\u00b7\u00f6\u00b7den", "brum\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Es m\u00f6cht euch zarte Kleinen/", "tokens": ["Es", "m\u00f6cht", "euch", "zar\u00b7te", "Klei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ein rauher Blick bescheinen/", "tokens": ["Ein", "rau\u00b7her", "Blick", "be\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "In W\u00fcsteneyen blasen.", "tokens": ["In", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Seht ihr das weisse Pflaster/", "tokens": ["Seht", "ihr", "das", "weis\u00b7se", "Pflas\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Des Halses Alabaster/", "tokens": ["Des", "Hal\u00b7ses", "A\u00b7la\u00b7bas\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Die Gl\u00e4ser-reine H\u00fcgel;", "tokens": ["Die", "Gl\u00e4\u00b7ser\u00b7rei\u00b7ne", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Hier stellet eur Gefl\u00fcgel.", "tokens": ["Hier", "stel\u00b7let", "eur", "Ge\u00b7fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Schaut an der Rose Schimmer/", "tokens": ["Schaut", "an", "der", "Ro\u00b7se", "Schim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Wie sie besafftet immer/", "tokens": ["Wie", "sie", "be\u00b7saff\u00b7tet", "im\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.30": {"text": "In hellen Flammen gl\u00e4ntzet/", "tokens": ["In", "hel\u00b7len", "Flam\u00b7men", "gl\u00e4nt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": " und doppelt sch\u00f6n bekr\u00e4ntzet.", "tokens": ["und", "dop\u00b7pelt", "sch\u00f6n", "be\u00b7kr\u00b7\u00e4nt\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Die funckelnden Corallen/", "tokens": ["Die", "fun\u00b7ckeln\u00b7den", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Der schwesterlichen Ballen", "tokens": ["Der", "schwes\u00b7ter\u00b7li\u00b7chen", "Bal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Und Purpur-rothen Klippen/", "tokens": ["Und", "Pur\u00b7pur\u00b7ro\u00b7then", "Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Der s\u00fc\u00df-geschwollnen Lippen.", "tokens": ["Der", "s\u00fc\u00df\u00b7ge\u00b7schwoll\u00b7nen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Hier sauget/ raubt und klaubet.", "tokens": ["Hier", "sau\u00b7get", "/", "raubt", "und", "klau\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Sonst wer den Honig raubet/", "tokens": ["Sonst", "wer", "den", "Ho\u00b7nig", "rau\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Und diesen Flu\u00df will lecken/", "tokens": ["Und", "die\u00b7sen", "Flu\u00df", "will", "le\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "\nden wird ein Gifft anstecken.", "tokens": ["den", "wird", "ein", "Gifft", "an\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Ein Gifft das mich verf\u00fchret/", "tokens": ["Ein", "Gifft", "das", "mich", "ver\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Das mir den Tod gebieret/", "tokens": ["Das", "mir", "den", "Tod", "ge\u00b7bie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Da ich noch nichts gesp\u00fcret/", "tokens": ["Da", "ich", "noch", "nichts", "ge\u00b7sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Und auch noch nichts ber\u00fchret.", "tokens": ["Und", "auch", "noch", "nichts", "be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Wie wird es dem ergehen/", "tokens": ["Wie", "wird", "es", "dem", "er\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Der sich will unterstehen", "tokens": ["Der", "sich", "will", "un\u00b7ter\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Zu k\u00fcssen ihre H\u00f6hen?", "tokens": ["Zu", "k\u00fcs\u00b7sen", "ih\u00b7re", "H\u00f6\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}