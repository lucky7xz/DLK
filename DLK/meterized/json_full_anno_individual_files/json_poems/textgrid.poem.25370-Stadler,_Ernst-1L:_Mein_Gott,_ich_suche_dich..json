{"textgrid.poem.25370": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Gott, ich suche dich.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott, ich suche dich.", "tokens": ["Mein", "Gott", ",", "ich", "su\u00b7che", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sieh mich vor deiner Schwelle knien", "tokens": ["Sieh", "mich", "vor", "dei\u00b7ner", "Schwel\u00b7le", "kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Einla\u00df betteln.", "tokens": ["Und", "Ein\u00b7la\u00df", "bet\u00b7teln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Sieh, ich bin verirrt, mich rei\u00dfen tausend Wege", "tokens": ["Sieh", ",", "ich", "bin", "ver\u00b7irrt", ",", "mich", "rei\u00b7\u00dfen", "tau\u00b7send", "We\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "CARD", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "fort ins Blinde,", "tokens": ["fort", "ins", "Blin\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und keiner tr\u00e4gt mich heim.", "tokens": ["Und", "kei\u00b7ner", "tr\u00e4gt", "mich", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df mich in deiner G\u00e4rten Obdach fliehn,", "tokens": ["La\u00df", "mich", "in", "dei\u00b7ner", "G\u00e4r\u00b7ten", "Ob\u00b7dach", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sich in ihrer Mittagsstille", "tokens": ["Da\u00df", "sich", "in", "ih\u00b7rer", "Mit\u00b7tags\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "mein versprengtes Leben wiederfinde.", "tokens": ["mein", "ver\u00b7spreng\u00b7tes", "Le\u00b7ben", "wie\u00b7der\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ich bin nur stets", "tokens": ["Ich", "bin", "nur", "stets"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "den bunten Lichtern nachgerannt,", "tokens": ["den", "bun\u00b7ten", "Lich\u00b7tern", "nach\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nach Wundern gierend, bis mir Leben,", "tokens": ["Nach", "Wun\u00b7dern", "gie\u00b7rend", ",", "bis", "mir", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wunsch und Ziel in Nacht verschwanden.", "tokens": ["Wunsch", "und", "Ziel", "in", "Nacht", "ver\u00b7schwan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nun graut der Tag. Nun fragt mein Herz", "tokens": ["Nun", "graut", "der", "Tag", ".", "Nun", "fragt", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "in seiner Taten Kerker eingespannt", "tokens": ["in", "sei\u00b7ner", "Ta\u00b7ten", "Ker\u00b7ker", "ein\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Voll Angst den Sinn", "tokens": ["Voll", "Angst", "den", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "der wirren und verbrausten Stunden.", "tokens": ["der", "wir\u00b7ren", "und", "ver\u00b7braus\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und keine Antwort kommt.", "tokens": ["Und", "kei\u00b7ne", "Ant\u00b7wort", "kommt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Ich f\u00fchle, was mein Bord an letzten Frachten tr\u00e4gt,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "was", "mein", "Bord", "an", "letz\u00b7ten", "Frach\u00b7ten", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In Wetterst\u00fcrmen", "tokens": ["In", "Wet\u00b7ter\u00b7st\u00fcr\u00b7men"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "ziellos durch die Meere schwanken,", "tokens": ["ziel\u00b7los", "durch", "die", "Mee\u00b7re", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und das im Morgen k\u00fchn und fahrtenfroh sich wiegte,", "tokens": ["Und", "das", "im", "Mor\u00b7gen", "k\u00fchn", "und", "fahr\u00b7ten\u00b7froh", "sich", "wieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "ADJD", "KON", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "meines Lebens Schiff zerschl\u00e4gt", "tokens": ["mei\u00b7nes", "Le\u00b7bens", "Schiff", "zer\u00b7schl\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "An dem Magnetberg eines irren Schicksals", "tokens": ["An", "dem", "Mag\u00b7net\u00b7berg", "ei\u00b7nes", "ir\u00b7ren", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "seine Planken. \u2013", "tokens": ["sei\u00b7ne", "Plan\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Still, Seele! Kennst du deine eigne Heimat nicht?", "tokens": ["Still", ",", "See\u00b7le", "!", "Kennst", "du", "dei\u00b7ne", "eig\u00b7ne", "Hei\u00b7mat", "nicht", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh doch: du bist in dir. Das ungewisse Licht,", "tokens": ["Sieh", "doch", ":", "du", "bist", "in", "dir", ".", "Das", "un\u00b7ge\u00b7wis\u00b7se", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PPER", "VAFIN", "APPR", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das dich verwirrte, war die ewige Lampe,", "tokens": ["Das", "dich", "ver\u00b7wirr\u00b7te", ",", "war", "die", "e\u00b7wi\u00b7ge", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die vor deines Lebens Altar brennt.", "tokens": ["die", "vor", "dei\u00b7nes", "Le\u00b7bens", "Al\u00b7tar", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Was zitterst du im Dunkel?", "tokens": ["Was", "zit\u00b7terst", "du", "im", "Dun\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bist du selber nicht das Instrument,", "tokens": ["Bist", "du", "sel\u00b7ber", "nicht", "das", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Darin der Aufruhr aller T\u00f6ne", "tokens": ["Da\u00b7rin", "der", "Auf\u00b7ruhr", "al\u00b7ler", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "sich zu hochzeitlichem Reigen schlingt?", "tokens": ["sich", "zu", "hoch\u00b7zeit\u00b7li\u00b7chem", "Rei\u00b7gen", "schlingt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "H\u00f6rst du die Kinderstimme nicht,", "tokens": ["H\u00f6rst", "du", "die", "Kin\u00b7der\u00b7stim\u00b7me", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "die aus der Tiefe leise dir entgegensingt?", "tokens": ["die", "aus", "der", "Tie\u00b7fe", "lei\u00b7se", "dir", "ent\u00b7ge\u00b7gen\u00b7singt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00fchlst nicht das reine Auge,", "tokens": ["F\u00fchlst", "nicht", "das", "rei\u00b7ne", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "das sich \u00fcber deiner N\u00e4chte wildste beugt \u2013", "tokens": ["das", "sich", "\u00fc\u00b7ber", "dei\u00b7ner", "N\u00e4ch\u00b7te", "wilds\u00b7te", "beugt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "O Brunnen, der aus gleichen Eutern tr\u00fcb", "tokens": ["O", "Brun\u00b7nen", ",", "der", "aus", "glei\u00b7chen", "Eu\u00b7tern", "tr\u00fcb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und klare Quellen s\u00e4ugt,", "tokens": ["und", "kla\u00b7re", "Quel\u00b7len", "s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Windrose deines Schicksals,", "tokens": ["Wind\u00b7ro\u00b7se", "dei\u00b7nes", "Schick\u00b7sals", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Sturm, Gewitternacht und sanftes Meer,", "tokens": ["Sturm", ",", "Ge\u00b7wit\u00b7ter\u00b7nacht", "und", "sanf\u00b7tes", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Dir selber alles:", "tokens": ["Dir", "sel\u00b7ber", "al\u00b7les", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Fegefeuer, Himmelfahrt und ewige Wiederkehr \u2013", "tokens": ["Fe\u00b7ge\u00b7feu\u00b7er", ",", "Him\u00b7mel\u00b7fahrt", "und", "e\u00b7wi\u00b7ge", "Wie\u00b7der\u00b7kehr", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Sieh doch, dein letzter Wunsch,", "tokens": ["Sieh", "doch", ",", "dein", "letz\u00b7ter", "Wunsch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "nach dem dein Leben hei\u00dfe H\u00e4nde ausgereckt,", "tokens": ["nach", "dem", "dein", "Le\u00b7ben", "hei\u00b7\u00dfe", "H\u00e4n\u00b7de", "aus\u00b7ge\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Stand schimmernd schon", "tokens": ["Stand", "schim\u00b7mernd", "schon"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "am Himmel deiner fr\u00fchsten Sehnsucht aufgesteckt.", "tokens": ["am", "Him\u00b7mel", "dei\u00b7ner", "fr\u00fchs\u00b7ten", "Sehn\u00b7sucht", "auf\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dein Schmerz und deine Lust lag immer schon", "tokens": ["Dein", "Schmerz", "und", "dei\u00b7ne", "Lust", "lag", "im\u00b7mer", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "in dir verschlossen wie in einem Schrein,", "tokens": ["in", "dir", "ver\u00b7schlos\u0