{"textgrid.poem.61761": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Siege", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr in hohen L\u00fcften", "tokens": ["Habt", "ihr", "in", "ho\u00b7hen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Donnerton geh\u00f6rt", "tokens": ["Den", "Don\u00b7ner\u00b7ton", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Forbach aus den Kl\u00fcften,", "tokens": ["Von", "For\u00b7bach", "aus", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Wei\u00dfenburg und W\u00f6rth?", "tokens": ["Von", "Wei\u00b7\u00dfen\u00b7burg", "und", "W\u00f6rth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Gottes Engel jagen", "tokens": ["Wie", "Got\u00b7tes", "En\u00b7gel", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Boten her vom Krieg:", "tokens": ["Die", "Bo\u00b7ten", "her", "vom", "Krieg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drei Schlachten sind geschlagen,", "tokens": ["Drei", "Schlach\u00b7ten", "sind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und jede Schlacht war Sieg.", "tokens": ["Und", "je\u00b7de", "Schlacht", "war", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Preis euch, ihr tapfern Bayern", "tokens": ["Preis", "euch", ",", "ihr", "tap\u00b7fern", "Bay\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Stahlhart und wetterbraun,", "tokens": ["Stahl\u00b7hart", "und", "wet\u00b7ter\u00b7braun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr den W\u00fcstengeiern", "tokens": ["Die", "ihr", "den", "W\u00fcs\u00b7ten\u00b7gei\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zuerst gestutzt die Klaun!", "tokens": ["Zu\u00b7erst", "ge\u00b7stutzt", "die", "Klaun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Preu\u00dfens Aar zusammen", "tokens": ["Mit", "Preu\u00b7\u00dfens", "Aar", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie trutztet ihr dem Tod,", "tokens": ["Wie", "trutz\u00b7tet", "ihr", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch \u00fcber euch in Flammen", "tokens": ["Hoch", "\u00fc\u00b7ber", "euch", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Reiches Morgenrot!", "tokens": ["Des", "Rei\u00b7ches", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ihr vom Gau der Chatten", "tokens": ["Und", "ihr", "vom", "Gau", "der", "Chat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ihr vom Neckarstrand", "tokens": ["Und", "ihr", "vom", "Ne\u00b7ckarst\u00b7rand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die aus Waldesschatten", "tokens": ["Und", "die", "aus", "Wal\u00b7des\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Th\u00fcringens H\u00f6hn gesandt,", "tokens": ["Th\u00fc\u00b7rin\u00b7gens", "H\u00f6hn", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ihr bracht, zum Keil gegliedert,", "tokens": ["Ihr", "bracht", ",", "zum", "Keil", "ge\u00b7glie\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Prachtgeschwader Sto\u00df;", "tokens": ["Der", "Pracht\u00b7ge\u00b7schwa\u00b7der", "Sto\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Traun, was sich so verbr\u00fcdert,", "tokens": ["Traun", ",", "was", "sich", "so", "ver\u00b7br\u00fc\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Das l\u00e4\u00dft sich nimmer los.", "tokens": ["Das", "l\u00e4\u00dft", "sich", "nim\u00b7mer", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und die ihr todverwegen,", "tokens": ["Und", "die", "ihr", "tod\u00b7ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Leichen rings umt\u00fcrmt,", "tokens": ["Von", "Lei\u00b7chen", "rings", "um\u00b7t\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im dichten Eisenregen", "tokens": ["Im", "dich\u00b7ten", "Ei\u00b7sen\u00b7re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den roten Fels erst\u00fcrmt,", "tokens": ["Den", "ro\u00b7ten", "Fels", "er\u00b7st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo blieb vor euch das Pochen", "tokens": ["Wo", "blieb", "vor", "euch", "das", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf Frankreichs Waffenruhm?", "tokens": ["Auf", "Fran\u00b7kreichs", "Waf\u00b7fen\u00b7ruhm", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Zauber ist gebrochen,", "tokens": ["Sein", "Zau\u00b7ber", "ist", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nachbricht das Kaisertum.", "tokens": ["Nach\u00b7bricht", "das", "Kai\u00b7ser\u00b7tum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So sitzt denn auf, ihr Reiter,", "tokens": ["So", "sitzt", "denn", "auf", ",", "ihr", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Rossen gebt den Sporn", "tokens": ["Den", "Ros\u00b7sen", "gebt", "den", "Sporn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tragt die Losung weiter:", "tokens": ["Und", "tragt", "die", "Lo\u00b7sung", "wei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hie Gott und deutscher Zorn!", "tokens": ["Hie", "Gott", "und", "deut\u00b7scher", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon lie\u00df der Wolf im Garne", "tokens": ["Schon", "lie\u00df", "der", "Wolf", "im", "Gar\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein blutig St\u00fcck vom Vlies,", "tokens": ["Ein", "blu\u00b7tig", "St\u00fcck", "vom", "Vlies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Maas hindurch, die Marne,", "tokens": ["Die", "Maas", "hin\u00b7durch", ",", "die", "Mar\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf, hetzt ihn bis Paris!", "tokens": ["Auf", ",", "hetzt", "ihn", "bis", "Pa\u00b7ris", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und ob die wunden Glieder", "tokens": ["Und", "ob", "die", "wun\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit der Verzweiflung Kraft", "tokens": ["Mit", "der", "Ver\u00b7zwei\u00b7flung", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er dort noch einmal wieder", "tokens": ["Er", "dort", "noch", "ein\u00b7mal", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Empor zum Sprunge rafft:", "tokens": ["Em\u00b7por", "zum", "Sprun\u00b7ge", "rafft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich schreckt nicht mehr sein Rasen,", "tokens": ["Dich", "schreckt", "nicht", "mehr", "sein", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O greiser Heldenf\u00fcrst!", "tokens": ["O", "grei\u00b7ser", "Hel\u00b7den\u00b7f\u00fcrst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df die Posaunen blasen,", "tokens": ["La\u00df", "die", "Po\u00b7sau\u00b7nen", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Babels Feste birst.", "tokens": ["Und", "Ba\u00b7bels", "Fes\u00b7te", "birst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der feigen Welt zum Neide", "tokens": ["Der", "fei\u00b7gen", "Welt", "zum", "Nei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann sei dein Werk vollf\u00fchrt.", "tokens": ["Dann", "sei", "dein", "Werk", "voll\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und du, nur du entscheide", "tok