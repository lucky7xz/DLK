{"textgrid.poem.41835": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist die Zeit, wo in Pal\u00e4sten", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die Zeit, wo in Pal\u00e4sten", "tokens": ["Das", "ist", "die", "Zeit", ",", "wo", "in", "Pa\u00b7l\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gas und Kerzenschein erhellt,", "tokens": ["Von", "Gas", "und", "Ker\u00b7zen\u00b7schein", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Tanz und Schmaus geladnen G\u00e4sten", "tokens": ["Zu", "Tanz", "und", "Schmaus", "ge\u00b7lad\u00b7nen", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Glanz getaucht erscheint die Welt.", "tokens": ["In", "Glanz", "ge\u00b7taucht", "er\u00b7scheint", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist die Zeit wo Schlitten klingeln", "tokens": ["Das", "ist", "die", "Zeit", "wo", "Schlit\u00b7ten", "klin\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "PWAV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf des Eises glatter Bahn", "tokens": ["Und", "auf", "des", "Ei\u00b7ses", "glat\u00b7ter", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Paare auch sich tanzend ringeln,", "tokens": ["Die", "Paa\u00b7re", "auch", "sich", "tan\u00b7zend", "rin\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich fliehen bald und bald sich nahn.", "tokens": ["Sich", "flie\u00b7hen", "bald", "und", "bald", "sich", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "KON", "ADV", "PRF", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist's, wo in Hauses Enge", "tokens": ["Die", "Zeit", "ist's", ",", "wo", "in", "Hau\u00b7ses", "En\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich alt und jung zusammen schlie\u00dft", "tokens": ["Sich", "alt", "und", "jung", "zu\u00b7sam\u00b7men", "schlie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fern von eitler Pracht Gedr\u00e4nge,", "tokens": ["Und", "fern", "von", "eit\u00b7ler", "Pracht", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heimisch trautes Gl\u00fcck genie\u00dft.", "tokens": ["Ein", "hei\u00b7misch", "trau\u00b7tes", "Gl\u00fcck", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenns drau\u00dfen schneit, gern am Kamine", "tokens": ["Wenns", "drau\u00b7\u00dfen", "schneit", ",", "gern", "am", "Ka\u00b7mi\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man einsam ferner Zeit gedenkt,", "tokens": ["Man", "ein\u00b7sam", "fer\u00b7ner", "Zeit", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald l\u00e4chelnd, bald mit Forschermiene", "tokens": ["Bald", "l\u00e4\u00b7chelnd", ",", "bald", "mit", "For\u00b7scher\u00b7mie\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In R\u00e4tselfragen sich versenkt.", "tokens": ["In", "R\u00e4t\u00b7sel\u00b7fra\u00b7gen", "sich", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Zeit ist's wo in kalter Kammer", "tokens": ["Die", "Zeit", "ist's", "wo", "in", "kal\u00b7ter", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Dunkel herrscht und bittre Not", "tokens": ["Nur", "Dun\u00b7kel", "herrscht", "und", "bitt\u00b7re", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Eis gefriert in allem Jammer", "tokens": ["Zu", "Eis", "ge\u00b7friert", "in", "al\u00b7lem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wasser und das St\u00fcckchen Brot!", "tokens": ["Das", "Was\u00b7ser", "und", "das", "St\u00fcck\u00b7chen", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und drau\u00dfen auch im Feld, und Garten", "tokens": ["Und", "drau\u00b7\u00dfen", "auch", "im", "Feld", ",", "und", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6glein klagen matt und weh \u2013", "tokens": ["Die", "V\u00f6\u00b7glein", "kla\u00b7gen", "matt", "und", "weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Menschenliebe alle warten,", "tokens": ["Auf", "Men\u00b7schen\u00b7lie\u00b7be", "al\u00b7le", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo grausam herrschen Eis und Schnee.", "tokens": ["Wo", "grau\u00b7sam", "herr\u00b7schen", "Eis", "und", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das ist die Zeit im Dunkeln tr\u00e4umen", "tokens": ["Das", "ist", "die", "Zeit", "im", "Dun\u00b7keln", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sinnen \u00fcber reich und arm \u2013", "tokens": ["Und", "sin\u00b7nen", "\u00fc\u00b7ber", "reich", "und", "arm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn hier die Becher \u00fcbersch\u00e4umen", "tokens": ["Wenn", "hier", "die", "Be\u00b7cher", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dort kein Tropfen lind und warm.", "tokens": ["Und", "dort", "kein", "Trop\u00b7fen", "lind", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Im Ballsaal welken tausend Bl\u00fcten", "tokens": ["Im", "Ball\u00b7saal", "wel\u00b7ken", "tau\u00b7send", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als sch\u00f6nster Schmuck in Frauenhand \u2013", "tokens": ["Als", "sch\u00f6ns\u00b7ter", "Schmuck", "in", "Frau\u00b7en\u00b7hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind, je herrlicher sie gl\u00fchten,", "tokens": ["Sie", "sind", ",", "je", "herr\u00b7li\u00b7cher", "sie", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Reichtums, nicht der Liebe Pfand!", "tokens": ["Des", "Reich\u00b7tums", ",", "nicht", "der", "Lie\u00b7be", "Pfand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O wollt bei ihnen recht erw\u00e4gen:", "tokens": ["O", "wollt", "bei", "ih\u00b7nen", "recht", "er\u00b7w\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es sei der Frauen Ideal", "tokens": ["Es", "sei", "der", "Frau\u00b7en", "I\u00b7deal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um sich zu breiten Trost und Segen \u2013", "tokens": ["Um", "sich", "zu", "brei\u00b7ten", "Trost", "und", "Se\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst ist das Leben \u00f6d und schal.", "tokens": ["Sonst", "ist", "das", "Le\u00b7ben", "\u00f6d", "und", "schal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer nicht im Winter denkt der Armen", "tokens": ["Wer", "nicht", "im", "Win\u00b7ter", "denkt", "der", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Segen auszustreuen wei\u00df,", "tokens": ["Und", "Se\u00b7gen", "aus\u00b7zu\u00b7streu\u00b7en", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird nie zu sch\u00f6ner Glut erwarmen,", "tokens": ["Wird", "nie", "zu", "sch\u00f6\u00b7ner", "Glut", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schmilzt allenthalben auch das Eis!", "tokens": ["Schmilzt", "al\u00b7len\u00b7thal\u00b7ben", "auch", "das", "Eis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "F\u00fcr solche ist kein Lenzeswehen,", "tokens": ["F\u00fcr", "sol\u00b7che", "ist", "kein", "Len\u00b7zes\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein V\u00f6gelein voll Dank und Preis \u2013", "tokens": ["Kein", "V\u00f6\u00b7ge\u00b7lein", "voll", "Dank", "und", "Preis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "NN", "KON", "NN", "$("], "meter": "