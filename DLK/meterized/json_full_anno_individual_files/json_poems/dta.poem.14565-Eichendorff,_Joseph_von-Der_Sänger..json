{"dta.poem.14565": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Eh' noch die Sonn' versunken:", "tokens": ["Eh'", "noch", "die", "Sonn'", "ver\u00b7sun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh'n durch die gold'nen Funken", "tokens": ["Geh'n", "durch", "die", "gold'\u00b7nen", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Still Engel in den Thalen,", "tokens": ["Still", "En\u00b7gel", "in", "den", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das giebt so leuchtend Strahlen", "tokens": ["Das", "giebt", "so", "leuch\u00b7tend", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Blumen rings und Zweigen. \u2014", "tokens": ["In", "Blu\u00b7men", "rings", "und", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie frommer Wiederhall", "tokens": ["Wie", "from\u00b7mer", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weht noch der Glocken Schall,", "tokens": ["Weht", "noch", "der", "Glo\u00b7cken", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wenn l\u00e4ngst die Th\u00e4ler schweigen.", "tokens": ["Wenn", "l\u00e4ngst", "die", "Th\u00e4\u00b7ler", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}