{"textgrid.poem.41280": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Res est sacra miser. Seneca", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder, den die Hand des schweren Schicksals kr\u00fcmmt,", "tokens": ["Ein", "je\u00b7der", ",", "den", "die", "Hand", "des", "schwe\u00b7ren", "Schick\u00b7sals", "kr\u00fcmmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem sie den letzten Hauch der m\u00fcden Hoffnung nimmt,", "tokens": ["Dem", "sie", "den", "letz\u00b7ten", "Hauch", "der", "m\u00fc\u00b7den", "Hoff\u00b7nung", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat ein bethr\u00e4ntes Recht zum Mitleid aller Herzen;", "tokens": ["Hat", "ein", "be\u00b7thr\u00e4n\u00b7tes", "Recht", "zum", "Mit\u00b7leid", "al\u00b7ler", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur Henker kitzeln sich bei andrer Schmach und Schmerzen.", "tokens": ["Nur", "Hen\u00b7ker", "kit\u00b7zeln", "sich", "bei", "an\u00b7drer", "Schmach", "und", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Gro\u00dfmuth ist voll Glimpf: sie hilft, sie schonet nur;", "tokens": ["Die", "Gro\u00df\u00b7muth", "ist", "voll", "Glimpf", ":", "sie", "hilft", ",", "sie", "scho\u00b7net", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und diese Regung kr\u00f6nt die sittliche Natur.", "tokens": ["Und", "die\u00b7se", "Re\u00b7gung", "kr\u00f6nt", "die", "sitt\u00b7li\u00b7che", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wie? wenn Fehler uns um Sturz und Abgrund leiten? ...", "tokens": ["Doch", "wie", "?", "wenn", "Feh\u00b7ler", "uns", "um", "Sturz", "und", "Ab\u00b7grund", "lei\u00b7ten", "?", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "$.", "KOUS", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wen straft kein Selbstbetrug? Wie menschlich ist's zu gleiten?", "tokens": ["Wen", "straft", "kein", "Selbst\u00b7be\u00b7trug", "?", "Wie", "menschlich", "ist's", "zu", "glei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Auch ein