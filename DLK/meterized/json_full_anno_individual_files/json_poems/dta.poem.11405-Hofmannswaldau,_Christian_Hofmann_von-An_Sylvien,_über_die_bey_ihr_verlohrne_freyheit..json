{"dta.poem.11405": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien,  \n \u00fcber die bey ihr verlohrne freyheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe n\u00e4chst bey dir die freyheit eingeb\u00fc\u00dft,", "tokens": ["Ich", "ha\u00b7be", "n\u00e4chst", "bey", "dir", "die", "frey\u00b7heit", "ein\u00b7ge\u00b7b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00f6chte mir sie gerne wiederholen:", "tokens": ["Und", "m\u00f6ch\u00b7te", "mir", "sie", "ger\u00b7ne", "wie\u00b7der\u00b7ho\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn deine sch\u00f6nheit hat mir sie gewi\u00df gestohlen,", "tokens": ["Denn", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "hat", "mir", "sie", "ge\u00b7wi\u00df", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wei\u00df, da\u00df sie die gr\u00f6ste diebin ist.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "sie", "die", "gr\u00f6s\u00b7te", "die\u00b7bin", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum kan ich nicht umhin, es hilfft kein schreyn noch fluchen;", "tokens": ["Drum", "kan", "ich", "nicht", "um\u00b7hin", ",", "es", "hilfft", "kein", "schreyn", "noch", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich mu\u00df sie durch und durch besuchen.", "tokens": ["Ich", "mu\u00df", "sie", "durch", "und", "durch", "be\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}