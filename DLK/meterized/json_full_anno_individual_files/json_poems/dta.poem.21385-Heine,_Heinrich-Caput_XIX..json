{"dta.poem.21385": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O, Danton, du hast dich sehr geirrt", "tokens": ["O", ",", "Dan\u00b7ton", ",", "du", "hast", "dich", "sehr", "ge\u00b7irrt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und mu\u00dftest den Irthum b\u00fc\u00dfen!", "tokens": ["Und", "mu\u00df\u00b7test", "den", "Ir\u00b7thum", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mitnehmen kann man das Vaterland", "tokens": ["Mit\u00b7neh\u00b7men", "kann", "man", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PIS", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An den Sohlen, an den F\u00fc\u00dfen.", "tokens": ["An", "den", "Soh\u00b7len", ",", "an", "den", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}