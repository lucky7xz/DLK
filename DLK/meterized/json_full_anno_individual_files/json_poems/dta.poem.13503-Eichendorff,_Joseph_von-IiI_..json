{"dta.poem.13503": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber die begl\u00e4nzten Gipfel", "tokens": ["Ue\u00b7ber", "die", "be\u00b7gl\u00e4nz\u00b7ten", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fernher kommt es wie ein Gr\u00fc\u00dfen,", "tokens": ["Fern\u00b7her", "kommt", "es", "wie", "ein", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcsternd neigen sich die Wipfel", "tokens": ["Fl\u00fcs\u00b7ternd", "nei\u00b7gen", "sich", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ob sie sich wollten k\u00fcssen.", "tokens": ["Als", "ob", "sie", "sich", "woll\u00b7ten", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ist er doch so sch\u00f6n und milde!", "tokens": ["Ist", "er", "doch", "so", "sch\u00f6n", "und", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmen gehen durch die Nacht,", "tokens": ["Stim\u00b7men", "ge\u00b7hen", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen heimlich von dem Bilde \u2014", "tokens": ["Sin\u00b7gen", "heim\u00b7lich", "von", "dem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ich bin so froh verwacht!", "tokens": ["Ach", ",", "ich", "bin", "so", "froh", "ver\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Plaudert nicht so laut, ihr Quellen!", "tokens": ["Plau\u00b7dert", "nicht", "so", "laut", ",", "ihr", "Quel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wissen darf es nicht der Morgen!", "tokens": ["Wis\u00b7sen", "darf", "es", "nicht", "der", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Mondnacht linde Wellen,", "tokens": ["In", "der", "Mond\u00b7nacht", "lin\u00b7de", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Senk' ich stille Gl\u00fcck und Sorgen. \u2014", "tokens": ["Senk'", "ich", "stil\u00b7le", "Gl\u00fcck", "und", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}