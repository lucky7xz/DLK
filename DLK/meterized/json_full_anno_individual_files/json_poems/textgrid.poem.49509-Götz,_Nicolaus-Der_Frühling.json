{"textgrid.poem.49509": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu begierig, unsre stille H\u00fctten,", "tokens": ["Zu", "be\u00b7gie\u00b7rig", ",", "uns\u00b7re", "stil\u00b7le", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unser Tibur wieder zu besuchen,", "tokens": ["Un\u00b7ser", "Ti\u00b7bur", "wie\u00b7der", "zu", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlich sich, Aquilo zum Troze, heimlich", "tokens": ["Schlich", "sich", ",", "A\u00b7qui\u00b7lo", "zum", "Tro\u00b7ze", ",", "heim\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "PRF", "$,", "NE", "APPRART", "NN", "$,", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unser Thal herein, ein S\u00f6hnchen Zephirs.", "tokens": ["Un\u00b7ser", "Thal", "her\u00b7ein", ",", "ein", "S\u00f6hn\u00b7chen", "Ze\u00b7phirs", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Durch sein Sorgen \u00fcberzog die Th\u00e4ler", "tokens": ["Durch", "sein", "Sor\u00b7gen", "\u00fc\u00b7ber\u00b7zog", "die", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bald ein Teppichwerk von blauen Veilgen,", "tokens": ["Bald", "ein", "Tep\u00b7pich\u00b7werk", "von", "blau\u00b7en", "Veil\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Die die Luft gelinde parfumirten.", "tokens": ["Die", "die", "Luft", "ge\u00b7lin\u00b7de", "par\u00b7fu\u00b7mir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unterm M\u00e4ntelchen der zarten Fl\u00fcgel", "tokens": ["Un\u00b7term", "M\u00e4n\u00b7tel\u00b7chen", "der", "zar\u00b7ten", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Bracht es viele, halb nur flicke, Amors,", "tokens": ["Bracht", "es", "vie\u00b7le", ",", "halb", "nur", "fli\u00b7cke", ",", "A\u00b7mors", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "ADJD", "ADV", "VVFIN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Welche piepend B\u00fcsch' und G\u00e4rten f\u00fcllten.", "tokens": ["Wel\u00b7che", "pie\u00b7pend", "B\u00fcsch'", "und", "G\u00e4r\u00b7ten", "f\u00fcll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Seit der Stunde zwitschern, wie betrunken,", "tokens": ["Seit", "der", "Stun\u00b7de", "zwit\u00b7schern", ",", "wie", "be\u00b7trun\u00b7ken", ","], "