{"dta.poem.8334": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken bei den Obstfr\u00fcch-  \n ten, welche unansehnlich sind, und  \n doch sch\u00f6n schmekken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Dinge \u00e4userlicher Schein,", "tokens": ["Der", "Din\u00b7ge", "\u00e4u\u00b7ser\u00b7li\u00b7cher", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schalen sind es nicht allein,", "tokens": ["Die", "Scha\u00b7len", "sind", "es", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die bei den nuzbarlichen Sachen,", "tokens": ["Die", "bei", "den", "nuz\u00b7bar\u00b7li\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die G\u00fcte und den Werth aus-", "tokens": ["Die", "G\u00fc\u00b7te", "und", "den", "Werth", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Fr\u00fcchte die von aussen sch\u00f6n,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "die", "von", "aus\u00b7sen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und herrlich, lieblich anzusehn,", "tokens": ["Und", "herr\u00b7lich", ",", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die k\u00f6nnen \u00f6fters, bei dem Schmekken,", "tokens": ["Die", "k\u00f6n\u00b7nen", "\u00f6f\u00b7ters", ",", "bei", "dem", "Schmek\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als herbe, Ekkel gnug erwekken.", "tokens": ["Als", "her\u00b7be", ",", "Ek\u00b7kel", "gnug", "er\u00b7wek\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hingegen die so schroffig hart,", "tokens": ["Hin\u00b7ge\u00b7gen", "die", "so", "schrof\u00b7fig", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sind \u00f6fters von recht m\u00fcrber Art,", "tokens": ["Sind", "\u00f6f\u00b7ters", "von", "recht", "m\u00fcr\u00b7ber", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit einem solchen Safft erf\u00fcllet,", "tokens": ["Mit", "ei\u00b7nem", "sol\u00b7chen", "Safft", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der lieblich auf der Zunge quillet.", "tokens": ["Der", "lieb\u00b7lich", "auf", "der", "Zun\u00b7ge", "quil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich merke hiebei dieses an,", "tokens": ["Ich", "mer\u00b7ke", "hie\u00b7bei", "die\u00b7ses", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df man dem Schein nicht trauen kan,", "tokens": ["Da\u00df", "man", "dem", "Schein", "nicht", "trau\u00b7en", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Beim Glanz der \u00e4userlichen Schalen,", "tokens": ["Beim", "Glanz", "der", "\u00e4u\u00b7ser\u00b7li\u00b7chen", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Fehlt ja der Kern zu vielenmahlen.", "tokens": ["Fehlt", "ja", "der", "Kern", "zu", "vie\u00b7len\u00b7mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Menschen deren Angesicht,", "tokens": ["Die", "Men\u00b7schen", "de\u00b7ren", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Offt aussieht als ein tr\u00fcbes Licht,", "tokens": ["Offt", "aus\u00b7sieht", "als", "ein", "tr\u00fc\u00b7bes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die offt ganz saur und m\u00fcrrisch scheinen,", "tokens": ["Die", "offt", "ganz", "saur", "und", "m\u00fcr\u00b7risch", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sind lieblicher, als wie wir meinen.", "tokens": ["Sind", "lieb\u00b7li\u00b7cher", ",", "als", "wie", "wir", "mei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Hingegen wo die Freundligkeit,", "tokens": ["Hin\u00b7ge\u00b7gen", "wo", "die", "Freund\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Vergn\u00fcgte Blikke von sich streut,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7te", "Blik\u00b7ke", "von", "sich", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wo Anmut im Gesicht zu lesen,", "tokens": ["Wo", "An\u00b7mut", "im", "Ge\u00b7sicht", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ist offt ein m\u00fcrrisch saures Wesen.", "tokens": ["Ist", "offt", "ein", "m\u00fcr\u00b7risch", "sau\u00b7res", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wer also nach den Schein urtheilt,", "tokens": ["Wer", "al\u00b7so", "nach", "den", "Schein", "ur\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der schliesset \u00f6fters \u00fcbereilt:", "tokens": ["Der", "schlies\u00b7set", "\u00f6f\u00b7ters", "\u00fc\u00b7be\u00b7reilt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Man mu\u00df erst eine Prob anstellen,", "tokens": ["Man", "mu\u00df", "erst", "ei\u00b7ne", "Prob", "an\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "So kan man recht ein Urtheil f\u00e4llen.", "tokens": ["So", "kan", "man", "recht", "ein", "Ur\u00b7theil", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}