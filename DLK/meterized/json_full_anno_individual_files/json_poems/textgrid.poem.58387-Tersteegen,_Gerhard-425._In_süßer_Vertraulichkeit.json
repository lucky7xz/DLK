{"textgrid.poem.58387": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "425. In s\u00fc\u00dfer Vertraulichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleibe still und wohlgemut, innig froh und klein,", "tokens": ["Blei\u00b7be", "still", "und", "wohl\u00b7ge\u00b7mut", ",", "in\u00b7nig", "froh", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Was dein s\u00fc\u00df' Vertrauen kr\u00e4nkt und die Liebe st\u00f6ret,", "tokens": ["Was", "dein", "s\u00fc\u00df'", "Ver\u00b7trau\u00b7en", "kr\u00e4nkt", "und", "die", "Lie\u00b7be", "st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was den Grund verdunkeln will, la\u00df in dich nicht ein \u2013", "tokens": ["Was", "den", "Grund", "ver\u00b7dun\u00b7keln", "will", ",", "la\u00df", "in", "dich", "nicht", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN", "$,", "VVIMP", "APPR", "PPER", "PTKNEG", "ART", "$("], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nur in deines Freundes Herz lieblich eingekehret!", "tokens": ["Nur", "in", "dei\u00b7nes", "Freun\u00b7des", "Herz", "lieb\u00b7lich", "ein\u00b7ge\u00b7keh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Wer nichts will als Gott allein,", "tokens": ["Wer", "nichts", "will", "als", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "KOUS", "NN", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Darf getrost im Glauben sein.", "tokens": ["Darf", "ge\u00b7trost", "im", "Glau\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bleibe still und wohlgemut, innig froh und klein,", "tokens": ["Blei\u00b7be", "still", "und", "wohl\u00b7ge\u00b7mut", ",", "in\u00b7nig", "froh", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Was dein s\u00fc\u00df' Vertrauen kr\u00e4nkt und die Liebe st\u00f6ret,", "tokens": ["Was", "dein", "s\u00fc\u00df'", "Ver\u00b7trau\u00b7en", "kr\u00e4nkt", "und", "die", "Lie\u00b7be", "st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was den Grund verdunkeln will, la\u00df in dich nicht ein \u2013", "tokens": ["Was", "den", "Grund", "ver\u00b7dun\u00b7keln", "will", ",", "la\u00df", "in", "dich", "nicht", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "A