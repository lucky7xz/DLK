{"dta.poem.7906": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Scheiden und Meiden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "So soll ich nun dich meiden,", "tokens": ["So", "soll", "ich", "nun", "dich", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meines Lebens Lust!", "tokens": ["Du", "mei\u00b7nes", "Le\u00b7bens", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du k\u00fcssest mich zum Scheiden,", "tokens": ["Du", "k\u00fcs\u00b7sest", "mich", "zum", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich dr\u00fccke dich an die Brust.", "tokens": ["Ich", "dr\u00fc\u00b7cke", "dich", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach Liebchen! hei\u00dft das meiden,", "tokens": ["Ach", "Lieb\u00b7chen", "!", "hei\u00dft", "das", "mei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn man sich herzt und k\u00fc\u00dft?", "tokens": ["Wenn", "man", "sich", "herzt", "und", "k\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach Liebchen! hei\u00dft das scheiden,", "tokens": ["Ach", "Lieb\u00b7chen", "!", "hei\u00dft", "das", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn man sich fest umschlie\u00dft?", "tokens": ["Wenn", "man", "sich", "fest", "um\u00b7schlie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}