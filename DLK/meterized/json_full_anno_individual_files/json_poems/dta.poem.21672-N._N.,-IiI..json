{"dta.poem.21672": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Helden sind gewichen aus der Welt", "tokens": ["Die", "Hel\u00b7den", "sind", "ge\u00b7wi\u00b7chen", "aus", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Zwerge herrschen, wo die Riesen stritten.", "tokens": ["Und", "Zwer\u00b7ge", "herr\u00b7schen", ",", "wo", "die", "Rie\u00b7sen", "strit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Israel, du Ceder fest und stark,", "tokens": ["O", "Is\u00b7rael", ",", "du", "Ce\u00b7der", "fest", "und", "stark", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hochragend stolz in aller V\u00f6lker Mitten,", "tokens": ["Hoch\u00b7ra\u00b7gend", "stolz", "in", "al\u00b7ler", "V\u00f6l\u00b7ker", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Dein Wipfel sinkt, du bist verdorrt im Mark.", "tokens": ["Dein", "Wip\u00b7fel", "sinkt", ",", "du", "bist", "ver\u00b7dorrt", "im", "Mark", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Bl\u00e4tterschmuck des Ruhmes dir entf\u00e4llt \u2014", "tokens": ["Der", "Bl\u00e4t\u00b7ter\u00b7schmuck", "des", "Ruh\u00b7mes", "dir", "ent\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Zebaoth, wie hast du das gelitten?", "tokens": ["O", "Ze\u00b7bao\u00b7th", ",", "wie", "hast", "du", "das", "ge\u00b7lit\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "VAFIN", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort ruhn, wo sich zum Thal Gilboa neigt,", "tokens": ["Dort", "ruhn", ",", "wo", "sich", "zum", "Thal", "Gil\u00b7boa", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PRF", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Edelsten in Israel erschlagen;", "tokens": ["Die", "E\u00b7dels\u00b7ten", "in", "Is\u00b7rael", "er\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Die K\u00f6nigsblume hat der Tod gepfl\u00fcckt.", "tokens": ["Die", "K\u00f6\u00b7nigs\u00b7blu\u00b7me", "hat", "der", "Tod", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umsonst die Eichen ja gen Himmel ragen:", "tokens": ["Um\u00b7sonst", "die", "Ei\u00b7chen", "ja", "gen", "Him\u00b7mel", "ra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Blitzstrahl zuckt \u2014 die Helden sind geknickt!", "tokens": ["Ein", "Blitz\u00b7strahl", "zuckt", "die", "Hel\u00b7den", "sind", "ge\u00b7knickt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein Zorn hat unsre Nacken tief gebeugt \u2014", "tokens": ["Dein", "Zorn", "hat", "uns\u00b7re", "Na\u00b7cken", "tief", "ge\u00b7beugt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer aber wollte drob zu murren wagen?", "tokens": ["Wer", "a\u00b7ber", "woll\u00b7te", "drob", "zu", "mur\u00b7ren", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie Simson nehme ich mein Saitenspiel,", "tokens": ["Wie", "Sim\u00b7son", "neh\u00b7me", "ich", "mein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die heilige Harfe von den Trauerweiden:", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Har\u00b7fe", "von", "den", "Trau\u00b7er\u00b7wei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von unserm tiefen Wehe sing\u2019 ich gern \u2014", "tokens": ["Von", "un\u00b7serm", "tie\u00b7fen", "We\u00b7he", "sing'", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mag sich der Feind an unserm Jammer weiden!", "tokens": ["Mag", "sich", "der", "Feind", "an", "un\u00b7serm", "Jam\u00b7mer", "wei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch dann wie Simson la\u00dft uns flehn zum Herrn,", "tokens": ["Doch", "dann", "wie", "Sim\u00b7son", "la\u00dft", "uns", "flehn", "zum", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NE", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis ihres Stolzes Tempel niederfiel,", "tokens": ["Bis", "ih\u00b7res", "Stol\u00b7zes", "Tem\u00b7pel", "nie\u00b7der\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201eda\u00df unsre Seele sterbe mit den Heiden!\u201c", "tokens": ["\u201e", "da\u00df", "uns\u00b7re", "See\u00b7le", "ster\u00b7be", "mit", "den", "Hei\u00b7den", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Berge von Gilboa, seid verflucht,", "tokens": ["Ihr", "Ber\u00b7ge", "von", "Gil\u00b7boa", ",", "seid", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dieweil des Herrn Gesalbter dort erstochen!", "tokens": ["Die\u00b7weil", "des", "Herrn", "Ge\u00b7salb\u00b7ter", "dort", "er\u00b7sto\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Regen netze euch noch Himmelsthau!", "tokens": ["Nicht", "Re\u00b7gen", "net\u00b7ze", "euch", "noch", "Him\u00b7mel\u00b7sthau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn Saul, des Starken, Schild ward dort zerbrochen \u2014", "tokens": ["Denn", "Saul", ",", "des", "Star\u00b7ken", ",", "Schild", "ward", "dort", "zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "NN", "$,", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum sollt ihr werden w\u00fcst und \u00f6d\u2019 und rauh!", "tokens": ["Drum", "sollt", "ihr", "wer\u00b7den", "w\u00fcst", "und", "\u00f6d'", "und", "rauh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Sonnenstrahl der Helden Gruft besucht,", "tokens": ["Kein", "Son\u00b7nen\u00b7strahl", "der", "Hel\u00b7den", "Gruft", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis ", "tokens": ["Bis"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Ich trage Leid um dich, mein Jonathan!", "tokens": ["Ich", "tra\u00b7ge", "Leid", "um", "dich", ",", "mein", "Jo\u00b7na\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz, o Bruder, ist mit dir gegangen.", "tokens": ["Mein", "Herz", ",", "o", "Bru\u00b7der", ",", "ist", "mit", "dir", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Frauenliebe heilt die Wunde nicht.", "tokens": ["Die", "Frau\u00b7en\u00b7lie\u00b7be", "heilt", "die", "Wun\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit diesen Thr\u00e4nen flieht von meinen Wangen", "tokens": ["Mit", "die\u00b7sen", "Thr\u00e4\u00b7nen", "flieht", "von", "mei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Lebens Mai, mein Herz f\u00fcr immer bricht.", "tokens": ["Des", "Le\u00b7bens", "Mai", ",", "mein", "Herz", "f\u00fcr", "im\u00b7mer", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein irdisch Gl\u00fcck dich mir ersetzen kann:", "tokens": ["Kein", "ir\u00b7disch", "Gl\u00fcck", "dich", "mir", "er\u00b7set\u00b7zen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du gingst \u2014 und Gl\u00fcck und Jugend sind vergangen!", "tokens": ["Du", "gingst", "und", "Gl\u00fcck", "und", "Ju\u00b7gend", "sind", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}