{"dta.poem.10028": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fabel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde sahe j\u00fcngst der L\u00fcffte sch\u00f6nes blau,", "tokens": ["Die", "Er\u00b7de", "sa\u00b7he", "j\u00fcngst", "der", "L\u00fcff\u00b7te", "sch\u00f6\u00b7nes", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit einem kleinen Neid, halb eifers\u00fcchtig an,", "tokens": ["Mit", "ei\u00b7nem", "klei\u00b7nen", "Neid", ",", "halb", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sprach: stoltziere nur, mit deinem blauem Licht,", "tokens": ["Und", "sprach", ":", "stolt\u00b7zie\u00b7re", "nur", ",", "mit", "dei\u00b7nem", "blau\u00b7em", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So \u00fcberm\u00fcthig nicht,", "tokens": ["So", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil ich, sowol als du, dergleichen zeigen kann.", "tokens": ["Weil", "ich", ",", "so\u00b7wol", "als", "du", ",", "derg\u00b7lei\u00b7chen", "zei\u00b7gen", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KON", "KOUS", "PPER", "$,", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schau mein Ultramarin; betrachte, wie der Pfau", "tokens": ["Schau", "mein", "Ult\u00b7ra\u00b7ma\u00b7rin", ";", "be\u00b7trach\u00b7te", ",", "wie", "der", "Pfau"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Jm blauen Schimmer prangt; schau den Sapphir. Vor", "tokens": ["Jm", "blau\u00b7en", "Schim\u00b7mer", "prangt", ";", "schau", "den", "Sap\u00b7phir", ".", "Vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$.", "ADJD", "ART", "NN", "$.", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Kann ich dir der Gentianellen", "tokens": ["Kann", "ich", "dir", "der", "Gen\u00b7ti\u00b7a\u00b7nel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Fast blendend blan entgegen stellen.", "tokens": ["Fast", "blen\u00b7dend", "blan", "ent\u00b7ge\u00b7gen", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Jhr voller Glantz mu\u00df dir,", "tokens": ["Ihr", "vol\u00b7ler", "Glantz", "mu\u00df", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Trotz deiner blauen Zier,", "tokens": ["Trotz", "dei\u00b7ner", "blau\u00b7en", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Noch mehr als du dir selbst gefallen kannst, gefallen.", "tokens": ["Noch", "mehr", "als", "du", "dir", "selbst", "ge\u00b7fal\u00b7len", "kannst", ",", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Lufft nahm diesen Hohn f\u00fcr kein Verh\u00f6hnen an;", "tokens": ["Die", "Lufft", "nahm", "die\u00b7sen", "Hohn", "f\u00fcr", "kein", "Ver\u00b7h\u00f6h\u00b7nen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vielmehr besahe sie, vergn\u00fcgt und sonder Neid,", "tokens": ["Viel\u00b7mehr", "be\u00b7sa\u00b7he", "sie", ",", "ver\u00b7gn\u00fcgt", "und", "son\u00b7der", "Neid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von diesem sch\u00f6nen Fr\u00fchlings-Kinde,", "tokens": ["Von", "die\u00b7sem", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das dem Saphir fast gleiche Kleid,", "tokens": ["Das", "dem", "Sa\u00b7phir", "fast", "glei\u00b7che", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Und lispelte darauf gelinde", "tokens": ["Und", "lis\u00b7pel\u00b7te", "da\u00b7rauf", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Erde diese Worte zu:", "tokens": ["Der", "Er\u00b7de", "die\u00b7se", "Wor\u00b7te", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich sehe deinen Schmuck nicht sonder Freuden.", "tokens": ["Ich", "se\u00b7he", "dei\u00b7nen", "Schmuck", "nicht", "son\u00b7der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "War\u00fcm besiehest du", "tokens": ["Wa\u00b7r\u00fcm", "be\u00b7sie\u00b7hest", "du"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Den meinen nicht auf gleiche Weise?", "tokens": ["Den", "mei\u00b7nen", "nicht", "auf", "glei\u00b7che", "Wei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "La\u00df uns doch, ohn uns zu beneiden,", "tokens": ["La\u00df", "uns", "doch", ",", "ohn", "uns", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Uns, da wir alle beide sch\u00f6n,", "tokens": ["Uns", ",", "da", "wir", "al\u00b7le", "bei\u00b7de", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "PIAT", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mit Freud und Aumuth, Dem zum Preise,", "tokens": ["Mit", "Freud", "und", "Au\u00b7muth", ",", "Dem", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der unser aller Quell und Ursprung ist, besehn!", "tokens": ["Der", "un\u00b7ser", "al\u00b7ler", "Quell", "und", "Ur\u00b7sprung", "ist", ",", "be\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "PIAT", "NN", "KON", "NN", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "La\u00df uns vielmehr uns in die Wette schm\u00fccken;", "tokens": ["La\u00df", "uns", "viel\u00b7mehr", "uns", "in", "die", "Wet\u00b7te", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Damit, wenn Geister uns erblicken,", "tokens": ["Da\u00b7mit", ",", "wenn", "Geis\u00b7ter", "uns", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die mit Verstand begabt, durch ein erstaunt entz\u00fccken,", "tokens": ["Die", "mit", "Ver\u00b7stand", "be\u00b7gabt", ",", "durch", "ein", "er\u00b7staunt", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "$,", "APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie in uns beyden GOTT, die Quell des Lichts, erh\u00f6hn.", "tokens": ["Sie", "in", "uns", "bey\u00b7den", "GoTT", ",", "die", "Quell", "des", "Lichts", ",", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PIAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn, sonder Glantz und Strahl Desselben Sonnen-", "tokens": ["Denn", ",", "son\u00b7der", "Glantz", "und", "Strahl", "Des\u00b7sel\u00b7ben", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KON", "NN", "KON", "NN", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Lichts,", "tokens": ["Lichts", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Sind wir, nicht nur nicht sch\u00f6n; wir sind ein wircklich", "tokens": ["Sind", "wir", ",", "nicht", "nur", "nicht", "sch\u00f6n", ";", "wir", "sind", "ein", "wir\u00b7ck\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PTKNEG", "ADV", "PTKNEG", "ADJD", "$.", "PPER", "VAFIN", "ART", "ADJD"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Nichts.", "tokens": ["Nichts", "."], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "+", "measure": "single.up"}, "line.34": {"text": "La\u00df deine sch\u00f6ne blaue Bluhme", "tokens": ["La\u00df", "dei\u00b7ne", "sch\u00f6\u00b7ne", "blau\u00b7e", "Bluh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Denn k\u00fcnfftig, zu des Sch\u00f6pfers Ruhme,", "tokens": ["Denn", "k\u00fcnff\u00b7tig", ",", "zu", "des", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "In einem blauen Feuer bl\u00fchen:", "tokens": ["In", "ei\u00b7nem", "blau\u00b7en", "Feu\u00b7er", "bl\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ich will, wie vor, zu Seiner Ehr,", "tokens": ["Ich", "will", ",", "wie", "vor", ",", "zu", "Sei\u00b7ner", "Ehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und zwar noch immer mehr und mehr", "tokens": ["Und", "zwar", "noch", "im\u00b7mer", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "In meinem blauen Schimmer gl\u00fchen.", "tokens": ["In", "mei\u00b7nem", "blau\u00b7en", "Schim\u00b7mer", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}