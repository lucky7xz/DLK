{"textgrid.poem.60566": {"metadata": {"author": {"name": "Lange, Samuel Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die St\u00fcrme legen sich, die Luft wird w\u00e4rmer,", "genre": "verse", "period": "N.A.", "pub_year": 1746, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die St\u00fcrme legen sich, die Luft wird w\u00e4rmer,", "tokens": ["Die", "St\u00fcr\u00b7me", "le\u00b7gen", "sich", ",", "die", "Luft", "wird", "w\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gr\u00fcnen Bl\u00e4tter kleiden schon die Aeste;", "tokens": ["Die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "klei\u00b7den", "schon", "die", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Luft riecht von der bunten Bl\u00fcte wieder;", "tokens": ["Die", "Luft", "riecht", "von", "der", "bun\u00b7ten", "Bl\u00fc\u00b7te", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun schm\u00fcckt sich das Jahr.", "tokens": ["Nun", "schm\u00fcckt", "sich", "das", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die br\u00fcnstge Nachtigall lockt nun die Gattin,", "tokens": ["Die", "br\u00fcnst\u00b7ge", "Nach\u00b7ti\u00b7gall", "lockt", "nun", "die", "Gat\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der weisse Mondenschein erhellt die N\u00e4chte,", "tokens": ["Der", "weis\u00b7se", "Mon\u00b7den\u00b7schein", "er\u00b7hellt", "die", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit weichem Gras bepolstert sich der Rasen,", "tokens": ["Mit", "wei\u00b7chem", "Gras", "be\u00b7pols\u00b7tert", "sich", "der", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wartet auf dich.", "tokens": ["Und", "war\u00b7tet", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Die Freundschaft und die Tugend, nebst der Dichtkunst", "tokens": ["Die", "Freund\u00b7schaft", "und", "die", "Tu\u00b7gend", ",", "nebst", "der", "Dicht\u00b7kunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umfassen sich. In leichten frohen Reihen", "tokens": ["Um\u00b7fas\u00b7sen", "sich", ".", "In", "leich\u00b7ten", "fro\u00b7hen", "Rei\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$.", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ber\u00fchren sie das Gras mit leichten F\u00fcssen,", "tokens": ["Be\u00b7r\u00fch\u00b7ren", "sie", "das", "Gras", "mit", "leich\u00b7ten", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und singen von dir.", "tokens": ["Und", "sin\u00b7gen", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Auf gr\u00fcnem Blatt bringt Doris gelbe Butter,", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Blatt", "bringt", "Do\u00b7ris", "gel\u00b7be", "But\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schneidet l\u00e4chelnd Scheiben von dem Schincken.", "tokens": ["Und", "schnei\u00b7det", "l\u00e4\u00b7chelnd", "Schei\u00b7ben", "von", "dem", "Schin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zufriedenheit und ungeschminckt Vertrauen", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "und", "un\u00b7ge\u00b7schminckt", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vers\u00fcssen die Kost.", "tokens": ["Ver\u00b7s\u00fcs\u00b7sen", "die", "Kost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Hier, komm und setze dich zu deinem Damon,", "tokens": ["Hier", ",", "komm", "und", "set\u00b7ze", "dich", "zu", "dei\u00b7nem", "Da\u00b7mon", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stimm dein scharffes Spiel etwas herunter.", "tokens": ["Und", "stimm", "dein", "scharf\u00b7fes", "Spiel", "et\u00b7was", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis es in meines klingt, und Doris, singe.", "tokens": ["Bis", "es", "in", "mei\u00b7nes", "klingt", ",", "und", "Do\u00b7ris", ",", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "VVFIN", "$,", "KON", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was fehlet uns denn?", "tokens": ["Was", "feh\u00b7let", "uns", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Die Unschuld bringt in meines Hilas Minen,", "tokens": ["Die", "Un\u00b7schuld", "bringt", "in", "mei\u00b7nes", "Hi\u00b7las", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liebkosend einen Strau\u00df von Wiesenbl\u00fcmchen,", "tokens": ["Lieb\u00b7ko\u00b7send", "ei\u00b7nen", "Strau\u00df", "von", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lallt dir zu. Gef\u00e4llt dir das mein Thirsis,", "tokens": ["Und", "lallt", "dir", "zu", ".", "Ge\u00b7f\u00e4llt", "dir", "das", "mein", "Thir\u00b7sis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So eile zu mir.", "tokens": ["So", "ei\u00b7le", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Sobald dich Pan von weiten wird erblicken,", "tokens": ["So\u00b7bald", "dich", "Pan", "von", "wei\u00b7ten", "wird", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "ADJA", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird er den Faunen und den Nymphen wincken,", "tokens": ["Wird", "er", "den", "Fau\u00b7nen", "und", "den", "Nym\u00b7phen", "win\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Zephyrs werden in den Bl\u00e4ttern rauschen,", "tokens": ["Die", "Ze\u00b7phyrs", "wer\u00b7den", "in", "den", "Bl\u00e4t\u00b7tern", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich zu begr\u00fcssen.", "tokens": ["Dich", "zu", "be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Schertz, die Anmuth, \u00fcben schon die Fl\u00fcgel,", "tokens": ["Der", "Schertz", ",", "die", "An\u00b7muth", ",", "\u00fc\u00b7ben", "schon", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und flattern dir auf halbem Weg entgegen,", "tokens": ["Und", "flat\u00b7tern", "dir", "auf", "hal\u00b7bem", "Weg", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kommen noch einmal so munter wieder", "tokens": ["Und", "kom\u00b7men", "noch", "ein\u00b7mal", "so", "mun\u00b7ter", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit dir zur\u00fccke.", "tokens": ["Mit", "dir", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Wir sorgen nicht, wer noch wird Kaiser werden;", "tokens": ["Wir", "sor\u00b7gen", "nicht", ",", "wer", "noch", "wird", "Kai\u00b7ser", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "ADV", "VAFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Franckreich auch im Ernst den Frieden liebe.", "tokens": ["Ob", "Fran\u00b7ck\u00b7reich", "auch", "im", "Ernst", "den", "Frie\u00b7den", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Ruh, die Dichtkunst, und ein gut Gewissen,", "tokens": ["Die", "Ruh", ",", "die", "Dicht\u00b7kunst", ",", "und", "ein", "gut", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Raubt uns kein Schicksal.", "tokens": ["Raubt", "uns", "kein", "Schick\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Und w\u00fcrde Mars uns dreye nicht verschonen,", "tokens": ["Und", "w\u00fcr\u00b7de", "Mars", "uns", "drey\u00b7e", "nicht", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PPER", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fcrde Doris ihren Hilas tragen,", "tokens": ["So", "w\u00fcr\u00b7de", "Do\u00b7ris", "ih\u00b7ren", "Hi\u00b7las", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir beyde f\u00fchrten sie, und suchten singend", "tokens": ["Wir", "bey\u00b7de", "f\u00fchr\u00b7ten", "sie", ",", "und", "such\u00b7ten", "sin\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einsame W\u00fcsten.", "tokens": ["Ein\u00b7sa\u00b7me", "W\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Und da bewohnten wir Dianens Grotten,", "tokens": ["Und", "da", "be\u00b7wohn\u00b7ten", "wir", "Di\u00b7a\u00b7nens", "Grot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und holten fr\u00f6lich Wasser, Holtz und Kr\u00e4uter.", "tokens": ["Und", "hol\u00b7ten", "fr\u00f6\u00b7lich", "Was\u00b7ser", ",", "Holtz", "und", "Kr\u00e4u\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entfernt von Lastern, unter deinen Lehren,", "tokens": ["Ent\u00b7fernt", "von", "Las\u00b7tern", ",", "un\u00b7ter", "dei\u00b7nen", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erw\u00fcchse mein Kind.", "tokens": ["Er\u00b7w\u00fcch\u00b7se", "mein", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Die Engel w\u00fcrden oft bey deinem Spiele,", "tokens": ["Die", "En\u00b7gel", "w\u00fcr\u00b7den", "oft", "bey", "dei\u00b7nem", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jugendlicher Sch\u00f6nheit, bey uns sichtbar,", "tokens": ["In", "ju\u00b7gend\u00b7li\u00b7cher", "Sch\u00f6n\u00b7heit", ",", "bey", "uns", "sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihrer Harf bey unsrer Fl\u00f6t und Leyer", "tokens": ["Mit", "ih\u00b7rer", "Harf", "bey", "uns\u00b7rer", "Fl\u00f6t", "und", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Den Vater zu loben.", "tokens": ["Den", "Va\u00b7ter", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Da s\u00e4hn wir auch in hundert graden B\u00e4umen", "tokens": ["Da", "s\u00e4hn", "wir", "auch", "in", "hun\u00b7dert", "gra\u00b7den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den eingeschnittnen Namen Doris wachsen;", "tokens": ["Den", "ein\u00b7ge\u00b7schnitt\u00b7nen", "Na\u00b7men", "Do\u00b7ris", "wach\u00b7sen", ";"], "token_info": ["word", "word", "word", 