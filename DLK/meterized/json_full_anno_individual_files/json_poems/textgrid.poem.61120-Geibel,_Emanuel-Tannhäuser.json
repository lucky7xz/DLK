{"textgrid.poem.61120": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Tannh\u00e4user", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird die Nacht so l\u00fcstern!", "tokens": ["Wie", "wird", "die", "Nacht", "so", "l\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie bl\u00fcht so reich der Wald!", "tokens": ["Wie", "bl\u00fcht", "so", "reich", "der", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In allen Wipfeln fl\u00fcstern", "tokens": ["In", "al\u00b7len", "Wip\u00b7feln", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Stimmen mannigfalt.", "tokens": ["Viel", "Stim\u00b7men", "man\u00b7nig\u00b7falt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die B\u00e4chlein blinken und rauschen,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "blin\u00b7ken", "und", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Blumen duften und gl\u00fchn,", "tokens": ["Die", "Blu\u00b7men", "duf\u00b7ten", "und", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die Marmorbilder lauschen", "tokens": ["Die", "Mar\u00b7mor\u00b7bil\u00b7der", "lau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hervor aus dunklem Gr\u00fcn.", "tokens": ["Her\u00b7vor", "aus", "dunk\u00b7lem", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall ruft: Zur\u00fcck! zur\u00fcck!", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ruft", ":", "Zu\u00b7r\u00fcck", "!", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Knab' schickt nur voraus den Blick;", "tokens": ["Der", "Knab'", "schickt", "nur", "vo\u00b7raus", "den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Herz ist wild, sein Sinn getr\u00fcbt,", "tokens": ["Sein", "Herz", "ist", "wild", ",", "sein", "Sinn", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergessen alles, was er liebt.", "tokens": ["Ver\u00b7ges\u00b7sen", "al\u00b7les", ",", "was", "er", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er kommt zum Schlo\u00df im Garten,", "tokens": ["Er", "kommt", "zum", "Schlo\u00df", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Fenster sind voll Glanz,", "tokens": ["Die", "Fens\u00b7ter", "sind", "voll", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Tor die Pagen warten,", "tokens": ["Am", "Tor", "die", "Pa\u00b7gen", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und droben klingt der Tanz.", "tokens": ["Und", "dro\u00b7ben", "klingt", "der", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er schreitet hinauf die Treppen,", "tokens": ["Er", "schrei\u00b7tet", "hin\u00b7auf", "die", "Trep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Er tritt hinein in den Saal,", "tokens": ["Er", "tritt", "hin\u00b7ein", "in", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da rauschen die Sammetschleppen,", "tokens": ["Da", "rau\u00b7schen", "die", "Sam\u00b7metsc\u00b7hlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da blinkt der Goldpokal.", "tokens": ["Da", "blinkt", "der", "Gold\u00b7po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Nachtigall ruft: Zur\u00fcck! zur\u00fcck!", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ruft", ":", "Zu\u00b7r\u00fcck", "!", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Knab' schickt nur voraus den Blick;", "tokens": ["Der", "Knab'", "schickt", "nur", "vo\u00b7raus", "den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Herz ist wild, sein Sinn getr\u00fcbt,", "tokens": ["Sein", "Herz", "ist", "wild", ",", "sein", "Sinn", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergessen alles, was er liebt.", "tokens": ["Ver\u00b7ges\u00b7sen", "al\u00b7les", ",", "was", "er", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die sch\u00f6nste von den Frauen", "tokens": ["Die", "sch\u00f6ns\u00b7te", "von", "den", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Reicht ihm den Becher hin,", "tokens": ["Reicht", "ihm", "den", "Be\u00b7cher", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihm rinnt ein s\u00fc\u00dfes Grauen", "tokens": ["Ihm", "rinnt", "ein", "s\u00fc\u00b7\u00dfes", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seltsam durch Herz und Sinn.", "tokens": ["Selt\u00b7sam", "durch", "Herz", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Er leert ihn bis zum Grunde,", "tokens": ["Er", "leert", "ihn", "bis", "zum", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da spricht am Tor der Zwerg:", "tokens": ["Da", "spricht", "am", "Tor", "der", "Zwerg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbder unsre bist zur Stunde,", "tokens": ["\u00bb", "der", "uns\u00b7re", "bist", "zur", "Stun\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dies ist der Venusberg.\u00ab", "tokens": ["Dies", "ist", "der", "Ve\u00b7nus\u00b7berg", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Nachtigall ruft nur noch von fern,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ruft", "nur", "noch", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Knaben treibt sein b\u00f6ser Stern;", "tokens": ["Den", "Kna\u00b7ben", "treibt", "sein", "b\u00f6\u00b7ser", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Herz ist wild, sein Sinn getr\u00fcbt,", "tokens": ["Sein", "Herz", "ist", "wild", ",", "sein", "Sinn", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergessen alles, was er liebt.", "tokens": ["Ver\u00b7ges\u00b7sen", "al\u00b7les", ",", "was", "er", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und endlich fort vom Reigen", "tokens": ["Und", "end\u00b7lich", "fort", "vom", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fchrt ihn das sch\u00f6ne Weib;", "tokens": ["F\u00fchrt", "ihn", "das", "sch\u00f6\u00b7ne", "Weib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Auge blickt so eigen,", "tokens": ["Ihr", "Au\u00b7ge", "blickt", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlockend gl\u00fcht ihr Leib.", "tokens": ["Ver\u00b7lo\u00b7ckend", "gl\u00fcht", "ihr", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fern von des Fests Gewimmel,", "tokens": ["Fern", "von", "des", "Fests", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da bl\u00fchen die Lauben so dicht -", "tokens": ["Da", "bl\u00fc\u00b7hen", "die", "Lau\u00b7ben", "so", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "In Wolken birgt am Himmel", "tokens": ["In", "Wol\u00b7ken", "birgt", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Mond sein Angesicht.", "tokens": ["Der", "Mond", "sein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Nachtigall Ruf ist lang verhallt,", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "Ruf", "ist", "lang", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "