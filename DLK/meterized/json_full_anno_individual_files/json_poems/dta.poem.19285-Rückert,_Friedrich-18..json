{"dta.poem.19285": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zweifel, ob der Mensch das H\u00f6chste denken kann,", "tokens": ["Der", "Zwei\u00b7fel", ",", "ob", "der", "Mensch", "das", "H\u00f6chs\u00b7te", "den\u00b7ken", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschwindet, wenn du recht dein Denken siehest an.", "tokens": ["Ver\u00b7schwin\u00b7det", ",", "wenn", "du", "recht", "dein", "Den\u00b7ken", "sie\u00b7hest", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer denkt in deinem Geist? der h\u00f6chste Geist allein.", "tokens": ["Wer", "denkt", "in", "dei\u00b7nem", "Geist", "?", "der", "h\u00f6chs\u00b7te", "Geist", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer zweifelt, ob er selbst sich denkbar m\u00f6chte seyn?", "tokens": ["Wer", "zwei\u00b7felt", ",", "ob", "er", "selbst", "sich", "denk\u00b7bar", "m\u00f6ch\u00b7te", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PRF", "PDS", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In den Gedanken mu\u00dft du die Gedanken senken:", "tokens": ["In", "den", "Ge\u00b7dan\u00b7ken", "mu\u00dft", "du", "die", "Ge\u00b7dan\u00b7ken", "sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur weil Gott in dir denkt, vermagst du Gott zu denken.", "tokens": ["Nur", "weil", "Gott", "in", "dir", "denkt", ",", "ver\u00b7magst", "du", "Gott", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}