{"textgrid.poem.26261": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sitzen hier in der Sonne, die wir nie missen.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sitzen hier in der Sonne, die wir nie missen.", "tokens": ["Wir", "sit\u00b7zen", "hier", "in", "der", "Son\u00b7ne", ",", "die", "wir", "nie", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir sitzen hier vor gef\u00fcllten Sch\u00fcsseln und Schalen.", "tokens": ["Wir", "sit\u00b7zen", "hier", "vor", "ge\u00b7f\u00fcll\u00b7ten", "Sch\u00fcs\u00b7seln", "und", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir, die wir am Leibe hier nichts vom Kriege wissen,", "tokens": ["Wir", ",", "die", "wir", "am", "Lei\u00b7be", "hier", "nichts", "vom", "Krie\u00b7ge", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "ADV", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nichts von der Todesk\u00e4lte und den Hungersqualen.", "tokens": ["Nichts", "von", "der", "To\u00b7des\u00b7k\u00e4l\u00b7te", "und", "den", "Hun\u00b7gers\u00b7qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Daheim ist kaum Wolle zu haben und kaum Essen.", "tokens": ["Da\u00b7heim", "ist", "kaum", "Wol\u00b7le", "zu", "ha\u00b7ben", "und", "kaum", "Es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "NN", "PTKZU", "VAINF", "KON", "ADV", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der S\u00e4ugling schon lernt den l\u00e4hmenden Hunger kennen.", "tokens": ["Der", "S\u00e4ug\u00b7ling", "schon", "lernt", "den", "l\u00e4h\u00b7men\u00b7den", "Hun\u00b7ger", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr sollt die zu Hause keinen Tag hier vergessen.", "tokens": ["Ihr", "sollt", "die", "zu", "Hau\u00b7se", "kei\u00b7nen", "Tag", "hier", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "APPR", "NN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn sie auch nicht laut und dringend ihr Leiden nennen.", "tokens": ["Wenn", "sie", "auch", "nicht", "laut", "und", "drin\u00b7gend", "ihr", "Lei\u00b7den", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Kartoffeln und Brot werden ihnen knapp bemessen.", "tokens": ["Kar\u00b7tof\u00b7feln", "und", "Brot", "wer\u00b7den", "ih\u00b7nen", "knapp", "be\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenig Fleisch wird in Grammen sparsam zugewogen.", "tokens": ["We\u00b7nig", "Fleisch", "wird", "in", "Gram\u00b7men", "spar\u00b7sam", "zu\u00b7ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sie bekommen ein Ei in der Woche zu essen.", "tokens": ["Sie", "be\u00b7kom\u00b7men", "ein", "Ei", "in", "der", "Wo\u00b7che", "zu", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Fett und Zucker sind allen so gut wie entzogen.", "tokens": ["Fett", "und", "Zu\u00b7cker", "sind", "al\u00b7len", "so", "gut", "wie", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PIS", "ADV", "ADJD", "KOKOM", "VVPP", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Frauen m\u00fcssen in Stunden und Stunden warten,", "tokens": ["Die", "Frau\u00b7en", "m\u00fcs\u00b7sen", "in", "Stun\u00b7den", "und", "Stun\u00b7den", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aufgestellt auf dem Pflaster vor den Ladent\u00fcren,", "tokens": ["Auf\u00b7ge\u00b7stellt", "auf", "dem", "Pflas\u00b7ter", "vor", "den", "La\u00b7den\u00b7t\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In der Hand die Brot-, Milch- und Kartoffelkarten", "tokens": ["In", "der", "Hand", "die", "Brot", ",", "Milch", "und", "Kar\u00b7tof\u00b7fel\u00b7kar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "TRUNC", "$,", "TRUNC", "KON", "NN"], "meter": "--+--+-++-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und d\u00fcrfen nicht Ungeduld und nicht K\u00e4lte sp\u00fcren.", "tokens": ["Und", "d\u00fcr\u00b7fen", "nicht", "Un\u00b7ge\u00b7duld", "und", "nicht", "K\u00e4l\u00b7te", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie wird das Mehl vorsichtig verteilt aus dem Spinde.", "tokens": ["Wie", "wird", "das", "Mehl", "vor\u00b7sich\u00b7tig", "ver\u00b7teilt", "aus", "dem", "Spin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mancher Magen hat lange kein Fleisch mehr gesehen.", "tokens": ["Man\u00b7cher", "Ma\u00b7gen", "hat", "lan\u00b7ge", "kein", "Fleisch", "mehr", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und manche Mutter hungert und gibt ihrem Kinde,", "tokens": ["Und", "man\u00b7che", "Mut\u00b7ter", "hun\u00b7gert", "und", "gibt", "ih\u00b7rem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "CARD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sie mu\u00df tags am eisernen Schraubstocke stehen.", "tokens": ["Und", "sie", "mu\u00df", "tags", "am", "ei\u00b7ser\u00b7nen", "Schraub\u00b7sto\u00b7cke", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Seit Monaten hat sich keiner mehr satt gegessen.", "tokens": ["Seit", "Mo\u00b7na\u00b7ten", "hat", "sich", "kei\u00b7ner", "mehr", "satt", "ge\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PRF", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Was n\u00fctzt den Reichen das goldene Geld im Kasten!", "tokens": ["Was", "n\u00fctzt", "den", "Rei\u00b7chen", "das", "gol\u00b7de\u00b7ne", "Geld", "im", "Kas\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch im Schlaf wird der Krieg nicht vom Hunger vergessen.", "tokens": ["Noch", "im", "Schlaf", "wird", "der", "Krieg", "nicht", "vom", "Hun\u00b7ger", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Beide, Reiche und Arme, f\u00fcrs Vaterland fasten.", "tokens": ["Bei\u00b7de", ",", "Rei\u00b7che", "und", "Ar\u00b7me", ",", "f\u00fcrs", "Va\u00b7ter\u00b7land", "fas\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NE", "KON", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Darum sollt ihr geben und geben und noch geben,", "tokens": ["Da\u00b7rum", "sollt", "ihr", "ge\u00b7ben", "und", "ge\u00b7ben", "und", "noch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "KON", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr Deutschen hier drau\u00dfen. Denn nicht mit Blut und N\u00f6ten", "tokens": ["Ihr", "Deut\u00b7schen", "hier", "drau\u00b7\u00dfen", ".", "Denn", "nicht", "mit", "Blut", "und", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$.", "KON", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Habt ihr zu k\u00e4mpfen um jede Stunde im Leben.", "tokens": ["Habt", "ihr", "zu", "k\u00e4mp\u00b7fen", "um", "je\u00b7de", "Stun\u00b7de", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Helft Frauen und Kindern daheim, die Not zu t\u00f6ten!", "tokens": ["Helft", "Frau\u00b7en", "und", "Kin\u00b7dern", "da\u00b7heim", ",", "die", "Not", "zu", "t\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Es k\u00e4mpfen dort nicht nur M\u00e4nner im Sch\u00fctzengraben.", "tokens": ["Es", "k\u00e4mp\u00b7fen", "dort", "nicht", "nur", "M\u00e4n\u00b7ner", "im", "Sch\u00fct\u00b7zen\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Frauen, Kinder bek\u00e4mpfen den Hunger, den kalten.", "tokens": ["Frau\u00b7en", ",", "Kin\u00b7der", "be\u00b7k\u00e4mp\u00b7fen", "den", "Hun\u00b7ger", ",", "den", "kal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deutsche, h\u00f6rt! Z\u00f6gert nicht mit eures Geldes Gaben.", "tokens": ["Deut\u00b7sche", ",", "h\u00f6rt", "!", "Z\u00f6\u00b7gert", "nicht", "mit", "eu\u00b7res", "Gel\u00b7des", "Ga\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$.", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Pflicht ist jetzt: Durchhalten mit ihnen, die aushalten!", "tokens": ["Pflicht", "ist", "jetzt", ":", "Durch\u00b7hal\u00b7ten", "mit", "ih\u00b7nen", ",", "die", "aus\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$.", "NN", "APPR", "PPER", "$,", "PRELS", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Gebt ihr viel, so ist das Viel noch wenig. Bedenket:", "tokens": ["Gebt", "ihr", "viel", ",", "so", "ist", "das", "Viel", "noch", "we\u00b7nig", ".", "Be\u00b7den\u00b7ket", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "PIS", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Was geben Deutschland jene, die den Hunger geben!", "tokens": ["Was", "ge\u00b7ben", "Deutschland", "je\u00b7ne", ",", "die", "den", "Hun\u00b7ger", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "PDS", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist dagegen nicht wenig, das was ihr hier schenket?", "tokens": ["Ist", "da\u00b7ge\u00b7gen", "nicht", "we\u00b7nig", ",", "das", "was", "ihr", "hier", "schen\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "ADV", "$,", "PRELS", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die zu Haus hungern, halten die Heimat am Leben.", "tokens": ["Die", "zu", "Haus", "hun\u00b7gern", ",", "hal\u00b7ten", "die", "Hei\u00b7mat", "am", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.10": {"line.1": {"text": "Wir sitzen hier in der Sonne, die wir nie missen.", "tokens": ["Wir", "sit\u00b7zen", "hier", "in", "der", "Son\u00b7ne", ",", "die", "wir", "nie", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir sitzen hier vor gef\u00fcllten Sch\u00fcsseln und Schalen.", "tokens": ["Wir", "sit\u00b7zen", "hier", "vor", "ge\u00b7f\u00fcll\u00b7ten", "Sch\u00fcs\u00b7seln", "und", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir, die wir am Leibe hier nichts vom Kriege wissen,", "tokens": ["Wir", ",", "die", "wir", "am", "Lei\u00b7be", "hier", "nichts", "vom", "Krie\u00b7ge", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "ADV", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nichts von der Todesk\u00e4lte und den Hungersqualen.", "tokens": ["Nichts", "von", "der", "To\u00b7des\u00b7k\u00e4l\u00b7te", "und", "den", "Hun\u00b7gers\u00b7qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART",