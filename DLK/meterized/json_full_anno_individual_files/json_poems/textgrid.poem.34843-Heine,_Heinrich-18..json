{"textgrid.poem.34843": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Fortuna, ganz umsunst", "tokens": ["Frau", "For\u00b7tu\u00b7na", ",", "ganz", "um\u00b7sunst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tust du spr\u00f6de! deine Gunst", "tokens": ["Tust", "du", "spr\u00f6\u00b7de", "!", "dei\u00b7ne", "Gunst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df ich mir, durch Kampf und Ringen,", "tokens": ["Wei\u00df", "ich", "mir", ",", "durch", "Kampf", "und", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Zu erbeuten, zu erzwingen.", "tokens": ["Zu", "er\u00b7beu\u00b7ten", ",", "zu", "er\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcberw\u00e4ltigt wirst du doch,", "tokens": ["\u00dc\u00b7berw\u00b7\u00e4l\u00b7tigt", "wirst", "du", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich spanne dich ins Joch,", "tokens": ["Und", "ich", "span\u00b7ne", "dich", "ins", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du streckst am End' die Waffen \u2013", "tokens": ["Und", "du", "streckst", "am", "End'", "die", "Waf\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber meine Wunden klaffen.", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Wun\u00b7den", "klaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es verstr\u00f6mt mein rotes Blut,", "tokens": ["Es", "ver\u00b7str\u00f6mt", "mein", "ro\u00b7tes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der sch\u00f6ne Lebensmut", "tokens": ["Und", "der", "sch\u00f6\u00b7ne", "Le\u00b7bens\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will erl\u00f6schen; ich erliege", "tokens": ["Will", "er\u00b7l\u00f6\u00b7schen", ";", "ich", "er\u00b7lie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VMFIN", "VVINF", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sterbe nach dem Siege.", "tokens": ["Und", "ich", "ster\u00b7be", "nach", "dem", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text":