{"textgrid.poem.67187": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von einem Menschen sag ich, wenn der ist gut", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einem Menschen sag ich, wenn der ist gut", "tokens": ["Von", "ei\u00b7nem", "Men\u00b7schen", "sag", "ich", ",", "wenn", "der", "ist", "gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PDS", "VAFIN", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und weise, was bedarf er? Ist irgend eins,", "tokens": ["Und", "wei\u00b7se", ",", "was", "be\u00b7darf", "er", "?", "Ist", "ir\u00b7gend", "eins", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$.", "VAFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das einer Seele gn\u00fcget? ist ein Halm, ist", "tokens": ["Das", "ei\u00b7ner", "See\u00b7le", "gn\u00fc\u00b7get", "?", "ist", "ein", "Halm", ",", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDS", "ART", "NN", "VVFIN", "$.", "VAFIN", "ART", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eine gereifteste Reb auf Erden", "tokens": ["Ei\u00b7ne", "ge\u00b7reif\u00b7tes\u00b7te", "Reb", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gewachsen, die ihn n\u00e4hre? Der Sinn ist des", "tokens": ["Ge\u00b7wach\u00b7sen", ",", "die", "ihn", "n\u00e4h\u00b7re", "?", "Der", "Sinn", "ist", "des"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Also. Ein Freund ist oft die Geliebte, viel", "tokens": ["Al\u00b7so", ".", "Ein", "Freund", "ist", "oft", "die", "Ge\u00b7lieb\u00b7te", ",", "viel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "ADV"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Kunst. O Teurer, dir sag ich die Wahrheit.", "tokens": ["Die", "Kunst", ".", "O", "Teu\u00b7rer", ",", "dir", "sag", "ich", "die", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NE", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "D\u00e4dalus Geist und des Walds ist deiner.", "tokens": ["D\u00e4\u00b7da\u00b7lus", "Geist", "und", "des", "Walds", "ist", "dei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Von einem Menschen sag ich, wenn der ist gut", "tokens": ["Von", "ei\u00b7nem", "Men\u00b7schen", "sag", "ich", ",", "wenn", "der", "ist", "gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PDS", "VAFIN", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und weise, was bedarf er? Ist