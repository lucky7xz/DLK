{"textgrid.poem.24787": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang kannte er die Muscheln nicht f\u00fcr sch\u00f6n,", "tokens": ["Lang", "kann\u00b7te", "er", "die", "Mu\u00b7scheln", "nicht", "f\u00fcr", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er war zu sehr aus einer Welt mit ihnen,", "tokens": ["Er", "war", "zu", "sehr", "aus", "ei\u00b7ner", "Welt", "mit", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADV", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Duft der Hyazinthen war ihm nichts", "tokens": ["Der", "Duft", "der", "Hy\u00b7a\u00b7zin\u00b7then", "war", "ihm", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nichts das Spiegelbild der eignen Mienen.", "tokens": ["Und", "nichts", "das", "Spie\u00b7gel\u00b7bild", "der", "eig\u00b7nen", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch alle seine Tage waren so", "tokens": ["Doch", "al\u00b7le", "sei\u00b7ne", "Ta\u00b7ge", "wa\u00b7ren", "so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ge\u00f6ffnet wie ein leierf\u00f6rmig Tal,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "wie", "ein", "lei\u00b7er\u00b7f\u00f6r\u00b7mig", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darin er Herr zugleich und Knecht zugleich", "tokens": ["Da\u00b7rin", "er", "Herr", "zu\u00b7gleich", "und", "Knecht", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "ADV", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des wei\u00dfen Lebens war und ohne Wahl.", "tokens": ["Des", "wei\u00b7\u00dfen", "Le\u00b7bens", "war", "und", "oh\u00b7ne", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie einer, der noch tut, was ihm nicht ziemt,", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "noch", "tut", ",", "was", "ihm", "nicht", "ziemt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ADV", "ADJD", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch nicht f\u00fcr lange, ging er auf den Wegen:", "tokens": ["Doch", "nicht", "f\u00fcr", "lan\u00b7ge", ",", "ging", "er", "auf", "den", "We\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADV", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Heimkehr und unendlichem Gespr\u00e4ch", "tokens": ["Der", "Heim\u00b7kehr", "und", "un\u00b7end\u00b7li\u00b7chem", "Ge\u00b7spr\u00e4ch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hob seine Seele ruhig sich entgegen.", "tokens": ["Hob", "sei\u00b7ne", "See\u00b7le", "ru\u00b7hig", "sich", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lang kannte er die Muscheln nicht f\u00fcr sch\u00f6n,", "tokens": ["Lang", "kann\u00b7te", "er", "die", "Mu\u00b7scheln", "nicht", "f\u00fcr", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er war zu sehr aus einer Welt mit ihnen,", "tokens": ["Er", "war", "zu", "sehr", "aus", "ei\u00b7ner", "Welt", "mit", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADV", "APPR", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Duft der Hyazinthen war ihm nichts", "tokens": ["Der", "Duft", "der", "Hy\u00b7a\u00b7zin\u00b7then", "war", "ihm", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nichts das Spiegelbild der eignen Mienen.", "tokens": ["Und", "nichts", "das", "Spie\u00b7gel\u00b7bild", "der", "eig\u00b7nen", "Mie\u00b7nen", "."], "to