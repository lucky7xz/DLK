ine.3": {"text": "Durch den hellsten Himmelsraum,", "tokens": ["Durch", "den", "hells\u00b7ten", "Him\u00b7mels\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an zweier Engel Hand,", "tokens": ["Und", "an", "zwei\u00b7er", "En\u00b7gel", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Welches Gl\u00fcck mein Herz empfand!", "tokens": ["Wel\u00b7ches", "Gl\u00fcck", "mein", "Herz", "emp\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vergessen hatt' ich gar:", "tokens": ["Und", "ver\u00b7ges\u00b7sen", "hatt'", "ich", "gar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, das ist nicht! ach, das war!", "tokens": ["Ach", ",", "das", "ist", "nicht", "!", "ach", ",", "das", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "PTKNEG", "$.", "XY", "$,", "PDS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie nach ihrem Angesicht", "tokens": ["Wie", "nach", "ih\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich das meine wendete,", "tokens": ["Sich", "das", "mei\u00b7ne", "wen\u00b7de\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannt' ich sie und wieder nicht,", "tokens": ["Kannt'", "ich", "sie", "und", "wie\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "KON", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ihr Licht mich blendete.", "tokens": ["Weil", "ihr", "Licht", "mich", "blen\u00b7de\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O wie tr\u00fcb' es endete!", "tokens": ["O", "wie", "tr\u00fcb'", "es", "en\u00b7de\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst im Traume sprach ich klar:", "tokens": ["Selbst", "im", "Trau\u00b7me", "sprach", "ich", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, das ist nicht! ach, das war!", "tokens": ["Ach", ",", "das", "ist", "nicht", "!", "ach", ",", "das", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "PTKNEG", "$.", "XY", "$,", "PDS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn mir in den Traum hinein", "tokens": ["Wenn", "mir", "in", "den", "Traum", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.