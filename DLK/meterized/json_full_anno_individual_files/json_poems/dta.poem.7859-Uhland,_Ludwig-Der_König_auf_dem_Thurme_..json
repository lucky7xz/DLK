{"dta.poem.7859": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6nig auf dem Thurme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Da liegen sie alle, die grauen H\u00f6hn,", "tokens": ["Da", "lie\u00b7gen", "sie", "al\u00b7le", ",", "die", "grau\u00b7en", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die dunkeln Th\u00e4ler, in milder Ruh;", "tokens": ["Die", "dun\u00b7keln", "Th\u00e4\u00b7ler", ",", "in", "mil\u00b7der", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Schlummer waltet, die L\u00fcfte wehn", "tokens": ["Der", "Schlum\u00b7mer", "wal\u00b7tet", ",", "die", "L\u00fcf\u00b7te", "wehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Keinen Laut der Klage mir zu.", "tokens": ["Kei\u00b7nen", "Laut", "der", "Kla\u00b7ge", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "F\u00fcr Alle hab\u2019 ich gesorgt und gestrebt,", "tokens": ["F\u00fcr", "Al\u00b7le", "hab'", "ich", "ge\u00b7sorgt", "und", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Sorgen trank ich den funkelnden Wein;", "tokens": ["Mit", "Sor\u00b7gen", "trank", "ich", "den", "fun\u00b7keln\u00b7den", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Nacht ist gekommen, der Himmel belebt,", "tokens": ["Die", "Nacht", "ist", "ge\u00b7kom\u00b7men", ",", "der", "Him\u00b7mel", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Meine Seele will ich erfreun.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "will", "ich", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "O du goldne Schrift durch den Sterneraum!", "tokens": ["O", "du", "gold\u00b7ne", "Schrift", "durch", "den", "Ster\u00b7ne\u00b7raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zu dir ja schau\u2019 ich liebend empor.", "tokens": ["Zu", "dir", "ja", "schau'", "ich", "lie\u00b7bend", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ihr Wunderkl\u00e4nge, vernommen kaum,", "tokens": ["Ihr", "Wun\u00b7der\u00b7kl\u00e4n\u00b7ge", ",", "ver\u00b7nom\u00b7men", "kaum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie bes\u00e4uselt ihr sehnlich mein Ohr!", "tokens": ["Wie", "be\u00b7s\u00e4u\u00b7selt", "ihr", "sehn\u00b7lich", "mein", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Mein Haar ist ergraut, mein Auge getr\u00fcbt,", "tokens": ["Mein", "Haar", "ist", "er\u00b7graut", ",", "mein", "Au\u00b7ge", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Siegeswaffen h\u00e4ngen im Saal,", "tokens": ["Die", "Sie\u00b7ges\u00b7waf\u00b7fen", "h\u00e4n\u00b7gen", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Habe Recht gesprochen und Recht ge\u00fcbt,", "tokens": ["Ha\u00b7be", "Recht", "ge\u00b7spro\u00b7chen", "und", "Recht", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wann darf ich rasten einmal?", "tokens": ["Wann", "darf", "ich", "ras\u00b7ten", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "O selige Rast, wie verlang\u2019 ich dein!", "tokens": ["O", "se\u00b7li\u00b7ge", "Rast", ",", "wie", "ver\u00b7lang'", "ich", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "O herrliche Nacht, wie s\u00e4umst du so lang,", "tokens": ["O", "herr\u00b7li\u00b7che", "Nacht", ",", "wie", "s\u00e4umst", "du", "so", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da ich schaue der Sterne lichteren Schein,", "tokens": ["Da", "ich", "schau\u00b7e", "der", "Ster\u00b7ne", "lich\u00b7te\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und h\u00f6re volleren Klang!", "tokens": ["Und", "h\u00f6\u00b7re", "vol\u00b7le\u00b7ren", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}