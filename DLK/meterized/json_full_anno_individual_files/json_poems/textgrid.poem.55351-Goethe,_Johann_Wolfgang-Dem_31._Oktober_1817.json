{"textgrid.poem.55351": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Dem 31. Oktober 1817", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreihundert Jahre hat sich schon", "tokens": ["Drei\u00b7hun\u00b7dert", "Jah\u00b7re", "hat", "sich", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Protestant erwiesen,", "tokens": ["Der", "Pro\u00b7tes\u00b7tant", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihn von Papst- und T\u00fcrkenthron", "tokens": ["Da\u00df", "ihn", "von", "Papst", "und", "T\u00fcr\u00b7ken\u00b7thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befehle ba\u00df verdrie\u00dfen.", "tokens": ["Be\u00b7feh\u00b7le", "ba\u00df", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was auch der Pfaffe sinnt und schleicht,", "tokens": ["Was", "auch", "der", "Pfaf\u00b7fe", "sinnt", "und", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Prediger steht zur Wache,", "tokens": ["Der", "Pre\u00b7di\u00b7ger", "steht", "zur", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["AR