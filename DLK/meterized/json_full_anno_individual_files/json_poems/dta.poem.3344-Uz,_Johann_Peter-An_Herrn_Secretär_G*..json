{"dta.poem.3344": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Du spieltest, Freund, mit Lieb und Sch\u00f6nen,", "tokens": ["Du", "spiel\u00b7test", ",", "Freund", ",", "mit", "Lieb", "und", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als einer der sie nicht gekannt,", "tokens": ["Als", "ei\u00b7ner", "der", "sie", "nicht", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis mitten in der Lust und s\u00fcsser Saiten T\u00f6nen", "tokens": ["Bis", "mit\u00b7ten", "in", "der", "Lust", "und", "s\u00fcs\u00b7ser", "Sai\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erfahrung peinlich dich verbrannt.", "tokens": ["Er\u00b7fah\u00b7rung", "pein\u00b7lich", "dich", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So scherzt ein munters Kind mit der geliebten Katze:", "tokens": ["So", "scherzt", "ein", "mun\u00b7ters", "Kind", "mit", "der", "ge\u00b7lieb\u00b7ten", "Kat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Knabe neckt sie lang, und ihre fromme Tatze", "tokens": ["Der", "Kna\u00b7be", "neckt", "sie", "lang", ",", "und", "ih\u00b7re", "from\u00b7me", "Tat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Scheint Sammet, scheint ihm unbewehrt,", "tokens": ["Scheint", "Sam\u00b7met", ",", "scheint", "ihm", "un\u00b7be\u00b7wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis ein geschwinder Schmerz und rinnend Blut ihn", "tokens": ["Bis", "ein", "ge\u00b7schwin\u00b7der", "Schmerz", "und", "rin\u00b7nend", "Blut", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJD", "NN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "lehrt,", "tokens": ["lehrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Da\u00df auch ein artig Thierchen kratze.", "tokens": ["Da\u00df", "auch", "ein", "ar\u00b7tig", "Thier\u00b7chen", "krat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "O M\u00e4dchen! M\u00e4dchen! flieht! umsonst ist mein Be-", "tokens": ["O", "M\u00e4d\u00b7chen", "!", "M\u00e4d\u00b7chen", "!", "flieht", "!", "um\u00b7sonst", "ist", "mein", "Be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "$.", "VVFIN", "$.", "ADV", "VAFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "m\u00fchn:", "tokens": ["m\u00fchn", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Wann ihr nicht flieht, ich kann nicht fliehn;", "tokens": ["Wann", "ihr", "nicht", "flieht", ",", "ich", "kann", "nicht", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und wenn ich noch so gerne wollte,", "tokens": ["Und", "wenn", "ich", "noch", "so", "ger\u00b7ne", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und als ein Weiser sollte.", "tokens": ["Und", "als", "ein", "Wei\u00b7ser", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Denn wider ein geliebt Gesicht", "tokens": ["Denn", "wi\u00b7der", "ein", "ge\u00b7liebt", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und eine sch\u00f6ne Brust hilft alle Weisheit nicht.", "tokens": ["Und", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Brust", "hilft", "al\u00b7le", "Weis\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch schw\u00f6r ich bey dem weisen Bart", "tokens": ["Doch", "schw\u00f6r", "ich", "bey", "dem", "wei\u00b7sen", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Des ersten Stoickers, des Mannes meiner Art:", "tokens": ["Des", "ers\u00b7ten", "Stoi\u00b7ckers", ",", "des", "Man\u00b7nes", "mei\u00b7ner", "Art", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ich schw\u00f6r, und, o verzeiht, ihr M\u00e4dchen! da\u00df ich", "tokens": ["Ich", "schw\u00f6r", ",", "und", ",", "o", "ver\u00b7zeiht", ",", "ihr", "M\u00e4d\u00b7chen", "!", "da\u00df", "ich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "$,", "FM", "VVFIN", "$,", "PPOSAT", "NN", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.21": {"text": "schw\u00f6re;", "tokens": ["schw\u00f6\u00b7re", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Mein Schwur gereichet euch zur Ehre:", "tokens": ["Mein", "Schwur", "ge\u00b7rei\u00b7chet", "euch", "zur", "Eh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nie will ich euch sehr nahe seyn;", "tokens": ["Nie", "will", "ich", "euch", "sehr", "na\u00b7he", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nie will ich bey vergn\u00fcgten Wein,", "tokens": ["Nie", "will", "ich", "bey", "ver\u00b7gn\u00fcg\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wie, leider! sonst geschehn, leichtsinnig euch besin-", "tokens": ["Wie", ",", "lei\u00b7der", "!", "sonst", "ge\u00b7schehn", ",", "leicht\u00b7sin\u00b7nig", "euch", "be\u00b7sin"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "ADV", "$.", "ADV", "VVPP", "$,", "ADJD", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Soll meine Leyer ja von eurem Reiz erklingen:", "tokens": ["Soll", "mei\u00b7ne", "Le\u00b7yer", "ja", "von", "eu\u00b7rem", "Reiz", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So mach ich mich dazu mit Fasten erst bereit,", "tokens": ["So", "mach", "ich", "mich", "da\u00b7zu", "mit", "Fas\u00b7ten", "erst", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und singe fern von euch und voller Sch\u00fcchternheit.", "tokens": ["Und", "sin\u00b7ge", "fern", "von", "euch", "und", "vol\u00b7ler", "Sch\u00fcch\u00b7tern\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn o! ich seh es und mit Schmerzen:", "tokens": ["Denn", "o", "!", "ich", "seh", "es", "und", "mit", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$.", "PPER", "VVFIN", "PPER", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Es l\u00e4\u00dft mit M\u00e4dchen sich nicht scherzen.", "tokens": ["Es", "l\u00e4\u00dft", "mit", "M\u00e4d\u00b7chen", "sich", "nicht", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}