{"textgrid.poem.49481": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: O m\u00f6cht ich, so wie ihr, geliebten Bienen, seyn!", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O m\u00f6cht ich, so wie ihr, geliebten Bienen, seyn!", "tokens": ["O", "m\u00f6cht", "ich", ",", "so", "wie", "ihr", ",", "ge\u00b7lieb\u00b7ten", "Bie\u00b7nen", ",", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,", "ADJA", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An innerm Geiste gros, obwohl von C\u00f6rper klein;", "tokens": ["An", "in\u00b7nerm", "Geis\u00b7te", "gros", ",", "ob\u00b7wohl", "von", "C\u00f6r\u00b7per", "klein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "KOUS", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "M\u00f6cht ich so schnell, wie ihr, so gl\u00fccklich im Bem\u00fchen", "tokens": ["M\u00f6cht", "ich", "so", "schnell", ",", "wie", "ihr", ",", "so", "gl\u00fcck\u00b7lich", "im", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wissenschaften Feld, so weit es ist, durchziehen;", "tokens": ["Der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "Feld", ",", "so", "weit", "es", "ist", ",", "durch\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So starck durch Emsigkeit, so f\u00e4hig durch Natur,", "tokens": ["So", "starck", "durch", "Em\u00b7sig\u00b7keit", ",", "so", "f\u00e4\u00b7hig", "durch", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Kunst zu K\u00fcnsten gehn, wie ihr von Flur auf Flur;", "tokens": ["Von", "Kunst", "zu", "K\u00fcns\u00b7ten", "gehn", ",", "wie", "ihr", "von", "Flur", "auf", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$,", "PWAV", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bem\u00fcht den treuen Freund durch Nutzen zu erg\u00f6tzen;", "tokens": ["Be\u00b7m\u00fcht", "den", "treu\u00b7en", "Freund", "durch", "Nut\u00b7zen", "zu", "er\u00b7g\u00f6t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bereit dem k\u00fchnen Feind den Angel anzusetzen.", "tokens": ["Be\u00b7reit", "dem", "k\u00fch\u00b7nen", "Feind", "den", "An\u00b7gel", "an\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "al