{"dta.poem.7310": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "2.  \n An Henriette von Hohenhausen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb, o N\u00e4he; Ferne, ach wie leid;", "tokens": ["Wie", "lieb", ",", "o", "N\u00e4\u00b7he", ";", "Fer\u00b7ne", ",", "ach", "wie", "leid", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "FM", "NN", "$.", "NN", "$,", "ADV", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie bald wird Gegenwart Vergangenheit!", "tokens": ["Wie", "bald", "wird", "Ge\u00b7gen\u00b7wart", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum hat Trauer denn so matten Schritt,", "tokens": ["Wa\u00b7rum", "hat", "Trau\u00b7er", "denn", "so", "mat\u00b7ten", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da doch so leicht die frohe Stunde glitt?", "tokens": ["Da", "doch", "so", "leicht", "die", "fro\u00b7he", "Stun\u00b7de", "glitt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, wer mir liebe Stunden k\u00f6nnte bannen,", "tokens": ["Ach", ",", "wer", "mir", "lie\u00b7be", "Stun\u00b7den", "k\u00f6nn\u00b7te", "ban\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Viel werther sollt' er seyn, als der verm\u00f6chte", "tokens": ["Viel", "wert\u00b7her", "sollt'", "er", "seyn", ",", "als", "der", "ver\u00b7m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der tr\u00fcben schlaffe Sehnen anzuspannen,", "tokens": ["Der", "tr\u00fc\u00b7ben", "schlaf\u00b7fe", "Seh\u00b7nen", "an\u00b7zu\u00b7span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn Leid im Herzen wirbt sich theure Rechte,", "tokens": ["Denn", "Leid", "im", "Her\u00b7zen", "wirbt", "sich", "theu\u00b7re", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und wer es nimmt, der nimmt ein Kleinod mit.", "tokens": ["Und", "wer", "es", "nimmt", ",", "der", "nimmt", "ein", "Klei\u00b7nod", "mit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Reich' mir die Hand! du hast mich froh gemacht.", "tokens": ["Reich'", "mir", "die", "Hand", "!", "du", "hast", "mich", "froh", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "In \u00f6der Fremde hab' ich dein gedacht,", "tokens": ["In", "\u00f6\u00b7der", "Frem\u00b7de", "hab'", "ich", "dein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Werd' oft noch sinnen deinem Blicke nach,", "tokens": ["Werd'", "oft", "noch", "sin\u00b7nen", "dei\u00b7nem", "Bli\u00b7cke", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mildes Auge hellt den tr\u00fcbsten Tag.", "tokens": ["So", "mil\u00b7des", "Au\u00b7ge", "hellt", "den", "tr\u00fcbs\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df Ferne denn zur N\u00e4he sich gestalten", "tokens": ["La\u00df", "Fer\u00b7ne", "denn", "zur", "N\u00e4\u00b7he", "sich", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "ADV", "APPRART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch Wechselwort und inniges Gedenken.", "tokens": ["Durch", "Wech\u00b7sel\u00b7wort", "und", "in\u00b7ni\u00b7ges", "Ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Reich' mir die Hand! \u2014 ich will sie treulich halten,", "tokens": ["Reich'", "mir", "die", "Hand", "!", "ich", "will", "sie", "treu\u00b7lich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und dr\u00fcber her mag immergr\u00fcn sich senken", "tokens": ["Und", "dr\u00fc\u00b7ber", "her", "mag", "im\u00b7mer\u00b7gr\u00fcn", "sich", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APZR", "VMFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Tannenzweig, ein schirmend Wetterdach.", "tokens": ["Der", "Tan\u00b7nen\u00b7zweig", ",", "ein", "schir\u00b7mend", "Wet\u00b7ter\u00b7dach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}