{"dta.poem.22806": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "112. Von dem Hertzen der heiligen  \n  Clara  de Montefalco.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist der Speer und Schwam/ die N\u00e4gel/ S\u00e4ul", "tokens": ["Hier", "ist", "der", "Speer", "und", "Schwam", "/", "die", "N\u00e4\u00b7gel", "/", "S\u00e4ul"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NE", "$(", "ART", "NN", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und Kron/(Sohn:", "tokens": ["und", "Kron", "/", "(", "Sohn", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "NN", "$(", "$(", "NN", "$."], "meter": "-++", "measure": "unknown.measure.di"}, "line.3": {"text": "Die Geisseln/ und auch gar da\u00df Creutz mit GOttes-", "tokens": ["Die", "Geis\u00b7seln", "/", "und", "auch", "gar", "da\u00df", "Creutz", "mit", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "ADV", "ADV", "KOUS", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drey Kugeln eines halts: Es kan nicht anderst sein/", "tokens": ["Drey", "Ku\u00b7geln", "ei\u00b7nes", "halts", ":", "Es", "kan", "nicht", "an\u00b7derst", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df Hertz ist GOttes