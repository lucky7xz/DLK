, "sa\u00b7get", ":", "Nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VVFIN", "$.", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich? versprochen? kannst du sagen!", "tokens": ["Ich", "?", "ver\u00b7spro\u00b7chen", "?", "kannst", "du", "sa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaube mir, es kann nicht seyn.", "tokens": ["Glau\u00b7be", "mir", ",", "es", "kann", "nicht", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O, ruft Mops, wollt ich nur schw\u00f6ren", "tokens": ["O", ",", "ruft", "Mops", ",", "wollt", "ich", "nur", "schw\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "NN", "$,", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Doch Amynt belauscht uns ja:", "tokens": ["Doch", "A\u00b7mynt", "be\u00b7lauscht", "uns", "ja", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gleich sollst du es selber h\u00f6ren;", "tokens": ["Gleich", "sollst", "du", "es", "sel\u00b7ber", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ja, ich such ihn, warte da.", "tokens": ["Ja", ",", "ich",