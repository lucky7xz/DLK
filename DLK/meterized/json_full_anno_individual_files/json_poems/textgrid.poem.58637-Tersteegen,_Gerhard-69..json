{"textgrid.poem.58637": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Such nicht viel Weisheit und Gewalt, noch Geld und Gut zu", "tokens": ["Such", "nicht", "viel", "Weis\u00b7heit", "und", "Ge\u00b7walt", ",", "noch", "Geld", "und", "Gut", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "PIAT", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "R\u00fchm dich auch nicht im innern Licht geschenkter Kraft und Gaben!", "tokens": ["R\u00fchm", "dich", "auch", "nicht", "im", "in\u00b7nern", "Licht", "ge\u00b7schenk\u00b7ter", "Kraft", "und", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKNEG", "APPRART", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Es ist des Herren allzumal, doch ist er's selber nicht,", "tokens": ["Es", "ist", "des", "Her\u00b7ren", "all\u00b7zu\u00b7mal", ",", "doch", "ist", "er's", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "ADV", "VAFIN", "PIS", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Drum bleib mit Gott allein vergn\u00fcgt, wenn alles sonst gebricht!", "tokens": ["Drum", "bleib", "mit", "Gott", "al\u00b7lein", "ver\u00b7gn\u00fcgt", ",", "wenn", "al\u00b7les", "sonst", "ge\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "NN", "ADV", "VVPP", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Wie sch\u00f6n und lauter ist der Sinn, der auf Gott selbst nur bauet,", "tokens": ["Wie", "sch\u00f6n", "und", "lau\u00b7ter", "ist", "der", "Sinn", ",", "der", "auf", "Gott", "selbst", "nur", "bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Der, was er ist und kann und hat, nicht als sein eigen schauet,", "tokens": ["Der", ",", "was", "er", "ist", "und", "kann", "und", "hat", ",", "nicht", "als", "sein", "ei\u00b7gen", "schau\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "VAFIN", "KON", "VMFIN", "KON", "VAFIN", "$,", "PTKNEG", "KOUS", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Der seinen Ruhm und h\u00f6chste Freud' in Gott alleine stellt,", "tokens": ["Der", "sei\u00b7nen", "Ruhm", "und", "h\u00f6chs\u00b7te", "Freud'", "in", "Gott", "al\u00b7lei\u00b7ne", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Da\u00df er dies h\u00f6chste Gut erkennt, da\u00df er dem Gott gef\u00e4llt!", "tokens": ["Da\u00df", "er", "dies", "h\u00f6chs\u00b7te", "Gut", "er\u00b7kennt", ",