{"dta.poem.20022": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Abendschiffahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn von heiliger Kapelle", "tokens": ["Wenn", "von", "hei\u00b7li\u00b7ger", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abendglocke fromm erschallet,", "tokens": ["A\u00b7bend\u00b7glo\u00b7cke", "fromm", "er\u00b7schal\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiller dann das Schiff auch wallet", "tokens": ["Stil\u00b7ler", "dann", "das", "Schiff", "auch", "wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die himmelblaue Welle;", "tokens": ["Durch", "die", "him\u00b7mel\u00b7blau\u00b7e", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann sinkt Schiffer betend nieder,", "tokens": ["Dann", "sinkt", "Schif\u00b7fer", "be\u00b7tend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und wie von dem Himmel helle", "tokens": ["Und", "wie", "von", "dem", "Him\u00b7mel", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Blicken aus den Wogen wieder", "tokens": ["Bli\u00b7cken", "aus", "den", "Wo\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mond und Sterne.", "tokens": ["Mond", "und", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Eines ist dann Wolk' und Welle,", "tokens": ["Ei\u00b7nes", "ist", "dann", "Wol\u00b7k'", "und", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Und die Engel tragen gerne,", "tokens": ["Und", "die", "En\u00b7gel", "tra\u00b7gen", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Umgewandelt zur Kapelle,", "tokens": ["Um\u00b7ge\u00b7wan\u00b7delt", "zur", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "So ein Schiff durch Mond und Sterne.", "tokens": ["So", "ein", "Schiff", "durch", "Mond", "und", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}