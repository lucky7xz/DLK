{"dta.poem.2049": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Pferd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses scheint vor allen Thieren einen Vorzug fast", "tokens": ["Die\u00b7ses", "scheint", "vor", "al\u00b7len", "Thie\u00b7ren", "ei\u00b7nen", "Vor\u00b7zug", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "zu haben,", "tokens": ["zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da es meist in allen St\u00e4nden, selber vom Monarchen an", "tokens": ["Da", "es", "meist", "in", "al\u00b7len", "St\u00e4n\u00b7den", ",", "sel\u00b7ber", "vom", "Mon\u00b7ar\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "$,", "ADV", "APPRART", "NN", "APPR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis zum Bauren, dient und n\u00fctzt, und man seiner vie-", "tokens": ["Bis", "zum", "Bau\u00b7ren", ",", "dient", "und", "n\u00fctzt", ",", "und", "man", "sei\u00b7ner", "vie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,", "KON", "PIS", "PPOSAT", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "len Gaben,", "tokens": ["len", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Nicht im Frieden, nicht im Kriegen, nirgend fast ent-", "tokens": ["Nicht", "im", "Frie\u00b7den", ",", "nicht", "im", "Krie\u00b7gen", ",", "nir\u00b7gend", "fast", "ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "behren kann.", "tokens": ["beh\u00b7ren", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Dieses Thier ist, uns zu helfen, Lasten f\u00fcr uns aufzu-", "tokens": ["Die\u00b7ses", "Thier", "ist", ",", "uns", "zu", "hel\u00b7fen", ",", "Las\u00b7ten", "f\u00fcr", "uns", "auf\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,", "NN", "APPR", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "heben,", "tokens": ["he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Fortzubringen, uns zu tragen, uns insonderheit ge-", "tokens": ["Fort\u00b7zu\u00b7brin\u00b7gen", ",", "uns", "zu", "tra\u00b7gen", ",", "uns", "in\u00b7son\u00b7der\u00b7heit", "ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "geben.", "tokens": ["ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Zu der Handlung, zu den Reisen, ist es brauchbar, und", "tokens": ["Zu", "der", "Hand\u00b7lung", ",", "zu", "den", "Rei\u00b7sen", ",", "ist", "es", "brauch\u00b7bar", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "das Feld", "tokens": ["das", "Feld"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Wird, zusammt der J\u00e4gerey, nur durch Pferde wohl-", "tokens": ["Wird", ",", "zu\u00b7sammt", "der", "J\u00e4\u00b7ge\u00b7rey", ",", "nur", "durch", "Pfer\u00b7de", "wohl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "bestellt;", "tokens": ["be\u00b7stellt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Zur Parade, zu den Posten. Ja, wer wird die Dienste", "tokens": ["Zur", "Pa\u00b7ra\u00b7de", ",", "zu", "den", "Pos\u00b7ten", ".", "Ja", ",", "wer", "wird", "die", "Diens\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$.", "PTKANT", "$,", "PWS", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "nennen,", "tokens": ["nen\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.18": {"text": "Die wir, so zum Nutz als Schutz, durch die\u00df Thier, er-", "tokens": ["Die", "wir", ",", "so", "zum", "Nutz", "als", "Schutz", ",", "durch", "die\u00df", "Thier", ",", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "$,", "ADV", "APPRART", "NN", "KOUS", "NN", "$,", "APPR", "PDS", "NN", "$,", "TRUNC"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "halten k\u00f6nnen?", "tokens": ["hal\u00b7ten", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Wenn man seinen Wuchs betrachtet, wenn man seinen", "tokens": ["Wenn", "man", "sei\u00b7nen", "Wuchs", "be\u00b7trach\u00b7tet", ",", "wenn", "man", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PIS", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Muth erw\u00e4gt,", "tokens": ["Muth", "er\u00b7w\u00e4gt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Scheint in adlicher Gestalt, auch ein Geist darinn ge-", "tokens": ["Scheint", "in", "ad\u00b7li\u00b7cher", "Ge\u00b7stalt", ",", "auch", "ein", "Geist", "da\u00b7rinn", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN", "PAV", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "legt,", "tokens": ["legt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Der f\u00fcr Pracht und Ruhm empfindlich; welches an den", "tokens": ["Der", "f\u00fcr", "Pracht", "und", "Ruhm", "emp\u00b7find\u00b7lich", ";", "wel\u00b7ches", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADJD", "$.", "PRELS", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "andern Thieren,", "tokens": ["an\u00b7dern", "Thie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Wenigstens in solcher Maa\u00dfe, und so deutlich, nicht zu", "tokens": ["We\u00b7nigs\u00b7tens", "in", "sol\u00b7cher", "Maa\u00b7\u00dfe", ",", "und", "so", "deut\u00b7lich", ",", "nicht", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "KON", "ADV", "ADJD", "$,", "PTKNEG", "PTKZU"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.27": {"text": "sp\u00fcren.", "tokens": ["sp\u00fc\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Wenn wir nun sowohl von au\u00dfen seinen Anstand, der", "tokens": ["Wenn", "wir", "nun", "so\u00b7wohl", "von", "au\u00b7\u00dfen", "sei\u00b7nen", "An\u00b7stand", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ADV", "PPOSAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.29": {"text": "so sch\u00f6n,", "tokens": ["so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}}}}}