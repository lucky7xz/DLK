{"textgrid.poem.54531": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Leiden/ Leid und Ach und letzt ergrimmten N\u00f6then/", "tokens": ["Nach", "Lei\u00b7den", "/", "Leid", "und", "Ach", "und", "letzt", "er\u00b7grimm\u00b7ten", "N\u00f6\u00b7then", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "KON", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem auf uns gezuckt- und eingestecktes Schwerdt/", "tokens": ["Nach", "dem", "auf", "uns", "ge\u00b7zuck\u00b7t", "und", "ein\u00b7ge\u00b7steck\u00b7tes", "Schwerdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "TRUNC", "KON", "ADJA", "NN", "$("], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Indem der s\u00fcsse Fried ins Vaterland einkehrt.", "tokens": ["In\u00b7dem", "der", "s\u00fcs\u00b7se", "Fried", "ins", "Va\u00b7ter\u00b7land", "ein\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und man ein Danck-Lied h\u00f6rt statt rasender Trompeten:", "tokens": ["Und", "man", "ein", "Dan\u00b7ck\u00b7Lied", "h\u00f6rt", "statt", "ra\u00b7sen\u00b7der", "Trom\u00b7pe\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+------+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Indem wir eins aus Lust und nicht durch Glut err\u00f6then/", "tokens": ["In\u00b7dem", "wir", "eins", "aus", "Lust", "und", "nicht", "durch", "Glut", "er\u00b7r\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schlie\u00df ich di\u00df rauhe Jahr und was mein Hertz beschwert:", "tokens": ["Schlie\u00df", "ich", "di\u00df", "rau\u00b7he", "Jahr", "und", "was", "mein", "Hertz", "be\u00b7schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADJA", "NN", "KON", "PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Hertz das nicht die Angst die unser Land verhert", "tokens": ["Mein", "Hertz", "das", "nicht", "die", "Angst", "die", "un\u00b7ser", "Land", "ver\u00b7hert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDS", "PTKNEG", "ART", "NN", "ART", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vermocht durch rauhen Sturm und linde Gifft zu t\u00f6dten.", "tokens": ["Ver\u00b7mocht", "durch", "rau\u00b7hen", "Sturm", "und", "lin\u00b7de", "Gifft", "zu", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gott wir haben di\u00df erlebet was du uns verheissen hast/", "tokens": ["Gott", "wir", "ha\u00b7ben", "di\u00df", "er\u00b7le\u00b7bet