{"dta.poem.23441": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Freie Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder, heut' ist freie Nacht!", "tokens": ["Br\u00fc\u00b7der", ",", "heut'", "ist", "frei\u00b7e", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfa, wie das Herz mir lacht!", "tokens": ["Hei\u00b7\u00dfa", ",", "wie", "das", "Herz", "mir", "lacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft es euch nur nicht verdrie\u00dfen:", "tokens": ["La\u00dft", "es", "euch", "nur", "nicht", "ver\u00b7drie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was man hat, soll man genie\u00dfen.", "tokens": ["Was", "man", "hat", ",", "soll", "man", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Gesellen insgemein,", "tokens": ["Ihr", "Ge\u00b7sel\u00b7len", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt mit mir ins Wirthshaus 'nein!", "tokens": ["Kommt", "mit", "mir", "ins", "Wirths\u00b7haus", "'n\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "NE", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Denn es wird ja doch auf Erden", "tokens": ["Denn", "es", "wird", "ja", "doch", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freier Tag so bald nicht werden.", "tokens": ["Frei\u00b7er", "Tag", "so", "bald", "nicht", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darum sei der freien Nacht", "tokens": ["Da\u00b7rum", "sei", "der", "frei\u00b7en", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ein Vivathoch gebracht!", "tokens": ["Auch", "ein", "Vi\u00b7vat\u00b7hoch", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns tanzen, la\u00dft uns trinken!", "tokens": ["La\u00dft", "uns", "tan\u00b7zen", ",", "la\u00dft", "uns", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft die freie Nacht nicht sinken!", "tokens": ["La\u00dft", "die", "frei\u00b7e", "Nacht", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}