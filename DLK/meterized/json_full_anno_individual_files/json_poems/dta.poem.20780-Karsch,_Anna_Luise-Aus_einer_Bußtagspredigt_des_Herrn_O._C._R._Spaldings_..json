{"dta.poem.20780": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Aus einer Bu\u00dftagspredigt  \n  des Herrn O. C. R. Spaldings .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn alles alles mu\u00df zerrinnen,               ", "tokens": ["Wenn", "al\u00b7les", "al\u00b7les", "mu\u00df", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was sich der Weltmann hier ersinnen,", "tokens": ["Was", "sich", "der", "Welt\u00b7mann", "hier", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erringen und erschleichen kann,", "tokens": ["Er\u00b7rin\u00b7gen", "und", "er\u00b7schlei\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Ruhm und G\u00fcter von dem Reichen", "tokens": ["Wenn", "Ruhm", "und", "G\u00fc\u00b7ter", "von", "dem", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Am Sterbebette treulos weichen:", "tokens": ["Am", "Ster\u00b7be\u00b7bet\u00b7te", "treu\u00b7los", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie gl\u00fccklich lebt alsdann der Mann,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "lebt", "als\u00b7dann", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sich die Furcht vor Gott zur besten Weisheit", "tokens": ["Der", "sich", "die", "Furcht", "vor", "Gott", "zur", "bes\u00b7ten", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "machte,", "tokens": ["mach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und B\u00f6ses meiden, zum Verstand,", "tokens": ["Und", "B\u00f6\u00b7ses", "mei\u00b7den", ",", "zum", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Stets recht gethan, stets edel dachte,", "tokens": ["Stets", "recht", "ge\u00b7than", ",", "stets", "e\u00b7del", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und in der Tugend Ruhe fand!", "tokens": ["Und", "in", "der", "Tu\u00b7gend", "Ru\u00b7he", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}