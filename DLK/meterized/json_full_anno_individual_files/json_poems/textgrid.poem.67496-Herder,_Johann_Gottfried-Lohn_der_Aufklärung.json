{"textgrid.poem.67496": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Lohn der Aufkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr entehretet uns, Ihr deutsche Weisen! Ich klage", "tokens": ["Ihr", "en\u00b7teh\u00b7re\u00b7tet", "uns", ",", "Ihr", "deut\u00b7sche", "Wei\u00b7sen", "!", "Ich", "kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-----+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Laut; denn Euer Volk habt Ihr entehret in Euch.", "tokens": ["Laut", ";", "denn", "Eu\u00b7er", "Volk", "habt", "Ihr", "en\u00b7teh\u00b7ret", "in", "Euch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Nektar reichetet Ihr den groben M\u00e4gen und salbtet", "tokens": ["Nek\u00b7tar", "rei\u00b7che\u00b7tet", "Ihr", "den", "gro\u00b7ben", "M\u00e4\u00b7gen", "und", "salb\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Mit Ambrosiaduft lange das stopplichte Haar.", "tokens": ["Mit", "A\u00b7mbro\u00b7sia\u00b7duft", "lan\u00b7ge", "das", "stopp\u00b7lich\u00b7te", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "D