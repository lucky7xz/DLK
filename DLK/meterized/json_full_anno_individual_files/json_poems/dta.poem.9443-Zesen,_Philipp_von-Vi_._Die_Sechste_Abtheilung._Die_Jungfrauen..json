{"dta.poem.9443": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n Die Sechste Abtheilung.  \n Die Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach/ Schwester/ wo bleibet dein liebstes verlangen/", "tokens": ["Ach", "/", "Schwes\u00b7ter", "/", "wo", "blei\u00b7bet", "dein", "liebs\u00b7tes", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "NN", "$(", "PWAV", "VVFIN", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Du spiegel der Sch\u00f6nen/ Du liebliche zier;", "tokens": ["Du", "spie\u00b7gel", "der", "Sch\u00f6\u00b7nen", "/", "Du", "lieb\u00b7li\u00b7che", "zier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dein Liebster ist trgend von hinnen gegangen/", "tokens": ["Dein", "Liebs\u00b7ter", "ist", "tr\u00b7gend", "von", "hin\u00b7nen", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "ADV", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir wollen Jhn suchen und bringen zu dier.", "tokens": ["Wir", "wol\u00b7len", "Jhn", "su\u00b7chen", "und", "brin\u00b7gen", "zu", "dier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "PDAT", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}