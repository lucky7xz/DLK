{"dta.poem.7358": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die beste Politik.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Von Allem was zu Leid und Frommen", "tokens": ["Von", "Al\u00b7lem", "was", "zu", "Leid", "und", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PWS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bisher das Leben mir gebracht,", "tokens": ["Bis\u00b7her", "das", "Le\u00b7ben", "mir", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Manches unverhofft gekommen,", "tokens": ["Ist", "Man\u00b7ches", "un\u00b7ver\u00b7hofft", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Manches hatt\u2019 ich \u00fcberdacht;", "tokens": ["Und", "Man\u00b7ches", "hatt'", "ich", "\u00fc\u00b7berd\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch seltsam! wo ich schlau und fein", "tokens": ["Doch", "selt\u00b7sam", "!", "wo", "ich", "schlau", "und", "fein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "PWAV", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich abgesorgt zu grauen Haaren,", "tokens": ["Mich", "ab\u00b7ge\u00b7sorgt", "zu", "grau\u00b7en", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da bin ich meistens abgefahren,", "tokens": ["Da", "bin", "ich", "meis\u00b7tens", "ab\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Unverhofftes schlug mir ein.", "tokens": ["Und", "Un\u00b7ver\u00b7hoff\u00b7tes", "schlug", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Jeder k\u00f6mmt doch gern zu Brode,", "tokens": ["Ein", "Je\u00b7der", "k\u00f6mmt", "doch", "gern", "zu", "Bro\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch blieben mir die G\u00f6nner kalt,", "tokens": ["Doch", "blie\u00b7ben", "mir", "die", "G\u00f6n\u00b7ner", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "That ich gleich klein wie eine Lode", "tokens": ["That", "ich", "gleich", "klein", "wie", "ei\u00b7ne", "Lo\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gen einen macht\u2019gen Eichenwald;", "tokens": ["Gen", "ei\u00b7nen", "macht'\u00b7gen", "Ei\u00b7chen\u00b7wald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur der \u00e4rmliche Student,", "tokens": ["Und", "nur", "der", "\u00e4rm\u00b7li\u00b7che", "Stu\u00b7dent", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bei dem ich manche Nacht verwachte,", "tokens": ["Bei", "dem", "ich", "man\u00b7che", "Nacht", "ver\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Mangel ihn auf\u2019s Lager brachte,", "tokens": ["Als", "Man\u00b7gel", "ihn", "auf's", "La\u00b7ger", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der dachte mein als Pr\u00e4sident.", "tokens": ["Der", "dach\u00b7te", "mein", "als", "Pr\u00e4\u00b7si\u00b7dent", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Frauen will man auch gefallen,", "tokens": ["Den", "Frau\u00b7en", "will", "man", "auch", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u2014 Zumal sieht man nicht \u00fcbel aus, \u2014", "tokens": ["Zu\u00b7mal", "sieht", "man", "nicht", "\u00fc\u00b7bel", "aus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "PTKNEG", "ADJD", "PTKVZ", "$,", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "In die Salons sah man mich wallen,", "tokens": ["In", "die", "Sa\u00b7lons", "sah", "man", "mich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Verschmitzt hinein, verdutzt heraus;", "tokens": ["Ver\u00b7schmitzt", "hin\u00b7ein", ",", "ver\u00b7dutzt", "he\u00b7raus", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur die t\u00e4glich recht und schlicht", "tokens": ["Und", "nur", "die", "t\u00e4g\u00b7lich", "recht", "und", "schlicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJD", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich wandeln sah im eignen Hause,", "tokens": ["Mich", "wan\u00b7deln", "sah", "im", "eig\u00b7nen", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die trug in meine kleine Klause", "tokens": ["Die", "trug", "in", "mei\u00b7ne", "klei\u00b7ne", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Lebens s\u00fc\u00dfestes Gedicht.", "tokens": ["Des", "Le\u00b7bens", "s\u00fc\u00b7\u00dfes\u00b7tes", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch Ruhm ist gar ein scharfer K\u00f6der,", "tokens": ["Auch", "Ruhm", "ist", "gar", "ein", "schar\u00b7fer", "K\u00f6\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe manchen Tag verschwitzt,", "tokens": ["Ich", "ha\u00b7be", "man\u00b7chen", "Tag", "ver\u00b7schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschnitzelt hab' ich manche Feder,", "tokens": ["Ver\u00b7schnit\u00b7zelt", "hab'", "ich", "man\u00b7che", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin doch schm\u00e4hlich abgeblitzt;", "tokens": ["Und", "bin", "doch", "schm\u00e4h\u00b7lich", "ab\u00b7ge\u00b7blitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur als ich, entmuthigt ganz,", "tokens": ["Und", "nur", "als", "ich", ",", "ent\u00b7mu\u00b7thigt", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gedanken flattern lie\u00df wie Flocken,", "tokens": ["Ge\u00b7dan\u00b7ken", "flat\u00b7tern", "lie\u00df", "wie", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da pl\u00f6tzlich fiel auf meine Locken", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "fiel", "auf", "mei\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein junger frischer Lorbeerkranz.", "tokens": ["Ein", "jun\u00b7ger", "fri\u00b7scher", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So hab' aus Allem ich gezogen", "tokens": ["So", "hab'", "aus", "Al\u00b7lem", "ich", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das treue Facit mir zuletzt,", "tokens": ["Das", "treu\u00b7e", "Fa\u00b7cit", "mir", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dem das Gl\u00fcck zumeist gewogen,", "tokens": ["Da\u00df", "dem", "das", "Gl\u00fcck", "zu\u00b7meist", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der es am mindesten gehetzt;", "tokens": ["Der", "es", "am", "min\u00b7des\u00b7ten", "ge\u00b7hetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "VVPP", "$."], "meter": "---+---+", "measure": "unknown.measure.di"}, "line.5": {"text": "Und da\u00df, wo Wirken ein Geschick", "tokens": ["Und", "da\u00df", ",", "wo", "Wir\u00b7ken", "ein", "Ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "$,", "PWAV", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach eigner Willk\u00fcr kann bereiten,", "tokens": ["Nach", "eig\u00b7ner", "Will\u00b7k\u00fcr", "kann", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Nur Offenheit zu allen Zeiten", "tokens": ["Nur", "Of\u00b7fen\u00b7heit", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die allerbeste Politik.", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7te", "Po\u00b7li\u00b7tik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}