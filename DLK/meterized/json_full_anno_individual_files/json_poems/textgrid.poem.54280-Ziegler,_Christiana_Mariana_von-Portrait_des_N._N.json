{"textgrid.poem.54280": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Portrait des N. N", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist galant, geschickt, polit, vern\u00fcnfftig, klug,", "tokens": ["Er", "ist", "ga\u00b7lant", ",", "ge\u00b7schickt", ",", "po\u00b7lit", ",", "ver\u00b7n\u00fcnff\u00b7tig", ",", "klug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVPP", "$,", "NE", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beredt, voll Eifersucht, im Lieben voller Trug.", "tokens": ["Be\u00b7redt", ",", "voll", "Ei\u00b7fer\u00b7sucht", ",", "im", "Lie\u00b7ben", "vol\u00b7ler", "Trug", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "NN", "$,", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald liebt er in der Stadt, bald liebt er auf dem Lande;", "tokens": ["Bald", "liebt", "er", "in", "der", "Stadt", ",", "bald", "liebt", "er", "auf", "dem", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich d\u00fcnckt, das heist ja wohl nicht lieben mit Bestande.", "tokens": ["Mich", "d\u00fcnckt", ",", "das", "heist", "ja", "wohl", "nicht", "lie\u00b7ben", "mit", "Be\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "ADV", "PTKNEG", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sperre meine Frau dereinst in K\u00e4fig ein,", "tokens": ["Ich", "sper\u00b7re", "mei\u00b7ne", "Frau", "de\u00b7reinst", "in", "K\u00e4\u00b7fig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das schw\u00f6rt er sans facon; was soll die Welt nun sagen?", "tokens": ["Das", "schw\u00f6rt", "er", "sans", "fa\u00b7con", ";", "was", "soll", "die", "Welt", "nun", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "FM", "FM", "$.", "PWS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Hencker m\u00f6chte so an ihn verm\u00e4hlet seyn,", "tokens": ["Der", "Hen\u00b7cker", "m\u00f6ch\u00b7te", "so", "an", "ihn", "ver\u00b7m\u00e4h\u00b7let", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er kr\u00f6nt die anderen, und will nicht H\u00f6rner tragen.", "tokens": ["Er", "kr\u00f6nt", "die", "an\u00b7de\u00b7ren", ",", "und", "will", "nicht", "H\u00f6r\u00b7ner", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "KON", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "me