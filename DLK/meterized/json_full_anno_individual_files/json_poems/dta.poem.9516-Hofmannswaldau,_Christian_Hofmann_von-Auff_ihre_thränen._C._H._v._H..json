{"dta.poem.9516": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre thr\u00e4nen.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die thr\u00e4nen stehen dir wie perlen im gesichte/", "tokens": ["Die", "thr\u00e4\u00b7nen", "ste\u00b7hen", "dir", "wie", "per\u00b7len", "im", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fliessen wie crystall durch wangen/ mund und brust/", "tokens": ["Und", "flies\u00b7sen", "wie", "crys\u00b7tall", "durch", "wan\u00b7gen", "/", "mund", "und", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "APPR", "NN", "$(", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein seuffzen halte ich nicht mehr vor ein gedichte/", "tokens": ["Dein", "seuff\u00b7zen", "hal\u00b7te", "ich", "nicht", "mehr", "vor", "ein", "ge\u00b7dich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was deine seele kr\u00e4nckt/ ist meiner wol bewust.", "tokens": ["Was", "dei\u00b7ne", "see\u00b7le", "kr\u00e4nckt", "/", "ist", "mei\u00b7ner", "wol", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "VAFIN", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und hat mir gleich die angst den treuen mund geschlossen/", "tokens": ["Und", "hat", "mir", "gleich", "die", "angst", "den", "treu\u00b7en", "mund", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den augen und der hand den z\u00fcgel angelegt/", "tokens": ["Den", "au\u00b7gen", "und", "der", "hand", "den", "z\u00fc\u00b7gel", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So schwer ich/ da\u00df ich mehr der thr\u00e4nen ausgegossen/", "tokens": ["So", "schwer", "ich", "/", "da\u00df", "ich", "mehr", "der", "thr\u00e4\u00b7nen", "aus\u00b7ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als unser Oder-strom der klaren tropffen tr\u00e4gt.", "tokens": ["Als", "un\u00b7ser", "O\u00b7der\u00b7strom", "der", "kla\u00b7ren", "tropf\u00b7fen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kan beyder thr\u00e4nen-flut allhier zusammen rinnen/", "tokens": ["Kan", "bey\u00b7der", "thr\u00e4\u00b7nen\u00b7flut", "all\u00b7hier", "zu\u00b7sam\u00b7men", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Komt beyder seuffzer wind verbunden in die lufft/", "tokens": ["Komt", "bey\u00b7der", "seuff\u00b7zer", "wind", "ver\u00b7bun\u00b7den", "in", "die", "lufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wird die Venus selbst ihr schiffen hier beginnen/", "tokens": ["So", "wird", "die", "Ve\u00b7nus", "selbst", "ihr", "schif\u00b7fen", "hier", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPER", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil sie bey reiner flut auch reiner wind berufft.", "tokens": ["Weil", "sie", "bey", "rei\u00b7ner", "flut", "auch", "rei\u00b7ner", "wind", "be\u00b7rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}