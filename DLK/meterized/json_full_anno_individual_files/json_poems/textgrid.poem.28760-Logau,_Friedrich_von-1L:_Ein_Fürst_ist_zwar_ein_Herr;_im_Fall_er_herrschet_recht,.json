{"textgrid.poem.28760": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein F\u00fcrst ist zwar ein Herr; im Fall er herrschet recht,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein F\u00fcrst ist zwar ein Herr; im Fall er herrschet recht,", "tokens": ["Ein", "F\u00fcrst", "ist", "zwar", "ein", "Herr", ";", "im", "Fall", "er", "herr\u00b7schet", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$.", "APPRART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist er seinem Volck als wie ein treuer Knecht;", "tokens": ["So", "ist", "er", "sei\u00b7nem", "Volck", "als", "wie", "ein", "treu\u00b7er", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er dient zu ihrem Heil, er m\u00fcht sich, da\u00df er schwitzt,", "tokens": ["Er", "dient", "zu", "ih\u00b7rem", "Heil", ",", "er", "m\u00fcht", "sich", ",", "da\u00df", "er", "schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sein vertrautes Volck gedieg- und r\u00fchglich sitzt;", "tokens": ["Da\u00df", "sein", "ver\u00b7trau\u00b7tes", "Volck", "ge\u00b7dieg", "und", "r\u00fchg\u00b7lich", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "TRUNC", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wacht, damit sein Volck fein sicher schlafen kan;", "tokens": ["Er", "wacht", ",", "da\u00b7mit", "sein", "Volck", "fein", "si\u00b7cher", "schla\u00b7fen", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er stellt sich f\u00fcr den Rie\u00df, nimmt allen Anlauff an,", "tokens": ["Er", "stellt", "sich", "f\u00fcr", "den", "Rie\u00df", ",", "nimmt", "al\u00b7len", "An\u00b7lauff", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist Nagel an der Wand, daran ein jeder henckt,", "tokens": ["Ist", "Na\u00b7gel", "an", "der", "Wand", ",", "da\u00b7ran", "ein", "je\u00b7der", "henckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN", "$,", "PAV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was ihn beschwert und dr\u00fcckt, was peiniget, was dr\u00e4ngt;", "tokens": ["Was", "ihn", "be\u00b7schwert", "und", "dr\u00fcckt", ",", "was", "pei\u00b7ni\u00b7get", ",", "was", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An Ehren ist er Herr, an Treuen ist er Knecht;", "tokens": ["An", "Eh\u00b7ren", "ist", "er", "Herr", ",", "an", "Treu\u00b7en", "ist", "er", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "$,", "APPR", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Herr, ders anders meint, der meint es schwerlich recht.", "tokens": ["Ein", "Herr", ",", "ders", "an\u00b7ders", "meint", ",", "der", "meint", "es", "schwer\u00b7lich", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein F\u00fcrst ist zwar ein Herr; im Fall er herrschet recht,", "tokens": ["Ein", "F\u00fcrst", "ist", "zwar", "ein", "Herr", ";", "im", "Fall", "er", "herr\u00b7schet", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$.", "APPRART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist er seinem Volck als wie ein treuer Knecht;", "tokens": ["So", "ist", "er", "sei\u00b7nem", "Volck", "als", "wie", "ein", "treu\u00b7er", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+