{"dta.poem.5836": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen lichter Strahl ist weniger gebogen,", "tokens": ["Der", "Son\u00b7nen", "lich\u00b7ter", "Strahl", "ist", "we\u00b7ni\u00b7ger", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er sich durch die Lufft ins Wasser senckt;", "tokens": ["Wenn", "er", "sich", "durch", "die", "Lufft", "ins", "Was\u00b7ser", "senckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird aber st\u00e4rcker abgelenckt,", "tokens": ["Wird", "a\u00b7ber", "st\u00e4r\u00b7cker", "ab\u00b7ge\u00b7lenckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er sich in die Lufft vom Wasser abgezogen.", "tokens": ["Wenn", "er", "sich", "in", "die", "Lufft", "vom", "Was\u00b7ser", "ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Weg geht nicht so leicht hinein.", "tokens": ["Der", "Weg", "geht", "nicht", "so", "leicht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die viele Graden nun von dem gebrochnen Schein", "tokens": ["Die", "vie\u00b7le", "Gra\u00b7den", "nun", "von", "dem", "ge\u00b7broch\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind deutlich gnug zu sehen", "tokens": ["Sind", "deut\u00b7lich", "gnug", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In C\u00f6rpern, die durchsichtig seyn,", "tokens": ["In", "C\u00f6r\u00b7pern", ",", "die", "durch\u00b7sich\u00b7tig", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wodurch der Sonnen Strahlen gehen.", "tokens": ["Wo\u00b7durch", "der", "Son\u00b7nen", "Strah\u00b7len", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}