u00df", "er", "hei\u00b7lig", ",", "fromm", ",", "ge\u00b7treu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Im Gem\u00fcte,", "tokens": ["Im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Ja die G\u00fcte", "tokens": ["Ja", "die", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Vnd die Langmuht selber sey.", "tokens": ["Vnd", "die", "Lang\u00b7muht", "sel\u00b7ber", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer giebt solcher massen", "tokens": ["Wer", "giebt", "sol\u00b7cher", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIAT", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Etwas auff den Tod?", "tokens": ["Et\u00b7was", "auff", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weis nicht Trost zu fassen", "tokens": ["Weis", "nicht", "Trost", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wider alle Noht?", "tokens": ["Wi\u00b7der", "al\u00b7le", "Noht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gott, bereit uns allzumal,", "tokens": ["Gott", ",", "be\u00b7reit", "uns", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df wir aus dem finstern Thal", "tokens": ["Da\u00df", "wir", "aus", "dem", "fins\u00b7tern", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieser Thr\u00e4nen", "tokens": ["Die\u00b7ser", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Stets uns sehnen", "tokens": ["Stets", "uns", "seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "In den ewign Himmels-Saal.", "tokens": ["In", "den", "e\u00b7wign", "Him\u00b7mels\u00b7Saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}