{"textgrid.poem.53395": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Christliche Sterbens-Lust", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bi\u00df zur Grabes-Kammer", "tokens": ["Bi\u00df", "zur", "Gra\u00b7bes\u00b7Kam\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vnd bi\u00df an den Tod,", "tokens": ["Vnd", "bi\u00df", "an", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Weiter mu\u00df kein Jammer,", "tokens": ["Wei\u00b7ter", "mu\u00df", "kein", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Weiter keine Noht.", "tokens": ["Wei\u00b7ter", "kei\u00b7ne", "Noht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hier h\u00e4lt uns die stille Ruh", "tokens": ["Hier", "h\u00e4lt", "uns", "die", "stil\u00b7le", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ewig Ohr- und Augen zu,", "tokens": ["E\u00b7wig", "Ohr", "und", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verh\u00fctet", "tokens": ["Und", "ver\u00b7h\u00fc\u00b7tet"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Da\u00df, was w\u00fctet,", "tokens": ["Da\u00df", ",", "was", "w\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Vns durchaus nicht Schaden thu.", "tokens": ["Vns", "durc\u00b7haus", "nicht", "Scha\u00b7den", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vnsre Seelen schweben", "tokens": ["Vns\u00b7re", "See\u00b7len", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hoch in Gottes Reich,", "tokens": ["Hoch", "in", "Got\u00b7tes", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da sie ewig leben", "tokens": ["Da", "sie", "e\u00b7wig", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Selbst den Engeln gleich,", "tokens": ["Selbst", "den", "En\u00b7geln", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Voller Glantz und Herrlicheit,", "tokens": ["Vol\u00b7ler", "Glantz", "und", "Herr\u00b7li\u00b7cheit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch dafern sie in der Zeit", "tokens": ["Doch", "da\u00b7fern", "sie", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Vnschuld lieben,", "tokens": ["Vn\u00b7schuld", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Gutes \u00fcben,", "tokens": ["Gu\u00b7tes", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Vnd der S\u00fcnden sich befreyt.", "tokens": ["Vnd", "der", "S\u00fcn\u00b7den", "sich", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mu\u00df dann in der Erden", "tokens": ["Mu\u00df", "dann", "in", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vnsrer Leiber Zier", "tokens": ["Vns\u00b7rer", "Lei\u00b7ber", "Zier"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Staub und Asche werden,", "tokens": ["Staub", "und", "A\u00b7sche", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ey die Stund' ist hier,", "tokens": ["Ey", "die", "Stund'", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da die\u00df Fleisch und die\u00df Gebein,", "tokens": ["Da", "die\u00df", "Fleisch", "und", "die\u00df", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "KON", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringt man es gleich traurig ein", "tokens": ["Bringt", "man", "es", "gleich", "trau\u00b7rig", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "ADJD", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt der H\u00f6len,", "tokens": ["Jetzt", "der", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Mit der Seelen", "tokens": ["Mit", "der", "See\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wieder wird vereinigt seyn.", "tokens": ["Wie\u00b7der", "wird", "ver\u00b7ei\u00b7nigt", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da wird man mit Preisen", "tokens": ["Da", "wird", "man", "mit", "Prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vor den H\u00f6chsten gehn,", "tokens": ["Vor", "den", "H\u00f6chs\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihm auff tausent Weisen", "tokens": ["Ihm", "auff", "tau\u00b7sent", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ehr' und Danck gestehn,", "tokens": ["Ehr'", "und", "Danck", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Singen nicht ohn Lust-Geschrey,", "tokens": ["Sin\u00b7gen", "nicht", "ohn", "Lust\u00b7Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er heilig, fromm, getreu", "tokens": ["Da\u00df", "er", "hei\u00b7lig", ",", "fromm", ",", "ge\u00b7treu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Im Gem\u00fcte,", "tokens": ["Im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Ja die G\u00fcte", "tokens": ["Ja", "die", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Vnd die Langmuht selber sey.", "tokens": ["Vnd", "die", "Lang\u00b7muht", "sel\u00b7ber", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer giebt solcher massen", "tokens": ["Wer", "giebt", "sol\u00b7cher", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIAT", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Etwas auff den Tod?", "tokens": ["Et\u00b7was", "auff", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "l