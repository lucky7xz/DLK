{"dta.poem.7809": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Ist dir zu hei\u00df:", "tokens": ["Ist", "dir", "zu", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "hol bey ihr Eis.", "tokens": ["hol", "bey", "ihr", "Eis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ist sie Eis:", "tokens": ["Und", "ist", "sie", "Eis", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "mach du ihr hei\u00df.", "tokens": ["mach", "du", "ihr", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}