{"dta.poem.20967": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.  \n Evens.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wie solte nicht ein Werck den H\u00f6chsten Ruhm", "tokens": ["Wie", "sol\u00b7te", "nicht", "ein", "Werck", "den", "H\u00f6chs\u00b7ten", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "erlangen/", "tokens": ["er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So zweymahl durch die Hand des H\u00f6chsten ist ge-", "tokens": ["So", "zwey\u00b7mahl", "durch", "die", "Hand", "des", "H\u00f6chs\u00b7ten", "ist", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gangen.", "tokens": ["gan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es drang ein s\u00fcsser Spruch mir durch die stoltzen", "tokens": ["Es", "drang", "ein", "s\u00fcs\u00b7ser", "Spruch", "mir", "durch", "die", "stolt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sinnen/", "tokens": ["Sin\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Jhr T\u00f6chter lernt von mir was falsche Worte", "tokens": ["Ihr", "T\u00f6ch\u00b7ter", "lernt", "von", "mir", "was", "fal\u00b7sche", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "k\u00f6nnen.", "tokens": ["k\u00f6n\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}