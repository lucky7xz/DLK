{"dta.poem.6785": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und nichts erweiset uns mit gr\u00f6\u00dfrer Klarheit", "tokens": ["Und", "nichts", "er\u00b7wei\u00b7set", "uns", "mit", "gr\u00f6\u00df\u00b7rer", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem, was vom Gesicht wir erst gesagt, die Wahrheit.", "tokens": ["Von", "dem", ",", "was", "vom", "Ge\u00b7sicht", "wir", "erst", "ge\u00b7sagt", ",", "die", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPRART", "NN", "PPER", "ADV", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein jedes Spiegel-Glas und Fern-Glas zeigt vollkommen", "tokens": ["Ein", "je\u00b7des", "Spie\u00b7gel\u00b7Glas", "und", "Fern\u00b7Glas", "zeigt", "voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NE", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Meinung, die wir angenommen.", "tokens": ["Die", "Mei\u00b7nung", ",", "die", "wir", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}