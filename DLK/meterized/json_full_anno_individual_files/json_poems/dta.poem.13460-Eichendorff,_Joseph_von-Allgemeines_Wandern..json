{"dta.poem.13460": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeines Wandern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Grund bis zu den Gipfeln,", "tokens": ["Vom", "Grund", "bis", "zu", "den", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So weit man sehen kann,", "tokens": ["So", "weit", "man", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt bl\u00fcht's in allen Wipfeln,", "tokens": ["Jetzt", "bl\u00fcht's", "in", "al\u00b7len", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun geht das Wandern an:", "tokens": ["Nun", "geht", "das", "Wan\u00b7dern", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Quellen von den Kl\u00fcften,", "tokens": ["Die", "Quel\u00b7len", "von", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Str\u00f6m' auf gr\u00fcnem Plan,", "tokens": ["Die", "Str\u00f6m'", "auf", "gr\u00fc\u00b7nem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lerchen hoch in L\u00fcften,", "tokens": ["Die", "Ler\u00b7chen", "hoch", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Dichter frisch voran.", "tokens": ["Der", "Dich\u00b7ter", "frisch", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und die im Thal verderben", "tokens": ["Und", "die", "im", "Thal", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In tr\u00fcber Sorgen Haft,", "tokens": ["In", "tr\u00fc\u00b7ber", "Sor\u00b7gen", "Haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er m\u00f6cht' sie Alle werben", "tokens": ["Er", "m\u00f6cht'", "sie", "Al\u00b7le", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu dieser Wanderschaft.", "tokens": ["Zu", "die\u00b7ser", "Wan\u00b7der\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und von den Bergen nieder", "tokens": ["Und", "von", "den", "Ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erschallt sein Lied in's Thal,", "tokens": ["Er\u00b7schallt", "sein", "Lied", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die zerstreuten Br\u00fcder", "tokens": ["Und", "die", "zer\u00b7streu\u00b7ten", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fa\u00dft Heimweh allzumal.", "tokens": ["Fa\u00dft", "Heim\u00b7weh", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da wird die Welt so munter", "tokens": ["Da", "wird", "die", "Welt", "so", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nimmt die Reiseschuh,", "tokens": ["Und", "nimmt", "die", "Rei\u00b7se\u00b7schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Liebchen mitten drunter", "tokens": ["Sein", "Lieb\u00b7chen", "mit\u00b7ten", "drun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nickt ihm heimlich zu.", "tokens": ["Die", "nickt", "ihm", "heim\u00b7lich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und \u00fcber Felsenw\u00e4nde", "tokens": ["Und", "\u00fc\u00b7ber", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auf dem gr\u00fcnen Plan", "tokens": ["Und", "auf", "dem", "gr\u00fc\u00b7nen", "Plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wirrt und jauchzt ohn' Ende \u2014", "tokens": ["Das", "wirrt", "und", "jauchzt", "ohn'", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun geht das Wandern an!", "tokens": ["Nun", "geht", "das", "Wan\u00b7dern", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}