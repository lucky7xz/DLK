{"dta.poem.7499": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich Rosenhauch auf einer Jungfrau Wangen", "tokens": ["Gleich", "Ro\u00b7sen\u00b7hauch", "auf", "ei\u00b7ner", "Jung\u00b7frau", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich den Abend im Gebirge prangen,", "tokens": ["Seh'", "ich", "den", "A\u00b7bend", "im", "Ge\u00b7bir\u00b7ge", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im zarten Dufte gl\u00fchen sie vor mir", "tokens": ["Im", "zar\u00b7ten", "Duf\u00b7te", "gl\u00fc\u00b7hen", "sie", "vor", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Gletscher, denen treu die Sonne hier", "tokens": ["Die", "Glet\u00b7scher", ",", "de\u00b7nen", "treu", "die", "Son\u00b7ne", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr erstes und ihr letztes L\u00e4cheln zeigt,", "tokens": ["Ihr", "ers\u00b7tes", "und", "ihr", "letz\u00b7tes", "L\u00e4\u00b7cheln", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und aus den Flammen wie ein Ph\u00f6nix steigt", "tokens": ["Und", "aus", "den", "Flam\u00b7men", "wie", "ein", "Ph\u00f6\u00b7nix", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Mond mit silberstrahlendem Gefieder,", "tokens": ["Der", "Mond", "mit", "sil\u00b7ber\u00b7strah\u00b7len\u00b7dem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In jede Woge taucht sein Bildni\u00df nieder,", "tokens": ["In", "je\u00b7de", "Wo\u00b7ge", "taucht", "sein", "Bild\u00b7ni\u00df", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob stumm sie ruht, ob leuchtend sie sich bricht,", "tokens": ["Ob", "stumm", "sie", "ruht", ",", "ob", "leuch\u00b7tend", "sie", "sich", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie wird verkl\u00e4rt und er vergi\u00dft sie nicht;", "tokens": ["Sie", "wird", "ver\u00b7kl\u00e4rt", "und", "er", "ver\u00b7gi\u00dft", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So mag der Geist der Welt in unser Denken,", "tokens": ["So", "mag", "der", "Geist", "der", "Welt", "in", "un\u00b7ser", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "In jede Bl\u00fcte, jede Brust sich senken.", "tokens": ["In", "je\u00b7de", "Bl\u00fc\u00b7te", ",", "je\u00b7de", "Brust", "sich", "sen\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dem Mond streut still mit schmeichelnder Geberde", "tokens": ["Dem", "Mond", "streut", "still", "mit", "schmei\u00b7cheln\u00b7der", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Goldw\u00f6lkchen auf die Bahn des Abends Wehn", "tokens": ["Gold\u00b7w\u00f6lk\u00b7chen", "auf", "die", "Bahn", "des", "A\u00b7bends", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Gleich Blumen, doch nicht Blumen dieser Erde,", "tokens": ["Gleich", "Blu\u00b7men", ",", "doch", "nicht", "Blu\u00b7men", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "PTKNEG", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die welken m\u00fcssen, ehe sie vergehn;", "tokens": ["Die", "wel\u00b7ken", "m\u00fcs\u00b7sen", ",", "e\u00b7he", "sie", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dort in den Nachen wirft mit kalter Hand", "tokens": ["Dort", "in", "den", "Na\u00b7chen", "wirft", "mit", "kal\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sein letztes Gold, das herbstlich gelbe Land,", "tokens": ["Sein", "letz\u00b7tes", "Gold", ",", "das", "herbst\u00b7lich", "gel\u00b7be", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und meine Seele sieht in s\u00fc\u00dfer Ruh", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "sieht", "in", "s\u00fc\u00b7\u00dfer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der Perlen Tr\u00e4ufeln von den Rudern zu,", "tokens": ["Der", "Per\u00b7len", "Tr\u00e4u\u00b7feln", "von", "den", "Ru\u00b7dern", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wie sie von Ringen hin zu Ringen t\u00f6nen,", "tokens": ["Wie", "sie", "von", "Rin\u00b7gen", "hin", "zu", "Rin\u00b7gen", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ein fliegendes Symbol der Ewigkeit,", "tokens": ["Ein", "flie\u00b7gen\u00b7des", "Sym\u00b7bol", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und endlich sich, von jeder Form befreit,", "tokens": ["Und", "end\u00b7lich", "sich", ",", "von", "je\u00b7der", "Form", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gestaltlos mit dem Element vers\u00f6hnen.", "tokens": ["Ge\u00b7stalt\u00b7los", "mit", "dem", "E\u00b7le\u00b7ment", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "O Geist, der \u00fcber diesen Wassern lebt,", "tokens": ["O", "Geist", ",", "der", "\u00fc\u00b7ber", "die\u00b7sen", "Was\u00b7sern", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der hier aus diesen k\u00fchlen Gr\u00fcnden thaut,", "tokens": ["Der", "hier", "aus", "die\u00b7sen", "k\u00fch\u00b7len", "Gr\u00fcn\u00b7den", "thaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der aus der Tiefe Himmel wiederblaut,", "tokens": ["Der", "aus", "der", "Tie\u00b7fe", "Him\u00b7mel", "wie\u00b7der\u00b7blaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Du Geist des Friedens, der mich jetzt umschwebt,", "tokens": ["Du", "Geist", "des", "Frie\u00b7dens", ",", "der", "mich", "jetzt", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der sich den Aether ma\u00dflos l\u00e4\u00dft entfalten,", "tokens": ["Der", "sich", "den", "A\u00b7e\u00b7ther", "ma\u00df\u00b7los", "l\u00e4\u00dft", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.30": {"text": "Der Erde stillen Drang zum Lenz gestalten \u2014", "tokens": ["Der", "Er\u00b7de", "stil\u00b7len", "Drang", "zum", "Lenz", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "So liebend beut die Luft des Vogels Schwingen,", "tokens": ["So", "lie\u00b7bend", "beut", "die", "Luft", "des", "Vo\u00b7gels", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Der Harfe Ton, um d'rin sich auszuklingen \u2014", "tokens": ["Der", "Har\u00b7fe", "Ton", ",", "um", "d'\u00b7rin", "sich", "aus\u00b7zu\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUI", "ADV", "PRF", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Was hast Du uns um diesen Stern betrogen,", "tokens": ["Was", "hast", "Du", "uns", "um", "die\u00b7sen", "Stern", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und, eh' es tagen wollte, uns entzogen", "tokens": ["Und", ",", "eh'", "es", "ta\u00b7gen", "woll\u00b7te", ",", "uns", "ent\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Den Genius, der Dir so rein verwandt,", "tokens": ["Den", "Ge\u00b7nius", ",", "der", "Dir", "so", "rein", "ver\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.36": {"text": "Sich in Dein All, wie Hauch in Hauch empfand,", "tokens": ["Sich", "in", "Dein", "All", ",", "wie", "Hauch", "in", "Hauch", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "D'rein wie in einer Blume Kelch sich senkte,", "tokens": ["D'\u00b7rein", "wie", "in", "ei\u00b7ner", "Blu\u00b7me", "Kelch", "sich", "senk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.38": {"text": "Und d'raus ein Herz, so gottesdurstig, tr\u00e4nkte?", "tokens": ["Und", "d'raus", "ein", "Herz", ",", "so", "got\u00b7tes\u00b7durs\u00b7tig", ",", "tr\u00e4nk\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Du hast ein Auge der Natur genommen,", "tokens": ["Du", "hast", "ein", "Au\u00b7ge", "der", "Na\u00b7tur", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Das ihr in ihre tiefste Seele sah,", "tokens": ["Das", "ihr", "in", "ih\u00b7re", "tiefs\u00b7te", "See\u00b7le", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Um einen Beter bist Du selbst gekommen \u2014", "tokens": ["Um", "ei\u00b7nen", "Be\u00b7ter", "bist", "Du", "selbst", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Um einen Beter? ei, so staunet, ja!", "tokens": ["Um", "ei\u00b7nen", "Be\u00b7ter", "?", "ei", ",", "so", "stau\u00b7net", ",", "ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$.", "NE", "$,", "ADV", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Um keinen Beter, ruhig, sicher, still, \u2014", "tokens": ["Um", "kei\u00b7nen", "Be\u00b7ter", ",", "ru\u00b7hig", ",", "si\u00b7cher", ",", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KOUI", "PIAT", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Die Flamme bebt, wenn sie nach oben will!", "tokens": ["Die", "Flam\u00b7me", "bebt", ",", "wenn", "sie", "nach", "o\u00b7ben", "will", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Um keinen Beter \u2014 nein, um keinen Wurm \u2014", "tokens": ["Um", "kei\u00b7nen", "Be\u00b7ter", "nein", ",", "um", "kei\u00b7nen", "Wurm"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "$(", "PTKANT", "$,", "KOUI", "PIAT", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Es tobt das Meer und lobt den Herrn im Sturm!", "tokens": ["Es", "tobt", "das", "Meer", "und", "lobt", "den", "Herrn", "im", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Der Blumen sch\u00f6nste brauchet einen Dorn,", "tokens": ["Der", "Blu\u00b7men", "sch\u00f6ns\u00b7te", "brau\u00b7chet", "ei\u00b7nen", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+"