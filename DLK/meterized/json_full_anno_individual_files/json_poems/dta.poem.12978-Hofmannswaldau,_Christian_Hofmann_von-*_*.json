{"dta.poem.12978": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Floris wei\u00df sich viel, weil sie die sch\u00f6nheit zieret:", "tokens": ["Die", "Flo\u00b7ris", "wei\u00df", "sich", "viel", ",", "weil", "sie", "die", "sch\u00f6n\u00b7heit", "zie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieser zierath ist gleichwol kein wahres guth,", "tokens": ["Und", "die\u00b7ser", "zie\u00b7rath", "ist", "gleich\u00b7wol", "kein", "wah\u00b7res", "guth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil der auf seinen grund so hoch geth\u00fcrmte muth", "tokens": ["Weil", "der", "auf", "sei\u00b7nen", "grund", "so", "hoch", "ge\u00b7th\u00fcrm\u00b7te", "muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie schnee zu wasser wird und nur verdru\u00df gebiehret.", "tokens": ["Wie", "schnee", "zu", "was\u00b7ser", "wird", "und", "nur", "ver\u00b7dru\u00df", "ge\u00b7bieh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKA", "ADJD", "VAFIN", "KON", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Floris hei\u00dft ja wol ein allerliebstes kind;", "tokens": ["Die", "Flo\u00b7ris", "hei\u00dft", "ja", "wol", "ein", "al\u00b7ler\u00b7liebs\u00b7tes", "kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der buhler menge w\u00fcnscht, da\u00df sie der himmel cr\u00f6ne;", "tokens": ["Der", "buh\u00b7ler", "men\u00b7ge", "w\u00fcnscht", ",", "da\u00df", "sie", "der", "him\u00b7mel", "cr\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein die sch\u00f6nen sind nur wenig jahre sch\u00f6ne,", "tokens": ["Al\u00b7lein", "die", "sch\u00f6\u00b7nen", "sind", "nur", "we\u00b7nig", "jah\u00b7re", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann kommt die lange zeit, da sie es nicht mehr sind.", "tokens": ["Dann", "kommt", "die", "lan\u00b7ge", "zeit", ",", "da", "sie", "es", "nicht", "mehr", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}