{"textgrid.poem.51195": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich dr\u00fcckte mich nach Hause in kalter Regennacht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dr\u00fcckte mich nach Hause in kalter Regennacht,", "tokens": ["Ich", "dr\u00fcck\u00b7te", "mich", "nach", "Hau\u00b7se", "in", "kal\u00b7ter", "Re\u00b7gen\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da stand er d\u00fcster schimmernd und lautlos auf der Wacht,", "tokens": ["Da", "stand", "er", "d\u00fcs\u00b7ter", "schim\u00b7mernd", "und", "laut\u00b7los", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Er fl\u00fcstert' leis: \u00bbMich hungert, ein Groschen, Herr, zu Brot!\u00ab", "tokens": ["Er", "fl\u00fcs\u00b7tert'", "leis", ":", "\u00bb", "Mich", "hun\u00b7gert", ",", "ein", "Gro\u00b7schen", ",", "Herr", ",", "zu", "Brot", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "NN", "$,", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erschrocken blieb ich stehen und wurde f\u00fcr ihn rot,", "tokens": ["Er\u00b7schro\u00b7cken", "blieb", "ich", "ste\u00b7hen", "und", "wur\u00b7de", "f\u00fcr", "ihn", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "KON", "VAFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den schlanken, den blanken, den schweren K\u00fcrassier.", "tokens": ["Den", "schlan\u00b7ken", ",", "den", "blan\u00b7ken", ",", "den", "schwe\u00b7ren", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Von Stahl der Helm und Harnisch gl\u00e4nzt' wie ein Spiegel klar;", "tokens": ["Von", "Stahl", "der", "Helm", "und", "Har\u00b7nisch", "gl\u00e4nzt'", "wie", "ein", "Spie\u00b7gel", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "NN", "VVFIN", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Waffenrock von Scharlach, im h\u00f6chsten Stiefelpaar,", "tokens": ["Im", "Waf\u00b7fen\u00b7rock", "von", "Schar\u00b7lach", ",", "im", "h\u00f6chs\u00b7ten", "Stie\u00b7fel\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So stand der schlanke, blanke, der schwere K\u00fcrassier.", "tokens": ["So", "stand", "der", "schlan\u00b7ke", ",", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Das nackte Schwert im Arme glich eines Cherubs Schwert,", "tokens": ["Das", "nack\u00b7te", "Schwert", "im", "Ar\u00b7me", "glich", "ei\u00b7nes", "Che\u00b7rubs", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ART", "NE", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und einen Rapp im Stalle, mit Hafer wohlgen\u00e4hrt,", "tokens": ["Und", "ei\u00b7nen", "Rapp", "im", "Stal\u00b7le", ",", "mit", "Ha\u00b7fer", "wohl\u00b7ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hat auch der schlanke, blanke, der schwere K\u00fcrassier.", "tokens": ["Hat", "auch", "der", "schlan\u00b7ke", ",", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ei, solch ein Land und Leute, das hab ich nie gesehn,", "tokens": ["Ei", ",", "solch", "ein", "Land", "und", "Leu\u00b7te", ",", "das", "hab", "ich", "nie", "ge\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "ART", "NN", "KON", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo so kostbare Bettler an Marmort\u00fcren stehn!", "tokens": ["Wo", "so", "kost\u00b7ba\u00b7re", "Bett\u00b7ler", "an", "Mar\u00b7mor\u00b7t\u00fc\u00b7ren", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier!", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ich trau mir kaum zu geben, und sch\u00e4me mich zu fliehn!", "tokens": ["Ich", "trau", "mir", "kaum", "zu", "ge\u00b7ben", ",", "und", "sch\u00e4\u00b7me", "mich", "zu", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch z\u00f6gernd wag ich endlich, das Beutelchen zu ziehn;", "tokens": ["Doch", "z\u00f6\u00b7gernd", "wag", "ich", "end\u00b7lich", ",", "das", "Beu\u00b7tel\u00b7chen", "zu", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O schlanker, o blanker, du schwerer K\u00fcrassier!", "tokens": ["O", "schlan\u00b7ker", ",", "o", "blan\u00b7ker", ",", "du", "schwe\u00b7rer", "K\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "FM", "ADJA", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Und als ich meinen Beutel will teilen mit ihm drauf,", "tokens": ["Und", "als", "ich", "mei\u00b7nen", "Beu\u00b7tel", "will", "tei\u00b7len", "mit", "ihm", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da rasselt die Karosse herbei im schnellen Lauf.", "tokens": ["Da", "ras\u00b7selt", "die", "Ka\u00b7ros\u00b7se", "her\u00b7bei", "im", "schnel\u00b7len", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf, schlanker, du blanker, du schwerer K\u00fcrassier!", "tokens": ["Auf", ",", "schlan\u00b7ker", ",", "du", "blan\u00b7ker", ",", "du", "schwe\u00b7rer", "K\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJD", "$,", "PPER", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Drin sa\u00df ein abgeflattert blutlos Agnatenweib;", "tokens": ["Drin", "sa\u00df", "ein", "ab\u00b7ge\u00b7flat\u00b7tert", "blut\u00b7los", "Ag\u00b7na\u00b7ten\u00b7weib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVPP", "ADJD", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Recke lie\u00df erklirren den starren Riesenleib.", "tokens": ["Der", "Re\u00b7cke", "lie\u00df", "er\u00b7klir\u00b7ren", "den", "star\u00b7ren", "Rie\u00b7sen\u00b7leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Verschwunden war der Wagen, ich reckte meine Hand \u2013", "tokens": ["Ver\u00b7schwun\u00b7den", "war", "der", "Wa\u00b7gen", ",", "ich", "reck\u00b7te", "mei\u00b7ne", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch wieder klirrt's und glitzert's, wie eine S\u00e4ule stand", "tokens": ["Doch", "wie\u00b7der", "klirrt's", "und", "glit\u00b7zert's", ",", "wie", "ei\u00b7ne", "S\u00e4u\u00b7le", "stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Vier seinesgleichen kamen mit Sporenschritt heran,", "tokens": ["Vier", "sei\u00b7nes\u00b7glei\u00b7chen", "ka\u00b7men", "mit", "Spo\u00b7ren\u00b7schritt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Parole wird gewechselt und abgel\u00f6st der Mann,", "tokens": ["Pa\u00b7ro\u00b7le", "wird", "ge\u00b7wech\u00b7selt", "und", "ab\u00b7ge\u00b7l\u00f6st", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "KON", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Er wend't kein Aug zur Seite und wechselt still den Ort,", "tokens": ["Er", "wend't", "kein", "Aug", "zur", "Sei\u00b7te", "und", "wech\u00b7selt", "still", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In Nacht und Nebel schreitet er mit den andern fort,", "tokens": ["In", "Nacht", "und", "Ne\u00b7bel", "schrei\u00b7tet", "er", "mit", "den", "an\u00b7dern", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Was m\u00f6gen das f\u00fcr Dinge, nachtschattenhafte, sein?", "tokens": ["Was", "m\u00f6\u00b7gen", "das", "f\u00fcr", "Din\u00b7ge", ",", "nacht\u00b7schat\u00b7ten\u00b7haf\u00b7te", ",", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "APPR", "NN", "$,", "VVFIN", "$,", "VAINF", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dacht ich und legt ein Gr\u00f6schlein furchtsam auf einen Stein", "tokens": ["Dacht", "ich", "und", "legt", "ein", "Gr\u00f6schlein", "furcht\u00b7sam", "auf", "ei\u00b7nen", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem schlanken, dem blanken, dem schweren K\u00fcrassier.", "tokens": ["Dem", "schlan\u00b7ken", ",", "dem", "blan\u00b7ken", ",", "dem", "schwe\u00b7ren", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PRELS", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.13": {"line.1": {"text": "Vielleicht so kommt er wieder, ich will nach Hause gehn!", "tokens": ["Viel\u00b7leicht", "so", "kommt", "er", "wie\u00b7der", ",", "ich", "will", "nach", "Hau\u00b7se", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es ist nicht gut den Nachtmahr im fremden Lande sehn,", "tokens": ["Es", "ist", "nicht", "gut", "den", "Nacht\u00b7mahr", "im", "frem\u00b7den", "Lan\u00b7de", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den schlanken, den blanken, den Hungerk\u00fcrassier!", "tokens": ["Den", "schlan\u00b7ken", ",", "den", "blan\u00b7ken", ",", "den", "Hun\u00b7ger\u00b7k\u00fc\u00b7ras\u00b7sier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.14": {"line.1": {"text": "Ich dr\u00fcckte mich nach Hause in kalter Regennacht,", "tokens": ["Ich", "dr\u00fcck\u00b7te", "mich", "nach", "Hau\u00b7se", "in", "kal\u00b7ter", "Re\u00b7gen\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da stand er d\u00fcster schimmernd und lautlos auf der Wacht,", "tokens": ["Da", "stand", "er", "d\u00fcs\u00b7ter", "schim\u00b7mernd", "und", "laut\u00b7los", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der schlanke, der blanke, der schwere K\u00fcrassier.", "tokens": ["Der", "schlan\u00b7ke", ",", "der", "blan\u00b7ke", ",", "der", "schwe\u00b7re", "K\u00fc\u00b7ras\u00b7sier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Er fl\u00fcstert' leis: \u00bbMich hungert, ein Groschen, Herr, zu Brot!\u00ab", "tokens": ["Er", "fl\u00fcs\u00b7tert'", "leis", ":", "\u00bb", "Mich", "hun\u00b7gert", ",", "ein", "Gro\u00b7schen", ",", "Herr", ",", "zu", "Brot", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "PPER