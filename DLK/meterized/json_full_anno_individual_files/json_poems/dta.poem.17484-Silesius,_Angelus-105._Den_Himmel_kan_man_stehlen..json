{"dta.poem.17484": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "105.  Den Himmel kan man stehlen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer heimblich guttes w\u00fcrckt/ sein Geld au\u00dftheilt ver-", "tokens": ["Wer", "heim\u00b7blich", "gut\u00b7tes", "w\u00fcrckt", "/", "sein", "Geld", "au\u00df\u00b7theilt", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADJA", "VVFIN", "$(", "PPOSAT", "NN", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "holen/", "tokens": ["ho\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der hat das Himmelreich gar meisterlich gestohlen.", "tokens": ["Der", "hat", "das", "Him\u00b7mel\u00b7reich", "gar", "meis\u00b7ter\u00b7lich", "ge\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}