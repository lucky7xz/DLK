{"dta.poem.12957": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er sich in sie verliebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jndem ich mein gesichte", "tokens": ["In\u00b7dem", "ich", "mein", "ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Flaviens gerolltes haar,", "tokens": ["Auf", "Fla\u00b7vi\u00b7ens", "ge\u00b7roll\u00b7tes", "haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre sch\u00f6nen augen richte,", "tokens": ["Und", "ih\u00b7re", "sch\u00f6\u00b7nen", "au\u00b7gen", "rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00e4llt mein hertz in doppelte gefahr.", "tokens": ["So", "f\u00e4llt", "mein", "hertz", "in", "dop\u00b7pel\u00b7te", "ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kurtz: Amor will mich t\u00f6den oder fangen:", "tokens": ["Kurtz", ":", "A\u00b7mor", "will", "mich", "t\u00f6\u00b7den", "o\u00b7der", "fan\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "NE", "VMFIN", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um nun das letzte zu erlangen,", "tokens": ["Um", "nun", "das", "letz\u00b7te", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So mu\u00df ihr sauber haar ihm statt des netzes seyn.", "tokens": ["So", "mu\u00df", "ihr", "sau\u00b7ber", "haar", "ihm", "statt", "des", "net\u00b7zes", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAFIN", "PPER", "APPR", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die augen aber sind die bogen;", "tokens": ["Die", "au\u00b7gen", "a\u00b7ber", "sind", "die", "bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die pfeilen liefert ihm der holden blicke schein.", "tokens": ["Die", "pfei\u00b7len", "lie\u00b7fert", "ihm", "der", "hol\u00b7den", "bli\u00b7cke", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jedoch wer hat sich ie dergleichen garn entzogen?", "tokens": ["Je\u00b7doch", "wer", "hat", "sich", "ie", "derg\u00b7lei\u00b7chen", "garn", "ent\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "PRF", "ADV", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kommt, pfeile! selbst, kommt h\u00e4uffig angeflogen!", "tokens": ["Kommt", ",", "pfei\u00b7le", "!", "selbst", ",", "kommt", "h\u00e4uf\u00b7fig", "an\u00b7ge\u00b7flo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "ADV", "$,", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Jhr seyd doch allzus\u00fc\u00df und sch\u00f6n,", "tokens": ["Ihr", "seyd", "doch", "all\u00b7zu\u00b7s\u00fc\u00df", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Um euch aus bloser furcht des todes zu entgehn.", "tokens": ["Um", "euch", "aus", "blo\u00b7ser", "furcht", "des", "to\u00b7des", "zu", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}