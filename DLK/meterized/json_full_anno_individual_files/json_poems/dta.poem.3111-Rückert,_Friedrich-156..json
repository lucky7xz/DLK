{"dta.poem.3111": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "156.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Selbsthochachtung wird zur Selbstverachtung treiben,", "tokens": ["Die", "Selbst\u00b7hoch\u00b7ach\u00b7tung", "wird", "zur", "Selbst\u00b7ver\u00b7ach\u00b7tung", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie endlich Asche wird vom Feuer \u00fcbrig bleiben.", "tokens": ["Wie", "end\u00b7lich", "A\u00b7sche", "wird", "vom", "Feu\u00b7er", "\u00fcb\u00b7rig", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VAFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein G\u00f6ttliches, o Mensch, mu\u00dft du in dir erkennen,", "tokens": ["Ein", "G\u00f6tt\u00b7li\u00b7ches", ",", "o", "Mensch", ",", "mu\u00dft", "du", "in", "dir", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mu\u00dft du's nicht dein Selbst, du selbst mu\u00dft sein dich nennen.", "tokens": ["Doch", "mu\u00dft", "du's", "nicht", "dein", "Selbst", ",", "du", "selbst", "mu\u00dft", "sein", "dich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "w