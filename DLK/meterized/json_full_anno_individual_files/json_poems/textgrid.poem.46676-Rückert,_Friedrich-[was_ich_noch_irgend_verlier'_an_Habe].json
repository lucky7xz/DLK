{"textgrid.poem.46676": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was ich noch irgend verlier' an Habe]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich noch irgend verlier' an Habe,", "tokens": ["Was", "ich", "noch", "ir\u00b7gend", "ver\u00b7lier'", "an", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Davon ich nicht viel zu verlieren habe;", "tokens": ["Da\u00b7von", "ich", "nicht", "viel", "zu", "ver\u00b7lie\u00b7ren", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denk' ich, wenn ich es habe verlor'n,", "tokens": ["Denk'", "ich", ",", "wenn", "ich", "es", "ha\u00b7be", "ver\u00b7lor'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Was ist an der schlechten Habe verlor'n,", "tokens": ["Was", "ist", "an", "der", "schlech\u00b7ten", "Ha\u00b7be", "ver\u00b7lor'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da ich viel bessre Habe verlor,", "tokens": ["Da", "ich", "viel", "bess\u00b7re", "Ha\u00b7be", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die ich nie wieder grabe hervor?", "tokens": ["Die", "ich", "nie", "wie\u00b7der", "gra\u00b7be", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ich wollt', ich s\u00e4h' meiner Kinder Spiel,", "tokens": ["Ich", "wollt'", ",", "ich", "s\u00e4h'", "mei\u00b7ner", "Kin\u00b7der", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "All andres w\u00e4r' mir ein Kinderspiel;", "tokens": ["All", "and\u00b7res", "w\u00e4r'", "mir", "ein", "Kin\u00b7der\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gern wollt' ich dem Gl\u00fcck sein Gl\u00fcck verkaufen,", "tokens": ["Gern", "wollt'", "ich", "dem", "Gl\u00fcck", "sein", "Gl\u00fcck", "ver\u00b7kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "K\u00f6nnt' ich mein Gl\u00fcck zur\u00fcckerkaufen.", "tokens": ["K\u00f6nnt'", "ich", "mein", "Gl\u00fcck", "zu\u00b7r\u00fc\u00b7cker\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"