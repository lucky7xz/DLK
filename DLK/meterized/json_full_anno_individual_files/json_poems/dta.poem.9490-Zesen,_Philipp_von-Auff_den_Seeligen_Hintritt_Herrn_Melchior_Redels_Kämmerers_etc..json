{"dta.poem.9490": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auff den Seeligen Hintritt Herrn  \n Melchior Redels K\u00e4mmerers etc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ojhr Hochgelehrten M\u00e4nner/", "tokens": ["Ojhr", "Hoch\u00b7ge\u00b7lehr\u00b7ten", "M\u00e4n\u00b7ner", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Derer Wei\u00dfheit und Verstand", "tokens": ["De\u00b7rer", "Wei\u00df\u00b7heit", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "unser Stadt ist wohlbekaudt!", "tokens": ["un\u00b7ser", "Stadt", "ist", "wohl\u00b7be\u00b7kaudt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und jhr Hochgeehrten Pf\u00e4nner/", "tokens": ["und", "jhr", "Hoch\u00b7geehr\u00b7ten", "Pf\u00e4n\u00b7ner", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Derer Redligkeit sich schwingt/", "tokens": ["De\u00b7rer", "Red\u00b7lig\u00b7keit", "sich", "schwingt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Derer Muth gen Himmel dringt/", "tokens": ["De\u00b7rer", "Muth", "gen", "Him\u00b7mel", "dringt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wolt jhr dann nun von uns weichen?", "tokens": ["Wolt", "jhr", "dann", "nun", "von", "uns", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "und all\u2019 ingesambt verbleichen?", "tokens": ["und", "all'", "in\u00b7ge\u00b7sambt", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wolt jhr an den s\u00fc\u00dfen Ort", "tokens": ["Wolt", "jhr", "an", "den", "s\u00fc\u00b7\u00dfen", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Reisen/ da nur Freude schwebet/", "tokens": ["Rei\u00b7sen", "/", "da", "nur", "Freu\u00b7de", "schwe\u00b7bet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADV", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da jhr mit den Engeln lebet", "tokens": ["Da", "jhr", "mit", "den", "En\u00b7geln", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Stets in freuden fort und fort?", "tokens": ["Stets", "in", "freu\u00b7den", "fort", "und", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}