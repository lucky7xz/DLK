{"dta.poem.264": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber zwey Verliebte.  \n  Paspi\u00e8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So k\u00fchlet und l\u00f6schet die l\u00fcsterne Flammen/", "tokens": ["So", "k\u00fch\u00b7let", "und", "l\u00f6\u00b7schet", "die", "l\u00fcs\u00b7ter\u00b7ne", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Umarmet die Leiber in hefftiger Brunst/", "tokens": ["Um\u00b7ar\u00b7met", "die", "Lei\u00b7ber", "in", "heff\u00b7ti\u00b7ger", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So fl\u00f6sset die Seelen und Geister zusammen/", "tokens": ["So", "fl\u00f6s\u00b7set", "die", "See\u00b7len", "und", "Geis\u00b7ter", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und theilet in Liebe die heimliche Gunst:", "tokens": ["Und", "thei\u00b7let", "in", "Lie\u00b7be", "die", "heim\u00b7li\u00b7che", "Gunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Vergn\u00fcget die Seelen durch wechselnde K\u00fcsse/", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "die", "See\u00b7len", "durch", "wech\u00b7seln\u00b7de", "K\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "So dr\u00fccket und b\u00fccket zusammen die Brust/", "tokens": ["So", "dr\u00fc\u00b7cket", "und", "b\u00fc\u00b7cket", "zu\u00b7sam\u00b7men", "die", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Vereinget mit Anmuth der ", "tokens": ["Ver\u00b7ein\u00b7get", "mit", "An\u00b7muth", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und schmecket den Zucker der h\u00f6hesten Lust.", "tokens": ["Und", "schme\u00b7cket", "den", "Zu\u00b7cker", "der", "h\u00f6\u00b7hes\u00b7ten", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Bek\u00fcsset eur Grab-Mahl auf Lippen und Br\u00fcsten/", "tokens": ["Be\u00b7k\u00fcs\u00b7set", "eur", "Grab\u00b7Mahl", "auf", "Lip\u00b7pen", "und", "Br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und schmecket in Anmuth den s\u00fcssesten Tod/", "tokens": ["Und", "schme\u00b7cket", "in", "An\u00b7muth", "den", "s\u00fcs\u00b7ses\u00b7ten", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zerfliesset in Freuden/ ersterbet in L\u00fcsten/", "tokens": ["Zer\u00b7flies\u00b7set", "in", "Freu\u00b7den", "/", "ers\u00b7ter\u00b7bet", "in", "L\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und kostet im Sterben ein liebliches Brodt.", "tokens": ["Und", "kos\u00b7tet", "im", "Ster\u00b7ben", "ein", "lieb\u00b7li\u00b7ches", "Brodt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Jhr sterbet in L\u00fcsten/ ihr ruhet in Freuden/", "tokens": ["Ihr", "ster\u00b7bet", "in", "L\u00fcs\u00b7ten", "/", "ihr", "ru\u00b7het", "in", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Drum sterbet nur offte/ und tausend mahl mehr/", "tokens": ["Drum", "ster\u00b7bet", "nur", "off\u00b7te", "/", "und", "tau\u00b7send", "mahl", "mehr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "VVFIN", "$(", "KON", "CARD", "ADV", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Seelen die k\u00f6nnen im Sterben sich weiden/", "tokens": ["Die", "See\u00b7len", "die", "k\u00f6n\u00b7nen", "im", "Ster\u00b7ben", "sich", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "APPRART", "NN", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Die Sch\u00e4tze der Liebe sind niemahls da leer.", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "der", "Lie\u00b7be", "sind", "nie\u00b7mahls", "da", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ersterbet nur h\u00e4uffig/ so offt\u2019 als ihr wollet/", "tokens": ["E\u00b7rster\u00b7bet", "nur", "h\u00e4uf\u00b7fig", "/", "so", "offt'", "als", "ihr", "wol\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$(", "ADV", "ADV", "KOUS", "PPER", "VMFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und w\u00fcnschet euch immer die s\u00fcsseste Pein/", "tokens": ["Und", "w\u00fcn\u00b7schet", "euch", "im\u00b7mer", "die", "s\u00fcs\u00b7ses\u00b7te", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So scharrt man mit Anmuth die Todten hinein.", "tokens": ["So", "scharrt", "man", "mit", "An\u00b7muth", "die", "Tod\u00b7ten", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ach s\u00fcsses Vergn\u00fcgen! ach liebliches Sterben!", "tokens": ["Ach", "s\u00fcs\u00b7ses", "Ver\u00b7gn\u00fc\u00b7gen", "!", "ach", "lieb\u00b7li\u00b7ches", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "XY", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wo h\u00e4uffige Anmuth dasselbe beseelt;", "tokens": ["Wo", "h\u00e4uf\u00b7fi\u00b7ge", "An\u00b7muth", "das\u00b7sel\u00b7be", "be\u00b7seelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PDAT", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Es w\u00fcnscht sich einjeder ein solches Verderbeu/", "tokens": ["Es", "w\u00fcnscht", "sich", "ein\u00b7je\u00b7der", "ein", "sol\u00b7ches", "Ver\u00b7der\u00b7beu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "PIAT", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Weil keine Betr\u00fcbni\u00df die Sterbenden qu\u00e4hlt.", "tokens": ["Weil", "kei\u00b7ne", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "die", "Ster\u00b7ben\u00b7den", "qu\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Jhr lebet im Sterben/ ihr spielet in Flammen/", "tokens": ["Ihr", "le\u00b7bet", "im", "Ster\u00b7ben", "/", "ihr", "spie\u00b7let", "in", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Jhr ruffet das Leben zu k\u00fcssen den Tod/", "tokens": ["Ihr", "ruf\u00b7fet", "das", "Le\u00b7ben", "zu", "k\u00fcs\u00b7sen", "den", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Jhr r\u00f6chelt/ und fliesset in Wollust zusammen/", "tokens": ["Ihr", "r\u00f6\u00b7chelt", "/", "und", "flies\u00b7set", "in", "Wol\u00b7lust", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und w\u00fcnschet nur immer die liebliche Noht.", "tokens": ["Und", "w\u00fcn\u00b7schet", "nur", "im\u00b7mer", "die", "lieb\u00b7li\u00b7che", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Seelen verlangen so immer zu leben/", "tokens": ["Die", "See\u00b7len", "ver\u00b7lan\u00b7gen", "so", "im\u00b7mer", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und lieben den Wechsel der t\u00f6dtlichen Lust/", "tokens": ["Und", "lie\u00b7ben", "den", "Wech\u00b7sel", "der", "t\u00f6dt\u00b7li\u00b7chen", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Sie wollen stets lebend die Geister auffgeben/", "tokens": ["Sie", "wol\u00b7len", "stets", "le\u00b7bend", "die", "Geis\u00b7ter", "auff\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "-+---+-+-+--", "measure": "dactylic.init"}, "line.8": {"text": "Und suchen ihr Grab-Mahl im Schoosse und Brust/", "tokens": ["Und", "su\u00b7chen", "ihr", "Grab\u00b7Mahl", "im", "Schoos\u00b7se", "und", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}}}}}