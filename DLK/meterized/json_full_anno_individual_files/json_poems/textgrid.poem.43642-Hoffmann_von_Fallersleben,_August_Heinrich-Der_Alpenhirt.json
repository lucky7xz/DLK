{"textgrid.poem.43642": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Alpenhirt", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort hoch auf der Alpe da ist meine Welt,", "tokens": ["Dort", "hoch", "auf", "der", "Al\u00b7pe", "da", "ist", "mei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da wo's mir auf Erden am Besten gef\u00e4llt:", "tokens": ["Da", "wo's", "mir", "auf", "Er\u00b7den", "am", "Bes\u00b7ten", "ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da duften die Kr\u00e4uter, da murmelt der Quell,", "tokens": ["Da", "duf\u00b7ten", "die", "Kr\u00e4u\u00b7ter", ",", "da", "mur\u00b7melt", "der", "Quell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da klingen die Gl\u00f6cklein so lustig und hell,", "tokens": ["Da", "klin\u00b7gen", "die", "Gl\u00f6c\u00b7klein", "so", "lus\u00b7tig", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Juchhe! so lustig und hell.", "tokens": ["Juch\u00b7he", "