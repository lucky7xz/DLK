{"textgrid.poem.37597": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Genius, du hast mir viel verliehen,", "tokens": ["Mein", "Ge\u00b7nius", ",", "du", "hast", "mir", "viel", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du kannst, was nie geahndet, mir erschlie\u00dfen,", "tokens": ["Du", "kannst", ",", "was", "nie", "ge\u00b7ahn\u00b7det", ",", "mir", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "ADV", "VVPP", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn deine Blicke fl\u00fcchtig mich begr\u00fc\u00dfen,", "tokens": ["Wenn", "dei\u00b7ne", "Bli\u00b7cke", "fl\u00fcch\u00b7tig", "mich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch dich gedeiht mir jegliches Bem\u00fchen.", "tokens": ["Durch", "dich", "ge\u00b7deiht", "mir", "jeg\u00b7li\u00b7ches", "Be\u00b7m\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "O k\u00f6nnt ich dich mit meinem Arm umschlie\u00dfen,", "tokens": ["O", "k\u00f6nnt", "ich", "dich",