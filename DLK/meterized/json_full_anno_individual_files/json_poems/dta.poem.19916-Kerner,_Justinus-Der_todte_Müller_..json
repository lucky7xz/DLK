{"dta.poem.19916": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der todte M\u00fcller .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sterne \u00fcber'm Thale stehn,", "tokens": ["Die", "Ster\u00b7ne", "\u00fc\u00b7ber'm", "Tha\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00fchlrad nur man h\u00f6ret.", "tokens": ["Das", "M\u00fchl\u00b7rad", "nur", "man", "h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum kranken M\u00fcller mu\u00df ich gehn,", "tokens": ["Zum", "kran\u00b7ken", "M\u00fcl\u00b7ler", "mu\u00df", "ich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat den Freund begehret.", "tokens": ["Er", "hat", "den", "Freund", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich steig hinab den Felsenstein,", "tokens": ["Ich", "steig", "hin\u00b7ab", "den", "Fel\u00b7sen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es donnert dumpf die M\u00fchle", "tokens": ["Es", "don\u00b7nert", "dumpf", "die", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eine Glocke t\u00f6nt darein:", "tokens": ["Und", "ei\u00b7ne", "Glo\u00b7cke", "t\u00f6nt", "da\u00b7rein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edie Arbeit ist am Ziele!\u201c", "tokens": ["\u201e", "die", "Ar\u00b7beit", "ist", "am", "Zie\u00b7le", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In M\u00fcllers Kammer tret' ich nun,", "tokens": ["In", "M\u00fcl\u00b7lers", "Kam\u00b7mer", "tret'", "ich", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Starr liegt des Greisen H\u00fclle,", "tokens": ["Starr", "liegt", "des", "Grei\u00b7sen", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es stockt sein Herz, die Pulse ruh'n,", "tokens": ["Es", "stockt", "sein", "Herz", ",", "die", "Pul\u00b7se", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und drau\u00dfen auch wird's stille.", "tokens": ["Und", "drau\u00b7\u00dfen", "auch", "wird's", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die treuen Lieben weinen sehr,", "tokens": ["Die", "treu\u00b7en", "Lie\u00b7ben", "wei\u00b7nen", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still bleibt sein Herz und k\u00fchle.", "tokens": ["Still", "bleibt", "sein", "Herz", "und", "k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wasser flie\u00dfen wohl daher,", "tokens": ["Die", "Was\u00b7ser", "flie\u00b7\u00dfen", "wohl", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still aber steht die M\u00fchle.", "tokens": ["Still", "a\u00b7ber", "steht", "die", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}