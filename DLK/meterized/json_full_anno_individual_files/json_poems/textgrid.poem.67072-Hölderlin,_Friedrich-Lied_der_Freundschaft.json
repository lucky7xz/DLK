measure": "trochaic.single"}}, "stanza.24": {"line.1": {"text": "Wankt er nun in Winterst\u00fcrmen,", "tokens": ["Wankt", "er", "nun", "in", "Win\u00b7ter\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wankt er, wo sich Wolken t\u00fcrmen", "tokens": ["Wankt", "er", ",", "wo", "sich", "Wol\u00b7ken", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PRF", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Leiter, ohne Stab,", "tokens": ["Oh\u00b7ne", "Lei\u00b7ter", ",", "oh\u00b7ne", "Stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauscht er abgebleicht und d\u00fcster", "tokens": ["Lauscht", "er", "ab\u00b7ge\u00b7bleicht", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bangem Mitternachtsgefl\u00fcster", "tokens": ["Ban\u00b7gem", "Mit\u00b7ter\u00b7nachts\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ahndungsvoll am frischen Grab,", "tokens": ["Ahn\u00b7dungs\u00b7voll", "am", "fri\u00b7schen", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "O da kehren all die Stunden", "tokens": ["O", "da", "keh\u00b7ren", "all", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelnd, wie sie hingeschwunden", "tokens": ["L\u00e4\u00b7chelnd", ",", "wie", "sie", "hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Schw\u00fcren, wahr und warm,", "tokens": ["Un\u00b7ter", "Schw\u00fc\u00b7ren", ",", "wahr", "und", "warm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still und sanft, wie Blumen sinken,", "tokens": ["Still", "und", "sanft", ",", "wie", "Blu\u00b7men", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruht er, bis die V\u00e4ter winken,", "tokens": ["Ruht", "er", ",", "bis", "die", "V\u00e4\u00b7ter", "win\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir, Erinnerung! im Arm.", "tokens": ["Dir", ",", "E\u00b7rin\u00b7ne\u00b7rung", "!", "im", "Arm", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Rauscht ihm dann des Todes Fl\u00fcgel,", "tokens": ["Rauscht", "ihm", "dann", "des", "To\u00b7des", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft er ruhig unterm H\u00fcgel,", "tokens": ["Schl\u00e4ft", "er", "ru\u00b7hig", "un\u00b7term", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sein Bund den Kranz ihm flicht,", "tokens": ["Wo", "sein", "Bund", "den", "Kranz", "ihm", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Locken seiner Br\u00fcder", "tokens": ["In", "den", "Lo\u00b7cken", "sei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4uselt noch sein Geist hernieder,", "tokens": ["S\u00e4u\u00b7selt", "noch", "sein", "Geist", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lispelt leis: Verge\u00dft mich nicht!", "tokens": ["Lis\u00b7pelt", "leis", ":", "Ver\u00b7ge\u00dft", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}