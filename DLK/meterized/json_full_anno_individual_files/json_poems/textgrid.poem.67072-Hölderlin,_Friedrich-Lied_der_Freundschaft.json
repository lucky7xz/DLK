{"textgrid.poem.67072": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Held am Siegesmahle", "tokens": ["Wie", "der", "Held", "am", "Sie\u00b7ges\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhen wir um die Pokale,", "tokens": ["Ru\u00b7hen", "wir", "um", "die", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der edle Wein ergl\u00fcht,", "tokens": ["Wo", "der", "ed\u00b7le", "Wein", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feurig Arm in Arm geschlungen,", "tokens": ["Feu\u00b7rig", "Arm", "in", "Arm", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trunken von Begeisterungen", "tokens": ["Trun\u00b7ken", "von", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Singen wir der Freundschaft Lied", "tokens": ["Sin\u00b7gen", "wir", "der", "Freund\u00b7schaft", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwebt herab aus k\u00fchlen L\u00fcften,", "tokens": ["Schwebt", "her\u00b7ab", "aus", "k\u00fch\u00b7len", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebet aus den Schlummergr\u00fcften,", "tokens": ["Schwe\u00b7bet", "aus", "den", "Schlum\u00b7mer\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helden der Vergangenheit!", "tokens": ["Hel\u00b7den", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt in unsern Kreis hernieder,", "tokens": ["Kommt", "in", "un\u00b7sern", "Kreis", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Staunt und sprecht: Da ist sie wieder,", "tokens": ["Staunt", "und", "sprecht", ":", "Da", "ist", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre deutsche Herzlichkeit!", "tokens": ["Uns\u00b7re", "deut\u00b7sche", "Herz\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Uns ist Wonne, Gut und Leben", "tokens": ["Uns", "ist", "Won\u00b7ne", ",", "Gut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr den Edlen hinzugeben,", "tokens": ["F\u00fcr", "den", "Ed\u00b7len", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr unser Herz geh\u00f6rt,", "tokens": ["Der", "f\u00fcr", "un\u00b7ser", "Herz", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der zu gro\u00df, in stolzen Reigen", "tokens": ["Der", "zu", "gro\u00df", ",", "in", "stol\u00b7zen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKA", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich vor eitlem Tand zu beugen,", "tokens": ["Sich", "vor", "eit\u00b7lem", "Tand", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott und Vaterland nur ehrt.", "tokens": ["Gott", "und", "Va\u00b7ter\u00b7land", "nur", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon erhebt das Herz sich freier,", "tokens": ["Schon", "er\u00b7hebt", "das", "Herz", "sich", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmer reicht zur frohen Feier", "tokens": ["W\u00e4r\u00b7mer", "reicht", "zur", "fro\u00b7hen", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon der Freund den Becher dar,", "tokens": ["Schon", "der", "Freund", "den", "Be\u00b7cher", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Freuden, ohne Leben", "tokens": ["Oh\u00b7ne", "Freu\u00b7den", ",", "oh\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kostet' er den Saft der Reben,", "tokens": ["Kos\u00b7tet'", "er", "den", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als er ohne Freunde war.", "tokens": ["Als", "er", "oh\u00b7ne", "Freun\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bruder! schleichen bang und tr\u00fcbe", "tokens": ["Bru\u00b7der", "!", "schlei\u00b7chen", "bang", "und", "tr\u00fc\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Tage? beugt der Liebe", "tokens": ["Dei\u00b7ne", "Ta\u00b7ge", "?", "beugt", "der", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Folterpein das M\u00e4nnerherz?", "tokens": ["Fol\u00b7ter\u00b7pein", "das", "M\u00e4n\u00b7ner\u00b7herz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzt im hei\u00dfen Durst nach Ehre", "tokens": ["St\u00fcrzt", "im", "hei\u00b7\u00dfen", "Durst", "nach", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir um Mitternacht die Z\u00e4hre?", "tokens": ["Dir", "um", "Mit\u00b7ter\u00b7nacht", "die", "Z\u00e4h\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bruder, segne deinen Schmerz!", "tokens": ["Bru\u00b7der", ",", "seg\u00b7ne", "dei\u00b7nen", "Schmerz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00f6nnten wir aus G\u00f6tterh\u00e4nden", "tokens": ["K\u00f6nn\u00b7ten", "wir", "aus", "G\u00f6t\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freuden dir und Leiden spenden,", "tokens": ["Freu\u00b7den", "dir", "und", "Lei\u00b7den", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ferne w\u00e4rst du da von Harm;", "tokens": ["Fer\u00b7ne", "w\u00e4rst", "du", "da", "von", "Harm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiser ist der Gott der Liebe:", "tokens": ["Wei\u00b7ser", "ist", "der", "Gott", "der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sorgen gibt er bang und tr\u00fcbe,", "tokens": ["Sor\u00b7gen", "gibt", "er", "bang", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde gibt er treu und warm.", "tokens": ["Freun\u00b7de", "gibt", "er", "treu", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "St\u00e4rke, wenn Verleumder schreien,", "tokens": ["St\u00e4r\u00b7ke", ",", "wenn", "Ver\u00b7leum\u00b7der", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahrheit, wenn Despoten dr\u00e4uen,", "tokens": ["Wahr\u00b7heit", ",", "wenn", "Des\u00b7po\u00b7ten", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4nnermut im Mi\u00dfgeschick,", "tokens": ["M\u00e4n\u00b7ner\u00b7mut", "im", "Mi\u00df\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Duldung, wenn die Schwachen sinken,", "tokens": ["Dul\u00b7dung", ",", "wenn", "die", "Schwa\u00b7chen", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Duldung, W\u00e4rme trinken", "tokens": ["Lie\u00b7be", ",", "Dul\u00b7dung", ",", "W\u00e4r\u00b7me", "trin\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freunde von des Freundes Blick.", "tokens": ["Freun\u00b7de", "von", "des", "Freun\u00b7des", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lieblich, wie der Sommerregen,", "tokens": ["Lieb\u00b7lich", ",", "wie", "der", "Som\u00b7mer\u00b7re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich, wie er, an Erntesegen,", "tokens": ["Reich", ",", "wie", "er", ",", "an", "Ern\u00b7te\u00b7se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Perle klar und hell,", "tokens": ["Wie", "die", "Per\u00b7le", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still, wie Edens Str\u00f6me gleiten,", "tokens": ["Still", ",", "wie", "E\u00b7dens", "Str\u00f6\u00b7me", "glei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlos, wie die Ewigkeiten,", "tokens": ["End\u00b7los", ",", "wie", "die", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fleu\u00dft der Freundschaft Silberquell.", "tokens": ["Fleu\u00dft", "der", "Freund\u00b7schaft", "Sil\u00b7ber\u00b7quell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Drum, so wollen, eh die Freuden", "tokens": ["Drum", ",", "so", "wol\u00b7len", ",", "eh", "die", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "ADV", "VMFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trennungen und Tode neiden,", "tokens": ["Tren\u00b7nun\u00b7gen", "und", "To\u00b7de", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir im hehren Eichenhain", "tokens": ["Wir", "im", "heh\u00b7ren", "Ei\u00b7chen\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder unter Fr\u00fchlingsrosen,", "tokens": ["O\u00b7der", "un\u00b7ter", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn am Becher Weste kosen,", "tokens": ["Wenn", "am", "Be\u00b7cher", "Wes\u00b7te", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrdig uns der Freundschaft freun.", "tokens": ["W\u00fcr\u00b7dig", "uns", "der", "Freund\u00b7schaft", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Rufet aus der trauten Halle", "tokens": ["Ru\u00b7fet", "aus", "der", "trau\u00b7ten", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Auserw\u00e4hlten alle", "tokens": ["Auch", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Ferne das Geschick,", "tokens": ["In", "die", "Fer\u00b7ne", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt, auf freundelosen Pfaden", "tokens": ["Bleibt", ",", "auf", "freun\u00b7de\u00b7lo\u00b7sen", "Pfa\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hinzugehn, mit Schmerz beladen,", "tokens": ["Hin\u00b7zu\u00b7gehn", ",", "mit", "Schmerz", "be\u00b7la\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIZU", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4nend ", "tokens": ["Tr\u00e4\u00b7nend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Wankt er nun in Winterst\u00fcrmen,", "tokens": ["Wankt", "er", "nun", "in", "Win\u00b7ter\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wankt er, wo sich Wolken t\u00fcrmen", "tokens": ["Wankt", "er", ",", "wo", "sich", "Wol\u00b7ken", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PRF", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Leiter, ohne Stab,", "tokens": ["Oh\u00b7ne", "Lei\u00b7ter", ",", "oh\u00b7ne", "Stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauscht er abgebleicht und d\u00fcster", "tokens": ["Lauscht", "er", "ab\u00b7ge\u00b7bleicht", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bangem Mitternachtsgefl\u00fcster", "tokens": ["Ban\u00b7gem", "Mit\u00b7ter\u00b7nachts\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ahndungsvoll am frischen Grab,", "tokens": ["Ahn\u00b7dungs\u00b7voll", "am", "fri\u00b7schen", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "O da kehren all die Stunden", "tokens": ["O", "da", "keh\u00b7ren", "all", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelnd, wie sie hingeschwunden", "tokens": ["L\u00e4\u00b7chelnd", ",", "wie", "sie", "hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Schw\u00fcren, wahr und warm,", "tokens": ["Un\u00b7ter", "Schw\u00fc\u00b7ren", ",", "wahr", "und", "warm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still und sanft, wie Blumen sinken,", "tokens": ["Still", "und", "sanft", ",", "wie", "Blu\u00b7men", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruht er, bis die V\u00e4ter winken,", "tokens": ["Ruht", "er", ",", "bis", "die", "V\u00e4\u00b7ter", "win\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir, Erinnerung! im Arm.", "tokens": ["Dir", ",", "E\u00b7rin\u00b7ne\u00b7rung", "!", "im", "Arm", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Rauscht ihm dann des Todes Fl\u00fcgel,", "tokens": ["Rauscht", "ihm", "dann", "des", "To\u00b7des", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft er ruhig unterm H\u00fcgel,", "tokens": ["Schl\u00e4ft", "er", "ru\u00b7hig", "un\u00b7term", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sein Bund den Kranz ihm flicht,", "tokens": ["Wo", "sein", "Bund", "den", "Kranz", "ihm", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Locken seiner Br\u00fcder", "tokens": ["In", "den", "Lo\u00b7cken", "sei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4uselt noch sein Geist hernieder,", "tokens": ["S\u00e4u\u00b7selt", "noch", "sein", "Geist", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lispelt leis: Verge\u00dft mich nicht!", "tokens": ["Lis\u00b7pelt", "leis", ":", "Ver\u00b7ge\u00dft", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line