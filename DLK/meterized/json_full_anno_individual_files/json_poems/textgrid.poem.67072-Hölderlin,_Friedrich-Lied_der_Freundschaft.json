 ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr unser Herz geh\u00f6rt,", "tokens": ["Der", "f\u00fcr", "un\u00b7ser", "Herz", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der zu gro\u00df, in stolzen Reigen", "tokens": ["Der", "zu", "gro\u00df", ",", "in", "stol\u00b7zen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKA", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich vor eitlem Tand zu beugen,", "tokens": ["Sich", "vor", "eit\u00b7lem", "Tand", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott und Vaterland nur ehrt.", "tokens": ["Gott", "und", "Va\u00b7ter\u00b7land", "nur", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Schon erhebt das Herz sich freier,", "tokens": ["Schon", "er\u00b7hebt", "das", "Herz", "sich", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmer reicht zur frohen Feier", "tokens": ["W\u00e4r\u00b7mer", "reicht", "zur", "fro\u00b7hen", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon der Freund den Becher dar,", "tokens": ["Schon", "der", "Freund", "den", "Be\u00b7cher", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Freuden, ohne Leben", "tokens": ["Oh\u00b7ne", "Freu\u00b7den", ",", "oh\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kostet' er den Saft der Reben,", "tokens": ["Kos\u00b7tet'", "er", "den", "Saft", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als er ohne Freunde war.", "tokens": ["Als", "er", "oh\u00b7ne", "Freun\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Bruder! schleichen bang und tr\u00fcbe", "tokens": ["Bru\u00b7der", "!", "schlei\u00b7chen", "bang", "und", "tr\u00fc\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Tage? beugt der Liebe", "tokens": ["Dei\u00b7ne", "Ta\u00b7ge", "?", "beugt", "der", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Folterpein das M\u00e4nnerherz?", "tokens": ["Fol\u00b7ter\u00b7pein", "das", "M\u00e4n\u00b7ner\u00b7herz", "?"], "token_info": ["word", "word", "word", "pun