euch", "nichts", "se\u00b7hen", ".", "Ich", "bin", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Bin ich das sch\u00f6n D\u00e4nnerl im Thal,", "tokens": ["Bin", "ich", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strick Socken;", "tokens": ["Strick", "So\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da kommen die Gassenbuben all,", "tokens": ["Da", "kom\u00b7men", "die", "Gas\u00b7sen\u00b7bu\u00b7ben", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wollen locken.", "tokens": ["Wol\u00b7len", "lo\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Geht, reist, ich mag nicht spielen,", "tokens": ["Geht", ",", "reist", ",", "ich", "mag", "nicht", "spie\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr seyd mir zu viele: Ich bin etc.", "tokens": ["Ihr", "seyd", "mir", "zu", "vie\u00b7le", ":", "Ich", "bin", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PIS", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Bin ich das sch\u00f6n D\u00e4nnerl im Thal,", "tokens": ["Bin", "ich", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thu giessen;", "tokens": ["Thu", "gies\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da kommen die Sch\u00fctzenbursch all,", "tokens": ["Da", "kom\u00b7men", "die", "Sch\u00fct\u00b7zen\u00b7bursch", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wollen schiessen.", "tokens": ["Wol\u00b7len", "schies\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Geht, lasset das nur bleiben,", "tokens": ["Geht", ",", "las\u00b7set", "das", "nur", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PDS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Blumen sind kein Scheiben: Ich bin etc.", "tokens": ["Mein", "Blu\u00b7men", "sind", "kein", "Schei\u00b7ben", ":", "Ich", "bin", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Bin ich das sch\u00f6n D\u00e4nnerl im Thal,", "tokens": ["Bin", "ich", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thu lieben;", "tokens": ["Thu", "lie\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da komnten Studentenbursch all", "tokens": ["Da", "komn\u00b7ten", "Stu\u00b7den\u00b7ten\u00b7bursch", "all"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PIAT"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit den Hiebern.", "tokens": ["Mit", "den", "Hie\u00b7bern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ja ja ihr meine Herren,", "tokens": ["Ja", "ja", "ihr", "mei\u00b7ne", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich will euch nicht aussperren:", "tokens": ["Ich", "will", "euch", "nicht", "aus\u00b7sper\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin das sch\u00f6n D\u00e4nnerl im Thal,", "tokens": ["Ich", "bin", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und bleib das sch\u00f6n D\u00e4nnerl allemal.", "tokens": ["Und", "bleib", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "al\u00b7le\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}