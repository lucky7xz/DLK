 bin das sch\u00f6n D\u00e4nnerl im Thal,", "tokens": ["Ich", "bin", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und bleib das sch\u00f6n D\u00e4nnerl allemal.", "tokens": ["Und", "bleib", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "al\u00b7le\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bin ich das sch\u00f6n D\u00e4nnerl im Thal,", "tokens": ["Bin", "ich", "das", "sch\u00f6n", "D\u00e4n\u00b7nerl", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strick B\u00e4ndlein;", "tokens": ["Strick", "B\u00e4n\u00b7dlein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da kommen die Schreibersbuben,", "tokens": ["Da", "kom\u00b7men", "die", "Schrei\u00b7bers\u00b7bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "All wollen t\u00e4ndeln.", "tokens": ["All", "wol\u00b7len", "t\u00e4n\u00b7deln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ich la\u00df euch nicht t\u00e4ndeln,", "tokens": ["Ich", "la\u00df", "euch", "nicht", "t\u00e4n\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"]