{"textgrid.poem.66698": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Messerschnitt. Was hilft's, sich zu bel\u00fcgen?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Messerschnitt. Was hilft's, sich zu bel\u00fcgen?", "tokens": ["Ein", "Mes\u00b7ser\u00b7schnitt", ".", "Was", "hilft's", ",", "sich", "zu", "be\u00b7l\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VVFIN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh die Welt, die anders ist als ich.", "tokens": ["Ich", "seh", "die", "Welt", ",", "die", "an\u00b7ders", "ist", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tatsachen herrschen. Ich auch mu\u00df mich f\u00fcgen", "tokens": ["Tat\u00b7sa\u00b7chen", "herr\u00b7schen", ".", "Ich", "auch", "mu\u00df", "mich", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "PPER", "ADV", "VMFIN", "PRF", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dem, was gef\u00fcgt ist unab\u00e4nderlich.", "tokens": ["Dem", ",", "was", "ge\u00b7f\u00fcgt", "ist", "un\u00b7ab\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "VVPP", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hart ist's, entzifferst du aus Menschenz\u00fcgen", "tokens": ["Hart", "ist's", ",", "ent\u00b7zif\u00b7ferst", "du", "aus", "Men\u00b7schen\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch deine Rechnung den brutalen Strich \u2013", "tokens": ["Durch", "dei\u00b7ne", "Rech\u00b7nung", "den", "bru\u00b7ta\u00b7len", "Strich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch besser ist's, Notwendiges klar erkennen,", "tokens": ["Doch", "bes\u00b7ser", "ist's", ",", "Not\u00b7wen\u00b7di\u00b7ges", "klar", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Als blind in weichem Wahne sich verrennen.", "tokens": ["Als", "blind", "in", "wei\u00b7chem", "Wah\u00b7ne", "sich", "ver\u00b7ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dank, Phantasie! Du schenkst mir aus dem Vollen", "tokens": ["Dank", ",", "Phan\u00b7ta\u00b7sie", "!", "Du", "schenkst", "mir", "aus", "dem", "Vol\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Wundertrank der dichterischen Welt.", "tokens": ["Den", "Wun\u00b7der\u00b7trank", "der", "dich\u00b7te\u00b7ri\u00b7schen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die h\u00f6chste Huldigung will ich dir zollen,", "tokens": ["Die", "h\u00f6chs\u00b7te", "Hul\u00b7di\u00b7gung", "will", "ich", "dir", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die mir der Dinge Wesen nicht entstellt.", "tokens": ["Die", "mir", "der", "Din\u00b7ge", "We\u00b7sen", "nicht", "ent\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein Stiefgeschwister aber mag sich trollen,", "tokens": ["Dein", "Stief\u00b7ge\u00b7schwis\u00b7ter", "a\u00b7ber", "mag", "sich", "trol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Phantastik, die mich um die Wahrheit prellt \u2013", "tokens": ["Phan\u00b7tas\u00b7tik", ",", "die", "mich", "um", "die", "Wahr\u00b7heit", "prellt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich lasse mich vom schwindlerischen Schalten", "tokens": ["Ich", "las\u00b7se", "mich", "vom", "schwind\u00b7le\u00b7ri\u00b7schen", "Schal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Seelencirce nicht zum Narren halten.", "tokens": ["Der", "See\u00b7len\u00b7cir\u00b7ce", "nicht", "zum", "Nar\u00b7ren", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich will die Wahrheit unbem\u00e4ntelt schauen,", "tokens": ["Ich", "will", "die", "Wahr\u00b7heit", "un\u00b7be\u00b7m\u00e4n\u00b7telt", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn sie noch so grausam schmerzlich ist,", "tokens": ["Und", "wenn", "sie", "noch", "so", "grau\u00b7sam", "schmerz\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Scheingebilden hab ich kein Vertrauen,", "tokens": ["Zu", "Schein\u00b7ge\u00b7bil\u00b7den", "hab", "ich", "kein", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seinem Trugnetz f\u00e4ngt sich der Sophist.", "tokens": ["In", "sei\u00b7nem", "Trug\u00b7netz", "f\u00e4ngt", "sich", "der", "So\u00b7phist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man mu\u00df das Leben platterdings verdauen,", "tokens": ["Man", "mu\u00df", "das", "Le\u00b7ben", "plat\u00b7ter\u00b7dings", "ver\u00b7dau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wahrheit lacht jeder Taschenspielerlist,", "tokens": ["Wahr\u00b7heit", "lacht", "je\u00b7der", "Ta\u00b7schen\u00b7spie\u00b7ler\u00b7list", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und unausweichlich waltet in den Dingen", "tokens": ["Und", "un\u00b7aus\u00b7weich\u00b7lich", "wal\u00b7tet", "in", "den", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Schicksal, dem sich f\u00fcgen freiste Schwingen ...", "tokens": ["Ein", "Schick\u00b7sal", ",", "dem", "sich", "f\u00fc\u00b7gen", "freis\u00b7te", "Schwin\u00b7gen", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVINF", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Messerschnitt. Was hilft's, sich zu bel\u00fcgen?", "tokens": ["Ein", "Mes\u00b7ser\u00b7schnitt", ".", "Was", "hilft's", ",", "sich", "zu", "be\u00b7l\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VVFIN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh die Welt, die anders ist als ich.", "tokens": ["Ich", "seh", "die", "Welt", ",", "die", "an\u00b7ders", "ist", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "w