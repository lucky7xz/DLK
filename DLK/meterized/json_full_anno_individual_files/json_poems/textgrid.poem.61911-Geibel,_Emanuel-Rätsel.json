{"textgrid.poem.61911": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4tsel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch H\u00f6ll' und durch Himmel erklingt's wie ein Hauch,", "tokens": ["Durch", "H\u00f6ll'", "und", "durch", "Him\u00b7mel", "er\u00b7klingt's", "wie", "ein", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und im leisesten Herzschlag vernimmst du es auch;", "tokens": ["Und", "im", "lei\u00b7ses\u00b7ten", "Herz\u00b7schlag", "ver\u00b7nimmst", "du", "es", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Es schwebt bei den Horen zuv\u00f6rderst im Reihn,", "tokens": ["Es", "schwebt", "bei", "den", "Ho\u00b7ren", "zu\u00b7v\u00f6r\u00b7derst", "im", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und was hoch ist und herrlich, das schlie\u00dfet es ein.", "tokens": ["Und", "was", "hoch", "ist", "und", "herr\u00b7lich", ",", "das", "schlie\u00b7\u00dfet", "es", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "KON", "ADJD", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Ob stumm auch, erscheint's dir in jeglicher Tat,", "tokens": ["Ob", "stumm", "auch", ",", "er\u00b7scheint's", "dir", "in", "jeg\u00b7li\u00b7cher", "Tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und die Heerschlacht beginnt's und beschlie\u00dfet im Rat;", "tokens": ["Und", "die", "Heer\u00b7schlacht", "be\u00b7ginnt's", "und", "be\u00b7schlie\u00b7\u00dfet", "im", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Aus der Lohe, der wehenden, winkt es dir zu,", "tokens": ["Aus", "der", "Lo\u00b7he", ",", "der", "we\u00b7hen\u00b7den", ",", "winkt", "es", "dir", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und es sch\u00e4rft sich im Licht und erstirbt in der Ruh'.", "tokens": ["Und", "es", "sch\u00e4rft", "sich", "im", "Licht", "und", "er\u00b7stirbt", "in", "der", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Dem Gedanken versagt sich's, nicht fa\u00dft's der Verstand,", "tokens": ["Dem", "Ge\u00b7dan\u00b7ken", "ver\u00b7sagt", "sich's", ",", "nicht", "fa\u00dft's", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Doch in Blindheit ergreif's, und du hast's in der Hand.", "tokens": ["Doch", "in", "Blind\u00b7heit", "er\u00b7greif's", ",", "und", "du", "hast's", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NE", "$,", "KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Sanft schwellt's dein Gef\u00fchl und vollendet dein Ich,", "tokens": ["Sanft", "schwellt's", "dein", "Ge\u00b7f\u00fchl", "und", "voll\u00b7en\u00b7det", "dein", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "PPER", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zu Erz wird das Herz, dem es treulos entwich.", "tokens": ["Und", "zu", "Erz", "wird", "das", "Herz", ",", "dem", "es", "treu\u00b7los", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Durch H\u00f6ll' und durch Himmel erklingt's wie ein Hauch,", "tokens": ["Durch", "H\u00f6ll'", "und", "durch", "Him\u00b7mel", "er\u00b7klingt's", "wie", "ein", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "m