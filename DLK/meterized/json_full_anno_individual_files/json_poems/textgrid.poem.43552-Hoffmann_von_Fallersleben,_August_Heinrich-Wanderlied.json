{"textgrid.poem.43552": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6gel singen, Blumen bl\u00fchen,", "tokens": ["V\u00f6\u00b7gel", "sin\u00b7gen", ",", "Blu\u00b7men", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcn ist wieder Wald und Feld.", "tokens": ["Gr\u00fcn", "ist", "wie\u00b7der", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so la\u00dft uns zieh'n und wandern", "tokens": ["O", "so", "la\u00dft", "uns", "zieh'n", "und", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVIMP", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem einen Ort zum andern", "tokens": ["Von", "dem", "ei\u00b7nen", "Ort", "zum", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die weite gr\u00fcne Welt.", "tokens": ["Durch", "die", "wei\u00b7te", "gr\u00fc\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie im Bauer sitzt der Vogel,", "tokens": ["Wie", "im", "Bau\u00b7er", "sitzt", "der", "Vo\u00b7gel