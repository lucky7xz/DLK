{"dta.poem.12225": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Prinz Vogelfrei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So h\u00e4ng ich denn auf krummem Aste", "tokens": ["So", "h\u00e4ng", "ich", "denn", "auf", "krum\u00b7mem", "As\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "                                 Hoch \u00fcber Meer und H\u00fcgelchen:", "tokens": ["Hoch", "\u00fc\u00b7ber", "Meer", "und", "H\u00fc\u00b7gel\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "                                 Ein Vogel lud mich her zu Gaste \u2014", "tokens": ["Ein", "Vo\u00b7gel", "lud", "mich", "her", "zu", "Gas\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APZR", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "                                 Ich flog ihm nach und rast\u2019 und raste", "tokens": ["Ich", "flog", "ihm", "nach", "und", "rast'", "und", "ras\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "                                     Und schlage mit den Fl\u00fcgelchen.", "tokens": ["Und", "schla\u00b7ge", "mit", "den", "Fl\u00fc\u00b7gel\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Das weisse Meer ist eingeschlafen,", "tokens": ["Das", "weis\u00b7se", "Meer", "ist", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "                                 Es schl\u00e4ft mir jedes Weh und Ach.", "tokens": ["Es", "schl\u00e4ft", "mir", "je\u00b7des", "Weh", "und", "Ach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "                                 Vergessen hab\u2019 ich Ziel und Hafen,", "tokens": ["Ver\u00b7ges\u00b7sen", "hab'", "ich", "Ziel", "und", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "                                 Vergessen Furcht und Lob und Strafen:", "tokens": ["Ver\u00b7ges\u00b7sen", "Furcht", "und", "Lob", "und", "Stra\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "                             Jetzt flieg ich jedem Vogel nach.", "tokens": ["Jetzt", "flieg", "ich", "je\u00b7dem", "Vo\u00b7gel", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur Schritt f\u00fcr Schritt \u2014 das ist kein Leben!", "tokens": ["Nur", "Schritt", "f\u00fcr", "Schritt", "das", "ist", "kein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$(", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "                                 St\u00e4ts Bein vor Bein macht m\u00fcd und schwer!", "tokens": ["St\u00e4ts", "Bein", "vor", "Bein", "macht", "m\u00fcd", "und", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "                                 Ich lass mich von den Winden heben,", "tokens": ["Ich", "lass", "mich", "von", "den", "Win\u00b7den", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "                                 Ich liebe es, mit Fl\u00fcgeln schweben", "tokens": ["Ich", "lie\u00b7be", "es", ",", "mit", "Fl\u00fc\u00b7geln", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "                             Und hinter jedem Vogel her.", "tokens": ["Und", "hin\u00b7ter", "je\u00b7dem", "Vo\u00b7gel", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vernunft? \u2014 das ist ein b\u00f6s Gesch\u00e4fte:", "tokens": ["Ver\u00b7nunft", "?", "das", "ist", "ein", "b\u00f6s", "Ge\u00b7sch\u00e4f\u00b7te", ":"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "                                 Vernunft und Zunge stolpern viel!", "tokens": ["Ver\u00b7nunft", "und", "Zun\u00b7ge", "stol\u00b7pern", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "                                 Das Fliegen gab mir neue Kr\u00e4fte", "tokens": ["Das", "Flie\u00b7gen", "gab", "mir", "neu\u00b7e", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "                                 Und lehrt\u2019 mich sch\u00f6nere Gesch\u00e4fte,", "tokens": ["Und", "lehrt'", "mich", "sch\u00f6\u00b7ne\u00b7re", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "                             Gesang und Scherz und Liederspiel.", "tokens": ["Ge\u00b7sang", "und", "Scherz", "und", "Lie\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Einsam zu denken \u2014 das ist weise,", "tokens": ["Ein\u00b7sam", "zu", "den\u00b7ken", "das", "ist", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$(", "PDS", "VAFIN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "             Einsam zu singen \u2014 das ist dumm!", "tokens": ["Ein\u00b7sam", "zu", "sin\u00b7gen", "das", "ist", "dumm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$(", "PDS", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "             So horcht mir denn auf meine Weise", "tokens": ["So", "horcht", "mir", "denn", "auf", "mei\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "             Und setzt euch still um mich im Kreise,", "tokens": ["Und", "setzt", "euch", "still", "um", "mich", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "         Ihr sch\u00f6nen V\u00f6gelchen, herum!", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "V\u00f6\u00b7gel\u00b7chen", ",", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}