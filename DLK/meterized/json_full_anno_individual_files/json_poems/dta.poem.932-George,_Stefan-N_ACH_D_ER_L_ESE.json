{"dta.poem.932": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wir schreiten auf und ab im reichen flitter               ", "tokens": ["Wir", "schrei\u00b7ten", "auf", "und", "ab", "im", "rei\u00b7chen", "flit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des buchenganges beinah bis zum thore               ", "tokens": ["Des", "bu\u00b7chen\u00b7gan\u00b7ges", "bei\u00b7nah", "bis", "zum", "tho\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sehen aussen in dem feld vom gitter               ", "tokens": ["Und", "se\u00b7hen", "aus\u00b7sen", "in", "dem", "feld", "vom", "git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den mandelbaum zum zweitenmal im flore             ", "tokens": ["Den", "man\u00b7del\u00b7baum", "zum", "zwei\u00b7ten\u00b7mal", "im", "flo\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADV", "APPRART", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir suchen nach den schattenfreien b&#228;nken               ", "tokens": ["Wir", "su\u00b7chen", "nach", "den", "schat\u00b7ten\u00b7frei\u00b7en", "b", "&#228;", "nken"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "XY", "$(", "VVINF"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dort wo uns niemals fremde stimmen scheuchten               ", "tokens": ["Dort", "wo", "uns", "nie\u00b7mals", "frem\u00b7de", "stim\u00b7men", "scheuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In tr&#228;umen unsre arme sich verschr&#228;nken               ", "tokens": ["In", "tr", "&#228;", "u\u00b7men", "uns\u00b7re", "ar\u00b7me", "sich", "ver\u00b7schr", "&#228;", "nken"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "NE", "$(", "VVFIN", "PPOSAT", "ADJA", "PRF", "ADJD", "$(", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wir laben uns am langen milden leuchten             ", "tokens": ["Wir", "la\u00b7ben", "uns", "am", "lan\u00b7gen", "mil\u00b7den", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir f&#252;hlen dankbar wie zu leisem brausen               ", "tokens": ["Wir", "f", "&#252;", "hlen", "dank\u00b7bar", "wie", "zu", "lei\u00b7sem", "brau\u00b7sen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "ADJD", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von wipfeln strahlenspuren auf uns tropfen               ", "tokens": ["Von", "wip\u00b7feln", "strah\u00b7len\u00b7spu\u00b7ren", "auf", "uns", "trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und blicken nur und horchen wenn in pausen               ", "tokens": ["Und", "bli\u00b7cken", "nur", "und", "hor\u00b7chen", "wenn", "in", "pau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "KOUS", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die reifen fr&#252;chte an den boden klopfen.             ", "tokens": ["Die", "rei\u00b7fen", "fr", "&#252;", "ch\u00b7te", "an", "den", "bo\u00b7den", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$(", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}