{"dta.poem.18110": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Gottfriede von K\u00fcnrath  \n Die Buchstaben versetzt:", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jst dir das blinde Gl\u00fcck geneiget und gewogen,", "tokens": ["Ist", "dir", "das", "blin\u00b7de", "Gl\u00fcck", "ge\u00b7nei\u00b7get", "und", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will jeder bey dir seyn an Freundschaft vorgezogen;", "tokens": ["Will", "je\u00b7der", "bey", "dir", "seyn", "an", "Freund\u00b7schaft", "vor\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "PPOSAT", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verschl\u00e4get dich der Wind ins weite Meer der Noth,", "tokens": ["Ver\u00b7schl\u00e4\u00b7get", "dich", "der", "Wind", "ins", "wei\u00b7te", "Meer", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So steht es w\u00fcst, und dann ist kein Freund treu, ohn Gott.", "tokens": ["So", "steht", "es", "w\u00fcst", ",", "und", "dann", "ist", "kein", "Freund", "treu", ",", "ohn", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "KON", "ADV", "VAFIN", "PIAT", "NN", "ADJD", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}}}}}