{"dta.poem.20015": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtlicher Besuch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist gegangen,", "tokens": ["Der", "Tag", "ist", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hier irr' ich allein,", "tokens": ["Hier", "irr'", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie graut mir hier au\u00dfen!", "tokens": ["Wie", "graut", "mir", "hier", "au\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "O la\u00df mich hinein.", "tokens": ["O", "la\u00df", "mich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hier innen ists dunkel,", "tokens": ["Hier", "in\u00b7nen", "ists", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die H\u00fctte ist klein,", "tokens": ["Die", "H\u00fct\u00b7te", "ist", "klein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Mond steht da drau\u00dfen,", "tokens": ["Der", "Mond", "steht", "da", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du bist nicht allein.", "tokens": ["Du", "bist", "nicht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und willst du nicht \u00f6ffnen,", "tokens": ["Und", "willst", "du", "nicht", "\u00f6ff\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So geh' ich in Wald", "tokens": ["So", "geh'", "ich", "in", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und blase mein H\u00f6rnlein", "tokens": ["Und", "bla\u00b7se", "mein", "H\u00f6rn\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das r\u00fcstig erschallt,", "tokens": ["Das", "r\u00fcs\u00b7tig", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und jage die Wolken", "tokens": ["Und", "ja\u00b7ge", "die", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Himmel wohl all',", "tokens": ["Vom", "Him\u00b7mel", "wohl", "all'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PIS", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Dann tanzen die Sterne", "tokens": ["Dann", "tan\u00b7zen", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zum lustigen Schall.", "tokens": ["Zum", "lus\u00b7ti\u00b7gen", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle, darfst glauben,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "darfst", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PAV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Indessen kein Leid,", "tokens": ["In\u00b7des\u00b7sen", "kein", "Leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich treibe wohl tr\u00e4umend", "tokens": ["Ich", "trei\u00b7be", "wohl", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Sch\u00e4flein zur Weid.", "tokens": ["Die", "Sch\u00e4f\u00b7lein", "zur", "Weid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich lausche dem Vogel,", "tokens": ["Ich", "lau\u00b7sche", "dem", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Er singet von Scherz,", "tokens": ["Er", "sin\u00b7get", "von", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ich liege bei Blumen \u2014", "tokens": ["Ich", "lie\u00b7ge", "bei", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das bringet nicht Schmerz.", "tokens": ["Das", "brin\u00b7get", "nicht", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}