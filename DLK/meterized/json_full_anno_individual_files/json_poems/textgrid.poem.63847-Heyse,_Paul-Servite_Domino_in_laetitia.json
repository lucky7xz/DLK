{"textgrid.poem.63847": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Servite Domino in laetitia", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonntag. Die Gassen still und leer.", "tokens": ["Sonn\u00b7tag", ".", "Die", "Gas\u00b7sen", "still", "und", "leer", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Kein Laut aus einem Hause dringt,", "tokens": ["Kein", "Laut", "aus", "ei\u00b7nem", "Hau\u00b7se", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur aus der hohen Kirche klingt", "tokens": ["Nur", "aus", "der", "ho\u00b7hen", "Kir\u00b7che", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Orgel Summen zu mir her.", "tokens": ["Der", "Or\u00b7gel", "Sum\u00b7men", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und durch die Pforte tret' ich sacht", "tokens": ["Und", "durch", "die", "Pfor\u00b7te", "tret'", "ich", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seh' in D\u00e4mmrung eingeh\u00fcllt", "tokens": ["Und", "seh'", "in", "D\u00e4mm\u00b7rung", "ein\u00b7ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Volk, das alle B\u00e4nke f\u00fcllt,", "tokens": ["Das", "Volk", ",", "das", "al\u00b7le", "B\u00e4n\u00b7ke", "f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Keiner hat des Ketzers Acht.", "tokens": ["Doch", "Kei\u00b7ner", "hat", "des", "Ket\u00b7zers", "Acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4nnlein und Weiblein hingeb\u00fcckt", "tokens": ["M\u00e4nn\u00b7lein", "und", "Weib\u00b7lein", "hin\u00b7ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und lautlos betend aus dem Buch,", "tokens": ["Und", "laut\u00b7los", "be\u00b7tend", "aus", "dem", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Frau'n im schwarzen Schleiertuch,", "tokens": ["Die", "Frau'n", "im", "schwar\u00b7zen", "Schlei\u00b7er\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das auch das j\u00fcngste M\u00e4gdlein schm\u00fcckt.", "tokens": ["Das", "auch", "das", "j\u00fcngs\u00b7te", "M\u00e4gd\u00b7lein", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Am Ehrenplatz beim Hochaltar", "tokens": ["Am", "Eh\u00b7ren\u00b7platz", "beim", "Hoc\u00b7hal\u00b7tar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein alter Vize-Sindaco.", "tokens": ["Mein", "al\u00b7ter", "Vi\u00b7ze\u00b7Sin\u00b7da\u00b7co", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da thront er, seiner W\u00fcrde froh,", "tokens": ["Da", "thront", "er", ",", "sei\u00b7ner", "W\u00fcr\u00b7de", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hochaufgestr\u00e4ubt sein schneewei\u00df Haar.", "tokens": ["Hoch\u00b7auf\u00b7ge\u00b7str\u00e4ubt", "sein", "schnee\u00b7wei\u00df", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Sindachessa im Gebet", "tokens": ["Die", "Sin\u00b7da\u00b7ches\u00b7sa", "im", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kniet bei den \u00c4rmsten an der T\u00fcr.", "tokens": ["Kniet", "bei", "den", "\u00c4rms\u00b7ten", "an", "der", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie hofft, Gott lohnt die Demut ihr;", "tokens": ["Sie", "hofft", ",", "Gott", "lohnt", "die", "De\u00b7mut", "ihr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sich erniedrigt, wird erh\u00f6ht.", "tokens": ["Wer", "sich", "er\u00b7nied\u00b7rigt", ",", "wird", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Durch bunte Scheiben gl\u00fcht herein", "tokens": ["Durch", "bun\u00b7te", "Schei\u00b7ben", "gl\u00fcht", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein warmer Glanz im Chore dort.", "tokens": ["Ein", "war\u00b7mer", "Glanz", "im", "Cho\u00b7re", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuweilen t\u00f6nt ein Priesterwort,", "tokens": ["Zu\u00b7wei\u00b7len", "t\u00f6nt", "ein", "Pries\u00b7ter\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kurz nur f\u00e4llt die Orgel ein.", "tokens": ["Und", "kurz", "nur", "f\u00e4llt", "die", "Or\u00b7gel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und jetzt das ", "tokens": ["Und", "jetzt", "das"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Doch r\u00fchrt noch Keines sich, zu gehn,", "tokens": ["Doch", "r\u00fchrt", "noch", "Kei\u00b7nes", "sich", ",", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "PRF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn auf des Orgelchores H\u00f6h'n", "tokens": ["Denn", "auf", "des", "Or\u00b7gel\u00b7cho\u00b7res", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beginnt nun erst das sch\u00f6nste Fest.", "tokens": ["Be\u00b7ginnt", "nun", "erst", "das", "sch\u00f6ns\u00b7te", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und alle lauschen andachtsvoll", "tokens": ["Und", "al\u00b7le", "lau\u00b7schen", "an\u00b7dachts\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Nachspiel, das so weltlich t\u00f6nt.", "tokens": ["Dem", "Nach\u00b7spiel", ",", "das", "so", "welt\u00b7lich", "t\u00f6nt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind's von Jugend auf gew\u00f6hnt,", "tokens": ["Sie", "sin\u00b7d's", "von", "Ju\u00b7gend", "auf", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df so das Hochamt enden soll.", "tokens": ["Da\u00df", "so", "das", "Hoc\u00b7hamt", "en\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was h\u00f6r' ich? Verdi? Ja, f\u00fcrwahr,", "tokens": ["Was", "h\u00f6r'", "ich", "?", "Ver\u00b7di", "?", "Ja", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$.", "PTKANT", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Traviata, Troubadour.", "tokens": ["Aus", "Tra\u00b7vi\u00b7a\u00b7ta", ",", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von frommen Weisen keine Spur", "tokens": ["Von", "from\u00b7men", "Wei\u00b7sen", "kei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Haus des Herrn, wie sonderbar!", "tokens": ["Im", "Haus", "des", "Herrn", ",", "wie", "son\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wild und wilder jauchzt und st\u00f6hnt", "tokens": ["Und", "wild", "und", "wil\u00b7der", "jauchzt", "und", "st\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliebte Lust und Leidenschaft,", "tokens": ["Ver\u00b7lieb\u00b7te", "Lust", "und", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis mit des ganzen Werkes Kraft", "tokens": ["Bis", "mit", "des", "gan\u00b7zen", "Wer\u00b7kes", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt ein flottes Tanzlied t\u00f6nt,", "tokens": ["Zu\u00b7letzt", "ein", "flot\u00b7tes", "Tanz\u00b7lied", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Im Polkatakt! Doch ringsumher", "tokens": ["Im", "Pol\u00b7ka\u00b7takt", "!", "Doch", "rings\u00b7um\u00b7her"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Einer hat ein Arg daran.", "tokens": ["Nicht", "Ei\u00b7ner", "hat", "ein", "Arg", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gern fingen sie zu tanzen an,", "tokens": ["Gern", "fin\u00b7gen", "sie", "zu", "tan\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's in der Kir