ct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Luft noch nicht zu wild durchschwirrt,", "tokens": ["Die", "Luft", "noch", "nicht", "zu", "wild", "durch\u00b7schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur hier und dort ein K\u00e4fer irrt;", "tokens": ["Nur", "hier", "und", "dort", "ein", "K\u00e4\u00b7fer", "irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Grillchen kichert nur,", "tokens": ["Im", "Grill\u00b7chen", "ki\u00b7chert", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Im Vogel jauchzt Natur.", "tokens": ["Im", "Vo\u00b7gel", "jauchzt", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vor\u00fcber schwebt ein geist'ger Duft,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "schwebt", "ein", "geist'\u00b7ger", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Aether durch den Dampf der Luft!", "tokens": ["Ein", "A\u00b7e\u00b7ther", "durch", "den", "Dampf", "der", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist's Engelsodem? Nein!", "tokens": ["Ist's", "En\u00b7gel\u00b7so\u00b7dem", "?", "Nein", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es ist der bl\u00fchnde Wein!", "tokens": ["Es", "ist", "der", "bl\u00fchn\u00b7de", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Mensch, genie\u00dfe dieser Zeit,", "tokens": ["O", "Mensch", ",", "ge\u00b7nie\u00b7\u00dfe", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und athme sie, wie Ewigkeit;", "tokens": ["Und", "ath\u00b7me", "sie", ",", "wie", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leg' dich am Quell ins Heu,", "tokens": ["Leg'", "dich", "am", "Quell", "ins", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erbau' dein Traumgeb\u00e4u!", "tokens": ["Er\u00b7bau'", "dein", "Traum\u00b7ge\u00b7b\u00e4u", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Geschwind, eh dich ein Tropfen weckt,", "tokens": ["Ge\u00b7schwind", ",", "eh", "dich", "ein", "Trop\u00b7fen", "weckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "pu