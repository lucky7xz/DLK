{"dta.poem.7179": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollte gar von solchen Erden", "tokens": ["Soll\u00b7te", "gar", "von", "sol\u00b7chen", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch der allerkleinste Staub", "tokens": ["Auch", "der", "al\u00b7ler\u00b7kleins\u00b7te", "Staub"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu viel tausend Nullen werden,", "tokens": ["Zu", "viel", "tau\u00b7send", "Nul\u00b7len", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu Zahlen jedes Laub;", "tokens": ["Und", "zu", "Zah\u00b7len", "je\u00b7des", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueberstiege doch vielmahlen", "tokens": ["Ue\u00b7bers\u00b7tie\u00b7ge", "doch", "viel\u00b7mah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "GoTTES Gr\u00f6sse diese Zahlen,", "tokens": ["GoT\u00b7TES", "Gr\u00f6s\u00b7se", "die\u00b7se", "Zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja selbst dieser Zahlen Heer,", "tokens": ["Ja", "selbst", "die\u00b7ser", "Zah\u00b7len", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn\u2019s in sich vermehret w\u00e4r\u2019.", "tokens": ["Wenn's", "in", "sich", "ver\u00b7meh\u00b7ret", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}