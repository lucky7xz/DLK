{"textgrid.poem.66509": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit der erste Liederton", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit der erste Liederton", "tokens": ["Seit", "der", "ers\u00b7te", "Lie\u00b7der\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klang in unsrem Kreise,", "tokens": ["Klang", "in", "uns\u00b7rem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat die Zeit vollendet schon", "tokens": ["Hat", "die", "Zeit", "voll\u00b7en\u00b7det", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manche Jahresreise;", "tokens": ["Man\u00b7che", "Jah\u00b7res\u00b7rei\u00b7se", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hat uns durch die Welt zerstreut,", "tokens": ["Hat", "uns", "durch", "die", "Welt", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und uns neu verbunden,", "tokens": ["Und", "uns", "neu", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und wir f\u00fchlen uns noch heut", "tokens": ["Und", "wir", "f\u00fch\u00b7len", "uns", "noch", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie in alten Stunden.", "tokens": ["Wie", "in", "al\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ist an W\u00fcnschen, hochgeschwellt,", "tokens": ["Ist", "an", "W\u00fcn\u00b7schen", ",", "hoch\u00b7ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manches auch gescheitert,", "tokens": ["Man\u00b7ches", "auch", "ge\u00b7schei\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchlt ein Jeder seine Welt", "tokens": ["F\u00fchlt", "ein", "Je\u00b7der", "sei\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Innen doch erweitert.", "tokens": ["In\u00b7nen", "doch", "er\u00b7wei\u00b7tert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und in ungeschw\u00e4chter Lust", "tokens": ["Und", "in", "un\u00b7ge\u00b7schw\u00e4ch\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu erh\u00f6htem Schaffen", "tokens": ["Zu", "er\u00b7h\u00f6h\u00b7tem", "Schaf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "R\u00fchren tapfer und bewu\u00dft", "tokens": ["R\u00fch\u00b7ren", "tap\u00b7fer", "und", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich die Geisteswaffen.", "tokens": ["Sich", "die", "Geis\u00b7tes\u00b7waf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Eine einz'ge Sonne scheint", "tokens": ["Ei\u00b7ne", "einz'\u00b7ge", "Son\u00b7ne", "scheint"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Stra\u00dfen viele,", "tokens": ["Auf", "der", "Stra\u00b7\u00dfen", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und das Fremdeste vereint", "tokens": ["Und", "das", "Frem\u00b7des\u00b7te", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in Einem Ziele.", "tokens": ["Sich", "in", "Ei\u00b7nem", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mag Jedweder seines Flugs", "tokens": ["Mag", "Jed\u00b7we\u00b7der", "sei\u00b7nes", "Flugs"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Letzten Kreis durchschw\u00e4rmen,", "tokens": ["Letz\u00b7ten", "Kreis", "durch\u00b7schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jeder Lichtglanz seines Zugs", "tokens": ["Je\u00b7der", "Licht\u00b7glanz", "sei\u00b7nes", "Zugs"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird uns All' erw\u00e4rmen.", "tokens": ["Wird", "uns", "All'", "er\u00b7w\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aus dem Tempel aber mit", "tokens": ["Aus", "dem", "Tem\u00b7pel", "a\u00b7ber", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmeichlern oder Lobern!", "tokens": ["Schmeich\u00b7lern", "o\u00b7der", "Lo\u00b7bern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wahrheit lehre Schritt f\u00fcr Schritt", "tokens": ["Wahr\u00b7heit", "leh\u00b7re", "Schritt", "f\u00fcr", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns die Bahn erobern!", "tokens": ["Uns", "die", "Bahn", "er\u00b7o\u00b7bern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wahrheit schafft am Dornenstrauch", "tokens": ["Wahr\u00b7heit", "schafft", "am", "Dor\u00b7nen\u00b7strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rosen, uns zu kr\u00f6nen,", "tokens": ["Ro\u00b7sen", ",", "uns", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn am Ziel der Opferrauch", "tokens": ["Wenn", "am", "Ziel", "der", "Op\u00b7fer\u00b7rauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Steigt im Reich des Sch\u00f6nen.", "tokens": ["Steigt", "im", "Reich", "des", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Doch wie nur der Sonnenschein", "tokens": ["Doch", "wie", "nur", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bringt die vollste Bl\u00fcthe,", "tokens": ["Bringt", "die", "volls\u00b7te", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will gehegt das Sch\u00f6ne sein", "tokens": ["Will", "ge\u00b7hegt", "das", "Sch\u00f6\u00b7ne", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVPP", "ART", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Frauen G\u00fcte.", "tokens": ["Durch", "der", "Frau\u00b7en", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und so wirkt, der Br\u00fcder Kreis", "tokens": ["Und", "so", "wirkt", ",", "der", "Br\u00fc\u00b7der", "Kreis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Innig zu verfestern,", "tokens": ["In\u00b7nig", "zu", "ver\u00b7fes\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "und zu lohnen M\u00fch' und Flei\u00df,", "tokens": ["und", "zu", "loh\u00b7nen", "M\u00fch'", "und", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jetzt die Schaar der Schwestern.", "tokens": ["Jetzt", "die", "Schaar", "der", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Preis und jedes Dankes Zoll", "tokens": ["Preis", "und", "je\u00b7des", "Dan\u00b7kes", "Zoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihnen, die das Streben", "tokens": ["Ih\u00b7nen", ",", "die", "das", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unsres Bundes anmutsvoll", "tokens": ["Uns\u00b7res", "Bun\u00b7des", "an\u00b7muts\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weihen und erheben!", "tokens": ["Wei\u00b7hen", "und", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn in heitrer Stunden Flucht", "tokens": ["Wenn", "in", "hei\u00b7trer", "Stun\u00b7den", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freuden ihnen lachten,", "tokens": ["Freu\u00b7den", "ih\u00b7nen", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ist's allein des Segens Frucht,", "tokens": ["Ist's", "al\u00b7lein", "des", "Se\u00b7gens", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den sie selbst uns brachten.", "tokens": ["Den", "sie", "selbst", "uns", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und so sehn wir mit der Zeit", "tokens": ["Und", "so", "sehn", "wir", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Enger uns verflochten,", "tokens": ["En\u00b7ger", "uns", "ver\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als in Tagen, scherzgeweiht,", "tokens": ["Als", "in", "Ta\u00b7gen", ",", "scherz\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir es ahnen mochten.", "tokens": ["Wir", "es", "ah\u00b7nen", "moch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lebe drum der gute Geist,", "tokens": ["Le\u00b7be", "drum", "der", "gu\u00b7te", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der bis diese Stunde", "tokens": ["Der", "bis", "die\u00b7se", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Frisch und kr\u00e4ftig sich erweist", "tokens": ["Frisch", "und", "kr\u00e4f\u00b7tig", "sich", "er\u00b7weist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsrem Freundesbunde!", "tokens": ["Uns\u00b7rem", "Freun\u00b7des\u00b7bun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Leb' auch dieser gute Tag,", "tokens": ["Leb'", "auch", "die\u00b7ser", "gu\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der im Jahreskreise", "tokens": ["Der", "im", "Jah\u00b7res\u00b7krei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Immer neu uns gr\u00fc\u00dfen mag", "tokens": ["Im\u00b7mer", "neu", "uns", "gr\u00fc\u00b7\u00dfen", "mag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der alten Weise!", "tokens": ["In", "der", "al\u00b7ten", "Wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und zuletzt? Kein Wie und Wo?", "tokens": ["Und", "zu\u00b7letzt", "?", "Kein", "Wie", "und", "Wo", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PIAT", "KOKOM", "KON", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt, das Glas zu heben!", "tokens": ["Kommt", ",", "das", "Glas", "zu", "he\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Br\u00fcder, Schwestern, la\u00dft uns froh,", "tokens": ["Br\u00fc\u00b7der", ",", "Schwes\u00b7tern", ",", "la\u00dft", "uns", "froh", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVIMP", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00dft uns Alle leben!", "tokens": ["La\u00dft", "uns", "Al\u00b7le", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Seit der erste Liederton", "tokens": ["Seit", "der", "ers\u00b7te", "Lie\u00b7der\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klang in unsrem Kreise,", "tokens": ["Klang", "in"