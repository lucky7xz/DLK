{"dta.poem.2549": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00fcnschte Nacht/ wir trauen sie dir an/", "tokens": ["Ge\u00b7w\u00fcnschte", "Nacht", "/", "wir", "trau\u00b7en", "sie", "dir", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So bald das Tage Liecht", "tokens": ["So", "bald", "das", "Ta\u00b7ge", "Liecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur durch die Wolcken sticht/", "tokens": ["Nur", "durch", "die", "Wol\u00b7cken", "sticht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So sind wir da/ und ist ihr was gethan", "tokens": ["So", "sind", "wir", "da", "/", "und", "ist", "ihr", "was", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$(", "KON", "VAFIN", "PPER", "PIS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An ihrer Sch\u00f6nheit Pracht", "tokens": ["An", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wir sagen kuhnlich aus; die Nacht hat di\u00df gemacht.", "tokens": ["Wir", "sa\u00b7gen", "kuhn\u00b7lich", "aus", ";", "die", "Nacht", "hat", "di\u00df", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}