{"dta.poem.20667": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den regierenden Reichsgrafen  \n von  \n  Stollberg-Wernigerode  \n \u00fcber  \n die Freude, Seinen einzigen Enkel gl\u00fccklich verm\u00e4hlt  \n zu sehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, den ich lange schon verehrte                 ", "tokens": ["Du", ",", "den", "ich", "lan\u00b7ge", "schon", "ver\u00b7ehr\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sohne, der Dir \u00e4hnlich sieht,", "tokens": ["Im", "Soh\u00b7ne", ",", "der", "Dir", "\u00e4hn\u00b7lich", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hundertmal mein Lied mit Wohlgefallen h\u00f6rte,", "tokens": ["Und", "hun\u00b7dert\u00b7mal", "mein", "Lied", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vernimm jezt auch mein Lied.", "tokens": ["Ver\u00b7nimm", "jezt", "auch", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Graf, der in aufgekl\u00e4rten Mienen", "tokens": ["Graf", ",", "der", "in", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Mie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dem Patriarchen gleichend ist,", "tokens": ["Dem", "Pat\u00b7ri\u00b7ar\u00b7chen", "glei\u00b7chend", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dessen Angesicht der Herr Dein Gott erschienen,", "tokens": ["Vor", "des\u00b7sen", "An\u00b7ge\u00b7sicht", "der", "Herr", "Dein", "Gott", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Du gewidmet bist.", "tokens": ["Dem", "Du", "ge\u00b7wid\u00b7met", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du liebensw\u00fcrdiger Gebieter", "tokens": ["Du", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7ger", "Ge\u00b7bie\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volkes, das Dich \u201eVater\u201e gr\u00fc\u00dft,", "tokens": ["Des", "Vol\u00b7kes", ",", "das", "Dich", "\u201e", "Va\u00b7ter", "\u201e", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$(", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Du begl\u00fcckter Greis, der dieses Lebens G\u00fcter", "tokens": ["O", "Du", "be\u00b7gl\u00fcck\u00b7ter", "Greis", ",", "der", "die\u00b7ses", "Le\u00b7bens", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "PRELS", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im hohen Grad genie\u00dft.", "tokens": ["Im", "ho\u00b7hen", "Grad", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was mu\u00df Dein Herz f\u00fcr Freude schmecken!", "tokens": ["Was", "mu\u00df", "Dein", "Herz", "f\u00fcr", "Freu\u00b7de", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So froh war vormals Abraham,", "tokens": ["So", "froh", "war", "vor\u00b7mals", "Ab\u00b7ra\u00b7ham", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als seiner Sarah Sohn vertraulich mit Rebeccen", "tokens": ["Als", "sei\u00b7ner", "Sa\u00b7rah", "Sohn", "ver\u00b7trau\u00b7lich", "mit", "Re\u00b7be\u00b7ccen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In ihre H\u00fctte kam.", "tokens": ["In", "ih\u00b7re", "H\u00fct\u00b7te", "kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So froh war Jacob einst in Gosen,", "tokens": ["So", "froh", "war", "Ja\u00b7cob", "einst", "in", "Go\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NE", "ADV", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wenn Joseph kam, und neben ihn,", "tokens": ["Wenn", "Jo\u00b7se\u00b7ph", "kam", ",", "und", "ne\u00b7ben", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,", "KON", "APPR", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Asnath sein Gemahl, den Alten liebzukosen,", "tokens": ["Die", "As\u00b7nath", "sein", "Ge\u00b7mahl", ",", "den", "Al\u00b7ten", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ganz entz\u00fccket schien.", "tokens": ["Der", "ganz", "ent\u00b7z\u00fc\u00b7cket", "schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein j\u00fcngster lieber Stammerhalter,", "tokens": ["Dein", "j\u00fcngs\u00b7ter", "lie\u00b7ber", "Stam\u00b7mer\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Enkel ", "tokens": ["Dein", "En\u00b7kel"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der tausend J\u00fcnglinge von einem gleichen Alter,", "tokens": ["Der", "tau\u00b7send", "J\u00fcng\u00b7lin\u00b7ge", "von", "ei\u00b7nem", "glei\u00b7chen", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An Weisheit \u00fcbertraf;", "tokens": ["An", "Weis\u00b7heit", "\u00fc\u00b7bert\u00b7raf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bringt seine sch\u00f6ne Neuverm\u00e4hlte,", "tokens": ["Bringt", "sei\u00b7ne", "sch\u00f6\u00b7ne", "Neu\u00b7ver\u00b7m\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allen Reizungen geziert,", "tokens": ["Mit", "al\u00b7len", "Rei\u00b7zun\u00b7gen", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und mit mehr Tugenden, als sie schon Jahre z\u00e4hlte,", "tokens": ["Und", "mit", "mehr", "Tu\u00b7gen\u00b7den", ",", "als", "sie", "schon", "Jah\u00b7re", "z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An Seinen Arm gef\u00fchrt.", "tokens": ["An", "Sei\u00b7nen", "Arm", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jezt schl\u00e4gt Dein Herzenspuls geschwinder,", "tokens": ["Jezt", "schl\u00e4gt", "Dein", "Her\u00b7zens\u00b7puls", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt l\u00e4chelst Du Sie liebreich an,", "tokens": ["Jezt", "l\u00e4\u00b7chelst", "Du", "Sie", "lieb\u00b7reich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprichst: \u201ekommt n\u00e4her, kommt in meinen Arm,", "tokens": ["Und", "sprichst", ":", "\u201e", "kommt", "n\u00e4\u00b7her", ",", "kommt", "in", "mei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "ADJD", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Kinder,", "tokens": ["Ihr", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "\u201eda\u00df ich Euch seegnen kann.", "tokens": ["\u201e", "da\u00df", "ich", "Euch", "seeg\u00b7nen", "kann", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201ekomm her, Du Dritte meiner T\u00f6chter,", "tokens": ["\u201e", "komm", "her", ",", "Du", "Drit\u00b7te", "mei\u00b7ner", "T\u00f6ch\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu, wie des Fr\u00fchlingsblume, sch\u00f6n,", "tokens": ["\u201e", "du", ",", "wie", "des", "Fr\u00fch\u00b7lings\u00b7blu\u00b7me", ",", "sch\u00f6n", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewachs in viel Tausende, bis Thronen und Ge-", "tokens": ["\u201e", "wachs", "in", "viel", "Tau\u00b7sen\u00b7de", ",", "bis", "Thro\u00b7nen", "und", "Ge"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PIS", "APPR", "PIAT", "NN", "$,", "KOUS", "NN", "KON", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "schlechter", "tokens": ["schlech\u00b7ter"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "\u201eauf Erden untergehn.", "tokens": ["\u201e", "auf", "Er\u00b7den", "un\u00b7ter\u00b7gehn", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u201eund Du, mein Sohn, der Du den Namen", "tokens": ["\u201e", "und", "Du", ",", "mein", "Sohn", ",", "der", "Du", "den", "Na\u00b7men"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edes Gottes Deiner V\u00e4ter ehrst:", "tokens": ["\u201e", "des", "Got\u00b7tes", "Dei\u00b7ner", "V\u00e4\u00b7ter", "ehrst", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eder Herr beh\u00fcte Dich, damit Du Deinem Saamen", "tokens": ["\u201e", "der", "Herr", "be\u00b7h\u00fc\u00b7te", "Dich", ",", "da\u00b7mit", "Du", "Dei\u00b7nem", "Saa\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eeinst seine Wege lehrst.\u201c", "tokens": ["\u201e", "einst", "sei\u00b7ne", "We\u00b7ge", "lehrst", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}