{"dta.poem.12085": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df am Baum und schrieb, und weil ich stille war,", "tokens": ["Ich", "sa\u00df", "am", "Baum", "und", "schrieb", ",", "und", "weil", "ich", "stil\u00b7le", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$,", "KON", "KOUS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wagte sich scheu heran ein Thierlein hie und dar.", "tokens": ["Wag\u00b7te", "sich", "scheu", "he\u00b7ran", "ein", "Thier\u00b7lein", "hie", "und", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "ART", "NN", "ADV", "KON", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Vorsichtig sp\u00e4hend schlich ein Eichhorn \u00fcber'n Zaun;", "tokens": ["Vor\u00b7sich\u00b7tig", "sp\u00e4\u00b7hend", "schlich", "ein", "Eich\u00b7horn", "\u00fc\u00b7ber'n", "Zaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Als ich die Hand erhob, wich es zur\u00fcck mit Graun.", "tokens": ["Als", "ich", "die", "Hand", "er\u00b7hob", ",", "wich", "es", "zu\u00b7r\u00fcck", "mit", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein V\u00f6glein wiegte sich hoch im Gezweig und sang;", "tokens": ["Ein", "V\u00f6\u00b7glein", "wieg\u00b7te", "sich", "hoch", "im", "Ge\u00b7zweig", "und", "sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ich das Haupt erhob, entflatterte es bang.", "tokens": ["Als", "ich", "das", "Haupt", "er\u00b7hob", ",", "ent\u00b7flat\u00b7ter\u00b7te", "es", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Schl\u00e4ngchen schl\u00e4ngelte durch Gras und Gries herbei;", "tokens": ["Ein", "Schl\u00e4ng\u00b7chen", "schl\u00e4n\u00b7gel\u00b7te", "durch", "Gras", "und", "Gries", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich hob den Fu\u00df, es floh alsob ich giftig sei.", "tokens": ["Ich", "hob", "den", "Fu\u00df", ",", "es", "floh", "al\u00b7sob", "ich", "gif\u00b7tig", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "VVFIN", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O Mensch, Herr der Natur und Schreck, Tyrann unhuldig", "tokens": ["O", "Mensch", ",", "Herr", "der", "Na\u00b7tur", "und", "Schreck", ",", "Ty\u00b7rann", "un\u00b7hul\u00b7dig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "KON", "NN", "$,", "NN", "ADJD"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Unschuld'ger Kreatur, du selber nicht unschuldig!", "tokens": ["Un\u00b7schuld'\u00b7ger", "Kre\u00b7a\u00b7tur", ",", "du", "sel\u00b7ber", "nicht", "un\u00b7schul\u00b7dig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}