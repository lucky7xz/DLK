{"textgrid.poem.36281": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Liebe lebend streb' und bilde Werke,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Liebe lebend streb' und bilde Werke,", "tokens": ["In", "Lie\u00b7be", "le\u00b7bend", "streb'", "und", "bil\u00b7de", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verkl\u00e4r' im Farbenglanz geliebte Leiden,", "tokens": ["Ver\u00b7kl\u00e4r'", "im", "Far\u00b7ben\u00b7glanz", "ge\u00b7lieb\u00b7te", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mal' in Liedern, die kein Licht beneiden,", "tokens": ["Und", "mal'", "in", "Lie\u00b7dern", ",", "die", "kein", "Licht", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Feuers Sch\u00f6nheit, das dich ewig st\u00e4rke.", "tokens": ["Des", "Feu\u00b7ers", "Sch\u00f6n\u00b7heit", ",", "das", "dich", "e\u00b7wig", "st\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun wisse, da\u00df ich mich verschwinden merke.", "tokens": ["Nun", "wis\u00b7se", ",", "da\u00df