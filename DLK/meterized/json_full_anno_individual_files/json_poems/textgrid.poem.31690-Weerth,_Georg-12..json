{"textgrid.poem.31690": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich wie mein Leben,", "tokens": ["Ich", "lie\u00b7be", "dich", "wie", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich liebe dich alsosehr;", "tokens": ["Ich", "lie\u00b7be", "dich", "al\u00b7so\u00b7sehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich m\u00f6cht einen Ring dir geben", "tokens": ["Ich", "m\u00f6cht", "ei\u00b7nen", "Ring", "dir", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Golde und Steinen schwer.", "tokens": ["Von", "Gol\u00b7de", "und", "Stei\u00b7nen", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6cht alle Blumen pfl\u00fccken", "tokens": ["Ich", "m\u00f6cht", "al\u00b7le", "Blu\u00b7men", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf schimmernden Bergesh\u00f6hn,", "tokens": ["Auf", "schim\u00b7mern\u00b7den", "Ber\u00b7ges\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Deine liebe Stirne zu schm\u00fccken", "tokens": ["Dei\u00b7ne", "lie\u00b7be", "Stir\u00b7ne", "zu", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und deine Locken sch\u00f6n.", "tokens": ["Und", "dei\u00b7ne", "Lo\u00b7cken", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6cht alle V\u00f6gel bringen", "tokens": ["Ich", "m\u00f6cht", "al\u00b7le", "V\u00f6\u00b7gel", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus Wiese, aus Berg und Wald:", "tokens": ["Aus", "Wie\u00b7se", ",", "aus", "Berg", "und", "Wald", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die sollten mir klingen und singen", "tokens": ["Die", "soll\u00b7ten", "mir", "klin\u00b7gen", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von deiner sch\u00f6nen Gestalt!", "tokens": ["Von", "dei\u00b7ner", "sch\u00f6\u00b7nen", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ich liebe dich wie mein Leben,", "tokens": ["Ich", "lie