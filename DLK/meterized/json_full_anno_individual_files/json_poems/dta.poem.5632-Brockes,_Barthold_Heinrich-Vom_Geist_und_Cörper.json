{"dta.poem.5632": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der so geheime Grund, der mir mein Wesen weist,", "tokens": ["Der", "so", "ge\u00b7hei\u00b7me", "Grund", ",", "der", "mir", "mein", "We\u00b7sen", "weist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem er sich von allen Dingen trennet,", "tokens": ["In\u00b7dem", "er", "sich", "von", "al\u00b7len", "Din\u00b7gen", "tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird alsobald erkennet:", "tokens": ["Wird", "al\u00b7so\u00b7bald", "er\u00b7ken\u00b7net", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dieses Wesen, welches dencket,", "tokens": ["Und", "die\u00b7ses", "We\u00b7sen", ",", "wel\u00b7ches", "den\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und auf sich selbst sich wieder r\u00fcckwerts lencket,", "tokens": ["Und", "auf", "sich", "selbst", "sich", "wie\u00b7der", "r\u00fcck\u00b7werts", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist, was wir nennen, ", "tokens": ["Ist", ",", "was", "wir", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}}}}