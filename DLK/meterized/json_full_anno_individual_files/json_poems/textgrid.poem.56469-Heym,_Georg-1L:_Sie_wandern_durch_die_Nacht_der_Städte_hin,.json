{"textgrid.poem.56469": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie wandern durch die Nacht der St\u00e4dte hin,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wandern durch die Nacht der St\u00e4dte hin,", "tokens": ["Sie", "wan\u00b7dern", "durch", "die", "Nacht", "der", "St\u00e4d\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schwarz sich ducken unter ihrem Fu\u00df.", "tokens": ["Die", "schwarz", "sich", "du\u00b7cken", "un\u00b7ter", "ih\u00b7rem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schifferb\u00e4rte stehen um ihr Kinn", "tokens": ["Wie", "Schif\u00b7fer\u00b7b\u00e4r\u00b7te", "ste\u00b7hen", "um", "ihr", "Kinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wolken schwarz vom Rauch und Kohlenru\u00df.", "tokens": ["Die", "Wol\u00b7ken", "schwarz", "vom", "Rauch", "und", "Koh\u00b7len\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr langer Schatten schwankt im H\u00e4usermeer", "tokens": ["Ihr", "lan\u00b7ger", "Schat\u00b7ten", "schwankt", "im", "H\u00e4u\u00b7ser\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00f6scht der Stra\u00dfen Lichterreihen aus.", "tokens": ["Und", "l\u00f6scht", "der", "Stra\u00b7\u00dfen", "Lich\u00b7ter\u00b7rei\u00b7hen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er kriecht wie Nebel auf dem Pflaster schwer", "tokens": ["Er", "kriecht", "wie", "Ne\u00b7bel", "auf", "dem", "Pflas\u00b7ter", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tastet langsam vorw\u00e4rts Haus f\u00fcr Haus.", "tokens": ["Und", "tas\u00b7tet", "lang\u00b7sam", "vor\u00b7w\u00e4rts", "Haus", "f\u00fcr", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den einen Fu\u00df auf einen Platz gestellt,", "tokens": ["Den", "ei\u00b7nen", "Fu\u00df", "auf", "ei\u00b7nen", "Platz", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den anderen gekniet auf einen Turm,", "tokens": ["Den", "an\u00b7de\u00b7ren", "ge\u00b7kniet", "auf", "ei\u00b7nen", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ragen sie auf, wo schwarz der Regen f\u00e4llt,", "tokens": ["Ra\u00b7gen", "sie", "auf", ",", "wo", "schwarz", "der", "Re\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Panspfeifen blasend in den Wolkensturm.", "tokens": ["Pan\u00b7spfei\u00b7fen", "bla\u00b7send", "in", "den", "Wol\u00b7ken\u00b7sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Um ihre F\u00fc\u00dfe kreist das Ritornell", "tokens": ["Um", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", "kreist", "das", "Ri\u00b7tor\u00b7nell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des St\u00e4dtemeers mit trauriger Musik,", "tokens": ["Des", "St\u00e4d\u00b7te\u00b7meers", "mit", "trau\u00b7ri\u00b7ger", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein gro\u00dfes Sterbelied. Bald dumpf, bald grell", "tokens": ["Ein", "gro\u00b7\u00dfes", "Ster\u00b7be\u00b7lied", ".", "Bald", "dumpf", ",", "bald", "grell"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wechselt der Ton, der in das Dunkel stieg.", "tokens": ["Wech\u00b7selt", "der", "Ton", ",", "der", "in", "das", "Dun\u00b7kel", "stieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Sie wandern an dem Strom, der schwarz und breit", "tokens": ["Sie", "wan\u00b7dern", "an", "dem", "Strom", ",", "der", "schwarz", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ein Reptil, den R\u00fccken gelb gefleckt", "tokens": ["Wie", "ein", "Rep\u00b7til", ",", "den", "R\u00fc\u00b7cken", "gelb", "ge\u00b7fleckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von den Laternen, in die Dunkelheit", "tokens": ["Von", "den", "La\u00b7ter\u00b7nen", ",", "in", "die", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich traurig w\u00e4lzt, die schwarz den Himmel deckt.", "tokens": ["Sich", "trau\u00b7rig", "w\u00e4lzt", ",", "die", "schwarz", "den", "Him\u00b7mel", "deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie lehnen schwer auf einer Br\u00fcckenwand", "tokens": ["Sie", "leh\u00b7nen", "schwer", "auf", "ei\u00b7ner", "Br\u00fc\u00b7cken\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und stecken ihre H\u00e4nde in den Schwarm", "tokens": ["Und", "ste\u00b7cken", "ih\u00b7re", "H\u00e4n\u00b7de", "in", "den", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschen aus, wie Faune, die am Rand", "tokens": ["Der", "Men\u00b7schen", "aus", ",", "wie", "Fau\u00b7ne", ",", "die", "am", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PWAV", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der S\u00fcmpfe bohren in den Schlamm den Arm.", "tokens": ["Der", "S\u00fcmp\u00b7fe", "boh\u00b7ren", "in", "den", "Schlamm", "den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Einer steht auf. Dem wei\u00dfen Monde h\u00e4ngt", "tokens": ["Ei\u00b7ner", "steht", "auf", ".", "Dem", "wei\u00b7\u00dfen", "Mon\u00b7de", "h\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Er eine schwarze Larve vor. Die Nacht,", "tokens": ["Er", "ei\u00b7ne", "schwar\u00b7ze", "Lar\u00b7ve", "vor", ".", "Die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die sich wie Blei vom finstern Himmel senkt,", "tokens": ["Die", "sich", "wie", "Blei", "vom", "fins\u00b7tern", "Him\u00b7mel", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOKOM", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dr\u00fcckt tief die H\u00e4user in des Dunkels Schacht.", "tokens": ["Dr\u00fcckt", "tief", "die", "H\u00e4u\u00b7ser", "in", "des", "Dun\u00b7kels", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der St\u00e4dte Schultern knacken. Und es birst", "tokens": ["Der", "St\u00e4d\u00b7te", "Schul\u00b7tern", "kna\u00b7cken", ".", "Und", "es", "birst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "$.", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Dach, daraus ein rotes Feuer schwemmt.", "tokens": ["Ein", "Dach", ",", "da\u00b7raus", "ein", "ro\u00b7tes", "Feu\u00b7er", "schwemmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Breitbeinig sitzen sie auf seinem First", "tokens": ["Breit\u00b7bei\u00b7nig", "sit\u00b7zen", "sie", "auf", "sei\u00b7nem", "First"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und schrein wie Katzen auf zum Firmament.", "tokens": ["Und", "schrein", "wie", "Kat\u00b7zen", "auf", "zum", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "In einer Stube voll von Finsternissen", "tokens": ["In", "ei\u00b7ner", "Stu\u00b7be", "voll", "von", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schreit eine W\u00f6chnerin in ihren Wehn.", "tokens": ["Schreit", "ei\u00b7ne", "W\u00f6ch\u00b7ne\u00b7rin", "in", "ih\u00b7ren", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr starker Leib ragt riesig aus den Kissen,", "tokens": ["Ihr", "star\u00b7ker", "Leib", "ragt", "rie\u00b7sig", "aus", "den", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den herum die gro\u00dfen Teufel stehn.", "tokens": ["Um", "den", "he\u00b7rum", "die", "gro\u00b7\u00dfen", "Teu\u00b7fel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "APZR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Sie h\u00e4lt sich zitternd an der Wehebank.", "tokens": ["Sie", "h\u00e4lt", "sich", "zit\u00b7ternd", "an", "der", "We\u00b7he\u00b7bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Zimmer schwankt um sie von ihrem Schrei,", "tokens": ["Das", "Zim\u00b7mer", "schwankt", "um", "sie", "von", "ih\u00b7rem", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da kommt die Frucht. Ihr Scho\u00df klafft rot und lang", "tokens": ["Da", "kommt", "die", "Frucht", ".", "Ihr", "Scho\u00df", "klafft", "rot", "und", "lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und blutend rei\u00dft er von der Frucht entzwei.", "tokens": ["Und", "blu\u00b7tend", "rei\u00dft", "er", "von", "der", "Frucht", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Teufel H\u00e4lse wachsen wie Giraffen.", "tokens": ["Der", "Teu\u00b7fel", "H\u00e4l\u00b7se", "wach\u00b7sen", "wie", "Gir\u00b7af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Kind hat keinen Kopf. Die Mutter h\u00e4lt", "tokens": ["Das", "Kind", "hat", "kei\u00b7nen", "Kopf", ".", "Die", "Mut\u00b7ter", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es vor sich hin. In ihrem R\u00fccken klaffen", "tokens": ["Es", "vor", "sich", "hin", ".", "In", "ih\u00b7rem", "R\u00fc\u00b7cken", "klaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PRF", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Schrecks Froschfinger, wenn sie r\u00fcckw\u00e4rts f\u00e4llt.", "tokens": ["Des", "Schrecks", "Froschfin\u00b7ger", ",", "wenn", "sie", "r\u00fcck\u00b7w\u00e4rts", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Doch die D\u00e4monen wachsen riesengro\u00df.", "tokens": ["Doch", "die", "D\u00e4\u00b7mo\u00b7nen", "wach\u00b7sen", "rie\u00b7sen\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Schl\u00e4fenhorn zerrei\u00dft den Himmel rot.", "tokens": ["Ihr", "Schl\u00e4\u00b7fen\u00b7horn", "zer\u00b7rei\u00dft", "den", "Him\u00b7mel", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erdbeben donnert durch der St\u00e4dte Scho\u00df", "tokens": ["Erd\u00b7be\u00b7ben", "don\u00b7nert", "durch", "der", "St\u00e4d\u00b7te", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Um ihren Huf, den Feuer \u00fcberloht.", "tokens": ["Um", "ih\u00b7ren", "Huf", ",", "den", "Feu\