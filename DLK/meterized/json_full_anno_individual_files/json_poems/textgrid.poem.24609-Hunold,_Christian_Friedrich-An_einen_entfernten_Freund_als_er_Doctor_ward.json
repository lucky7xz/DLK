{"textgrid.poem.24609": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen entfernten Freund/ als er Doctor ward", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelehrte Wissenschafft/ und andre kluge Lehren.", "tokens": ["Ge\u00b7lehr\u00b7te", "Wis\u00b7sen\u00b7schafft", "/", "und", "and\u00b7re", "klu\u00b7ge", "Leh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Stra\u00dfburg sah es an/ und aus zu Friedenheit/", "tokens": ["Dein", "Stra\u00df\u00b7burg", "sah", "es", "an", "/", "und", "aus", "zu", "Frie\u00b7den\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "APPR", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wolt es den Wehrten Sohn nach seinen W\u00fcrden ehren.", "tokens": ["Wolt", "es", "den", "Wehr\u00b7ten", "Sohn", "nach", "sei\u00b7nen", "W\u00fcr\u00b7den", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heut ist der erste Tag/ da dich dein Vaterland/", "tokens": ["Heut", "ist", "der", "ers\u00b7te", "Tag", "/", "da", "dich", "dein", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER