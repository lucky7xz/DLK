{"dta.poem.13262": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier schwieg die Fr\u00f6mmigkeit; so schien die K\u00f6niginn", "tokens": ["Hier", "schwieg", "die", "Fr\u00f6m\u00b7mig\u00b7keit", ";", "so", "schien", "die", "K\u00f6\u00b7ni\u00b7ginn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "390Als l\u00e4g ihr dieser Schein besonders in dem Sinn.", "tokens": ["l\u00e4g", "ihr", "die\u00b7ser", "Schein", "be\u00b7son\u00b7ders", "in", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Gleich fieng sie an das Herz, die Regung zu erwecken,", "tokens": ["Gleich", "fi\u00b7eng", "sie", "an", "das", "Herz", ",", "die", "Re\u00b7gung", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und was sie von dem Mann vermeine, zu entdecken.", "tokens": ["Und", "was", "sie", "von", "dem", "Mann", "ver\u00b7mei\u00b7ne", ",", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "PTKZU", "VVINF