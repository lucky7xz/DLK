{"dta.poem.14385": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und jeglicher hegt sich", "tokens": ["Und", "jeg\u00b7li\u00b7cher", "hegt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sein Liebchen im Arm,", "tokens": ["Sein", "Lieb\u00b7chen", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So wirrt und bewegt sich", "tokens": ["So", "wirrt", "und", "be\u00b7wegt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der selige Schwarm. \u2014", "tokens": ["Der", "se\u00b7li\u00b7ge", "Schwarm", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}