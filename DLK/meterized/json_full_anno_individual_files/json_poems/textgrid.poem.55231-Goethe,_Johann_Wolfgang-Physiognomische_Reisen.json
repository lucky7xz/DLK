{"textgrid.poem.55231": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Physiognomische Reisen", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollt es wahr sein, was uns der rohe Wandrer verk\u00fcndet,", "tokens": ["Sollt", "es", "wahr", "sein", ",", "was", "uns", "der", "ro\u00b7he", "Wand\u00b7rer", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df die Menschengestalt von allen sichtlichen Dingen", "tokens": ["Da\u00df", "die", "Men\u00b7schen\u00b7ge\u00b7stalt", "von", "al\u00b7len", "sicht\u00b7li\u00b7chen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ganz allein uns l\u00fcge, da\u00df wir, was edel und albern,", "tokens": ["Ganz", "al\u00b7lein", "uns", "l\u00fc\u00b7ge", ",", "da\u00df", "wir", ",", "was", "e\u00b7del", "und", "al\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was beschr\u00e4nkt und gro\u00df, im Angesichte zu suchen,", "tokens": ["Was", "be\u00b7schr\u00e4nkt", "und", "gro\u00df", ",", "im", "An\u00b7ge\u00b7sich\u00b7te", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "ADJD", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Eitele Toren sind, betrogne, betr\u00fcgende Toren?", "tokens": ["Ei\u00b7te\u00b7le", "To\u00b7ren", "sind", ",", "be\u00b7trog\u00b7ne", ",", "be\u00b7tr\u00fc\u00b7gen\u00b7de", "To\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Ach, wir sind auf den dunkelen Pfad des verworrenen Lebens", "tokens": ["Ach", ",", "wir", "sind", "auf", "den", "dun\u00b7ke\u00b7len", "Pfad", "des", "ver\u00b7wor\u00b7re\u00b7nen", "Le\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-----+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wieder zur\u00fcckgescheucht, der Schimmer zu N\u00e4chten verfinstert.", "tokens": ["Wie\u00b7der", "zu\u00b7r\u00fcck\u00b7ge\u00b7scheucht", ",", "der", "Schim\u00b7mer", "zu", "N\u00e4ch\u00b7ten", "ver\u00b7fins\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Hebet eure zweifelnden Stirnen empor, ihr Geliebten!", "tokens": ["He\u00b7bet", "eu\u00b7re", "zwei\u00b7feln\u00b7den", "Stir\u00b7nen", "em\u00b7por", ",", "ihr", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+--+--+--+-", 