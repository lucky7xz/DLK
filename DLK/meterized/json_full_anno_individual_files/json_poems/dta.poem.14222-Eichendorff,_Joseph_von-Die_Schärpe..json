{"dta.poem.14222": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00e4rpe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz, das ist ein kluges Kind,", "tokens": ["Mein", "Schatz", ",", "das", "ist", "ein", "klu\u00b7ges", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die spricht: \u201eWillst du nicht fechten:", "tokens": ["Die", "spricht", ":", "\u201e", "Willst", "du", "nicht", "fech\u00b7ten", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir zwei geschied'ne Leute sind,", "tokens": ["Wir", "zwei", "ge\u00b7schie\u00b7d'\u00b7ne", "Leu\u00b7te", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erschlagen dich die Schlechten:", "tokens": ["Er\u00b7schla\u00b7gen", "dich", "die", "Schlech\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch keins von beiden dran gewinnt.\u201c", "tokens": ["Auch", "keins", "von", "bei\u00b7den", "dran", "ge\u00b7winnt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "APPR", "PIAT", "PAV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Schatz, das ist ein kluges Kind,", "tokens": ["Mein", "Schatz", ",", "das", "ist", "ein", "klu\u00b7ges", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr die will ich ", "tokens": ["F\u00fcr", "die", "will", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "VMFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}}}}}