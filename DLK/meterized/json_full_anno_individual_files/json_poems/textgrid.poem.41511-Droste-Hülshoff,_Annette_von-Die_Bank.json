, "measure": "iambic.tetra"}, "line.8": {"text": "Was Liebes komme hergezogen!", "tokens": ["Was", "Lie\u00b7bes", "kom\u00b7me", "her\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Mit jedem Schritt wei\u00df er zu gehn,", "tokens": ["Mit", "je\u00b7dem", "Schritt", "wei\u00df", "er", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich anzuformen alle Z\u00fcge;", "tokens": ["Sich", "an\u00b7zu\u00b7for\u00b7men", "al\u00b7le", "Z\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mag er denn am Hange stehn,", "tokens": ["So", "mag", "er", "denn", "am", "Han\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wert Phantom, geliebte L\u00fcge;", "tokens": ["Ein", "wert", "Phan\u00b7tom", ",", "ge\u00b7lieb\u00b7te", "L\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich aber hoffe f\u00fcr und f\u00fcr,", "tokens": ["Ich", "a\u00b7ber", "hof\u00b7fe", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "S