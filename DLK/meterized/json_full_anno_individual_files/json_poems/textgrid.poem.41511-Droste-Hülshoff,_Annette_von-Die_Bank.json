{"textgrid.poem.41511": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Bank", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Parke wei\u00df ich eine Bank,", "tokens": ["Im", "Par\u00b7ke", "wei\u00df", "ich", "ei\u00b7ne", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schattenreichste nicht von allen,", "tokens": ["Die", "schat\u00b7ten\u00b7reichs\u00b7te", "nicht", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Erlen lassen, d\u00fcnn und schlank,", "tokens": ["Nur", "Er\u00b7len", "las\u00b7sen", ",", "d\u00fcnn", "und", "schlank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcber karge Streifen wallen;", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "kar\u00b7ge", "Strei\u00b7fen", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sitz' ich manchen Sommertag", "tokens": ["Da", "sitz'", "ich", "man\u00b7chen", "Som\u00b7mer\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und la\u00df mich r\u00f6sten von der Sonnen,", "tokens": ["Und", "la\u00df", "mich", "r\u00f6s\u00b7ten", "von", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Rings keiner Quelle Pl\u00e4tschern wach,", "tokens": ["Rings", "kei\u00b7ner", "Quel\u00b7le", "Pl\u00e4t\u00b7schern", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch mir im Herzen springt der Bronnen.", "tokens": ["Doch", "mir", "im", "Her\u00b7zen", "springt", "der", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies ist der Fleck, wo man den Weg", "tokens": ["Dies", "ist", "der", "Fleck", ",", "wo", "man", "den", "Weg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach allen Seiten kann bestreichen,", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", "kann", "be\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das staub'ge Gleis, den gr\u00fcnen Steg,", "tokens": ["Das", "staub'\u00b7ge", "Gleis", ",", "den", "gr\u00fc\u00b7nen", "Steg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dort die Lichtung in den Eichen:", "tokens": ["Und", "dort", "die", "Lich\u00b7tung", "in", "den", "Ei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach manche, manche liebe Spur", "tokens": ["Ach", "man\u00b7che", ",", "man\u00b7che", "lie\u00b7be", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PIS", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist unterm Rade aufgeflogen!", "tokens": ["Ist", "un\u00b7term", "Ra\u00b7de", "auf\u00b7ge\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was mich erfreut, bek\u00fcmmert, nur", "tokens": ["Was", "mich", "er\u00b7freut", ",", "be\u00b7k\u00fcm\u00b7mert", ",", "nur"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dr\u00fcben kam es hergezogen.", "tokens": ["Von", "dr\u00fc\u00b7ben", "kam", "es", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du frommer Greis im schlichten Kleid,", "tokens": ["Du", "from\u00b7mer", "Greis", "im", "schlich\u00b7ten", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getreuer Freund seit zwanzig Jahren,", "tokens": ["Ge\u00b7treu\u00b7er", "Freund", "seit", "zwan\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem keine Wege schlimm und weit,", "tokens": ["Dem", "kei\u00b7ne", "We\u00b7ge", "schlimm", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Galt es den heil'gen Dienst zu wahren,", "tokens": ["Galt", "es", "den", "heil'\u00b7gen", "Dienst", "zu", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wie oft sah ich den schweren Schlag", "tokens": ["Wie", "oft", "sah", "ich", "den", "schwe\u00b7ren", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich drehn mit ungeschickten H\u00e4nden,", "tokens": ["Dich", "drehn", "mit", "un\u00b7ge\u00b7schick\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und langsam steigend nach und nach", "tokens": ["Und", "lang\u00b7sam", "stei\u00b7gend", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein K\u00e4ppchen an des Dammes W\u00e4nden.", "tokens": ["Dein", "K\u00e4pp\u00b7chen", "an", "des", "Dam\u00b7mes", "W\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du in meines Herzens Grund,", "tokens": ["Und", "du", "in", "mei\u00b7nes", "Her\u00b7zens", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein lieber schlanker blonder Junge,", "tokens": ["Mein", "lie\u00b7ber", "schlan\u00b7ker", "blon\u00b7der", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deiner B\u00fcchs' und braunem Hund,", "tokens": ["Mit", "dei\u00b7ner", "B\u00fcchs'", "und", "brau\u00b7nem", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du klares Aug' und muntre Zunge,", "tokens": ["Du", "kla\u00b7res", "Aug'", "und", "mun\u00b7tre", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie oft h\u00f6rt' ich dein Pfeifen nah,", "tokens": ["Wie", "oft", "h\u00f6rt'", "ich", "dein", "Pfei\u00b7fen", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn zu der Dogge du gesprochen;", "tokens": ["Wenn", "zu", "der", "Dog\u00b7ge", "du", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein lieber Bruder warst du ja,", "tokens": ["Mein", "lie\u00b7ber", "Bru\u00b7der", "warst", "du", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie sollte mir das Herz nicht pochen?", "tokens": ["Wie", "soll\u00b7te", "mir", "das", "Herz", "nicht", "po\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und manches was die Zeit verweht,", "tokens": ["Und", "man\u00b7ches", "was", "die", "Zeit", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und manches was sie lie\u00df erkalten,", "tokens": ["Und", "man\u00b7ches", "was", "sie", "lie\u00df", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Banquos K\u00f6nigsreihe geht", "tokens": ["Wie", "Ban\u00b7quos", "K\u00f6\u00b7nigs\u00b7rei\u00b7he", "geht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trabt es aus des Waldes Spalten.", "tokens": ["Und", "trabt", "es", "aus", "des", "Wal\u00b7des", "Spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch was mir noch geblieben und", "tokens": ["Auch", "was", "mir", "noch", "ge\u00b7blie\u00b7ben", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PPER", "ADV", "VVPP", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was neu erbl\u00fcht im Lebensgarten,", "tokens": ["Was", "neu", "er\u00b7bl\u00fcht", "im", "Le\u00b7bens\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der werten Freunde heitrer Bund,", "tokens": ["Der", "wer\u00b7ten", "Freun\u00b7de", "hei\u00b7trer", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dr\u00fcben mu\u00df ich ihn erwarten.", "tokens": ["Von", "dr\u00fc\u00b7ben", "mu\u00df", "ich", "ihn", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So sitz' ich Stunden wie gebannt,", "tokens": ["So", "sitz'", "ich", "Stun\u00b7den", "wie", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Gestern halb und halb im Heute,", "tokens": ["Im", "Ge\u00b7stern", "halb", "und", "halb", "im", "Heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mein gutes Fernrohr in der Hand", "tokens": ["Mein", "gu\u00b7tes", "Fern\u00b7rohr", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und la\u00df es streifen durch die Weite.", "tokens": ["Und", "la\u00df", "es", "strei\u00b7fen", "durch", "die", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Am Damme steht ein wilder Strauch,", "tokens": ["Am", "Dam\u00b7me", "steht", "ein", "wil\u00b7der", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O, schm\u00e4hlich hat mich der betrogen!", "tokens": ["O", ",", "schm\u00e4h\u00b7lich", "hat", "mich", "der", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "PPER", "ART", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "R\u00fchrt ihn der Wind, so mein' ich auch", "tokens": ["R\u00fchrt", "ihn", "der", "Wind", ",", "so", "mein'", "ich", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was Liebes komme hergezogen!", "tokens": ["Was", "Lie\u00b7bes", "kom\u00b7me", "her\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit jedem Schritt wei\u00df er zu gehn,", "tokens": ["Mit", "je\u00b7dem", "Schritt", "wei\u00df", "er", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich anzuformen alle Z\u00fcge;", "tokens": ["Sich", "an\u00b7zu\u00b7for\u00b7men", "al\u00b7le", "Z\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mag er denn am Hange stehn,", "tokens": ["So", "mag", "er", "denn", "am", "Han\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wert Phantom, geliebte L\u00fcge;", "tokens": ["Ein", "wert", "Phan\u00b7tom", ",", "ge\u00b7lieb\u00b7te", "L\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich aber hoffe f\u00fcr und f\u00fcr,", "tokens": ["Ich", "a\u00b7ber", "hof\u00b7fe", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sofern ich mich des Lebens freue,", "tokens": ["So\u00b7fern", "ich", "mich", "des", "Le\u00b7bens", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text"