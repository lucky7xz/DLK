{"dta.poem.13504": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt wandr' ich erst gern!", "tokens": ["Jetzt", "wandr'", "ich", "erst", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Am Fenster nun lauschen", "tokens": ["Am", "Fens\u00b7ter", "nun", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die M\u00e4dchen, es rauschen", "tokens": ["Die", "M\u00e4d\u00b7chen", ",", "es", "rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Brunnen von fern.", "tokens": ["Die", "Brun\u00b7nen", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Aus schimmernden B\u00fcschen", "tokens": ["Aus", "schim\u00b7mern\u00b7den", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihr Plaudern so lieb", "tokens": ["Ihr", "Plau\u00b7dern", "so", "lieb"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Erkenn' ich dazwischen,", "tokens": ["Er\u00b7kenn'", "ich", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ich h\u00f6re mein Lieb!", "tokens": ["Ich", "h\u00f6\u00b7re", "mein", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Kind h\u00fct' dich! bei Nacht", "tokens": ["Kind", "h\u00fct'", "dich", "!", "bei", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Pflegt Amor zu wandern,", "tokens": ["Pflegt", "A\u00b7mor", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "PTKZU", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ruft leise die andern,", "tokens": ["Ruft", "lei\u00b7se", "die", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da schreiten erwacht", "tokens": ["Da", "schrei\u00b7ten", "er\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die G\u00f6tter zur Halle", "tokens": ["Die", "G\u00f6t\u00b7ter", "zur", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In's Freie hinaus,", "tokens": ["In's", "Frei\u00b7e", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es bringt sie dir alle", "tokens": ["Es", "bringt", "sie", "dir", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Dichter in's Haus.", "tokens": ["Der", "Dich\u00b7ter", "in's", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}