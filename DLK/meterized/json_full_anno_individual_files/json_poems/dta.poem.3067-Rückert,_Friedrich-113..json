{"dta.poem.3067": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "113.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zweifl' und verzweifle nicht an deines Gottes Huld;", "tokens": ["Zweifl'", "und", "ver\u00b7zweif\u00b7le", "nicht", "an", "dei\u00b7nes", "Got\u00b7tes", "Huld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er gab dir manches Gut, vergab dir manche Schuld.", "tokens": ["Er", "gab", "dir", "man\u00b7ches", "Gut", ",", "ver\u00b7gab", "dir", "man\u00b7che", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und was er dir versagt, das war dir nicht zum Heil;", "tokens": ["Und", "was", "er", "dir", "ver\u00b7sagt", ",", "das", "war", "dir", "nicht", "zum", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einst wirst du's einsehn ganz, und siehst es schon zum Theil.", "tokens": ["Einst", "wirst", "du's", "ein\u00b7sehn", "ganz", ",", "und", "siehst", "es", "schon", "zum", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV"