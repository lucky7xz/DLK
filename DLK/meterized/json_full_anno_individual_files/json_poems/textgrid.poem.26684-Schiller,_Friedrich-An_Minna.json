{"textgrid.poem.26684": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Minna", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4um ich? Ist mein Auge tr\u00fcber?", "tokens": ["Tr\u00e4um", "ich", "?", "Ist", "mein", "Au\u00b7ge", "tr\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nebelts mir ums Angesicht?", "tokens": ["Ne\u00b7belts", "mir", "ums", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Minna geht vor\u00fcber?", "tokens": ["Mei\u00b7ne", "Min\u00b7na", "geht", "vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Minna kennt mich nicht?", "tokens": ["Mei\u00b7ne", "Min\u00b7na", "kennt", "mich", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die am Arme seichter Laffen", "tokens": ["Die", "am", "Ar\u00b7me", "seich\u00b7ter", "Laf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00e4hend mit dem F\u00e4cher ficht,", "tokens": ["Bl\u00e4\u00b7hend", "mit", "dem", "F\u00e4\u00b7cher", "ficht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer satt sich zu begaffen? \u2013", "tokens": ["Nim\u00b7mer", "satt", "sich", "zu", "be\u00b7ga\u00b7ffen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Meine Minna ist es nicht.", "tokens": ["Mei\u00b7ne", "Min\u00b7na", "ist", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von dem Sonnenhute nicken", "tokens": ["Von", "dem", "Son\u00b7nen\u00b7hu\u00b7te", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolze Federn, mein Geschenk,", "tokens": ["Stol\u00b7ze", "Fe\u00b7dern", ",", "mein", "Ge\u00b7schenk", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleifen, die den Busen schm\u00fccken,", "tokens": ["Schlei\u00b7fen", ",", "die", "den", "Bu\u00b7sen", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufen: Minna, sei gedenk!", "tokens": ["Ru\u00b7fen", ":", "Min\u00b7na", ",", "sei", "ge\u00b7denk", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blumen, die ich selbst erzogen,", "tokens": ["Blu\u00b7men", ",", "die", "ich", "selbst", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieren Brust und Locken noch", "tokens": ["Zie\u00b7ren", "Brust", "und", "Lo\u00b7cken", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach die Brust, die mir gelogen! \u2013", "tokens": ["Ach", "die", "Brust", ",", "die", "mir", "ge\u00b7lo\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Blumen bl\u00fchen doch!", "tokens": ["Und", "die", "Blu\u00b7men", "bl\u00fc\u00b7hen", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh! umh\u00fcpft von leeren Schmeichlern!", "tokens": ["Geh", "!", "um\u00b7h\u00fcpft", "von", "lee\u00b7ren", "Schmeich\u00b7lern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh! vergi\u00df auf ewig mich.", "tokens": ["Geh", "!", "ver\u00b7gi\u00df", "auf", "e\u00b7wig", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "APPR", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberliefert feilen Heuchlern,", "tokens": ["\u00dc\u00b7berl\u00b7ie\u00b7fert", "fei\u00b7len", "Heuch\u00b7lern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eitles Weib, veracht ich dich.", "tokens": ["Eit\u00b7les", "Weib", ",", "ver\u00b7acht", "ich", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh! dir hat ein Herz geschlagen,", "tokens": ["Geh", "!", "dir", "hat", "ein", "Herz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir ein Herz, das edel schlug,", "tokens": ["Dir", "ein", "Herz", ",", "das", "e\u00b7del", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gro\u00df genug, den Schmerz zu tragen,", "tokens": ["Gro\u00df", "ge\u00b7nug", ",", "den", "Schmerz", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es einer Hure schlug.", "tokens": ["Da\u00df", "es", "ei\u00b7ner", "Hu\u00b7re", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6nheit hat dein Herz verdorben,", "tokens": ["Sch\u00f6n\u00b7heit", "hat", "dein", "Herz", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Gesichtchen! sch\u00e4me dich.", "tokens": ["Dein", "Ge\u00b7sicht\u00b7chen", "!", "sch\u00e4\u00b7me", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen ist sein Glanz erstorben,", "tokens": ["Mor\u00b7gen", "ist", "sein", "Glanz", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Rose bl\u00e4ttert sich.", "tokens": ["Sei\u00b7ne", "Ro\u00b7se", "bl\u00e4t\u00b7tert", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwalben, die im Lenze minnen,", "tokens": ["Schwal\u00b7ben", ",", "die", "im", "Len\u00b7ze", "min\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fliehen, wenn der Nordwind weht,", "tokens": ["Flie\u00b7hen", ",", "wenn", "der", "Nord\u00b7wind", "weht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Buhler scheucht dein Herbst von hinnen,", "tokens": ["Buh\u00b7ler", "scheucht", "dein", "Herbst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Freund hast du verschm\u00e4ht.", "tokens": ["Ei\u00b7nen", "Freund", "hast", "du", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In den Tr\u00fcmmern deiner Sch\u00f6ne", "tokens": ["In", "den", "Tr\u00fcm\u00b7mern", "dei\u00b7ner", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich dich verlassen gehn,", "tokens": ["Seh", "ich", "dich", "ver\u00b7las\u00b7sen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weinend in die Blumenszene", "tokens": ["Wei\u00b7nend", "in", "die", "Blu\u00b7mens\u00b7ze\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Mais zur\u00fccke sehn.", "tokens": ["Dei\u00b7nes", "Mais", "zu\u00b7r\u00fc\u00b7cke", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mit hei\u00dfem Liebesgeize", "tokens": ["Die", "mit", "hei\u00b7\u00dfem", "Lie\u00b7bes\u00b7gei\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinem Ku\u00df entgegenflohn,", "tokens": ["Dei\u00b7nem", "Ku\u00df", "ent\u00b7ge\u00b7gen\u00b7flohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zischen dem erloschnen Reize,", "tokens": ["Zi\u00b7schen", "dem", "er\u00b7lo\u00b7schnen", "Rei\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lachen deinem Winter Hohn.", "tokens": ["La\u00b7chen", "dei\u00b7nem", "Win\u00b7ter", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6nheit hat dein Herz verdorben,", "tokens": ["Sch\u00f6n\u00b7heit", "hat", "dein", "Herz", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Gesichtchen! \u2013 sch\u00e4me dich.", "tokens": ["Dein", "Ge\u00b7sicht\u00b7chen", "!", "\u2013", "sch\u00e4\u00b7me", "dich", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen ist sein Glanz erstorben,", "tokens": ["Mor\u00b7gen", "ist", "sein", "Glanz", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Rose bl\u00e4ttert sich \u2013", "tokens": ["Sei\u00b7ne", "Ro\u00b7se", "bl\u00e4t\u00b7tert", "sich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha! wie will ich dann dich h\u00f6hnen!", "tokens": ["Ha", "!", "wie", "will", "ich", "dann", "dich", "h\u00f6h\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6hnen? Gott bewahre mich!", "tokens": ["H\u00f6h\u00b7nen", "?", "Gott", "be\u00b7wah\u00b7re", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weinen will ich bittre Tr\u00e4nen,", "tokens": ["Wei\u00b7nen", "will", "ich", "bitt\u00b7re", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weinen, Minna, \u00fcber dich.", "tokens": ["Wei\u00b7nen", ",", "Min\u00b7na", ",", "\u00fc\u00b7ber", "dich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tr\u00e4um ich? Ist mein Auge tr\u00fcber?", "tokens": ["Tr\u00e4um", "ich", "?", "Ist", "mein", "Au\u00b7ge", "tr\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nebelts mir ums Angesicht?", "tokens": ["Ne\u00b7belts", "mir", "ums", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Minna geht vor\u00fcber?", "tokens": ["Mei\u00b7ne", "Min\u00b7na", "geht", "vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "punct"], "po