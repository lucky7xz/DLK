{"dta.poem.10947": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n unumschr\u00e4nkte Gr\u00f6\u00dfe der Gottheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, selbst von gro\u00dfen Geistern, die Meynung oftermals\ngeh\u00f6rt:", "tokens": ["Ich", "ha\u00b7be", ",", "selbst", "von", "gro\u00b7\u00dfen", "Geis\u00b7tern", ",", "die", "Mey\u00b7nung", "of\u00b7ter\u00b7mals", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ob w\u00e4r ein Mensch, ja nicht einmal die ganze Menschheit,", "tokens": ["Ob", "w\u00e4r", "ein", "Mensch", ",", "ja", "nicht", "ein\u00b7mal", "die", "gan\u00b7ze", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "$,", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Gott, ein solch unendlich Wesen,", "tokens": ["Da\u00df", "Gott", ",", "ein", "solch", "un\u00b7end\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Millionen Sonnen schuf, und viele Millionen Welte", "tokens": ["Das", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", "schuf", ",", "und", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "Wel\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "NN"], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Zum Vorwurf seiner Macht erlesen,", "tokens": ["Zum", "Vor\u00b7wurf", "sei\u00b7ner", "Macht", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die er blo\u00df durch ein Wort erh\u00e4lt,", "tokens": ["Die", "er", "blo\u00df", "durch", "ein", "Wort", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Von seiner Unerme\u00dflichkeit fast schimpflich sich herunter lasse,", "tokens": ["Von", "sei\u00b7ner", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", "fast", "schimpf\u00b7lich", "sich", "her\u00b7un\u00b7ter", "las\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "PRF", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Und mit derselben Kleinigkeit auf eine Weise sich befasse.", "tokens": ["Und", "mit", "der\u00b7sel\u00b7ben", "Klei\u00b7nig\u00b7keit", "auf", "ei\u00b7ne", "Wei\u00b7se", "sich", "be\u00b7fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.9": {"text": "Ja, wenn auch dieses m\u00f6glich w\u00e4re, so w\u00fcrde die\u00df daraus ent-", "tokens": ["Ja", ",", "wenn", "auch", "die\u00b7ses", "m\u00f6g\u00b7lich", "w\u00e4\u00b7re", ",", "so", "w\u00fcr\u00b7de", "die\u00df", "da\u00b7raus", "ent"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "PDAT", "ADJD", "VAFIN", "$,", "ADV", "VAFIN", "PDS", "PAV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Da\u00df auch der Gottheit Majest\u00e4t sich gar, annoch mit kleinern", "tokens": ["Da\u00df", "auch", "der", "Got\u00b7theit", "Ma\u00b7jes\u00b7t\u00e4t", "sich", "gar", ",", "an\u00b7noch", "mit", "klei\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "PRF", "ADV", "$,", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Besch\u00e4fftigen und plagen m\u00fcsse, ", "tokens": ["Be\u00b7sch\u00e4ff\u00b7ti\u00b7gen", "und", "pla\u00b7gen", "m\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und dieses halten sie f\u00fcr ungereimte Schl\u00fcsse.", "tokens": ["Und", "die\u00b7ses", "hal\u00b7ten", "sie", "f\u00fcr", "un\u00b7ge\u00b7reim\u00b7te", "Schl\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man kann hierin hingegen klar entdecken,", "tokens": ["Man", "kann", "hie\u00b7rin", "hin\u00b7ge\u00b7gen", "klar", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df hierin eine L\u00e4sterung", "tokens": ["Da\u00df", "hie\u00b7rin", "ei\u00b7ne", "L\u00e4s\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.15": {"text": "F\u00fcr unsern Gott, f\u00fcr uns Verzweifelung,", "tokens": ["F\u00fcr", "un\u00b7sern", "Gott", ",", "f\u00fcr", "uns", "Ver\u00b7zwei\u00b7fe\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.16": {"text": "In dieser schlimmen Meynung, stecken.", "tokens": ["In", "die\u00b7ser", "schlim\u00b7men", "Mey\u00b7nung", ",", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "F\u00fcr uns kann nichts ungl\u00fccklichers auf Erden,", "tokens": ["F\u00fcr", "uns", "kann", "nichts", "un\u00b7gl\u00fcck\u00b7li\u00b7chers", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Als solch ein Schlu\u00df ersonnen werden.", "tokens": ["Als", "solch", "ein", "Schlu\u00df", "er\u00b7son\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Denn w\u00e4ren wir, weil wir nicht Gottes Obacht werth,", "tokens": ["Denn", "w\u00e4\u00b7ren", "wir", ",", "weil", "wir", "nicht", "Got\u00b7tes", "O\u00b7bacht", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auch nicht von Jhm geliebt, gef\u00fchrt, ern\u00e4hrt:", "tokens": ["Auch", "nicht", "von", "Jhm", "ge\u00b7liebt", ",", "ge\u00b7f\u00fchrt", ",", "er\u00b7n\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "W\u00e4r unser Gottesdienst, Glaub, Hoffnung, Seligkeit,", "tokens": ["W\u00e4r", "un\u00b7ser", "Got\u00b7tes\u00b7dienst", ",", "Glaub", ",", "Hoff\u00b7nung", ",", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein eitler Tand, und wir, in kurzer Zeit,", "tokens": ["Ein", "eit\u00b7ler", "Tand", ",", "und", "wir", ",", "in", "kur\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nachdem uns hier so manches Leid beschwehrt,", "tokens": ["Nach\u00b7dem", "uns", "hier", "so", "man\u00b7ches", "Leid", "be\u00b7schwehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Weit \u00e4rger, als ein Vieh, in nichts verkehrt.", "tokens": ["Weit", "\u00e4r\u00b7ger", ",", "als", "ein", "Vieh", ",", "in", "nichts", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Was Gott betrifft, scheint nur die Meynung gro\u00df zu seyn,", "tokens": ["Was", "Gott", "be\u00b7tr\u00b7ifft", ",", "scheint", "nur", "die", "Mey\u00b7nung", "gro\u00df", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und seine Majest\u00e4t zu ehren:", "tokens": ["Und", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "zu", "eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Doch ists wahrhaftig nur ein Schein.", "tokens": ["Doch", "ists", "wahr\u00b7haf\u00b7tig", "nur", "ein", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sie scheinet Gottes Ruhm zu mehren,", "tokens": ["Sie", "schei\u00b7net", "Got\u00b7tes", "Ruhm", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und machet seine Gr\u00f6\u00dfe klein.", "tokens": ["Und", "ma\u00b7chet", "sei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sie schr\u00e4nket seine Lieb und Macht und Weisheit ein;", "tokens": ["Sie", "schr\u00e4n\u00b7ket", "sei\u00b7ne", "Lieb", "und", "Macht", "und", "Weis\u00b7heit", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ja h\u00f6hnet ihn, statt ihn zu preisen:", "tokens": ["Ja", "h\u00f6h\u00b7net", "ihn", ",", "statt", "ihn", "zu", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wie dieses hieraus klar und deutlich zu erweisen:", "tokens": ["Wie", "die\u00b7ses", "hier\u00b7aus", "klar", "und", "deut\u00b7lich", "zu", "er\u00b7wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da auch das Gr\u00f6\u00dfte, was ihr sehet,", "tokens": ["Da", "auch", "das", "Gr\u00f6\u00df\u00b7te", ",", "was", "ihr", "se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Aus Kleinigkeiten blo\u00df bestehet;", "tokens": ["Aus", "Klei\u00b7nig\u00b7kei\u00b7ten", "blo\u00df", "be\u00b7ste\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "So wird ein Gott ja mehr erh\u00f6het,", "tokens": ["So", "wird", "ein", "Gott", "ja", "mehr", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "In den verehrenden Gedanken,", "tokens": ["In", "den", "ver\u00b7eh\u00b7ren\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Da\u00df er so wohl was klein, als das, so gro\u00df, regiere,", "tokens": ["Da\u00df", "er", "so", "wohl", "was", "klein", ",", "als", "das", ",", "so", "gro\u00df", ",", "re\u00b7gie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PWS", "ADJD", "$,", "KOUS", "PDS", "$,", "ADV", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als wenn man ihm gewisse Schranken", "tokens": ["Als", "wenn", "man", "ihm", "ge\u00b7wis\u00b7se", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Zu setzen unternimmt, wo sich sein Blick verliere.", "tokens": ["Zu", "set\u00b7zen", "un\u00b7ter\u00b7nimmt", ",", "wo", "sich", "sein", "Blick", "ver\u00b7lie\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ey! sprecht, wie weit doch seine Schranken gehn,", "tokens": ["Ey", "!", "sprecht", ",", "wie", "weit", "doch", "sei\u00b7ne", "Schran\u00b7ken", "gehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Aus wie viel Theilen eigentlich", "tokens": ["Aus", "wie", "viel", "Thei\u00b7len", "ei\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "M\u00fc\u00dft, eurer Meynung nach, bestehen", "tokens": ["M\u00fc\u00dft", ",", "eu\u00b7rer", "Mey\u00b7nung", "nach", ",", "be\u00b7ste\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Ein Wesen, eh die Gottheit sich", "tokens": ["Ein", "We\u00b7sen", ",", "eh", "die", "Got\u00b7theit", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Nach ihm bem\u00fcht, sich umzusehen?", "tokens": ["Nach", "ihm", "be\u00b7m\u00fcht", ",", "sich", "um\u00b7zu\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PRF", "VVIZU", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.45": {"text": "W\u00e4r es nur nach dem Werth allein:", "tokens": ["W\u00e4r", "es", "nur", "nach", "dem", "Werth", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.46": {"text": "So w\u00fcrd ihm ja fast nichts, auf dieser Erden,", "tokens": ["So", "w\u00fcrd", "ihm", "ja", "fast", "nichts", ",", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PIS", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Ja kaum die Erde selbst, ein w\u00fcrdger Vorwurf werden,", "tokens": ["Ja", "kaum", "die", "Er\u00b7de", "selbst", ",", "ein", "w\u00fcrd\u00b7ger", "Vor\u00b7wurf", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und Gottes Aufsicht w\u00fcrdig seyn.", "tokens": ["Und", "Got\u00b7tes", "Auf\u00b7sicht", "w\u00fcr\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Die\u00df zeiget die Vernunft, und da\u00df es wahr,", "tokens": ["Die\u00df", "zei\u00b7get", "die", "Ver\u00b7nunft", ",", "und", "da\u00df", "es", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KON", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Zeigt ebenfals die Bibel klar.", "tokens": ["Zeigt", "e\u00b7ben\u00b7fals", "die", "Bi\u00b7bel", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Sie spricht: Es sorge Gott f\u00fcr Sperlinge so gar;", "tokens": ["Sie", "spricht", ":", "Es", "sor\u00b7ge", "Gott", "f\u00fcr", "Sper\u00b7lin\u00b7ge", "so", "gar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NN", "APPR", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Sie spricht: Es f\u00e4llt, ohn unsers Vaters Willen,", "tokens": ["Sie", "spricht", ":", "Es", "f\u00e4llt", ",", "ohn", "un\u00b7sers", "Va\u00b7ters", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KOUI", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Von unserm Haupt kein einzigs Haar,", "tokens": ["Von", "un\u00b7serm", "Haupt", "kein", "ein\u00b7zigs", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Und alles mu\u00df des Sch\u00f6pfers Wink erf\u00fcllen.", "tokens": ["Und", "al\u00b7les", "mu\u00df", "des", "Sch\u00f6p\u00b7fers", "Wink", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "Es ist demnach gewi\u00df, da\u00df unsre Lehre,", "tokens": ["Es", "ist", "dem\u00b7nach", "ge\u00b7wi\u00df", ",", "da\u00df", "uns\u00b7re", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "(zumalen dieser Satz) des Sch\u00f6pfers Ehre", "tokens": ["(", "zu\u00b7ma\u00b7len", "die\u00b7ser", "Satz", ")", "des", "Sch\u00f6p\u00b7fers", "Eh\u00b7re"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PDAT", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Aufs Herrlichste verbreit, auch unsern Glauben mehre:", "tokens": ["Aufs", "Herr\u00b7lichs\u00b7te", "ver\u00b7breit", ",", "auch", "un\u00b7sern", "Glau\u00b7ben", "meh\u00b7re", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Das Kleinste scheint f\u00fcr Gott so wenig klein,", "tokens": ["Das", "Kleins\u00b7te", "scheint", "f\u00fcr", "Gott", "so", "we\u00b7nig", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.59": {"text": "Als wie das Gr\u00f6\u00dfte gro\u00df zu seyn.", "tokens": ["Als", "wie", "das", "Gr\u00f6\u00df\u00b7te", "gro\u00df", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}