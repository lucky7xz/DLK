{"dta.poem.17707": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "329.  Die gantze Dreyfaltigkeit hufft zur  \n Seeligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Allmacht zeucht mich auf/ die Wei\u00dfheit weist mich an/", "tokens": ["Die", "All\u00b7macht", "zeucht", "mich", "auf", "/", "die", "Wei\u00df\u00b7heit", "weist", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die G\u00fctte hilffet mir/ da\u00df ich inn Himmel kan.", "tokens": ["Die", "G\u00fct\u00b7te", "hilf\u00b7fet", "mir", "/", "da\u00df", "ich", "inn", "Him\u00b7mel", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPER", "APPR", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}