{"dta.poem.12287": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kurze Weile .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So w\u00fcnsch ich ihr ein gute Nacht,               ", "tokens": ["So", "w\u00fcnsch", "ich", "ihr", "ein", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei der ich war alleine,", "tokens": ["Bei", "der", "ich", "war", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein traurig Wort sie zu mir sprach,", "tokens": ["Kein", "trau\u00b7rig", "Wort", "sie", "zu", "mir", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wir uns sollten scheiden:", "tokens": ["Da", "wir", "uns", "soll\u00b7ten", "schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201escheid nicht mit Leid,", "tokens": ["\u201e", "scheid", "nicht", "mit", "Leid", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "\u201egott wei\u00df die Zeit,", "tokens": ["\u201e", "gott", "wei\u00df", "die", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "\u201edie Wiederkehr bringt Freuden.\u201c", "tokens": ["\u201e", "die", "Wie\u00b7der\u00b7kehr", "bringt", "Freu\u00b7den", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da ich am j\u00fcngsten bei ihr war,", "tokens": ["Da", "ich", "am", "j\u00fcng\u00b7sten", "bei", "ihr", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Angesicht wollt r\u00f6then,", "tokens": ["Ihr", "An\u00b7ge\u00b7sicht", "wollt", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hat die rothe Sonn gethan,", "tokens": ["Das", "hat", "die", "ro\u00b7the", "Sonn", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wir in Scheidens-N\u00f6then;", "tokens": ["Als", "wir", "in", "Schei\u00b7den\u00b7sN\u00f6\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel Scherz viel Schmerz,", "tokens": ["Viel", "Scherz", "viel", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Brach ihr das Herz,", "tokens": ["Brach", "ihr", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das bin ich innen worden.", "tokens": ["Das", "bin", "ich", "in\u00b7nen", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das M\u00e4gdlein an der Zinnen stand,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "an", "der", "Zin\u00b7nen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hub kl\u00e4glich an zu weinen:", "tokens": ["Hub", "kl\u00e4g\u00b7lich", "an", "zu", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201egedenk daran du junger Knab,", "tokens": ["\u201e", "ge\u00b7denk", "da\u00b7ran", "du", "jun\u00b7ger", "Knab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PAV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ela\u00df mich nicht lang allein,", "tokens": ["\u201e", "la\u00df", "mich", "nicht", "lang", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "\u201ekehr wieder bald,", "tokens": ["\u201e", "kehr", "wie\u00b7der", "bald", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "\u201edein lieb Gestalt,", "tokens": ["\u201e", "dein", "lieb", "Ge\u00b7stalt", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "\u201el\u00f6st mich aus schweren Tr\u00e4umen.\u201c", "tokens": ["\u201e", "l\u00f6st", "mich", "aus", "schwe\u00b7ren", "Tr\u00e4u\u00b7men", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Knabe \u00fcber die Heyde ritt,", "tokens": ["Der", "Kna\u00b7be", "\u00fc\u00b7ber", "die", "Hey\u00b7de", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein R\u00f6\u00dflein warf er rumme:", "tokens": ["Sein", "R\u00f6\u00df\u00b7lein", "warf", "er", "rum\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201egedenk daran mein feines Lieb,", "tokens": ["\u201e", "ge\u00b7denk", "da\u00b7ran", "mein", "fei\u00b7nes", "Lieb", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edein Red werf du nicht umme,", "tokens": ["\u201e", "dein", "Red", "werf", "du", "nicht", "um\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u201ebeschertes Gl\u00fcck", "tokens": ["\u201e", "be\u00b7scher\u00b7tes", "Gl\u00fcck"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "\u201enimm nie zur\u00fcck,", "tokens": ["\u201e", "nimm", "nie", "zu\u00b7r\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "\u201eade ich fahr mein Stra\u00dfen.\u201c", "tokens": ["\u201e", "a\u00b7de", "ich", "fahr", "mein", "Stra\u00b7\u00dfen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Der uns das Liedlein neu es sang,", "tokens": ["Der", "uns", "das", "Lied\u00b7lein", "neu", "es", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Neuem hats gesungen,", "tokens": ["Von", "Neu\u00b7em", "hats", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hat gethan ein freier Knab,", "tokens": ["Das", "hat", "ge\u00b7than", "ein", "frei\u00b7er", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ihm gar wohl gelungen,", "tokens": ["Ist", "ihm", "gar", "wohl", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Er singt uns das,", "tokens": ["Er", "singt", "uns", "das", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Darzu noch ba\u00df", "tokens": ["Dar\u00b7zu", "noch", "ba\u00df"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hats M\u00e4gdlein \u00fcberkommen.", "tokens": ["Hats", "M\u00e4gd\u00b7lein", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}