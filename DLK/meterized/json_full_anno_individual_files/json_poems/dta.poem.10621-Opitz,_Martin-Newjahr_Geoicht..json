{"dta.poem.10621": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Newjahr Geoicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn hat jhre Rei\u00df auff dieses Jahr vollendet/", "tokens": ["Die", "Sonn", "hat", "jhre", "Rei\u00df", "auff", "die\u00b7ses", "Jahr", "voll\u00b7en\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Lieb/ jhr endet noch die harten Sinne nicht/", "tokens": ["Mein", "Lieb", "/", "jhr", "en\u00b7det", "noch", "die", "har\u00b7ten", "Sin\u00b7ne", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonn hat jhren schein nun wieder her gewendet/", "tokens": ["Die", "Sonn", "hat", "jhren", "schein", "nun", "wie\u00b7der", "her", "ge\u00b7wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJD", "ADV", "ADV", "ADV", "VVPP", "$("], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Ihr wendet von mir ab der sch\u00f6nen Augen Liecht.", "tokens": ["Ihr", "wen\u00b7det", "von", "mir", "ab", "der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was w\u00fcnsch ich euch dann jetzt/ mein Augentrost/ vor Gaben/", "tokens": ["Was", "w\u00fcnsch", "ich", "euch", "dann", "jetzt", "/", "mein", "Au\u00b7gen\u00b7trost", "/", "vor", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$(", "PPOSAT", "NN", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In diesem newen Jahr? Gelt? Das besitzt jhr schon.", "tokens": ["In", "die\u00b7sem", "ne\u00b7wen", "Jahr", "?", "Gelt", "?", "Das", "be\u00b7sitzt", "jhr", "schon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "NN", "$.", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Gut Gl\u00fcck? Auch di\u00df ist hier. Wolt jhr dann Sch\u00f6nheit haben?", "tokens": ["Gut", "Gl\u00fcck", "?", "Auch", "di\u00df", "ist", "hier", ".", "Wolt", "jhr", "dann", "Sch\u00f6n\u00b7heit", "ha\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ADV", "PDS", "VAFIN", "ADV", "$.", "VMFIN", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr habt sie allbereit/ vnd wist zuvil darvon.", "tokens": ["Ihr", "habt", "sie", "all\u00b7be\u00b7reit", "/", "vnd", "wist", "zu\u00b7vil", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$(", "KON", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch etwas ist in euch/ wo fern ich es mag sagen/", "tokens": ["Noch", "et\u00b7was", "ist", "in", "euch", "/", "wo", "fern", "ich", "es", "mag", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "APPR", "PPER", "$(", "PWAV", "ADJD", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Davon kompt alles Leidt vnd trawren bey mir her/", "tokens": ["Da\u00b7von", "kompt", "al\u00b7les", "Leidt", "vnd", "traw\u00b7ren", "bey", "mir", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein grosses Bollwerck steht vmb ewer Hertz geschlagen/", "tokens": ["Ein", "gros\u00b7ses", "Boll\u00b7werck", "steht", "vmb", "e\u00b7wer", "Hertz", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df m\u00f6cht ich gerne sehn/ da\u00df es gefellet wer.", "tokens": ["Di\u00df", "m\u00f6cht", "ich", "ger\u00b7ne", "sehn", "/", "da\u00df", "es", "ge\u00b7fel\u00b7let", "wer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$(", "KOUS", "PPER", "VVPP", "PWS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die feste Mawer macht/ da\u00df meine freye Sinnen/", "tokens": ["Die", "fes\u00b7te", "Ma\u00b7wer", "macht", "/", "da\u00df", "mei\u00b7ne", "frey\u00b7e", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein vnverf\u00e4lschte Lieb/ vnd trewe Dienste nicht", "tokens": ["Mein", "vn\u00b7ver\u00b7f\u00e4lschte", "Lieb", "/", "vnd", "tre\u00b7we", "Diens\u00b7te", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Des Hertzens hohes Schlo\u00df verm\u00f6gen zugewinnen/", "tokens": ["Des", "Hert\u00b7zens", "ho\u00b7hes", "Schlo\u00df", "ver\u00b7m\u00f6\u00b7gen", "zu\u00b7ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Schantz ist allzu starck dem anlauff zugericht.", "tokens": ["Die", "Schantz", "ist", "all\u00b7zu", "starck", "dem", "an\u00b7lauff", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach da\u00df durch diesen Wahll Cupido wolte schiessen/", "tokens": ["Ach", "da\u00df", "durch", "die\u00b7sen", "Wahll", "Cu\u00b7pi\u00b7do", "wol\u00b7te", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "APPR", "PDAT", "NN", "NE", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo nicht/ so geb er mir den Bogen vnd Gewalt/", "tokens": ["Wo", "nicht", "/", "so", "geb", "er", "mir", "den", "Bo\u00b7gen", "vnd", "Ge\u00b7walt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$(", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich solt ein grosses Loch bald haben durch gerissen/", "tokens": ["Ich", "solt", "ein", "gros\u00b7ses", "Loch", "bald", "ha\u00b7ben", "durch", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "VAFIN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da ich mein L\u00e4ger hett vnd st\u00e4then auffenthalt.", "tokens": ["Da", "ich", "mein", "L\u00e4\u00b7ger", "hett", "vnd", "st\u00e4\u00b7then", "auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Seyt nun mit meinem Schatz vnd h\u00f6chstem Gut verehret/", "tokens": ["Seyt", "nun", "mit", "mei\u00b7nem", "Schatz", "vnd", "h\u00f6chs\u00b7tem", "Gut", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dem Hertzen/ welchs ich euch zum newen Jahre sendt/", "tokens": ["Dem", "Hert\u00b7zen", "/", "welchs", "ich", "euch", "zum", "ne\u00b7wen", "Jah\u00b7re", "sendt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Verwahret es ja wol/ da\u00df es nicht wirdt versehret/", "tokens": ["Ver\u00b7wah\u00b7ret", "es", "ja", "wol", "/", "da\u00df", "es", "nicht", "wirdt", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie das vergangne Jahr/ so jetzundt hat ein endt.", "tokens": ["Wie", "das", "ver\u00b7gang\u00b7ne", "Jahr", "/", "so", "je\u00b7tzundt", "hat", "ein", "endt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}