{"dta.poem.20837": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist aller Freyheit blo\u00df;", "tokens": ["Der", "Mensch", "ist", "al\u00b7ler", "Frey\u00b7heit", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was er aus der G\u00f6tter Scho\u00df", "tokens": ["Und", "was", "er", "aus", "der", "G\u00f6t\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Flammen und vor Hitze f\u00fchret/", "tokens": ["Vor", "Flam\u00b7men", "und", "vor", "Hit\u00b7ze", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergeht/ und f\u00e4lt in kurtzer Zeit/", "tokens": ["Ver\u00b7geht", "/", "und", "f\u00e4lt", "in", "kurt\u00b7zer", "Zeit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dieweil noch die Empfindligkeit", "tokens": ["Die\u00b7weil", "noch", "die", "Emp\u00b7find\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Leibes seine Seele r\u00fchret/", "tokens": ["Des", "Lei\u00b7bes", "sei\u00b7ne", "See\u00b7le", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Worauf all unser Hoffnung denckt/", "tokens": ["Wo\u00b7rauf", "all", "un\u00b7ser", "Hoff\u00b7nung", "denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liegt in dem Grabe tieff versenckt/", "tokens": ["Liegt", "in", "dem", "Gra\u00b7be", "tieff", "ver\u00b7senckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da wird dem Weisen recht gewehret", "tokens": ["Da", "wird", "dem", "Wei\u00b7sen", "recht", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Auszug seiner besten Lust/", "tokens": ["Der", "Aus\u00b7zug", "sei\u00b7ner", "bes\u00b7ten", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dem kein Gebrechen ist bewust/", "tokens": ["Dem", "kein", "Ge\u00b7bre\u00b7chen", "ist", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und Plutons Feuer nicht verzehret.", "tokens": ["Und", "Plu\u00b7tons", "Feu\u00b7er", "nicht", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}