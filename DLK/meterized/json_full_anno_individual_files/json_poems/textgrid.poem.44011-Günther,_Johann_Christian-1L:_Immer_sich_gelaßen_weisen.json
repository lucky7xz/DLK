_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird man stets erdulden m\u00fc\u00dfen.", "tokens": ["Wird", "man", "stets", "er\u00b7dul\u00b7den", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Rosen in der Welt zu brechen,", "tokens": ["Ro\u00b7sen", "in", "der", "Welt", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir uns nicht d\u00f6rften scheun,", "tokens": ["Wo", "wir", "uns", "nicht", "d\u00f6rf\u00b7ten", "scheun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df uns keine Dornen stechen,", "tokens": ["Da\u00df", "uns", "kei\u00b7ne", "Dor\u00b7nen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird wohl was Unm\u00f6glichs seyn;", "tokens": ["Wird", "wohl", "was", "Un\u00b7m\u00f6g\u00b7lichs", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn dergleichen Rosenstr\u00e4uche", "tokens": ["Denn", "derg\u00b7lei\u00b7chen", "Ro\u00b7sen\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "NN"], "meter": "+-+-+-+-", "measu