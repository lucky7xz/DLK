{"textgrid.poem.43268": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df es wohl, nur Trotz und Widerspruch", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es wohl, nur Trotz und Widerspruch", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "nur", "Trotz", "und", "Wi\u00b7der\u00b7spruch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt ihr aus jedem meiner Verse reden,", "tokens": ["H\u00f6rt", "ihr", "aus", "je\u00b7dem", "mei\u00b7ner", "Ver\u00b7se", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieses kleine unscheinbare Buch,", "tokens": ["Und", "die\u00b7ses", "klei\u00b7ne", "un\u00b7schein\u00b7ba\u00b7re", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr werdet es verdammen und befehden.", "tokens": ["Ihr", "wer\u00b7det", "es", "ver\u00b7dam\u00b7men", "und", "be\u00b7feh\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oh thut es nicht! ... weil ich nicht singen kann", "tokens": ["Oh", "thut", "es", "nicht", "!", "...", "weil", "ich", "nicht", "sin\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Freude Lied, sollt ihr nicht f\u00fcrder grollen,", "tokens": ["Der", "Freu\u00b7de", "Lied", ",", "sollt", "ihr", "nicht", "f\u00fcr\u00b7der", "grol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was meine Muse trauervoll ersann, \u2013", "tokens": ["Was", "mei\u00b7ne", "Mu\u00b7se", "trau\u00b7er\u00b7voll", "er\u00b7sann", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Glaubt mir, ich hab' es oft nicht singen ", "tokens": ["Glaubt", "mir", ",", "ich", "hab'", "es", "oft", "nicht", "sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich es dennoch immer wieder sang,", "tokens": ["Wenn", "ich", "es", "den\u00b7noch", "im\u00b7mer", "wie\u00b7der", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ahnte mir, da\u00df wo an fernem Orte", "tokens": ["So", "ahn\u00b7te", "mir", ",", "da\u00df", "wo", "an", "fer\u00b7nem", "Or\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFI