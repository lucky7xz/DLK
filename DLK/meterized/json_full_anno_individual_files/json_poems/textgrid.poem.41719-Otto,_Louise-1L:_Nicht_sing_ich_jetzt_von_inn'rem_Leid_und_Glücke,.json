{"textgrid.poem.41719": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht sing ich jetzt von inn'rem Leid und Gl\u00fccke,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht sing ich jetzt von inn'rem Leid und Gl\u00fccke,", "tokens": ["Nicht", "sing", "ich", "jetzt", "von", "inn'\u00b7rem", "Leid", "und", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das einzig meiner Seele nur geh\u00f6rt \u2013", "tokens": ["Das", "ein\u00b7zig", "mei\u00b7ner", "See\u00b7le", "nur", "ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich weise meines Schicksals Weh zur\u00fccke,", "tokens": ["Ich", "wei\u00b7se", "mei\u00b7nes", "Schick\u00b7sals", "Weh", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Gramversinken bin ich aufgest\u00f6rt,", "tokens": ["Vom", "Gram\u00b7ver\u00b7sin\u00b7ken", "bin", "ich", "auf\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "All ihren St\u00fcrmen will ich hin mich geben,", "tokens": ["All", "ih\u00b7ren", "St\u00fcr\u00b7men", "will", "ich", "hin", "mich", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf ihrem Meere gilt es mit zu schiffen,", "tokens": ["Auf", "ih\u00b7rem", "Mee\u00b7re", "gilt", "es", "mit", "zu", "schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und seine Brandung hat auch mich ergriffen.", "tokens": ["Und", "sei\u00b7ne", "Bran\u00b7dung", "hat", "auch", "mich", "er\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch ist's nicht lang \u2013 da klopften in die H\u00e4nde", "tokens": ["Noch", "ist's", "nicht", "lang", "\u2013", "da", "klopf\u00b7ten", "in", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "$(", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir jubelvoll zum ", "tokens": ["Wir", "ju\u00b7bel\u00b7voll", "zum"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der mancher engen Schranke setzt ein Ende", "tokens": ["Der", "man\u00b7cher", "en\u00b7gen", "Schran\u00b7ke", "setzt", "ein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch das der V\u00f6lker Trennung fallen mu\u00df.", "tokens": ["Durch", "das", "der", "V\u00f6l\u00b7ker", "Tren\u00b7nung", "fal\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht lange ist's da\u00df uns die ", "tokens": ["Nicht", "lan\u00b7ge", "ist's", "da\u00df", "uns", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "KOUS", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum schnellen Anschlu\u00df an die Nachbarn mahnen,", "tokens": ["Zum", "schnel\u00b7len", "An\u00b7schlu\u00df", "an", "die", "Nach\u00b7barn", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Deutschen nun zu Kindern eines Landes", "tokens": ["Die", "Deut\u00b7schen", "nun", "zu", "Kin\u00b7dern", "ei\u00b7nes", "Lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Geeint im Segen eines Liebesbandes.", "tokens": ["Ge\u00b7eint", "im", "Se\u00b7gen", "ei\u00b7nes", "Lie\u00b7bes\u00b7ban\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und drauf am ", "tokens": ["Und", "drauf", "am"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Von deutscher Freiheit liegt noch ungehoben,", "tokens": ["Von", "deut\u00b7scher", "Frei\u00b7heit", "liegt", "noch", "un\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da klang ein Lied: \u00bb", "tokens": ["Da", "klang", "ein", "Lied", ":", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Als Frankreich drohte, klang mit Trotzes Toben;", "tokens": ["Als", "Fran\u00b7kreich", "droh\u00b7te", ",", "klang", "mit", "Trot\u00b7zes", "To\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und eh' verhallt das Lied \u2013 ein neu Get\u00f6n", "tokens": ["Und", "eh'", "ver\u00b7hallt", "das", "Lied", "\u2013", "ein", "neu", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$(", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Rheine her wie Glocken hell und sch\u00f6n:", "tokens": ["Vom", "Rhei\u00b7ne", "her", "wie", "Glo\u00b7cken", "hell", "und", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "PTKVZ", "KOKOM", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit Gott wird deutscher Geist das Werk vollbringen!", "tokens": ["Mit", "Gott", "wird", "deut\u00b7scher", "Geist", "das", "Werk", "voll\u00b7brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Dom in K\u00f6ln zur Ehr dem Christengotte \u2013", "tokens": ["Der", "Dom", "in", "K\u00f6ln", "zur", "Ehr", "dem", "Chris\u00b7ten\u00b7got\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch dient dem Gott der Freiheit nicht die Welt?", "tokens": ["Doch", "dient", "dem", "Gott", "der", "Frei\u00b7heit", "nicht", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch ein Lied braust \u2013 es gilt der ", "tokens": ["Noch", "ein", "Lied", "braust", "\u2013", "es", "gilt", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "ART"], "meter": "-+++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "So rief Begeisterung mit k\u00fchnen Sehnen", "tokens": ["So", "rief", "Be\u00b7geis\u00b7te\u00b7rung", "mit", "k\u00fch\u00b7nen", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durchs Vaterland mit immer neuen Pl\u00e4nen,", "tokens": ["Durchs", "Va\u00b7ter\u00b7land", "mit", "im\u00b7mer", "neu\u00b7en", "Pl\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da pl\u00f6tzlich drang ein Schreckensruf in's Land:", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "drang", "ein", "Schre\u00b7ckens\u00b7ruf", "in's", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbschweigt!\u00ab donnerts laut \u2013 \u00bbdenn ", "tokens": ["\u00bb", "schweigt", "!", "\u00ab", "don\u00b7nerts", "laut", "\u2013", "\u00bb", "denn"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word"], "pos": ["$(", "VVFIN", "$.", "$(", "ADV", "ADJD", "$(", "$(", "KON"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wie einst im Feuerbusch der Herr erschienen", "tokens": ["Wie", "einst", "im", "Feu\u00b7er\u00b7busch", "der", "Herr", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem heilgen Manne im gelobten Land,", "tokens": ["Dem", "heil\u00b7gen", "Man\u00b7ne", "im", "ge\u00b7lob\u00b7ten", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschien er jetzt und weckt uns ihm zu dienen", "tokens": ["Er\u00b7schien", "er", "jetzt", "und", "weckt", "uns", "ihm", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Feuersgluten j\u00e4hlings uns gesandt!", "tokens": ["Durch", "Feu\u00b7ers\u00b7glu\u00b7ten", "j\u00e4h\u00b7lings", "uns", "ge\u00b7sandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus seinen Tr\u00fcmmern steige Hamburg wieder", "tokens": ["Aus", "sei\u00b7nen", "Tr\u00fcm\u00b7mern", "stei\u00b7ge", "Ham\u00b7burg", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sch\u00f6n wie ein Dom und stark wie m\u00e4chtge T\u00fcrme,", "tokens": ["Sch\u00f6n", "wie", "ein", "Dom", "und", "stark", "wie", "m\u00e4cht\u00b7ge", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "KON", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu seinem Aufbau rufen deutsche Br\u00fcder:", "tokens": ["Zu", "sei\u00b7nem", "Auf\u00b7bau", "ru\u00b7fen", "deut\u00b7sche", "Br\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das ganze Deutschland steht dem Bau zum Schirme.", "tokens": ["Das", "gan\u00b7ze", "Deutschland", "steht", "dem", "Bau", "zum", "Schir\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"