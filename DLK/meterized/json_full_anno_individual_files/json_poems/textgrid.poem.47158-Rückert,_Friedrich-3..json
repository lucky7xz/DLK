{"textgrid.poem.47158": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, meine Freunde raten,", "tokens": ["Lieb\u00b7chen", ",", "mei\u00b7ne", "Freun\u00b7de", "ra\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edlem Lehrstand mich zu weihn,", "tokens": ["Ed\u00b7lem", "Lehr\u00b7stand", "mich", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auszustreuen goldne Saaten", "tokens": ["Aus\u00b7zu\u00b7streu\u00b7en", "gold\u00b7ne", "Saa\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Jugend frische Reihn.", "tokens": ["In", "der", "Ju\u00b7gend", "fri\u00b7sche", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob in mir ich solche K\u00f6rner", "tokens": ["Ob", "in", "mir", "ich", "sol\u00b7che", "K\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heg', ist wenig mir bewu\u00dft;", "tokens": ["Heg'", ",", "ist", "we\u00b7nig", "mir", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PIS", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie zu s\u00e4en zwischen D\u00f6rner", "tokens": ["Sie", "zu", "s\u00e4\u00b7en", "zwi\u00b7schen", "D\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich v\u00f6llig keine Lust.", "tokens": ["Hab'", "ich", "v\u00f6l\u00b7lig", "kei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bin ich selb doch in der Wilde", "tokens": ["Bin", "ich", "selb", "doch", "in", "der", "Wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgewachsen ohne Zucht.", "tokens": ["Auf\u00b7ge\u00b7wach\u00b7sen", "oh\u00b7ne", "Zucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne da\u00df ich andre bilde,", "tokens": ["Oh\u00b7ne", "da\u00df", "ich", "and\u00b7re", "bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich tragen meine Frucht.", "tokens": ["Will", "ich", "tra\u00b7gen", "mei\u00b7ne", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bin geworden, was ich konnte;", "tokens": ["Bin", "ge\u00b7wor\u00b7den", ",", "was", "ich", "konn\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAPP", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd' ein jeder, was er kann!", "tokens": ["Werd'", "ein", "je\u00b7der", ",", "was", "er", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich mich an keinem sonnte,", "tokens": ["Wie", "ich", "mich", "an", "kei\u00b7nem", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Biet' ich Licht auch keinem an.", "tokens": ["Biet'", "ich", "Licht", "auch", "kei\u00b7nem", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sollt' ich ernst gelehrte Sachen", "tokens": ["Sollt'", "ich", "ernst", "ge\u00b7lehr\u00b7te", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pred'gen? Mir ein schlechter Spa\u00df,", "tokens": ["Pre\u00b7d'\u00b7gen", "?", "Mir", "ein", "schlech\u00b7ter", "Spa\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oder lehren Verse machen?", "tokens": ["O\u00b7der", "leh\u00b7ren", "Ver\u00b7se", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber kann ein jeder das.", "tokens": ["Sel\u00b7ber", "kann", "ein", "je\u00b7der", "das", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PIAT", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebchen! Ab vom Lehrerstuhle", "tokens": ["Lieb\u00b7chen", "!", "Ab", "vom", "Leh\u00b7rer\u00b7stuh\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wendet sich zu dir mein Sinn.", "tokens": ["Wen\u00b7det", "sich", "zu", "dir", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich halten soll die Schule,", "tokens": ["Wo", "ich", "hal\u00b7ten", "soll", "die", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dft du sein die Sch\u00fclerin.", "tokens": ["Mu\u00dft", "du", "sein", "die", "Sch\u00fc\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Meine Weisheit will ich tr\u00e4ufen", "tokens": ["Mei\u00b7ne", "Weis\u00b7heit", "will", "ich", "tr\u00e4u\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir mit K\u00fcssen in die Brust,", "tokens": ["Dir", "mit", "K\u00fcs\u00b7sen", "in", "die", "Brust", ","],