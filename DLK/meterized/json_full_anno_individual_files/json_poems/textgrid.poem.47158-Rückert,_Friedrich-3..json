 "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dft du sein die Sch\u00fclerin.", "tokens": ["Mu\u00dft", "du", "sein", "die", "Sch\u00fc\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Meine Weisheit will ich tr\u00e4ufen", "tokens": ["Mei\u00b7ne", "Weis\u00b7heit", "will", "ich", "tr\u00e4u\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir mit K\u00fcssen in die Brust,", "tokens": ["Dir", "mit", "K\u00fcs\u00b7sen", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Geistesbl\u00fcten h\u00e4ufen", "tokens": ["Al\u00b7le", "Geis\u00b7tes\u00b7bl\u00fc\u00b7ten", "h\u00e4u\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um dich her zu Schmuck und Lust.", "tokens": ["Um", "dich", "her", "zu", "Schmuck", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Warum sollt' ich meine Saaten", "tokens": ["Wa\u00b7rum", "sollt'", "ich", "mei\u00b7ne", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fremden Feldern anvertraun,", "tokens": ["Frem\u00b7den", "Fel\u00b7dern", "an\u00b7ver\u00b7traun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mich Gott so wohl beraten,", "tokens": ["Da", "mich", "Gott", "so", "wohl", "be\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich darf mein eignes baun?", "tokens": ["Da\u00df", "ich", "darf", "mein", "eig\u00b7nes", "baun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Pflanzen will ich stets vom frischen", "tokens": ["Pflan\u00b7zen", "will", "ich", "stets", "vom", "fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich meiner Ernten freun,", "tokens": ["Und", "mich", "mei\u00b7ner", "Ern\u00b7ten", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein Fremder soll mir zwischen", "tokens": ["Und", "kein", "Frem\u00b7der", "soll", "mir", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Weizen Unkraut streun.", "tokens": ["Mei\u00b7nen", "Wei\u00b7zen", "Un\u00b7kraut", "streun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["