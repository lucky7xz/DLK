{"dta.poem.21480": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Ixion.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ha, brause nur, rausche nur, rollendes Rad, unerm\u00fcdlich", "tokens": ["Ha", ",", "brau\u00b7se", "nur", ",", "rau\u00b7sche", "nur", ",", "rol\u00b7len\u00b7des", "Rad", ",", "un\u00b7er\u00b7m\u00fcd\u00b7lich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "ADJD"], "meter": "++--+--+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sich schwingend in feuriger Gluth, wohl tanzt vor den Augen", "tokens": ["Sich", "schwin\u00b7gend", "in", "feu\u00b7ri\u00b7ger", "Gluth", ",", "wohl", "tanzt", "vor", "den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hinflie\u00dfend in Nichts, in verwirrendes Flimmern, mir Alles,", "tokens": ["Hin\u00b7flie\u00b7\u00dfend", "in", "Nichts", ",", "in", "ver\u00b7wir\u00b7ren\u00b7des", "Flim\u00b7mern", ",", "mir", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "PIS", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Wohl sitzt kein Fleck mir an dem zermarterten Leibe,", "tokens": ["Wohl", "sitzt", "kein", "Fleck", "mir", "an", "dem", "zer\u00b7mar\u00b7ter\u00b7ten", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der wund nicht w\u00e4re, zerfleischt und blutig gerissen:", "tokens": ["Der", "wund", "nicht", "w\u00e4\u00b7re", ",", "zer\u00b7fleischt", "und", "blu\u00b7tig", "ge\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VAFIN", "$,", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und ha, und dennoch, G\u00f6tter im rosigen Lichte,", "tokens": ["Und", "ha", ",", "und", "den\u00b7noch", ",", "G\u00f6t\u00b7ter", "im", "ro\u00b7si\u00b7gen", "Lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "KON", "ADV", "$,", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die nieder mich warfen, hinab in\u2019s Dunkel des Hades,", "tokens": ["Die", "nie\u00b7der", "mich", "war\u00b7fen", ",", "hin\u00b7ab", "in's", "Dun\u00b7kel", "des", "Ha\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVFIN", "$,", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Auf\u2019s Rad mir flochten, unsterbliche Qualen zu leiden,", "tokens": ["Auf's", "Rad", "mir", "floch\u00b7ten", ",", "uns\u00b7terb\u00b7li\u00b7che", "Qua\u00b7len", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die Glieder, den eurigen gleich so g\u00f6ttlich erbl\u00fchend,", "tokens": ["Die", "Glie\u00b7der", ",", "den", "eu\u00b7ri\u00b7gen", "gleich", "so", "g\u00f6tt\u00b7lich", "er\u00b7bl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "PPOSS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Eins k\u00f6nnt ihr nimmer zerrei\u00dfen in mir und ert\u00f6dten:", "tokens": ["Eins", "k\u00f6nnt", "ihr", "nim\u00b7mer", "zer\u00b7rei\u00b7\u00dfen", "in", "mir", "und", "er\u00b7t\u00f6d\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Hier unter den Augen und unter der Stirne den Funken,", "tokens": ["Hier", "un\u00b7ter", "den", "Au\u00b7gen", "und", "un\u00b7ter", "der", "Stir\u00b7ne", "den", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.12": {"text": "Der ewig erweckt uns\u00e4gliche Wonnenerinn\u2019rung", "tokens": ["Der", "e\u00b7wig", "er\u00b7weckt", "un\u00b7s\u00e4g\u00b7li\u00b7che", "Won\u00b7ne\u00b7ne\u00b7rinn'\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "ADJA", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Im Herzen zutiefst, da\u00df schwelgend es ruht", "tokens": ["Im", "Her\u00b7zen", "zu\u00b7tiefst", ",", "da\u00df", "schwel\u00b7gend", "es", "ruht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KOUS", "ADJD", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Auf rosigem Pf\u00fchl, da\u00df heller noch strahlt", "tokens": ["Auf", "ro\u00b7si\u00b7gem", "Pf\u00fchl", ",", "da\u00df", "hel\u00b7ler", "noch", "strahlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJD", "ADV", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Das trotzige Aug\u2019, und laut es erklingt,", "tokens": ["Das", "trot\u00b7zi\u00b7ge", "Aug'", ",", "und", "laut", "es", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Stolz leuchtenden Blick\u2019s:", "tokens": ["Stolz", "leuch\u00b7ten\u00b7den", "Blick's", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Ich habe das H\u00f6chste besessen!", "tokens": ["Ich", "ha\u00b7be", "das", "H\u00f6chs\u00b7te", "be\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Horch, schwirr\u2019n nicht \u00fcber die Todesgefilde, die bleichen,", "tokens": ["Horch", ",", "schwirr'n", "nicht", "\u00fc\u00b7ber", "die", "To\u00b7des\u00b7ge\u00b7fil\u00b7de", ",", "die", "blei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gleich Fliegengesumme die luftigen Schatten der Seelen,", "tokens": ["Gleich", "Flie\u00b7gen\u00b7ge\u00b7sum\u00b7me", "die", "luf\u00b7ti\u00b7gen", "Schat\u00b7ten", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Wie \u00e4ngstliches Kindergefl\u00fcster in dunkelnder Stille?", "tokens": ["Wie", "\u00e4ngst\u00b7li\u00b7ches", "Kin\u00b7der\u00b7ge\u00b7fl\u00fcs\u00b7ter", "in", "dun\u00b7keln\u00b7der", "Stil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Schwebt weiter ihr Schatten von Schatten, mit grausenerf\u00fclltem", "tokens": ["Schwebt", "wei\u00b7ter", "ihr", "Schat\u00b7ten", "von", "Schat\u00b7ten", ",", "mit", "grau\u00b7sen\u00b7er\u00b7f\u00fcll\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,", "APPR", "ADJA"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.5": {"text": "Abscheu euch wendend hinweg von dem riesigen Frevler,", "tokens": ["Ab\u00b7scheu", "euch", "wen\u00b7dend", "hin\u00b7weg", "von", "dem", "rie\u00b7si\u00b7gen", "Frev\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.6": {"text": "Dem Frevler \u2014 so sagen sie wohl hier drunten und droben,", "tokens": ["Dem", "Frev\u00b7ler", "so", "sa\u00b7gen", "sie", "wohl", "hier", "drun\u00b7ten", "und", "dro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dem Undankbaren, dem Gast und der Himmlischen Liebling,", "tokens": ["Dem", "Un\u00b7dank\u00b7ba\u00b7ren", ",", "dem", "Gast", "und", "der", "Himm\u00b7li\u00b7schen", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der schn\u00f6de begehrte die Gattin des H\u00f6chsten, des ", "tokens": ["Der", "schn\u00f6\u00b7de", "be\u00b7gehr\u00b7te", "die", "Gat\u00b7tin", "des", "H\u00f6chs\u00b7ten", ",", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "ADJA", "ART", "NN", "ART", "NN", "$,", "ART"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.9": {"text": "Wildw\u00fcthigen Rasens, die Augen, die Seele geblendet,", "tokens": ["Wild\u00b7w\u00fct\u00b7hi\u00b7gen", "Ra\u00b7sens", ",", "die", "Au\u00b7gen", ",", "die", "See\u00b7le", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Dem g\u00f6ttlich gelaunt zum Weib die Olympischen gaben", "tokens": ["Dem", "g\u00f6tt\u00b7lich", "ge\u00b7launt", "zum", "Weib", "die", "O\u00b7lym\u00b7pi\u00b7schen", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ein Rosengew\u00f6lk in flammender Gluth zu umfangen,", "tokens": ["Ein", "Ro\u00b7sen\u00b7ge\u00b7w\u00f6lk", "in", "flam\u00b7men\u00b7der", "Gluth", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Eh\u2019 nach ihm folgte die ", "tokens": ["Eh'", "nach", "ihm", "folg\u00b7te", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "So t\u00f6net es hier, so t\u00f6net es dort:", "tokens": ["So", "t\u00f6\u00b7net", "es", "hier", ",", "so", "t\u00f6\u00b7net", "es", "dort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und du, mein Herz? du l\u00e4chelst dazu!", "tokens": ["Und", "du", ",", "mein", "Herz", "?", "du", "l\u00e4\u00b7chelst", "da\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "O rollendes Rad, noch brausender klingt\u2019s,", "tokens": ["O", "rol\u00b7len\u00b7des", "Rad", ",", "noch", "brau\u00b7sen\u00b7der", "klingt's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wild rauschender noch:", "tokens": ["Wild", "rau\u00b7schen\u00b7der", "noch", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Ich habe das H\u00f6chste besessen!", "tokens": ["Ich", "ha\u00b7be", "das", "H\u00f6chs\u00b7te", "be\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Aufgl\u00e4nzt das gro\u00dfe, das leuchtende Auge der ", "tokens": ["Auf\u00b7gl\u00e4nzt", "das", "gro\u00b7\u00dfe", ",", "das", "leuch\u00b7ten\u00b7de", "Au\u00b7ge", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "ART"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Neuem in mir, vor mir; beim Mahle der G\u00f6tter", "tokens": ["Von", "Neu\u00b7em", "in", "mir", ",", "vor", "mir", ";", "beim", "Mah\u00b7le", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gewahr\u2019 ich mich selber, gelagert im hohen Olympos;", "tokens": ["Ge\u00b7wahr'", "ich", "mich", "sel\u00b7ber", ",", "ge\u00b7la\u00b7gert", "im", "ho\u00b7hen", "O\u00b7lym\u00b7pos", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "$,", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Stumm seh\u2019 ich die edle Gestalt, von heimlichem Grame", "tokens": ["Stumm", "seh'", "ich", "die", "ed\u00b7le", "Ge\u00b7stalt", ",", "von", "heim\u00b7li\u00b7chem", "Gra\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Gequ\u00e4lt; ich denke wie sie an den frevelnden Leichtsinn", "tokens": ["Ge\u00b7qu\u00e4lt", ";", "ich", "den\u00b7ke", "wie", "sie", "an", "den", "fre\u00b7veln\u00b7den", "Leicht\u00b7sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "KOKOM", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Des l\u00fcsternen Gatten, und Mitleid, heiliges Mitleid", "tokens": ["Des", "l\u00fcs\u00b7ter\u00b7nen", "Gat\u00b7ten", ",", "und", "Mit\u00b7leid", ",", "hei\u00b7li\u00b7ges", "Mit\u00b7leid"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN", "$,", "ADJA", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Empfind\u2019 ich zu ihr, dem Weib in der herrlichen G\u00f6ttin,", "tokens": ["Emp\u00b7find'", "ich", "zu", "ihr", ",", "dem", "Weib", "in", "der", "herr\u00b7li\u00b7chen", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und was mich noch herrlicher d\u00e4ucht, theilnehmende Liebe.", "tokens": ["Und", "was", "mich", "noch", "herr\u00b7li\u00b7cher", "d\u00e4ucht", ",", "theil\u00b7neh\u00b7men\u00b7