{"textgrid.poem.32670": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines Leipziger Magisters", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jedes Ding hat seine Zeit,", "tokens": ["Ein", "je\u00b7des", "Ding", "hat", "sei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So auch das Liedersingen;", "tokens": ["So", "auch", "das", "Lie\u00b7der\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum will ich euch, ihr Musen, heut", "tokens": ["Drum", "will", "ich", "euch", ",", "ihr", "Mu\u00b7sen", ",", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein letztes Opfer bringen.", "tokens": ["Mein", "letz\u00b7tes", "Op\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gl\u00fcck, mit dem ihr uns beschenkt,", "tokens": ["Das", "Gl\u00fcck", ",", "mit", "dem", "ihr", "uns", "be\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist flatterhaft und eitel;", "tokens": ["Ist", "flat\u00b7ter\u00b7haft", "und", "ei\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich leb' in Niedrigkeit versenkt", "tokens": ["Ich", "leb'", "in", "Nied\u00b7rig\u00b7keit", "ver\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ohne Geld im Beutel.", "tokens": ["Und", "oh\u00b7ne", "Geld", "im", "Beu\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was half es jede Messe mir,", "tokens": ["Was", "half", "es", "je\u00b7de", "Mes\u00b7se", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zw\u00f6lf Bogen anzuf\u00fcllen?", "tokens": ["Zw\u00f6lf", "Bo\u00b7gen", "an\u00b7zu\u00b7f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schrieb, in Theokrits Manier,", "tokens": ["Ich", "schrieb", ",", "in", "Theo\u00b7krits", "Ma\u00b7nier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die lieblichsten Idyllen:", "tokens": ["Die", "lieb\u00b7lichs\u00b7ten", "I\u00b7dyl\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Sang in Horazens hohem Ton", "tokens": ["Sang", "in", "Ho\u00b7ra\u00b7zens", "ho\u00b7hem", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Bald Oden, bald Satiren,", "tokens": ["Bald", "O\u00b7den", ",", "bald", "Sa\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wu\u00dfte, wie Anakreon,", "tokens": ["Und", "wu\u00df\u00b7te", ",", "wie", "A\u00b7nak\u00b7re\u00b7on", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Barbyton zu r\u00fchren:", "tokens": ["Das", "Bar\u00b7by\u00b7ton", "zu", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hatt' eine Epop\u00f6' gemacht,", "tokens": ["Hatt'", "ei\u00b7ne", "E\u00b7po\u00b7p\u00f6'", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcbertraf Homeren;", "tokens": ["Und", "\u00fc\u00b7bert\u00b7raf", "Ho\u00b7me\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lie\u00df mich nicht minder Tag und Nacht", "tokens": ["Lie\u00df", "mich", "nicht", "min\u00b7der", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Gellerts Grabe h\u00f6ren.", "tokens": ["Bei", "Gel\u00b7lerts", "Gra\u00b7be", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst, es lie\u00df mich jedermann", "tokens": ["Um\u00b7sonst", ",", "es", "lie\u00df", "mich", "je\u00b7der\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In stillem Kummer schmachten,", "tokens": ["In", "stil\u00b7lem", "Kum\u00b7mer", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Rezensenten singen an,", "tokens": ["Und", "Re\u00b7zen\u00b7sen\u00b7ten", "sin\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich gr\u00f6blich zu verachten.", "tokens": ["Mich", "gr\u00f6b\u00b7lich", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl! undankbares Vaterland!", "tokens": ["Wohl", "!", "un\u00b7dank\u00b7ba\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ich zu z\u00e4rtlich liebte;", "tokens": ["Das", "ich", "zu", "z\u00e4rt\u00b7lich", "lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh! Hier vergehen sie im Brand,", "tokens": ["Sieh", "!", "Hier", "ver\u00b7ge\u00b7hen", "sie", "im", "Brand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die teuren Manuskripte.", "tokens": ["Die", "teu\u00b7ren", "Ma\u00b7nus\u00b7krip\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du wolltest sie aus Blindheit nicht,", "tokens": ["Du", "woll\u00b7test", "sie", "aus", "Blind\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sie verdienten, lesen;", "tokens": ["Wie", "sie", "ver\u00b7dien\u00b7ten", ",", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun sieh mit weinendem Gesicht,", "tokens": ["Nun", "sieh", "mit", "wei\u00b7nen\u00b7dem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den teuren Schmuck verwesen.", "tokens": ["Den", "teu\u00b7ren", "Schmuck", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der edlen \u00dcbersetzungskunst", "tokens": ["Der", "ed\u00b7len", "\u00dc\u00b7bers\u00b7et\u00b7zungs\u00b7kunst"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich mich nun ergeben.", "tokens": ["Will", "ich", "mich", "nun", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr kann sie, als der Musen Gunst,", "tokens": ["Mehr", "kann", "sie", ",", "als", "der", "Mu\u00b7sen", "Gunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kann uns Nahrung geben.", "tokens": ["Sie", "kann", "uns", "Nah\u00b7rung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O, m\u00f6cht' ich eure Sprache doch,", "tokens": ["O", ",", "m\u00f6cht'", "ich", "eu\u00b7re", "Spra\u00b7che", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Britten, schon verstehen!", "tokens": ["Ihr", "Brit\u00b7ten", ",", "schon", "ver\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Man sollte diese Messe noch", "tokens": ["Man", "soll\u00b7te", "die\u00b7se", "Mes\u00b7se", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von mir ein Pr\u00f6bchen sehen.", "tokens": ["Von", "mir", "ein", "Pr\u00f6b\u00b7chen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein jedes Ding hat seine Zeit,", "tokens": ["Ein", "je\u00b7des", "Ding", "hat", "sei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So auch das Liedersingen;", "tokens": ["So", "auch", "das", "Lie\u00b7der\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum will ich euch, ihr Musen, heut", "tokens": ["Drum", "will", "ich", "euch", ",", "ihr", "Mu\u00b7sen", ",", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein letztes Opfer bringen.", "tokens": ["Mein", "letz\u00b7tes", "Op\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gl\u00fcck, mit dem ihr uns beschenkt,", "tokens": ["Das", "Gl\u00fcck", ",", "mit", "dem", "ihr", "uns", "be\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist flatterhaft und eitel;", "tokens": ["Ist", "flat\u00b7ter\u00b7haft", "und", "ei\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}