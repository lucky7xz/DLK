", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst, es lie\u00df mich jedermann", "tokens": ["Um\u00b7sonst", ",", "es", "lie\u00df", "mich", "je\u00b7der\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In stillem Kummer schmachten,", "tokens": ["In", "stil\u00b7lem", "Kum\u00b7mer", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Rezensenten singen an,", "tokens": ["Und", "Re\u00b7zen\u00b7sen\u00b7ten", "sin\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich gr\u00f6blich zu verachten.", "tokens": ["Mich", "gr\u00f6b\u00b7lich", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wohl! undankbares Vaterland!", "tokens": ["Wohl", "!", "un\u00b7dank\u00b7ba\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ich zu z\u00e4rtlich liebte;", "tokens": ["Das", "ich", "zu", "z\u00e4rt\u00b7lich", "lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieh! Hier vergehen sie im Brand,", "tokens": ["Sieh", "!", "Hier", "ver\u00b7ge\u00b7hen", "sie", "im", "Brand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die teuren Manuskripte.", "tokens": ["Die", "teu\u00b7ren", "Ma\u00b7nus\u00b7krip\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du wolltest sie aus Blindheit nicht,", "tokens": ["Du", "woll\u00b7test", "sie", "aus", "Blind\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sie verdienten, lesen;", "tokens": ["Wie", "sie", "ver\u00b7dien\u00b7ten", ",", "le\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun sieh mit weinendem Gesicht,", "tokens": ["Nun", "sieh", "mit", "wei\u00b7nen\u00b7dem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den teuren Schmuck verwesen.", "tokens": ["Den", "teu\u00b7ren", "Schmuck", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der edlen \u00dcbersetzungskunst", "tokens": ["Der", "ed\u00b7len", "\u00dc\u00b7bers\u00b7et\u00b7zungs\u00b7kunst"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich mich nun ergeben.", "tokens": ["Will", "ich", "mich", "nun", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr kann sie, als der Musen Gunst,", "tokens": ["Mehr", "kann", "sie", ",", "als", "der", "Mu\u00b7sen", "Gunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kann uns Nahrung geben.", "tokens": ["Sie", "kann", "uns", "Nah\u00b7rung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O, m\u00f6cht' ich eure Sprache doch,", "tokens": ["O", ",", "m\u00f6cht'", "ich", "eu\u00b7re", "Spra\u00b7che", "doch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Britten, schon verstehen!", "tokens": ["Ihr", "Brit\u00b7ten", ",", "schon", "ver\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Man sollte diese Messe noch", "tokens": ["Man", "soll\u00b7te", "die\u00b7se", "Mes\u00b7se", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von mir ein Pr\u00f6bchen sehen.", "tokens": ["Von", "mir", "ein", "Pr\u00f6b\u00b7chen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}