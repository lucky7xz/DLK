{"textgrid.poem.63055": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die englischen Fr\u00e4uleins", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die englischen Fr\u00e4uleins gehen in langer Kette durch die Stadt,", "tokens": ["Die", "eng\u00b7li\u00b7schen", "Fr\u00e4u\u00b7leins", "ge\u00b7hen", "in", "lan\u00b7ger", "Ket\u00b7te", "durch", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zwei und zwei, in ihren schwarzen M\u00e4nteln wie Morcheln, die man aus dem Boden gerissen hat.", "tokens": ["Zwei", "und", "zwei", ",", "in", "ih\u00b7ren", "schwar\u00b7zen", "M\u00e4n\u00b7teln", "wie", "Mor\u00b7cheln", ",", "die", "man", "aus", "dem", "Bo\u00b7den", "ge\u00b7ris\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "$,", "APPR", "PPOSAT", "ADJA", "NN", "KOKOM", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+--+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Aber im Sommer tragen sie violette", "tokens": ["A\u00b7ber", "im", "Som\u00b7mer", "tra\u00b7gen", "sie", "vi\u00b7o\u00b7let\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sch\u00e4rpen um den Leib. Sie schlafen allein im Bette.", "tokens": ["Sch\u00e4r\u00b7pen", "um", "den", "Leib", ".", "Sie", "schla\u00b7fen", "al\u00b7lein", "im", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.6": {"text": "Manche ist so sch\u00f6n,", "tokens": ["Man\u00b7che", "ist", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Man m\u00f6chte einmal mit ihr schlafen gehn.", "tokens": ["Man", "m\u00f6ch\u00b7te", "ein\u00b7mal", "mit", "ihr", "schla\u00b7fen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aber sie sind so klein und klein in ihren schwarzen Kapuzen,", "tokens": ["A