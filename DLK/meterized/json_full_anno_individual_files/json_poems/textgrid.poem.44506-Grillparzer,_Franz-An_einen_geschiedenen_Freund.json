{"textgrid.poem.44506": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An einen geschiedenen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du gegangen, m\u00fcd der ewgen Kriege,", "tokens": ["Bist", "du", "ge\u00b7gan\u00b7gen", ",", "m\u00fcd", "der", "ew\u00b7gen", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Einsicht mit der Torheit ficht und schl\u00e4gt,", "tokens": ["Die", "Ein\u00b7sicht", "mit", "der", "Tor\u00b7heit", "ficht", "und", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hast, verzweifelnd an dem sp\u00e4ten Siege,", "tokens": ["Und", "hast", ",", "ver\u00b7zwei\u00b7felnd", "an", "dem", "sp\u00e4\u00b7ten", "Sie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die wohlgebrauchten Waffen hingelegt?", "tokens": ["Die", "wohl\u00b7ge\u00b7brauch\u00b7ten", "Waf\u00b7fen", "hin\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl gut! denn ob man steh, ob unterliege,", "tokens": ["Wohl", "gut", "!", "denn", "ob", "man", "steh", ",", "ob", "un\u00b7ter\u00b7lie\u00b7ge", ","], "t