{"textgrid.poem.42273": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Ode der funfzigj\u00e4hrigen Jubelfeier der Georgia Augusta am 17. September 1787 gewidmet von mehrern zu G\u00f6ttingen Studierenden", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhabenster, der du da\u00df All gestaltet,", "tokens": ["Er\u00b7ha\u00b7bens\u00b7ter", ",", "der", "du", "da\u00df", "All", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Zu deiner Herrlichkeit Pallast,", "tokens": ["Zu", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in ein Lichtgewand, aus Finsternis entfaltet,", "tokens": ["Und", "in", "ein", "Licht\u00b7ge\u00b7wand", ",", "aus", "Fins\u00b7ter\u00b7nis", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Werk gekleidet hast!", "tokens": ["Dein", "Werk", "ge\u00b7klei\u00b7det", "hast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hast im Raum, wo deine Sonne lodert,", "tokens": ["Du", "hast", "im", "Raum", ",", "wo", "dei\u00b7ne", "Son\u00b7ne", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Ein Zentralziel aller Kraft,", "tokens": ["Um", "Ein", "Zent\u00b7ral\u00b7zi\u00b7el", "al\u00b7ler", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zu dem erhabnen Tanz die Sph\u00e4ren aufgefodert,", "tokens": ["Zu", "dem", "er\u00b7hab\u00b7nen", "Tanz", "die", "Sph\u00e4\u00b7ren", "auf\u00b7ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nimmermehr erschlafft!", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "er\u00b7schlafft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es schwebt mit ihm, an Harmonieen-Banden,", "tokens": ["Es", "schwebt", "mit", "ihm", ",", "an", "Har\u00b7mo\u00b7nie\u00b7en\u00b7Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hohe Welt-Choral dahin,", "tokens": ["Der", "ho\u00b7he", "Welt\u00b7Cho\u00b7ral", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom dem Pythagoras und Newton viel verstanden,", "tokens": ["Vom", "dem", "Py\u00b7tha\u00b7go\u00b7ras", "und", "New\u00b7ton", "viel", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ART", "NE", "KON", "NE", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Keplers tiefer Sinn.", "tokens": ["Und", "Ke\u00b7plers", "tie\u00b7fer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Geistesall, wo Form des Raums verschwindet,", "tokens": ["Im", "Geis\u00b7te\u00b7sall", ",", "wo", "Form", "des", "Raums", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo dumpf der Sinn des Zeitstroms Fall", "tokens": ["Wo", "dumpf", "der", "Sinn", "des", "Zeits\u00b7troms", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur noch vernimmt, hast du weit Gr\u00f6\u00dfer dich verk\u00fcndet,", "tokens": ["Nur", "noch", "ver\u00b7nimmt", ",", "hast", "du", "weit", "Gr\u00f6\u00b7\u00dfer", "dich", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als in dem Sinnenall.", "tokens": ["Als", "in", "dem", "Sin\u00b7ne\u00b7nall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da lodern hoch, mit wunderbarem Glanze,", "tokens": ["Da", "lo\u00b7dern", "hoch", ",", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonnen ", "tokens": ["Die", "Son\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Um die, \u2013 so willst du es \u2013 sich in vereintem Tanze", "tokens": ["Um", "die", ",", "\u2013", "so", "willst", "du", "es", "\u2013", "sich", "in", "ver\u00b7ein\u00b7tem", "Tan\u00b7ze"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "$,", "$(", "ADV", "VMFIN", "PPER", "PPER", "$(", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Geistes K\u00fcnste drehn.", "tokens": ["Des", "Geis\u00b7tes", "K\u00fcns\u00b7te", "drehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vereinigung ersehnen die drei Flammen", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gung", "er\u00b7seh\u00b7nen", "die", "drei", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch wechselsweisen Zug und Drang.", "tokens": ["Durch", "wech\u00b7sels\u00b7wei\u00b7sen", "Zug", "und", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch hier rauscht die Musik der Sph\u00e4ren laut zusammen", "tokens": ["Auch", "hier", "rauscht", "die", "Mu\u00b7sik", "der", "Sph\u00e4\u00b7ren", "laut", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "VVINF"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In Einen Chorgesang;", "tokens": ["In", "Ei\u00b7nen", "Chor\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und rauschet fort, von Einem Strom gezogen,", "tokens": ["Und", "rau\u00b7schet", "fort", ",", "von", "Ei\u00b7nem", "Strom", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Strome der Vollkommenheit.", "tokens": ["Vom", "Stro\u00b7me", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Niagara st\u00fcrzt der seine lichten Wogen", "tokens": ["Ein", "Nia\u00b7ga\u00b7ra", "st\u00fcrzt", "der", "sei\u00b7ne", "lich\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ART", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ins Meer der Seligkeit. \u2013", "tokens": ["Ins", "Meer", "der", "Se\u00b7lig\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Georgia, die auch Gesang und Reigen", "tokens": ["Ge\u00b7or\u00b7gia", ",", "die", "auch", "Ge\u00b7sang", "und", "Rei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhabner Geistesk\u00fcnste f\u00fchrt,", "tokens": ["Er\u00b7hab\u00b7ner", "Geis\u00b7tes\u00b7k\u00fcns\u00b7te", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tritt heut vor deinen Thron, ihr Haupt vor dir zu neigen,", "tokens": ["Tritt", "heut", "vor", "dei\u00b7nen", "Thron", ",", "ihr", "Haupt", "vor", "dir", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Anbetung geb\u00fchrt.", "tokens": ["Dem", "An\u00b7be\u00b7tung", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gefiel bisher dir h\u00f6chsten Chorageten", "tokens": ["Ge\u00b7fiel", "bis\u00b7her", "dir", "h\u00f6chs\u00b7ten", "Cho\u00b7ra\u00b7ge\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Einklang mit dem gro\u00dfen Chor", "tokens": ["Ihr", "Ein\u00b7klang", "mit", "dem", "gro\u00b7\u00dfen", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6pfung, so vernimm, was ihre S\u00f6hne beten,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", ",", "so", "ver\u00b7nimm", ",", "was", "ih\u00b7re", "S\u00f6h\u00b7ne", "be\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Herr, mit mildem Ohr!", "tokens": ["O", "Herr", ",", "mit", "mil\u00b7dem", "Ohr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Gesegn' ihr heut im Jubelfeierkleide", "tokens": ["Ge\u00b7segn'", "ihr", "heut", "im", "Ju\u00b7bel\u00b7fei\u00b7er\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Wunsch, den jede Brust ihr weiht,", "tokens": ["Den", "Wunsch", ",", "den", "je\u00b7de", "Brust", "ihr", "weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bis zu G\u00f6tterkraft den Lebenswein der Freude,", "tokens": ["Und", "bis", "zu", "G\u00f6t\u00b7ter\u00b7kraft", "den", "Le\u00b7bens\u00b7wein", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ihr Georg ihr beut!", "tokens": ["Den", "ihr", "Ge\u00b7org", "ihr", "beut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hoch aufgefrischt von dieses Tages Wonnen,", "tokens": ["Hoch", "auf\u00b7ge\u00b7frischt", "von", "die\u00b7ses", "Ta\u00b7ges", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deiner Segenskr\u00e4fte voll,", "tokens": ["Und", "dei\u00b7ner", "Se\u00b7gens\u00b7kr\u00e4f\u00b7te", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhalte sich ihr Schwung um die drei Geistes-Sonnen,", "tokens": ["Er\u00b7hal\u00b7te", "sich", "ihr", "Schwung", "um", "die", "drei", "Geis\u00b7tes\u00b7Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um die sie schweben soll!", "tokens": ["Um", "die", "sie", "schwe\u00b7ben", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nie m\u00fcsse sie des Rhythmus Kunst verlernen,", "tokens": ["Nie", "m\u00fcs\u00b7se", "sie", "des", "Rhyth\u00b7mus", "Kunst", "ver\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Glied an Glied ins Ganze f\u00fcgt!", "tokens": ["Die", "Glied", "an", "Glied", "ins", "Gan\u00b7ze", "f\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fliege sie den Flug mit ihren Folge-Sternen,", "tokens": ["So", "flie\u00b7ge", "sie", "den", "Flug", "mit", "ih\u00b7ren", "Fol\u00b7ge\u00b7S\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den alles Leben fliegt!", "tokens": ["Den", "al\u00b7les", "Le\u00b7ben", "fliegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und werde stets zum Ziele fortgezogen,", "tokens": ["Und", "wer\u00b7de", "stets", "zum", "Zie\u00b7le", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das nur der Gottgeweihte sieht,", "tokens": ["Das", "nur", "der", "Gott\u00b7ge\u00b7weih\u00b7te", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin mit Ozeans-Gewalt der Kr\u00e4fte Wogen", "tokens": ["Wo\u00b7hin", "mit", "O\u00b7ze\u00b7ans\u00b7Ge\u00b7walt", "der", "Kr\u00e4f\u00b7te", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Kraft der Kr\u00e4fte zieht!", "tokens": ["Die", "Kraft", "der", "Kr\u00e4f\u00b7te", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Erhabenster, der du da\u00df All gestaltet,", "tokens": ["Er\u00b7ha\u00b7bens\u00b7ter", ",", "der", "du", "da\u00df", "All", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Zu deiner Herrlichkeit Pallast,", "tokens": ["Zu", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in ein Lichtgewand, aus Finsternis entfaltet,", "tokens": ["Und", "in", "ein", "Licht\u00b7ge\u00b7wand", ",", "aus", "Fins\u00b7ter\u00b7nis", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Werk gekleidet hast!", "tokens": ["Dein", "Werk", "ge\u00b7klei\u00b7det", "hast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Du hast im Raum, wo deine Sonne lodert,", "tokens": ["Du", "hast", "im", "Raum", ",", "wo", "dei\u00b7ne", "Son\u00b7ne", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Ein Zentralziel aller Kraft,", "tokens": ["Um", "Ein", "Zent\u00b7ral\u00b7zi\u00b7el", "al\u00b7ler", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Zu dem erhabnen Tanz die Sph\u00e4ren aufgefodert,", "tokens": ["Zu", "dem", "er\u00b7hab\u00b7nen", "Tanz", "die", "Sph\u00e4\u00b7ren", "auf\u00b7ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nimmermehr erschlafft!", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "er\u00b7schlafft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es schwebt mit ihm, an Harmonieen-Banden,", "tokens": ["Es", "schwebt", "mit", "ihm", ",", "an", "Har\u00b7mo\u00b7nie\u00b7en\u00b7Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hohe Welt-Choral dahin,", "tokens": ["Der", "ho\u00b7he", "Welt\u00b7Cho\u00b7ral", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom dem Pythagoras und Newton viel verstanden,", "tokens": ["Vom", "dem", "Py\u00b7tha\u00b7go\u00b7ras", "und", "New\u00b7ton", "viel", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ART", "NE", "KON", "NE", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Keplers tiefer Sinn.", "tokens": ["Und", "Ke\u00b7plers", "tie\u00b7fer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Im Geistesall, wo Form des Raums verschwindet,", "tokens": ["Im", "Geis\u00b7te\u00b7sall", ",", "wo", "Form", "des", "Raums", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo dumpf der Sinn des Zeitstroms Fall", "tokens": ["Wo", "dumpf", "der", "Sinn", "des", "Zeits\u00b7troms", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur noch vernimmt, hast du weit Gr\u00f6\u00dfer dich verk\u00fcndet,", "tokens": ["Nur", "noch", "ver\u00b7nimmt", ",", "hast", "du", "weit", "Gr\u00f6\u00b7\u00dfer", "dich", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als in dem Sinnenall.", "tokens": ["Als", "in", "dem", "Sin\u00b7ne\u00b7nall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Da lodern hoch, mit wunderbarem Glanze,", "tokens": ["Da", "lo\u00b7dern", "hoch", ",", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Glan\u00b7ze", ","], "token_info": ["wo