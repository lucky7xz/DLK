{"textgrid.poem.42717": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[\u00bbNein,\u00ab schimpfte die Ringelnatter, \u00bbdie Mode]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnein,\u00ab schimpfte die Ringelnatter, \u00bbdie Mode", "tokens": ["\u00bb", "nein", ",", "\u00ab", "schimpf\u00b7te", "die", "Rin\u00b7gel\u00b7nat\u00b7ter", ",", "\u00bb", "die", "Mo\u00b7de"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "PTKANT", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von heutzutage, die wurmt mich zu Tode.", "tokens": ["Von", "heut\u00b7zu\u00b7ta\u00b7ge", ",", "die", "wurmt", "mich", "zu", "To\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PRELS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt soll man t\u00e4glich, sage und schreibe,", "tokens": ["Jetzt", "soll", "man", "t\u00e4g\u00b7lich", ",", "sa\u00b7ge", "und", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zweimal die W\u00e4sche wechseln am Leibe.", "tokens": ["Zwei\u00b7mal", "die", "W\u00e4\u00b7sche", "wech\u00b7seln", "am", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und immer schlimmer wird's mit den Jahren.", "tokens": ["Und", "im\u00b7mer", "schlim\u00b7mer", "wird's", "mit", "den", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es ist rein um aus der Haut zu fahren!\u00ab", "tokens": ["Es", "ist", "rein", "um", "aus", "der", "Haut", "zu", "fah\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So schimpfte die Ringelnatter laut,", "tokens": ["So", "schimpf\u00b7te", "die", "Rin\u00b7gel\u00b7nat\u00b7ter", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wirklich fuhr sie aus der Haut.", "tokens": ["Und", "wirk\u00b7lich", "fuhr", "sie", "aus", "der", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Vorfall war nicht ohne Bedeutung,", "tokens": ["Der", "Vor\u00b7fall", "war", "nicht", "oh\u00b7ne", "Be\u00b7deu\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn zoologisch nennt man das H\u00e4utung.", "tokens": ["Denn", "zoo\u00b7lo\u00b7gisch", "nennt", "man", "das", "H\u00e4u\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "