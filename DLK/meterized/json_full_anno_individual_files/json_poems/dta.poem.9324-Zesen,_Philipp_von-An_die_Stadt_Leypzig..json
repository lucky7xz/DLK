{"dta.poem.9324": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "An die Stadt Leypzig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist dis f\u00fcr ein sch\u00f6ner toon? wes ist dis s\u00fc\u00dfe singen?", "tokens": ["Was", "ist", "dis", "f\u00fcr", "ein", "sch\u00f6\u00b7ner", "toon", "?", "wes", "ist", "dis", "s\u00fc\u00b7\u00dfe", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJA", "NN", "$.", "PWS", "VAFIN", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Das sich erbeben kann so hoch und brechen durch den Neid/", "tokens": ["Das", "sich", "er\u00b7be\u00b7ben", "kann", "so", "hoch", "und", "bre\u00b7chen", "durch", "den", "Neid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVINF", "VMFIN", "ADV", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Das Leypzig/ dich ber\u00fchmet macht/ du sch\u00f6ne zier der zeit.", "tokens": ["Das", "Leyp\u00b7zig", "/", "dich", "be\u00b7r\u00fch\u00b7met", "macht", "/", "du", "sch\u00f6\u00b7ne", "zier", "der", "zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "PPER", "VVFIN", "VVFIN", "$(", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wie l\u00e4sse nicht Ph\u00f6bus selbst in dier die bohen Lieder klingen/", "tokens": ["Wie", "l\u00e4s\u00b7se", "nicht", "Ph\u00f6\u00b7bus", "selbst", "in", "dier", "die", "bo\u00b7hen", "Lie\u00b7der", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "NE", "ADV", "APPR", "PDAT", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "und lehrt in Deutscher Poesie die g\u00fcldnen setten zwingen?", "tokens": ["und", "lehrt", "in", "Deut\u00b7scher", "Poe\u00b7sie", "die", "g\u00fcld\u00b7nen", "set\u00b7ten", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dann Flemming \u00fcbte sich in dier mit singen allbereit/", "tokens": ["Dann", "Flem\u00b7ming", "\u00fcb\u00b7te", "sich", "in", "dier", "mit", "sin\u00b7gen", "all\u00b7be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PRF", "APPR", "PDAT", "APPR", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "dem Lund vnd Olearius nichts fehlt an zierligkeit;", "tokens": ["dem", "Lund", "vnd", "O\u00b7lea\u00b7rius", "nichts", "fehlt", "an", "zier\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ja Hartman/ Bremen/ Finckeltaus und Heinsius sich schwingen", "tokens": ["ja", "Hart\u00b7man", "/", "Bre\u00b7men", "/", "Fin\u00b7ckel\u00b7taus", "und", "Hein\u00b7si\u00b7us", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$(", "NE", "$(", "NN", "KON", "NE", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "bi\u00df an den blauen Himmel nauf und geben dier den prei\u00df/", "tokens": ["bi\u00df", "an", "den", "blau\u00b7en", "Him\u00b7mel", "nauf", "und", "ge\u00b7ben", "dier", "den", "prei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "weil sie/ ", "tokens": ["weil", "sie", "/"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "den Opitzinnen abgesiegt und jhr vers\u00fc\u00dftes spielen", "tokens": ["den", "O\u00b7pit\u00b7zin\u00b7nen", "ab\u00b7ge\u00b7siegt", "und", "jhr", "ver\u00b7s\u00fc\u00df\u00b7tes", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "bey fr\u00fch- und sp\u00e4ter Abends-zeit erlernet und ge\u00fcbt;", "tokens": ["bey", "fr\u00fch", "und", "sp\u00e4\u00b7ter", "A\u00b7bends\u00b7zeit", "er\u00b7ler\u00b7net", "und", "ge\u00b7\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dr\u00fcmb dich/ ", "tokens": ["Dr\u00fcmb", "dich", "/"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der S\u00e4nger Printz spitzt auf dein Lob die zahrten federkielen!", "tokens": ["Der", "S\u00e4n\u00b7ger", "Printz", "spitzt", "auf", "dein", "Lob", "die", "zahr\u00b7ten", "fe\u00b7der\u00b7kie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}