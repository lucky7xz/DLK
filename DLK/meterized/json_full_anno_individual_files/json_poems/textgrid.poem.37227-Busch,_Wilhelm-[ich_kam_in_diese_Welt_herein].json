{"textgrid.poem.37227": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich kam in diese Welt herein]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam in diese Welt herein,", "tokens": ["Ich", "kam", "in", "die\u00b7se", "Welt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich ba\u00df zu am\u00fcsieren,", "tokens": ["Mich", "ba\u00df", "zu", "a\u00b7m\u00fc\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollte gern was Rechtes sein", "tokens": ["Ich", "woll\u00b7te", "gern", "was", "Rech\u00b7tes", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dfte mich immer genieren.", "tokens": ["Und", "mu\u00df\u00b7te", "mich", "im\u00b7mer", "ge\u00b7nie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Oft war ich hoffnungsvoll und froh", "tokens": ["Oft", "war", "ich", "hoff\u00b7nungs\u00b7voll", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sp\u00e4ter kam es doch nicht so.", "tokens": ["Und", "sp\u00e4\u00b7ter", "kam", "es", "doch", "nicht", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun lauf ich manchen Donnerstag", "tokens": ["Nun", "lauf", "ich", "man\u00b7chen", "Don\u00b7ners\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hienieden schon herummer,", "tokens": ["Hien\u00b7ie\u00b7den", "schon", "her\u00b7um\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie ich mich drehn und wenden mag,", "tokens": ["Wie", "ich", "mich", "drehn", "und", "wen\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "'s ist immer der alte Kummer.", "tokens": ["'s", "ist", "im\u00b7mer", "der", "al\u00b7te", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Bald klopft vor Schmerz und bald vor Lust", "tokens": ["Bald", "klopft", "vor", "Schmerz", "und", "bald", "vor", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das rote Ding in meiner Brust.", "tokens": ["Das", "ro\u00b7te", "Ding", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter":