{"dta.poem.3874": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "4) Als er ihm zur Bade-Cur  gratuli rte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Gesegnet sey dein Bad, gesegnet deine Reise,", "tokens": ["Ge\u00b7seg\u00b7net", "sey", "dein", "Bad", ",", "ge\u00b7seg\u00b7net", "dei\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit des H\u00f6chsten Huld sich t\u00e4glich neu", "tokens": ["Da\u00b7mit", "des", "H\u00f6chs\u00b7ten", "Huld", "sich", "t\u00e4g\u00b7lich", "neu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erweise;", "tokens": ["er\u00b7wei\u00b7se", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Gott nehme di\u00df Gebet von meinen Lippen an,", "tokens": ["Gott", "neh\u00b7me", "di\u00df", "Ge\u00b7bet", "von", "mei\u00b7nen", "Lip\u00b7pen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit man seine Treu in Ehrfurcht r\u00fchmen kan.", "tokens": ["Da\u00b7mit", "man", "sei\u00b7ne", "Treu", "in", "Ehr\u00b7furcht", "r\u00fch\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Prudel, M\u00fchlen-Bad, das Saltz und andre", "tokens": ["Der", "Pru\u00b7del", ",", "M\u00fch\u00b7len\u00b7Bad", ",", "das", "Saltz", "und", "and\u00b7re"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NE", "$,", "ART", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sachen,", "tokens": ["Sa\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Die werden deinen Leib aufs neue lebend machen.", "tokens": ["Die", "wer\u00b7den", "dei\u00b7nen", "Leib", "aufs", "neu\u00b7e", "le\u00b7bend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch siehe auf dich selbst; mein Vater! schone dich,", "tokens": ["Doch", "sie\u00b7he", "auf", "dich", "selbst", ";", "mein", "Va\u00b7ter", "!", "scho\u00b7ne", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPER", "ADV", "$.", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich flehe Tag und Nacht, ich seuffze \u00e4ngstiglich.", "tokens": ["Ich", "fle\u00b7he", "Tag", "und", "Nacht", ",", "ich", "seuff\u00b7ze", "\u00e4ngs\u00b7tig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Land, ein Ort, ein Volck voll kriegerischer", "tokens": ["Ein", "Land", ",", "ein", "Ort", ",", "ein", "Volck", "voll", "krie\u00b7ge\u00b7ri\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Waffen,", "tokens": ["Waf\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die k\u00f6nnen bey der Cur auch vieles Schrecken", "tokens": ["Die", "k\u00f6n\u00b7nen", "bey", "der", "Cur", "auch", "vie\u00b7les", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "ADV", "PIS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "schaffen,", "tokens": ["schaf\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Und da ein harter Thon an dieser Gr\u00e4ntze schallt,", "tokens": ["Und", "da", "ein", "har\u00b7ter", "Thon", "an", "die\u00b7ser", "Gr\u00e4nt\u00b7ze", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In Eger, wo es blitzt, wo St\u00fcck und Donner knallt,", "tokens": ["In", "E\u00b7ger", ",", "wo", "es", "blitzt", ",", "wo", "St\u00fcck", "und", "Don\u00b7ner", "knallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So wolle GOttes Heer, das Heer der Cherubinen,", "tokens": ["So", "wol\u00b7le", "Got\u00b7tes", "Heer", ",", "das", "Heer", "der", "Che\u00b7ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dir auch bey deiner Cur zum besten Schutze dienen.", "tokens": ["Dir", "auch", "bey", "dei\u00b7ner", "Cur", "zum", "bes\u00b7ten", "Schut\u00b7ze", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So brauche es vergn\u00fcgt, geniesse der Alee,", "tokens": ["So", "brau\u00b7che", "es", "ver\u00b7gn\u00fcgt", ",", "ge\u00b7nies\u00b7se", "der", "A\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Coffee ist nicht dein Tranck, bediene dich des Thee;", "tokens": ["Cof\u00b7fee", "ist", "nicht", "dein", "Tranck", ",", "be\u00b7die\u00b7ne", "dich", "des", "Thee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Genie\u00df das gute Bier, sey deiner Sorgen Meister,", "tokens": ["Ge\u00b7nie\u00df", "das", "gu\u00b7te", "Bier", ",", "sey", "dei\u00b7ner", "Sor\u00b7gen", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Komm gl\u00fccklich wieder an, best\u00e4rck die Lebens-", "tokens": ["Komm", "gl\u00fcck\u00b7lich", "wie\u00b7der", "an", ",", "be\u00b7st\u00e4rck", "die", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ", "$,", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Geister.", "tokens": ["Geis\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Ich zehle jeden Tag, ich zehle jede Stund,", "tokens": ["Ich", "zeh\u00b7le", "je\u00b7den", "Tag", ",", "ich", "zeh\u00b7le", "je\u00b7de", "Stund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bis du mir schreiben wirst: mein Sohn, ", "tokens": ["Bis", "du", "mir", "schrei\u00b7ben", "wirst", ":", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VAFIN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "gesund.", "tokens": ["ge\u00b7sund", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}