{"dta.poem.8856": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXV .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Das junge, neue Laub den Baum:", "tokens": ["Das", "jun\u00b7ge", ",", "neu\u00b7e", "Laub", "den", "Baum", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gr\u00fcnt um deine Wange rings", "tokens": ["So", "gr\u00fcnt", "um", "dei\u00b7ne", "Wan\u00b7ge", "rings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der frische, dunkle, weiche Flaum;", "tokens": ["Der", "fri\u00b7sche", ",", "dunk\u00b7le", ",", "wei\u00b7che", "Flaum", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr sch\u00f6ne Weiber w\u00e4r's ein Gl\u00fcck,", "tokens": ["F\u00fcr", "sch\u00f6\u00b7ne", "Wei\u00b7ber", "w\u00e4r's", "ein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur zu ber\u00fchren deinen Saum!", "tokens": ["Nur", "zu", "be\u00b7r\u00fch\u00b7ren", "dei\u00b7nen", "Saum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch warfst du deinem Nacken um", "tokens": ["Doch", "warfst", "du", "dei\u00b7nem", "Na\u00b7cken", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der reinen, keuschen Sitte Zaum.", "tokens": ["Der", "rei\u00b7nen", ",", "keu\u00b7schen", "Sit\u00b7te", "Zaum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O bringe Wein und komm zu mir,", "tokens": ["O", "brin\u00b7ge", "Wein", "und", "komm", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im hohen Grase hier ist Raum!", "tokens": ["Im", "ho\u00b7hen", "Gra\u00b7se", "hier", "ist", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es letze deiner Zunge Wort", "tokens": ["Es", "let\u00b7ze", "dei\u00b7ner", "Zun\u00b7ge", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Ohr mir und der Wein den Gaum;", "tokens": ["Das", "Ohr", "mir", "und", "der", "Wein", "den", "Gaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Rausch erh\u00f6ht die Wange dir,", "tokens": ["Der", "Rausch", "er\u00b7h\u00f6ht", "die", "Wan\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00df steigen dir zu Kopf den Schaum!", "tokens": ["La\u00df", "stei\u00b7gen", "dir", "zu", "Kopf", "den", "Schaum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "La\u00df hier uns tr\u00e4umen, Arm in Arm,", "tokens": ["La\u00df", "hier", "uns", "tr\u00e4u\u00b7men", ",", "Arm", "in", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVINF", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Jugend kurzen Morgentraum!", "tokens": ["Der", "Ju\u00b7gend", "kur\u00b7zen", "Mor\u00b7gen\u00b7traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}