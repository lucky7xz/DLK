{"dta.poem.11381": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als tit. Herr Wolff Abraham von Ger\u00df-  \n dorf zum amts-hauptmann  \n erwehlet wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein wohlseyn, edles land! ist gros und ungemein:", "tokens": ["Dein", "wohl\u00b7seyn", ",", "ed\u00b7les", "land", "!", "ist", "gros", "und", "un\u00b7ge\u00b7mein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der himmel scheinet dir vor andern sehr geneiget:", "tokens": ["Der", "him\u00b7mel", "schei\u00b7net", "dir", "vor", "an\u00b7dern", "sehr", "ge\u00b7nei\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hast du, wie Rhodus, schon nicht t\u00e4glich sonnen-schein;", "tokens": ["Hast", "du", ",", "wie", "Rho\u00b7dus", ",", "schon", "nicht", "t\u00e4g\u00b7lich", "son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "NE", "$,", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So siehst du, wie dein glantz aus vielen sonnen steiget.", "tokens": ["So", "siehst", "du", ",", "wie", "dein", "glantz", "aus", "vie\u00b7len", "son\u00b7nen", "stei\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein rechtes sonnen-licht ist Friderich August,", "tokens": ["Dein", "rech\u00b7tes", "son\u00b7nen\u00b7licht", "ist", "Fri\u00b7de\u00b7rich", "Au\u00b7gust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der so viel huld und schutz, als Ph\u00f6bus, strahlen giebet:", "tokens": ["Der", "so", "viel", "huld", "und", "schutz", ",", "als", "Ph\u00f6\u00b7bus", ",", "strah\u00b7len", "gie\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "KON", "ADJD", "$,", "KOUS", "NE", "$,", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein land-vogt, welcher dich, wie dieser vater, liebet,", "tokens": ["Dein", "lan\u00b7dvogt", ",", "wel\u00b7cher", "dich", ",", "wie", "die\u00b7ser", "va\u00b7ter", ",", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "$,", "PWAV", "PDAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Tr\u00e4gt seines f\u00fcrsten bild mehr in als auf der brust;", "tokens": ["Tr\u00e4gt", "sei\u00b7nes", "f\u00fcrs\u00b7ten", "bild", "mehr", "in", "als", "auf", "der", "brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ob von diesem zwar an ehren-vollem leben", "tokens": ["Und", "ob", "von", "die\u00b7sem", "zwar", "an", "eh\u00b7ren\u00b7vol\u00b7lem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "ADV", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Viel grosse sich bem\u00fchn, den widerschein zu geben:", "tokens": ["Viel", "gros\u00b7se", "sich", "be\u00b7m\u00fchn", ",", "den", "wi\u00b7der\u00b7schein", "zu", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PRF", "VVINF", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So sieht man doch, da\u00df der, so ihm am nahmen gleicht,", "tokens": ["So", "sieht", "man", "doch", ",", "da\u00df", "der", ",", "so", "ihm", "am", "nah\u00b7men", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "ART", "$,", "ADV", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den abri\u00df auch zugleich von seiner tugend reicht;", "tokens": ["Den", "ab\u00b7ri\u00df", "auch", "zu\u00b7gleich", "von", "sei\u00b7ner", "tu\u00b7gend", "reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So da\u00df sie allerseits in deinen frohen grentzen,", "tokens": ["So", "da\u00df", "sie", "al\u00b7ler\u00b7seits", "in", "dei\u00b7nen", "fro\u00b7hen", "grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den neben-sonnen gleich, von einem lichte gl\u00e4ntzen.", "tokens": ["Den", "ne\u00b7ben\u00b7son\u00b7nen", "gleich", ",", "von", "ei\u00b7nem", "lich\u00b7te", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Es legt der adel itzt ein klares zeugni\u00df dar,", "tokens": ["Es", "legt", "der", "a\u00b7del", "itzt", "ein", "kla\u00b7res", "zeug\u00b7ni\u00df", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, grosser Ger\u00dfdorff! dich zu seinem haupte wehlet.", "tokens": ["Der", ",", "gros\u00b7ser", "Ger\u00df\u00b7dorff", "!", "dich", "zu", "sei\u00b7nem", "haup\u00b7te", "weh\u00b7let", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "$.", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hat, weil ihm so wohl bey deinem vater war,", "tokens": ["Er", "hat", ",", "weil", "ihm", "so", "wohl", "bey", "dei\u00b7nem", "va\u00b7ter", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein gleiches ehren-maas dem sohne zugezehlet.", "tokens": ["Ein", "glei\u00b7ches", "eh\u00b7ren\u00b7maas", "dem", "soh\u00b7ne", "zu\u00b7ge\u00b7zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man tr\u00e4gt ein solches amt nur klugen h\u00e4uptern an:", "tokens": ["Man", "tr\u00e4gt", "ein", "sol\u00b7ches", "amt", "nur", "klu\u00b7gen", "h\u00e4up\u00b7tern", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "PIAT", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer starcke schultern hat, dem wird viel aufgeleget:", "tokens": ["Wer", "star\u00b7cke", "schul\u00b7tern", "hat", ",", "dem", "wird", "viel", "auf\u00b7ge\u00b7le\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVINF", "VAFIN", "$,", "PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Atlas mu\u00df es seyn, der so viel wolcken tr\u00e4get,", "tokens": ["Ein", "At\u00b7las", "mu\u00df", "es", "seyn", ",", "der", "so", "viel", "wol\u00b7cken", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf dessen gipfel selbst der himmel ruhen kan.", "tokens": ["Auf", "des\u00b7sen", "gip\u00b7fel", "selbst", "der", "him\u00b7mel", "ru\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es kan kein Phaeton den sonnen-wagen f\u00fchren,", "tokens": ["Es", "kan", "kein", "Phae\u00b7ton", "den", "son\u00b7nen\u00b7wa\u00b7gen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und auch kein schwaches kind ein schweres schiff regieren.", "tokens": ["Und", "auch", "kein", "schwa\u00b7ches", "kind", "ein", "schwe\u00b7res", "schiff", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil nun dein seltner witz und hurtiger verstand,", "tokens": ["Weil", "nun", "dein", "selt\u00b7ner", "witz", "und", "hur\u00b7ti\u00b7ger", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den schnellen blitzen gleich, fast \u00fcberall bekannt;", "tokens": ["Den", "schnel\u00b7len", "blit\u00b7zen", "gleich", ",", "fast", "\u00fc\u00b7be\u00b7rall", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So kan man bey dein amt, als eine crone, schreiben:", "tokens": ["So", "kan", "man", "bey", "dein", "amt", ",", "als", "ei\u00b7ne", "cro\u00b7ne", ",", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie mu\u00df dem w\u00fcrdigsten allein zu theile bleiben.", "tokens": ["Sie", "mu\u00df", "dem", "w\u00fcr\u00b7digs\u00b7ten", "al\u00b7lein", "zu", "thei\u00b7le", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "VVFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Natur und himmel hat darzu dich ausger\u00fcst;", "tokens": ["Na\u00b7tur", "und", "him\u00b7mel", "hat", "dar\u00b7zu", "dich", "aus\u00b7ge\u00b7r\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem deine jugend-zeit nur halb bekannt gewesen,", "tokens": ["Wem", "dei\u00b7ne", "ju\u00b7gend\u00b7zeit", "nur", "halb", "be\u00b7kannt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Spricht, da\u00df du jeden tag an witz gewachsen bist,", "tokens": ["Spricht", ",", "da\u00df", "du", "je\u00b7den", "tag", "an", "witz", "ge\u00b7wach\u00b7sen", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df man bl\u00fcth\u2019 und frucht von dir zugleich gelesen:", "tokens": ["Und", "da\u00df", "man", "bl\u00fcth'", "und", "frucht", "von", "dir", "zu\u00b7gleich", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "KON", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein auge hat nur das, was r\u00fchmlich war, geliebt,", "tokens": ["Dein", "au\u00b7ge", "hat", "nur", "das", ",", "was", "r\u00fchm\u00b7lich", "war", ",", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PDS", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und weil dir wohl bewust, da\u00df reisen und studiren", "tokens": ["Und", "weil", "dir", "wohl", "be\u00b7wust", ",", "da\u00df", "rei\u00b7sen", "und", "stu\u00b7di\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des adels pracht-geb\u00e4u, gleich zweyen pfeilern, zieren;", "tokens": ["Des", "a\u00b7dels", "pracht\u00b7ge\u00b7b\u00e4u", ",", "gleich", "zwe\u00b7yen", "pfei\u00b7lern", ",", "zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ADV", "VVINF", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hast du billich dich in beyden so ge\u00fcbt,", "tokens": ["So", "hast", "du", "bil\u00b7lich", "dich", "in", "bey\u00b7den", "so", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PRF", "APPR", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ruhm und gl\u00fccke sich mit allen ihren gaben", "tokens": ["Da\u00df", "ruhm", "und", "gl\u00fc\u00b7cke", "sich", "mit", "al\u00b7len", "ih\u00b7ren", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "VVFIN", "PRF", "APPR", "PIAT", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Um deine freundschafft fast hernach gezancket haben.", "tokens": ["Um", "dei\u00b7ne", "freund\u00b7schafft", "fast", "her\u00b7nach", "ge\u00b7zan\u00b7cket", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da du dreyzehn jahr die ehren-volle last", "tokens": ["Und", "da", "du", "drey\u00b7zehn", "jahr", "die", "eh\u00b7ren\u00b7vol\u00b7le", "last"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "CARD", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als landes-\u00e4ltester mit ruhm getragen hast,", "tokens": ["Als", "lan\u00b7des\u00b7\u00e4l\u00b7tes\u00b7ter", "mit", "ruhm", "ge\u00b7tra\u00b7gen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ist ja des f\u00fcrsten gunst, der st\u00e4nde wahl zu loben,", "tokens": ["Ist", "ja", "des", "f\u00fcrs\u00b7ten", "gunst", ",", "der", "st\u00e4n\u00b7de", "wahl", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die dich nach aller wunsch zum hauptmann hat erhoben.", "tokens": ["Die", "dich", "nach", "al\u00b7ler", "wunsch", "zum", "haupt\u00b7mann", "hat", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "ADJD", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und also wei\u00df man fast von dieser w\u00fcrde nicht:", "tokens": ["Und", "al\u00b7so", "wei\u00df", "man", "fast", "von", "die\u00b7ser", "w\u00fcr\u00b7de", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "APPR", "PDS", "VAFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob selbte mehr durch dich, als du durch sie, geehret?", "tokens": ["Ob", "selb\u00b7te", "mehr", "durch", "dich", ",", "als", "du", "durch", "sie", ",", "ge\u00b7eh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexa