{"textgrid.poem.34393": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[beuge die Zweige nieder]", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beuge die Zweige nieder, herbstlicher Wind,", "tokens": ["Beu\u00b7ge", "die", "Zwei\u00b7ge", "nie\u00b7der", ",", "herbst\u00b7li\u00b7cher", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "wirble die Bl\u00e4tter empor", "tokens": ["wirb\u00b7le", "die", "Bl\u00e4t\u00b7ter", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "und s\u00e4ttige mir die brennende Stirne", "tokens": ["und", "s\u00e4t\u00b7ti\u00b7ge", "mir", "die", "bren\u00b7nen\u00b7de", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mit den verwehten Tropfen der Nacht!", "tokens": ["mit", "den", "ver\u00b7weh\u00b7ten", "Trop\u00b7fen", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sternlos lastende Nacht,", "tokens": ["Stern\u00b7los", "las\u00b7ten\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "willig leihst du dem Werk der Zerst\u00f6rung,", "tokens": ["wil\u00b7lig", "leihst", "d