{"dta.poem.13110": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eso schliesset dann und sprecht! ist meine Kunst im Stand?", "tokens": ["\u201e", "so", "schlies\u00b7set", "dann", "und", "sprecht", "!", "ist", "mei\u00b7ne", "Kunst", "im", "Stand", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "KON", "VVFIN", "$.", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ekann ich der sp\u00e4ten Welt, uns, und dem Vaterland", "tokens": ["\u201e", "kann", "ich", "der", "sp\u00e4\u00b7ten", "Welt", ",", "uns", ",", "und", "dem", "Va\u00b7ter\u00b7land"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eein solches Ehren-Mahl vor das Gesichte bauen,", "tokens": ["\u201e", "ein", "sol\u00b7ches", "Eh\u00b7ren\u00b7Mahl", "vor", "das", "Ge\u00b7sich\u00b7te", "bau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eauf dem ", "tokens": ["\u201e", "auf", "dem"], "token_info": ["punct", "word", "word"], "pos": ["$(", "APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "\u201eseyd aber ihr vielleicht noch zweifelhaft dabey,", "tokens": ["\u201e", "seyd", "a\u00b7ber", "ihr", "viel\u00b7leicht", "noch", "zwei\u00b7fel\u00b7haft", "da\u00b7bey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PPER", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "450\u201dOb jemand t\u00fcchtiger, als ich, zu finden sey?", "tokens": ["\"", "Ob", "je\u00b7mand", "t\u00fcch\u00b7ti\u00b7ger", ",", "als", "ich", ",", "zu", "fin\u00b7den", "sey", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ADJD", "$,", "KOUS", "PPER", "$,", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201enein! niemand schmeichle sich der Dicht-Kunst vorzugehen:", "tokens": ["\u201e", "nein", "!", "nie\u00b7mand", "schmeich\u00b7le", "sich", "der", "Dicht\u00b7Kunst", "vor\u00b7zu\u00b7ge\u00b7hen", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PIS", "VVFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eder Himmel selbst hat mich zu diesem Recht versehen.", "tokens": ["\u201e", "der", "Him\u00b7mel", "selbst", "hat", "mich", "zu", "die\u00b7sem", "Recht", "ver\u00b7se\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201evon ihm entspringt mein Geist. Wer hat so festen Grund?", "tokens": ["\u201e", "von", "ihm", "ent\u00b7springt", "mein", "Geist", ".", "Wer", "hat", "so", "fes\u00b7ten", "Grund", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "PWS", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eman red! ich weiche gern; man mache mir es kund!", "tokens": ["\u201e", "man", "red", "!", "ich", "wei\u00b7che", "gern", ";", "man", "ma\u00b7che", "mir", "es", "kund", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$.", "PIS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}