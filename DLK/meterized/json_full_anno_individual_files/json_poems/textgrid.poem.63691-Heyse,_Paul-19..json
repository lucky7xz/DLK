{"textgrid.poem.63691": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Leben gibt's, behauptet ein Franzos,", "tokens": ["Im", "Le\u00b7ben", "gibt's", ",", "be\u00b7haup\u00b7tet", "ein", "Fran\u00b7zos", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00f6ne Momente, gute Viertelstunden,", "tokens": ["Sch\u00f6\u00b7ne", "Mo\u00b7men\u00b7te", ",", "gu\u00b7te", "Vier\u00b7tel\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und da\u00df sie oft sich ausgedehnt zu Stunden,", "tokens": ["Und", "da\u00df", "sie", "oft", "sich", "aus\u00b7ge\u00b7dehnt", "zu", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PRF", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestreiten dreiste Pessimisten blo\u00df.", "tokens": ["Be\u00b7strei\u00b7ten", "dreis\u00b7te", "Pes\u00b7si\u00b7mis\u00b7ten", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hab' auch ich in dieser W\u00e4lder Scho\u00df,", "tokens": ["So", "hab'", "auch", "ich", "in", "die\u00b7ser", "W\u00e4l\u00b7der", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich's zu leben lohnt, gar oft empfunden,", "tokens": ["Da\u00df", "sich's", "zu", "le\u00b7ben", "lohnt", ",", "gar", "oft", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn ich erst zur Stadt mich heimgefunden,", "tokens": ["Doch", "wenn", "ich", "erst", "zur", "Stadt", "mich", "heim\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird mir ein Gl\u00fcck zu teil, nicht minder gro\u00df.", "tokens": ["Wird", "mir", "ein", "Gl\u00fcck", "zu", "teil", ",", "nicht", "min\u00b7der", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie wird mir nach dem h\u00e4uslich heitern Mahl", "tokens": ["Wie", "wird", "mir", "nach", "dem", "h\u00e4us\u00b7lich", "hei\u00b7tern", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am stillen Abend die Zigarre schmecken", "tokens": ["Am", "stil\u00b7len", "A\u00b7bend", "die", "Zi\u00b7gar\u00b7re", "schme\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei der gewohnten Lampe trautem Strahl!", "tokens": ["Bei", "der", "ge\u00b7wohn\u00b7ten", "Lam\u00b7pe", "trau\u00b7tem", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dann \u2013 im eignen Bett mich auszustrecken,", "tokens": ["Und", "dann", "\u2013", "im", "eig\u00b7nen", "Bett", "mich", "aus\u00b7zu\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht f\u00fcrchtend mehr, es werde der Choral", "tokens": ["Nicht", "f\u00fcrch\u00b7tend", "mehr", ",", "es", "wer\u00b7de", "der", "Cho\u00b7ral"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADV", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Kurmusik zu fr\u00fche schon mich wecken!", "tokens": ["Der", "Kur\u00b7mu\u00b7sik", "zu", "fr\u00fc\u00b7he", "schon", "mich", "we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im Leben gibt's, behauptet ein Franzos,", "tokens": ["Im", "Le\u00b7ben", "gibt's", ",", "be\u00b7haup\u00b7tet", "ein", "Fran\u00b7zos", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sch\