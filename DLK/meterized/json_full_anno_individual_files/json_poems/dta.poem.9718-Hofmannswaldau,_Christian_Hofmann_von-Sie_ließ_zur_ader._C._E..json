{"dta.poem.9718": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie lie\u00df zur ader.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Doris neulich lie\u00df ihr schwartz-gebranntes blut/", "tokens": ["Als", "Do\u00b7ris", "neu\u00b7lich", "lie\u00df", "ihr", "schwartz\u00b7ge\u00b7brann\u00b7tes", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und man mir hoffnung gab: Sie w\u00fcrd itzt besser werden/", "tokens": ["Und", "man", "mir", "hoff\u00b7nung", "gab", ":", "Sie", "w\u00fcrd", "itzt", "bes\u00b7ser", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So rieff ich: nur umsonst! Sie zeigt ja selbst der erden/", "tokens": ["So", "rieff", "ich", ":", "nur", "um\u00b7sonst", "!", "Sie", "zeigt", "ja", "selbst", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df auch/ wie kranck sie ist/ das blut nicht an ihr gut.", "tokens": ["Da\u00df", "auch", "/", "wie", "kranck", "sie", "ist", "/", "das", "blut", "nicht", "an", "ihr", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "$(", "ART", "NN", "PTKNEG", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}