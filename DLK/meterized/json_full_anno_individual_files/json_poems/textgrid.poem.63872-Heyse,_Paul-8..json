{"textgrid.poem.63872": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe versucht so manchesmal mit Lieb' und Leben es leicht zu nehmen,", "tokens": ["Ich", "ha\u00b7be", "ver\u00b7sucht", "so", "man\u00b7ches\u00b7mal", "mit", "Lieb'", "und", "Le\u00b7ben", "es", "leicht", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "ADV", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+---+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zum Trost im irdischen Jammertal mit Lieb' und Leben es leicht zu nehmen.", "tokens": ["Zum", "Trost", "im", "ir\u00b7di\u00b7schen", "Jam\u00b7mer\u00b7tal", "mit", "Lieb'", "und", "Le\u00b7ben", "es", "leicht", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Wie oft verw\u00fcnscht' ich mein schweres Blut, dem Salomo's Weisheit nicht zu eigen,", "tokens": ["Wie", "oft", "ver\u00b7w\u00fcnscht'", "ich", "mein", "schwe\u00b7res", "Blut", ",", "dem", "Sa\u00b7lo\u00b7mo's", "Weis\u00b7heit", "nicht", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "ART", "NE", "NN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Der riet, weil alles eitel zumal, mit Lieb' und Leben es leicht zu nehmen.", "tokens": ["Der", "riet", ",", "weil", "al\u00b7les", "ei\u00b7tel", "zu\u00b7mal", ",", "mit", "Lieb'", "und", "Le\u00b7ben", "es", "leicht", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "ADV", "$,", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Und doch, wem Mutter Natur versagt ein sorglos unbedenklich Gewissen,", "tokens": ["Und", "doch", ",", "wem", "Mut\u00b7ter", "Na\u00b7tur", "ver\u00b7sagt", "ein", "sorg\u00b7los", "un\u00b7be\u00b7denk\u00b7lich", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "NN", "NN", "VVFIN", "ART", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Der mu\u00df verzichten zu seiner Qual, mit Lieb' und Leben es leicht zu nehmen.", "tokens": ["Der", "mu\u00df", "ver\u00b7zich\u00b7ten", "zu", "sei\u00b7ner", "Qual", ",", "mit", "Lieb'", "und", "Le\u00b7ben", "es", "leicht", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Den Dichtern freilich erlaubt die Welt, sich \u00fcber manches hinwegzusetzen,", "tokens": ["Den", "Dich\u00b7tern", "frei\u00b7lich