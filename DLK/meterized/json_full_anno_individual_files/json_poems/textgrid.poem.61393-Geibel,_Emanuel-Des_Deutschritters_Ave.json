{"textgrid.poem.61393": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des Deutschritters Ave", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbherr Ott vom B\u00fchl, nun dr\u00e4ngt die Not,", "tokens": ["\u00bb", "herr", "Ott", "vom", "B\u00fchl", ",", "nun", "dr\u00e4ngt", "die", "Not", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun zeigt, wie treu Ihr's meint!", "tokens": ["Nun", "zeigt", ",", "wie", "treu", "Ih\u00b7r's", "meint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Das Feld ist rot, und die Br\u00fcder sind tot,", "tokens": ["Das", "Feld", "ist", "rot", ",", "und", "die", "Br\u00fc\u00b7der", "sind", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hinter uns rasselt der Feind.", "tokens": ["Und", "hin\u00b7ter", "uns", "ras\u00b7selt", "der", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wohl klag' ich manch gebrochnen Speer,", "tokens": ["Wohl", "klag'", "ich", "manch", "ge\u00b7broch\u00b7nen", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch Wappenschild zerspalten;", "tokens": ["Manch", "Wap\u00b7pen\u00b7schild", "zer\u00b7spal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch schmerzt's um den heiligen Kelch mich noch mehr", "tokens": ["Doch", "schmerzt's", "um", "den", "hei\u00b7li\u00b7gen", "Kelch", "mich", "noch", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "ADV", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In meines Mantels Falten.", "tokens": ["In", "mei\u00b7nes", "Man\u00b7tels", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Schlachtfeld tranken wir alle daraus,", "tokens": ["Im", "Schlacht\u00b7feld", "tran\u00b7ken", "wir", "al\u00b7le", "da\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIS", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu s\u00fchnen uns mit Gott;", "tokens": ["Zu", "s\u00fch\u00b7nen", "uns", "mit", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll nun beim w\u00fcsten Siegesschmaus", "tokens": ["Soll", "nun", "beim", "w\u00fcs\u00b7ten", "Sie\u00b7ges\u00b7schmaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Heid' ihn schwingen zum Spott?", "tokens": ["Der", "Heid'", "ihn", "schwin\u00b7gen", "zum", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Herr Ott, und f\u00fchlt Ihr Euch stark und jung,", "tokens": ["Herr", "Ott", ",", "und", "f\u00fchlt", "Ihr", "Euch", "stark", "und", "jung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KON", "VVFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch einmal wendet das Ro\u00df,", "tokens": ["Noch", "ein\u00b7mal", "wen\u00b7det", "das", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Versucht mit scharfem Schwertesschwung", "tokens": ["Ver\u00b7sucht", "mit", "schar\u00b7fem", "Schwer\u00b7tes\u00b7schwung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal zu hemmen den Tro\u00df.", "tokens": ["Noch", "ein\u00b7mal", "zu", "hem\u00b7men", "den", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und haltet Ihr nur so lang ihn auf,", "tokens": ["Und", "hal\u00b7tet", "Ihr", "nur", "so", "lang", "ihn", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Ihr ein Ave sagt,", "tokens": ["Als", "Ihr", "ein", "A\u00b7ve", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So rettet meines Hengstes Lauf", "tokens": ["So", "ret\u00b7tet", "mei\u00b7nes", "Hengs\u00b7tes", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kelch, um den Ihr's wagt.\u00ab", "tokens": ["Den", "Kelch", ",", "um", "den", "Ih\u00b7r's", "wagt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PIS", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Herrn Otts Besinnen war nicht gro\u00df,", "tokens": ["Herrn", "Otts", "Be\u00b7sin\u00b7nen", "war", "nicht", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach: \u00bbJa\u00ab, und weiter nichts;", "tokens": ["Sprach", ":", "\u00bb", "Ja", "\u00ab", ",", "und", "wei\u00b7ter", "nichts", ";"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PTKANT", "$(", "$,", "KON", "ADV", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Meisters Ro\u00df von dannen scho\u00df", "tokens": ["Des", "Meis\u00b7ters", "Ro\u00df", "von", "dan\u00b7nen", "scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Strahl des Mondenlichts.", "tokens": ["Im", "Strahl", "des", "Mon\u00b7den\u00b7lichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und als das Kreuz auf dem Mantel wei\u00df", "tokens": ["Und", "als", "das", "Kreuz", "auf", "dem", "Man\u00b7tel", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht mehr zu kennen war,", "tokens": ["Nicht", "mehr", "zu", "ken\u00b7nen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sauste schon auf G\u00e4ulen hei\u00df", "tokens": ["Da", "saus\u00b7te", "schon", "auf", "G\u00e4u\u00b7len", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heran der Litauer Schar;", "tokens": ["He\u00b7ran", "der", "Li\u00b7tau\u00b7er", "Schar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und als der Mantel fern im Schwung", "tokens": ["Und", "als", "der", "Man\u00b7tel", "fern", "im", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur schien wie ein fliegender Schwan,", "tokens": ["Nur", "schien", "wie", "ein", "flie\u00b7gen\u00b7der", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da fielen sie den Ritter jung", "tokens": ["Da", "fie\u00b7len", "sie", "den", "Rit\u00b7ter", "jung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit grimmigen Streichen an.", "tokens": ["Mit", "grim\u00b7mi\u00b7gen", "Strei\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Die krummen Schwerter blinkten frei,", "tokens": ["Die", "krum\u00b7men", "Schwer\u00b7ter", "blink\u00b7ten", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es rasselten dumpf die Keulen,", "tokens": ["Es", "ras\u00b7sel\u00b7ten", "dumpf", "die", "Keu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dazwischen ging ihr Kampfgeschrei", "tokens": ["Da\u00b7zwi\u00b7schen", "ging", "ihr", "Kampf\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hungriger W\u00f6lfe Heulen.", "tokens": ["Wie", "hung\u00b7ri\u00b7ger", "W\u00f6l\u00b7fe", "Heu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Herr Ott vom B\u00fchl sprach: \u00bbAve Marie!\u00ab", "tokens": ["Herr", "Ott", "vom", "B\u00fchl", "sprach", ":", "\u00bb", "A\u00b7ve", "Ma\u00b7rie", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "VVFIN", "$.", "$(", "NE", "NE", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und f\u00fchrt' einen Hieb, der traf;", "tokens": ["Und", "f\u00fchrt'", "ei\u00b7nen", "Hieb", ",", "der", "traf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Hauptmann flog vom Sattel aufs Knie", "tokens": ["Der", "Haupt\u00b7mann", "flog", "vom", "Sat\u00b7tel", "aufs", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit durchgespaltnem Schlaf.", "tokens": ["Mit", "durch\u00b7ge\u00b7spalt\u00b7nem", "Schlaf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Das zweite Wort der Held dann sprach", "tokens": ["Das", "zwei\u00b7te", "Wort", "der", "Held", "dann", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hieb noch kr\u00e4ftiger schier;", "tokens": ["Und", "hieb", "noch", "kr\u00e4f\u00b7ti\u00b7ger", "schier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Bannertr\u00e4ger zusammenbrach,", "tokens": ["Der", "Ban\u00b7ner\u00b7tr\u00e4\u00b7ger", "zu\u00b7sam\u00b7men\u00b7brach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und \u00fcber ihn fiel das Panier.", "tokens": ["Und", "\u00fc\u00b7ber", "ihn", "fiel", "das", "Pa\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Und Wort um Wort und Streich um Streich,", "tokens": ["Und", "Wort", "um", "Wort", "und", "Streich", "um", "Streich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war ein tapfer Gebet:", "tokens": ["Das", "war", "ein", "tap\u00b7fer", "Ge\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei jedem Spruch lag alsogleich", "tokens": ["Bei", "je\u00b7dem", "Spruch", "lag", "al\u00b7so\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Heide dahingem\u00e4ht.", "tokens": ["Ein", "Hei\u00b7de", "da\u00b7hin\u00b7ge\u00b7m\u00e4ht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Und es klaffte dem Ritter das Stahlhemd weit,", "tokens": ["Und", "es", "klaff\u00b7te", "dem", "Rit\u00b7ter", "das", "Stahl\u00b7hemd", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und es f\u00e4rbten die Ringe sich rot,", "tokens": ["Und", "es", "f\u00e4rb\u00b7ten", "die", "Rin\u00b7ge", "sich", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Er aber ward nicht la\u00df im Streit,", "tokens": ["Er", "a\u00b7ber", "ward", "nicht", "la\u00df", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder Schlag war Tod.", "tokens": ["Und", "je\u00b7der", "Schlag", "war", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und es barst sein Schild, und es sank sein Pferd,", "tokens": ["Und", "es", "barst", "sein", "Schild", ",", "und", "es", "sank", "sein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da k\u00e4mpft' er fort zu Fu\u00df;", "tokens": ["Da", "k\u00e4mpft'", "er", "fort", "zu", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit beiden H\u00e4nden schwang er das Schwe