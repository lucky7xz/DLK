{"textgrid.poem.67285": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl oftmal, wenn ich locke, kommt geflogen,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl oftmal, wenn ich locke, kommt geflogen,", "tokens": ["Wohl", "oft\u00b7mal", ",", "wenn", "ich", "lo\u00b7cke", ",", "kommt", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie's Turtelt\u00e4ubchen mir durch's Fenster schwebt,", "tokens": ["Wie's", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "mir", "durch's", "Fens\u00b7ter", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Mus' in s\u00fc\u00dfer Liebe mir gewogen;", "tokens": ["Die", "Mus'", "in", "s\u00fc\u00b7\u00dfer", "Lie\u00b7be", "mir", "ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und folgsam meiner Leyer Sait' erbebt;", "tokens": ["Und", "folg\u00b7sam", "mei\u00b7ner", "Le\u00b7yer", "Sait'", "er\u00b7bebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht mein ist, ihren Lippen ist entsogen", "tokens": ["Nicht", "mein", "ist", ",", "ih\u00b7ren", "Lip\u00b7pen", "ist", "ent\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "VAFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Sang, der sich mit meiner Stimm' erhebt,", "tokens": ["Der", "Sang", ",", "der", "sich", "mit", "mei\u00b7ner", "Stimm'", "er\u00b7hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er schweift umher, und wohl will ich mich h\u00fcten,", "tokens": ["Er", "schweift", "um\u00b7her", ",", "und", "wohl", "will", "ich", "mich", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem freien Flug als Steurer zu gebieten.", "tokens": ["Dem", "frei\u00b7en", "Flug", "als", "Steu\u00b7rer", "zu", "ge\u00b7bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch G\u00f6tterm\u00e4dchen hat auch ihre Launen,", "tokens": ["Doch", "G\u00f6t\u00b7ter\u00b7m\u00e4d\u00b7chen", "hat", "auch", "ih\u00b7re", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir weigert meine Bitte Jahre lang", "tokens": ["Mir", "wei\u00b7gert", "mei\u00b7ne", "Bit\u00b7te", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schon, vertrau' ich's euch ihr werdet staunen,", "tokens": ["Sie", "schon", ",", "ver\u00b7trau'", "ich's", "euch", "ihr", "wer\u00b7det", "stau\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VVFIN", "PIS", "PPER", "PPER", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn mir geb\u00fchrt Gew\u00e4hrung und ihr Dank;", "tokens": ["Denn", "mir", "ge\u00b7b\u00fchrt", "Ge\u00b7w\u00e4h\u00b7rung", "und", "ihr", "Dank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich fleh' umsonst, sie woll' in's Ohr mir raunen,", "tokens": ["Ich", "fleh'", "um\u00b7sonst", ",", "sie", "woll'", "in's", "Ohr", "mir", "rau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ohne sie dem S\u00e4nger nie gelang,", "tokens": ["Was", "oh\u00b7ne", "sie", "dem", "S\u00e4n\u00b7ger", "nie", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und zwar f\u00fcr Dich, der ich als Ritter diene,", "tokens": ["Und", "zwar", "f\u00fcr", "Dich", ",", "der", "ich", "als", "Rit\u00b7ter", "die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die liebt, und wie! der alte Ohm, Nandine!", "tokens": ["Die", "liebt", ",", "und", "wie", "!", "der", "al\u00b7te", "Ohm", ",", "Nan\u00b7di\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "PWAV", "$.", "ART", "ADJA", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In ihre Grillen mu\u00df ich wohl mich f\u00fcgen,", "tokens": ["In", "ih\u00b7re", "Gril\u00b7len", "mu\u00df", "ich", "wohl", "mich", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sch\u00f6pfen, was im eignen Born mir quillt,", "tokens": ["Und", "sch\u00f6p\u00b7fen", ",", "was", "im", "eig\u00b7nen", "Born", "mir", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wird ein schlichtes Spr\u00fcchlein Dir gen\u00fcgen,", "tokens": ["Doch", "wird", "ein", "schlich\u00b7tes", "Spr\u00fcch\u00b7lein", "Dir", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ohne Muse nun dies Bl\u00e4ttchen f\u00fcllt? \u2013 \u2013", "tokens": ["Das", "oh\u00b7ne", "Mu\u00b7se", "nun", "dies", "Bl\u00e4tt\u00b7chen", "f\u00fcllt", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "APPR", "NN", "ADV", "PDS", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da schwebt Sie her! In ihren Himmelsz\u00fcgen", "tokens": ["Da", "schwebt", "Sie", "her", "!", "In", "ih\u00b7ren", "Him\u00b7mels\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Seherblick das R\u00e4thsel sich enth\u00fcllt,", "tokens": ["Dem", "Se\u00b7her\u00b7blick", "das", "R\u00e4th\u00b7sel", "sich", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie wollte zwischen Dir und mir die Dritte", "tokens": ["Sie", "woll\u00b7te", "zwi\u00b7schen", "Dir", "und", "mir", "die", "Drit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht seyn, drum blieb sie spr\u00f6de meiner Bitte.", "tokens": ["Nicht", "seyn", ",", "drum", "blieb", "sie", "spr\u00f6\u00b7de", "mei\u00b7ner", "Bit\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "$,", "PAV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die lose Nymphe spottet, la\u00df sie scherzen,", "tokens": ["Die", "lo\u00b7se", "Nym\u00b7phe", "spot\u00b7tet", ",", "la\u00df", "sie", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich dr\u00fcck' indessen Deine zarte Hand", "tokens": ["Ich", "dr\u00fcck", "in\u00b7des\u00b7sen", "Dei\u00b7ne", "zar\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An meine Lipp' und warm wallt mir im Herzen", "tokens": ["An", "mei\u00b7ne", "Lipp'", "und", "warm", "wallt", "mir", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freude Dank f\u00fcr unser Sippschaftsband.", "tokens": ["Der", "Freu\u00b7de", "Dank", "f\u00fcr", "un\u00b7ser", "Sipp\u00b7schafts\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, aber nicht vermag ich's zu verschmerzen,", "tokens": ["Ach", ",", "a\u00b7ber", "nicht", "ver\u00b7mag", "ich's", "zu", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PTKNEG", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df zwischen uns sich dehnt ein weites Land!", "tokens": ["Da\u00df", "zwi\u00b7schen", "uns", "sich", "dehnt", "ein", "wei\u00b7tes", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wollst oft verg\u00f6nnen Deiner Engelmiene", "tokens": ["Wollst", "oft", "ver\u00b7g\u00f6n\u00b7nen", "Dei\u00b7ner", "En\u00b7gel\u00b7mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Holdseligen Genu\u00df mir, o Nandine!", "tokens": ["Hold\u00b7se\u00b7li\u00b7gen", "Ge\u00b7nu\u00df", "mir", ",", "o", "Nan\u00b7di\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "FM", "NE", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wohl oftmal, wenn ich locke, kommt geflogen,", "tokens": ["Wohl", "oft\u00b7mal", ",", "wenn", "ich", "lo\u00b7cke", ",", "kommt", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie's Turtelt\u00e4ubchen mir durch's Fenster schwebt,", "tokens": ["Wie's", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "mir", "durch's", "Fens\u00b7ter", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Mus' in s\u00fc\u00dfer Liebe mir gewogen;", "tokens": ["Die", "Mus'", "in", "s\u00fc\u00b7\u00dfer", "Lie\u00b7be", "mir", "ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und folgsam meiner Leye