{"dta.poem.6605": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer die Materie besieht und \u00fcberlegt,", "tokens": ["Wer", "die", "Ma\u00b7te\u00b7rie", "be\u00b7sieht", "und", "\u00fc\u00b7ber\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie sey geformt, sie sey bewegt;", "tokens": ["Sie", "sey", "ge\u00b7formt", ",", "sie", "sey", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So kan, wenn wir es wol ergr\u00fcnden,", "tokens": ["So", "kan", ",", "wenn", "wir", "es", "wol", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Theilchen, die auch noch so klein,", "tokens": ["In", "Theil\u00b7chen", ",", "die", "auch", "noch", "so", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mannigfalt gedehnt und ausgespannet seyn,", "tokens": ["So", "man\u00b7nig\u00b7falt", "ge\u00b7dehnt", "und", "aus\u00b7ge\u00b7span\u00b7net", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man etwas c\u00f6rperliches finden.", "tokens": ["Man", "et\u00b7was", "c\u00f6r\u00b7per\u00b7li\u00b7ches", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}