{"textgrid.poem.50767": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "15", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Fischlein in dem Netz", "tokens": ["Wie", "ein", "Fisc\u00b7hlein", "in", "dem", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Dom mich eingefangen,", "tokens": ["Hat", "der", "Dom", "mich", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da bin ich festgebannt \u2013", "tokens": ["Und", "da", "bin", "ich", "fest\u00b7ge\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum bin ich hingegangen?", "tokens": ["Wa\u00b7rum", "bin", "ich", "hin\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! wie unter K\u00fcrbisbl\u00fcten", "tokens": ["Ach", "!", "wie", "un\u00b7ter", "K\u00fcr\u00b7bis\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgenfeucht ein R\u00f6slein blitzt:", "tokens": ["Mor\u00b7gen\u00b7feucht", "ein", "R\u00f6s\u00b7lein", "blitzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwischen breiten B\u00fcrgersfrauen", "tokens": ["Zwi\u00b7schen", "brei\u00b7ten", "B\u00fcr\u00b7gers\u00b7frau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dort mein feines Liebchen sitzt!", "tokens": ["Dort", "mein", "fei\u00b7nes", "Lieb\u00b7chen", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gemeinde schl\u00e4ft und schnarcht,", "tokens": ["Die", "Ge\u00b7mein\u00b7de", "schl\u00e4ft", "und", "schnarcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Laub im Walde rauschet,", "tokens": ["Wie", "das", "Laub", "im", "Wal\u00b7de", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Bettler an der T\u00fcr", "tokens": ["Und", "der", "Bett\u00b7ler", "an", "der", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein R\u00e4uber auf sie lauschet.", "tokens": ["Wie", "ein", "R\u00e4u\u00b7ber", "auf", "sie", "lau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ein freundlich Wiesenb\u00e4chlein", "tokens": ["Doch", "ein", "freund\u00b7lich", "Wie\u00b7sen\u00b7b\u00e4ch\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Murmelnd durchs Geb\u00fcsche flieht:", "tokens": ["Mur\u00b7melnd", "durchs", "Ge\u00b7b\u00fc\u00b7sche", "flieht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So die lange, d\u00fcnne Predigt", "tokens": ["So", "die", "lan\u00b7ge", ",", "d\u00fcn\u00b7ne", "Pre\u00b7digt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schl\u00e4ngelnd um die Pfeiler zieht!", "tokens": ["Schl\u00e4n\u00b7gelnd", "um", "die", "Pfei\u00b7ler", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eichenb\u00e4ume, alt und schlank,", "tokens": ["Ei\u00b7chen\u00b7b\u00e4u\u00b7me", ",", "alt", "und", "schlank", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All die gotischen Pfeiler ragen,", "tokens": ["All", "die", "go\u00b7ti\u00b7schen", "Pfei\u00b7ler", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hoch ein zierlich Bl\u00e4tterdach", "tokens": ["Hoch", "ein", "zier\u00b7lich", "Bl\u00e4t\u00b7ter\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre breiten \u00c4ste tragen;", "tokens": ["Ih\u00b7re", "brei\u00b7ten", "\u00c4s\u00b7te", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drunter durch spielt hin und wieder", "tokens": ["Drun\u00b7ter", "durch", "spielt", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den D\u00e4mmer der Sonnenschein \u2013", "tokens": ["In", "den", "D\u00e4m\u00b7mer", "der", "Son\u00b7nen\u00b7schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Wachend sind in dieser Stille", "tokens": ["Wa\u00b7chend", "sind", "in", "die\u00b7ser", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur mein Lieb und ich allein.", "tokens": ["Nur", "mein", "Lieb", "und", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwischen uns spinnt sich ein Netz", "tokens": ["Zwi\u00b7schen", "uns", "spinnt", "sich", "ein", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Buntgef\u00e4rbter Sonnenstrahlen,", "tokens": ["Bunt\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die den Taufstein mittendrin", "tokens": ["Die", "den", "Tauf\u00b7stein", "mit\u00b7ten\u00b7drin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feenhaft ganz \u00fcbermalen.", "tokens": ["Fe\u00b7en\u00b7haft", "ganz", "\u00fc\u00b7berm\u00b7a\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosenketten, Liebesg\u00f6tter", "tokens": ["Ro\u00b7sen\u00b7ket\u00b7ten", ",", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flattern um den alten Knauf,", "tokens": ["Flat\u00b7tern", "um", "den", "al\u00b7ten", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darob wacht in unsren Herzen", "tokens": ["Da\u00b7rob", "wacht", "in", "un\u00b7sren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine hei\u00dfe Sehnsucht auf!", "tokens": ["Ei\u00b7ne", "hei\u00b7\u00dfe", "Sehn\u00b7sucht", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weit hinaus, ins Morgenland,", "tokens": ["Weit", "hin\u00b7aus", ",", "ins", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm, mein Schatz, und la\u00df uns fliehen!", "tokens": ["Komm", ",", "mein", "Schatz", ",", "und", "la\u00df", "uns", "flie\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Palmen schwanken am Meer,", "tokens": ["Wo", "die", "Pal\u00b7men", "schwan\u00b7ken", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Rosen hoch wie Feuer gl\u00fchen,", "tokens": ["Ro\u00b7sen", "hoch", "wie", "Feu\u00b7er", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flutend um die gro\u00dfe Sonne", "tokens": ["Flu\u00b7tend", "um", "die", "gro\u00b7\u00dfe", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Grundlos tief die Himmel blaun:", "tokens": ["Grund\u00b7los", "tief", "die", "Him\u00b7mel", "blaun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Angesichts der freien Wogen", "tokens": ["An\u00b7ge\u00b7sichts", "der", "frei\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei und ewig uns zu traun!", "tokens": ["Frei", "und", "e\u00b7wig", "uns", "z