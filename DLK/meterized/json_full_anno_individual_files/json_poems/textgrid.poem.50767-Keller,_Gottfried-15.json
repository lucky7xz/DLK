aic.tetra"}, "line.3": {"text": "Die den Taufstein mittendrin", "tokens": ["Die", "den", "Tauf\u00b7stein", "mit\u00b7ten\u00b7drin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feenhaft ganz \u00fcbermalen.", "tokens": ["Fe\u00b7en\u00b7haft", "ganz", "\u00fc\u00b7berm\u00b7a\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosenketten, Liebesg\u00f6tter", "tokens": ["Ro\u00b7sen\u00b7ket\u00b7ten", ",", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flattern um den alten Knauf,", "tokens": ["Flat\u00b7tern", "um", "den", "al\u00b7ten", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darob wacht in unsren Herzen", "tokens": ["Da\u00b7rob", "wacht", "in", "un\u00b7sren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine hei\u00dfe Sehnsucht auf!", "tokens": ["Ei\u00b7ne", "hei\u00b7\u00dfe", "Sehn\u00b7sucht", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Weit hinaus, ins Morgenland,", "tokens": ["Weit", "hin\u00b7aus", ",", "ins", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm, mein Schatz, und la\u00df uns fliehen!", "tokens": ["Komm", ",", "mein", "Schatz", ",", "und", "la\u00df", "uns", "flie\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Palmen schwanken am Meer,", "tokens": ["Wo", "die", "Pal\u00b7men", "schwan\u00b7ken", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Rosen hoch wie Feuer gl\u00fchen,", "tokens": ["Ro\u00b7sen", "hoch", "wie", "Feu\u00b7er", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flutend um die gro\u00dfe Sonne", "tokens": ["Flu\u00b7tend", "um", "die", "gro\u00b7\u00dfe", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Grundlos tief die Himmel blaun:", "tokens": ["Grund\u00b7los", "tief", "die", "Him\u00b7mel", "blaun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Angesichts der freien Wogen", "tokens": ["An\u00b7ge\u00b7sichts", "der", "frei\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei und ewig uns zu traun!", "tokens": ["Frei", "und", "e\u00b7wig", "uns", "zu", "traun", "!"], "token_info": ["word", "word", "word", "wo