{"dta.poem.9551": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich ihm zum neuen jahre schenckete.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel f\u00e4ngt dis jahr mit lauter thr\u00e4nen an/", "tokens": ["Der", "him\u00b7mel", "f\u00e4ngt", "dis", "jahr", "mit", "lau\u00b7ter", "thr\u00e4\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeigt der erden nichts als nebel/ dampff und regen:", "tokens": ["Und", "zeigt", "der", "er\u00b7den", "nichts", "als", "ne\u00b7bel", "/", "dampff", "und", "re\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIS", "KOKOM", "NE", "$(", "PAV", "KON", "ADJA", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch Leonore hat das widerspiel gethan/", "tokens": ["Doch", "Le\u00b7o\u00b7no\u00b7re", "hat", "das", "wi\u00b7der\u00b7spiel", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PDS", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will auff diesen tag die st\u00fcrme niederlegen.", "tokens": ["Und", "will", "auff", "die\u00b7sen", "tag", "die", "st\u00fcr\u00b7me", "nie\u00b7der\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich hatte sie erz\u00fcrnt/ und machte mich bereit/", "tokens": ["Ich", "hat\u00b7te", "sie", "er\u00b7z\u00fcrnt", "/", "und", "mach\u00b7te", "mich", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die straffen ihrer hand mit demuth zu empfangen:", "tokens": ["Die", "straf\u00b7fen", "ih\u00b7rer", "hand", "mit", "de\u00b7muth", "zu", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "So kehrt sie nacht in tag/ und leid in fr\u00f6ligkeit/", "tokens": ["So", "kehrt", "sie", "nacht", "in", "tag", "/", "und", "leid", "in", "fr\u00f6\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$(", "KON", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und streicht die blasse furcht von meinen todten-wangen.", "tokens": ["Und", "streicht", "die", "blas\u00b7se", "furcht", "von", "mei\u00b7nen", "tod\u00b7ten\u00b7wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor ruthen schickt sie mir sich selber und ihr hertz;", "tokens": ["Vor", "ru\u00b7then", "schickt", "sie", "mir", "sich", "sel\u00b7ber", "und", "ihr", "hertz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "PPER", "PRF", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Was k\u00f6nt\u2019 ich bessers wohl von einem richter hoffen?", "tokens": ["Was", "k\u00f6nt'", "ich", "bes\u00b7sers", "wohl", "von", "ei\u00b7nem", "rich\u00b7ter", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein geist war voller angst/ die seele voller schmertz/", "tokens": ["Mein", "geist", "war", "vol\u00b7ler", "angst", "/", "die", "see\u00b7le", "vol\u00b7ler", "schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$(", "ART", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun steht mir unverhofft mein gantzer himmel offen.", "tokens": ["Nun", "steht", "mir", "un\u00b7ver\u00b7hofft", "mein", "gant\u00b7zer", "him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Begl\u00fccktes neues jahr! wo brieffe b\u00fcrgen seyn/", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "neu\u00b7es", "jahr", "!", "wo", "brief\u00b7fe", "b\u00fcr\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$.", "PWAV", "VVFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und die gedancken so wie ihre worte klingen.", "tokens": ["Und", "die", "ge\u00b7dan\u00b7cken", "so", "wie", "ih\u00b7re", "wor\u00b7te", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach aber sch\u00f6nstes bild! du zeigest sonnen-schein/", "tokens": ["Ach", "a\u00b7ber", "sch\u00f6ns\u00b7tes", "bild", "!", "du", "zei\u00b7gest", "son\u00b7nen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJA", "NN", "$.", "PPER", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wilst mir in der that nur tr\u00fcben regen bringen.", "tokens": ["Und", "wilst", "mir", "in", "der", "that", "nur", "tr\u00fc\u00b7ben", "re\u00b7gen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "VVFIN", "ADV", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du schenckest was du schon an andre l\u00e4ngst verschenckt/", "tokens": ["Du", "schen\u00b7ckest", "was", "du", "schon", "an", "and\u00b7re", "l\u00e4ngst", "ver\u00b7schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "ADV", "APPR", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und giebst mir/ was du doch mit rechte nicht kanst geben.", "tokens": ["Und", "giebst", "mir", "/", "was", "du", "doch", "mit", "rech\u00b7te", "nicht", "kanst", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "PWS", "PPER", "ADV", "APPR", "ADJA", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.19": {"text": "Wie will dein Gelidor/ den du hierdurch gekr\u00e4nckt/", "tokens": ["Wie", "will", "dein", "Ge\u00b7li\u00b7dor", "/", "den", "du", "hier\u00b7durch", "ge\u00b7kr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "$(", "ART", "PPER", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nun k\u00fcnfftig ohne dich und sonder hertze leben?", "tokens": ["Nun", "k\u00fcnff\u00b7tig", "oh\u00b7ne", "dich", "und", "son\u00b7der", "hert\u00b7ze", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn deines nimst du ihm/ und seines hast du schon.", "tokens": ["Denn", "dei\u00b7nes", "nimst", "du", "ihm", "/", "und", "sei\u00b7nes", "hast", "du", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PPER", "PPER", "$(", "KON", "PPOSAT", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch was bek\u00fcm\u0303ern mich doch fremde qual und sehmertzen?", "tokens": ["Doch", "was", "be\u00b7k\u00fcm\u0303ern", "mich", "doch", "frem\u00b7de", "qual", "und", "seh\u00b7mert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "ADJA", "NN", "KON", "VVINF", "$."], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Was giebet mir hier Rom? was Gelidor vor lohn/", "tokens": ["Was", "gie\u00b7bet", "mir", "hier", "Rom", "?", "was", "Ge\u00b7li\u00b7dor", "vor", "lohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "NE", "$.", "PWS", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df ich mein gl\u00fccke soll zu seiner lust verschertzen?", "tokens": ["Da\u00df", "ich", "mein", "gl\u00fc\u00b7cke", "soll", "zu", "sei\u00b7ner", "lust", "ver\u00b7schert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer schenckt/ der schaue zu/ wie viel er halten kan.", "tokens": ["Wer", "schenckt", "/", "der", "schau\u00b7e", "zu", "/", "wie", "viel", "er", "hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ART", "ADJA", "PTKZU", "$(", "PWAV", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich nehme/ was du mir so willig angetragen.", "tokens": ["Ich", "neh\u00b7me", "/", "was", "du", "mir", "so", "wil\u00b7lig", "an\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du kanst nun/ wilstu gleich/ was deine wahl gethan/", "tokens": ["Du", "kanst", "nun", "/", "wils\u00b7tu", "gleich", "/", "was", "dei\u00b7ne", "wahl", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$(", "VMFIN", "ADV", "$(", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nicht ohne schand und schmach gar wohl zur\u00fccke schlagen.", "tokens": ["Nicht", "oh\u00b7ne", "schand", "und", "schmach", "gar", "wohl", "zu\u00b7r\u00fc\u00b7cke", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Cupido lehne mir die waffen deiner hand/", "tokens": ["Cu\u00b7pi\u00b7do", "leh\u00b7ne", "mir", "die", "waf\u00b7fen", "dei\u00b7ner", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Befe\u00dfle dieses hertz mit ketten von rubinen/", "tokens": ["Be\u00b7fe\u00df\u00b7le", "die\u00b7ses", "hertz", "mit", "ket\u00b7ten", "von", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "VVFIN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und schreib mit g\u00f6ldner schrifft an seine kammer-wand:", "tokens": ["Und", "schreib", "mit", "g\u00f6ld\u00b7ner", "schrifft", "an", "sei\u00b7ne", "kam\u00b7mer\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df niemand in der welt kan zweyen herren dienen.", "tokens": ["Da\u00df", "nie\u00b7mand", "in", "der", "welt", "kan", "zwe\u00b7yen", "her\u00b7ren", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VMFIN", "VVINF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}