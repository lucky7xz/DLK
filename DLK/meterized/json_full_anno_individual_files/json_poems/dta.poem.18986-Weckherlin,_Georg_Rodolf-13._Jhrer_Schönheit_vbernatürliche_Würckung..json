{"dta.poem.18986": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "13.  \n  Jhrer Sch\u00f6nheit vbernat\u00fcrliche  \n W\u00fcrckung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah/ als jhr gesicht/ der Morgenr\u00f6htin gleich/", "tokens": ["Ich", "sah", "/", "als", "jhr", "ge\u00b7sicht", "/", "der", "Mor\u00b7gen\u00b7r\u00f6h\u00b7tin", "gleich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVPP", "$(", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als jhre zwilling brust/ so wei\u00df als schnee zu sehen/", "tokens": ["Als", "jhre", "zwil\u00b7ling", "brust", "/", "so", "wei\u00df", "als", "schnee", "zu", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VVFIN", "KOKOM", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Vnd jhren glatten hals vil taussent ringlein reich", "tokens": ["Vnd", "jhren", "glat\u00b7ten", "hals", "vil", "taus\u00b7sent", "rin\u00b7glein", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Von jhrem krausen gold vmbgaben/ Sie auff-", "tokens": ["Von", "jhrem", "krau\u00b7sen", "gold", "vmb\u00b7ga\u00b7ben", "/", "Sie", "auf\u00b7f"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$(", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "stehen.", "tokens": ["ste\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Auffstehen sah ich Sie/ so kunstlo\u00df als liebreich/", "tokens": ["Auffs\u00b7te\u00b7hen", "sah", "ich", "Sie", "/", "so", "kunst\u00b7lo\u00df", "als", "lieb\u00b7reich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$(", "ADV", "ADJD", "KOKOM", "ADJD", "$("], "meter": "+--+-+-++-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit solcher sch\u00f6nheit schatz ohn m\u00fch/ ohn sorg", "tokens": ["Mit", "sol\u00b7cher", "sch\u00f6n\u00b7heit", "schatz", "ohn", "m\u00fch", "/", "ohn", "sorg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "APPR", "ADJD", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "versehen/", "tokens": ["ver\u00b7se\u00b7hen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da\u00df Sie so sch\u00f6n/ so fr\u00fch/ in der Lieb K\u00f6nigreich", "tokens": ["Da\u00df", "Sie", "so", "sch\u00f6n", "/", "so", "fr\u00fch", "/", "in", "der", "Lieb", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$(", "ADV", "ADJD", "$(", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kont andern vmb mittag gezieret weit vorgehen.", "tokens": ["Kont", "an\u00b7dern", "vmb", "mit\u00b7tag", "ge\u00b7zie\u00b7ret", "weit", "vor\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVPP", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Al\u00dfbald ich Sie ersah/ O wunder/ schryh ich bald/", "tokens": ["Al\u00df\u00b7bald", "ich", "Sie", "er\u00b7sah", "/", "O", "wun\u00b7der", "/", "schryh", "ich", "bald", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "NE", "NN", "$(", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was kan von diser brunst vnd disem band mich", "tokens": ["Was", "kan", "von", "di\u00b7ser", "brunst", "vnd", "di\u00b7sem", "band", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PDAT", "NN", "KON", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "freyhen/", "tokens": ["frey\u00b7hen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wan g\u00f6tlich Sie an macht/ vn\u0303 g\u00f6tlich an gestalt;", "tokens": ["Wan", "g\u00f6t\u00b7lich", "Sie", "an", "macht", "/", "v\u00f1", "g\u00f6t\u00b7lich", "an", "ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "VVFIN", "$(", "ADV", "ADJD", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vnd wan/ als sie mir wolt jhr angesicht verleyhen/", "tokens": ["Vnd", "wan", "/", "als", "sie", "mir", "wolt", "jhr", "an\u00b7ge\u00b7sicht", "ver\u00b7ley\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$(", "KOUS", "PPER", "PPER", "VMFIN", "PPER", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je bloser jhre brust/ je st\u00e4rcker jhr gewalt/", "tokens": ["Je", "blo\u00b7ser", "jhre", "brust", "/", "je", "st\u00e4r\u00b7cker", "jhr", "ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$(", "ADV", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Je freyher jhre haar/ je mehr sie mich entfreyhen.", "tokens": ["Je", "frey\u00b7her", "jhre", "haar", "/", "je", "mehr", "sie", "mich", "ent\u00b7frey\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}