{"dta.poem.6405": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was nun die Kunst auch immer thut,", "tokens": ["Was", "nun", "die", "Kunst", "auch", "im\u00b7mer", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mancherley Geschmack noch immer zu verbessern,", "tokens": ["So", "man\u00b7cher\u00b7ley", "Ge\u00b7schmack", "noch", "im\u00b7mer", "zu", "ver\u00b7bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sammt allem, was man uns daher bringt, wo die Gluht", "tokens": ["Sammt", "al\u00b7lem", ",", "was", "man", "uns", "da\u00b7her", "bringt", ",", "wo", "die", "Gluht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PIS", "PRF", "PAV", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Der Sonnen sich erhebt, auch da, woselbst sie ruht,", "tokens": ["Der", "Son\u00b7nen", "sich", "er\u00b7hebt", ",", "auch", "da", ",", "wo\u00b7selbst", "sie", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Um die Empfindlichkeit im Schmecken zu vergr\u00f6ssern,", "tokens": ["Um", "die", "Emp\u00b7find\u00b7lich\u00b7keit", "im", "Schme\u00b7cken", "zu", "ver\u00b7gr\u00f6s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So Pfeffer, als Muscat, Caneel und N\u00e4gelein,", "tokens": ["So", "Pfef\u00b7fer", ",", "als", "Mus\u00b7cat", ",", "Ca\u00b7neel", "und", "N\u00e4\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Die von den Schiffern uns, durch mancherley Gefahr", "tokens": ["Die", "von", "den", "Schif\u00b7fern", "uns", ",", "durch", "man\u00b7cher\u00b7ley", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ungest\u00fcmen Wind\u2019 und Wellen,", "tokens": ["Der", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Wind'", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gebracht und zugef\u00fchret seyn;", "tokens": ["Ge\u00b7bracht", "und", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was hat es sonst der Zung annehmlichs vorzustellen,", "tokens": ["Was", "hat", "es", "sonst", "der", "Zung", "an\u00b7nehm\u00b7lichs", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ART", "NN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Derselben sanfft zu schmeicheln, ja so gar", "tokens": ["Der\u00b7sel\u00b7ben", "sanfft", "zu", "schmei\u00b7cheln", ",", "ja", "so", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zu reitzen? als nur blos die heisse Krafft allein,", "tokens": ["Zu", "reit\u00b7zen", "?", "als", "nur", "blos", "die", "heis\u00b7se", "Krafft", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Womit sie Indien und Afriea beschencken.", "tokens": ["Wo\u00b7mit", "sie", "In\u00b7di\u00b7en", "und", "Af\u00b7ri\u00b7ea", "be\u00b7schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus kleinen C\u00f6rperlein, die fertig sich zu lencken,", "tokens": ["Aus", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein", ",", "die", "fer\u00b7tig", "sich", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bestehn dieselbigen, die durch der Sonnen Hitze", "tokens": ["Be\u00b7stehn", "die\u00b7sel\u00b7bi\u00b7gen", ",", "die", "durch", "der", "Son\u00b7nen", "Hit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-----+-+-+-", "measure": "dactylic.init"}, "line.16": {"text": "Ger\u00fcndet und zugleich versehn mit mancher Spitze,", "tokens": ["Ge\u00b7r\u00fcn\u00b7det", "und", "zu\u00b7gleich", "ver\u00b7sehn", "mit", "man\u00b7cher", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wodurch sie Zung und Gaum durchdringen, k\u00fctzeln, stechen,", "tokens": ["Wo\u00b7durch", "sie", "Zung", "und", "Gaum", "durch\u00b7drin\u00b7gen", ",", "k\u00fct\u00b7zeln", ",", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und machen, da\u00df gar offt, um schn\u00f6der Wollust Tand,", "tokens": ["Und", "ma\u00b7chen", ",", "da\u00df", "gar", "offt", ",", "um", "schn\u00f6\u00b7der", "Wol\u00b7lust", "Tand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "ADV", "ADV", "$,", "KOUI", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wir der Gesundheit gantz vergessen und sie schw\u00e4chen.", "tokens": ["Wir", "der", "Ge\u00b7sund\u00b7heit", "gantz", "ver\u00b7ges\u00b7sen", "und", "sie", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVPP", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}