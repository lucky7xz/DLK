{"textgrid.poem.64527": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Gallier, der gallisch nur verstand,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gallier, der gallisch nur verstand,", "tokens": ["Ein", "Gal\u00b7lier", ",", "der", "gal\u00b7lisch", "nur", "ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und das allein reich, stark und zierlich fand,", "tokens": ["Und", "das", "al\u00b7lein", "reich", ",", "stark", "und", "zier\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(das Deutsche hat er stets durch schalen Spott entehrt,", "tokens": ["(", "das", "Deut\u00b7sche", "hat", "er", "stets", "durch", "scha\u00b7len", "Spott", "ent\u00b7ehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihn f\u00fcr dies Verdienst ein deutscher Hof ern\u00e4hrt)", "tokens": ["Weil", "ihn", "f\u00fcr", "dies", "Ver\u00b7dienst", "ein", "deut\u00b7scher", "Hof", "er\u00b7n\u00e4hrt", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDS", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den bat ich: Nennt mir doch auf gallisch", "tokens": ["Den", "bat", "ich", ":", "Nennt", "mir", "doch", "auf", "gal\u00b7lisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbherr Deutscher, k\u00f6nnt ihr mich im Ernst so seltsam fragen?", "tokens": ["\u00bb", "herr", "Deut\u00b7scher", ",", "k\u00f6nnt", "ihr", "mich", "im", "Ernst", "so", "selt\u00b7sam", "fra\u00b7gen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Gallier beh\u00e4lt die griechschen T\u00f6ne.\u00ab", "tokens": ["Der", "Gal\u00b7lier", "be\u00b7h\u00e4lt", "die", "griech\u00b7schen", "T\u00f6\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nun wohl, Monsieur, wir k\u00f6nnen ", "tokens": ["Nun", "wohl", ",", "Mon\u00b7si\u00b7eur", ",", "wir", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "FM", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Gallier, der gallisch nur verstand,", "tokens": ["Ein", "Gal\u00b7lier", ",", "der", "gal\u00b7lisch", "nur", "ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "