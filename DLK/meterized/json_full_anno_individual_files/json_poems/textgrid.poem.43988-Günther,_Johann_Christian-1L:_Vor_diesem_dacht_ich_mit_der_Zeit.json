, "Wunt\u00b7sch", "mehr", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als da\u00df ich mit ihr ruhig leben", "tokens": ["Als", "da\u00df", "ich", "mit", "ihr", "ru\u00b7hig", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPOSAT", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und einmahl freudig sterben kan.", "tokens": ["Und", "ein\u00b7mahl", "freu\u00b7dig", "ster\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor diesem dacht ich mit der Zeit", "tokens": ["Vor", "die\u00b7sem", "dacht", "ich", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gro\u00df und vornehm Thier zu werden,", "tokens": ["Ein", "gro\u00df", "und", "vor\u00b7nehm", "Thier", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sucht in Kleidung und Gebehrden", "tokens": ["Ich", "sucht", "in", "Klei\u00b7dung", "und", "Ge\u00b7behr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allen einen Unterscheid;", "tokens": ["Vor", "al\u00b7len", "ei\u00b7nen", "Un\u00b7