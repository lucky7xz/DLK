word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bleibt Phillis mir nur treu ergeben,", "tokens": ["Bleibt", "Phil\u00b7lis", "mir", "nur", "treu", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ficht mich wohl kein Wuntsch mehr an,", "tokens": ["So", "ficht", "mich", "wohl", "kein", "Wunt\u00b7sch", "mehr", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als da\u00df ich mit ihr ruhig leben", "tokens": ["Als", "da\u00df", "ich", "mit", "ihr", "ru\u00b7hig", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPOSAT", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und einmahl freudig sterben kan.", "tokens": ["Und", "ein\u00b7mahl", "freu\u00b7dig", "ster\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}