: ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Womit ich oftmahls aus den Schrancken", "tokens": ["Wo\u00b7mit", "ich", "oft\u00b7mahls", "aus", "den", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gemeines Gl\u00fccks geflogen bin?", "tokens": ["Ge\u00b7mei\u00b7nes", "Gl\u00fccks", "ge\u00b7flo\u00b7gen", "bin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es reizt mich kein ber\u00fchmter Tittel,", "tokens": ["Es", "reizt", "mich", "kein", "be\u00b7r\u00fchm\u00b7ter", "Tit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es r\u00fchrt mich weder Hof noch Pracht,", "tokens": ["Es", "r\u00fchrt", "mich", "we\u00b7der", "Hof", "noch", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich finde, deucht mich, viel im Kittel,", "tokens": ["Ich", "fin\u00b7de", ",", "deucht", "mich", ",", "viel", "im", "Kit\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was kluge Seelen gl\u00fccklich macht.", "tokens": ["Was", "klu\u00b7ge", "See\u00b7len", "gl\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"]