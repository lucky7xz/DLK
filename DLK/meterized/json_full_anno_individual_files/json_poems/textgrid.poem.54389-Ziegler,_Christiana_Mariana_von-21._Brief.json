{"textgrid.poem.54389": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "21. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochwohlgebohrner Herr, gepriesner \u2013 \u2013 \u2013", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ner", "Herr", ",", "ge\u00b7pri\u00b7es\u00b7ner", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$(", "$(", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ists m\u00f6glich? spielst du noch mit Freyheit, Herz und Hand?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "spielst", "du", "noch", "mit", "Frey\u00b7heit", ",", "Herz", "und", "Hand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein, nein, das hei\u00dft kein Spiel, du tritst zum Ehstandsorden", "tokens": ["Nein", ",", "nein", ",", "das", "hei\u00dft", "kein", "Spiel", ",", "du", "tritst", "zum", "Eh\u00b7stand\u00b7sor\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PDS", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bist mit Vorbedacht ein Br\u00e4utigam geworden:", "tokens": ["Und", "bist", "mit", "Vor\u00b7be\u00b7dacht", "ein", "Br\u00e4u\u00b7ti\u00b7gam", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ART", "NE", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Wehlen ist gerecht, gerecht ich dein Entschlu\u00df;", "tokens": ["Dein", "Weh\u00b7len", "ist", "ge\u00b7recht", ",", "ge\u00b7recht", "ich", "dein", "Ent\u00b7schlu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So theile Lieb und Gunst, so theile Scherz und Ku\u00df.", "tokens": ["So", "thei\u00b7le", "Lieb", "und", "Gunst", ",", "so", "thei\u00b7le", "Scherz", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du lebst im Paradies; genie\u00df auch um der Fr\u00fcchte", "tokens": ["Du", "lebst", "im", "Pa\u00b7ra\u00b7dies", ";", "ge\u00b7nie\u00df", "auch", "um", "der", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die vielen reizend sind, doch nur ein Schaugerichte.", "tokens": ["Die", "vie\u00b7len", "rei\u00b7zend", "sind", ",", "doch", "nur", "ein", "Schau\u00b7ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie sanfte ruht dein Haupt, wie s\u00fc\u00df ist deine Rast,", "tokens": ["Wie", "sanf\u00b7te", "ruht", "dein", "Haupt", ",", "wie", "s\u00fc\u00df", "ist", "dei\u00b7ne", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nachdem du vielerley zuvor erfahren hast!", "tokens": ["Nach\u00b7dem", "du", "vie\u00b7ler\u00b7ley", "zu\u00b7vor", "er\u00b7fah\u00b7ren", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Itzt denkt das Gl\u00fcck an dich, es wird dir noch gewogen,", "tokens": ["Itzt", "denkt", "das", "Gl\u00fcck", "an", "dich", ",", "es", "wird", "dir", "noch", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und hat ein sch\u00f6nes Kind zu deiner Lust erzogen.", "tokens": ["Und", "hat", "ein", "sch\u00f6\u00b7nes", "Kind", "zu", "dei\u00b7ner", "Lust", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich kenne dich mein Freund! doch nicht dein Ehgemal", "tokens": ["Ich", "ken\u00b7ne", "dich", "mein", "Freund", "!", "doch", "nicht", "dein", "Eh\u00b7ge\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das dich anitzt vergn\u00fcgt; doch h\u00f6r ich \u00fcberall", "tokens": ["Das", "dich", "a\u00b7nitzt", "ver\u00b7gn\u00fcgt", ";", "doch", "h\u00f6r", "ich", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von ihrer Artigkeit, und angenehmen Sitten.", "tokens": ["Von", "ih\u00b7rer", "Ar\u00b7tig\u00b7keit", ",", "und", "an\u00b7ge\u00b7neh\u00b7men", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Deswegen will ich dich aus alter Freundschaft bitten;", "tokens": ["Des\u00b7we\u00b7gen", "will", "ich", "dich", "aus", "al\u00b7ter", "Freund\u00b7schaft", "bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sprich mir mit ihr zugleich in meinem Zimmer zu,", "tokens": ["Sprich", "mir", "mit", "ihr", "zu\u00b7gleich", "in", "mei\u00b7nem", "Zim\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Erzehl mir dein Geschick in ungest\u00f6rter Ruh;", "tokens": ["Er\u00b7zehl", "mir", "dein", "Ge\u00b7schick", "in", "un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich nehme theil daran, gleich wie du an dem meinen.", "tokens": ["Ich", "neh\u00b7me", "theil", "da\u00b7ran", ",", "gleich", "wie", "du", "an", "dem", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$,", "ADV", "KOKOM", "PPER", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich seh dich wahrlich noch um den von \u2013 \u2013 weinen.", "tokens": ["Ich", "seh", "dich", "wahr\u00b7lich", "noch", "um", "den", "von", "\u2013", "\u2013", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "APPR", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dein Herze brach zugleich bey meinem Ungemach,", "tokens": ["Dein", "Her\u00b7ze", "brach", "zu\u00b7gleich", "bey", "mei\u00b7nem", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er starb in deinem Arm, es kr\u00e4nkte dich mein ach!", "tokens": ["Er", "starb", "in", "dei\u00b7nem", "Arm", ",", "es", "kr\u00e4nk\u00b7te", "dich", "mein", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "ITJ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du warest sehr bem\u00fcht, mein Elend zu beklagen,", "tokens": ["Du", "wa\u00b7rest", "sehr", "be\u00b7m\u00fcht", ",", "mein", "E\u00b7lend", "zu", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wolltst zu meinem Trost die Helfte mit ertragen;", "tokens": ["Und", "wolltst", "zu", "mei\u00b7nem", "Trost", "die", "Helf\u00b7te", "mit", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Davor ich dir gewi\u00df auch noch verbunden bin.", "tokens": ["Da\u00b7vor", "ich", "dir", "ge\u00b7wi\u00df", "auch", "noch", "ver\u00b7bun\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn ein rechtschaffner Freund bleibt stets ein Hauptgewinn.", "tokens": ["Denn", "ein", "recht\u00b7schaff\u00b7ner", "Freund", "bleibt", "stets", "ein", "Haupt\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du wirst in diesem Brief kein Hochzeitlied erblicken.", "tokens": ["Du", "wirst", "in", "die\u00b7sem", "Brief", "kein", "Hoch\u00b7zeit\u00b7lied", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das Fest ist schon vorbey, drum m\u00f6cht es sich nicht schicken.", "tokens": ["Das", "Fest", "ist", "schon", "vor\u00b7bey", ",", "drum", "m\u00f6cht", "es", "sich", "nicht", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "PAV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und wenn ich ja zum Schlu\u00df noch etwas w\u00fcnschen soll:", "tokens": ["Und", "wenn", "ich", "ja", "zum", "Schlu\u00df", "noch", "et\u00b7was", "w\u00fcn\u00b7schen", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So geh dirs wie ich will, so gehts dir immer wohl.", "tokens": ["So", "geh", "dirs", "wie", "ich", "will", ",", "so", "gehts", "dir", "im\u00b7mer", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hochwohlgebohrner Herr, gepriesner \u2013 \u2013 \u2013", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ner", "Herr", ",", "ge\u00b7pri\u00b7es\u00b7ner", "\u2013", "\u2013", 