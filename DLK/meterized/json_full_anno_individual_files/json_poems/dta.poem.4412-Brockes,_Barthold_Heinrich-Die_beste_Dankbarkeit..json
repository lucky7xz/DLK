{"dta.poem.4412": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beste Dankbarkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh das lieblich-gr\u00fcne Gras,", "tokens": ["Ich", "seh", "das", "lieb\u00b7lich\u00b7gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn es vom Thau des Morgens na\u00df,", "tokens": ["Wenn", "es", "vom", "Thau", "des", "Mor\u00b7gens", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie im bunten Feuer glimmen.", "tokens": ["Als", "wie", "im", "bun\u00b7ten", "Feu\u00b7er", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh der Sonne g\u00fcld\u2019ne Gluht", "tokens": ["Ich", "seh", "der", "Son\u00b7ne", "g\u00fcld'\u00b7ne", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf reiner B\u00e4che glatter Flut,", "tokens": ["Auf", "rei\u00b7ner", "B\u00e4\u00b7che", "glat\u00b7ter", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein fliessend Silber, schwimmen.", "tokens": ["Als", "wie", "ein", "flies\u00b7send", "Sil\u00b7ber", ",", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch diesen Schein, durch dieses Gl\u00e4nzen", "tokens": ["Durch", "die\u00b7sen", "Schein", ",", "durch", "die\u00b7ses", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entreisset sich die frohe Sele", "tokens": ["E\u00b7ntreis\u00b7set", "sich", "die", "fro\u00b7he", "Se\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus ihres ird\u2019schen C\u00f6rpers H\u00f6le,", "tokens": ["Aus", "ih\u00b7res", "ird'\u00b7schen", "C\u00f6r\u00b7pers", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Aus den ihr sonst gewohnten Grenzen.", "tokens": ["Aus", "den", "ihr", "sonst", "ge\u00b7wohn\u00b7ten", "Gren\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch dieses Feuers bunten Schein", "tokens": ["Durch", "die\u00b7ses", "Feu\u00b7ers", "bun\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wird sie recht als auf einem Wagen", "tokens": ["Wird", "sie", "recht", "als", "auf", "ei\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Von Feu\u2019r und Glanz empor getragen.", "tokens": ["Von", "Feu'r", "und", "Glanz", "em\u00b7por", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sie steigt durch die so sch\u00f6nen Flammen", "tokens": ["Sie", "steigt", "durch", "die", "so", "sch\u00f6\u00b7nen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zu Dem, aus Dessen tiefem Meer", "tokens": ["Zu", "Dem", ",", "aus", "Des\u00b7sen", "tie\u00b7fem", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von Lieb\u2019 und Licht so manches Heer", "tokens": ["Von", "Lieb'", "und", "Licht", "so", "man\u00b7ches", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Von Sonnen und von Welten stammen.", "tokens": ["Von", "Son\u00b7nen", "und", "von", "Wel\u00b7ten", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sie z\u00fcndet Jhm der Andacht Kerze,", "tokens": ["Sie", "z\u00fcn\u00b7det", "Jhm", "der", "An\u00b7dacht", "Ker\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zu Seiner Ehr\u2019, in Ehrfurcht an,", "tokens": ["Zu", "Sei\u00b7ner", "Ehr'", ",", "in", "Ehr\u00b7furcht", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und, weil sie sonst nichts geben kann,", "tokens": ["Und", ",", "weil", "sie", "sonst", "nichts", "ge\u00b7ben", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So gibt sie Jhm im Dank ihr Herze.", "tokens": ["So", "gibt", "sie", "Jhm", "im", "Dank", "ihr", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}