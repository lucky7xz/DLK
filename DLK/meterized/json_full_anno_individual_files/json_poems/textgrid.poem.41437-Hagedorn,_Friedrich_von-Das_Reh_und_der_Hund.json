{"textgrid.poem.41437": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Reh und der Hund", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein zartes Reh, das gar zu sicher ruht,", "tokens": ["Ein", "zar\u00b7tes", "Reh", ",", "das", "gar", "zu", "si\u00b7cher", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhascht ein Hund, der keinen Dickigt scheute.", "tokens": ["Er\u00b7hascht", "ein", "Hund", ",", "der", "kei\u00b7nen", "Di\u00b7ckigt", "scheu\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er bei\u00dft es an, leckt das vergoss'ne Blut,", "tokens": ["Er", "bei\u00dft", "es", "an", ",", "leckt", "das", "ver\u00b7goss'\u00b7ne", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und k\u00fc\u00dft zugleich die angenehme Beute.", "tokens": ["Und", "k\u00fc\u00dft", "zu\u00b7gleich", "die", "an\u00b7ge\u00b7neh\u00b7me", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da seufzt das Wild: Welch' Mitleid r\u00fchret dich?", "tokens": ["Da", "seufzt", "das", "Wild", ":", "Welch'", "Mit\u00b7leid", "r\u00fch\u00b7ret", "dich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du k\u00f6mmst als Feind, und raubest mir das Leben,", "tokens": ["Du", "k\u00f6mmst", "als", "Feind", ",", "und", "rau\u00b7best", "mir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und mir willst du doch solche K\u00fcsse geben,", "tokens": ["Und", "mir", "willst", "du", "doch", "sol\u00b7che", "K\u00fcs\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als w\u00e4re dir kein Freund so lieb, als ich?", "tokens": ["Als", "w\u00e4\u00b7re", "dir", "kein", "Freund", "so", "lieb", ",", "als", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIAT", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich bitte sehr, h\u00f6r' auf mit deinen Bissen;", "tokens": ["Ich", "bit\u00b7te", "sehr", ",", "h\u00f6r'", "auf", "mit", "dei\u00b7nen", "Bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wo nicht, verschone mich mit K\u00fcs