{"textgrid.poem.29464": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrst und Herr, wie eine Glud brennt ihr immerdar,", "tokens": ["F\u00fcrst", "und", "Herr", ",", "wie", "ei\u00b7ne", "Glud", "brennt", "ihr", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weil Euch stets an Eurer Seit Euer Feuer war,", "tokens": ["Weil", "Euch", "stets", "an", "Eu\u00b7rer", "Seit", "Eu\u00b7er", "Feu\u00b7er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Feuer, das der Tugend Schein theilt in weiten Krei\u00df,", "tokens": ["Feu\u00b7er", ",", "das", "der", "Tu\u00b7gend", "Schein", "theilt", "in", "wei\u00b7ten", "Krei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Feuer, das durch seinen Glantz andrem nimmt den Preis,", "tokens": ["Feu\u00b7er", ",", "das", "durch", "sei\u00b7nen", "Glantz", "an\u00b7drem", "nimmt", "den", "Preis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Feuer, das Euch Freude gibt, das Euch w\u00e4rmt und leucht,", "tokens": ["Feu\u00b7er", ",", "das", "Euch", "Freu\u00b7de", "gibt", ",", "das", "Euch", "w\u00e4rmt", "und", "leucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Feuer, dem die Ehre dient, da\u00df die Bosheit fleucht.", "tokens": ["Feu\u00b7er", ",", "dem", "die", "Eh\u00b7re", "dient", ",", "da\u00df", "die", "Bos\u00b7heit", "fleucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "F\u00fcrst und Herr, wie eine Glud brennet immerdar!", "tokens": ["F\u00fcrst", "und", "Herr", ",", "wie", "ei\u00b7ne", "Glud", "bren\u00b7net", "im\u00b7mer\u00b7dar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Da\u00df das Feuer und die Glud lesche nie kein Jahr!", "tokens": ["Da\u00df", "das", "Feu\u00b7er", "und", "die", "Glud", "le\u00b7sche", "nie", "kein", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "F\u00fcrst und Herr, wie eine Glud brennt ihr imm