{"dta.poem.18042": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Der Weise.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Midas trotzt auf den Besitz der Sch\u00e4tze,V. 1. Auf den Besitz der Sch\u00e4tze)\n\nDas Possessivum mein, dein,\nsein, kann \u00f6fters, wie hier ge-\nschehen, mit gutem Nachdruck\ndurch ein Substantivum gegeben\nwerden. Und da der Poet hier\n\nvon dem Besitz nicht aber von\ndem Genu\u00df redet, so dienet die\nWahl dieses Wortes den Trotz\neines solchen Midas recht l\u00e4-\ncherlich zu machen.", "tokens": ["Ein", "Mi\u00b7das", "trotzt", "auf", "den", "Be\u00b7sitz", "der", "Sch\u00e4t\u00b7ze", ",", "V.", "1.", "Auf", "den", "Be\u00b7sitz", "der", "Sch\u00e4t\u00b7ze", ")", "Das", "Pos\u00b7ses\u00b7si\u00b7vum", "mein", ",", "dein", ",", "sein", ",", "kann", "\u00f6f\u00b7ters", ",", "wie", "hier", "ge", "sche\u00b7hen", ",", "mit", "gu\u00b7tem", "Nach\u00b7druck", "durch", "ein", "Subs\u00b7tan\u00b7ti\u00b7vum", "ge\u00b7ge\u00b7ben", "wer\u00b7den", ".", "Und", "da", "der", "Po\u00b7et", "hier", "von", "dem", "Be\u00b7sitz", "nicht", "a\u00b7ber", "von", "dem", "Ge\u00b7nu\u00df", "re\u00b7det", ",", "so", "die\u00b7net", "die", "Wahl", "die\u00b7ses", "Wor\u00b7tes", "den", "Trotz", "ei\u00b7nes", "sol\u00b7chen", "Mi\u00b7das", "recht", "l\u00e4", "cher\u00b7lich", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "abbreviation", "ordinal", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "NE", "ADJA", "APPR", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "$,", "PPOSAT", "$,", "VAINF", "$,", "VMFIN", "ADV", "$,", "PWAV", "ADV", "TRUNC", "VVINF", "$,", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "VAINF", "$.", "KON", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PDAT", "NN", "ART", "NN", "ART", "PIAT", "NN", "ADV", "TRUNC", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+---+-+-+--+-+--+--+--+-+-+--+--+-+-+-+--+-+-+--+--+-+--+-+-+--+-+-+--+--+--+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Um die der Geitz nach fernen Ufern reis\u2019t.", "tokens": ["Um", "die", "der", "Geitz", "nach", "fer\u00b7nen", "U\u00b7fern", "reis'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUI", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pr\u00fcft auch der Thor der Wahrheit ew\u2019ge S\u00e4tze,", "tokens": ["Pr\u00fcft", "auch", "der", "Thor", "der", "Wahr\u00b7heit", "ew'\u00b7ge", "S\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Weisen Gl\u00fcck, den echten Helden-Geist,", "tokens": ["Des", "Wei\u00b7sen", "Gl\u00fcck", ",", "den", "ech\u00b7ten", "Hel\u00b7den\u00b7Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach dem allein der Reichen Neid nicht strebet?", "tokens": ["Nach", "dem", "al\u00b7lein", "der", "Rei\u00b7chen", "Neid", "nicht", "stre\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Weiser lebt", "tokens": ["Ein", "Wei\u00b7ser", "lebt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jhm Geld und ", "tokens": ["Jhm", "Geld", "und"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "KON"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Beschweret gleich sein wuchernd Gut nicht Schiffe,", "tokens": ["Be\u00b7schwe\u00b7ret", "gleich", "sein", "wu\u00b7chernd", "Gut", "nicht", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "ADJD", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er darf sich gro\u00df, er darf sich gl\u00fccklich preisen;", "tokens": ["Er", "darf", "sich", "gro\u00df", ",", "er", "darf", "sich", "gl\u00fcck\u00b7lich", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein fremder Fluch versaltzet seine Speisen.", "tokens": ["Kein", "frem\u00b7der", "Fluch", "ver\u00b7salt\u00b7zet", "sei\u00b7ne", "Spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er schl\u00e4ft mit Lust, wo andrer ", "tokens": ["Er", "schl\u00e4ft", "mit", "Lust", ",", "wo", "an\u00b7drer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWAV", "PIS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wann Boreas um Dach und Fenster heult,", "tokens": ["Wann", "Bo\u00b7re\u00b7as", "um", "Dach", "und", "Fens\u00b7ter", "heult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Frachten droht, und Mast und Kiel ereilt,", "tokens": ["Den", "Frach\u00b7ten", "droht", ",", "und", "Mast", "und", "Kiel", "er\u00b7eilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So oft der Herr der Wasser und der Erden.", "tokens": ["So", "oft", "der", "Herr", "der", "Was\u00b7ser", "und", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Kr\u00e4mer beugt, da\u00df sie nicht F\u00fcrsten werden.", "tokens": ["Die", "Kr\u00e4\u00b7mer", "beugt", ",", "da\u00df", "sie", "nicht", "F\u00fcrs\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was Recht und Flei\u00df und Zeit und Gl\u00fcck ihm geben,", "tokens": ["Was", "Recht", "und", "Flei\u00df", "und", "Zeit", "und", "Gl\u00fcck", "ihm", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwaltet er", "tokens": ["Ver\u00b7wal\u00b7tet", "er"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und meidet den, der den ", "tokens": ["Und", "mei\u00b7det", "den", ",", "der", "den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der jeden Tag nur dem Gewerbe weiht,", "tokens": ["Der", "je\u00b7den", "Tag", "nur", "dem", "Ge\u00b7wer\u00b7be", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und j\u00fcdisch lacht, so oft er sieht und h\u00f6ret,", "tokens": ["Und", "j\u00fc\u00b7disch", "lacht", ",", "so", "oft", "er", "sieht", "und", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie die Vernunft Geschmack und Wahrheit ehret.", "tokens": ["Wie", "die", "Ver\u00b7nunft", "Ge\u00b7schmack", "und", "Wahr\u00b7heit", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie edel ist die Neigung echter Britten!", "tokens": ["Wie", "e\u00b7del", "ist", "die", "Nei\u00b7gung", "ech\u00b7ter", "Brit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jhr Ueberflu\u00df ", "tokens": ["Ihr", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Handlung Frucht und was ihr Muth erstritten", "tokens": ["Der", "Hand\u00b7lung", "Frucht", "und", "was", "ihr", "Muth", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird, unbereut, Verdiensten zugewandt;", "tokens": ["Wird", ",", "un\u00b7be\u00b7reut", ",", "Ver\u00b7diens\u00b7ten", "zu\u00b7ge\u00b7wandt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gunst kr\u00f6nt den Flei\u00df, den Macht und Freyheit sch\u00fctzen:", "tokens": ["Gunst", "kr\u00f6nt", "den", "Flei\u00df", ",", "den", "Macht", "und", "Frey\u00b7heit", "sch\u00fct\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Freyheit! dort, nur dort ist deine Wonne,", "tokens": ["O", "Frey\u00b7heit", "!", "dort", ",", "nur", "dort", "ist", "dei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "$,", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der St\u00e4dte Schmuck, der Segen jeder Flur,", "tokens": ["Der", "St\u00e4d\u00b7te", "Schmuck", ",", "der", "Se\u00b7gen", "je\u00b7der", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Starck wie das Meer, erquickend wie die Sonne,", "tokens": ["Starck", "wie", "das", "Meer", ",", "er\u00b7qui\u00b7ckend", "wie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "VVPP", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sch\u00f6n wie ihr Licht, und reich wie die Natur.", "tokens": ["Sch\u00f6n", "wie", "ihr", "Licht", ",", "und", "reich", "wie", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "$,", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und nicht zu viel von deiner W\u00fcrde kennen!", "tokens": ["Und", "nicht", "zu", "viel", "von", "dei\u00b7ner", "W\u00fcr\u00b7de", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKA", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer hei\u00dft oft gro\u00df? Der schnell nach Ehren klettert,", "tokens": ["Wer", "hei\u00dft", "oft", "gro\u00df", "?", "Der", "schnell", "nach", "Eh\u00b7ren", "klet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$.", "ART", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den K\u00fchnheit hebt, die H\u00f6he schwindlicht macht.", "tokens": ["Den", "K\u00fchn\u00b7heit", "hebt", ",", "die", "H\u00f6\u00b7he", "schwind\u00b7licht", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wer ist gro\u00df? Der F\u00fcrsten nicht verg\u00f6ttert,", "tokens": ["Doch", "wer", "ist", "gro\u00df", "?", "Der", "F\u00fcrs\u00b7ten", "nicht", "ver\u00b7g\u00f6t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADJD", "$.", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wahrheit sucht und Recht und Wahrheit findet.", "tokens": ["Der", "Wahr\u00b7heit", "sucht", "und", "Recht", "und", "Wahr\u00b7heit", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und seinen Werth auf Witz und Tugend gr\u00fcndet.", "tokens": ["Und", "sei\u00b7nen", "Werth", "auf", "Witz", "und", "Tu\u00b7gend", "gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein solcher kennt die Eitelkeit der W\u00fcrden,", "tokens": ["Ein", "sol\u00b7cher", "kennt", "die", "Ei\u00b7tel\u00b7keit", "der", "W\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In die das Gl\u00fcck zu selten Kluge steckt.", "tokens": ["In", "die", "das", "Gl\u00fcck", "zu", "sel\u00b7ten", "Klu\u00b7ge", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhm strahlt kein Stern, der kleine Hertzen deckt.", "tokens": ["Jhm", "strahlt", "kein", "Stern", ",", "der", "klei\u00b7ne", "Hert\u00b7zen", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Geist, durch den ein Cato gro\u00df geworden,", "tokens": ["Der", "Geist", ",", "durch", "den", "ein", "Ca\u00b7to", "gro\u00df", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NE", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "F\u00e4hrt in kein Band und ruht auf keinem Orden.", "tokens": ["F\u00e4hrt", "in", "kein", "Band", "und", "ruht", "auf", "kei\u00b7nem", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wann ma