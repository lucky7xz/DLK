{"dta.poem.18277": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Dito.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Drum spart euch eure klugen Reden,", "tokens": ["Drum", "spart", "euch", "eu\u00b7re", "klu\u00b7gen", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt doch ein alter Pfiffikus:", "tokens": ["Sagt", "doch", "ein", "al\u00b7ter", "Pfif\u00b7fi\u00b7kus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht jede Formel pa\u00dft auf Jeden.", "tokens": ["Nicht", "je\u00b7de", "For\u00b7mel", "pa\u00dft", "auf", "Je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir h\u00e4tt es so, mir so behagt,", "tokens": ["Mir", "h\u00e4tt", "es", "so", ",", "mir", "so", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon gut, schon gut, ihr lieben Leute;", "tokens": ["Schon", "gut", ",", "schon", "gut", ",", "ihr", "lie\u00b7ben", "Leu\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wi\u00dft ja, was das Sprichwort sagt,", "tokens": ["Ihr", "wi\u00dft", "ja", ",", "was", "das", "Sprich\u00b7wort", "sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der J\u00e4ger pfeift, es bellt die Meute!", "tokens": ["Der", "J\u00e4\u00b7ger", "pfeift", ",", "es", "bellt", "die", "Meu\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df ihr auch der Weisheit Schlu\u00df,", "tokens": ["Doch", "da\u00df", "ihr", "auch", "der", "Weis\u00b7heit", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wahrheit Wahrheit m\u00f6gt erfahren,", "tokens": ["Der", "Wahr\u00b7heit", "Wahr\u00b7heit", "m\u00f6gt", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt jener selbe Pfiffikus:", "tokens": ["Sagt", "je\u00b7ner", "sel\u00b7be", "Pfif\u00b7fi\u00b7kus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Thorheit w\u00e4chst oft mit den Jahren!", "tokens": ["Die", "Thor\u00b7heit", "w\u00e4chst", "oft", "mit", "den", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}