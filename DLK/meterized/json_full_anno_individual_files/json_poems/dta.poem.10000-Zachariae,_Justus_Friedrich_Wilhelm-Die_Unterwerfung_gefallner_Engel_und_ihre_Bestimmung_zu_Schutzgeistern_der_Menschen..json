{"dta.poem.10000": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und schnell fielen sie all\u2019 aufs Antlitz, und netz-", "tokens": ["Und", "schnell", "fie\u00b7len", "sie", "all'", "aufs", "Ant\u00b7litz", ",", "und", "netz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$,", "KON", "TRUNC"], "meter": "-++--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jetzt mit Thr\u00e4nen der Freude, den Staub. Drauf schlo\u00df", "tokens": ["Jetzt", "mit", "Thr\u00e4\u00b7nen", "der", "Freu\u00b7de", ",", "den", "Staub", ".", "Drauf", "schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "$,", "ART", "NN", "$.", "PAV", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Hinter Orion, und Gabriel, an; sie zogen von dannen", "tokens": ["Hin\u00b7ter", "O\u00b7rion", ",", "und", "Ga\u00b7bri\u00b7el", ",", "an", ";", "sie", "zo\u00b7gen", "von", "dan\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "NE", "$,", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nach der neuerschaffenen Welt; viel weite Bezirke", "tokens": ["Nach", "der", "neu\u00b7er\u00b7schaf\u00b7fe\u00b7nen", "Welt", ";", "viel", "wei\u00b7te", "Be\u00b7zir\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Eilten sie durch; viel weiter, als dieser Erde Bezirke,", "tokens": ["Eil\u00b7ten", "sie", "durch", ";", "viel", "wei\u00b7ter", ",", "als", "die\u00b7ser", "Er\u00b7de", "Be\u00b7zir\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "ADV", "$,", "KOUS", "PDAT", "NN", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wenn sie sich auch in die L\u00e4ng\u2019 erstreckte; bis endlich", "tokens": ["Wenn", "sie", "sich", "auch", "in", "die", "L\u00e4ng'", "er\u00b7streck\u00b7te", ";", "bis", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "KON", "ADV"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hohe krystallne Mauren erschienen, mit Zinnen und", "tokens": ["Ho\u00b7he", "krys\u00b7tall\u00b7ne", "Mau\u00b7ren", "er\u00b7schie\u00b7nen", ",", "mit", "Zin\u00b7nen", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "VVPP", "$,", "APPR", "NN", "KON"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Von hellleuchtenden Saphir geschm\u00fcckt. Die gl\u00e4nzen-", "tokens": ["Von", "hell\u00b7leuch\u00b7ten\u00b7den", "Sa\u00b7phir", "ge\u00b7schm\u00fcckt", ".", "Die", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "ART", "TRUNC"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Thaten von selber sich auf, sie sahn erstaunend hinunter", "tokens": ["Tha\u00b7ten", "von", "sel\u00b7ber", "sich", "auf", ",", "sie", "sahn", "er\u00b7stau\u00b7nend", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "VVPP", "PTKVZ"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "In die Reiche der Nacht und des Chaos. Ein stralen-", "tokens": ["In", "die", "Rei\u00b7che", "der", "Nacht", "und", "des", "Chaos", ".", "Ein", "stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NE", "ART", "NN", "KON", "ART", "NN", "$.", "ART", "TRUNC"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Durch die Tiefen des Chaos zur neuen Sch\u00f6pfung her-", "tokens": ["Durch", "die", "Tie\u00b7fen", "des", "Chaos", "zur", "neu\u00b7en", "Sch\u00f6p\u00b7fung", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Welcher von selbst vor dem Sch\u00f6pfer entstand; so wie", "tokens": ["Wel\u00b7cher", "von", "selbst", "vor", "dem", "Sch\u00f6p\u00b7fer", "ent\u00b7stand", ";", "so", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "APPR", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "ADV", "KOKOM"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "In die Tiefen der Nacht, die Erd\u2019 und den Himmel zu", "tokens": ["In", "die", "Tie\u00b7fen", "der", "Nacht", ",", "die", "Erd'", "und", "den", "Him\u00b7mel", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "PTKZU"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Da sie sich ietzo den Thoren genaht, da wandte noch", "tokens": ["Da", "sie", "sich", "iet\u00b7zo", "den", "Tho\u00b7ren", "ge\u00b7naht", ",", "da", "wand\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Traurig Orion sich um, und eine Z\u00e4hre der Wehmuth", "tokens": ["Trau\u00b7rig", "O\u00b7rion", "sich", "um", ",", "und", "ei\u00b7ne", "Z\u00e4h\u00b7re", "der", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "PRF", "PTKVZ", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+--++", "measure": "iambic.septa.invert"}, "line.16": {"text": "Rann ihm vom Antlitz, indem er sich nun vom Him-", "tokens": ["Rann", "ihm", "vom", "Ant\u00b7litz", ",", "in\u00b7dem", "er", "sich", "nun", "vom", "Him"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}