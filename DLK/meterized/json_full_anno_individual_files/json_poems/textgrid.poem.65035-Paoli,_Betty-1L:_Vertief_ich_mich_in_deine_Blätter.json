{"textgrid.poem.65035": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vertief ich mich in deine Bl\u00e4tter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vertief ich mich in deine Bl\u00e4tter", "tokens": ["Ver\u00b7tief", "ich", "mich", "in", "dei\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wehts um mich wie Fr\u00fchlingsduft!", "tokens": ["Da", "wehts", "um", "mich", "wie", "Fr\u00fch\u00b7lings\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lerche jubelndes Geschmetter", "tokens": ["Der", "Ler\u00b7che", "ju\u00b7beln\u00b7des", "Ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durcht\u00f6nt die frische Morgenluft,", "tokens": ["Durch\u00b7t\u00f6nt", "die", "fri\u00b7sche", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die jungen Halme keimen, sprie\u00dfen,", "tokens": ["Die", "jun\u00b7gen", "Hal\u00b7me", "kei\u00b7men", ",", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Himmel str\u00f6mt ein Meer von Licht", "tokens": ["Vom", "Him\u00b7mel", "str\u00f6mt", "ein", "Meer", "von", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in die Menschenbrust ergie\u00dfen", "tokens": ["Und", "in", "die", "Men\u00b7schen\u00b7brust", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich Hoffnung, Ruh und Zuversicht.", "tokens": ["Sich", "Hoff\u00b7nung", ",", "Ruh", "und", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die falschen Freuden, nicht'gen Schmerzen,", "tokens": ["Die", "fal\u00b7schen", "Freu\u00b7den", ",", "nicht'\u00b7gen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erde Wust und ihren Tand", "tokens": ["Der", "Er\u00b7de", "Wust", "und", "ih\u00b7ren", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimmst du hinweg von unserm Herzen", "tokens": ["Nimmst", "du", "hin\u00b7weg", "von", "un\u00b7serm", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit leiser, liebevoller Hand.", "tokens": ["Mit", "lei\u00b7ser", ",", "lie\u00b7be\u00b7vol\u00b7ler", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von vielverschlungnen Irrewegen", "tokens": ["Von", "viel\u00b7ver\u00b7schlung\u00b7nen", "Ir\u00b7re\u00b7we\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur\u00fcck auf ewig lichte Spur", "tokens": ["Zu\u00b7r\u00fcck", "auf", "e\u00b7wig", "lich\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchrt mild uns deines Wortes Segen,", "tokens": ["F\u00fchrt", "mild", "uns", "dei\u00b7nes", "Wor\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du treuer Dolmetsch der Natur.", "tokens": ["Du", "treu\u00b7er", "Dol\u00b7metsch", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ob Kampf und Zwiespalt dich umringe", "tokens": ["Ob", "Kampf", "und", "Zwies\u00b7palt", "dich", "um\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirren Scheines Truggewalt,", "tokens": ["Und", "wir\u00b7ren", "Schei\u00b7nes", "Trug\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein heller Blick erschaut die Dinge", "tokens": ["Dein", "hel\u00b7ler", "Blick", "er\u00b7schaut", "die", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer heil'gen Urgestalt.", "tokens": ["In", "ih\u00b7rer", "heil'\u00b7gen", "Ur\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Geist durchbricht die engen Schranken", "tokens": ["Dein", "Geist", "durch\u00b7bricht", "die", "en\u00b7gen", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von eitler Satzung aufgestellt,", "tokens": ["Von", "eit\u00b7ler", "Sat\u00b7zung", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fchrt den reinen Gottgedanken", "tokens": ["Und", "f\u00fchrt", "den", "rei\u00b7nen", "Gott\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als heitern Sieger durch die Welt.", "tokens": ["Als", "hei\u00b7tern", "Sie\u00b7ger", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum winken deiner Dichtung Strahlen", "tokens": ["Drum", "win\u00b7ken", "dei\u00b7ner", "Dich\u00b7tung", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns wie der Stern der Weisen zu,", "tokens": ["Uns", "wie", "der", "Stern", "der", "Wei\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum finden tausend bittre Qualen", "tokens": ["Drum", "fin\u00b7den", "tau\u00b7send", "bitt\u00b7re", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Saume deines Mantels Ruh,", "tokens": ["Am", "Sau\u00b7me", "dei\u00b7nes", "Man\u00b7tels", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum bist du als Prophet zu ehren,", "tokens": ["Drum", "bist", "du", "als", "Pro\u00b7phet", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den tr\u00f6stend die Natur gesandt,", "tokens": ["Den", "tr\u00f6s\u00b7tend", "die", "Na\u00b7tur", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und den, das Wunder noch zu mehren,", "tokens": ["Und", "den", ",", "das", "Wun\u00b7der", "noch", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Menschen freudig anerkannt.", "tokens": ["Die", "Men\u00b7schen", "freu\u00b7dig", "an\u00b7er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein Blitz, kein Schlag kann den erreichen", "tokens": ["Kein", "Blitz", ",", "kein", "Schlag", "kann", "den", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "ART", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der \u00fcber den Gewittern steht!", "tokens": ["Der", "\u00fc\u00b7ber", "den", "Ge\u00b7wit\u00b7tern", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So stehst du in des Geistes Reichen", "tokens": ["So", "stehst", "du", "in", "des", "Geis\u00b7tes", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In still erhabner Majest\u00e4t.", "tokens": ["In", "still", "er\u00b7hab\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu tiefem, ahnungsvollem Schweigen", "tokens": ["Zu", "tie\u00b7fem", ",", "ah\u00b7nungs\u00b7vol\u00b7lem", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verklingt der Erdenstimmen Chor", "tokens": ["Ver\u00b7klingt", "der", "Er\u00b7dens\u00b7tim\u00b7men", "Chor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nur der Liebe D\u00fcfte steigen", "tokens": ["Und", "nur", "der", "Lie\u00b7be", "D\u00fcf\u00b7te", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Opferhauch zu dir empor.", "tokens": ["Wie", "Op\u00b7fer\u00b7hauch", "zu", "dir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vertief ich mich in deine Bl\u00e4tter", "tokens": ["Ver\u00b7tief", "ich", "mich", "in", "dei\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wehts um mich wie Fr\u00fchlingsduft!", "tokens": ["Da", "wehts", "um", "mich", "wie", "Fr\u00fch\u00b7lings\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lerche jubelndes Geschmetter", "tokens": ["Der", "Ler\u00b7che", "ju\u00b7beln\u00b7des", "Ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durcht\u00f6nt die frische Morgenluft,", "tokens": ["Durch\u00b7t\u00f6nt", "die", "fri\u00b7sche", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die