{"dta.poem.14636": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Unten endlos nichts als Wasser,", "tokens": ["Un\u00b7ten", "end\u00b7los", "nichts", "als", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Droben Himmel still und weit,", "tokens": ["Dro\u00b7ben", "Him\u00b7mel", "still", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das G\u00f6tterland, das blasse,", "tokens": ["Nur", "das", "G\u00f6t\u00b7ter\u00b7land", ",", "das", "blas\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lag in Meereseinsamkeit,", "tokens": ["Lag", "in", "Mee\u00b7re\u00b7sein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo auf farbenlosen Matten", "tokens": ["Wo", "auf", "far\u00b7ben\u00b7lo\u00b7sen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gipfel wie in Tr\u00e4umen steh'n,", "tokens": ["Gip\u00b7fel", "wie", "in", "Tr\u00e4u\u00b7men", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Gestalten ohne Schatten", "tokens": ["Und", "Ge\u00b7stal\u00b7ten", "oh\u00b7ne", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig lautlos sich ergeh'n.", "tokens": ["E\u00b7wig", "laut\u00b7los", "sich", "er\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}