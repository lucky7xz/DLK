{"dta.poem.19572": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Schiff befuhr das Meer. Aufrauschend quoll", "tokens": ["Ein", "Schiff", "be\u00b7fuhr", "das", "Meer", ".", "Auf\u00b7rau\u00b7schend", "quoll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Fluth am Kiel. Er suchte Pylos' Strand.", "tokens": ["Die", "Fluth", "am", "Kiel", ".", "Er", "such\u00b7te", "Py\u00b7los'", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$.", "PPER", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Steuer f\u00fchrt' ein J\u00fcngling kummervoll,", "tokens": ["Das", "Steu\u00b7er", "f\u00fchrt'", "ein", "J\u00fcng\u00b7ling", "kum\u00b7mer\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem fr\u00fch des Vaters Rath und Hilfe schwand.", "tokens": ["Dem", "fr\u00fch", "des", "Va\u00b7ters", "Rath", "und", "Hil\u00b7fe", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Gl\u00fcckbed\u00fcrft'ge hie\u00df Telemachos", "tokens": ["Der", "Gl\u00fc\u00b7ck\u00b7be\u00b7d\u00fcrft'\u00b7ge", "hie\u00df", "Te\u00b7le\u00b7ma\u00b7chos"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und schaute nach des Segels n\u00e4cht'gem Flug,", "tokens": ["Und", "schau\u00b7te", "nach", "des", "Se\u00b7gels", "n\u00e4cht'\u00b7gem", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dicht neben ihm der hohe Fahrtgeno\u00df,", "tokens": ["Dicht", "ne\u00b7ben", "ihm", "der", "ho\u00b7he", "Fahrt\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Athene war's, die Mentors Z\u00fcge trug.", "tokens": ["A\u00b7the\u00b7ne", "wa\u00b7r's", ",", "die", "Men\u00b7tors", "Z\u00fc\u00b7ge", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Unendlich brach hervor der Sterne Heer,", "tokens": ["Un\u00b7end\u00b7lich", "brach", "her\u00b7vor", "der", "Ster\u00b7ne", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die lichten Waller wu\u00dften ihre Bahn ...", "tokens": ["Die", "lich\u00b7ten", "Wal\u00b7ler", "wu\u00df\u00b7ten", "ih\u00b7re", "Bahn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sprach die Tochter Zeus' auf dunkelm Meer:", "tokens": ["Da", "sprach", "die", "Toch\u00b7ter", "Zeus'", "auf", "dun\u00b7kelm", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u201ejetzt, J\u00fcngling, ruf' mit mir die G\u00f6tter an!\u201c", "tokens": ["\u201e", "jetzt", ",", "J\u00fcng\u00b7ling", ",", "ruf'", "mit", "mir", "die", "G\u00f6t\u00b7ter", "an", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die H\u00e4nde, wie der Staubgeborne fleht,", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "wie", "der", "Staub\u00b7ge\u00b7bor\u00b7ne", "fleht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhob sie ausgebreitet in die Nacht \u2014", "tokens": ["Er\u00b7hob", "sie", "aus\u00b7ge\u00b7brei\u00b7tet", "in", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sie erh\u00f6rte selber das Gebet,", "tokens": ["Und", "sie", "er\u00b7h\u00f6r\u00b7te", "sel\u00b7ber", "das", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von ihr f\u00fcr den Verlassnen dargebracht.", "tokens": ["Von", "ihr", "f\u00fcr", "den", "Ver\u00b7lass\u00b7nen", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}