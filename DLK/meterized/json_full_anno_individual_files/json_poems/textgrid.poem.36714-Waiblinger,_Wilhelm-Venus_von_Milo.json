{"textgrid.poem.36714": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Venus von Milo", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschen steigen zum Himmel: zur sch\u00f6nen olympischen Blume", "tokens": ["Men\u00b7schen", "stei\u00b7gen", "zum", "Him\u00b7mel", ":", "zur", "sch\u00f6\u00b7nen", "o\u00b7lym\u00b7pi\u00b7schen", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$.", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schlie\u00dfet der irdische Keim dr\u00fcben im Lichte sich auf.", "tokens": ["Schlie\u00b7\u00dfet", "der", "ir\u00b7di\u00b7sche", "Keim", "dr\u00fc\u00b7ben", "im", "Lich\u00b7te", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Geist verschmilzt sich mit Geist, und im freier entfalteten Leben", "tokens": ["Geist", "ver\u00b7schmilzt", "sich", "mit", "Geist", ",", "und", "im", "frei\u00b7er", "ent\u00b7fal\u00b7te\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wird die sterbliche Form sch\u00f6ner und heil'ger verkl\u00e4rt.", "tokens": ["Wird", "die", "sterb\u00b7li\u00b7che", "Form", "sch\u00f6\u00b7ner", "und", "heil'\u00b7ger", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "KON", "ADJD", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "So zum vollendetern Bild durch ein m\u00e4chtiges Wunder verwandelt,", "tokens": ["So", "zum", "voll\u00b7en\u00b7de\u00b7tern", "Bild", "durch", "ein", "m\u00e4ch\u00b7ti\u00b7ges", "Wun\u00b7der", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Lenkest den irdischen Sinn du auf das Himmlische hin.", "tokens": ["Len\u00b7kest", "den", "ir\u00b7di\u00b7schen", "Sinn", "du", "auf", "das", "Himm\u00b7li\u00b7sche", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Menschen steigen zum Himmel: zur sch\u00f6nen olympischen Blume", "tokens": ["Men\u00b7schen", "stei\u00b7gen", "zum", "Him\u00b7mel", ":", "zur", "sch\u00f6\u00b7nen", "o\u00b7lym\u00b7pi\u00b7schen", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$.", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Schlie\u00dfet der irdische Keim dr\u00fcben im Lichte sich auf.", "tokens": ["Schlie\u00b7\u00dfet", "der", "ir\u00b7di\u00b7sche", "Keim", "dr\u00fc\u00b7ben", "im", "Lich\u00b7te", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "PRF", "PTKVZ", "$.