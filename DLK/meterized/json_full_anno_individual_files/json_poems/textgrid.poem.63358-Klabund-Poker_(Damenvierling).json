{"textgrid.poem.63358": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Poker (Damenvierling)", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem je die Muse sich vervierfacht bot,", "tokens": ["Wem", "je", "die", "Mu\u00b7se", "sich", "ver\u00b7vier\u00b7facht", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wandelt trunken \u00fcber diese Auen.", "tokens": ["Der", "wan\u00b7delt", "trun\u00b7ken", "\u00fc\u00b7ber", "die\u00b7se", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was d\u00fcnken ihn die Haus- und Stra\u00dfenfrauen,", "tokens": ["Was", "d\u00fcn\u00b7ken", "ihn", "die", "Haus", "und", "Stra\u00b7\u00dfen\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was Narzissenwind im Abendrot.", "tokens": ["Und", "was", "Nar\u00b7zis\u00b7sen\u00b7wind", "im", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er schl\u00e4gt drei K\u00f6nige bedeutsam tot.", "tokens": ["Er", "schl\u00e4gt", "drei", "K\u00f6\u00b7ni\u00b7ge", "be\u00b7deut\u00b7sam", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Selbst eine volle Hand darf er beschauen.", "tokens": ["Selbst", "ei\u00b7ne", "vol\u00b7le", "Hand", "darf", "er", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er sch\u00fcttet in den Abgrund jenen lauen", "tokens": ["Er", "sch\u00fct\u00b7tet", "in", "den", "Ab\u00b7grund", "je\u00b7nen", "lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PDS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00fcbel voll Jammertum und Menschennot.", "tokens": ["K\u00fc\u00b7bel", "voll", "Jam\u00b7mer\u00b7tum", "und", "Men\u00b7schen\u00b7not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Melpomene, du mit der Maske Pik,", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", ",", "du", "mit", "der", "Mas\u00b7ke", "Pik", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Thalia, Sterngel\u00e4chter hell im Herzen,", "tokens": ["Tha\u00b7lia", ",", "Stern\u00b7ge\u00b7l\u00e4ch\u00b7ter", "hell", "im", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du Klio, trefflich, mit dem Zeichen Sieg \u2013", "tokens": ["Du", "Klio", ",", "treff\u00b7lich", ",", "mit", "dem", "Zei\u00b7chen", "Sieg", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADJD", "$,", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Oft stand ich sumpfversunken tief in Schmerzen,", "tokens": ["Oft", "stand", "ich", "sumpf\u00b7ver\u00b7sun\u00b7ken", "tief", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da winkte, da\u00df die Seele mondw\u00e4rts stieg,", "tokens": ["Da", "wink\u00b7te", ",", "da\u00df", "die", "See\u00b7le", "mond\u00b7w\u00e4rts", "stieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kalliope mit goldnen Hochzeitskerzen.", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", "mit", "gold\u00b7nen", "Hoch\u00b7zeits\u00b7ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Wem je die Muse sich vervierfacht bot,", "tokens": ["Wem", "je", "die", "Mu\u00b7se", "sich", "ver\u00b7vier\u00b7facht", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wandelt trunken \u00fcber diese Auen.", "tokens": ["Der", "wan\u00b7delt", "trun\u00b7ken", "\u00fc\u00b7ber", "die\u00b7se", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word"