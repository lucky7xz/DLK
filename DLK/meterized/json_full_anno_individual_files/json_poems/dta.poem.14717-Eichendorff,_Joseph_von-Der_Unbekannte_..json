{"dta.poem.14717": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehast viel erfahren, willst Du ewig wandern?\u201c", "tokens": ["\u201e", "hast", "viel", "er\u00b7fah\u00b7ren", ",", "willst", "Du", "e\u00b7wig", "wan\u00b7dern", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADV", "VVINF", "$,", "VMFIN", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprach drauf sein Wirth mit herzlichem Vertrau'n,", "tokens": ["Sprach", "drauf", "sein", "Wirth", "mit", "herz\u00b7li\u00b7chem", "Ver\u00b7trau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201ehier kannst Du froh genie\u00dfen wie die andern,", "tokens": ["\u201e", "hier", "kannst", "Du", "froh", "ge\u00b7nie\u00b7\u00dfen", "wie", "die", "an\u00b7dern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am eig'nen Heerd Dein kleines G\u00e4rtchen bau'n,", "tokens": ["Am", "eig'\u00b7nen", "Heerd", "Dein", "klei\u00b7nes", "G\u00e4rt\u00b7chen", "bau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Nachbars T\u00f6chter haben reiche Truhen,", "tokens": ["Des", "Nach\u00b7bars", "T\u00f6ch\u00b7ter", "ha\u00b7ben", "rei\u00b7che", "Tru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ruh' endlich aus, brauchst nicht allein zu ruhen.\u201c", "tokens": ["Ruh'", "end\u00b7lich", "aus", ",", "brauchst", "nicht", "al\u00b7lein", "zu", "ru\u00b7hen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}