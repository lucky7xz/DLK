{"dta.poem.6769": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es leitet das Gesicht, es folgt ihm der Verstand.", "tokens": ["Es", "lei\u00b7tet", "das", "Ge\u00b7sicht", ",", "es", "folgt", "ihm", "der", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom C\u00f6rper, der sehr gro\u00df, wird seine Gr\u00f6ss\u2019 erkannt,", "tokens": ["Vom", "C\u00f6r\u00b7per", ",", "der", "sehr", "gro\u00df", ",", "wird", "sei\u00b7ne", "Gr\u00f6ss'", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil, wann man seine Gr\u00f6ss\u2019 erkennen will und wissen,", "tokens": ["Weil", ",", "wann", "man", "sei\u00b7ne", "Gr\u00f6ss'", "er\u00b7ken\u00b7nen", "will", "und", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PIS", "PPOSAT", "NN", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir unsrer Augen Stand ver\u00e4ndern m\u00fcssen,", "tokens": ["Wir", "uns\u00b7rer", "Au\u00b7gen", "Stand", "ver\u00b7\u00e4n\u00b7dern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um selben gantz zu \u00fcbersehn.", "tokens": ["Um", "sel\u00b7ben", "gantz", "zu", "\u00fc\u00b7ber\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man wird, auf welche Weis\u2019 er sich beweget,", "tokens": ["Man", "wird", ",", "auf", "wel\u00b7che", "Weis'", "er", "sich", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "APPR", "PWAT", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch sichre Folge bald verstehn,", "tokens": ["Durch", "sich\u00b7re", "Fol\u00b7ge", "bald", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sich, um ihn zu sehn, das Auge mit ihm reget.", "tokens": ["Wenn", "sich", ",", "um", "ihn", "zu", "sehn", ",", "das", "Au\u00b7ge", "mit", "ihm", "re\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie oder wo es stets am ersten Ort sich bindet,", "tokens": ["Wie", "o\u00b7der", "wo", "es", "stets", "am", "ers\u00b7ten", "Ort", "sich", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PWAV", "PPER", "ADV", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Vorwurff schnell aus unsern Blick verschwindet.", "tokens": ["Der", "Vor\u00b7wurff", "schnell", "aus", "un\u00b7sern", "Blick", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}