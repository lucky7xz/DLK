{"textgrid.poem.57866": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Verschwiegenheit", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht weit von hier, wo sieben Linden winken", "tokens": ["Nicht", "weit", "von", "hier", ",", "wo", "sie\u00b7ben", "Lin\u00b7den", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "APPR", "ADV", "$,", "PWAV", "VVFIN", "NE", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wirtshaus an der Heeresstra\u00dfe steht;", "tokens": ["Ein", "Wirts\u00b7haus", "an", "der", "Hee\u00b7res\u00b7stra\u00b7\u00dfe", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Junggeselle, der des Weges kommet,", "tokens": ["Kein", "Jung\u00b7ge\u00b7sel\u00b7le", ",", "der", "des", "We\u00b7ges", "kom\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An dieser St\u00e4tte gern vor\u00fcbergeht.", "tokens": ["An", "die\u00b7ser", "St\u00e4t\u00b7te", "gern", "vor\u00b7\u00fc\u00b7ber\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn eine Wirtin jung und sch\u00f6n von Mienen", "tokens": ["Denn", "ei\u00b7ne", "Wir\u00b7tin", "jung", "und", "sch\u00f6n", "von", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steht in der T\u00fcr und sieht ihn freundlich an;", "tokens": ["Steht", "in", "der", "T\u00fcr", "und", "sieht", "ihn", "freund\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der holden \u00c4uglein Blick zu widerstehen", "tokens": ["Der", "hol\u00b7den", "\u00c4ug\u00b7lein", "Blick", "zu", "wi\u00b7der\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermag so leicht kein junger Wandersmann.", "tokens": ["Ver\u00b7mag", "so", "leicht", "kein", "jun\u00b7ger", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie setzt sich zu ihm, dr\u00fcckt ihm seine H\u00e4nde", "tokens": ["Sie", "setzt", "sich", "zu", "ihm", ",", "dr\u00fcckt", "ihm", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und klagt verstohlen ihm ihr Herzeleid;", "tokens": ["Und", "klagt", "ver\u00b7stoh\u00b7len", "ihm", "ihr", "Her\u00b7ze\u00b7leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie ward an einen alten Mann verkuppelt", "tokens": ["Sie", "ward", "an", "ei\u00b7nen", "al\u00b7ten", "Mann", "ver\u00b7kup\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wei\u00df nichts von der sch\u00f6nsten Z\u00e4rtlichkeit.", "tokens": ["Und", "wei\u00df", "nichts", "von", "der", "sch\u00f6ns\u00b7ten", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das \u00dcbrige, davon will ich nichts sagen,", "tokens": ["Das", "\u00dcb\u00b7ri\u00b7ge", ",", "da\u00b7von", "will", "ich", "nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dieweil ich selber dorten kehrte ein;", "tokens": ["Die\u00b7weil", "ich", "sel\u00b7ber", "dor\u00b7ten", "kehr\u00b7te", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe soll man nicht mit Spott belohnen,", "tokens": ["Die", "Lie\u00b7be", "soll", "man", "nicht", "mit", "Spott", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum soll mein Lied hier auch zu Ende sein.", "tokens": ["Drum", "sol