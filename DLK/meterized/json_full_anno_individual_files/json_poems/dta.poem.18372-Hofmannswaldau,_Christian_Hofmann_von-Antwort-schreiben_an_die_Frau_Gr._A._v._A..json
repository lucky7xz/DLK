{"dta.poem.18372": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort-schreiben an die Frau  \n Gr. A. v. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald ich dich und deinen einschlu\u00df la\u00df/", "tokens": ["So", "bald", "ich", "dich", "und", "dei\u00b7nen", "ein\u00b7schlu\u00df", "la\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O werthes blat! von sch\u00f6nster hand geschrieben/", "tokens": ["O", "wert\u00b7hes", "blat", "!", "von", "sch\u00f6ns\u00b7ter", "hand", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wurd ich der/ der seiner selbst verga\u00df/", "tokens": ["Da", "wurd", "ich", "der", "/", "der", "sei\u00b7ner", "selbst", "ver\u00b7ga\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "$(", "ART", "PPOSAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil ich mich sah von der best\u00e4ndig lieben/", "tokens": ["Weil", "ich", "mich", "sah", "von", "der", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die auf der welt in warheit/ in der that", "tokens": ["Die", "auf", "der", "welt", "in", "war\u00b7heit", "/", "in", "der", "that"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "$(", "APPR", "ART", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An redlichkeit/ an treu/ verstand und g\u00fcte/", "tokens": ["An", "red\u00b7lich\u00b7keit", "/", "an", "treu", "/", "ver\u00b7stand", "und", "g\u00fc\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "ADJD", "$(", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "An sch\u00f6nem leib/ noch sch\u00f6nerem gem\u00fcthe/", "tokens": ["An", "sch\u00f6\u00b7nem", "leib", "/", "noch", "sch\u00f6\u00b7ne\u00b7rem", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An tugend selbst/ nicht ihres gleichen hat.", "tokens": ["An", "tu\u00b7gend", "selbst", "/", "nicht", "ih\u00b7res", "glei\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$(", "PTKNEG", "PPOSAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wer bin ich doch und mein verdienst mit mir/", "tokens": ["Wer", "bin", "ich", "doch", "und", "mein", "ver\u00b7dienst", "mit", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "KON", "PPOSAT", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mich solcher gunst und liebe werth zu sch\u00e4tzen?", "tokens": ["Mich", "sol\u00b7cher", "gunst", "und", "lie\u00b7be", "werth", "zu", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das heist sich selbst zu tief herunter setzen/", "tokens": ["Das", "heist", "sich", "selbst", "zu", "tief", "her\u00b7un\u00b7ter", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKA", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wenn man ein nichts zieht vielem etwas f\u00fcr.", "tokens": ["Wenn", "man", "ein", "nichts", "zieht", "vie\u00b7lem", "et\u00b7was", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "PIS", "VVFIN", "PIS", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wer kiefelstein vor diamanten kiest/", "tokens": ["Wer", "kie\u00b7fel\u00b7stein", "vor", "di\u00b7a\u00b7man\u00b7ten", "kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wer kohlen gr\u00e4bt/ und l\u00e4st den goldklump liegen/", "tokens": ["Wer", "koh\u00b7len", "gr\u00e4bt", "/", "und", "l\u00e4st", "den", "gold\u00b7klump", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VVFIN", "$(", "KON", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wem kleinigkeit sein grosses wesen ist/", "tokens": ["Wem", "klei\u00b7nig\u00b7keit", "sein", "gros\u00b7ses", "we\u00b7sen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und kan mit witz sich doch dabey vergn\u00fcgen:", "tokens": ["Und", "kan", "mit", "witz", "sich", "doch", "da\u00b7bey", "ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PRF", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den zwingt f\u00fcrwahr der himmel selbst darzu", "tokens": ["Den", "zwingt", "f\u00fcr\u00b7wahr", "der", "him\u00b7mel", "selbst", "dar\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "ADV", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Durch ein ger\u00fcst von sehr verborgnen stricken/", "tokens": ["Durch", "ein", "ge\u00b7r\u00fcst", "von", "sehr", "ver\u00b7borg\u00b7nen", "stri\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Das heimlich wirckt/ und l\u00e4st uns keine ruh/", "tokens": ["Das", "heim\u00b7lich", "wirckt", "/", "und", "l\u00e4st", "uns", "kei\u00b7ne", "ruh", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$(", "KON", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Bi\u00df man sich mu\u00df in sein verh\u00e4ngni\u00df schicken.", "tokens": ["Bi\u00df", "man", "sich", "mu\u00df", "in", "sein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Man f\u00fchlt den trieb und merckt doch keinen zwang/", "tokens": ["Man", "f\u00fchlt", "den", "trieb", "und", "merckt", "doch", "kei\u00b7nen", "zwang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "VVFIN", "KON", "VVFIN", "ADV", "PIAT", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die reden sind samt ihrer krafft verborgen;", "tokens": ["Die", "re\u00b7den", "sind", "samt", "ih\u00b7rer", "krafft", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Ein wei\u00df nicht was durch unsichtbaren gang", "tokens": ["Ein", "wei\u00df", "nicht", "was", "durch", "un\u00b7sicht\u00b7ba\u00b7ren", "gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PTKNEG", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Verwickelt uns in wei\u00df nicht was f\u00fcr sorgen.", "tokens": ["Ver\u00b7wi\u00b7ckelt", "uns", "in", "wei\u00df", "nicht", "was", "f\u00fcr", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "VVFIN", "PTKNEG", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Da h\u00f6rt man nichts von klugem unterscheid/", "tokens": ["Da", "h\u00f6rt", "man", "nichts", "von", "klu\u00b7gem", "un\u00b7ter\u00b7scheid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da hebt sich auf/ was werth und unwerth machet/", "tokens": ["Da", "hebt", "sich", "auf", "/", "was", "werth", "und", "un\u00b7werth", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "$(", "PWS", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Der sitzt im schoo\u00df/ und jener stirbt vor neid;", "tokens": ["Der", "sitzt", "im", "schoo\u00df", "/", "und", "je\u00b7ner", "stirbt", "vor", "neid", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "VVFIN", "$(", "KON", "PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der wird geliebt/ ein ander wird verlachet.", "tokens": ["Der", "wird", "ge\u00b7liebt", "/", "ein", "an\u00b7der", "wird", "ver\u00b7la\u00b7chet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$(", "ART", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Hier seht ihr euch leibhafftig vorgestellt/", "tokens": ["Hier", "seht", "ihr", "euch", "leib\u00b7haff\u00b7tig", "vor\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Moin ander ich/ mein eintziges vergn\u00fcgen/", "tokens": ["Moin", "an\u00b7der", "ich", "/", "mein", "eint\u00b7zi\u00b7ges", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "$(", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Ich bin das nichts/ ihr mir die gantze welt;", "tokens": ["Ich", "bin", "das", "nichts", "/", "ihr", "mir", "die", "gant\u00b7ze", "welt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIS", "$(", "PPER", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "An statt ich solt zu euren f\u00fcssen liegen/", "tokens": ["An", "statt", "ich", "solt", "zu", "eu\u00b7ren", "f\u00fcs\u00b7sen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "So liebt ihr mich und zwar mit h\u00f6chster treu/", "tokens": ["So", "liebt", "ihr", "mich", "und", "zwar", "mit", "h\u00f6chs\u00b7ter", "treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "ADV", "APPR", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Mit reinigkeit den engeln gleich zu sch\u00e4tzen;", "tokens": ["Mit", "rei\u00b7nig\u00b7keit", "den", "en\u00b7geln", "gleich", "zu", "sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Mit keuscher brunst/ die immer kan ergetzen/", "tokens": ["Mit", "keu\u00b7scher", "brunst", "/", "die", "im\u00b7mer", "kan", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "ART", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und ohne schuld wird alle morgen neu.", "tokens": ["Und", "oh\u00b7ne", "schuld", "wird", "al\u00b7le", "mor\u00b7gen", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Fahrt fort/ mein schatz/ mein allerh\u00f6chstes gut/", "tokens": ["Fahrt", "fort", "/", "mein", "schatz", "/", "mein", "al\u00b7ler\u00b7h\u00f6chs\u00b7tes", "gut", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Durch dieses band uns ewig zu verbinden/", "tokens": ["Durch", "die\u00b7ses", "band", "uns", "e\u00b7wig", "zu", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "In meiner brust soll sich kein ende finden/", "tokens": ["In", "mei\u00b7ner", "brust", "soll", "sich", "kein", "en\u00b7de", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Das schwer ich euch bey unsrer sch\u00f6nen glut.", "tokens": ["Das", "schwer", "ich", "euch", "bey", "uns\u00b7rer", "sch\u00f6\u00b7nen", "glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "So lange sich mein blut in adern regt/", "tokens": ["So", "lan\u00b7ge", "sich", "mein", "blut", "in", "a\u00b7dern", "regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "PPOSAT", "NN", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und meinem leib empfindlichkeit wird geben;", "tokens": ["Und", "mei\u00b7nem", "leib", "emp\u00b7find\u00b7lich\u00b7keit", "wird", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "So lange noch mein hertz zur lincken schl\u00e4gt/", "tokens": ["So", "lan\u00b7ge", "noch", "mein", "hertz", "zur", "lin\u00b7cken", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "So lange seyd auch ihr mein liebstes leben.", "tokens": ["So", "lan\u00b7ge", "seyd", "auch", "ihr", "mein", "liebs\u00b7tes", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "PPER", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Nun fragt euch selbst/ ob man dergleichen mann/", "tokens": ["Nun", "fragt", "euch", "selbst", "/", "ob", "man", "derg\u00b7lei\u00b7chen", "mann", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PIS", "PIS", "NN", "$("], "meter": "-+-+-+-+-