{"dta.poem.20489": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin verletzt durch deinen augen-strahl/", "tokens": ["Ich", "bin", "ver\u00b7letzt", "durch", "dei\u00b7nen", "au\u00b7gen\u00b7strahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seinen plitz in meine brust getrieben/", "tokens": ["Der", "sei\u00b7nen", "plitz", "in", "mei\u00b7ne", "brust", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll/ Lesbia! du ursprung dieser qvaal/", "tokens": ["Soll", "/", "Les\u00b7bia", "!", "du", "ur\u00b7sprung", "die\u00b7ser", "qvaal", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "NE", "$.", "PPER", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Vergehen nicht mein hertze gantz im lieben/", "tokens": ["Ver\u00b7ge\u00b7hen", "nicht", "mein", "hert\u00b7ze", "gantz", "im", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "ADV", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So halte doch nur einen augenblick", "tokens": ["So", "hal\u00b7te", "doch", "nur", "ei\u00b7nen", "au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "den strahl zur\u00fcck.", "tokens": ["den", "strahl", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn brennt die nacht der liebes-flam\u0303e nicht/", "tokens": ["Wenn", "brennt", "die", "nacht", "der", "lie\u00b7bes\u00b7flam\u0303e", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als die zur glut dem menschen ist erkohren?", "tokens": ["Als", "die", "zur", "glut", "dem", "men\u00b7schen", "ist", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPRART", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein gantzes meer lescht nicht ihr sch\u00f6neslicht/", "tokens": ["Ein", "gant\u00b7zes", "meer", "lescht", "nicht", "ihr", "sch\u00f6\u00b7nes\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dessen abgrund Venus ward gebohren/", "tokens": ["In", "des\u00b7sen", "ab\u00b7grund", "Ve\u00b7nus", "ward", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In wellen schwamm di\u00df sch\u00f6ne ungeheur/", "tokens": ["In", "wel\u00b7len", "schwamm", "di\u00df", "sch\u00f6\u00b7ne", "un\u00b7ge\u00b7heur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVFIN", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und bleibt ein feur.", "tokens": ["Und", "bleibt", "ein", "feur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mein hertz besteht aus wachs und nicht aus ei\u00df/", "tokens": ["Mein", "hertz", "be\u00b7steht", "aus", "wachs", "und", "nicht", "aus", "ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "KON", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchl und seh/ wie deine augen plitzen/", "tokens": ["Ich", "f\u00fchl", "und", "seh", "/", "wie", "dei\u00b7ne", "au\u00b7gen", "plit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zweyfache glut ist sterblichen zu hei\u00df/", "tokens": ["Zwey\u00b7fa\u00b7che", "glut", "ist", "sterb\u00b7li\u00b7chen", "zu", "hei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Was wunder/ wenn zwo sonnen mich erhitzen/", "tokens": ["Was", "wun\u00b7der", "/", "wenn", "zwo", "son\u00b7nen", "mich", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "CARD", "ADJA", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die nur der himmel seiner sch\u00f6nheit preist/", "tokens": ["Die", "nur", "der", "him\u00b7mel", "sei\u00b7ner", "sch\u00f6n\u00b7heit", "preist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und brennen heist.", "tokens": ["Und", "bren\u00b7nen", "heist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nicht dencke/ da\u00df es blosse worte seyn/", "tokens": ["Nicht", "den\u00b7cke", "/", "da\u00df", "es", "blos\u00b7se", "wor\u00b7te", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "KOUS", "PPER", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Welch hertz kan wohl bey deiner glut erkalten?", "tokens": ["Welch", "hertz", "kan", "wohl", "bey", "dei\u00b7ner", "glut", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du weist/ ich bin kein engel und kein stein/", "tokens": ["Du", "weist", "/", "ich", "bin", "kein", "en\u00b7gel", "und", "kein", "stein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich mu\u00df des blutes regung lassen walten/", "tokens": ["Ich", "mu\u00df", "des", "blu\u00b7tes", "re\u00b7gung", "las\u00b7sen", "wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die GOtt dem menschen schon im paradie\u00df", "tokens": ["Die", "Gott", "dem", "men\u00b7schen", "schon", "im", "pa\u00b7ra\u00b7die\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ins hertze blie\u00df.", "tokens": ["Ins", "hert\u00b7ze", "blie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Drum z\u00fcrne nicht ob diesen meinen brand/", "tokens": ["Drum", "z\u00fcr\u00b7ne", "nicht", "ob", "die\u00b7sen", "mei\u00b7nen", "brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "KOUS", "PDAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sich aus deiner augen glut entsponnen/", "tokens": ["Der", "sich", "aus", "dei\u00b7ner", "au\u00b7gen", "glut", "ent\u00b7spon\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist/ mein kind/ ein werck von deiner hand/", "tokens": ["Es", "ist", "/", "mein", "kind", "/", "ein", "werck", "von", "dei\u00b7ner", "hand", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach! dencke nach und straffe deine sonnen/", "tokens": ["Ach", "!", "den\u00b7cke", "nach", "und", "straf\u00b7fe", "dei\u00b7ne", "son\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "APPR", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aus welchen dieses feur/ so in mir glimmt/", "tokens": ["Aus", "wel\u00b7chen", "die\u00b7ses", "feur", "/", "so", "in", "mir", "glimmt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PDAT", "NN", "$(", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den ursprung nimmt.", "tokens": ["Den", "ur\u00b7sprung", "nimmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich liebe dann was deine krafft versehrt/", "tokens": ["Ich", "lie\u00b7be", "dann", "was", "dei\u00b7ne", "krafft", "ver\u00b7sehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein niedrig seyn kan deinen ruhm nicht tilgen/", "tokens": ["Mein", "nied\u00b7rig", "seyn", "kan", "dei\u00b7nen", "ruhm", "nicht", "til\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAINF", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sonne bleibet doch in gleichem werth/", "tokens": ["Die", "son\u00b7ne", "blei\u00b7bet", "doch", "in", "glei\u00b7chem", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mahlt gleich ihr gold ein kleeblat nebst den lilgen/", "tokens": ["Mahlt", "gleich", "ihr", "gold", "ein", "klee\u00b7blat", "nebst", "den", "lil\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df mich bey deinem warmen sonnenschein", "tokens": ["La\u00df", "mich", "bey", "dei\u00b7nem", "war\u00b7men", "son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein kleeblat seyn.", "tokens": ["Ein", "klee\u00b7blat", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}