{"textgrid.poem.61419": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum erstenmal, nachdem in Lust und Leid", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum erstenmal, nachdem in Lust und Leid", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", ",", "nach\u00b7dem", "in", "Lust", "und", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich manches Lied zum Spiel den Winden gab,", "tokens": ["Ich", "man\u00b7ches", "Lied", "zum", "Spiel", "den", "Win\u00b7den", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Betret' ich heut der B\u00fchne wechselnd Reich;", "tokens": ["Be\u00b7tret'", "ich", "heut", "der", "B\u00fch\u00b7ne", "wech\u00b7selnd", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht mit leichtem Sinne. Nein, ich wei\u00df,", "tokens": ["Und", "nicht", "mit", "leich\u00b7tem", "Sin\u00b7ne", ".", "Nein", ",", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$.", "PTKANT", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Gro\u00dfes ich mit junger Kraft gewagt.", "tokens": ["Da\u00df", "Gro\u00b7\u00dfes", "ich", "mit", "jun\u00b7ger", "Kraft", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn nicht geziemt es mehr, den M\u00fc\u00dfiggang", "tokens": ["Denn", "nicht", "ge\u00b7ziemt", "es", "mehr", ",", "den", "M\u00fc\u00b7\u00dfig\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im g\u00f6tterlosen Haus durch fl\u00fccht'gen Reiz", "tokens": ["Im", "g\u00f6t\u00b7ter\u00b7lo\u00b7sen", "Haus", "durch", "fl\u00fccht'\u00b7gen", "Reiz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und kurze \u00dcberraschung zu zerstreun;", "tokens": ["Und", "kur\u00b7ze", "\u00dc\u00b7berr\u00b7as\u00b7chung", "zu", "zer\u00b7streun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es sei die B\u00fchne, was dereinst sie war,", "tokens": ["Es", "sei", "die", "B\u00fch\u00b7ne", ",", "was", "de\u00b7reinst", "sie", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein Heiligtum; es sei das Trauerspiel", "tokens": ["Ein", "Hei\u00b7lig\u00b7tum", ";", "es", "sei", "das", "Trau\u00b7er\u00b7spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein dunkler Spiegel, drin, zum Bild gefa\u00dft,", "tokens": ["Ein", "dunk\u00b7ler", "Spie\u00b7gel", ",", "drin", ",", "zum", "Bild", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das ewige Gesetz des Weltengangs", "tokens": ["Das", "e\u00b7wi\u00b7ge", "Ge\u00b7setz", "des", "Wel\u00b7ten\u00b7gangs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gestaltenreich dem Volk sich offenbart.", "tokens": ["Ge\u00b7stal\u00b7ten\u00b7reich", "dem", "Volk", "sich", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum wolle keiner, der in Zeit und Vorzeit", "tokens": ["Drum", "wol\u00b7le", "kei\u00b7ner", ",", "der", "in", "Zeit", "und", "Vor\u00b7zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Gottes m\u00e4cht'ges Schreiten nie vernahm", "tokens": ["Des", "Got\u00b7tes", "m\u00e4cht'\u00b7ges", "Schrei\u00b7ten", "nie", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nicht die S\u00fchnung kennt und nicht das Ma\u00df,", "tokens": ["Und", "nicht", "die", "S\u00fch\u00b7nung", "kennt", "und", "nicht", "das", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier Priester sein. Und wer zu opfern kommt,", "tokens": ["Hier", "Pries\u00b7ter", "sein", ".", "Und", "wer", "zu", "op\u00b7fern", "kommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAINF", "$.", "KON", "PWS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sei reines Sinns und nahe sich in Ehrfurcht", "tokens": ["Sei", "rei\u00b7nes", "Sinns", "und", "na\u00b7he", "sich", "in", "Ehr\u00b7furcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "KON", "APPR", "PRF", "APPR", "NN"], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Der ernsten Muse, der gewaltigen,", "tokens": ["Der", "erns\u00b7ten", "Mu\u00b7se", ",", "der", "ge\u00b7wal\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die hochherwandelnd Tat und Missetat", "tokens": ["Die", "hoch\u00b7her\u00b7wan\u00b7delnd", "Tat", "und", "Mis\u00b7se\u00b7tat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Sterblichen in erzner Schale w\u00e4gt.", "tokens": ["Der", "Sterb\u00b7li\u00b7chen", "in", "erz\u00b7ner", "Scha\u00b7le", "w\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So tret' auch ich heran, und wie ich schreite,", "tokens": ["So", "tret'", "auch", "ich", "he\u00b7ran", ",", "und", "wie", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PTKVZ", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewegt ein leiser Schauer mir die Brust,", "tokens": ["Be\u00b7wegt", "ein", "lei\u00b7ser", "Schau\u00b7er", "mir", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hebt mir ", "tokens": ["Doch", "hebt", "mir"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Nach W\u00fcrdigem, und wer des Lebens Kraft", "tokens": ["Nach", "W\u00fcr\u00b7di\u00b7gem", ",", "und", "wer", "des", "Le\u00b7bens", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "KON", "PWS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Gro\u00dfes setzt, den f\u00fchret gern ein Gott", "tokens": ["An", "Gro\u00b7\u00dfes", "setzt", ",", "den", "f\u00fch\u00b7ret", "gern", "ein", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "ART", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zuletzt ans Ziel, ob er auf seiner Bahn", "tokens": ["Zu\u00b7letzt", "ans", "Ziel", ",", "ob", "er", "auf", "sei\u00b7ner", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch viel erdulden m\u00fcsse.", "tokens": ["Auch", "viel", "er\u00b7dul\u00b7den", "m\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Langsam ringt", "tokens": ["Lang\u00b7sam", "ringt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Im dunkeln Schacht die Flut, bis hoffend sie", "tokens": ["Im", "dun\u00b7keln", "Schacht", "die", "Flut", ",", "bis", "hof\u00b7fend", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hervorspringt und das hei\u00dfersehnte Licht,", "tokens": ["Her\u00b7vor\u00b7springt", "und", "das", "hei\u00b7\u00dfer\u00b7sehn\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den goldnen Tag mit klaren Augen gr\u00fc\u00dft;", "tokens": ["Den", "gold\u00b7nen", "Tag", "mit", "kla\u00b7ren", "Au\u00b7gen", "gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch dann noch rinnt sie leiser durchs Gestein,", "tokens": ["Auch", "dann", "noch", "rinnt", "sie", "lei\u00b7ser", "durchs", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In steter Kr\u00fcmmung ihre Pfade suchend;", "tokens": ["In", "ste\u00b7ter", "Kr\u00fcm\u00b7mung", "ih\u00b7re", "Pfa\u00b7de", "su\u00b7chend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch gn\u00e4dig schlie\u00dfet sich der Himmel auf", "tokens": ["Doch", "gn\u00e4\u00b7dig", "schlie\u00b7\u00dfet", "sich", "der", "Him\u00b7mel", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und schickt den frischen Wolkensohn, den Regen,", "tokens": ["Und", "schickt", "den", "fri\u00b7schen", "Wol\u00b7ken\u00b7sohn", ",", "den", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und sendet ihr die fr\u00f6hlichen Geschwister,", "tokens": ["Und", "sen\u00b7det", "ihr", "die", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die felsgebornen, vom Gebirg' herab.", "tokens": ["Die", "fels\u00b7ge\u00b7bor\u00b7nen", ",", "vom", "Ge\u00b7bir\u00b7g'", "her\u00b7ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Da schwillt sie k\u00fchn empor, gekr\u00e4ftigt bricht sie", "tokens": ["Da", "schwillt", "sie", "k\u00fchn", "em\u00b7por", ",", "ge\u00b7kr\u00e4f\u00b7tigt", "bricht", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "VVPP", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Durch Klippentr\u00fcmmer sich die eigne Bahn,", "tokens": ["Durch", "Klip\u00b7pen\u00b7tr\u00fcm\u00b7mer", "sich", "die", "eig\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und endlich, siegreich durch die T\u00e4ler wandelnd,", "tokens": ["Und", "end\u00b7lich", ",", "sieg\u00b7reich", "durch", "die", "T\u00e4\u00b7ler", "wan\u00b7delnd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Tr\u00e4nkt sie die Flur, und spiegelt sie die Sonne,", "tokens": ["Tr\u00e4nkt", "sie", "die", "Flur", ",", "und", "spie\u00b7gelt", "sie", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein goldner Strom des Segens.", "tokens": ["Ein", "gold\u00b7ner", "Strom", "des", "Se\u00b7gens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Also reift", "tokens": ["Al\u00b7so", "reift"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Auch Weisheit langsam, und ein andres bringt", "tokens": ["Auch", "Weis\u00b7heit", "lang\u00b7sam", ",", "und", "ein", "and\u00b7res", "bringt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJD", "$,", "KON", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Jugend rascher Sinn, ein anderes", "tokens": ["Der", "Ju\u00b7gend", "ra\u00b7scher", "Sinn", ",", "ein", "an\u00b7de\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus reichem Schatz des Manns gepr\u00fcfter Geist.", "tokens": ["Aus", "rei\u00b7chem", "Schatz", "des", "Manns", "ge\u00b7pr\u00fcf\u00b7ter", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich habe heute nur ein J\u00fcnglingswerk;", "tokens": ["Ich", "ha\u00b7be", "heu\u00b7te", "nur", "ein", "J\u00fcng\u00b7lings\u00b7werk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch leg' ich's dankbar als die einz'ge Gabe,", "tokens": ["Doch", "leg'", "ich's", "dank\u00b7bar", "als", "die", "einz'\u00b7ge", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die deinesgleichen ich zu bieten wei\u00df,", "tokens": ["Die", "dei\u00b7nes\u00b7glei\u00b7chen", "ich", "zu", "bie\u00b7ten", "wei\u00df", "