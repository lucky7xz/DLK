{"dta.poem.1048": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der 4. Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dan wie kont er immer finden", "tokens": ["Dan", "wie", "kont", "er", "im\u00b7mer", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer sch\u00f6nheit gleichen brand", "tokens": ["Ei\u00b7ner", "sch\u00f6n\u00b7heit", "glei\u00b7chen", "brand"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd was anders sehlen band", "tokens": ["Vnd", "was", "an\u00b7ders", "seh\u00b7len", "band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kont jhn wol so hart verbinden?", "tokens": ["Kont", "jhn", "wol", "so", "hart", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein aug l\u00fcfert allezeit", "tokens": ["Sein", "aug", "l\u00fc\u00b7fert", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Seine sehl deiner sch\u00f6nheit;", "tokens": ["Sei\u00b7ne", "sehl", "dei\u00b7ner", "sch\u00f6n\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wa ersein gesicht hinkehret/", "tokens": ["Wa", "er\u00b7sein", "ge\u00b7sicht", "hin\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wirt dadurch sein lieb vermehret:", "tokens": ["Wirt", "da\u00b7durch", "sein", "lieb", "ver\u00b7meh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dan alle sch\u00f6nheiten sunst/", "tokens": ["Dan", "al\u00b7le", "sch\u00f6n\u00b7hei\u00b7ten", "sunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Welche sich je vor Euch naigen", "tokens": ["Wel\u00b7che", "sich", "je", "vor", "Euch", "nai\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADV", "APPR", "PPER", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Herrlich von Natur vnd kunst/", "tokens": ["Herr\u00b7lich", "von", "Na\u00b7tur", "vnd", "kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Thun nichts dan jhm nur anzaigen/", "tokens": ["Thun", "nichts", "dan", "jhm", "nur", "an\u00b7zai\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "ADV", "VVIZU", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wievil dich der himmeln macht", "tokens": ["Wie\u00b7vil", "dich", "der", "him\u00b7meln", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Treflicher dan Sie gemacht.", "tokens": ["Tref\u00b7li\u00b7cher", "dan", "Sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}