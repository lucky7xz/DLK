{"dta.poem.16818": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "177.  Es wird nicht alls gerichtet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen die in Gott mit Christo sind verschl\u0169gen/", "tokens": ["Die", "Men\u00b7schen", "die", "in", "Gott", "mit", "Chris\u00b7to", "sind", "ver\u00b7schl\u0169\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "APPR", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind durchs Gericht\u2019 und Tod gantz seelig durchgedrun-", "tokens": ["Sind", "durchs", "Ge\u00b7richt'", "und", "Tod", "gantz", "see\u00b7lig", "durch\u00b7ge\u00b7drun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "KON", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}}}}}