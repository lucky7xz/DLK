{"dta.poem.10937": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! in deiner Werke Pracht,", "tokens": ["Herr", "!", "in", "dei\u00b7ner", "Wer\u00b7ke", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich deine weise Macht,", "tokens": ["Kann", "ich", "dei\u00b7ne", "wei\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit ger\u00fchrtem Herzen, sehn.", "tokens": ["Mit", "ge\u00b7r\u00fchr\u00b7tem", "Her\u00b7zen", ",", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ist alles doch so sch\u00f6n!", "tokens": ["Wie", "ist", "al\u00b7les", "doch", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer kann ihre Menge z\u00e4hlen!", "tokens": ["Wer", "kann", "ih\u00b7re", "Men\u00b7ge", "z\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ruf ich, mit erfreuter Seelen.", "tokens": ["Ruf", "ich", ",", "mit", "er\u00b7freu\u00b7ter", "See\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fc\u00dft ich doch, zu deinem Preise,", "tokens": ["W\u00fc\u00dft", "ich", "doch", ",", "zu", "dei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine dir gef\u00e4llge Weise,", "tokens": ["Ei\u00b7ne", "dir", "ge\u00b7f\u00e4ll\u00b7ge", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dich in ihnen zu erh\u00f6hn!", "tokens": ["Dich", "in", "ih\u00b7nen", "zu", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}