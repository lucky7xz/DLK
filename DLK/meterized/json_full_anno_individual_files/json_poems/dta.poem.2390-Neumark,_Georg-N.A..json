{"dta.poem.2390": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": " als dieses war geschehn/", "tokens": ["als", "die\u00b7ses", "war", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wars eben \u00fcm die Zeit wenn Titan von uns gehn", "tokens": ["Wars", "e\u00b7ben", "\u00fcm", "die", "Zeit", "wenn", "Ti\u00b7tan", "von", "uns", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "KOUS", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich verbergen wil/ hergegen wenn die Sterne", "tokens": ["Und", "sich", "ver\u00b7ber\u00b7gen", "wil", "/", "her\u00b7ge\u00b7gen", "wenn", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PRF", "VVINF", "VMFIN", "$(", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem F\u00fchrer Hesperus/ in grosser Zahl von ferne", "tokens": ["Dem", "F\u00fch\u00b7rer", "Hes\u00b7pe\u00b7rus", "/", "in", "gros\u00b7ser", "Zahl", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$(", "APPR", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie Diener folgen nach/ da\u00df sie mit Dienstbarkeit/", "tokens": ["Wie", "Die\u00b7ner", "fol\u00b7gen", "nach", "/", "da\u00df", "sie", "mit", "Dienst\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "$(", "KOUS", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als ihrer K\u00f6nigin Latonen stehn bereit", "tokens": ["Als", "ih\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", "La\u00b7to\u00b7nen", "stehn", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und warten gleichsam auf/ dar\u00fcm denn unser Sch\u00e4fer", "tokens": ["Und", "war\u00b7ten", "gleich\u00b7sam", "auf", "/", "da\u00b7r\u00fcm", "denn", "un\u00b7ser", "Sch\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "$(", "PAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der wegen seiner Angst/ gleich einem matten Schl\u00e4fer", "tokens": ["Der", "we\u00b7gen", "sei\u00b7ner", "Angst", "/", "gleich", "ei\u00b7nem", "mat\u00b7ten", "Schl\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu seiner Heerde kehrt\u2019 und sie nach Hause trieb\u2019", "tokens": ["Zu", "sei\u00b7ner", "Heer\u00b7de", "kehrt'", "und", "sie", "nach", "Hau\u00b7se", "trieb'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In die bezeunte St\u00e4ll\u2019/ auf da\u00df sie sicher blieb\u2019", "tokens": ["In", "die", "be\u00b7ze\u00b7un\u00b7te", "St\u00e4ll'", "/", "auf", "da\u00df", "sie", "si\u00b7cher", "blieb'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "APPR", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Und frey vor grimmer K\u00e4lt\u2019. In dem er aber jagte", "tokens": ["Und", "frey", "vor", "grim\u00b7mer", "K\u00e4lt'", ".", "In", "dem", "er", "a\u00b7ber", "jag\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$.", "APPR", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein Vieh nach Hause zu/ in dem ihn/ sag ich/ plagte", "tokens": ["Sein", "Vieh", "nach", "Hau\u00b7se", "zu", "/", "in", "dem", "ihn", "/", "sag", "ich", "/", "plag\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PTKZU", "$(", "APPR", "PRELS", "PPER", "$(", "VVIMP", "PPER", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein bittres Hertzeleid sang er mit schwacher Stimm\u2019", "tokens": ["Sein", "bitt\u00b7res", "Hert\u00b7ze\u00b7leid", "sang", "er", "mit", "schwa\u00b7cher", "Stimm'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und eingemischtem Seuftzen", "tokens": ["Und", "ein\u00b7ge\u00b7mischtem", "Seuft\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Die\u00df Klagelied:", "tokens": ["Die\u00df", "Kla\u00b7ge\u00b7lied", ":"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}