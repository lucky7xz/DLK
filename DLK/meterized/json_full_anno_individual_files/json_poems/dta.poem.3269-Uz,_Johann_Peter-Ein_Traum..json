{"dta.poem.3269": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Ein Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "O Traum, der mich entz\u00fccket!", "tokens": ["O", "Traum", ",", "der", "mich", "ent\u00b7z\u00fc\u00b7cket", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hab ich nicht erblicket!", "tokens": ["Was", "hab", "ich", "nicht", "er\u00b7bli\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich warf die m\u00fcden Glieder", "tokens": ["Ich", "warf", "die", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem Thale nieder,", "tokens": ["In", "ei\u00b7nem", "Tha\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo einen Teich, der silbern flo\u00df,", "tokens": ["Wo", "ei\u00b7nen", "Teich", ",", "der", "sil\u00b7bern", "flo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein schattigtes Geb\u00fcsch umschlo\u00df.", "tokens": ["Ein", "schat\u00b7tig\u00b7tes", "Ge\u00b7b\u00fcsch", "um\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sah ich durch die Str\u00e4uche", "tokens": ["Da", "sah", "ich", "durch", "die", "Str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein M\u00e4dchen bey dem Teiche.", "tokens": ["Mein", "M\u00e4d\u00b7chen", "bey", "dem", "Tei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hatte sich, zum Baden,", "tokens": ["Das", "hat\u00b7te", "sich", ",", "zum", "Ba\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kleider meist entladen,", "tokens": ["Der", "Klei\u00b7der", "meist", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bis auf ein untreu wei\u00df Gewand,", "tokens": ["Bis", "auf", "ein", "un\u00b7treu", "wei\u00df", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das keinem L\u00fcftchen widerstand.", "tokens": ["Das", "kei\u00b7nem", "L\u00fcft\u00b7chen", "wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der freye Busen lachte,", "tokens": ["Der", "frey\u00b7e", "Bu\u00b7sen", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Jugend reizend machte.", "tokens": ["Den", "Ju\u00b7gend", "rei\u00b7zend", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Blick blieb l\u00fcstern stehen", "tokens": ["Mein", "Blick", "blieb", "l\u00fcs\u00b7tern", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey diesen regen H\u00f6hen,", "tokens": ["Bey", "die\u00b7sen", "re\u00b7gen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo Zephyr unter Liljen blies,", "tokens": ["Wo", "Ze\u00b7phyr", "un\u00b7ter", "Lil\u00b7jen", "blies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich die Wollust greifen lie\u00df.", "tokens": ["Und", "sich", "die", "Wol\u00b7lust", "grei\u00b7fen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie fieng nun an, o Freuden!", "tokens": ["Sie", "fi\u00b7eng", "nun", "an", ",", "o", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich vollends auszukleiden:", "tokens": ["Sich", "vol\u00b7lends", "aus\u00b7zu\u00b7klei\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, eh\u2019 es noch geschiehet,", "tokens": ["Doch", ",", "eh'", "es", "noch", "ge\u00b7schie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwach ich und sie fliehet.", "tokens": ["Er\u00b7wach", "ich", "und", "sie", "flie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O schlief ich doch von neuem ein!", "tokens": ["O", "schlief", "ich", "doch", "von", "neu\u00b7em", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun wird sie wohl im Wasser seyn.", "tokens": ["Nun", "wird", "sie", "wohl", "im", "Was\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}