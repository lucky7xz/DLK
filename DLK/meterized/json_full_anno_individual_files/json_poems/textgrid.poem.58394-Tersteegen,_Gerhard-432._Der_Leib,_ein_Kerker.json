{"textgrid.poem.58394": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "432. Der Leib, ein Kerker", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Leib auf dieser Erd'", "tokens": ["Der", "Leib", "auf", "die\u00b7ser", "Erd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Stets unsern Geist beschwert,", "tokens": ["Stets", "un\u00b7sern", "Geist", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sitzet als gefangen;", "tokens": ["Er", "sit\u00b7zet", "als", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wer den Herren liebt,", "tokens": ["Doch", "wer", "den", "Her\u00b7ren", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sei nur unbetr\u00fcbt,", "tokens": ["Der", "sei", "nur", "un\u00b7be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er wird bald heimgelangen.", "tokens": ["Er", "wird", "bald", "heim\u00b7ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bald bricht der Strick entzwei,", "tokens": ["Bald", "bricht", "der", "Strick", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dann kann das V\u00f6glein frei", "tokens": ["Dann", "kann", "das", "V\u00f6\u00b7glein", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In Gottes Luft sich schwingen;", "tokens": ["In", "Got\u00b7tes", "Luft", "sich", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Indessen singt es hier", "tokens": ["In\u00b7des\u00b7sen", "singt", "es", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Zum Herren mit Begier,", "tokens": ["Zum", "Her\u00b7ren", "mit", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df es m\u00f6g' wohl gelingen.", "tokens": ["Da\u00df", "es", "m\u00f6g'", "wohl", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Der Leib auf dieser Erd'", "tokens": ["Der", "Leib", "auf", "die\u00b7ser", "Erd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Stets unsern Geist beschwert,", "tokens": ["Stets", "un\u00b7sern", "Geist", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "p