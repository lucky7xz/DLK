{"textgrid.poem.42190": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An Franziska de Warens", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern dacht ich eines Kusses,", "tokens": ["Ge\u00b7stern", "dacht", "ich", "ei\u00b7nes", "Kus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie ihn deine Mutter gab;", "tokens": ["Wie", "ihn", "dei\u00b7ne", "Mut\u00b7ter", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Erinnrung des Genusses", "tokens": ["In", "E\u00b7rinn\u00b7rung", "des", "Ge\u00b7nus\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leckt ich mir die Lippen ab.", "tokens": ["Leckt", "ich", "mir", "die", "Lip\u00b7pen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach das war so warm, so saftig,", "tokens": ["Ach", "das", "war", "so", "warm", ",", "so", "saf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df, ich wei\u00df nicht, wie's geschah", "tokens": ["Da\u00df", ",", "ich", "wei\u00df", "nicht", ",", "wie's", "ge\u00b7schah"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich ich sie ganz leibhaftig", "tokens": ["Pl\u00f6tz\u00b7lich", "ich", "sie", "ganz", "leib\u00b7haf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder bei mir sitzen sah;", "tokens": ["Wie\u00b7der", "bei", "mir", "sit\u00b7zen", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lauschte, wie sie sang und lachte,", "tokens": ["Lauschte", ",", "wie", "sie", "sang", "und", "lach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Manch bedeutungsvolles Wort;", "tokens": ["Manch", "be\u00b7deu\u00b7tungs\u00b7vol\u00b7les", "Wort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber als ich dein gedachte,", "tokens": ["A\u00b7ber", "als", "ich", "dein", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War sie pl\u00f6tzlich wieder fort.", "tokens": ["War", "sie", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.