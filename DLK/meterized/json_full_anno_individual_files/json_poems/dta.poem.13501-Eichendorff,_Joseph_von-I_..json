{"dta.poem.13501": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Jeder nennet froh die Seine,", "tokens": ["Je\u00b7der", "nen\u00b7net", "froh", "die", "Sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "ART", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich nur stehe hier alleine,", "tokens": ["Ich", "nur", "ste\u00b7he", "hier", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn was fr\u00fcge wohl die Eine:", "tokens": ["Denn", "was", "fr\u00fc\u00b7ge", "wohl", "die", "Ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wen der Fremdling eben meine?", "tokens": ["Wen", "der", "Fremd\u00b7ling", "e\u00b7ben", "mei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so mu\u00df ich, wie im Strome dort die Welle,", "tokens": ["Und", "so", "mu\u00df", "ich", ",", "wie", "im", "Stro\u00b7me", "dort", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Ungeh\u00f6rt verrauschen an des Fr\u00fchlings Schwelle.", "tokens": ["Un\u00b7ge\u00b7h\u00f6rt", "ver\u00b7rau\u00b7schen", "an", "des", "Fr\u00fch\u00b7lings", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}