{"textgrid.poem.35891": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdie Sonne scheint heiter am Himmelszelt,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Sonne scheint heiter am Himmelszelt,", "tokens": ["\u00bb", "die", "Son\u00b7ne", "scheint", "hei\u00b7ter", "am", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich will hinaus gehn in's weite Feld.", "tokens": ["Ich", "will", "hin\u00b7aus", "gehn", "in's", "wei\u00b7te", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APZR", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich schreite wohl \u00fcber den klaren Schnee,", "tokens": ["Ich", "schrei\u00b7te", "wohl", "\u00fc\u00b7ber", "den", "kla\u00b7ren", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Im Herzen doch trag' ich manch dunkeles Weh.", "tokens": ["Im", "Her\u00b7zen", "doch", "trag'", "ich", "manch", "dun\u00b7ke\u00b7les", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sonne winkt mir so freundlich zu,", "tokens": ["Die", "Son\u00b7ne", "winkt", "mir", "so", "freund\u00b7lich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als frage sie, was ich denn weinen thu'.", "tokens": ["Als", "fra\u00b7ge", "sie", ",", "was", "ich", "denn", "wei\u00b7nen", "thu'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O liebe Sonne, o herrliches Licht,", "tokens": ["O", "lie\u00b7be", "Son\u00b7ne", ",", "o", "herr\u00b7li\u00b7ches", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du verstehst ja die Schmerzen der Menschen nicht!", "tokens": ["Du", "ver\u00b7stehst", "ja", "die", "Schmer\u00b7zen", "der", "Men\u00b7schen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Daheim die Mutter liegt bleich und krank,", "tokens": ["Da\u00b7heim", "die", "Mut\u00b7ter", "liegt", "bleich", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist Wittwe schon zwei Jahre lang,", "tokens": ["Ist", "Witt\u00b7we", "schon", "zwei", "Jah\u00b7re", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O ja, daheim ist bittere Noth,", "tokens": ["O", "ja", ",", "da\u00b7heim", "ist", "bit\u00b7te\u00b7re", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist f\u00fcr den kleinen Bruder kein Brod.", "tokens": ["Ist", "f\u00fcr", "den", "klei\u00b7nen", "Bru\u00b7der", "kein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Ich n\u00e4he und stricke wol Nacht wie Tag", "tokens": ["Ich", "n\u00e4\u00b7he", "und", "stri\u00b7cke", "wol", "Nacht", "wie", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "NN", "KOKOM", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schaue, was ich erwerben mag;", "tokens": ["Und", "schau\u00b7e", ",", "was", "ich", "er\u00b7wer\u00b7ben", "mag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich sticke die Blumen so bunt und fein;", "tokens": ["Ich", "sti\u00b7cke", "die", "Blu\u00b7men", "so", "bunt", "und", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mir selber doch bl\u00fchet kein Bl\u00fcmelein.", "tokens": ["Mir", "sel\u00b7ber", "doch", "bl\u00fc\u00b7het", "kein", "Bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Was scheinst du denn, Sonne, so hell und warm?", "tokens": ["Was", "scheinst", "du", "denn", ",", "Son\u00b7ne", ",", "so", "hell", "und", "warm", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mich weckest du nicht aus meinem Harm.", "tokens": ["Mich", "we\u00b7ckest", "du", "nicht", "aus", "mei\u00b7nem", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O, l\u00e4g' ich tief unter dem k\u00fchlen Schnee,", "tokens": ["O", ",", "l\u00e4g'", "ich", "tief", "un\u00b7ter", "dem", "k\u00fch\u00b7len", "Schnee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da w\u00fc\u00dft' ich doch nichts von Jammer und Weh!\u00ab\u2013", "tokens": ["Da", "w\u00fc\u00dft'", "ich", "doch", "nichts", "von", "Jam\u00b7mer", "und", "Weh", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "APPR", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und weiter wandert Margareth;", "tokens": ["Und", "wei\u00b7ter", "wan\u00b7dert", "Mar\u00b7ga\u00b7reth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da steht ein Kreuz am Weg erh\u00f6ht,", "tokens": ["Da", "steht", "ein", "Kreuz", "am", "Weg", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das spricht zu ihr von Schuld und Huld", "tokens": ["Das", "spricht", "zu", "ihr", "von", "Schuld", "und", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mahnt an heilige Geduld.", "tokens": ["Und", "mahnt", "an", "hei\u00b7li\u00b7ge", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Fromm gr\u00fc\u00dfet sie das Leidensbild,", "tokens": ["Fromm", "gr\u00fc\u00b7\u00dfet", "sie", "das", "Lei\u00b7dens\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das blickt auf sie so ernst, so mild,", "tokens": ["Das", "blickt", "auf", "sie", "so", "ernst", ",", "so", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da f\u00fchlt sie Scham und Reueschmerz,", "tokens": ["Da", "f\u00fchlt", "sie", "Scham", "und", "Reu\u00b7esc\u00b7hmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Friede kehrt in's kranke Herz.", "tokens": ["Und", "Frie\u00b7de", "kehrt", "in's", "kran\u00b7ke", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbo Vater, vergieb mir die schwere Schuld,", "tokens": ["\u00bb", "o", "Va\u00b7ter", ",", "ver\u00b7gieb", "mir", "die", "schwe\u00b7re", "Schuld", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verzeihe mein Zagen und gieb Geduld!", "tokens": ["Ver\u00b7zei\u00b7he", "mein", "Za\u00b7gen", "und", "gieb", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich wei\u00df ja, Du bist nimmer fern;", "tokens": ["Ich", "wei\u00df", "ja", ",", "Du", "bist", "nim\u00b7mer", "fern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun will ich Alles tragen gern.", "tokens": ["Nun", "will", "ich", "Al\u00b7les", "tra\u00b7gen", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun will ich still nach Hause geh'n,", "tokens": ["Nun", "will", "ich", "still", "nach", "Hau\u00b7se", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will herzlich beten und flei\u00dfig n\u00e4h'n,", "tokens": ["Will", "herz\u00b7lich", "be\u00b7ten", "und", "flei\u00b7\u00dfig", "n\u00e4h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Mutter pflegen mit sanftem Muth;", "tokens": ["Die", "Mut\u00b7ter", "pfle\u00b7gen", "mit", "sanf\u00b7tem", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich wei\u00df ja, Du machst Alles gut.\u00ab", "tokens": ["Ich", "wei\u00df", "ja", ",", "Du", "machst", "Al\u00b7les", "gut", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PIS", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbdie Sonne scheint heiter am Himmelszelt,", "tokens": ["\u00bb", "die", "Son\u00b7ne", "scheint", "hei\u00b7ter", "am", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich will hinaus gehn in's weite Feld.", "tokens": ["Ich", "will", "hin\u00b7aus", "geh