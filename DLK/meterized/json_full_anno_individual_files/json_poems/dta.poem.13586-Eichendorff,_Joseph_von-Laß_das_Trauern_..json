{"dta.poem.13586": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "La\u00df das Trauern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, mein Herz, das bange Trauern", "tokens": ["La\u00df", ",", "mein", "Herz", ",", "das", "ban\u00b7ge", "Trau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$,", "PRELS", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um vergang'nes Erdengl\u00fcck,", "tokens": ["Um", "ver\u00b7gang'\u00b7nes", "Er\u00b7den\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, von diesen Felsenmauern", "tokens": ["Ach", ",", "von", "die\u00b7sen", "Fel\u00b7sen\u00b7mau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweifet nur umsonst der Blick!", "tokens": ["Schwei\u00b7fet", "nur", "um\u00b7sonst", "der", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind denn alle fortgegangen:", "tokens": ["Sind", "denn", "al\u00b7le", "fort\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jugend, Sang und Fr\u00fchlingslust?", "tokens": ["Ju\u00b7gend", ",", "Sang", "und", "Fr\u00fch\u00b7lings\u00b7lust", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lassen, scheidend, nur Verlangen", "tokens": ["Las\u00b7sen", ",", "schei\u00b7dend", ",", "nur", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam mir in meiner Brust?", "tokens": ["Ein\u00b7sam", "mir", "in", "mei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "V\u00f6glein hoch in L\u00fcften reisen,", "tokens": ["V\u00f6\u00b7glein", "hoch", "in", "L\u00fcf\u00b7ten", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schiffe fahren auf der See,", "tokens": ["Schif\u00b7fe", "fah\u00b7ren", "auf", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Seegel, ihre Weisen", "tokens": ["Ih\u00b7re", "See\u00b7gel", ",", "ih\u00b7re", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehren nur des Herzens Weh.", "tokens": ["Meh\u00b7ren", "nur", "des", "Her\u00b7zens", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist vorbei das bunte Ziehen,", "tokens": ["Ist", "vor\u00b7bei", "das", "bun\u00b7te", "Zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Lustig \u00fcber Berg und Kluft,", "tokens": ["Lus\u00b7tig", "\u00fc\u00b7ber", "Berg", "und", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Bilder wechselnd fliehen,", "tokens": ["Wenn", "die", "Bil\u00b7der", "wech\u00b7selnd", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waldhorn immer weiter ruft?", "tokens": ["Wald\u00b7horn", "im\u00b7mer", "wei\u00b7ter", "ruft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Soll die Lieb' auf sonn'gen Matten", "tokens": ["Soll", "die", "Lieb'", "auf", "sonn'\u00b7gen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mehr bau'n ihr pr\u00e4chtig Zelt,", "tokens": ["Nicht", "mehr", "bau'n", "ihr", "pr\u00e4ch\u00b7tig", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebergolden Wald und Schatten", "tokens": ["Ue\u00b7ber\u00b7gol\u00b7den", "Wald", "und", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die weite, sch\u00f6ne Welt? \u2014", "tokens": ["Und", "die", "wei\u00b7te", ",", "sch\u00f6\u00b7ne", "Welt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df das Bangen, la\u00df das Trauern,", "tokens": ["La\u00df", "das", "Ban\u00b7gen", ",", "la\u00df", "das", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helle wieder nur den Blick!", "tokens": ["Hel\u00b7le", "wie\u00b7der", "nur", "den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern von dieser Felsen Mauern,", "tokens": ["Fern", "von", "die\u00b7ser", "Fel\u00b7sen", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht Dir noch gar manches Gl\u00fcck!", "tokens": ["Bl\u00fcht", "Dir", "noch", "gar", "man\u00b7ches", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}