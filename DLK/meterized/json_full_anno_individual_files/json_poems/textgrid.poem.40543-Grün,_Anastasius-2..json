rt,", "tokens": ["\u00bb", "herr", ",", "vom", "Licht\u00b7kranz", ",", "der", "dich", "ziert", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte meinen Erdentagen", "tokens": ["H\u00e4t\u00b7te", "mei\u00b7nen", "Er\u00b7den\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht wohl auch ein Strahl geb\u00fchrt?\u00ab", "tokens": ["Nicht", "wohl", "auch", "ein", "Strahl", "ge\u00b7b\u00fchrt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.19": {"line.1": {"text": "Harfen schlagen Engelch\u00f6re", "tokens": ["Har\u00b7fen", "schla\u00b7gen", "En\u00b7gelc\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um des Allgewalt'gen Thron,", "tokens": ["Um", "des", "All\u00b7ge\u00b7walt'\u00b7gen", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du rufst mit einer Z\u00e4hre,", "tokens": ["Und", "du", "rufst", "mit", "ei\u00b7ner", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Furchtlos, doch im Schmerzenton:", "tokens": ["Furcht\u00b7los", ",", "doch", "im", "Schmer\u00b7zen\u00b7ton", ":"], "token_info": ["word", "punct", "word"