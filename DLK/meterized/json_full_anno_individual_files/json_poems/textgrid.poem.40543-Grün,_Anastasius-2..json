{"textgrid.poem.40543": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst an jenem gro\u00dfen Tage,", "tokens": ["Einst", "an", "je\u00b7nem", "gro\u00b7\u00dfen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir treten allzumal", "tokens": ["Wenn", "wir", "tre\u00b7ten", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Ew'gen Hofgelage", "tokens": ["An", "des", "Ew'\u00b7gen", "Hof\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den offnen Himmelssaal:", "tokens": ["In", "den", "off\u00b7nen", "Him\u00b7mels\u00b7saal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da wird bang manch Herz erzittern,", "tokens": ["Da", "wird", "bang", "manch", "Herz", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PIAT", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Scheu gesenkt sein manch ein Blick;", "tokens": ["Scheu", "ge\u00b7senkt", "sein", "manch", "ein", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAINF", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dein Herz, das wird nicht zittern", "tokens": ["Doch", "dein", "Herz", ",", "das", "wird", "nicht", "zit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht senken sich dein Blick.", "tokens": ["Und", "nicht", "sen\u00b7ken", "sich", "dein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dein Fu\u00df, er wird nicht wanken,", "tokens": ["Und", "dein", "Fu\u00df", ",", "er", "wird", "nicht", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiten wirst du fest und grad,", "tokens": ["Schrei\u00b7ten", "wirst", "du", "fest", "und", "grad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht wie Einer, der zu danken,", "tokens": ["Nicht", "wie", "Ei\u00b7ner", ",", "der", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "PIS", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, wie der zu fordern naht!", "tokens": ["Nein", ",", "wie", "der", "zu", "for\u00b7dern", "naht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie im F\u00fcrstensaal der Arme", "tokens": ["Wie", "im", "F\u00fcrs\u00b7ten\u00b7saal", "der", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolzen Auges rings erblickt,", "tokens": ["Stol\u00b7zen", "Au\u00b7ges", "rings", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mit seinem Schwei\u00df und Harme", "tokens": ["Da\u00df", "mit", "sei\u00b7nem", "Schwei\u00df", "und", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die Majest\u00e4t hier schm\u00fcckt!", "tokens": ["Sich", "die", "Ma\u00b7jes\u00b7t\u00e4t", "hier", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn du zu des Ew'gen F\u00fc\u00dfen", "tokens": ["Wenn", "du", "zu", "des", "Ew'\u00b7gen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Blumenozean", "tokens": ["Ei\u00b7nen", "Blu\u00b7me\u00b7no\u00b7ze\u00b7an"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst in Farbenwogen sprie\u00dfen,", "tokens": ["Siehst", "in", "Far\u00b7ben\u00b7wo\u00b7gen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufst du frei und k\u00fchn hinan:", "tokens": ["Rufst", "du", "frei", "und", "k\u00fchn", "hi\u00b7nan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbherr, von diesen Rosen eine", "tokens": ["\u00bb", "herr", ",", "von", "die\u00b7sen", "Ro\u00b7sen", "ei\u00b7ne"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "APPR", "PDAT", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War schon einst als Knospe mein!", "tokens": ["War", "schon", "einst", "als", "Knos\u00b7pe", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "KOUS", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Arm ward ich, seit sie die deine,", "tokens": ["Arm", "ward", "ich", ",", "seit", "sie", "die", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "PPOSAT", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Du nicht reicher, seit sie dein!\u00ab", "tokens": ["Du", "nicht", "rei\u00b7cher", ",", "seit", "sie", "dein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eine Glorie siehst du wallen,", "tokens": ["Ei\u00b7ne", "Glo\u00b7rie", "siehst", "du", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das Haupt des Ew'gen kr\u00e4nzt,", "tokens": ["Die", "das", "Haupt", "des", "Ew'\u00b7gen", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Morgenr\u00f6then allen,", "tokens": ["Aus", "den", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Erde je gegl\u00e4nzt.", "tokens": ["Die", "der", "Er\u00b7de", "je", "ge\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ohne Scheu wirst du nun fragen:", "tokens": ["Oh\u00b7ne", "Scheu", "wirst", "du", "nun", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbherr, vom Lichtkranz, der dich ziert,", "tokens": ["\u00bb", "herr", ",", "vom", "Licht\u00b7kranz", ",", "der", "dich", "ziert", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte meinen Erdentagen", "tokens": ["H\u00e4t\u00b7te", "mei\u00b7nen", "Er\u00b7den\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht wohl auch ein Strahl geb\u00fchrt?\u00ab", "tokens": ["Nicht", "wohl", "auch", "ein", "Strahl", "ge\u00b7b\u00fchrt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.9": {"line.1": {"text": "Harfen schlagen Engelch\u00f6re", "tokens": ["Har\u00b7fen", "schla\u00b7gen", "En\u00b7gelc\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um des Allgewalt'gen Thron,", "tokens": ["Um", "des", "All\u00b7ge\u00b7walt'\u00b7gen", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du rufst mit einer Z\u00e4hre,", "tokens": ["Und", "du", "rufst", "mit", "ei\u00b7ner", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Furchtlos, doch im Schmerzenton:", "tokens": ["Furcht\u00b7los", ",", "doch", "im", "Schmer\u00b7zen\u00b7ton", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbherr, es war zum Erdgeleite", "tokens": ["\u00bb", "herr", ",", "es", "war", "zum", "Erd\u00b7ge\u00b7lei\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer dieser Engel mein!", "tokens": ["Ei\u00b7ner", "die\u00b7ser", "En\u00b7gel", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du nahmst mir ihn von der Seite, \u2013", "tokens": ["Du", "nahmst", "mir", "ihn", "von", "der", "Sei\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Hergewankt bin ich allein!\u00ab", "tokens": ["Her\u00b7ge\u00b7wankt", "bin", "ich", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Einst an jenem gro\u00dfen Tage,", "tokens": ["Einst", "an", "je\u00b7nem", "gro\u00b7\u00dfen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir treten allzumal", "tokens": ["Wenn", "wir", "tre\u00b7ten", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Ew'gen Hofgelage", "tokens": ["An", "des", "Ew'\u00b7gen", "Hof\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den offnen Himmelssaal:", "tokens": ["In", "den", "off\u00b7nen", "Him\u00b7mels\u00b7saal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da wird bang manch He