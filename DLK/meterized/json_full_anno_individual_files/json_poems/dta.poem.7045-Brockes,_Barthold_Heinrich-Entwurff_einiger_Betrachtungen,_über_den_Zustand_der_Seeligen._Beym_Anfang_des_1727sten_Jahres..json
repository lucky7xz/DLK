{"dta.poem.7045": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beym Ungl\u00fcck wird es gleichfalls n\u00fctzen.", "tokens": ["Beym", "Un\u00b7gl\u00fcck", "wird", "es", "gleich\u00b7falls", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein besserer Begriff von k\u00fcnfft\u2019ger Seeligkeit", "tokens": ["Ein", "bes\u00b7se\u00b7rer", "Be\u00b7griff", "von", "k\u00fcnf\u00b7ft'\u00b7ger", "See\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wird hier die Unvollkommenheit,", "tokens": ["Wird", "hier", "die", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja alles Ungl\u00fccks Sturm und Krachen,", "tokens": ["Ja", "al\u00b7les", "Un\u00b7gl\u00fccks", "Sturm", "und", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns leichter und ertr\u00e4glich machen,", "tokens": ["Uns", "leich\u00b7ter", "und", "er\u00b7tr\u00e4g\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sonderlich uns f\u00fcr Verzweifflung sch\u00fctzen.", "tokens": ["Und", "son\u00b7der\u00b7lich", "uns", "f\u00fcr", "Ver\u00b7zweif\u00b7flung", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}