{"dta.poem.6903": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seelen, welche sich weit \u00fcbern C\u00f6rper heben,", "tokens": ["Die", "See\u00b7len", ",", "wel\u00b7che", "sich", "weit", "\u00fc\u00b7bern", "C\u00f6r\u00b7per", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die k\u00f6nnen ihrem Recht und Vorzug nichts vergeben.", "tokens": ["Die", "k\u00f6n\u00b7nen", "ih\u00b7rem", "Recht", "und", "Vor\u00b7zug", "nichts", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "KON", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vernunfft ist doch Vernunfft, es mag dieselbe seyn", "tokens": ["Ver\u00b7nunfft", "ist", "doch", "Ver\u00b7nunfft", ",", "es", "mag", "die\u00b7sel\u00b7be", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "NN", "$,", "PPER", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gezogen, oder wild: dies Gut ist nur gemein", "tokens": ["Ge\u00b7zo\u00b7gen", ",", "o\u00b7der", "wild", ":", "dies", "Gut", "ist", "nur", "ge\u00b7mein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "ADJD", "$.", "PDS", "ADJD", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Menschheit. W\u00fcrcklich sind zu allen Zeiten,", "tokens": ["Der", "Menschheit", ".", "W\u00fcrck\u00b7lich", "sind", "zu", "al\u00b7len", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Es m\u00f6gen auch", "tokens": ["Es", "m\u00f6\u00b7gen", "auch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Sitten, der Gebrauch", "tokens": ["Die", "Sit\u00b7ten", ",", "der", "Ge\u00b7brauch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Noch so verschiedlich seyn, und mit einander streiten;", "tokens": ["Noch", "so", "ver\u00b7schied\u00b7lich", "seyn", ",", "und", "mit", "ein\u00b7an\u00b7der", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$,", "KON", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Menschen \u00fcberf\u00fchrt von einem Wahrheits-Glantz:", "tokens": ["Die", "Men\u00b7schen", "\u00fc\u00b7berf\u00b7\u00fchrt", "von", "ei\u00b7nem", "Wahr\u00b7heits\u00b7Glantz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob keiner desfalls gleich den andern je um Rath", "tokens": ["Ob", "kei\u00b7ner", "des\u00b7falls", "gleich", "den", "an\u00b7dern", "je", "um", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "ADJA", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gefragt, und sich mit ihm verstanden hat.", "tokens": ["Ge\u00b7fragt", ",", "und", "sich", "mit", "ihm", "ver\u00b7stan\u00b7den", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PRF", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Denn da\u00df ein Theil so gro\u00df nicht, als sein Gantz;", "tokens": ["Denn", "da\u00df", "ein", "Theil", "so", "gro\u00df", "nicht", ",", "als", "sein", "Gantz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ist eine Wahrheit, die man von sich selber sp\u00fcret.", "tokens": ["Ist", "ei\u00b7ne", "Wahr\u00b7heit", ",", "die", "man", "von", "sich", "sel\u00b7ber", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es braucht es der aus China nicht,", "tokens": ["Es", "braucht", "es", "der", "aus", "Chi\u00b7na", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "APPR", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df er davon den Lappen \u00fcberf\u00fchret.", "tokens": ["Da\u00df", "er", "da\u00b7von", "den", "Lap\u00b7pen", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}