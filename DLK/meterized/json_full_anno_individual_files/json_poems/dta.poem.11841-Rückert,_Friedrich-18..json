{"dta.poem.11841": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sturm der Menschenwelt bewegt dich wenig nur,", "tokens": ["Der", "Sturm", "der", "Men\u00b7schen\u00b7welt", "be\u00b7wegt", "dich", "we\u00b7nig", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr verst\u00f6rt dich noch das Schwanken der Natur.", "tokens": ["Viel\u00b7mehr", "ver\u00b7st\u00f6rt", "dich", "noch", "das", "Schwan\u00b7ken", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als k\u00fcmmerte dich gar vom Menschen nicht das Beste,", "tokens": ["Als", "k\u00fcm\u00b7mer\u00b7te", "dich", "gar", "vom", "Men\u00b7schen", "nicht", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nur der Sch\u00f6pfung Gang dir bliebe st\u00e4t und feste.", "tokens": ["Wenn", "nur", "der", "Sch\u00f6p\u00b7fung", "Gang", "dir", "blie\u00b7be", "st\u00e4t", "und", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "PPER", "VVFIN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wie du still dich kannst bei Schicksalswechsel fassen,", "tokens": ["Doch", "wie", "du", "still", "dich", "kannst", "bei", "Schick\u00b7sals\u00b7wech\u00b7sel", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VMFIN", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00dft du endlich auch die Wetterlaunen lassen;", "tokens": ["So", "mu\u00dft", "du", "end\u00b7lich", "auch", "die", "Wet\u00b7ter\u00b7lau\u00b7nen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und merken, da\u00df am Band der Ordnung eine Hand", "tokens": ["Und", "mer\u00b7ken", ",", "da\u00df", "am", "Band", "der", "Ord\u00b7nung", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "APPRART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt Menschenwankelmut und Wetterunbestand.", "tokens": ["H\u00e4lt", "Men\u00b7schen\u00b7wan\u00b7kel\u00b7mut", "und", "Wet\u00b7te\u00b7run\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wie zur Weltgeschicht' Unheil und V\u00f6lkerplage,", "tokens": ["Und", "wie", "zur", "Welt\u00b7ge\u00b7schicht'", "Un\u00b7heil", "und", "V\u00f6l\u00b7ker\u00b7pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So zum Kalender auch geh\u00f6ren schlechte Tage.", "tokens": ["So", "zum", "Ka\u00b7len\u00b7der", "auch", "ge\u00b7h\u00f6\u00b7ren", "schlech\u00b7te", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Drum wirke, dichte nur am angefangnen weiter,", "tokens": ["Drum", "wir\u00b7ke", ",", "dich\u00b7te", "nur", "am", "an\u00b7ge\u00b7fang\u00b7nen", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn tr\u00fcb der Himmel ist, bis er wird wieder heiter.", "tokens": ["Wenn", "tr\u00fcb", "der", "Him\u00b7mel", "ist", ",", "bis", "er", "wird", "wie\u00b7der", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Frosch allein verstummt bei k\u00fchler Nacht im Sumpf;", "tokens": ["Der", "Frosch", "al\u00b7lein", "ver\u00b7stummt", "bei", "k\u00fch\u00b7ler", "Nacht", "im", "Sumpf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nachtigall singt fort, wenn auch ein wenig dumpf.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "fort", ",", "wenn", "auch", "ein", "we\u00b7nig", "dumpf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "ART", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}