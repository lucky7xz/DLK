{"dta.poem.11385": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der auf eine zeit gl\u00fccklich verungl\u00fcckte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weich dem verh\u00e4ngni\u00df itzt, du mehr als tapffrer mann!", "tokens": ["Weich", "dem", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "itzt", ",", "du", "mehr", "als", "tapf\u00b7frer", "mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "$,", "PPER", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "La\u00df den ergrimmten neid nach seinen willen toben!", "tokens": ["La\u00df", "den", "er\u00b7grimm\u00b7ten", "neid", "nach", "sei\u00b7nen", "wil\u00b7len", "to\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn eben dieses sind der tugend wahre proben,", "tokens": ["Denn", "e\u00b7ben", "die\u00b7ses", "sind", "der", "tu\u00b7gend", "wah\u00b7re", "pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gl\u00fcck und ungl\u00fcck sieht mit gleichen augen an:", "tokens": ["Die", "gl\u00fcck", "und", "un\u00b7gl\u00fcck", "sieht", "mit", "glei\u00b7chen", "au\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die, wenn gleich alles sich will wider sie erregen,", "tokens": ["Die", ",", "wenn", "gleich", "al\u00b7les", "sich", "will", "wi\u00b7der", "sie", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADV", "PIS", "PRF", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch im geringsten nicht l\u00e4st ihren muth bewegen.", "tokens": ["Doch", "im", "ge\u00b7rings\u00b7ten", "nicht", "l\u00e4st", "ih\u00b7ren", "muth", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "PTKNEG", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erfahrung und verstand hat dir ja l\u00e4ngst entdeckt,", "tokens": ["Er\u00b7fah\u00b7rung", "und", "ver\u00b7stand", "hat", "dir", "ja", "l\u00e4ngst", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie nur des hofes luft auf lauterm eise leite,", "tokens": ["Wie", "nur", "des", "ho\u00b7fes", "luft", "auf", "lau\u00b7term", "ei\u00b7se", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und offtmahls sonder schuld ein fallen zubereite,", "tokens": ["Und", "offt\u00b7mahls", "son\u00b7der", "schuld", "ein", "fal\u00b7len", "zu\u00b7be\u00b7rei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ART", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihre falsche kost am allerbesten schmeckt.", "tokens": ["Wenn", "ih\u00b7re", "fal\u00b7sche", "kost", "am", "al\u00b7ler\u00b7bes\u00b7ten", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier will die sonne nicht stets sonder wolcken scheinen:", "tokens": ["Hier", "will", "die", "son\u00b7ne", "nicht", "stets", "son\u00b7der", "wol\u00b7cken", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der, so den morgen lacht, mu\u00df offt den abend weinen.", "tokens": ["Der", ",", "so", "den", "mor\u00b7gen", "lacht", ",", "mu\u00df", "offt", "den", "a\u00b7bend", "wei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df auch nicht allezeit herr, oder vaterland,", "tokens": ["Da\u00df", "auch", "nicht", "al\u00b7le\u00b7zeit", "herr", ",", "o\u00b7der", "va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ADV", "PTKVZ", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit gleicher danckbarkeit bezahle m\u00fch und wunden,", "tokens": ["Mit", "glei\u00b7cher", "dan\u00b7ck\u00b7bar\u00b7keit", "be\u00b7zah\u00b7le", "m\u00fch", "und", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die mancher braver held nur blos um sie empfunden,", "tokens": ["Die", "man\u00b7cher", "bra\u00b7ver", "held", "nur", "blos", "um", "sie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als er sein tapffres blut in ihren dienst verwand;", "tokens": ["Als", "er", "sein", "tapf\u00b7fres", "blut", "in", "ih\u00b7ren", "dienst", "ver\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das w\u00fcrde, wenn er nur anitzo noch am leben,", "tokens": ["Das", "w\u00fcr\u00b7de", ",", "wenn", "er", "nur", "a\u00b7nit\u00b7zo", "noch", "am", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Uns der \u2012 \u2012 \u2012 \u2012 ein wahres zeugni\u00df geben.", "tokens": ["Uns", "der", "\u2012", "\u2012", "\u2012", "\u2012", "ein", "wah\u00b7res", "zeug\u00b7ni\u00df", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "$(", "$(", "$(", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie manchem wird gelohnt, als ob er mit verrath,", "tokens": ["Wie", "man\u00b7chem", "wird", "ge\u00b7lohnt", ",", "als", "ob", "er", "mit", "ver\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "VVPP", "$,", "KOKOM", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit untreu, trug und list, sich \u00fcberall beschm\u00fctzet,", "tokens": ["Mit", "un\u00b7treu", ",", "trug", "und", "list", ",", "sich", "\u00fc\u00b7be\u00b7rall", "be\u00b7schm\u00fct\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "$,", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der doch der unschuld selbst in ihrem schose sitzet,", "tokens": ["Der", "doch", "der", "un\u00b7schuld", "selbst", "in", "ih\u00b7rem", "scho\u00b7se", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nichts denn reine treu erwiesen in der that.", "tokens": ["Und", "nichts", "denn", "rei\u00b7ne", "treu", "er\u00b7wie\u00b7sen", "in", "der", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADJA", "ADJD", "VVPP", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier ist Parmenio, und zeigt die m\u00f6rder-wunde,", "tokens": ["Hier", "ist", "Par\u00b7me\u00b7nio", ",", "und", "zeigt", "die", "m\u00f6r\u00b7der\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "KON", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Die vor den langen dienst er sonder schuld empfunde.", "tokens": ["Die", "vor", "den", "lan\u00b7gen", "dienst", "er", "son\u00b7der", "schuld", "emp\u00b7fun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was that nicht Mutius, als mit der rechten hand", "tokens": ["Was", "that", "nicht", "Mu\u00b7tius", ",", "als", "mit", "der", "rech\u00b7ten", "hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "NE", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jm auge seines feinds er jeue flamme nehrte,", "tokens": ["Jm", "au\u00b7ge", "sei\u00b7nes", "feinds", "er", "jeu\u00b7e", "flam\u00b7me", "nehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zwar derselben fleisch, doch nicht den ruhm verzehrte,", "tokens": ["Die", "zwar", "der\u00b7sel\u00b7ben", "fleisch", ",", "doch", "nicht", "den", "ruhm", "ver\u00b7zehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PDAT", "NN", "$,", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ruhm, der sonder schlacht den feind hier \u00fcberwand.", "tokens": ["Den", "ruhm", ",", "der", "son\u00b7der", "schlacht", "den", "feind", "hier", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den feind, der sich getraut viel reiche zu bekriegen,", "tokens": ["Den", "feind", ",", "der", "sich", "ge\u00b7traut", "viel", "rei\u00b7che", "zu", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "PIAT", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan blos ein Mutius mit seinem muth besiegen.", "tokens": ["Kan", "blos", "ein", "Mu\u00b7tius", "mit", "sei\u00b7nem", "muth", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Nun dieser nahm die hand; du nimmst den ehren-ruhm,", "tokens": ["Nun", "die\u00b7ser", "nahm", "die", "hand", ";", "du", "nimmst", "den", "eh\u00b7ren\u00b7ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lernest ihn anitzt die feuer-proben kennen:", "tokens": ["Und", "ler\u00b7nest", "ihn", "a\u00b7nitzt", "die", "feu\u00b7er\u00b7pro\u00b7ben", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt mu\u00df er in der gluth der gr\u00f6sten neider brennen;", "tokens": ["Jtzt", "mu\u00df", "er", "in", "der", "gluth", "der", "gr\u00f6s\u00b7ten", "nei\u00b7der", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch bleibt die reinigkeit sein stetes eigenthum.", "tokens": ["Doch", "bleibt", "die", "rei\u00b7nig\u00b7keit", "sein", "ste\u00b7tes", "ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein felsen-fester mund wird seinen lorbeer finden,", "tokens": ["Dein", "fel\u00b7sen\u00b7fes\u00b7ter", "mund", "wird", "sei\u00b7nen", "lor\u00b7beer", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mehr, denn Mutius, die neider \u00fcberwinden.", "tokens": ["Und", "mehr", ",", "denn", "Mu\u00b7tius", ",", "die", "nei\u00b7der", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "NE", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Dein unfall zwinget dir die hohen geister ein:", "tokens": ["Dein", "un\u00b7fall", "zwin\u00b7get", "dir", "die", "ho\u00b7hen", "geis\u00b7ter", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch gro\u00dfmuth und geduld wirst du gewi\u00dflich schauen,", "tokens": ["Durch", "gro\u00df\u00b7muth", "und", "ge\u00b7duld", "wirst", "du", "ge\u00b7wi\u00df\u00b7lich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie dir dein eigner feind wird ehren-staffeln bauen,", "tokens": ["Wie", "dir", "dein", "eig\u00b7ner", "feind", "wird", "eh\u00b7ren\u00b7staf\u00b7feln", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie sein untergang wird dein erh\u00f6hen seyn;", "tokens": ["Und", "wie", "sein", "un\u00b7ter\u00b7gang", "wird", "dein", "er\u00b7h\u00f6\u00b7hen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie er noch selber sich wird d