{"dta.poem.8007": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Fr\u00e4ulein sah vom Schlosse", "tokens": ["Ein", "Fr\u00e4u\u00b7lein", "sah", "vom", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in\u2019s tiefe Thal.", "tokens": ["Hin\u00b7ab", "in's", "tie\u00b7fe", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Vater kam zu Rosse,", "tokens": ["Ihr", "Va\u00b7ter", "kam", "zu", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er trug ein Kleid von Stahl.", "tokens": ["Er", "trug", "ein", "Kleid", "von", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ewillkomm, Herr Vater, Gottwillkomm!", "tokens": ["\u201e", "will\u00b7komm", ",", "Herr", "Va\u00b7ter", ",", "Gott\u00b7will\u00b7komm", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was bringst du deinem Kinde?", "tokens": ["Was", "bringst", "du", "dei\u00b7nem", "Kin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich war wohl still und fromm.\u201c", "tokens": ["Ich", "war", "wohl", "still", "und", "fromm", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein Kind im weissen Kleide!", "tokens": ["\u201e", "mein", "Kind", "im", "weis\u00b7sen", "Klei\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl hab\u2019 ich dein gedacht.", "tokens": ["Wohl", "hab'", "ich", "dein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen sind dein\u2019 Freude,", "tokens": ["Die", "Blu\u00b7men", "sind", "dein'", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr als des Goldes Pracht.", "tokens": ["Mehr", "als", "des", "Gol\u00b7des", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das Bl\u00fcmlein, klar wie Silber, hier", "tokens": ["Das", "Bl\u00fcm\u00b7lein", ",", "klar", "wie", "Sil\u00b7ber", ",", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm ich dem k\u00fchnen G\u00e4rtner,", "tokens": ["Nahm", "ich", "dem", "k\u00fch\u00b7nen", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gab ihm den Tod daf\u00fcr.\u201c", "tokens": ["Gab", "ihm", "den", "Tod", "da\u00b7f\u00fcr", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ewie war er so verwegen?", "tokens": ["\u201e", "wie", "war", "er", "so", "ver\u00b7we\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warum erschlugst du ihn?", "tokens": ["Wa\u00b7rum", "er\u00b7schlugst", "du", "ihn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er th\u00e4t der Bl\u00fcmlein pflegen,", "tokens": ["Er", "th\u00e4t", "der", "Bl\u00fcm\u00b7lein", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die werden nun verbl\u00fchn.\u201c", "tokens": ["Die", "wer\u00b7den", "nun", "ver\u00b7bl\u00fchn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eer hat mir wunderk\u00fchn versagt", "tokens": ["\u201e", "er", "hat", "mir", "wun\u00b7der\u00b7k\u00fchn", "ver\u00b7sagt"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sch\u00f6nste Blum\u2019 im Garten,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Blum'", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die spart\u2019 er seiner Magd.\u201c", "tokens": ["Die", "spart'", "er", "sei\u00b7ner", "Magd", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Bl\u00fcmlein lag der Zarten", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "lag", "der", "Zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihrer weichen Brust.", "tokens": ["An", "ih\u00b7rer", "wei\u00b7chen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ging in einen Garten,", "tokens": ["Sie", "ging", "in", "ei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der war wohl ihre Lust.", "tokens": ["Der", "war", "wohl", "ih\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da schwoll ein frischer H\u00fcgel auf,", "tokens": ["Da", "schwoll", "ein", "fri\u00b7scher", "H\u00fc\u00b7gel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dort bei den weissen Lilien,", "tokens": ["Dort", "bei", "den", "weis\u00b7sen", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie setzte sich darauf.", "tokens": ["Sie", "setz\u00b7te", "sich", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eo k\u00f6nnt\u2019 ich thun zur Stunde", "tokens": ["\u201e", "o", "k\u00f6nnt'", "ich", "thun", "zur", "Stun\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "VMFIN", "PPER", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den lieben Schwestern gleich!", "tokens": ["Den", "lie\u00b7ben", "Schwes\u00b7tern", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch\u2019s Bl\u00fcmlein gibt kein\u2019 Wunde,", "tokens": ["Doch's", "Bl\u00fcm\u00b7lein", "gibt", "kein'", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist so zart und weich.\u201c", "tokens": ["Es", "ist", "so", "zart", "und", "weich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf\u2019s Bl\u00fcmlein sah sie, bleich und krank,", "tokens": ["Auf's", "Bl\u00fcm\u00b7lein", "sah", "sie", ",", "bleich", "und", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df ihr Bl\u00fcmlein welkte,", "tokens": ["Bis", "da\u00df", "ihr", "Bl\u00fcm\u00b7lein", "welk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis da\u00df sie niedersank.", "tokens": ["Bis", "da\u00df", "sie", "nie\u00b7der\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}