{"textgrid.poem.52944": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich Dich fand, bevor ich heimgegangen,", "tokens": ["Da\u00df", "ich", "Dich", "fand", ",", "be\u00b7vor", "ich", "heim\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df nicht, soll mich's freuen oder schmerzen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "soll", "mich's", "freu\u00b7en", "o\u00b7der", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VMFIN", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und soll ich weilen bei der oder fliehen?", "tokens": ["Und", "soll", "ich", "wei\u00b7len", "bei", "der", "o\u00b7der", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fertig mit jedem liebenden Verlangen", "tokens": ["Fer\u00b7tig", "mit", "je\u00b7dem", "lie\u00b7ben\u00b7den", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Hatt' ich schon abgeschlossen mit dem Herzen", "tokens": ["Hatt'", "ich", "schon", "ab\u00b7ge\u00b7schlos\u00b7sen", "mit", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dachte unter fremden Melodien", "tokens": ["Und", "dach\u00b7te", "un\u00b7ter", "frem\u00b7den", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "K\u00fchl meines Wegs zu ziehen.", "tokens": ["K\u00fchl", "mei\u00b7nes", "Wegs", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nun windest Du den schweren Wanderstecken", "tokens": ["Nun", "win\u00b7dest", "Du", "den", "schwe\u00b7ren", "Wan\u00b7der\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mir aus der Hand und zwingst mich zu Dir nieder;", "tokens": ["Mir", "aus", "der", "Hand", "und", "zwingst", "mich", "zu", "Dir", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ach! tust Du wohl den alten Geist zu wecken,", "tokens": ["Ach", "!", "tust", "Du", "wohl", "den", "al\u00b7ten", "Geist", "zu", "we\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Jugendtr\u00e4um