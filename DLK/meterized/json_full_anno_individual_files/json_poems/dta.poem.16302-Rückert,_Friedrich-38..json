{"dta.poem.16302": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist der Tropfen Thau am Halm, und nicht zu klein", "tokens": ["Sch\u00f6n", "ist", "der", "Trop\u00b7fen", "Thau", "am", "Halm", ",", "und", "nicht", "zu", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$,", "KON", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der gro\u00dfen Sonne selbst ein Spiegelglas zu seyn.", "tokens": ["Der", "gro\u00b7\u00dfen", "Son\u00b7ne", "selbst", "ein", "Spie\u00b7gel\u00b7glas", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist das B\u00e4chlein dann, das kaum zu k\u00fcssen wagt", "tokens": ["Sch\u00f6n", "ist", "das", "B\u00e4ch\u00b7lein", "dann", ",", "das", "kaum", "zu", "k\u00fcs\u00b7sen", "wagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blum', und murmellaut zu werden halb noch zagt.", "tokens": ["Die", "Blum'", ",", "und", "mur\u00b7mel\u00b7laut", "zu", "wer\u00b7den", "halb", "noch", "zagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADJD", "PTKZU", "VAINF", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und sch\u00f6n ist auch der Strom, der sich mit Kraft ergie\u00dft,", "tokens": ["Und", "sch\u00f6n", "ist", "auch", "der", "Strom", ",", "der", "sich", "mit", "Kraft", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Spiel der Woge sich mit Rauschen selbst genie\u00dft.", "tokens": ["Im", "Spiel", "der", "Wo\u00b7ge", "sich", "mit", "Rau\u00b7schen", "selbst", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und so freu immer dich, wenn Sch\u00f6nes dir und Gutes", "tokens": ["Und", "so", "freu", "im\u00b7mer", "dich", ",", "wenn", "Sch\u00f6\u00b7nes", "dir", "und", "Gu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "PPER", "$,", "KOUS", "NN", "PPER", "KON", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Quillt, Thau, Bach oder Strom, perl' oder riesl' und flut' es.", "tokens": ["Quillt", ",", "Thau", ",", "Bach", "o\u00b7der", "Strom", ",", "perl'", "o\u00b7der", "riesl'", "und", "flut'", "es", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NE", "KON", "NN", "$,", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}