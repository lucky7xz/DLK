{"textgrid.poem.57954": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Steuerrad", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem k\u00fchlen Grunde,", "tokens": ["In", "ei\u00b7nem", "k\u00fch\u00b7len", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da rauscht das Steuerrad,", "tokens": ["Da", "rauscht", "das", "Steu\u00b7er\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u00dcberschu\u00df ging fl\u00f6ten,", "tokens": ["Der", "\u00dc\u00b7bersc\u00b7hu\u00df", "ging", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den man getr\u00e4umet hat.", "tokens": ["Den", "man", "ge\u00b7tr\u00e4u\u00b7met", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Gegenwart ist greulich,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "ist", "greu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zukunft, die ist grau,", "tokens": ["Die", "Zu\u00b7kunft", ",", "die", "ist", "grau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns wird betr\u00e4chtlich miese,", "tokens": ["Uns", "wird", "be\u00b7tr\u00e4cht\u00b7lich", "mie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns wird erheblich mau.", "tokens": ["Uns", "wird", "er\u00b7heb\u00b7lich", "mau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Worthalter hat gesprochen,", "tokens": ["Der", "Wort\u00b7hal\u00b7ter", "hat", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er sprach's voll tiefem Weh,", "tokens": ["Er", "sprach's", "voll", "tie\u00b7fem", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns ging ein kaltes Grausen", "tokens": ["Uns", "ging", "ein", "kal\u00b7tes", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Mark und Pfennige.", "tokens": ["Durch", "Mark", "und", "Pfen\u00b7ni\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sprach von neuen Steuern", "tokens": ["Er", "sprach", "von", "neu\u00b7en", "Steu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu uns, der gute Mann;", "tokens": ["Zu", "uns", ",", "der", "gu\u00b7te", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun Kinder, kauft euch K\u00e4mme,", "tokens": ["Nun", "Kin\u00b7der", ",", "kauft", "euch", "K\u00e4m\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die lausige Zeit kommt heran.", "tokens": ["Die", "lau\u00b7si\u00b7ge", "Zeit", "kommt", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "In einem k\u00fchlen Grunde,", "tokens": ["In", "ei\u00b7nem", "k\u00fch\u00b7len", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da rauscht das Steuerrad,", "tokens": ["Da", "rauscht", "das", "Steu\u00b7er\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u00dcberschu\u00df ging fl\u00f6ten,", "tokens": ["Der", "\u00dc\u00b7bersc\u00b7hu\u00df", "ging", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den man getr\u00e4umet hat.", "tokens": ["Den", "man", "ge\u00b7tr\u00e4u\u00b7met", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Gegenwart ist greulich,", "tokens": ["