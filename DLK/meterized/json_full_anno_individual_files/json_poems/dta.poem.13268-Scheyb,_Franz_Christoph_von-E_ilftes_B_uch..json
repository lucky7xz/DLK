{"dta.poem.13268": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer fieng die Weisheit an, und trug die Worte vor:", "tokens": ["Hjer", "fi\u00b7eng", "die", "Weis\u00b7heit", "an", ",", "und", "trug", "die", "Wor\u00b7te", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "430\u201dDes P\u00f6bels Stimme dringt in jedem Fall empor.", "tokens": ["\"", "Des", "P\u00f6\u00b7bels", "Stim\u00b7me", "dringt", "in", "je\u00b7dem", "Fall", "em\u00b7por", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eer glaubt, was Wunder hei\u00dft, besteh\u2019 in seinen Grillen,", "tokens": ["\u201e", "er", "glaubt", ",", "was", "Wun\u00b7der", "hei\u00dft", ",", "be\u00b7steh'", "in", "sei\u00b7nen", "Gril\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWS", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund alles m\u00fcsse sich nach seinem Wahn erf\u00fcllen.", "tokens": ["\u201e", "und", "al\u00b7les", "m\u00fcs\u00b7se", "sich", "nach", "sei\u00b7nem", "Wahn", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIS", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201everzeih, ", "tokens": ["\u201e", "ver\u00b7zeih", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVIMP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "\u201eist, der des P\u00f6bels Ruff den schlechtsten Beyfall gibt.", "tokens": ["\u201e", "ist", ",", "der", "des", "P\u00f6\u00b7bels", "Ruff", "den", "schlechts\u00b7ten", "Bey\u00b7fall", "gibt", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "435\u201dDer P\u00f6bel scheuet mich; so darff ich mich beklagen,", "tokens": ["\"", "Der", "P\u00f6\u00b7bel", "scheu\u00b7et", "mich", ";", "so", "darff", "ich", "mich", "be\u00b7kla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund was ein kluges Herz vom P\u00f6bel halte, sagen.", "tokens": ["\u201e", "und", "was", "ein", "klu\u00b7ges", "Herz", "vom", "P\u00f6\u00b7bel", "hal\u00b7te", ",", "sa\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "PWS", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eer ist im Dencken schwach, in den Begriffen klein;", "tokens": ["\u201e", "er", "ist", "im", "Den\u00b7cken", "schwach", ",", "in", "den", "Be\u00b7grif\u00b7fen", "klein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPRART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201enichts als der Eigensinn gibt ihm das Urtheil ein.", "tokens": ["\u201e", "nichts", "als", "der", "Ei\u00b7gen\u00b7sinn", "gibt", "ihm", "das", "Ur\u00b7theil", "ein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "KOKOM", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "\u201eje mehr der Sachen Lauf ein d\u00fcstrer Schleyr verh\u00fcllet,", "tokens": ["\u201e", "je", "mehr", "der", "Sa\u00b7chen", "Lauf", "ein", "d\u00fcst\u00b7rer", "Schleyr", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "440\u201dJe mehr ist er mit Lust, sie zu verstehn, erf\u00fcllet.", "tokens": ["\"", "Je", "mehr", "ist", "er", "mit", "Lust", ",", "sie", "zu", "ver\u00b7stehn", ",", "er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PPER", "APPR", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "\u201ewann nur ein Blick davon ihm in das Auge f\u00e4llt,", "tokens": ["\u201e", "wann", "nur", "ein", "Blick", "da\u00b7von", "ihm", "in", "das", "Au\u00b7ge", "f\u00e4llt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "PAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eso glaubt er, alles sey so, wie er will, bestellt.", "tokens": ["\u201e", "so", "glaubt", "er", ",", "al\u00b7les", "sey", "so", ",", "wie", "er", "will", ",", "be\u00b7stellt", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PIS", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}