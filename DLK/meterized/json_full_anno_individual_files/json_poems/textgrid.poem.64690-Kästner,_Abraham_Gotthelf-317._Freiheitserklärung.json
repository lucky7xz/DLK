{"textgrid.poem.64690": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "317. Freiheitserkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frei seyd ihr nun! Und Br\u00fcder! Gleich begl\u00fcckt!", "tokens": ["Frei", "seyd", "ihr", "nun", "!", "Und", "Br\u00fc\u00b7der", "!", "Gleich", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$.", "KON", "NN", "$.", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sind gest\u00fcrzt, die euch bisher gedr\u00fcckt!", "tokens": ["Sie", "sind", "ge\u00b7st\u00fcrzt", ",", "die", "euch", "bis\u00b7her", "ge\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sie von euch so lange Zeit genommen,", "tokens": ["Was", "sie", "von", "euch", "so", "lan\u00b7ge", "Zeit", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das m\u00fcssen wir, und noch viel mehr bekommen;", "tokens": ["Das", "m\u00fcs\u00b7sen", "wir", ",", "und", "noch", "viel", "mehr", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "KON", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was eure St\u00e