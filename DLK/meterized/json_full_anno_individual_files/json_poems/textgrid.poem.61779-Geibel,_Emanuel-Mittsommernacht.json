{"textgrid.poem.61779": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Mittsommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Gew\u00f6lk die Sterne lauschen,", "tokens": ["Durchs", "Ge\u00b7w\u00f6lk", "die", "Ster\u00b7ne", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Lilie Duft erwacht;", "tokens": ["Und", "der", "Li\u00b7lie", "Duft", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Willst du mich, wie sonst, berauschen,", "tokens": ["Willst", "du", "mich", ",", "wie", "sonst", ",", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "PWAV", "ADV", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkelschw\u00fcle Sommernacht?", "tokens": ["Dun\u00b7kel\u00b7schw\u00fc\u00b7le", "Som\u00b7mer\u00b7nacht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deiner Elfen Schw\u00e4rme kreisen", "tokens": ["Dei\u00b7ner", "El\u00b7fen", "Schw\u00e4r\u00b7me", "krei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockend wieder um mich her,", "tokens": ["Lo\u00b7ckend", "wie\u00b7der", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct