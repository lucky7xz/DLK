 ["PDS", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm von den Sonnenpfaden,", "tokens": ["Komm", "von", "den", "Son\u00b7nen\u00b7pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Komm, still' uns dies Geschrei,", "tokens": ["Komm", ",", "still'", "uns", "dies", "Ge\u00b7schrei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df hell den Degen klirren", "tokens": ["La\u00df", "hell", "den", "De\u00b7gen", "klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von deiner Sternenburg,", "tokens": ["Von", "dei\u00b7ner", "Ster\u00b7nen\u00b7burg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hau' von den w\u00fcsten Wirren", "tokens": ["Hau'", "von", "den", "w\u00fcs\u00b7ten", "Wir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den ganzen Jammer durch.", "tokens": ["Den", "gan\u00b7zen", "Jam\u00b7mer", "durch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}