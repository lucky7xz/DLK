{"textgrid.poem.59699": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Zug an Gott", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Gott, komm, Gott vom Himmel", "tokens": ["Komm", ",", "Gott", ",", "komm", ",", "Gott", "vom", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sieh in Gnaden drein,", "tokens": ["Und", "sieh", "in", "Gna\u00b7den", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchleuchte das Gewimmel", "tokens": ["Durch\u00b7leuch\u00b7te", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Nacht mit Sonnenschein,", "tokens": ["Der", "Nacht", "mit", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entwirre die Verwirrung,", "tokens": ["Ent\u00b7wir\u00b7re", "die", "Ver\u00b7wir\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ohne Licht und Rat", "tokens": ["Die", "oh\u00b7ne", "Licht", "und", "Rat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stets tiefer in Verirrung", "tokens": ["Stets", "tie\u00b7fer", "in", "Ver\u00b7ir\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verfahren hat den Pfad.", "tokens": ["Ver\u00b7fah\u00b7ren", "hat", "den", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Komm, Gott, komm, Gott der Gnaden!", "tokens": ["Komm", ",", "Gott", ",", "komm", ",", "Gott", "der", "Gna\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hilft nicht Sonnenschein,", "tokens": ["Und", "hilft", "nicht", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So komm mit Blitz geladen", "tokens": ["So", "komm", "mit", "Blitz", "ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und blitz' und donnre drein,", "tokens": ["Und", "blitz'", "und", "donn\u00b7re", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wieder innewerden", "tokens": ["Da\u00df", "wie\u00b7der", "in\u00b7ne\u00b7wer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erbebend Herr und Knecht,", "tokens": ["Er\u00b7be\u00b7bend", "Herr", "und", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Da\u00df Gott regiert auf Erden", "tokens": ["Da\u00df", "Gott", "re\u00b7giert", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und pflegt das h\u00f6chste Recht.", "tokens": ["Und", "pflegt", "das", "h\u00f6chs\u00b7te", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war der Tag gekommen,", "tokens": ["Es", "war", "der", "Tag", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tag der bittern Schmach,", "tokens": ["Der", "Tag", "der", "bit\u00b7tern", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tapfern, Weisen, Frommen", "tokens": ["Der", "Tap\u00b7fern", ",", "Wei\u00b7sen", ",", "From\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das deutsche Herz zerbrach,", "tokens": ["Das", "deut\u00b7sche", "Herz", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es lag von d\u00fcstern Schanden", "tokens": ["Es", "lag", "von", "d\u00fcs\u00b7tern", "Schan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Befleckt das Vaterland", "tokens": ["Be\u00b7fleckt", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In Ketten und in Banden", "tokens": ["In", "Ket\u00b7ten", "und", "in", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch welschen Trug und Tand:", "tokens": ["Durch", "wel\u00b7schen", "Trug", "und", "Tand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Gefesselt in Verstrickung", "tokens": ["Ge\u00b7fes\u00b7selt", "in", "Ver\u00b7stri\u00b7ckung"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der freie deutsche Mann \u2013", "tokens": ["Der", "frei\u00b7e", "deut\u00b7sche", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wehte mit Erquickung", "tokens": ["Da", "weh\u00b7te", "mit", "Er\u00b7quic\u00b7kung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Geist von dir ihn an,", "tokens": ["Der", "Geist", "von", "dir", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da schlug dein hehres Mahnen", "tokens": ["Da", "schlug", "dein", "heh\u00b7res", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Blitz ihm durch die Brust,", "tokens": ["Wie", "Blitz", "ihm", "durch", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gl\u00fcck brausten seine Fahnen,", "tokens": ["Gl\u00fcck", "braus\u00b7ten", "sei\u00b7ne", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Atem Siegeslust.", "tokens": ["Sein", "A\u00b7tem", "Sie\u00b7ges\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach! Von den sch\u00f6nen Tagen,", "tokens": ["Ach", "!", "Von", "den", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von jener Wonnezeit", "tokens": ["Von", "je\u00b7ner", "Won\u00b7ne\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "T\u00f6nt's heut nur wie von Sagen", "tokens": ["T\u00f6nt's", "heut", "nur", "wie", "von", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus l\u00e4ngst verklungner Zeit,", "tokens": ["Aus", "l\u00e4ngst", "ver\u00b7klung\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verworren und beklommen", "tokens": ["Ver\u00b7wor\u00b7ren", "und", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wei\u00df keiner kaum, wohin,", "tokens": ["Wei\u00df", "kei\u00b7ner", "kaum", ",", "wo\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PWAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Weisen selbst und Frommen", "tokens": ["Den", "Wei\u00b7sen", "selbst", "und", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steht still der kluge Sinn.", "tokens": ["Steht", "still", "der", "klu\u00b7ge", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn ach, die einen rufen:", "tokens": ["Denn", "ach", ",", "die", "ei\u00b7nen", "ru\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die andern aber fluchen:", "tokens": ["Die", "an\u00b7dern", "a\u00b7ber", "flu\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und b\u00f6ser Geister Schwirrung", "tokens": ["Und", "b\u00f6\u00b7ser", "Geis\u00b7ter", "Schwir\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umschwirrt dies Nachtgeheul \u2013", "tokens": ["Um\u00b7schwirrt", "dies", "Nacht\u00b7ge\u00b7heul", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verwirrung auf Verwirrung,", "tokens": ["Ver\u00b7wir\u00b7rung", "auf", "Ver\u00b7wir\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stets dichter wird der Kn\u00e4ul.", "tokens": ["Stets", "dich\u00b7ter", "wird", "der", "Kn\u00e4ul", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So wirrt sich's durcheinander,", "tokens": ["So", "wirrt", "sich's", "durch\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So tobt und schreit es laut \u2013", "tokens": ["So", "tobt", "und", "schreit", "es", "laut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist der Alexander,", "tokens": ["Wo", "ist", "der", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der diesen Zank durchhaut?", "tokens": ["Der", "die\u00b7sen", "Zank", "durch\u00b7haut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo lebt der hohe Meister,", "tokens": ["Wo", "lebt", "der", "ho\u00b7he", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo dr\u00e4ut der m\u00e4cht'ge Bann,", "tokens": ["Wo", "dr\u00e4ut", "der", "m\u00e4cht'\u00b7ge", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der diesen Krieg der Geister", "tokens": ["Der", "die\u00b7sen", "Krieg", "der", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Frieden zwingen kann?", "tokens": ["Zum", "Frie\u00b7den", "zwin\u00b7gen", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das bist du, Gott der Gnaden,", "tokens": ["Das", "bist", "du", ",", "Gott", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm von den Sonnenpfaden,", "tokens": ["Komm", "von", "den", "Son\u00b7nen\u00b7pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Komm, still' uns dies Geschrei,", "tokens": ["Komm", ",", "still'", "uns", "dies", "Ge\u00b7schrei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df hell den Degen klirren", "tokens": ["La\u00df", "hell", "den", "De\u00b7gen", "klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von deiner Sternenburg,", "tokens": ["Von", "dei\u00b7ner", "Ster\u00b7nen\u00b7burg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hau' von den w\u00fcsten Wirren", "tokens": ["Hau'", "von", "den", "w\u00fcs\u00b7ten", "Wir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den ganzen Jammer durch.", "tokens": ["Den", "gan\u00b7zen", "Jam\u00b7mer", "durch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Komm, Gott, komm, Gott vom Himmel", "tokens": ["Komm", ",", "Gott", ",", "komm", ",", "Gott", "vom", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sieh in Gnaden drein,", "tokens": ["Und", "sieh", "in", "Gna\u00b7den", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchleuchte das Gewimmel", "tokens": ["Durch\u00b7leuch\u00b7te", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Nacht mit Sonnenschein,", "tokens": ["Der", "Nacht", "mit", "Son\u00b7nen\u00b7schein", ","], 