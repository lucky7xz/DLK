{"dta.poem.20290": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Amaranthen/ \u00fcber sein an sie  \n geschicktes bildni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein bildni\u00df hast du hier auff d\u00fcnnes gla\u00df geleget/", "tokens": ["Mein", "bild\u00b7ni\u00df", "hast", "du", "hier", "auff", "d\u00fcn\u00b7nes", "gla\u00df", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es scheint/ da\u00df zwischen mensch und gla\u00df verwandni\u00df sey/", "tokens": ["Es", "scheint", "/", "da\u00df", "zwi\u00b7schen", "mensch", "und", "gla\u00df", "ver\u00b7wand\u00b7ni\u00df", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "NE", "KON", "VVFIN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn die gebrechlichkeit ist beyden eingepr\u00e4get/", "tokens": ["Denn", "die", "ge\u00b7brech\u00b7lich\u00b7keit", "ist", "bey\u00b7den", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie seyn von dem verderb fast keine stunde frey.", "tokens": ["Sie", "seyn", "von", "dem", "ver\u00b7derb", "fast", "kei\u00b7ne", "stun\u00b7de", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "APPR", "ART", "NN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bald ein gla\u00df zerbricht/ kan auch ein mensch vergehen.", "tokens": ["So", "bald", "ein", "gla\u00df", "zer\u00b7bricht", "/", "kan", "auch", "ein", "mensch", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$(", "VMFIN", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das gla\u00df zerbricht der mensch/ den menschen Gottes hand;", "tokens": ["Das", "gla\u00df", "zer\u00b7bricht", "der", "mensch", "/", "den", "men\u00b7schen", "Got\u00b7tes", "hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es k\u00f6nnen beyde nicht die l\u00e4nge recht bestehen/", "tokens": ["Es", "k\u00f6n\u00b7nen", "bey\u00b7de", "nicht", "die", "l\u00e4n\u00b7ge", "recht", "be\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihr end und anfang ist fast nichts als asch und sand.", "tokens": ["Ihr", "end", "und", "an\u00b7fang", "ist", "fast", "nichts", "als", "asch", "und", "sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "NN", "VAFIN", "ADV", "PIS", "KOKOM", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zerbricht das gla\u00df nicht gantz/ so kriegt es schn\u00f6de flecken/", "tokens": ["Zer\u00b7bricht", "das", "gla\u00df", "nicht", "gantz", "/", "so", "kriegt", "es", "schn\u00f6\u00b7de", "fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "PTKNEG", "ADV", "$(", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Laufft von dem wetter an/ und wird sehr ungestalt:", "tokens": ["Laufft", "von", "dem", "wet\u00b7ter", "an", "/", "und", "wird", "sehr", "un\u00b7ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$(", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So will die kranckheit offt uns allen schein verdecken/", "tokens": ["So", "will", "die", "kran\u00b7ck\u00b7heit", "offt", "uns", "al\u00b7len", "schein", "ver\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und macht gem\u00fcth und leib verdrie\u00dflich/ schwach und kalt.", "tokens": ["Und", "macht", "ge\u00b7m\u00fcth", "und", "leib", "ver\u00b7drie\u00df\u00b7lich", "/", "schwach", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "NN", "ADJD", "$(", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zerf\u00e4llt das sch\u00f6nste gla\u00df/ wer achtet dessen st\u00fccke?", "tokens": ["Zer\u00b7f\u00e4llt", "das", "sch\u00f6ns\u00b7te", "gla\u00df", "/", "wer", "ach\u00b7tet", "des\u00b7sen", "st\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "PWS", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man st\u00f6\u00dft es sch\u00e4ndlich hin/ als schlechten ziegel grau\u00df:", "tokens": ["Man", "st\u00f6\u00dft", "es", "sch\u00e4nd\u00b7lich", "hin", "/", "als", "schlech\u00b7ten", "zie\u00b7gel", "grau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die menschen sparen nicht den menschen ihre t\u00fccke/", "tokens": ["Die", "men\u00b7schen", "spa\u00b7ren", "nicht", "den", "men\u00b7schen", "ih\u00b7re", "t\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man hat uns kaum verscharrt/ so ist die freundschafft aus.", "tokens": ["Man", "hat", "uns", "kaum", "ver\u00b7scharrt", "/", "so", "ist", "die", "freund\u00b7schafft", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$(", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ruhm/ nahme und gestalt ist allzubald verschwunden/", "tokens": ["Ruhm", "/", "nah\u00b7me", "und", "ge\u00b7stalt", "ist", "all\u00b7zu\u00b7bald", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "KON", "VVPP", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn man uns nach gebrauch das letzte hembde giebt.", "tokens": ["Wenn", "man", "uns", "nach", "ge\u00b7brauch", "das", "letz\u00b7te", "hemb\u00b7de", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo hat man dieser zeit wohl einen freund gefunden/", "tokens": ["Wo", "hat", "man", "die\u00b7ser", "zeit", "wohl", "ei\u00b7nen", "freund", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "PDAT", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So an das grab gedenckt/ und nach dem tode liebt.", "tokens": ["So", "an", "das", "grab", "ge\u00b7denckt", "/", "und", "nach", "dem", "to\u00b7de", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier ist das d\u00fcnne gla\u00df/ wilt du es bald zerbrechen/", "tokens": ["Hier", "ist", "das", "d\u00fcn\u00b7ne", "gla\u00df", "/", "wilt", "du", "es", "bald", "zer\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So nehm ich es von dir vor keine feindschafft an;", "tokens": ["So", "nehm", "ich", "es", "von", "dir", "vor", "kei\u00b7ne", "feind\u00b7schafft", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn Amaranthen wei\u00df ich nicht zu widersprechen/", "tokens": ["Denn", "A\u00b7ma\u00b7ran\u00b7then", "wei\u00df", "ich", "nicht", "zu", "wi\u00b7der\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Indem mich ihre hand in nichts verletzen kan.", "tokens": ["In\u00b7dem", "mich", "ih\u00b7re", "hand", "in", "nichts", "ver\u00b7let\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}