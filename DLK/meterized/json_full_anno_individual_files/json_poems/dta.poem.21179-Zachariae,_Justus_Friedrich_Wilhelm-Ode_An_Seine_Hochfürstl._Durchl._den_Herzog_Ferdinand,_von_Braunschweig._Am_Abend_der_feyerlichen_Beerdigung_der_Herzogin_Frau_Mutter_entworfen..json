{"dta.poem.21179": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ode  \n An Seine Hochf\u00fcrstl. Durchl.  \n  den Herzog  \n  Ferdinand,  \n von Braunschweig.  \n Am Abend der feyerlichen Beerdigung  \n  der  \n  Herzogin Frau Mutter  \n entworfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der Traurige, der so gebeugt,", "tokens": ["Wer", "ist", "der", "Trau\u00b7ri\u00b7ge", ",", "der", "so", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So ganz von Schmerz erf\u00fcllt,", "tokens": ["So", "ganz", "von", "Schmerz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In schwarzen Leichenflohr geh\u00fcllt,", "tokens": ["In", "schwar\u00b7zen", "Lei\u00b7chen\u00b7flohr", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Blick zur Erde neigt?", "tokens": ["Den", "Blick", "zur", "Er\u00b7de", "neigt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie, Muse, Ferdinand? Ja! Sieh ihn stehn", "tokens": ["Wie", ",", "Mu\u00b7se", ",", "Fer\u00b7di\u00b7nand", "?", "Ja", "!", "Sieh", "ihn", "stehn"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "NN", "$,", "NE", "$.", "PTKANT", "$.", "NE", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An seiner Mutter Grab.", "tokens": ["An", "sei\u00b7ner", "Mut\u00b7ter", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die heisse Thr\u00e4ne rollt herab;", "tokens": ["Die", "heis\u00b7se", "Thr\u00e4\u00b7ne", "rollt", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kan Jhn trauren sehn,", "tokens": ["Wer", "kan", "Jhn", "trau\u00b7ren", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und unempfindlich seyn? Flie\u00dft, Thr\u00e4nen, flie\u00dft,", "tokens": ["Und", "un\u00b7emp\u00b7find\u00b7lich", "seyn", "?", "Flie\u00dft", ",", "Thr\u00e4\u00b7nen", ",", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr den Helden ehrt!", "tokens": ["Die", "ihr", "den", "Hel\u00b7den", "ehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sehr war sie die F\u00fcrstin wehrt,", "tokens": ["Wie", "sehr", "war", "sie", "die", "F\u00fcrs\u00b7tin", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um die er sie vergie\u00dft!", "tokens": ["Um", "die", "er", "sie", "ver\u00b7gie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O du, ietzt mehr als F\u00fcrst, indem du weinst,", "tokens": ["O", "du", ",", "ietzt", "mehr", "als", "F\u00fcrst", ",", "in\u00b7dem", "du", "weinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADV", "KOUS", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bewundrung schaut dich an.", "tokens": ["Be\u00b7wund\u00b7rung", "schaut", "dich", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie gro\u00df der F\u00fcrst, der weinen kan,", "tokens": ["Wie", "gro\u00df", "der", "F\u00fcrst", ",", "der", "wei\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So menschlich, wie du weinst!", "tokens": ["So", "menschlich", ",", "wie", "du", "weinst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Der wird einst in der Schlacht, wenn nun das Feld", "tokens": ["Der", "wird", "einst", "in", "der", "Schlacht", ",", "wenn", "nun", "das", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll von Erschlagnen liegt,", "tokens": ["Voll", "von", "Er\u00b7schlag\u00b7nen", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch dann noch weinen, wenn er siegt,", "tokens": ["Auch", "dann", "noch", "wei\u00b7nen", ",", "wenn", "er", "siegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mehr seyn, als ein Held.", "tokens": ["Und", "mehr", "seyn", ",", "als", "ein", "Held", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAINF", "$,", "KOUS", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Doch folg ihm weiter! Sieh, ietzt \u00f6fnet sich", "tokens": ["Doch", "folg", "ihm", "wei\u00b7ter", "!", "Sieh", ",", "ietzt", "\u00f6f\u00b7net", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "PTKVZ", "$.", "NE", "$,", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die dunkle F\u00fcrstengruft.", "tokens": ["Die", "dunk\u00b7le", "F\u00fcrs\u00b7ten\u00b7gruft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er geht, wohin sein Herz ihn ruft,", "tokens": ["Er", "geht", ",", "wo\u00b7hin", "sein", "Herz", "ihn", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht, Tod, noch n\u00e4her dich.", "tokens": ["Sieht", ",", "Tod", ",", "noch", "n\u00e4\u00b7her", "dich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie gro\u00df, wie schaudervoll, wie voll Gewalt", "tokens": ["Wie", "gro\u00df", ",", "wie", "schau\u00b7der\u00b7voll", ",", "wie", "voll", "Ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist dieser Anblick nicht!", "tokens": ["Ist", "die\u00b7ser", "An\u00b7blick", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie steht hier Sarg an Sarg! Wie spricht", "tokens": ["Wie", "steht", "hier", "Sarg", "an", "Sarg", "!", "Wie", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "NN", "APPR", "NN", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Todes Schreckgestalt!", "tokens": ["Des", "To\u00b7des", "Schreck\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier schlummern sie nunmehr, o Ferdinand,", "tokens": ["Hier", "schlum\u00b7mern", "sie", "nun\u00b7mehr", ",", "o", "Fer\u00b7di\u00b7nand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Helden, die voll Muth,", "tokens": ["Die", "Hel\u00b7den", ",", "die", "voll", "Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit dir aus Einem Stamm, ihr Blut", "tokens": ["Mit", "dir", "aus", "Ei\u00b7nem", "Stamm", ",", "ihr", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verspritzt f\u00fcrs Vaterland.", "tokens": ["Ver\u00b7spritzt", "f\u00fcrs", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hier liegt dein Albrecht, dort der tapfre Franz,", "tokens": ["Hier", "liegt", "dein", "Al\u00b7brecht", ",", "dort", "der", "tapf\u00b7re", "Franz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie fielen in der Schlacht;", "tokens": ["Sie", "fie\u00b7len", "in", "der", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch schlummern sie nicht hier in Nacht,", "tokens": ["Doch", "schlum\u00b7mern", "sie", "nicht", "hier", "in", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie deckt des Nachruhms Kranz.", "tokens": ["Sie", "deckt", "des", "Nach\u00b7ruhms", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und hier, (du weinst aufs neu, o Muse!) hier", "tokens": ["Und", "hier", ",", "(", "du", "weinst", "aufs", "neu", ",", "o", "Mu\u00b7se", "!", ")", "hier"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["KON", "ADV", "$,", "$(", "PPER", "ADV", "APPRART", "ADJD", "$,", "FM", "NN", "$.", "$(", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Liebling \u2014 Nenne nicht", "tokens": ["Dein", "Lieb\u00b7ling", "Nen\u00b7ne", "nicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Namen, der das Herz uns bricht!", "tokens": ["Den", "Na\u00b7men", ",", "der", "das", "Herz", "uns", "bricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Ferdinand, von Dir,", "tokens": ["O", "Fer\u00b7di\u00b7nand", ",", "von", "Dir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Von seines Bruders Muth zum Ruhm gef\u00fchrt,", "tokens": ["Von", "sei\u00b7nes", "Bru\u00b7ders", "Muth", "zum", "Ruhm", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fiel Er, der junge Held;", "tokens": ["Fiel", "Er", ",", "der", "jun\u00b7ge", "Held", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wie die zarte Blume f\u00e4llt,", "tokens": ["So", "wie", "die", "zar\u00b7te", "Blu\u00b7me", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie der Nord ber\u00fchrt.", "tokens": ["Wenn", "sie", "der", "Nord", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie oft, o F\u00fcrstengrab, er\u00f6fnet sich", "tokens": ["Wie", "oft", ",", "o", "F\u00fcrs\u00b7ten\u00b7grab", ",", "er\u00b7\u00f6f\u00b7net", "sich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein f\u00fcrchterliches Thor?", "tokens": ["Dein", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "Thor", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Braunschweigs Stamm aufs neu verlohr", "tokens": ["Was", "Braun\u00b7schweigs", "Stamm", "aufs", "neu", "ver\u00b7lohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "NN", "APPRART", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey lange gnung f\u00fcr dich!", "tokens": ["Sey", "lan\u00b7ge", "gnung", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "La\u00df ab, o Vorsehung, mit diesem Schlag!", "tokens": ["La\u00df", "ab", ",", "o", "Vor\u00b7se\u00b7hung", ",", "mit", "die\u00b7sem", "Schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch ruft der nahe Krieg", "tokens": ["Noch", "ruft", "der", "na\u00b7he", "Krieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Helden fort zum Ruhm, zum Sieg,", "tokens": ["Die", "Hel\u00b7den", "fort", "zum", "Ruhm", ",", "zum", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum f\u00fcrchterlichen Tag,", "tokens": ["Zum", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}