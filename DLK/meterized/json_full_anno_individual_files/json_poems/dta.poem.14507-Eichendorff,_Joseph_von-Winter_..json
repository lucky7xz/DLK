{"dta.poem.14507": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und hat ausgeklungen", "tokens": ["Und", "hat", "aus\u00b7ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Liebes-Lust und Leid,", "tokens": ["Lie\u00b7bes\u00b7Lust", "und", "Leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um die wir gerungen", "tokens": ["Um", "die", "wir", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der sch\u00f6nsten Zeit;", "tokens": ["In", "der", "sch\u00f6ns\u00b7ten", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}