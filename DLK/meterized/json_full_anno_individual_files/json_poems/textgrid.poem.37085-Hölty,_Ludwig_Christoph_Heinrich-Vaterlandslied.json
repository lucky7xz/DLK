{"textgrid.poem.37085": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vaterlandslied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesegnet mir, mein Vaterland,", "tokens": ["Ge\u00b7seg\u00b7net", "mir", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich so viele Tugend fand,", "tokens": ["Wo", "ich", "so", "vie\u00b7le", "Tu\u00b7gend", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesegnet mir, mein Vaterland!", "tokens": ["Ge\u00b7seg\u00b7net", "mir", ",", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4nner haben Heldenmuth,", "tokens": ["Die", "M\u00e4n\u00b7ner", "ha\u00b7ben", "Hel\u00b7den\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstr\u00f6men Patriotenblut,", "tokens": ["Ver\u00b7str\u00f6\u00b7men", "Pat\u00b7ri\u00b7o\u00b7ten\u00b7blut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind edel auch dabey, und gut.", "tokens": ["Sind", "e\u00b7del", "auch", "da\u00b7bey", ",", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "