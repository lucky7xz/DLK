{"dta.poem.23508": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das macht allein der falsch argwohn/", "tokens": ["Das", "macht", "al\u00b7lein", "der", "falsch", "arg\u00b7wohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ein b\u00f6ser gebrauch vermehret/", "tokens": ["Den", "ein", "b\u00f6\u00b7ser", "ge\u00b7brauch", "ver\u00b7meh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der klaidet manche werck mit hohn", "tokens": ["Der", "klai\u00b7det", "man\u00b7che", "werck", "mit", "hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Welche doch die Natur selbs ehret.", "tokens": ["Wel\u00b7che", "doch", "die", "Na\u00b7tur", "selbs", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Vnser kampf/ gehend ab ohn blut/", "tokens": ["Vn\u00b7ser", "kampf", "/", "ge\u00b7hend", "ab", "ohn", "blut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVPP", "PTKVZ", "APPR", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Macht weder waisen noch witfrawen:", "tokens": ["Macht", "we\u00b7der", "wai\u00b7sen", "noch", "wit\u00b7fra\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Vnd wazu ist doch der welt gut", "tokens": ["Vnd", "wa\u00b7zu", "ist", "doch", "der", "welt", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Dapfersten stechen vnd hawen?", "tokens": ["Der", "Dap\u00b7fers\u00b7ten", "ste\u00b7chen", "vnd", "ha\u00b7wen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}