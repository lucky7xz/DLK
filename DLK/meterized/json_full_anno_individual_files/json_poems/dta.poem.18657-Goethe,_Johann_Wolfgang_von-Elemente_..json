{"dta.poem.18657": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Elemente .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Aus wie vielen Elementen", "tokens": ["Aus", "wie", "vie\u00b7len", "E\u00b7le\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "PIAT", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Soll ein \u00e4chtes Lied sich n\u00e4hren?", "tokens": ["Soll", "ein", "\u00e4ch\u00b7tes", "Lied", "sich", "n\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass es Layen gern empfinden,", "tokens": ["Dass", "es", "La\u00b7yen", "gern", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meister es mit Freuden h\u00f6ren.", "tokens": ["Meis\u00b7ter", "es", "mit", "Freu\u00b7den", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe sey vor allen Dingen", "tokens": ["Lie\u00b7be", "sey", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Thema, wenn wir singen;", "tokens": ["Un\u00b7ser", "The\u00b7ma", ",", "wenn", "wir", "sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann sie gar das Lied durchdringen,", "tokens": ["Kann", "sie", "gar", "das", "Lied", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird\u2019s um desto besser klingen.", "tokens": ["Wird's", "um", "des\u00b7to", "bes\u00b7ser", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann muss Klang der Gl\u00e4ser t\u00f6nen,", "tokens": ["Dann", "muss", "Klang", "der", "Gl\u00e4\u00b7ser", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Rubin des Weins ergl\u00e4nzen:", "tokens": ["Und", "Ru\u00b7bin", "des", "Weins", "er\u00b7gl\u00e4n\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn f\u00fcr Liebende, f\u00fcr Trinker", "tokens": ["Denn", "f\u00fcr", "Lie\u00b7ben\u00b7de", ",", "f\u00fcr", "Trin\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Winkt man mit den sch\u00f6nsten Kr\u00e4nzen.", "tokens": ["Winkt", "man", "mit", "den", "sch\u00f6ns\u00b7ten", "Kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Waffenklang wird auch gefodert,", "tokens": ["Waf\u00b7fen\u00b7klang", "wird", "auch", "ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass auch die Trommete schmettre;", "tokens": ["Dass", "auch", "die", "Trom\u00b7me\u00b7te", "schmett\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Dass, wenn Gl\u00fcck zu Flammen lodert,", "tokens": ["Dass", ",", "wenn", "Gl\u00fcck", "zu", "Flam\u00b7men", "lo\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich im Sieg der Held verg\u00f6ttre.", "tokens": ["Sich", "im", "Sieg", "der", "Held", "ver\u00b7g\u00f6t\u00b7tre", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann zuletzt ist unerl\u00e4sslich,", "tokens": ["Dann", "zu\u00b7letzt", "ist", "un\u00b7er\u00b7l\u00e4ss\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass der Dichter manches hasse,", "tokens": ["Dass", "der", "Dich\u00b7ter", "man\u00b7ches", "has\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was unleidlich ist und h\u00e4sslich", "tokens": ["Was", "un\u00b7leid\u00b7lich", "ist", "und", "h\u00e4ss\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nicht wie Sch\u00f6nes leben lasse.", "tokens": ["Nicht", "wie", "Sch\u00f6\u00b7nes", "le\u00b7ben", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weiss der S\u00e4nger dieser Viere", "tokens": ["Weiss", "der", "S\u00e4n\u00b7ger", "die\u00b7ser", "Vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Urgewalt\u2019gen Stoff zu mischen,", "tokens": ["Ur\u00b7ge\u00b7walt'\u00b7gen", "Stoff", "zu", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hafis gleich wird er die V\u00f6lker", "tokens": ["Ha\u00b7fis", "gleich", "wird", "er", "die", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ewig freuen und erfrischen.", "tokens": ["E\u00b7wig", "freu\u00b7en", "und", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}