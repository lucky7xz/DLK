{"dta.poem.20357": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen Hahnrey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die h\u00f6rner tr\u00e4gt der mann/ das weib verdienet sie.", "tokens": ["Die", "h\u00f6r\u00b7ner", "tr\u00e4gt", "der", "mann", "/", "das", "weib", "ver\u00b7die\u00b7net", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das tragen bringt ihr lust/ ihm lauter last und m\u00fch.", "tokens": ["Das", "tra\u00b7gen", "bringt", "ihr", "lust", "/", "ihm", "lau\u00b7ter", "last", "und", "m\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "PPER", "VVFIN", "$(", "PPER", "ADJD", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie f\u00fchlt die h\u00f6rner wohl/ und wird dadurch erqvicket;", "tokens": ["Sie", "f\u00fchlt", "die", "h\u00f6r\u00b7ner", "wohl", "/", "und", "wird", "da\u00b7durch", "er\u00b7qvic\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "$(", "KON", "VAFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er aber f\u00fchlt sie nicht/ und doch wird er gedr\u00fccket.", "tokens": ["Er", "a\u00b7ber", "f\u00fchlt", "sie", "nicht", "/", "und", "doch", "wird", "er", "ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$(", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Allein so schwer sie sind/ wird doch bi\u00dfher geglaubt/", "tokens": ["Al\u00b7lein", "so", "schwer", "sie", "sind", "/", "wird", "doch", "bi\u00df\u00b7her", "ge\u00b7glaubt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "VAFIN", "$(", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df er sie tragen mu\u00df als seines weibes haupt.", "tokens": ["Da\u00df", "er", "sie", "tra\u00b7gen", "mu\u00df", "als", "sei\u00b7nes", "wei\u00b7bes", "haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}