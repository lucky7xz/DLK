{"dta.poem.13206": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem sie dieses sprach, und hier zu schliessen schien,", "tokens": ["Nach\u00b7dem", "sie", "die\u00b7ses", "sprach", ",", "und", "hier", "zu", "schlies\u00b7sen", "schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "KON", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sah man ihren Blick nach beiden Seiten ziehn;", "tokens": ["So", "sah", "man", "ih\u00b7ren", "Blick", "nach", "bei\u00b7den", "Sei\u00b7ten", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als h\u00f6rte sie, was ihr der Tugenden Gem\u00fcthe", "tokens": ["Als", "h\u00f6r\u00b7te", "sie", ",", "was", "ihr", "der", "Tu\u00b7gen\u00b7den", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "PWS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch etwa selbst der Thron bey dieser Frage riethe.", "tokens": ["Auch", "et\u00b7wa", "selbst", "der", "Thron", "bey", "die\u00b7ser", "Fra\u00b7ge", "rie\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man stellte sich nicht vor, wa\u00df ihr Beginnen sey;", "tokens": ["Man", "stell\u00b7te", "sich", "nicht", "vor", ",", "wa\u00df", "ihr", "Be\u00b7gin\u00b7nen", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "290Bi\u00df endlich sie den Plaz der zarten Kinder Reih,", "tokens": ["end\u00b7lich", "sie", "den", "Plaz", "der", "zar\u00b7ten", "Kin\u00b7der", "Reih", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Absonderlich das Paar der theuren S\u00f6hne wiese,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "das", "Paar", "der", "theu\u00b7ren", "S\u00f6h\u00b7ne", "wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sagte: \u201eTugenden! werfft euern Blick auf diese,", "tokens": ["Und", "sag\u00b7te", ":", "\u201e", "Tu\u00b7gen\u00b7den", "!", "werfft", "eu\u00b7ern", "Blick", "auf", "die\u00b7se", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$.", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eauf diese Kinder her! die seynd mein gr\u00f6\u00dfter Schaz:", "tokens": ["\u201e", "auf", "die\u00b7se", "Kin\u00b7der", "her", "!", "die", "seynd", "mein", "gr\u00f6\u00df\u00b7ter", "Schaz", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "PTKVZ", "$.", "ART", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ehierauf beruht das Heil, die Hoffnung dieses Staats.", "tokens": ["\u201e", "hier\u00b7auf", "be\u00b7ruht", "das", "Heil", ",", "die", "Hoff\u00b7nung", "die\u00b7ses", "Staats", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "295\u201dWollt ihr dieselbigen zu der Belohnung nehmen,", "tokens": ["\"", "Wollt", "ihr", "die\u00b7sel\u00b7bi\u00b7gen", "zu", "der", "Be\u00b7loh\u00b7nung", "neh\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PDS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u201eauch meine T\u00f6chter dort, und die nach ihnen k\u00e4men?", "tokens": ["\u201e", "auch", "mei\u00b7ne", "T\u00f6ch\u00b7ter", "dort", ",", "und", "die", "nach", "ih\u00b7nen", "k\u00e4\u00b7men", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "ADV", "$,", "KON", "ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201enichts kostbarers hab ich; erw\u00e4hlet sie zum Pfand!", "tokens": ["\u201e", "nichts", "kost\u00b7ba\u00b7rers", "hab", "ich", ";", "er\u00b7w\u00e4h\u00b7let", "sie", "zum", "Pfand", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "ADV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edie\u00df stiftet zwischen euch und mir ein neues Band.", "tokens": ["\u201e", "die\u00df", "stif\u00b7tet", "zwi\u00b7schen", "euch", "und", "mir", "ein", "neu\u00b7es", "Band", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "APPR", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}