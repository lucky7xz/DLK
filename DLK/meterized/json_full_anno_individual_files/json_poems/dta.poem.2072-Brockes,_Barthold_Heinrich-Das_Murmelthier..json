{"dta.poem.2072": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Murmelthier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Bewohner der Gebirge, das verschlafne Murmel-", "tokens": ["Der", "Be\u00b7woh\u00b7ner", "der", "Ge\u00b7bir\u00b7ge", ",", "das", "ver\u00b7schlaf\u00b7ne", "Mur\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "thier,", "tokens": ["thier", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ist von einer andern Art. Die\u00df, wenn es im Schnee u. Frost,", "tokens": ["Ist", "von", "ei\u00b7ner", "an\u00b7dern", "Art", ".", "Die\u00df", ",", "wenn", "es", "im", "Schnee", "u.", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$.", "PDS", "$,", "KOUS", "PPER", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+--++", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Seine meiste Zeit verschl\u00e4ft, k\u00f6mmt im Fr\u00fchling nur herf\u00fcr", "tokens": ["Sei\u00b7ne", "meis\u00b7te", "Zeit", "ver\u00b7schl\u00e4ft", ",", "k\u00f6mmt", "im", "Fr\u00fch\u00b7ling", "nur", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "ADV", "ADV"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und besorgt zu k\u00fcnft\u2019gem Winter auf das neue Nest und", "tokens": ["Und", "be\u00b7sorgt", "zu", "k\u00fcnft'\u00b7gem", "Win\u00b7ter", "auf", "das", "neu\u00b7e", "Nest", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Kost.", "tokens": ["Kost", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Jhre L\u00f6cher richten sie mit besondrer Ordnung ein;", "tokens": ["Ih\u00b7re", "L\u00f6\u00b7cher", "rich\u00b7ten", "sie", "mit", "be\u00b7sond\u00b7rer", "Ord\u00b7nung", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Da\u00df sie n\u00e4mlich frey von Unrath, und best\u00e4ndig reinlich seyn,", "tokens": ["Da\u00df", "sie", "n\u00e4m\u00b7lich", "frey", "von", "Un\u00b7rath", ",", "und", "be\u00b7st\u00e4n\u00b7dig", "rein\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "NN", "$,", "KON", "ADJD", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Graben sie ein eignes Loch. Wie sie zu den n\u00f6th\u2019gen Dingen,", "tokens": ["Gra\u00b7ben", "sie", "ein", "eig\u00b7nes", "Loch", ".", "Wie", "sie", "zu", "den", "n\u00f6\u00b7th'\u00b7gen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$.", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Die sie zu dem Lager brauchen, Gras und Heu zusammen", "tokens": ["Die", "sie", "zu", "dem", "La\u00b7ger", "brau\u00b7chen", ",", "Gras", "und", "Heu", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "NN", "KON", "NN", "PTKVZ"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "bringen,", "tokens": ["brin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Ist wohl recht verwunderlich. Eins sieht man sich nie-", "tokens": ["Ist", "wohl", "recht", "ver\u00b7wun\u00b7der\u00b7lich", ".", "Eins", "sieht", "man", "sich", "nie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$.", "NN", "VVFIN", "PIS", "PRF", "TRUNC"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.13": {"text": "derstrecken,", "tokens": ["der\u00b7stre\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Die vier Beine \u00fcber sich, da die andern es bedecken,", "tokens": ["Die", "vier", "Bei\u00b7ne", "\u00fc\u00b7ber", "sich", ",", "da", "die", "an\u00b7dern", "es", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "PRF", "$,", "KOUS", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Und so viel es halten kann,", "tokens": ["Und", "so", "viel", "es", "hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Busch und Strauchwerk auf ihn tragen,", "tokens": ["Busch", "und", "Strauch\u00b7werk", "auf", "ihn", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Dann fass\u2019t jedes dessen Schwanz sanfte mit den Z\u00e4hnen an,", "tokens": ["Dann", "fass't", "je\u00b7des", "des\u00b7sen", "Schwanz", "sanf\u00b7te", "mit", "den", "Z\u00e4h\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "PDS", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Und so schleppen sie gew\u00f6hnlich diesen lebendigen Wagen", "tokens": ["Und", "so", "schlep\u00b7pen", "sie", "ge\u00b7w\u00f6hn\u00b7lich", "die\u00b7sen", "le\u00b7ben\u00b7di\u00b7gen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PDAT", "ADJA", "NN"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Als bey einer Deichsel fort. Die\u00df thut einer nach dem", "tokens": ["Als", "bey", "ei\u00b7ner", "Deich\u00b7sel", "fort", ".", "Die\u00df", "thut", "ei\u00b7ner", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKVZ", "$.", "PDS", "VVFIN", "ART", "APPR", "ART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "andern,", "tokens": ["an\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["PIS", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Jeder, wenn die Reih ihn trifft, mu\u00df nach dieser Ordnung", "tokens": ["Je\u00b7der", ",", "wenn", "die", "Reih", "ihn", "trifft", ",", "mu\u00df", "nach", "die\u00b7ser", "Ord\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "wandern,", "tokens": ["wan\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Keiner wird damit verschonet. Wenn es fri\u00dft, sitzt dieses Thier,", "tokens": ["Kei\u00b7ner", "wird", "da\u00b7mit", "ver\u00b7scho\u00b7net", ".", "Wenn", "es", "fri\u00dft", ",", "sitzt", "die\u00b7ses", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "H\u00e4lt die Kost in rordern Pfoten, wie ein Eichhorn, fast wie", "tokens": ["H\u00e4lt", "die", "Kost", "in", "ror\u00b7dern", "Pfo\u00b7ten", ",", "wie", "ein", "Eich\u00b7horn", ",", "fast", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$,", "ADV", "KOKOM"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.25": {"text": "wir.", "tokens": ["wir", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Fleisch vom Murmelthier soll niedlich, wie das beste Wild-", "tokens": ["Fleisch", "vom", "Mur\u00b7melt\u00b7hier", "soll", "nied\u00b7lich", ",", "wie", "das", "bes\u00b7te", "Wild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VMFIN", "ADJD", "$,", "PWAV", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.27": {"text": "pr\u00e4t, schmecken,", "tokens": ["pr\u00e4t", ",", "schme\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Und sein Schmalz ist trefflich heilsam. Kann man also auch", "tokens": ["Und", "sein", "Schmalz", "ist", "treff\u00b7lich", "heil\u00b7sam", ".", "Kann", "man", "al\u00b7so", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$.", "VMFIN", "PIS", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "in ihnen,", "tokens": ["in", "ih\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Da sie uns zu mancherley Nutzen und Ergetzen dienen,", "tokens": ["Da", "sie", "uns", "zu", "man\u00b7cher\u00b7ley", "Nut\u00b7zen", "und", "Er\u00b7get\u00b7zen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Eines weisen Wesens Absicht, Weisheit, Macht und Lieb\u2019", "tokens": ["Ei\u00b7nes", "wei\u00b7sen", "We\u00b7sens", "Ab\u00b7sicht", ",", "Weis\u00b7heit", ",", "Macht", "und", "Lieb'"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.32": {"text": "entdecken.", "tokens": ["ent\u00b7de\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}