{"dta.poem.12014": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast, vom Gl\u00fcck belehnt, ein sch\u00f6nes Fleckchen Erde;", "tokens": ["Du", "hast", ",", "vom", "Gl\u00fcck", "be\u00b7lehnt", ",", "ein", "sch\u00f6\u00b7nes", "Fleck\u00b7chen", "Er\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genie\u00df es recht, da\u00df dirs ein St\u00fcckchen Himmel werde.", "tokens": ["Ge\u00b7nie\u00df", "es", "recht", ",", "da\u00df", "dirs", "ein", "St\u00fcck\u00b7chen", "Him\u00b7mel", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich w\u00fcnsche dir nicht ganz ein sorgenfreies Lo\u00df,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "nicht", "ganz", "ein", "sor\u00b7gen\u00b7frei\u00b7es", "Lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur gegen den Genu\u00df die Sorge nicht zu gro\u00df.", "tokens": ["Nur", "ge\u00b7gen", "den", "Ge\u00b7nu\u00df", "die", "Sor\u00b7ge", "nicht", "zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein wenig Salz ist gut, auch Pfeffer, am Gericht,", "tokens": ["Ein", "we\u00b7nig", "Salz", "ist", "gut", ",", "auch", "Pfef\u00b7fer", ",", "am", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur \u00fcbersalzen sei's und \u00fcberpfeffert nicht.", "tokens": ["Nur", "\u00fc\u00b7bers\u00b7al\u00b7zen", "sei's", "und", "\u00fc\u00b7berp\u00b7fef\u00b7fert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}