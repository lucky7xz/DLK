{"dta.poem.13786": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den Morgen freudig stehn", "tokens": ["Ich", "sah", "den", "Mor\u00b7gen", "freu\u00b7dig", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf der Felsenschwelle,", "tokens": ["Hoch", "auf", "der", "Fel\u00b7sen\u00b7schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Adler ziehn und Str\u00f6me gehn,", "tokens": ["Die", "Ad\u00b7ler", "ziehn", "und", "Str\u00f6\u00b7me", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprang hinaus in's Helle.\u201c", "tokens": ["Und", "sprang", "hin\u00b7aus", "in's", "Hel\u00b7le", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}