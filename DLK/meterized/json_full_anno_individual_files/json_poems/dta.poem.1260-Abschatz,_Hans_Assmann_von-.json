{"dta.poem.1260": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was dienet mir der blassen Sternen Krantz/", "tokens": ["Was", "die\u00b7net", "mir", "der", "blas\u00b7sen", "Ster\u00b7nen", "Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn mich erleucht der hellen Sonne Glantz?", "tokens": ["Wenn", "mich", "er\u00b7leucht", "der", "hel\u00b7len", "Son\u00b7ne", "Glantz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der lichte Tag besieget iede Nacht/", "tokens": ["Der", "lich\u00b7te", "Tag", "be\u00b7sie\u00b7get", "ie\u00b7de", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Mond und Stern nur halb-erleuchtet macht.", "tokens": ["Die", "Mond", "und", "Stern", "nur", "halb\u00b7er\u00b7leuch\u00b7tet", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verblendt mein schwaches Auge gleich der ungewohnte", "tokens": ["Ver\u00b7blendt", "mein", "schwa\u00b7ches", "Au\u00b7ge", "gleich", "der", "un\u00b7ge\u00b7wohn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schein/", "tokens": ["Schein", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Soll mir doch eine Sonne mehr als tausend Sternen", "tokens": ["Soll", "mir", "doch", "ei\u00b7ne", "Son\u00b7ne", "mehr", "als", "tau\u00b7send", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}}}}}