{"dta.poem.15882": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Freunde haben, doch zuviel nicht allen traun,", "tokens": ["Viel", "Freun\u00b7de", "ha\u00b7ben", ",", "doch", "zu\u00b7viel", "nicht", "al\u00b7len", "traun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "ADV", "PIS", "PTKNEG", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist eine Weisheit, die mich wenig kann erbaun.", "tokens": ["Ist", "ei\u00b7ne", "Weis\u00b7heit", ",", "die", "mich", "we\u00b7nig", "kann", "er\u00b7baun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel lieber will ich doch nur wen'ge, denen ich", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "doch", "nur", "wen'\u00b7ge", ",", "de\u00b7nen", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darf viel traun, einen nur, dem ich vertraun darf mich.", "tokens": ["Darf", "viel", "traun", ",", "ei\u00b7nen", "nur", ",", "dem", "ich", "ver\u00b7traun", "darf", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,", "ART", "ADV", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}