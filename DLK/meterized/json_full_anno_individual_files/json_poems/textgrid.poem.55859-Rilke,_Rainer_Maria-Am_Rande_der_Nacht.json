{"textgrid.poem.55859": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Am Rande der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Stube und diese Weite,", "tokens": ["Mei\u00b7ne", "Stu\u00b7be", "und", "die\u00b7se", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PDAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "wach \u00fcber nachtendem Land, \u2013", "tokens": ["wach", "\u00fc\u00b7ber", "nach\u00b7ten\u00b7dem", "Land", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "ist Eines. Ich bin eine Saite,", "tokens": ["ist", "Ei\u00b7nes", ".", "Ich", "bin", "ei\u00b7ne", "Sai\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00fcber rauschende breite", "tokens": ["\u00fc\u00b7ber", "rau\u00b7schen\u00b7de", "brei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Resonanzen gespannt.", "tokens": ["Re\u00b7so\u00b7nan\u00b7zen", "ge\u00b7spannt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Dinge sind Geigenleiber,", "tokens": ["Die", "Din\u00b7ge", "sind", "Gei\u00b7gen\u00b7lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "von murrendem Dunkel voll;", "tokens": ["von", "mur\u00b7ren\u00b7dem", "Dun\u00b7kel", "voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "drin tr\u00e4umt das Weinen der Weiber,", "tokens": ["drin", "tr\u00e4umt", "das", "Wei\u00b7nen", "der", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "drin r\u00fchrt sich im Schlafe der Groll", "tokens": ["drin", "r\u00fchrt", "sich", "im", "Schla\u00b7fe", "der", "Groll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "ganzer Geschlechter.....", "tokens": ["gan\u00b7zer", "Ge\u00b7schlech\u00b7ter", "....."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ich soll", "tokens": ["Ich", "soll"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "silbern erzittern: dann wird", "tokens": ["sil\u00b7bern", "er\u00b7zit\u00b7tern", ":", "dann", "wird"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$.", "ADV", "VAFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Alles unter mir leben,", "tokens": ["Al\u00b7les", "un\u00b7ter", "mir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "und was in den Dingen irrt,", "tokens": ["und", "was", "in", "den", "Din\u00b7gen", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "wird nach dem Lichte streben,", "tokens": ["wird", "nach", "dem", "Lich\u00b7te", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "das von meinem tanzenden Tone,", "tokens": ["das", "von", "mei\u00b7nem", "tan\u00b7zen\u00b7den", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "um welchen der Himmel wellt,", "tokens": ["um", "wel\u00b7chen", "der", "Him\u00b7mel", "wellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "durch schmale, schmachtende Spalten", "tokens": ["durch", "schma\u00b7le", ",", "schmach\u00b7ten\u00b7de", "Spal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "in die alten", "tokens": ["in", "die", "al\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Abgr\u00fcnde ohne", "tokens": ["Ab\u00b7gr\u00fcn\u00b7de", "oh\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Ende f\u00e4llt...", "tokens": ["En\u00b7de", "f\u00e4llt", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$(