{"dta.poem.10839": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  stolze Haman.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Hochmuth kommt zum Fall, der an-\ndre sucht zu f\u00e4llen:", "tokens": ["Der", "Hoch\u00b7muth", "kommt", "zum", "Fall", ",", "der", "an", "dre", "sucht", "zu", "f\u00e4l\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "TRUNC", "PIS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gings den Haman dort; so geht", "tokens": ["So", "gings", "den", "Ha\u00b7man", "dort", ";", "so", "geht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihm sind gleich gesinnt. Das himmlische Ge-", "tokens": ["Die", "ihm", "sind", "gleich", "ge\u00b7sinnt", ".", "Das", "himm\u00b7li\u00b7sche", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestraffet allemahl, wenn man sich auf das Gl\u00fck,", "tokens": ["Be\u00b7straf\u00b7fet", "al\u00b7le\u00b7mahl", ",", "wenn", "man", "sich", "auf", "das", "Gl\u00fck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PIS", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das sich leicht dreht, verl\u00e4\u00dft. Ein wunderbahres", "tokens": ["Das", "sich", "leicht", "dreht", ",", "ver\u00b7l\u00e4\u00dft", ".", "Ein", "wun\u00b7der\u00b7bah\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "PRF", "ADJD", "VVFIN", "$,", "VVFIN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lenkts da\u00df die Unschuld mu\u00df dennoch am Ende sie-", "tokens": ["Lenkts", "da\u00df", "die", "Un\u00b7schuld", "mu\u00df", "den\u00b7noch", "am", "En\u00b7de", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der stolze Haman kan in seinem Beispiel lehrn,", "tokens": ["Der", "stol\u00b7ze", "Ha\u00b7man", "kan", "in", "sei\u00b7nem", "Bei\u00b7spiel", "lehrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie wunderlich es sich kan in der Welt verkehrn,", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "es", "sich", "kan", "in", "der", "Welt", "ver\u00b7kehrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er baut den Galgen auf, die Unschuld dran zu schur-", "tokens": ["Er", "baut", "den", "Gal\u00b7gen", "auf", ",", "die", "Un\u00b7schuld", "dran", "zu", "schur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "PAV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Unschuld wird erh\u00f6ht, er mu\u00df ins Ungl\u00fck st\u00fcr-", "tokens": ["Die", "Un\u00b7schuld", "wird", "er\u00b7h\u00f6ht", ",", "er", "mu\u00df", "ins", "Un\u00b7gl\u00fck", "st\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPER", "VMFIN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Mardochai ward recht wunderbahr begl\u00fckt,", "tokens": ["Der", "Mar\u00b7doc\u00b7hai", "ward", "recht", "wun\u00b7der\u00b7bahr", "be\u00b7gl\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und er wird an dem Baum, den er gesezt, gestrikt;", "tokens": ["Und", "er", "wird", "an", "dem", "Baum", ",", "den", "er", "ge\u00b7sezt", ",", "ge\u00b7strikt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Jhr Stolzen spiegelt euch; es kann euch auch so ge-", "tokens": ["Ihr", "Stol\u00b7zen", "spie\u00b7gelt", "euch", ";", "es", "kann", "euch", "auch", "so", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr meinet jezo zwar ganz fest und hoch zu stehen;", "tokens": ["Ihr", "mei\u00b7net", "je\u00b7zo", "zwar", "ganz", "fest", "und", "hoch", "zu", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Gl\u00fcksrad drehet sich, wie ihr an Haman seht;", "tokens": ["Das", "Gl\u00fc\u00b7ksrad", "dre\u00b7het", "sich", ",", "wie", "ihr", "an", "Ha\u00b7man", "seht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PWAV", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sagt nicht, da er gehenkt, sey er dennoch erh\u00f6ht;", "tokens": ["Sagt", "nicht", ",", "da", "er", "ge\u00b7henkt", ",", "sey", "er", "den\u00b7noch", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das ist ein falscher Wiz, das Strik ist schon zer-", "tokens": ["Das", "ist", "ein", "fal\u00b7scher", "Wiz", ",", "das", "St\u00b7rik", "ist", "schon", "zer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Und sein verfaultes Aas zum Abgrund hingeschmis-", "tokens": ["Und", "sein", "ver\u00b7faul\u00b7tes", "Aas", "zum", "Ab\u00b7grund", "hin\u00b7ge\u00b7schmis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auch dies Erh\u00f6hen zeigt; da\u00df Hochmuth komm zum", "tokens": ["Auch", "dies", "Er\u00b7h\u00f6\u00b7hen", "zeigt", ";", "da\u00df", "Hoch\u00b7muth", "komm", "zum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "NN", "VVFIN", "$.", "KOUS", "NN", "VVFIN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wo hohe Berge sind, ist nah ein tieffes Thal.", "tokens": ["Wo", "ho\u00b7he", "Ber\u00b7ge", "sind", ",", "ist", "nah", "ein", "tief\u00b7fes", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$,", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}