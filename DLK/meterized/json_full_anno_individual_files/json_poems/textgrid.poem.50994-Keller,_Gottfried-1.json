{"textgrid.poem.50994": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die \u00fcppige Sommerzeit,", "tokens": ["Das", "ist", "die", "\u00fcp\u00b7pi\u00b7ge", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo alles so schweigend bl\u00fcht und gl\u00fcht,", "tokens": ["Wo", "al\u00b7les", "so", "schwei\u00b7gend", "bl\u00fcht", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des Juli stolzierende Herrlichkeit", "tokens": ["Des", "Ju\u00b7li", "stol\u00b7zie\u00b7ren\u00b7de", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Langsam das schimmernde Land durchzieht.", "tokens": ["Lang\u00b7sam", "das", "schim\u00b7mern\u00b7de", "Land", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6r ein heimliches Dr\u00f6hnen gehn", "tokens": ["Ich", "h\u00f6r", "ein", "heim\u00b7li\u00b7ches", "Dr\u00f6h\u00b7nen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fern in des Gebirg