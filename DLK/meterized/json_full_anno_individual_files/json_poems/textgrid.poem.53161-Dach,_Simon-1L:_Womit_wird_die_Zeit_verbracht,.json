mt", "euch", "von", "den", "Sor\u00b7gen", "Rhu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprechet guten Freunden zu,", "tokens": ["Spre\u00b7chet", "gu\u00b7ten", "Freun\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchet Spiel und s\u00fcsse Lieder,", "tokens": ["Su\u00b7chet", "Spiel", "und", "s\u00fcs\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut, was guter Lust gef\u00e4llt,", "tokens": ["Thut", ",", "was", "gu\u00b7ter", "Lust", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geht zur Hochzeit hin und wieder,", "tokens": ["Geht", "zur", "Hoch\u00b7zeit", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die iezt Amor h\u00e4uffig h\u00e4lt.", "tokens": ["Die", "iezt", "A\u00b7mor", "h\u00e4uf\u00b7fig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer zu lieben Mittel wei\u00df,", "tokens": ["Wer", "zu", "lie\u00b7ben", "Mit\u00b7tel", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Krieg' in jhr des Sieges Preis;", "tokens": ["Krieg'", "in", "jhr", "des", "Sie\u00b7ges", "Preis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Venus schenckt ietzt jhren Knechten", "tokens": ["Ve\u00b7nus", "schenckt", "ietzt", "jhren", "Knech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihrer Wollust Nectar ein,", "tokens": ["Ih\u00b7rer", "Wol\u00b7lust", "Nec\u00b7tar", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heisset mit den langen N\u00e4chten", "tokens": ["Heis\u00b7set", "mit", "den", "lan\u00b7gen", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Ihre Lust auch l\u00e4nger seyn.", "tokens": ["Ih\u00b7re", "Lust", "auch", "l\u00e4n\u00b7ger", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat zu lieben wer nicht Fug,", "tokens": ["Hat", "zu", "lie\u00b7ben", "wer", "nicht", "Fug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "PWS", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzund sieht er Wege gnug", "tokens": ["Je\u00b7tzund", "sieht", "er", "We\u00b7ge", "gnug"], "token_info": ["wor