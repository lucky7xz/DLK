{"textgrid.poem.53161": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Womit wird die Zeit verbracht,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Womit wird die Zeit verbracht,", "tokens": ["Wo\u00b7mit", "wird", "die", "Zeit", "ver\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun der Herbst sich zu uns macht,", "tokens": ["Nun", "der", "Herbst", "sich", "zu", "uns", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun Gefild und Wald mus trawren,", "tokens": ["Nun", "Ge\u00b7fild", "und", "Wald", "mus", "traw\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df uns au\u00dfzugehen grawt", "tokens": ["Da\u00df", "uns", "au\u00df\u00b7zu\u00b7ge\u00b7hen", "grawt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd man au\u00dferhalb der mawren", "tokens": ["Vnd", "man", "au\u00b7\u00dfer\u00b7halb", "der", "maw\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts als Wust und Vnlust schawt?", "tokens": ["Nichts", "als", "Wust", "und", "Vn\u00b7lust", "schawt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punc