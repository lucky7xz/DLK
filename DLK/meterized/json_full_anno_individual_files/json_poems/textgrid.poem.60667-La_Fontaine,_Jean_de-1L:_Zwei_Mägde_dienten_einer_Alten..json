{"textgrid.poem.60667": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei M\u00e4gde dienten einer Alten.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei M\u00e4gde dienten einer Alten.", "tokens": ["Zwei", "M\u00e4g\u00b7de", "dien\u00b7ten", "ei\u00b7ner", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie spannen, da\u00df die Parzen im Vergleich mit ihnen", "tokens": ["Sie", "span\u00b7nen", ",", "da\u00df", "die", "Par\u00b7zen", "im", "Ver\u00b7gleich", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als St\u00fcmperinnen nur erschienen.", "tokens": ["Als", "St\u00fcm\u00b7pe\u00b7rin\u00b7nen", "nur", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Arbeit stets in Gang zu halten,", "tokens": ["Die", "Ar\u00b7beit", "stets", "in", "Gang", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das war der geizigen Alten einziges Sorgen.", "tokens": ["Das", "war", "der", "gei\u00b7zi\u00b7gen", "Al\u00b7ten", "ein\u00b7zi\u00b7ges", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn Ph\u00f6bus mit verkl\u00e4rten Mienen", "tokens": ["Wenn", "Ph\u00f6\u00b7bus", "mit", "ver\u00b7kl\u00e4r\u00b7ten", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Thetys verlie\u00df am fr\u00fchsten Morgen,", "tokens": ["The\u00b7tys", "ver\u00b7lie\u00df", "am", "fr\u00fchs\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Begann das Spiel von Rad und Spindel schon,", "tokens": ["Be\u00b7gann", "das", "Spiel", "von", "Rad", "und", "Spin\u00b7del", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und emsig trieb man's, bis der Tag entflohn.", "tokens": ["Und", "em\u00b7sig", "trieb", "man's", ",", "bis", "der", "Tag", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kaum trat Auroras Wagen in die Bahn,", "tokens": ["Kaum", "trat", "Au\u00b7ro\u00b7ras", "Wa\u00b7gen", "in", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da rief auch schon ein schlimmer Hahn.", "tokens": ["Da", "rief", "auch", "schon", "ein", "schlim\u00b7mer", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf sprang sofort die Alte, dies noch schlimmer Weib,", "tokens": ["Auf", "sprang", "so\u00b7fort", "die", "Al\u00b7te", ",", "dies", "noch", "schlim\u00b7mer", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "ART", "NN", "$,", "PDS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und warf den Lumpenrock sich um den Leib,", "tokens": ["Und", "warf", "den", "Lum\u00b7pen\u00b7rock", "sich", "um", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der kalt und d\u00fcrr wie ihre Spindel war,", "tokens": ["Der", "kalt", "und", "d\u00fcrr", "wie", "ih\u00b7re", "Spin\u00b7del", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und machte Licht und lief schnurstracks zum Bette,", "tokens": ["Und", "mach\u00b7te", "Licht", "und", "lief", "schnur\u00b7stracks", "zum", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In dem das arme M\u00e4gdepaar", "tokens": ["In", "dem", "das", "ar\u00b7me", "M\u00e4g\u00b7de\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Gar gern ein Weilchen noch geschlafen h\u00e4tte", "tokens": ["Gar", "gern", "ein", "Weil\u00b7chen", "noch", "ge\u00b7schla\u00b7fen", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mit all dem Appetit der jungen Jahre.", "tokens": ["Mit", "all", "dem", "Ap\u00b7pe\u00b7tit", "der", "jun\u00b7gen", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die eine fuhr sich blinzelnd durch die Haare,", "tokens": ["Die", "ei\u00b7ne", "fuhr", "sich", "blin\u00b7zelnd", "durch", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die andre streckte sich mit m\u00fcdem G\u00e4hnen,", "tokens": ["Die", "and\u00b7re", "streck\u00b7te", "sich", "mit", "m\u00fc\u00b7dem", "G\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und beide murmelten voll Unbehagen", "tokens": ["Und", "bei\u00b7de", "mur\u00b7mel\u00b7ten", "voll", "Un\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und mit gefletschten Z\u00e4hnen:", "tokens": ["Und", "mit", "ge\u00b7fletschten", "Z\u00e4h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "\u00bbverw\u00fcnschter Hahn, dir geht es an den Kragen!\u00ab", "tokens": ["\u00bb", "ver\u00b7w\u00fcnschter", "Hahn", ",", "dir", "geht", "es", "an", "den", "Kra\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Und heimlich fingen sie das Vieh.", "tokens": ["Und", "heim\u00b7lich", "fin\u00b7gen", "sie", "das", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Durch seine Weckergurgel fuhr das Messer.", "tokens": ["Durch", "sei\u00b7ne", "We\u00b7cker\u00b7gur\u00b7gel", "fuhr", "das", "Mes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Doch machte dieser Mord ihr Los nicht besser,", "tokens": ["Doch", "mach\u00b7te", "die\u00b7ser", "Mord", "ihr", "Los", "nicht", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Im Gegenteil, kaum schliefen sie,", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "kaum", "schlie\u00b7fen", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Da ri\u00df das Weib in Furcht, die Stunde zu vers\u00e4umen,", "tokens": ["Da", "ri\u00df", "das", "Weib", "in", "Furcht", ",", "die", "Stun\u00b7de", "zu", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Gleich einem Poltergeist das Paar schon aus den Tr\u00e4umen.", "tokens": ["Gleich", "ei\u00b7nem", "Pol\u00b7ter\u00b7geist", "das", "Paar", "schon", "aus", "den", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Man will aus \u00fcbler Lage sich befrein", "tokens": ["Man", "will", "aus", "\u00fcb\u00b7ler", "La\u00b7ge", "sich", "be\u00b7fr\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und rennt sich nur noch mehr hinein.", "tokens": ["Und", "rennt", "sich", "nur", "noch", "mehr", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Beweis: die M\u00e4gde und ihr Lohn.", "tokens": ["Be\u00b7weis", ":", "die", "M\u00e4g\u00b7de", "und", "ihr", "Lohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die Alte an des Hahnes Stelle nun", "tokens": ["Die", "Al\u00b7te", "an", "des", "Hah\u00b7nes", "Stel\u00b7le", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Lie\u00df sie den Sprung von Szylla in Charybdis tun.", "tokens": ["Lie\u00df", "sie", "den", "Sprung", "von", "Szyl\u00b7la", "in", "Cha\u00b7ryb\u00b7dis", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NE", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwei M\u00e4gde dienten einer Alten.", "tokens": ["Zwei", "M\u00e4g\u00b7de", "dien\u00b7ten", "ei\u00b7ner", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie spannen, da\u00df die Parzen im Vergleich mit ihnen", "tokens": ["Sie", "span\u00b7nen", ",", "da\u00df", "die", "Par\u00b7zen", "im", "Ver\u00b7gleich", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als St\u00fcmperinnen nur erschienen.", "tokens": ["Als", "St\u00fcm\u00b7pe\u00b7rin\u00b7nen", "nur", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Arbeit stets in Gang zu halten,", "tokens": ["Die", "Ar\u00b7beit", "st