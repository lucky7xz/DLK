{"textgrid.poem.48194": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin hinauf, hinab gezogen", "tokens": ["Ich", "bin", "hin\u00b7auf", ",", "hin\u00b7ab", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und suchte Gl\u00fcck und sucht' es weit,", "tokens": ["Und", "such\u00b7te", "Gl\u00fcck", "und", "sucht'", "es", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat mein Suchen mich betrogen,", "tokens": ["Es", "hat", "mein", "Su\u00b7chen", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was ich fand, war Einsamkeit.", "tokens": ["Und", "was", "ich", "fand", ",", "war", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rte, wie das Leben l\u00e4rmte,", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "wie", "das", "Le\u00b7ben", "l\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah sein tausendfarbig Licht,", "tokens": ["Ich", "sah", "sein", "tau\u00b7send\u00b7far\u00b7big", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war kein Licht, das mich erw\u00e4rmte,", "tokens": ["Es", "war", "kein", "Licht", ",", "das", "mich", "er\u00b7w\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und echtes Leben war es nicht.", "tokens": ["Und", "ech\u00b7tes", "Le\u00b7ben", "war", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und endlich bin ich heimgegangen", "tokens": ["Und", "end\u00b7lich", "bin", "ich", "heim\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu alter Stell' und alter Lieb',", "tokens": ["Zu", "al\u00b7ter", "Stell'", "und", "al\u00b7ter", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von mir ab fiel das Verlangen,", "tokens": ["Und", "von", "mir", "ab", "fiel", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das einst mich in die Ferne trieb.",