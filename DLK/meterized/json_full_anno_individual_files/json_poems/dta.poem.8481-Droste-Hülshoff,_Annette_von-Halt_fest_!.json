{"dta.poem.8481": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Halt fest !", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Halt fest den Freund, den einmal du erworben,", "tokens": ["Halt", "fest", "den", "Freund", ",", "den", "ein\u00b7mal", "du", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er l\u00e4\u00dft dir keine St\u00e4tte f\u00fcr das Neue;", "tokens": ["Er", "l\u00e4\u00dft", "dir", "kei\u00b7ne", "St\u00e4t\u00b7te", "f\u00fcr", "das", "Neu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft, wie das Haus, in dem ein Leib gestorben,", "tokens": ["L\u00e4\u00dft", ",", "wie", "das", "Haus", ",", "in", "dem", "ein", "Leib", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unrein das Herz, wo modert eine Treue;", "tokens": ["Un\u00b7rein", "das", "Herz", ",", "wo", "mo\u00b7dert", "ei\u00b7ne", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Meinst du, dein sei der H\u00e4nde Druck, der Strahl", "tokens": ["Meinst", "du", ",", "dein", "sei", "der", "H\u00e4n\u00b7de", "Druck", ",", "der", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des eignen Auges arglos und voll Liebe?", "tokens": ["Des", "eig\u00b7nen", "Au\u00b7ges", "arg\u00b7los", "und", "voll", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dr\u00fcckst du zum zweitenmal, blickst du zum zweitenmal,", "tokens": ["Dr\u00fcckst", "du", "zum", "zwei\u00b7ten\u00b7mal", ",", "blickst", "du", "zum", "zwei\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADV", "$,", "VVFIN", "PPER", "APPRART", "ADV", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Die Frucht ist fleckig und der Spiegel tr\u00fcbe.", "tokens": ["Die", "Frucht", "ist", "flec\u00b7kig", "und", "der", "Spie\u00b7gel", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Halt fest dein Wort, o fest wie deine Seele;", "tokens": ["Halt", "fest", "dein", "Wort", ",", "o", "fest", "wie", "dei\u00b7ne", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "FM", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So stolz und freudig mag kein Lorbeer ranken,", "tokens": ["So", "stolz", "und", "freu\u00b7dig", "mag", "kein", "Lor\u00b7beer", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er das Brandmal auf der Stirne hehle,", "tokens": ["Da\u00df", "er", "das", "Brand\u00b7mal", "auf", "der", "Stir\u00b7ne", "heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unter\u2019m Druck des Wortes konnte wanken;", "tokens": ["Die", "un\u00b7ter'm", "Druck", "des", "Wor\u00b7tes", "konn\u00b7te", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der \u00e4rmste Bettler, so ein ehrlich Herz,", "tokens": ["Der", "\u00e4rms\u00b7te", "Bett\u00b7ler", ",", "so", "ein", "ehr\u00b7lich", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wird wie ein K\u00f6nig dir gen\u00fcber treten,", "tokens": ["Wird", "wie", "ein", "K\u00f6\u00b7nig", "dir", "ge\u00b7n\u00fc\u00b7ber", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und du? du zupfst den Lorbeer niederw\u00e4rts", "tokens": ["Und", "du", "?", "du", "zupfst", "den", "Lor\u00b7beer", "nie\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und heimlich mu\u00dft du dein ", "tokens": ["Und", "heim\u00b7lich", "mu\u00dft", "du", "dein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Halt fest den Glauben, la\u00df ihn dir gen\u00fcgen!", "tokens": ["Halt", "fest", "den", "Glau\u00b7ben", ",", "la\u00df", "ihn", "dir", "ge\u00b7n\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVIMP", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer m\u00f6cht\u2019 sein Blut mit fremdem Ichor tauschen!", "tokens": ["Wer", "m\u00f6cht'", "sein", "Blut", "mit", "frem\u00b7dem", "I\u00b7chor", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versto\u00dfe nicht den Cherub deiner Wiegen,", "tokens": ["Ver\u00b7sto\u00b7\u00dfe", "nicht", "den", "Che\u00b7rub", "dei\u00b7ner", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus jedem Blatt wird dir sein Fl\u00fcgel rauschen!", "tokens": ["Aus", "je\u00b7dem", "Blatt", "wird", "dir", "sein", "Fl\u00fc\u00b7gel", "rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ist dein Geist zu stark, vielleicht zu blind,", "tokens": ["Und", "ist", "dein", "Geist", "zu", "stark", ",", "viel\u00b7leicht", "zu", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$,", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In seiner Hand das Flammenschwert zu sehen,", "tokens": ["In", "sei\u00b7ner", "Hand", "das", "Flam\u00b7men\u00b7schwert", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So zweifle nicht, er wird, ein weinend Kind,", "tokens": ["So", "zweif\u00b7le", "nicht", ",", "er", "wird", ",", "ein", "wei\u00b7nend", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An deinem letzten \u00f6den Lager stehen.", "tokens": ["An", "dei\u00b7nem", "letz\u00b7ten", "\u00f6\u00b7den", "La\u00b7ger", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dann die Gabe, gn\u00e4dig dir verliehen,", "tokens": ["Und", "dann", "die", "Ga\u00b7be", ",", "gn\u00e4\u00b7dig", "dir", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den k\u00f6stlichen Moment, den gottgesandten,", "tokens": ["Den", "k\u00f6st\u00b7li\u00b7chen", "Mo\u00b7ment", ",", "den", "gott\u00b7ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O fessle, fessle seinen Quell im Fliehen,", "tokens": ["O", "fess\u00b7le", ",", "fess\u00b7le", "sei\u00b7nen", "Quell", "im", "Flie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Halt jeden Tropfen h\u00f6her als Demanten;", "tokens": ["Halt", "je\u00b7den", "Trop\u00b7fen", "h\u00f6\u00b7her", "als", "De\u00b7man\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch schl\u00e4ft die Stunde, doch sie wacht dereinst", "tokens": ["Noch", "schl\u00e4ft", "die", "Stun\u00b7de", ",", "doch", "sie", "wacht", "de\u00b7reinst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da deinem Willen sich die Kraft entwunden,", "tokens": ["Da", "dei\u00b7nem", "Wil\u00b7len", "sich", "die", "Kraft", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo du verloren schwere Thr\u00e4nen weinst", "tokens": ["Wo", "du", "ver\u00b7lo\u00b7ren", "schwe\u00b7re", "Thr\u00e4\u00b7nen", "weinst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In die Charybdis deiner todten Stunden!", "tokens": ["In", "die", "Cha\u00b7ryb\u00b7dis", "dei\u00b7ner", "tod\u00b7ten", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vor Allem aber halt das Kind der Schmerzen,", "tokens": ["Vor", "Al\u00b7lem", "a\u00b7ber", "halt", "das", "Kind", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein angefochtnes Selbst, von Gott gegeben.", "tokens": ["Dein", "an\u00b7ge\u00b7focht\u00b7nes", "Selbst", ",", "von", "Gott", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O sauge nicht das Blut aus deinem Herzen,", "tokens": ["O", "sau\u00b7ge", "nicht", "das", "Blut", "aus", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um einen Seelenbastard zu beleben;", "tokens": ["Um", "ei\u00b7nen", "See\u00b7len\u00b7bas\u00b7tard", "zu", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df, wenn dir einstens vor dem Golem graut,", "tokens": ["Da\u00df", ",", "wenn", "dir", "eins\u00b7tens", "vor", "dem", "Go\u00b7lem", "graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es zu dir trete nicht mit leisen Klagen:", "tokens": ["Es", "zu", "dir", "tre\u00b7te", "nicht", "mit", "lei\u00b7sen", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u201eso war ich, und so ward ich dir vertraut,", "tokens": ["\u201e", "so", "war", "ich", ",", "und", "so", "ward", "ich", "dir", "ver\u00b7traut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Unsel\u2019ger, warum hast du mich erschlagen!\u201c", "tokens": ["Un\u00b7sel'\u00b7ger", ",", "wa\u00b7rum", "hast", "du", "mich", "er\u00b7schla\u00b7gen", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum fest, nur fest, nur keinen Schritt zur Seite,", "tokens": ["Drum", "fest", ",", "nur", "fest", ",", "nur", "kei\u00b7nen", "Schritt", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "ADV", "PTKVZ", "$,", "ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel hat die Pfade wohl bezeichnet,", "tokens": ["Der", "Him\u00b7mel", "hat", "die", "Pfa\u00b7de", "wohl", "be\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein reines Aug\u2019 erkennt sie aus der Weite,", "tokens": ["Ein", "rei\u00b7nes", "Aug'", "er\u00b7kennt", "sie", "aus", "der", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nur der Wille hat den Pfad verl\u00e4ugnet;", "tokens": ["Und", "nur", "der", "Wil\u00b7le", "hat", "den", "Pfad", "ver\u00b7l\u00e4ug\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns allen ward der Compa\u00df eingedr\u00fcckt,", "tokens": ["Uns", "al\u00b7len", "ward", "der", "Com\u00b7pa\u00df", "ein\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch keiner hat ihn aus der Brust gerissen,", "tokens": ["Noch", "kei\u00b7ner", "hat",