{"textgrid.poem.37693": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen, f\u00fchret dich dein Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen, f\u00fchret dich dein Knabe", "tokens": ["M\u00e4d\u00b7chen", ",", "f\u00fch\u00b7ret", "dich", "dein", "Kna\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem letzten Abendscheine", "tokens": ["In", "dem", "letz\u00b7ten", "A\u00b7bend\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier zu meinem stillen Grabe,", "tokens": ["Hier", "zu", "mei\u00b7nem", "stil\u00b7len", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er wagt es nicht alleine,", "tokens": ["Und", "er", "wagt", "es", "nicht", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fc\u00df' ihn einmal mir zu Ehren,", "tokens": ["K\u00fc\u00df'", "ihn", "ein\u00b7mal", "mir", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das sind meine Seelenmessen;", "tokens": ["Das", "sind", "mei\u00b7ne", "See\u00b7len\u00b7mes\u00b7sen", ";"], "toke