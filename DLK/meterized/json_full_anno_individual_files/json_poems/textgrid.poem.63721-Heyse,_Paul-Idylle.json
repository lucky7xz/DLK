{"textgrid.poem.63721": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Idylle", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Junges Weib, wie manche Stunde", "tokens": ["Jun\u00b7ges", "Weib", ",", "wie", "man\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich deinem Gl\u00fccke zu,", "tokens": ["Seh'", "ich", "dei\u00b7nem", "Gl\u00fc\u00b7cke", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie du auf dem S\u00f6ller droben", "tokens": ["Wie", "du", "auf", "dem", "S\u00f6l\u00b7ler", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaltest ohne Rast und Ruh.", "tokens": ["Schal\u00b7test", "oh\u00b7ne", "Rast", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4hrend du mit kr\u00e4ft'gem Arme", "tokens": ["W\u00e4h\u00b7rend", "du", "mit", "kr\u00e4ft'\u00b7gem", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberm Haupt den Rocken schwingst,", "tokens": ["\u00dc\u00b7berm", "Haupt", "den", "Ro\u00b7cken", "schwingst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schnurrt herab die flinke Spindel,", "tokens": ["Schnurrt", "her\u00b7ab", "die", "flin\u00b7ke", "Spin\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du l\u00e4chelst und du singst.", "tokens": ["Und", "du", "l\u00e4\u00b7chelst", "und", "du", "singst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Singst ein Wiegenlied dem Kleinsten,", "tokens": ["Singst", "ein", "Wie\u00b7gen\u00b7lied", "dem", "Kleins\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das du schaukelst st\u00e4t und leis,", "tokens": ["Das", "du", "schau\u00b7kelst", "st\u00e4t", "und", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es tanzt dazu dein Knabe", "tokens": ["Und", "es", "tanzt", "da\u00b7zu", "dein", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Schwesterchen im Kreis.", "tokens": ["Mit", "dem", "Schwes\u00b7ter\u00b7chen", "im", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tarantella tanzt die Kleine,", "tokens": ["Ta\u00b7ran\u00b7tel\u00b7la", "tanzt", "die", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch in ihren ersten Schuhn,", "tokens": ["Noch", "in", "ih\u00b7ren", "ers\u00b7ten", "Schuhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klatscht den Takt mit beiden H\u00e4ndchen,", "tokens": ["Klatscht", "den", "Takt", "mit", "bei\u00b7den", "H\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, wie's die Gro\u00dfen tun.", "tokens": ["Al\u00b7les", ",", "wie's", "die", "Gro\u00b7\u00dfen", "tun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sie werden es nicht m\u00fcd,", "tokens": ["Und", "sie", "wer\u00b7den", "es", "nicht", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis dem kleinen Paar die Wange", "tokens": ["Bis", "dem", "klei\u00b7nen", "Paar", "die", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkel wie Granate gl\u00fcht.", "tokens": ["Dun\u00b7kel", "wie", "Gra\u00b7na\u00b7te", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Jetzt Orangen aus dem K\u00f6rbchen", "tokens": ["Jetzt", "O\u00b7ran\u00b7gen", "aus", "dem", "K\u00f6rb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Br\u00f6tchen aus dem Schrank", "tokens": ["Und", "ein", "Br\u00f6t\u00b7chen", "aus", "dem", "Schrank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Teilst du aus zum Abendimbi\u00df,", "tokens": ["Teilst", "du", "aus", "zum", "A\u00b7ben\u00b7dim\u00b7bi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und sie k\u00fcssen dich zum Dank.", "tokens": ["Und", "sie", "k\u00fcs\u00b7sen", "dich", "zum", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und das Kind verlangt zu trinken,", "tokens": ["Und", "das", "Kind", "ver\u00b7langt", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das H\u00fcndchen springt und bellt,", "tokens": ["Und", "das", "H\u00fcnd\u00b7chen", "springt", "und", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die kleinen V\u00f6gel wissen,", "tokens": ["Und", "die", "klei\u00b7nen", "V\u00f6\u00b7gel", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man offne Tafel h\u00e4lt.", "tokens": ["Wo", "man", "off\u00b7ne", "Ta\u00b7fel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun kommt die Nacht, so duftig, mild und klar.", "tokens": ["Nun", "kommt", "die", "Nacht", ",", "so", "duf\u00b7tig", ",", "mild", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kinder schl\u00e4fert's. In dem Bettchen dort", "tokens": ["Die", "Kin\u00b7der", "schl\u00e4\u00b7fert'", "s.", "In", "dem", "Bett\u00b7chen", "dort"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bringst du zur Ruh das kleine T\u00e4nzerpaar;", "tokens": ["Bringst", "du", "zur", "Ruh", "das", "klei\u00b7ne", "T\u00e4n\u00b7zer\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das J\u00fcngste schl\u00e4ft im Wiegenkorbe fort.", "tokens": ["Das", "J\u00fcngs\u00b7te", "schl\u00e4ft", "im", "Wie\u00b7gen\u00b7kor\u00b7be", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du aber trittst hinaus, und vom Balkon,", "tokens": ["Du", "a\u00b7ber", "trittst", "hin\u00b7aus", ",", "und", "vom", "Bal\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$,", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Liedchen summend, sacht das Haupt gewiegt,", "tokens": ["Ein", "Lied\u00b7chen", "sum\u00b7mend", ",", "sacht", "das", "Haupt", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Blickst du umher. Es klingt kein falscher Ton", "tokens": ["Blickst", "du", "um\u00b7her", ".", "Es", "klingt", "kein", "fal\u00b7scher", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch dieses Herz, das tief in Frieden liegt.", "tokens": ["Durch", "die\u00b7ses", "Herz", ",", "das", "tief", "in", "Frie\u00b7den", "liegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Seitdem du atmest, kennst du alles hier,", "tokens": ["Seit\u00b7dem", "du", "at\u00b7mest", ",", "kennst", "du", "al\u00b7les", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stadt, Meer und Menschen. Doch was k\u00fcmmert's dich?", "tokens": ["Stadt", ",", "Meer", "und", "Men\u00b7schen", ".", "Doch", "was", "k\u00fcm\u00b7mert's", "dich", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$.", "KON", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Heimat selbst \u2013 zur Fremde ward sie dir,", "tokens": ["Die", "Hei\u00b7mat", "selbst", "\u2013", "zur", "Frem\u00b7de", "ward", "sie", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "APPRART", "NN", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seit Ein Gef\u00fchl den Busen dir beschlich.", "tokens": ["Seit", "Ein", "Ge\u00b7f\u00fchl", "den", "Bu\u00b7sen", "dir", "be\u00b7schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Mann \u2013 Kinder \u2013 Haus, und dr\u00fcber nur ein Grab.", "tokens": ["Mann", "\u2013", "Kin\u00b7der", "\u2013", "Haus", ",", "und", "dr\u00fc\u00b7ber", "nur", "ein", "Grab", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$,", "KON", "PAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du nickst wie tr\u00e4umend, gr\u00fc\u00dft dich die und der.", "tokens": ["Du", "nickst", "wie", "tr\u00e4u\u00b7mend", ",", "gr\u00fc\u00dft", "dich", "die", "und", "der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$,", "VVFIN", "PPER", "ART", "KON", "ART", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Nachtwind s\u00e4uselt gassenauf und -ab,", "tokens": ["Der", "Nacht\u00b7wind", "s\u00e4u\u00b7selt", "gas\u00b7sen\u00b7auf", "und", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Mond geht auf; du \u00fcberblickst das Meer.", "tokens": ["Der", "Mond", "geht", "auf", ";", "du", "\u00fc\u00b7berb\u00b7lickst", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ein Nachen von Neapel! Vogelschnell", "tokens": ["Ein", "Na\u00b7chen", "von", "Nea\u00b7pel", "!", "Vo\u00b7gel\u00b7schnell"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "NE", "$.", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durchschneidet er die Flut. Du sp\u00e4hst und sp\u00e4hst \u2013", "tokens": ["Durch\u00b7schnei\u00b7det", "er", "die", "Flut", ".", "Du", "sp\u00e4hst", "und", "sp\u00e4hst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist er's? \u2013 Dein Aug antwortet freudenhell,", "tokens": ["Ist", "er's", "?", "\u2013", "Dein", "Aug", "ant\u00b7wor\u00b7tet", "freu\u00b7den\u00b7hell", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$.", "$(", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein L\u00e4mpchen z\u00fcndest du, mit dem du wehst.", "tokens": ["Ein", "L\u00e4mp\u00b7chen", "z\u00fcn\u00b7dest", "du", ",", "mit", "dem", "du", "wehst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Noch kurze Frist, dann klingt ein rascher Fu\u00df,", "tokens": ["Noch", "kur\u00b7ze", "Frist", ",", "dann", "klingt", "ein", "ra\u00b7scher", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Knabe lacht im Schlaf, das H\u00fcndchen bellt,", "tokens": ["Der", "Kna\u00b7be", "lacht", "im", "Schlaf", ",", "das", "H\u00fcnd\u00b7chen", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00fcre geht \u2013 Willkommen, Gru\u00df und Ku\u00df,", "tokens": ["Die", "T\u00fc\u00b7re", "geht", "\u2013", "Will\u00b7kom\u00b7men", ",", "Gru\u00df", "und", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und in zwei Armen h\u00e4ltst du deine Welt!", "tok