meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbnimm dich in acht! Der Flu\u00df ist tief.\u00ab", "tokens": ["\u00bb", "nimm", "dich", "in", "acht", "!", "Der", "Flu\u00df", "ist", "tief", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "CARD", "$.", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da lief mir was durchs ganze Blut,", "tokens": ["Da", "lief", "mir", "was", "durchs", "gan\u00b7ze", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRELS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh, so ist's ein liebes M\u00e4dchen;", "tokens": ["Ich", "seh", ",", "so", "ist's", "ein", "lie\u00b7bes", "M\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fragte sie: \u00bbWie hei\u00dft du?\u00ab \u2013 \u00bbK\u00e4thchen!\u00ab", "tokens": ["Ich", "frag\u00b7te", "sie", ":", "\u00bb", "Wie", "hei\u00dft", "du", "?", "\u00ab", "\u2013", "\u00bb", "K\u00e4th\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$(", "PWAV", "VVFIN", "PPER", "$.", "$(", "$(", "$(", "NE", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbo sch\u00f6nes K\u00e4thchen! Du bist gut.", "tokens": ["\u00bb", "o", "sch\u00f6\u00b7nes", "K\u00e4th\u00b7chen", "!", "Du", "bist", "gut", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du h\u00e4ltst vom Tode mich zur\u00fcck,", "tokens": ["Du", "h\u00e4ltst", "vom", "To\u00b7de", "mich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf immer dank ich dir mein Leben;", "tokens": ["Auf", "im\u00b7mer", "dank", "ich", "dir", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein das hei\u00dft mir wenig geben,", "tokens": ["Al\u00b7lein", "das", "hei\u00dft", "mir", "we\u00b7nig", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun sei auch meines Lebens Gl\u00fcck!\u00ab", "tokens": ["Nun", "sei", "auch", "mei\u00b7nes", "Le\u00b7bens", "Gl\u00fcck", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und dann klagt ich ihr meine Not,", "tokens": ["Und", "dann", "klagt", "ich", "ihr", "mei\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sie schlug die Augen lieblich nieder;", "tokens": ["Sie", "sch