{"textgrid.poem.46081": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leb wohl du Jahr voll Tr\u00e4nen!", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb wohl du Jahr voll Tr\u00e4nen!", "tokens": ["Leb", "wohl", "du", "Jahr", "voll", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O lasse mich an deinem letzten Tag", "tokens": ["O", "las\u00b7se", "mich", "an", "dei\u00b7nem", "letz\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal selig w\u00e4hnen,", "tokens": ["Noch", "ein\u00b7mal", "se\u00b7lig", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich an einem Kinderherzen lag!", "tokens": ["Da\u00df", "ich", "an", "ei\u00b7nem", "Kin\u00b7der\u00b7her\u00b7zen", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geh hin du Jahr voll Tr\u00e4nen!", "tokens": ["Geh", "hin", "du", "Jahr", "voll", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tritt glaubend hin vor Gottes Thron,", "tokens": ["Tritt", "glau\u00b7bend", "hin", "vor", "Got\u00b7tes", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird um krankes Sehnen", "tokens": ["Er", "wird", "um", "kran\u00b7kes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich strenge richten, nimmer doch um Hohn!", "tokens": ["Dich", "stren\u00b7ge", "rich\u00b7ten", ",", "nim\u00b7mer", "doch", "um", "Hohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O selig Jahr voll Tr\u00e4nen!", "tokens": ["O", "se\u00b7lig", "Jahr", "voll", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "ADJD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "War dir auch fr\u00fch das tiefre Wort geraubt,", "tokens": ["War", "dir", "auch", "fr\u00fch", "das", "tief\u00b7re", "Wort", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So war der Strom der Tr\u00e4nen", "tokens": ["So", "war", "der", "Strom", "der", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihren F\u00fc\u00dfen oft dir doch erlaubt!", "tokens": ["Zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "oft", "dir", "doch", "er\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O liebes Jahr voll Tr\u00e4nen!", "tokens": ["O", "lie\u00b7bes", "Jahr", "voll", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O dichte Saat, wie segnend reift dein Schmerz,", "tokens": ["O", "dich\u00b7te", "Saat", ",", "wie", "seg\u00b7nend", "reift", "dein", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O hochbelohnt! mein Sehnen!", "tokens": ["O", "hoch\u00b7be\u00b7lohnt", "!", "mein", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich f\u00fchlte jauchzend, ja! sie hat ein Herz!", "tokens": ["Ich", "f\u00fchl\u00b7te", "jauch\u00b7zend", ",", "ja", "!", "sie", "hat", "ein", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Jahr von hei\u00dfen Tr\u00e4nen!", "tokens": ["O", "Jahr", "von", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geheimnisvoller, als sie wei\u00df, berauscht,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", ",", "als", "sie", "wei\u00df", ",", "be\u00b7rauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was all sie kann versch\u00f6nen,", "tokens": ["Was", "all", "sie", "kann", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast in Tr\u00e4nen sterbend es belauscht.", "tokens": ["Du", "hast", "in", "Tr\u00e4\u00b7nen", "ster\u00b7bend", "es", "be\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Jahr voll bitt'rer Tr\u00e4nen!", "tokens": ["O", "Jahr", "voll", "bitt'\u00b7rer", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist irgend Gottes Wahrheit offenbar,", "tokens": ["Ist", "ir\u00b7gend", "Got\u00b7tes", "Wahr\u00b7heit", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist vieles hier nur W\u00e4hnen,", "tokens": ["Ist", "vie\u00b7les", "hier", "nur", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So opfre, weine darum am Altar!", "tokens": ["So", "opf\u00b7re", ",", "wei\u00b7ne", "da\u00b7rum", "am", "Al\u00b7tar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "O Jahr voll tiefer Tr\u00e4nen!", "tokens": ["O", "Jahr", "voll", "tie\u00b7fer", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du magst vertraut dein armes m\u00fcdes Haupt,", "tokens": ["Du", "magst", "ver\u00b7traut", "dein", "ar\u00b7mes", "m\u00fc\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ans Kreuz nur ruhig lehnen,", "tokens": ["Ans", "Kreuz", "nur", "ru\u00b7hig", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast geliebet, hast gehofft, geglaubt.", "tokens": ["Du", "hast", "ge\u00b7lie\u00b7bet", ",", "hast", "ge\u00b7hofft", ",", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O teures Jahr voll Tr\u00e4nen!", "tokens": ["O", "teu\u00b7res", "Jahr", "voll", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bist in bittrer Reue Flut getauft,", "tokens": ["Du", "bist", "in", "bit\u00b7trer", "Reu\u00b7e", "Flut", "ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der wird uns auch vers\u00f6hnen,", "tokens": ["Der", "wird", "uns", "auch", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der uns mit seiner Weihe Blut erkauft.", "tokens": ["Der", "uns", "mit", "sei\u00b7ner", "Wei\u00b7he", "Blut", "er\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Geh hin! du Jahr voll Tr\u00e4nen!", "tokens": ["Geh", "hin", "!", "du", "Jahr", "voll", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh, werfe dich zu ihren F\u00fc\u00dfen hin!", "tokens": ["Geh", ",", "wer\u00b7fe", "dich", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wasche sie mit Tr\u00e4nen", "tokens": ["Und", "wa\u00b7sche", "sie", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sag ihr, da\u00df ich ihr armer Bruder bin!", "tokens": ["Sag", "ihr", ",", "da\u00df", "ich", "ihr", "ar\u00b7mer", "Bru\u00b7der", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ihr Bruder ganz in Tr\u00e4nen,", "tokens": ["Ihr", "Bru\u00b7der", "ganz", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr kranker Bruder, um die eigne Schuld,", "tokens": ["Ihr", "kran\u00b7ker", "Bru\u00b7der", ",", "um", "die", "eig\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um fremde Schuld in Tr\u00e4nen,", "tokens": ["Um", "frem\u00b7de", "Schuld", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Bruder weinend um der V\u00e4ter Schuld!", "tokens": ["Ihr", "Bru\u00b7der", "wei\u00b7nend", "um", "der", "V\u00e4\u00b7ter", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "O sterbe Jahr in Tr\u00e4nen", "tokens": ["O", "ster\u00b7be", "Jahr", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"te