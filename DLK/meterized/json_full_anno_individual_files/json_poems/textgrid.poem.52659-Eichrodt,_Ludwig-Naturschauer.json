{"textgrid.poem.52659": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Naturschauer", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wellen rauschen, es nachtet,", "tokens": ["Die", "Wel\u00b7len", "rau\u00b7schen", ",", "es", "nach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Mond scheint mit Verdru\u00df,", "tokens": ["Der", "Mond", "scheint", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geschlagene Soldaten", "tokens": ["Ge\u00b7schla\u00b7ge\u00b7ne", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Marschiren \u00fcber den Flu\u00df.", "tokens": ["Mar\u00b7schi\u00b7ren", "\u00fc\u00b7ber", "den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Dort nicken die ungeheuern", "tokens": ["Dort", "ni\u00b7cken", "die", "un\u00b7ge\u00b7heu\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Felsberge gespensterhaft,", "tokens": ["Fels\u00b7ber\u00b7ge", "ge\u00b7spens\u00b7ter\u00b7haft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es raget \u00fcber dem Wirrsal", "tokens": ["Es", "ra\u00b7get", "\u00fc\u00b7ber", "dem", "Wirr\u00b7sal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die d\u00fcstre Prachtlandschaft.", "tokens": ["Die", "d\u00fcst\u00b7re", "Pracht\u00b7land\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Schauer, machtvoll, herrlich,", "tokens": ["Ein", "Schau\u00b7er", ",", "macht\u00b7voll", ",", "herr\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weht durch die stumme Natur,", "tokens": ["Weht", "durch", "die", "stum\u00b7me", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Als w\u00e4re ein Gott gestorben,", "tokens": ["Als", "w\u00e4\u00b7re", "ein", "Gott", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist ringsum Trauer nur.", "tokens": ["Ist", "ring\u00b7sum", "Trau\u00b7er", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch in den wunden Gem\u00fcthern", "tokens": ["Auch", "in", "den", "wun\u00b7den", "Ge\u00b7m\u00fc\u00b7thern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Krieger lagert Nacht,", "tokens": ["Der", "Krie\u00b7ger", "la\u00b7gert", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In tausend Herzen dunkelts,", "tokens": ["In", "tau\u00b7send", "Her\u00b7zen", "dun\u00b7kelts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch es dunkelt ohne Pracht.", "tokens": ["Doch", "es", "dun\u00b7kelt", "oh\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Genommen ist die Fahne,", "tokens": ["Ge\u00b7nom\u00b7men", "ist", "die", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gefallen der beste Held,", "tokens": ["Ge\u00b7fal\u00b7len", "der", "bes\u00b7te", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Vaterland ist verloren", "tokens": ["Das", "Va\u00b7ter\u00b7land", "ist", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u2013 Gewonnen die weite Welt.", "tokens": ["\u2013", "Ge\u00b7won\u00b7nen", "die", "wei\u00b7te", "Welt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Wellen rauschen, es nachtet,", "tok