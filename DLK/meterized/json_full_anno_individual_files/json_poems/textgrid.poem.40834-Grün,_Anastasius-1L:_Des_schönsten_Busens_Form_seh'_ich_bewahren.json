{"textgrid.poem.40834": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des sch\u00f6nsten Busens Form seh' ich bewahren", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des sch\u00f6nsten Busens Form seh' ich bewahren", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "Bu\u00b7sens", "Form", "seh'", "ich", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich, graue Lava, Aphroditens Becher!", "tokens": ["Dich", ",", "grau\u00b7e", "La\u00b7va", ",", "A\u00b7phro\u00b7di\u00b7tens", "Be\u00b7cher", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "NE", "NE", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Liebe Trank, den ew'gen, feuerklaren,", "tokens": ["Der", "Lie\u00b7be", "Trank", ",", "den", "ew'\u00b7gen", ",", "feu\u00b7er\u00b7kla\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schl\u00fcrf' ich aus dir, ein durst'ger Liebeszecher!", "tokens": ["Schl\u00fcr\u00b7f'", "ich", "aus", "dir", ",", "ein", "dur\u00b7st'\u00b7ger", "Lie\u00b7bes\u00b7ze\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich seh' die sch\u00f6nste von Pompeji's Frauen", "tokens": ["Ich", "seh'", "die", "sch\u00f6ns\u00b7te", "von", "Pom\u00b7pe\u00b7ji's", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "NE", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Garten, der sich sonnig vor ihr breitet!", "tokens": ["Im", "Gar\u00b7ten", ",", "der", "sich", "son\u00b7nig", "vor", "ihr", "brei\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl ist er sch\u00f6n und bl\u00fcthenvoll zu schauen,", "tokens": ["Wohl", "ist", "er", "sch\u00f6n", "und", "bl\u00fc\u00b7then\u00b7voll", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sch\u00f6ner, \u00fcpp'ger bl\u00fcht, die ihn durchschreitet.", "tokens": ["Doch", "sch\u00f6\u00b7ner", ",", "\u00fcpp'\u00b7ger", "bl\u00fcht", ",", "die", "ihn", "durch\u00b7schrei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es h\u00e4lt Akanth und Bux als Wacht von Zwergen", "tokens": ["Es", "h\u00e4lt", "A\u00b7kan\u00b7th", "und", "Bux", "als", "Wacht", "von", "Zwer\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "KON", "NE", "KOUS", "NN", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Haft Viol' und Ros' im gr\u00fcnen Erker;", "tokens": ["In", "Haft", "Vi\u00b7ol'", "und", "Ros'", "im", "gr\u00fc\u00b7nen", "Er\u00b7ker", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Mieder doch mag als Gefangne bergen", "tokens": ["Ihr", "Mie\u00b7der", "doch", "mag", "als", "Ge\u00b7fang\u00b7ne", "ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "KOUS", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zwei sch\u00f6nre R\u00f6slein wohl in seinem Kerker.", "tokens": ["Zwei", "sch\u00f6n\u00b7re", "R\u00f6s\u00b7lein", "wohl", "in", "sei\u00b7nem", "Ker\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich seh' als Silberschaft den Springquell steigen", "tokens": ["Ich", "seh'", "als", "Sil\u00b7ber\u00b7schaft", "den", "Spring\u00b7quell", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihn als Schnee millionenflockig fallen,", "tokens": ["Und", "ihn", "als", "Schnee", "mil\u00b7li\u00b7o\u00b7nen\u00b7flo\u00b7ckig", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Gleich einer Trauerweid' aus Silberzweigen,", "tokens": ["Gleich", "ei\u00b7ner", "Trau\u00b7e\u00b7rweid'", "aus", "Sil\u00b7ber\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sch\u00f6ner, wei\u00dfer ihren Busen wallen!", "tokens": ["Doch", "sch\u00f6\u00b7ner", ",", "wei\u00b7\u00dfer", "ih\u00b7ren", "Bu\u00b7sen", "wal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da sieht der Geist des Feuerbergs hernieder", "tokens": ["Da", "sieht", "der", "Geist", "des", "Feu\u00b7er\u00b7bergs", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Flammenthron; ihn fa\u00dft die Macht der Liebe!", "tokens": ["Vom", "Flam\u00b7men\u00b7thron", ";", "ihn", "fa\u00dft", "die", "Macht", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bebt, wenn euch G\u00f6tter hassen, Erdenbr\u00fcder,", "tokens": ["Bebt", ",", "wenn", "euch", "G\u00f6t\u00b7ter", "has\u00b7sen", ",", "Er\u00b7den\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch auch nicht minder bebt ob ihrer Liebe!", "tokens": ["Doch", "auch", "nicht", "min\u00b7der", "bebt", "ob", "ih\u00b7rer", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVFIN", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schon eilt, da\u00df ihn kein Sp\u00e4her \u00fcberrasche,", "tokens": ["Schon", "eilt", ",", "da\u00df", "ihn", "kein", "Sp\u00e4\u00b7her", "\u00fc\u00b7berr\u00b7a\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Mohrensklave, jene schwarze Wolke,", "tokens": ["Sein", "Moh\u00b7rens\u00b7kla\u00b7ve", ",", "je\u00b7ne", "schwar\u00b7ze", "Wol\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem Schleier \u2013 ach, von Staub und Asche! \u2013", "tokens": ["Mit", "ei\u00b7nem", "Schlei\u00b7er", "\u2013", "ach", ",", "von", "Staub", "und", "A\u00b7sche", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "XY", "$,", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebe Haus zu h\u00fcllen vor dem Volke!", "tokens": ["Der", "Lie\u00b7be", "Haus", "zu", "h\u00fcl\u00b7len", "vor", "dem", "Vol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schon mu\u00df dem Kuppler nach, da\u00df er nicht weile,", "tokens": ["Schon", "mu\u00df", "dem", "Kupp\u00b7ler", "nach", ",", "da\u00df", "er", "nicht", "wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Sklavenvogt, der Sturm, jetzt brausend fahren;", "tokens": ["Sein", "Skla\u00b7ven\u00b7vogt", ",", "der", "Sturm", ",", "jetzt", "brau\u00b7send", "fah\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der peitscht mit Feuerruthen ihn zur Eile", "tokens": ["Der", "peitscht", "mit", "Feu\u00b7er\u00b7ru\u00b7then", "ihn", "zur", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zaust in seinen schwarzen, krausen Haaren!", "tokens": ["Und", "zaust", "in", "sei\u00b7nen", "schwar\u00b7zen", ",", "krau\u00b7sen", "Haa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Schon tobt herab der Herr die Bergestreppe,", "tokens": ["Schon", "tobt", "her\u00b7ab", "der", "Herr", "die", "Ber\u00b7ge\u00b7strep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Purpurmantel gl\u00fch'nder Laven wallend;", "tokens": ["Im", "Pur\u00b7pur\u00b7man\u00b7tel", "gl\u00fch'n\u00b7der", "La\u00b7ven", "wal\u00b7lend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vesuv als Page h\u00e4lt den Saum der Schleppe,", "tokens": ["Ve\u00b7suv", "als", "Pa\u00b7ge", "h\u00e4lt", "den", "Saum", "der", "Schlep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ries'gem Bogen seinem Arm entfallend!", "tokens": ["In", "ries'\u00b7gem", "Bo\u00b7gen", "sei\u00b7nem", "Arm", "ent\u00b7fal\u00b7lend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So ungest\u00fcm hetzt Jenen Liebeshitze,", "tokens": ["So", "un\u00b7ge\u00b7st\u00fcm", "hetzt", "Je\u00b7nen", "Lie\u00b7bes\u00b7hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df aus der Feuerkron' im Niederwallen", "tokens": ["Da\u00df", "aus", "der", "Feu\u00b7er\u00b7kron'", "im", "Nie\u00b7der\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm Diamanten: flammenhelle Blitze,", "tokens": ["Ihm", "Di\u00b7a\u00b7man\u00b7ten", ":", "flam\u00b7me\u00b7nhel\u00b7le", "Blit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Granaten: gl\u00fch'nde Felsen taumelnd fallen!", "tokens": ["Gra\u00b7na\u00b7ten", ":", "gl\u00fch'n\u00b7de", "Fel\u00b7sen", "tau\u00b7melnd", "fal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Schon ist er da, die Arme ausgebreitet,", "tokens": ["Schon", "ist", "er", "da", ",", "die", "Ar\u00b7me", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die feur'gen, da\u00df den s\u00fc\u00dfen Leib er hasche!", "tokens": ["Die", "feur'\u00b7gen", ",", "da\u00df", "den", "s\u00fc\u00b7\u00dfen", "Leib", "er", "ha\u00b7sche", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ab von seinem Herzen dieser gleitet,", "tokens": ["Doch", "ab", "von", "sei\u00b7nem", "Her\u00b7zen", "die\u00b7ser", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "PPOSAT", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und knickt zur Erd' als eine Handvoll Asche.", "tokens": ["Und", "knickt", "zur", "Erd'", "als", "ei\u00b7ne", "Hand\u00b7voll", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Rosen sind verdorrt am Hochzeitfeste!", "tokens": ["Die", "Ro\u00b7sen", "sind", "ver\u00b7dorrt", "am", "Hoch\u00b7zeit\u00b7fes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Quellen sind versiegt im Gartengrunde!", "tokens": ["Die", "Quel\u00b7len", "sind", "ver\u00b7siegt", "im", "Gar\u00b7ten\u00b7grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur in des K\u00f6nigsmantels Lava pre\u00dfte", "tokens": ["Nur", "in", "des", "K\u00f6\u00b7nigs\u00b7man\u00b7tels", "La\u00b7va", "pre\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich ab des sch\u00f6nsten Busens volle Runde.", "tokens": ["Sich", "ab", "des", "sch\u00f6ns\u00b7ten", "Bu\u00b7sens", "vol\u00b7le", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da sprach der Gott: \u00bbWeib, deines Leibes Sch\u00f6ne", "tokens": ["Da", "sprach", "der", "Gott", ":", "\u00bb", "Weib", ",", "dei\u00b7nes", "Lei\u00b7bes", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verweh' nicht, Rosen gleich, im Ku\u00df der Winde!", "tokens": ["Ver\u00b7weh'", "nicht", ",", "Ro\u00b7sen", "gleich", 