{"dta.poem.8836": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch du bist allzusch\u00f6n, dich w\u00fcnscht' ich weit hinweg!", "tokens": ["Doch", "du", "bist", "all\u00b7zu\u00b7sch\u00f6n", ",", "dich", "w\u00fcnscht'", "ich", "weit", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn, wie zu gut ich wei\u00df, sobald die Liebe naht,", "tokens": ["Denn", ",", "wie", "zu", "gut", "ich", "wei\u00df", ",", "so\u00b7bald", "die", "Lie\u00b7be", "naht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PTKA", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So flieht die schelmische Gelegenheit hinweg!", "tokens": ["So", "flieht", "die", "schel\u00b7mi\u00b7sche", "Ge\u00b7le\u00b7gen\u00b7heit", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer stand gef\u00fchlbegabt dir gegen\u00fcber je,", "tokens": ["Wer", "stand", "ge\u00b7f\u00fchl\u00b7be\u00b7gabt", "dir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und schlug die Augen auf, und ging befreit hinweg?", "tokens": ["Und", "schlug", "die", "Au\u00b7gen", "auf", ",", "und", "ging", "be\u00b7freit", "hin\u00b7weg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch Andre find' ich sch\u00f6n, doch hebst du, wenn du", "tokens": ["Auch", "And\u00b7re", "find'", "ich", "sch\u00f6n", ",", "doch", "hebst", "du", ",", "wenn", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Mich \u00fcber jede Wahl und jeden Streit hinweg!", "tokens": ["Mich", "\u00fc\u00b7ber", "je\u00b7de", "Wahl", "und", "je\u00b7den", "Streit", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn je sich in dein Haar verwickelt meine Hand,", "tokens": ["Wenn", "je", "sich", "in", "dein", "Haar", "ver\u00b7wi\u00b7ckelt", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So f\u00fchre mich der Tod, ich bin bereit, hinweg!", "tokens": ["So", "f\u00fch\u00b7re", "mich", "der", "Tod", ",", "ich", "bin", "be\u00b7reit", ",", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}