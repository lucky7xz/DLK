{"dta.poem.21700": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Admiral Hosier's Geist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Spaniens Heersmacht war geschlagen.", "tokens": ["Spa\u00b7ni\u00b7ens", "Heers\u00b7macht", "war", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Neben ", "tokens": ["Ne\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Triumphirend Englands Flotte.", "tokens": ["Tri\u00b7um\u00b7phi\u00b7rend", "En\u00b7glands", "Flot\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiss und glorreich war der Tag.", "tokens": ["Heiss", "und", "glor\u00b7reich", "war", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vernon nun und seine Braven", "tokens": ["Ver\u00b7non", "nun", "und", "sei\u00b7ne", "Bra\u00b7ven"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ruhten nach der schw\u00fclen Schlacht.", "tokens": ["Ruh\u00b7ten", "nach", "der", "schw\u00fc\u00b7len", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wimpel str\u00f6mten, Becher klangen,", "tokens": ["Wim\u00b7pel", "str\u00f6m\u00b7ten", ",", "Be\u00b7cher", "klan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Siegsgeschrey durchscholl die Nacht.", "tokens": ["Siegs\u00b7ge\u00b7schrey", "durch\u00b7scholl", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pl\u00f6tzlich gellt' ein gr\u00e4sslich Heulen", "tokens": ["Pl\u00f6tz\u00b7lich", "gellt'", "ein", "gr\u00e4ss\u00b7lich", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der dunkeln Fluth hervor.", "tokens": ["Aus", "der", "dun\u00b7keln", "Fluth", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geister schw\u00e4rmten graunlich wimmernd", "tokens": ["Geis\u00b7ter", "schw\u00e4rm\u00b7ten", "graun\u00b7lich", "wim\u00b7mernd"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rechts und links und rings empor.", "tokens": ["Rechts", "und", "links", "und", "rings", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Statt der Leichent\u00fccher h\u00fcllten", "tokens": ["Statt", "der", "Lei\u00b7chen\u00b7t\u00fc\u00b7cher", "h\u00fcll\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hangematten jeden ein.", "tokens": ["Han\u00b7ge\u00b7mat\u00b7ten", "je\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder schien mit scheelen Blicken", "tokens": ["Je\u00b7der", "schien", "mit", "schee\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Portobello zu bedr\u00e4un.", "tokens": ["Por\u00b7to\u00b7bel\u00b7lo", "zu", "be\u00b7dr\u00e4un", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mondstrahl flittert' auf den Wogen;", "tokens": ["Mond\u00b7strahl", "flit\u00b7tert'", "auf", "den", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem matterhellten Meer", "tokens": ["Auf", "dem", "mat\u00b7ter\u00b7hell\u00b7ten", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hob sich ", "tokens": ["Hob", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Musterte das nicht'ge Heer,", "tokens": ["Mus\u00b7ter\u00b7te", "das", "nicht'\u00b7ge", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwebte dann zu Vernon's Grusse", "tokens": ["Schweb\u00b7te", "dann", "zu", "Ver\u00b7non's", "Grus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An des ", "tokens": ["An", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Dreymal tausend Geister schw\u00e4rmten", "tokens": ["Drey\u00b7mal", "tau\u00b7send", "Geis\u00b7ter", "schw\u00e4rm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rings um ihn und er begann:", "tokens": ["Rings", "um", "ihn", "und", "er", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6ret, h\u00f6ret Hosier's Rede!", "tokens": ["H\u00f6\u00b7ret", ",", "h\u00f6\u00b7ret", "Ho\u00b7sier's", "Re\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hosier's irrer Geist bin ich.", "tokens": ["Ho\u00b7sier's", "ir\u00b7rer", "Geist", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier, wo ihr euch Ruhm erwandet,", "tokens": ["Hier", ",", "wo", "ihr", "euch", "Ruhm", "er\u00b7wan\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier traf das Verderben mich.", "tokens": ["Hier", "traf", "das", "Ver\u00b7der\u00b7ben", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Euch bringt Portobello Ehre,", "tokens": ["Euch", "bringt", "Por\u00b7to\u00b7bel\u00b7lo", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Mir bringt's bittres Herzeleid.", "tokens": ["Mir", "bringt's", "bitt\u00b7res", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Portobello! Portobello!", "tokens": ["Por\u00b7to\u00b7bel\u00b7lo", "!", "Por\u00b7to\u00b7bel\u00b7lo", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wehe dir in Ewigkeit.", "tokens": ["We\u00b7he", "dir", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht ihr diese nichtgen Schaaren.", "tokens": ["Seht", "ihr", "die\u00b7se", "nicht\u00b7gen", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krieger waren's brav und stark.", "tokens": ["Krie\u00b7ger", "wa\u00b7ren's", "brav", "und", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feuer f\u00fcllt' einst ihre Adern,", "tokens": ["Feu\u00b7er", "f\u00fcllt'", "einst", "ih\u00b7re", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Knochen L\u00f6wenmark.", "tokens": ["Ih\u00b7re", "Kno\u00b7chen", "L\u00f6\u00b7wen\u00b7mark", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht ihr jene h\u00f6hern Schatten", "tokens": ["Seht", "ihr", "je\u00b7ne", "h\u00f6\u00b7hern", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit den Blicken starr und stier?", "tokens": ["Mit", "den", "Bli\u00b7cken", "starr", "und", "stier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Feldherrn waren's, wackrer Vernon,", "tokens": ["Feld\u00b7herrn", "wa\u00b7ren's", ",", "wack\u00b7rer", "Ver\u00b7non", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Tapfre Feldherrn einst, wie wir.", "tokens": ["Tapf\u00b7re", "Feld\u00b7herrn", "einst", ",", "wie", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Her kam ich mit zwanzig Segeln,", "tokens": ["Her", "kam", "ich", "mit", "zwan\u00b7zig", "Se\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lag dem Feind im Angesicht.", "tokens": ["Lag", "dem", "Feind", "im", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch was stand in meiner Order:", "tokens": ["Doch", "was", "stand", "in", "mei\u00b7ner", "Or\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eschlagen, Feldherr, sollst du nicht!\u201c", "tokens": ["\u201e", "schla\u00b7gen", ",", "Feld\u00b7herr", ",", "sollst", "du", "nicht", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4tt' ich dich ins Meer geschleudert,", "tokens": ["H\u00e4tt'", "ich", "dich", "ins", "Meer", "ge\u00b7schleu\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ungl\u00fccksorder, brav gek\u00e4mpft,", "tokens": ["Un\u00b7gl\u00fcck\u00b7sor\u00b7der", ",", "brav", "ge\u00b7k\u00e4mpft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stadt und Land h\u00e4tt' ich erobert,", "tokens": ["Stadt", "und", "Land", "h\u00e4tt'", "ich", "er\u00b7o\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spanien, deinen Stolz ged\u00e4mpft.", "tokens": ["Spa\u00b7ni\u00b7en", ",", "dei\u00b7nen", "Stolz", "ge\u00b7d\u00e4mpft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "F\u00fcrchten durft' ich nichts vom Feinde,", "tokens": ["F\u00fcrch\u00b7ten", "durft'", "ich", "nichts", "vom", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den die Feigheit schon bezwang.", "tokens": ["Den", "die", "Feig\u00b7heit", "schon", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwanzigen w\u00e4r' leicht gelungen,", "tokens": ["Zwan\u00b7zi\u00b7gen", "w\u00e4r'", "leicht", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Was selbstsechstem dir gelang.", "tokens": ["Was", "selbst\u00b7sechs\u00b7tem", "dir", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimmer h\u00e4tte diese K\u00fcste", "tokens": ["Nim\u00b7mer", "h\u00e4t\u00b7te", "die\u00b7se", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Schande dann geschaut.", "tokens": ["Uns\u00b7re", "Schan\u00b7de", "dann", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer w\u00e4ren unsre Leiber", "tokens": ["Nim\u00b7mer", "w\u00e4\u00b7ren", "uns\u00b7re", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann dem \u00f6den Meer vertraut.", "tokens": ["Dann", "dem", "\u00f6\u00b7den", "Meer", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Spaniens stolze Gallionen", "tokens": ["Spa\u00b7ni\u00b7ens", "stol\u00b7ze", "Gal\u00b7li\u00b7o\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4tt' ich glorreich heimgef\u00fchrt,", "tokens": ["H\u00e4tt'", "ich", "glor\u00b7reich", "heim\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte dann als Hochverr\u00e4ther", "tokens": ["H\u00e4t\u00b7te", "dann", "als", "Hoch\u00b7ver\u00b7r\u00e4\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Willig das Schaffott verziert.", "tokens": ["Wil\u00b7lig", "das", "Schaf\u00b7fott", "ver\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Besser mit dem Ruhm gestorben:", "tokens": ["Bes\u00b7ser", "mit", "dem", "Ruhm", "ge\u00b7stor\u00b7ben", ":"], "token_i