{"textgrid.poem.50691": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Witz ohne Einsicht", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geb' es gerne zu, ", "tokens": ["Ich", "geb'", "es", "ger\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch schreibst du, wenn du schreibst, als w\u00e4r'st du nicht recht klug;", "tokens": ["Doch", "schreibst", "du", ",", "wenn", "du", "schreibst", ",", "als", "w\u00e4r'st", "du", "nicht", "recht", "klug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein scharf- und spitzer Kiel verletzet den Verstand", "tokens": ["Dein", "scha\u00b7rf", "und", "spit\u00b7zer", "Kiel", "ver\u00b7let\u00b7zet", "den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ist ein blankes Schwert in eines Tollen Hand;", "tokens": ["Und", "ist", "ein", "blan\u00b7kes", "Schwert", "in", "ei\u00b7nes", "Tol\u00b7len", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du schreibst, was sinnreich ist, doch was sich nicht geziemt,", "tokens": ["Du", "schreibst", ",", "was", "sinn\u00b7reich", "ist", ",", "doch", "was", "sich", "nicht", "ge\u00b7ziemt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KON", "PWS", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deine Thorheit wird durch deinen Witz ber\u00fchmt.", "tokens": ["Und", "dei\u00b7ne", "Thor\u00b7heit", "wird", "durch", "dei\u00b7nen", "Witz", "be\u00b7r\u00fchmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich geb' es gerne zu, ", "tokens": ["Ich", "geb'", "es", "ger\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch schreibst du, wenn du schreibst, als w\u00e4r'st du nicht recht klug;", "tokens": ["Doch", "schreibst", "du", ",", "wenn", "du", "schreibst", ",", "als", "w\u00e4r'st", "du", "nicht", "recht", "klug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein scharf- und spitzer Kiel verletzet den Verstand", "tokens": ["Dein", "scha\u00b7rf", "und", "spit\u00b7zer", "Kiel", "ver\u00b7let\u00b7zet", "den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", 