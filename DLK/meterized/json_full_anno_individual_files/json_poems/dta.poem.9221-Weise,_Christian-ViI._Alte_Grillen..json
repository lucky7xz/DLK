{"dta.poem.9221": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Alte Grillen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Auf ihr stoppeln meiner liebe!", "tokens": ["Auf", "ihr", "stop\u00b7peln", "mei\u00b7ner", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denckt einmahl an jene zeit/", "tokens": ["Denckt", "ein\u00b7mahl", "an", "je\u00b7ne", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich meine dienstbarkeit", "tokens": ["Als", "ich", "mei\u00b7ne", "dienst\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer stillen gunst verschriebe/", "tokens": ["Ei\u00b7ner", "stil\u00b7len", "gunst", "ver\u00b7schrie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein hertz ein s\u00fcsses band", "tokens": ["Und", "mein", "hertz", "ein", "s\u00fcs\u00b7ses", "band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An der liebsten hertzen fand.", "tokens": ["An", "der", "liebs\u00b7ten", "hert\u00b7zen", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. War ich nicht dem losen kinde", "tokens": ["War", "ich", "nicht", "dem", "lo\u00b7sen", "kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleichsam auf den hals gepicht?", "tokens": ["Gleich\u00b7sam", "auf", "den", "hals", "ge\u00b7picht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche stunde fragt ich nicht/", "tokens": ["Wel\u00b7che", "stun\u00b7de", "fragt", "ich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo und wie es um sie st\u00fcnde?", "tokens": ["Wo", "und", "wie", "es", "um", "sie", "st\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch ein blickgen schien ein jahr/", "tokens": ["Auch", "ein", "blick\u00b7gen", "schien", "ein", "jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "ART", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wann sie nicht zugegen war.", "tokens": ["Wann", "sie", "nicht", "zu\u00b7ge\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich entschlug mich aller freude/", "tokens": ["Ich", "ent\u00b7schlug", "mich", "al\u00b7ler", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die gesellschafft war mein tod/", "tokens": ["Die", "ge\u00b7sell\u00b7schafft", "war", "mein", "tod", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mir neue lust gebot/", "tokens": ["Was", "mir", "neu\u00b7e", "lust", "ge\u00b7bot", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zehlt ich unter meine feinde:", "tokens": ["Zehlt", "ich", "un\u00b7ter", "mei\u00b7ne", "fein\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es solt in ihr allein", "tokens": ["Denn", "es", "solt", "in", "ihr", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine freundschafft fruchtbar seyn.", "tokens": ["Mei\u00b7ne", "freund\u00b7schafft", "frucht\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Sie vergn\u00fcgte mein verlangen/", "tokens": ["Sie", "ver\u00b7gn\u00fcg\u00b7te", "mein", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhre sch\u00f6nheit war mein ruhm/", "tokens": ["Ih\u00b7re", "sch\u00f6n\u00b7heit", "war", "mein", "ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhre gunst mein eigenthum/", "tokens": ["Ih\u00b7re", "gunst", "mein", "ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die rosen ihrer wangen/", "tokens": ["Und", "die", "ro\u00b7sen", "ih\u00b7rer", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sie mir zu eigen gab/", "tokens": ["Die", "sie", "mir", "zu", "ei\u00b7gen", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKA", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brach ich unverhindert ab.", "tokens": ["Brach", "ich", "un\u00b7ver\u00b7hin\u00b7dert", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ach/ wie hoch wahr ich gestiegen/", "tokens": ["Ach", "/", "wie", "hoch", "wahr", "ich", "ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "PWAV", "ADJD", "ADJD", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Venus schien mir voll:", "tokens": ["Mei\u00b7ne", "Ve\u00b7nus", "schien", "mir", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch/ nach dem ich fallen soll/", "tokens": ["Doch", "/", "nach", "dem", "ich", "fal\u00b7len", "soll", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ich desto lieber liegen:", "tokens": ["Werd", "ich", "des\u00b7to", "lie\u00b7ber", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn des gl\u00fcckes wechsel-stand", "tokens": ["Denn", "des", "gl\u00fc\u00b7ckes", "wech\u00b7sel\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat sich schleunig umgewand.", "tokens": ["Hat", "sich", "schleu\u00b7nig", "um\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Nun/ ich mu\u00df mich drein ergeben:", "tokens": ["Nun", "/", "ich", "mu\u00df", "mich", "drein", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich kan doch nicht daf\u00fcr/", "tokens": ["Denn", "ich", "kan", "doch", "nicht", "da\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich armer/ sol ich hier", "tokens": ["Und", "ich", "ar\u00b7mer", "/", "sol", "ich", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "$(", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine freude mehr erleben/", "tokens": ["Kei\u00b7ne", "freu\u00b7de", "mehr", "er\u00b7le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So verliehr ich doch das liecht", "tokens": ["So", "ver\u00b7liehr", "ich", "doch", "das", "liecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer andern hoffnung nicht.", "tokens": ["Ei\u00b7ner", "an\u00b7dern", "hoff\u00b7nung", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Unterdessen will ich dencken/", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "will", "ich", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich nicht besitzen kan/", "tokens": ["Was", "ich", "nicht", "be\u00b7sit\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die lust der alten bahn", "tokens": ["Und", "die", "lust", "der", "al\u00b7ten", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Mir in mein ged\u00e4chn\u00fc\u00df schencken/", "tokens": ["Mir", "in", "mein", "ge\u00b7d\u00e4ch\u00b7n\u00fc\u00df", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich gleich in meinem sinn", "tokens": ["Ob", "ich", "gleich", "in", "mei\u00b7nem", "sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein armer jude bin.", "tokens": ["Nur", "ein", "ar\u00b7mer", "ju\u00b7de", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}