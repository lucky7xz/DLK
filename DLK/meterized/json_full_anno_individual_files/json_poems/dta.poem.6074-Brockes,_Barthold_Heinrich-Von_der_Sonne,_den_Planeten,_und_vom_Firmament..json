{"dta.poem.6074": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zuweilen wird man G\u00e4ng' am Firmament gewahr", "tokens": ["Zu\u00b7wei\u00b7len", "wird", "man", "G\u00e4ng'", "am", "Fir\u00b7ma\u00b7ment", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von C\u00f6rpern, welche wir nicht kennen,", "tokens": ["Von", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "wir", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die bald ein funckelnd Schwerdt, und bald ein brennend Haar", "tokens": ["Die", "bald", "ein", "fun\u00b7ckelnd", "Schwerdt", ",", "und", "bald", "ein", "bren\u00b7nend", "Haar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJD", "NN", "$,", "KON", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns zeigen, welche wir mit allem Rechte nennen", "tokens": ["Uns", "zei\u00b7gen", ",", "wel\u00b7che", "wir", "mit", "al\u00b7lem", "Rech\u00b7te", "nen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PPER", "APPR", "PIS", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Planeten, so verr\u00fcckt, und die vermuhtlich wandern", "tokens": ["Pla\u00b7ne\u00b7ten", ",", "so", "ver\u00b7r\u00fcckt", ",", "und", "die", "ver\u00b7muht\u00b7lich", "wan\u00b7dern"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD", "$,", "KON", "ART", "ADJD", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Aus einem Wirbel in den andern.", "tokens": ["Aus", "ei\u00b7nem", "Wir\u00b7bel", "in", "den", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}