{"textgrid.poem.26305": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Unter dem gro\u00dfen Waringienbaum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter dem gro\u00dfen Waringienbaum,", "tokens": ["Un\u00b7ter", "dem", "gro\u00b7\u00dfen", "Wa\u00b7rin\u00b7gien\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der da tr\u00e4gt den n\u00e4chtlichen Raum,", "tokens": ["Der", "da", "tr\u00e4gt", "den", "n\u00e4cht\u00b7li\u00b7chen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sitze ich bei den blo\u00dfen Sternen", "tokens": ["Sit\u00b7ze", "ich", "bei", "den", "blo\u00b7\u00dfen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie unter kleinen blauen Laternen,", "tokens": ["Wie", "un\u00b7ter", "klei\u00b7nen", "blau\u00b7en", "La\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die ihre Gedanken haben, still,", "tokens": ["Die", "ih\u00b7re", "Ge\u00b7dan\u00b7ken", "ha\u00b7ben", ",", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00dcber das, was ein jeder will.", "tokens": ["\u00dc\u00b7ber", "das", ",", "was", "ein", "je\u00b7der", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ART", "PIS", "VMFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "In der lampenhellen Moschee", "tokens": ["In", "der", "lam\u00b7pen\u00b7hel\u00b7len", "Mo\u00b7schee"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Stehen die S\u00e4ulen, gebaut wie aus Schnee,", "tokens": ["Ste\u00b7hen", "die", "S\u00e4u\u00b7len", ",", "ge\u00b7baut", "wie", "aus", "Schnee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVPP", "KOKOM", "APPR", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nicht weit von des Baumes finsternden Zweigen.", "tokens": ["Nicht", "weit", "von", "des", "Bau\u00b7mes", "fins\u00b7tern\u00b7den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Vorbeter singt \u00fcber die R\u00fccken, die sich dort neigen.", "tokens": ["Der", "Vor\u00b7be\u00b7ter", "singt", "\u00fc\u00b7ber", "die", "R\u00fc\u00b7cken", ",", "die", "sich", "dort", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+--++--+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ruft ein Vogel im Waringienge\u00e4ste.", "tokens": ["Es", "ruft", "ein", "Vo\u00b7gel", "im", "