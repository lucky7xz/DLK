{"dta.poem.20831": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "da:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der in der s\u00fcssen Stille lebet/", "tokens": ["Der", "in", "der", "s\u00fcs\u00b7sen", "Stil\u00b7le", "le\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und blo\u00df aus Lust der Wissenschafft/", "tokens": ["Und", "blo\u00df", "aus", "Lust", "der", "Wis\u00b7sen\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in die K\u00fcnste tieff vergr\u00e4bet/", "tokens": ["Sich", "in", "die", "K\u00fcns\u00b7te", "tieff", "ver\u00b7gr\u00e4\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint ja dem Tode stets verhafft/", "tokens": ["Scheint", "ja", "dem", "To\u00b7de", "stets", "ver\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann er der Wollust unergeben/", "tokens": ["Wann", "er", "der", "Wol\u00b7lust", "un\u00b7er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Blo\u00df wil in den Gedancken schweben", "tokens": ["Blo\u00df", "wil", "in", "den", "Ge\u00b7dan\u00b7cken", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dahin ihm sein Verstand ger\u00fcckt;", "tokens": ["Da\u00b7hin", "ihm", "sein", "Ver\u00b7stand", "ge\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So lebt der Todt ihm in dem Hertzen/", "tokens": ["So", "lebt", "der", "Todt", "ihm", "in", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er dencket wie er ist bestrickt/", "tokens": ["Er", "den\u00b7cket", "wie", "er", "ist", "be\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schaut Stein/ Feuer/ Schwerd und Kertzen/", "tokens": ["Und", "schaut", "Stein", "/", "Feu\u00b7er", "/", "Schwerd", "und", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Wollust acht er wie die Noth/", "tokens": ["Die", "Wol\u00b7lust", "acht", "er", "wie", "die", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "PPER", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und wann ihn endlich trifft der Todt/", "tokens": ["Und", "wann", "ihn", "end\u00b7lich", "trifft", "der", "Todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So ist es ihm kein neuer Schmertzen.", "tokens": ["So", "ist", "es", "ihm", "kein", "neu\u00b7er", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}