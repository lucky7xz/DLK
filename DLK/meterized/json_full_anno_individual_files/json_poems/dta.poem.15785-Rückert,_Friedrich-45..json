{"dta.poem.15785": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar viel Personen sind beisammen im Poeten,", "tokens": ["Gar", "viel", "Per\u00b7so\u00b7nen", "sind", "bei\u00b7sam\u00b7men", "im", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die auf die B\u00fchne mit und nach einander treten,", "tokens": ["Die", "auf", "die", "B\u00fch\u00b7ne", "mit", "und", "nach", "ein\u00b7an\u00b7der", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "KON", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sich widersprechen, sich erg\u00e4nzen, sich erkl\u00e4ren,", "tokens": ["Sich", "wi\u00b7der\u00b7spre\u00b7chen", ",", "sich", "er\u00b7g\u00e4n\u00b7zen", ",", "sich", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "PRF", "VVINF", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Eine sind und thun alsob sie viele w\u00e4ren.", "tokens": ["Doch", "Ei\u00b7ne", "sind", "und", "thun", "al\u00b7sob", "sie", "vie\u00b7le", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "KON", "VVINF", "VVFIN", "PPER", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum so viele? da\u00df, wofern nicht allen alle,", "tokens": ["Wa\u00b7rum", "so", "vie\u00b7le", "?", "da\u00df", ",", "wo\u00b7fern", "nicht", "al\u00b7len", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "$.", "KOUS", "$,", "KOUS", "PTKNEG", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch diese dem von euch, und jenem die gefalle.", "tokens": ["Doch", "die\u00b7se", "dem", "von", "euch", ",", "und", "je\u00b7nem", "die", "ge\u00b7fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "APPR", "PPER", "$,", "KON", "PDAT", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}