{"dta.poem.8357": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das vergebliche W\u00fcnschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschen Torheit wird man offter-\nmahls gewahr,", "tokens": ["Der", "Men\u00b7schen", "Tor\u00b7heit", "wird", "man", "off\u00b7ter", "mahls", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PIS", "TRUNC", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man ihr W\u00fcnschen h\u00f6rt: denn", "tokens": ["Wenn", "man", "ihr", "W\u00fcn\u00b7schen", "h\u00f6rt", ":", "denn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$.", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df wir in Eitelkeit dasselbige verlangen,", "tokens": ["Da\u00df", "wir", "in", "Ei\u00b7tel\u00b7keit", "das\u00b7sel\u00b7bi\u00b7ge", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Woran der Seelen Trieb und inre Neigung han-", "tokens": ["Wo\u00b7ran", "der", "See\u00b7len", "Trieb", "und", "in\u00b7re", "Nei\u00b7gung", "han"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der eine w\u00fcnschet sich den Abgott dieser Welt,", "tokens": ["Der", "ei\u00b7ne", "w\u00fcn\u00b7schet", "sich", "den", "Ab\u00b7gott", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das blizende Metal, den g\u00fcldnen Staub, nur", "tokens": ["Das", "bli\u00b7zen\u00b7de", "Me\u00b7tal", ",", "den", "g\u00fcld\u00b7nen", "Staub", ",", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jhm deucht da\u00df er begl\u00fckt wenn er sich an den Sch\u00e4-", "tokens": ["Jhm", "deucht", "da\u00df", "er", "be\u00b7gl\u00fckt", "wenn", "er", "sich", "an", "den", "Sch\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "KOUS", "PPER", "PRF", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des klingenden Gespiels nur k\u00f6nte stets erg\u00f6zen;", "tokens": ["Des", "klin\u00b7gen\u00b7den", "Ge\u00b7spiels", "nur", "k\u00f6n\u00b7te", "stets", "er\u00b7g\u00f6\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der andre sieht das Geld, als einen Drekklump", "tokens": ["Der", "and\u00b7re", "sieht", "das", "Geld", ",", "als", "ei\u00b7nen", "Drek\u00b7klump"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Der keinen Mensch begl\u00fckt, vergn\u00fcget machen", "tokens": ["Der", "kei\u00b7nen", "Mensch", "be\u00b7gl\u00fckt", ",", "ver\u00b7gn\u00fc\u00b7get", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Er lacht den Geizhals aus, und denkt auf ein Ver-", "tokens": ["Er", "lacht", "den", "Geiz\u00b7hals", "aus", ",", "und", "denkt", "auf", "ein", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ach! spricht sein sehnend Herz k\u00f6nt ich dasselbe krie-", "tokens": ["Ach", "!", "spricht", "sein", "seh\u00b7nend", "Herz", "k\u00f6nt", "ich", "das\u00b7sel\u00b7be", "krie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So w\u00e4r ich recht begl\u00fckt; was w\u00fcnschet die Be-", "tokens": ["So", "w\u00e4r", "ich", "recht", "be\u00b7gl\u00fckt", ";", "was", "w\u00fcn\u00b7schet", "die", "Be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "PWS", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den Himmel auf der Welt, den zieht er allen", "tokens": ["Den", "Him\u00b7mel", "auf", "der", "Welt", ",", "den", "zieht", "er", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Wunsch der christlich klingt, ist nicht so zu", "tokens": ["Der", "Wunsch", "der", "christ\u00b7lich", "klingt", ",", "ist", "nicht", "so", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Er w\u00fcnscht das Paradies der T\u00fcrken nur zu se-", "tokens": ["Er", "w\u00fcnscht", "das", "Pa\u00b7ra\u00b7dies", "der", "T\u00fcr\u00b7ken", "nur", "zu", "se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.17": {"text": "Er liebt die faule Ruh, und eine Lagerstatt,", "tokens": ["Er", "liebt", "die", "fau\u00b7le", "Ruh", ",", "und", "ei\u00b7ne", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo sein Geschmak nur das, was er verlanget", "tokens": ["Wo", "sein", "Ge\u00b7schmak", "nur", "das", ",", "was", "er", "ver\u00b7lan\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "PDS", "$,", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der andre w\u00fcnschet sich noch ein viel anders Gl\u00fck-", "tokens": ["Der", "and\u00b7re", "w\u00fcn\u00b7schet", "sich", "noch", "ein", "viel", "an\u00b7ders", "Gl\u00fc\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ADV", "ART", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und was denn? Bei dem Brodt nur eitle Liebes", "tokens": ["Und", "was", "denn", "?", "Bei", "dem", "Brodt", "nur", "eit\u00b7le", "Lie\u00b7bes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "$.", "APPR", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Von der die ihm gef\u00e4llt. Der dritte schnappt nach", "tokens": ["Von", "der", "die", "ihm", "ge\u00b7f\u00e4llt", ".", "Der", "drit\u00b7te", "schnappt", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PRELS", "PPER", "VVPP", "$.", "ART", "ADJA", "VVFIN", "APPR"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Nach einer eitlen Ehr, nach des Gem\u00fchtes", "tokens": ["Nach", "ei\u00b7ner", "eit\u00b7len", "Ehr", ",", "nach", "des", "Ge\u00b7m\u00fch\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Er glaubt das w\u00e4r sein Ziel, k\u00f6nt er es nur erlan-", "tokens": ["Er", "glaubt", "das", "w\u00e4r", "sein", "Ziel", ",", "k\u00f6nt", "er", "es", "nur", "er\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So k\u00f6nt er in der Welt in hellen Glanze pran-", "tokens": ["So", "k\u00f6nt", "er", "in", "der", "Welt", "in", "hel\u00b7len", "Glan\u00b7ze", "pran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der vierte hat ein Gut darnach die Sehnsucht", "tokens": ["Der", "vier\u00b7te", "hat", "ein", "Gut", "dar\u00b7nach", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Er w\u00fcnscht, dieweil sein Herz auch an der Erde", "tokens": ["Er", "w\u00fcnscht", ",", "die\u00b7weil", "sein", "Herz", "auch", "an", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Nach blinder Maulwurfs Art, nur stets herum zu", "tokens": ["Nach", "blin\u00b7der", "Maul\u00b7wurfs", "Art", ",", "nur", "stets", "he\u00b7rum", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ADV", "ADV", "APZR", "PTKZU"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "Und auf den Erdenball mit leeren Nichts zu spie-", "tokens": ["Und", "auf", "den", "Er\u00b7den\u00b7ball", "mit", "lee\u00b7ren", "Nichts", "zu", "spie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der blosse Grillen liebt, und sich daran behagt,", "tokens": ["Der", "blos\u00b7se", "Gril\u00b7len", "liebt", ",", "und", "sich", "da\u00b7ran", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn er gleich einen Wurm, an alten B\u00fcchern", "tokens": ["Wenn", "er", "gleich", "ei\u00b7nen", "Wurm", ",", "an", "al\u00b7ten", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Der w\u00fcnschet nichtes mehr, als nur den Schaz zu", "tokens": ["Der", "w\u00fcn\u00b7schet", "nich\u00b7tes", "mehr", ",", "als", "nur", "den", "Schaz", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Den jene graue Welt in ihren Schut vergraben.", "tokens": ["Den", "je\u00b7ne", "grau\u00b7e", "Welt", "in", "ih\u00b7ren", "Schut", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dem Wissenschafft gef\u00e4llt, der strebet nur allein,", "tokens": ["Dem", "Wis\u00b7sen\u00b7schafft", "ge\u00b7f\u00e4llt", ",", "der", "stre\u00b7bet", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df er Besizer k\u00f6nn von allen B\u00fcchern seyn,", "tokens": ["Da\u00df", "er", "Be\u00b7si\u00b7zer", "k\u00f6nn", "von", "al\u00b7len", "B\u00fc\u00b7chern", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Die jemahls ausgehekt; Er meint er sey genesen,", "tokens": ["Die", "je\u00b7mahls", "aus\u00b7ge\u00b7hekt", ";", "Er", "meint", "er", "sey", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn er mehr Vorrath hat, als er wird k\u00f6nnen", "tokens": ["Wenn", "er", "mehr", "Vor\u00b7rath", "hat", ",", "als", "er", "wird", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Der Menschen Wunsch also und sein begierger", "tokens": ["Der", "Men\u00b7schen", "Wunsch", "al\u00b7so", "und", "sein", "be\u00b7gier\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Der richtet sich auch stets nach solchen Ziele hin,", "tokens": ["Der", "rich\u00b7tet", "sich", "auch", "stets", "nach", "sol\u00b7chen", "Zie\u00b7le", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wohin die Neigung geht: und wenn man eitel denket;", "tokens": ["Wo\u00b7hin", "die", "Nei\u00b7gung", "geht", ":", "und", "wenn", "man", "ei\u00b7tel", "den\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$.", "KON", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So wird die Neigung auch zum eitlen Zwek gelenket.", "tokens": ["So", "wird", "die", "Nei\u00b7gung", "auch", "zum", "eit\u00b7len", "Zwek", "ge\u00b7len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer das erhalten hat, wornach er sich gesehnt,", "tokens": ["Wer", "das", "er\u00b7hal\u00b7ten", "hat", ",", "wor\u00b7nach", "er", "sich", "ge\u00b7sehnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVPP", "VAFIN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der findet allemahl, wie sich sein Wunsch aus-", "tokens": ["Der", "fin\u00b7det", "al\u00b7le\u00b7mahl", ",", "wie", "sich", "sein", "Wunsch", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWAV", "PRF", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Wer hundert Thaler nur vorhero haben wollen,", "tokens": ["Wer", "hun\u00b7dert", "Tha\u00b7ler", "nur", "vor\u00b7he\u00b7ro", "ha\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "ADV", "ADV", "VAINF", "VMFIN", "$,"], "me