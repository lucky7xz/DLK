{"textgrid.poem.41801": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast im Kerker nicht den Mut verloren,", "tokens": ["Du", "hast", "im", "Ker\u00b7ker", "nicht", "den", "Mut", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wu\u00dftest es, da\u00df Gott mit Dir geblieben,", "tokens": ["Du", "wu\u00df\u00b7test", "es", ",", "da\u00df", "Gott", "mit", "Dir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir und der Freiheit galt Dein freudig Lieben,", "tokens": ["Mir", "und", "der", "Frei\u00b7heit", "galt", "Dein", "freu\u00b7dig", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der V\u00f6lkerfreiheit, der Du Dich verschworen.", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7frei\u00b7heit", ",", "der", "Du", "Dich", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir f\u00fchlten f\u00fcr einander uns geboren", "tokens": ["Wir", "f\u00fchl\u00b7ten", "f\u00fcr", "ein\u00b7an\u00b7der", "uns", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hatten uns der Ewigkeit verschrieben \u2013", "tokens": ["Und", "hat\u00b7ten", "uns", "der", "E\u00b7wig\u00b7keit", "ver\u00b7schrie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich hat kein Kerker, hat kein Grab vertrieben,", "tokens": ["Mich", "hat", "kein", "Ker\u00b7ker", ",", "hat", "kein", "Grab", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steh' ich auch weinend jetzt an seinen Thoren.", "tokens": ["Steh'", "ich", "auch", "wei\u00b7nend", "jetzt", "an", "sei\u00b7nen", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit Schwert und Leyer standest Du im Leben,", "tokens": ["Mit", "Schwert", "und", "Le\u00b7yer", "stan\u00b7dest", "Du", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Dienst der Freiheit, bist in ihm gestorben", "tokens": ["Im", "Dienst", "der", "Frei\u00b7heit", ",", "bist", "in", "ihm", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "wor