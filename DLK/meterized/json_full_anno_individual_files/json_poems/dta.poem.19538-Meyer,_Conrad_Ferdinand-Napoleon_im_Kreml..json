{"dta.poem.19538": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Napoleon im Kreml.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Er nickt mit seinem gro\u00dfen Haupt", "tokens": ["Er", "nickt", "mit", "sei\u00b7nem", "gro\u00b7\u00dfen", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Feuer eines fremden Herds:", "tokens": ["Am", "Feu\u00b7er", "ei\u00b7nes", "frem\u00b7den", "Herds", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Traum erblickt er einen Geist,", "tokens": ["Im", "Traum", "er\u00b7blickt", "er", "ei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seines Purpurs Spange l\u00f6st.", "tokens": ["Der", "sei\u00b7nes", "Pur\u00b7purs", "Span\u00b7ge", "l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der D\u00e4mon schreit mit wilder Gier:", "tokens": ["Der", "D\u00e4\u00b7mon", "schreit", "mit", "wil\u00b7der", "Gier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emich l\u00fcstet nach dem rothen Kleid!", "tokens": ["\u201e", "mich", "l\u00fcs\u00b7tet", "nach", "dem", "ro\u00b7then", "Kleid", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ungez\u00e4hlter Menschen Blut", "tokens": ["In", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Men\u00b7schen", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Getaucht, verf\u00e4rbt der Purpur nicht!\u201c", "tokens": ["Ge\u00b7taucht", ",", "ver\u00b7f\u00e4rbt", "der", "Pur\u00b7pur", "nicht", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Beiden rangen Leib an Leib.", "tokens": ["Die", "Bei\u00b7den", "ran\u00b7gen", "Leib", "an", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egieb her!\u201c \u201eGieb her!\u201c Der D\u00e4mon fleucht", "tokens": ["\u201e", "gieb", "her", "!", "\u201c", "\u201e", "Gieb", "her", "!", "\u201c", "Der", "D\u00e4\u00b7mon", "fleucht"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "PTKVZ", "$.", "$(", "$(", "VVIMP", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit spitzen Fl\u00fcgeln durch die Nacht", "tokens": ["Mit", "spit\u00b7zen", "Fl\u00fc\u00b7geln", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schleift den Purpur hinter sich.", "tokens": ["Und", "schleift", "den", "Pur\u00b7pur", "hin\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wo der Purpur flatternd fliegt,", "tokens": ["Und", "wo", "der", "Pur\u00b7pur", "flat\u00b7ternd", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spr\u00fchn Funken, lodern Flammen auf!", "tokens": ["Spr\u00fchn", "Fun\u00b7ken", ",", "lo\u00b7dern", "Flam\u00b7men", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Corse f\u00e4hrt aus seinem Traum", "tokens": ["Der", "Cor\u00b7se", "f\u00e4hrt", "aus", "sei\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und starrt in Moskau's weiten Brand.", "tokens": ["Und", "starrt", "in", "Mos\u00b7kau's", "wei\u00b7ten", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}