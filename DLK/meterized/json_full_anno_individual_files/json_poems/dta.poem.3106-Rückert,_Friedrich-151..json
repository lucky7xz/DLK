{"dta.poem.3106": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "151.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe klar genug, was ich zu sehen brauche:", "tokens": ["Ich", "se\u00b7he", "klar", "ge\u00b7nug", ",", "was", "ich", "zu", "se\u00b7hen", "brau\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganze Sch\u00f6pfung lebt von Gottes Lebenshauche.", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "lebt", "von", "Got\u00b7tes", "Le\u00b7bens\u00b7hau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sie den Hauch empfing, das ist von Nacht umhangen,", "tokens": ["Wie", "sie", "den", "Hauch", "emp\u00b7fing", ",", "das", "ist", "von", "Nacht", "um\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir aber preisen Gott, da\u00df sie den Hauch empfangen.", "tokens": ["Wir", "a\u00b7ber", "prei\u00b7sen", "Gott", ",", "da\u00df", "sie", "den", "Hauch", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hauchen wir, ich und du, uns unserm Urhauch zu!", "tokens": ["Hau\u00b7chen", "wir", ",", "ich", "und", "du", ",", "uns", "un\u00b7serm", "Ur\u00b7hauch", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "KON", "PPER", "$,", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Zur Ruh der Seligkeit f\u00fchrt ew'ger Lieb' Unruh.", "tokens": ["Zur", "Ruh", "der", "Se\u00b7lig\u00b7keit", "f\u00fchrt", "ew'\u00b7ger", "Lieb'", "Un\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}