{"dta.poem.9943": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ich selbst schon nicht viel gelte/ gilt doch/ was mich machet/", "tokens": ["Ob", "ich", "selbst", "schon", "nicht", "viel", "gel\u00b7te", "/", "gilt", "doch", "/", "was", "mich", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "VVFIN", "$(", "VVFIN", "ADV", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Keiner der kan mich erlangen/ der nicht feuer machen will.", "tokens": ["Kei\u00b7ner", "der", "kan", "mich", "er\u00b7lan\u00b7gen", "/", "der", "nicht", "feu\u00b7er", "ma\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "VMFIN", "PRF", "VVINF", "$(", "ART", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Feuer hilfft zu meinem wesen/ gibt mir aber nichts von sich.", "tokens": ["Feu\u00b7er", "hilfft", "zu", "mei\u00b7nem", "we\u00b7sen", "/", "gibt", "mir", "a\u00b7ber", "nichts", "von", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "ADV", "PIS", "APPR", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Lufft bringt mir den gr\u00f6sten schaden un\u0303 der wind verjaget mich.", "tokens": ["Lufft", "bringt", "mir", "den", "gr\u00f6s\u00b7ten", "scha\u00b7den", "u\u00f1", "der", "wind", "ver\u00b7ja\u00b7get", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "++--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}