{"dta.poem.20777": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An ein gl\u00fcckliches Volk.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aleides stritt mit L\u00f6w und Hyder;               ", "tokens": ["A\u00b7lei\u00b7des", "stritt", "mit", "L\u00f6w", "und", "Hy\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und matt von K\u00e4mpfen ward Achill;", "tokens": ["Und", "matt", "von", "K\u00e4mp\u00b7fen", "ward", "A\u00b7chill", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ein Monarche wird noch m\u00fcder,", "tokens": ["Doch", "ein", "Mon\u00b7ar\u00b7che", "wird", "noch", "m\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir, zu muthig Volk, Gesetze geben will.", "tokens": ["Der", "dir", ",", "zu", "mut\u00b7hig", "Volk", ",", "Ge\u00b7set\u00b7ze", "ge\u00b7ben", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PTKA", "ADJD", "NN", "$,", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der allerschwerste Kampf ist leichter auszuf\u00fchren.", "tokens": ["Der", "al\u00b7ler\u00b7schwers\u00b7te", "Kampf", "ist", "leich\u00b7ter", "aus\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als \u00fcber Gl\u00fcckliche gebietend Herrscher seyn:", "tokens": ["Als", "\u00fc\u00b7ber", "Gl\u00fcck\u00b7li\u00b7che", "ge\u00b7bie\u00b7tend", "Herr\u00b7scher", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVPP", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird nicht ein Gott in dir regieren,", "tokens": ["Wird", "nicht", "ein", "Gott", "in", "dir", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So ist des Scepters Macht zu klein.", "tokens": ["So", "ist", "des", "Scep\u00b7ters", "Macht", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}