{"textgrid.poem.52077": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht wundre dich, da\u00df man zu Tantz und Sprunge schl\u00e4gt,", "tokens": ["Nicht", "wund\u00b7re", "dich", ",", "da\u00df", "man", "zu", "Tantz", "und", "Sprun\u00b7ge", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df der V\u00e4ter Schaar, die graue Kutten tr\u00e4gt,", "tokens": ["Und", "da\u00df", "der", "V\u00e4\u00b7ter", "Schaar", ",", "die", "grau\u00b7e", "Kut\u00b7ten", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lustig sich erzeigt und in die Welt wil sehen,", "tokens": ["So", "lus\u00b7tig", "sich", "er\u00b7zeigt", "und", "in", "die", "Welt", "wil", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVPP", "KON", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil es nicht heute pflegt am ersten zu geschehen.", "tokens": ["Weil", "es", "nicht", "heu\u00b7te", "pflegt", "am", "ers\u00b7ten", "zu", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Wein ist hier nicht Wein, die Trachten keine Tracht,", "tokens": ["Der", "Wein", "ist", "hier", "nicht", "Wein", ",", "die", "Trach\u00b7ten", "kei\u00b7ne", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gedencke, da\u00df du auch ein Theil darzu gebracht:", "tokens": ["Ge\u00b7den\u00b7cke", ",", "da\u00df", "du", "auch", "ein", "Theil", "dar\u00b7zu", "ge\u00b7bracht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als zu dem Opffer sich ein ieder muste finden,", "tokens": ["Als", "zu", "dem", "Opf\u00b7fer", "sich", "ein", "ie\u00b7der", "mus\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Speisen und der Tranck, das sind der Leute S\u00fcnden.", "tokens": ["Die", "Spei\u00b7sen", "und", "der", "Tranck", ",", "das", "sind", "der", "Leu\u00b7te", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.