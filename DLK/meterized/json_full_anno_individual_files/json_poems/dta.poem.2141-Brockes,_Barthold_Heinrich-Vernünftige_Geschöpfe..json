{"dta.poem.2141": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vern\u00fcnftige Gesch\u00f6pfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So gro\u00df als wie du selbst, mu\u00df deine Liebe,", "tokens": ["So", "gro\u00df", "als", "wie", "du", "selbst", ",", "mu\u00df", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PWAV", "PPER", "ADV", "$,", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Wesen aller Wesen, seyn!", "tokens": ["O", "We\u00b7sen", "al\u00b7ler", "We\u00b7sen", ",", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem du Wesen, die so klein,", "tokens": ["In\u00b7dem", "du", "We\u00b7sen", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wunderkraft, auf dich zu denken,", "tokens": ["Die", "Wun\u00b7der\u00b7kraft", ",", "auf", "dich", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schon hier gew\u00fcrdiget zu schenken.", "tokens": ["Schon", "hier", "ge\u00b7w\u00fcr\u00b7di\u00b7get", "zu", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die\u00df Denken ist es ganz allein,", "tokens": ["Die\u00df", "Den\u00b7ken", "ist", "es", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch welches wir, vor allen Thieren,", "tokens": ["Durch", "wel\u00b7ches", "wir", ",", "vor", "al\u00b7len", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die nichts von diesem Vorzug sp\u00fcren,", "tokens": ["Die", "nichts", "von", "die\u00b7sem", "Vor\u00b7zug", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vern\u00fcnftige Gesch\u00f6pfe seyn.", "tokens": ["Ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7sch\u00f6p\u00b7fe", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}