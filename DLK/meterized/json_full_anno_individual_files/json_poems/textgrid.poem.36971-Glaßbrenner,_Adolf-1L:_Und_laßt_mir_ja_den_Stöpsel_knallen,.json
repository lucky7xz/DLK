{"textgrid.poem.36971": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und la\u00dft mir ja den St\u00f6psel knallen,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und la\u00dft mir ja den St\u00f6psel knallen,", "tokens": ["Und", "la\u00dft", "mir", "ja", "den", "St\u00f6p\u00b7sel", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihr der Erde Nektar trinkt!", "tokens": ["Wenn", "ihr", "der", "Er\u00b7de", "Nek\u00b7tar", "trinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie unser Jubel mu\u00df es schallen,", "tokens": ["Wie", "un\u00b7ser", "Ju\u00b7bel", "mu\u00df", "es", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn von dem Geist die Fessel springt.", "tokens": ["Wenn", "von", "dem", "Geist", "die", "Fes\u00b7sel", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Liebe Born, der Freiheit Born,", "tokens": ["Der", "Lie\u00b7be", "Born", ",", "der", "Frei\u00b7heit", "Born", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sch\u00e4umt wie unser edle Zorn!", "tokens": ["Er", "sch\u00e4umt", "wie", "un\u00b7ser", "ed\u00b7le", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Champagnerlust, Champagnersang!", "tokens": ["Cham\u00b7pag\u00b7ner\u00b7lust", ",", "Cham\u00b7pag\u00b7ner\u00b7sang", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Alten droben unsern Dank!", "tokens": ["Dem", "Al\u00b7ten", "dro\u00b7ben", "un\u00b7sern", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hinunter, rasch hinunter,", "tokens": ["Hin\u00b7un\u00b7ter", ",", "rasch", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das Herz wird schon so munter!", "tokens": ["Das", "Herz", "wird", "schon", "so", "mun\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Hinunter edles Musengold,", "tokens": ["Hin\u00b7un\u00b7ter", "ed\u00b7les", "Mu\u00b7sen\u00b7gold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hinunter s\u00fc\u00dfer S\u00e4ngersold,", "tokens": ["Hin\u00b7un\u00b7ter", "s\u00fc\u00b7\u00dfer", "S\u00e4n\u00b7ger\u00b7sold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So feurig, lieblich und so hold,", "tokens": ["So", "feu\u00b7rig", ",", "lieb\u00b7lich", "und", "so", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du Stern, wenn b\u00f6ser Donner rollt,", "tokens": ["Du", "Stern", ",", "wenn", "b\u00f6\u00b7ser", "Don\u00b7ner", "rollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und Alles, Alles, was ihr wollt!", "tokens": ["Und", "Al\u00b7les", ",", "Al\u00b7les", ",", "was", "ihr", "wollt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hinunter, hinunter!", "tokens": ["Hin\u00b7un\u00b7ter", ",", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Das Herz ist schon so munter.", "tokens": ["Das", "Herz", "ist", "schon", "so", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er zischt, wie unsere Feinde zischen,", "tokens": ["Er", "zischt", ",", "wie", "un\u00b7se\u00b7re", "Fein\u00b7de", "zi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn uns die freie G\u00f6ttin winkt,", "tokens": ["Wenn", "uns", "die", "frei\u00b7e", "G\u00f6t\u00b7tin", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus dem Quell, dem immerfrischen,", "tokens": ["Und", "aus", "dem", "Quell", ",", "dem", "im\u00b7mer\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begeisterung die Seele trinkt.", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "die", "See\u00b7le", "trinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O bl\u00fchend, bl\u00fchend Zauberreich!", "tokens": ["O", "bl\u00fc\u00b7hend", ",", "bl\u00fc\u00b7hend", "Zau\u00b7ber\u00b7reich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier ist der Mensch dem Menschen gleich!", "tokens": ["Hier", "ist", "der", "Mensch", "dem", "Men\u00b7schen", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Champagnerlust, Champagnersang!", "tokens": ["Cham\u00b7pag\u00b7ner\u00b7lust", ",", "Cham\u00b7pag\u00b7ner\u00b7sang", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir achten keinen Ruhm und Rang!", "tokens": ["Wir", "ach\u00b7ten", "kei\u00b7nen", "Ruhm", "und", "Rang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hinunter, rasch hinunter,", "tokens": ["Hin\u00b7un\u00b7ter", ",", "rasch", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Im Kopfe wird es bunter!", "tokens": ["Im", "Kop\u00b7fe", "wird", "es", "bun\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Hinunter edles Musengold,", "tokens": ["Hin\u00b7un\u00b7ter", "ed\u00b7les", "Mu\u00b7sen\u00b7gold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hinunter s\u00fc\u00dfer S\u00e4ngersold,", "tokens": ["Hin\u00b7un\u00b7ter", "s\u00fc\u00b7\u00dfer", "S\u00e4n\u00b7ger\u00b7sold", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So feurig, lieblich und so hold,", "tokens": ["So", "feu\u00b7rig", ",", "lieb\u00b7lich", "und", "so", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du Stern, wenn b\u00f6ser Donner rollt,", "tokens": ["Du", "Stern", ",", "wenn", "b\u00f6\u00b7ser", "Don\u00b7ner", "rollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und Alles, Alles, was ihr wollt!", "tokens": ["Und", "Al\u00b7les", ",", "Al\u00b7les", ",", "was", "ihr", "wollt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Hinunter, hinunter!", "tokens": ["Hin\u00b7un\u00b7ter", ",", "hin\u00b7un\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Im Kopf' wird's immer bunter.", "tokens": ["Im", "Kopf'", "wird's", "im\u00b7mer", "bun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sprengt eine neue Fessel wieder,", "tokens": ["Sprengt", "ei\u00b7ne", "neu\u00b7e", "Fes\u00b7sel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die h\u00f6chste Erdenlust!", "tokens": ["Das", "ist", "die", "h\u00f6chs\u00b7te", "Er\u00b7den\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es perlt der Wein wie s\u00fc\u00dfe Lieder", "tokens": ["Es", "perlt", "der", "Wein", "wie", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus unsrer wonnevollen Brust.", "tokens": ["Aus", "uns\u00b7rer", "won\u00b7ne\u00b7vol\u00b7len", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun giebt es keinen Herrscher mehr!", "tokens": ["Nun", "giebt", "es", "kei\u00b7nen", "Herr\u00b7scher", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun bl\u00fcht die weite Welt umher!", "tokens": ["Nun", "bl\u00fcht", "die", "wei\u00b7te", "Welt", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Champagnerlust, Champagnersang,", "tokens": ["Cham\u00b7pag\u00b7ner\u00b7lust", ",", "Cham\u00b7pag\u00b7ner\u00b7sang", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun sind wir Alle frei und frank!", "tokens": ["Nun", "sind", "wir", "Al\u00b7le", "frei", "und", "frank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hinunter, rasch hinunter,", "tokens": ["Hin\u00b7un\u00b7ter", ",", "rasch", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den h\u00f6chsten St\u00f6psel 'runter!", "tokens": ["Den", "h\u00f6chs\u00b7ten", "St\u00f6p\u00b7sel", "'r\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct