{"textgrid.poem.65839": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "113", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich dir fern bin, ist mein Aug' im ", "tokens": ["Seit", "ich", "dir", "fern", "bin", ",", "ist", "mein", "Aug'", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "APPRART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und jenes, das mich f\u00fchrt von Ort zu Ort,", "tokens": ["Und", "je\u00b7nes", ",", "das", "mich", "f\u00fchrt", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Teilt seine T\u00e4tigkeit; zum Teil ist's blind;", "tokens": ["Teilt", "sei\u00b7ne", "T\u00e4\u00b7tig\u00b7keit", ";", "zum", "Teil", "ist's", "blind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Scheint sehend, doch in Wahrheit ist's verdorrt.", "tokens": ["Scheint", "se\u00b7hend", ",", "doch", "in", "Wahr\u00b7heit", "ist's", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn keine Formen, keinen Widerschein", "tokens": ["Denn", "kei\u00b7ne", "For\u00b7men", ",", "kei\u00b7nen", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Blum' und Vogel, was sich zu ihm dr\u00e4ngt,", "tokens": ["Von", "Blum'", "und", "Vo\u00b7gel", ",", "was", "sich", "zu", "ihm", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nichts bringt sein schnelles Sehn dem Herzen ein,", "tokens": ["Nichts", "bringt", "sein", "schnel\u00b7les", "Sehn", "dem", "Her\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ja seine Sehkraft h\u00e4lt nicht, was sie f\u00e4ngt.", "tokens": ["Ja", "sei\u00b7ne", "Seh\u00b7kraft", "h\u00e4lt", "nicht", ",", "was", "sie", "f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn sch\u00f6n und h\u00e4\u00dflich, was es schauen mag,", "tokens": ["Denn", "sch\u00f6n", "und", "h\u00e4\u00df\u00b7lich", ",", "was", "es", "schau\u00b7en", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Unf\u00f6rmlichkeit, wie s\u00fc\u00dfestes Vergn\u00fcgen,", "tokens": ["Un\u00b7f\u00f6rm\u00