, "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja, das Leben w\u00e4re Wahn,", "tokens": ["Ja", ",", "das", "Le\u00b7ben", "w\u00e4\u00b7re", "Wahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn es nicht des Himmels Bahn.", "tokens": ["Wenn", "es", "nicht", "des", "Him\u00b7mels", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sagt, was ist hier Lieben?", "tokens": ["Sagt", ",", "was", "ist", "hier", "Lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "ADV", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alles, was uns hier entz\u00fcckt,", "tokens": ["Al\u00b7les", ",", "was", "uns", "hier", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns der Erde frei entr\u00fcckt!", "tokens": ["Uns", "der", "Er\u00b7de", "frei", "ent\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr fragt, wo er geblieben?", "tokens": ["Und", "ihr", "fragt", ",", "wo", "er", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Droben wei\u00df das Herz die Lust,", "tokens": ["Dro\u00b7ben", "wei\u00df", "das", "Herz", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die es suchte unbewu\u00dft,", "tokens": ["Die", "es", "such\u00b7te", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Schmerz entr\u00e4thselt sich,", "tokens": ["Und", "der", "Schmerz", "ent\u00b7r\u00e4th\u00b7selt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und was mit der Zeiten wich,", "tokens": ["Und", "was", "mit", "der", "Zei\u00b7ten", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Kommt dort endlich doch zur\u00fcck.", "tokens": ["Kommt", "dort", "end\u00b7lich", "doch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Trauert nicht um Todesgl\u00fcck!", "tokens": ["Trau\u00b7ert", "nicht", "um", "To\u00b7des\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}