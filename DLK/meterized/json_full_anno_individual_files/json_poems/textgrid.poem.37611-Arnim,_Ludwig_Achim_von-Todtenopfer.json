{"textgrid.poem.37611": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Todtenopfer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun die Schlacht vor\u00fcber,", "tokens": ["Nun", "die", "Schlacht", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nun die Lebenden gez\u00e4hlt,", "tokens": ["Nun", "die", "Le\u00b7ben\u00b7den", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach der Todte, der uns fehlt,", "tokens": ["Ach", "der", "Tod\u00b7te", ",", "der", "uns", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "War vor allen uns doch lieber;", "tokens": ["War", "vor", "al\u00b7len", "uns", "doch", "lie\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Nachruf klingt so tr\u00fcb:", "tokens": ["Und", "der", "Nach\u00b7ruf", "klingt", "so", "tr\u00fcb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach der Todte war uns lieb,", "tokens": ["Ach", "der", "Tod\u00b7te", "war", "uns", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Nacht, die uns umgiebt,", "tokens": ["Und", "die", "Nacht", ",", "die", "uns", "um\u00b7giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat vor allen ihn geliebt,", "tokens": ["Hat", "vor", "al\u00b7len", "ihn", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hat ihn an ihr Herz gelegt,", "tokens": ["Hat", "ihn", "an", "ihr", "Herz", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und so schwer an Thr\u00e4nen tr\u00e4gt.", "tokens": ["Und", "so", "schwer", "an", "Thr\u00e4\u00b7nen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wache auf, du Treuer,", "tokens": ["Wa\u00b7che", "auf", ",", "du", "Treu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ruft der Geister Himmelsschaar,", "tokens": ["Ruft", "der", "Geis\u00b7ter", "Him\u00b7mels\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Stern gl\u00e4nzt freudenklar", "tokens": ["Je\u00b7der", "Stern", "gl\u00e4nzt", "freu\u00b7den\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem neuen Schlachtenfeuer,", "tokens": ["Zu", "dem", "neu\u00b7en", "Schlach\u00b7ten\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh auf himmlischem Gefild", "tokens": ["Sieh", "auf", "himm\u00b7li\u00b7schem", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fromme fest und B\u00f6se wild,", "tokens": ["From\u00b7me", "fest", "und", "B\u00f6\u00b7se", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon erhebt sich Wolkendampf", "tokens": ["Schon", "er\u00b7hebt", "sich", "Wol\u00b7ken\u00b7dampf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem h\u00f6hern Geisterkampf", "tokens": ["Von", "dem", "h\u00f6\u00b7hern", "Geis\u00b7ter\u00b7kampf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00dcber unserm Sternenzelt", "tokens": ["\u00dc\u00b7ber", "un\u00b7serm", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sind sie schon zum Kampf gestellt.", "tokens": ["Sind", "sie", "schon", "zum", "Kampf", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kennst du deine Br\u00fcder,", "tokens": ["Kennst", "du", "dei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die du lange hast vermi\u00dft,", "tokens": ["Die", "du", "lan\u00b7ge", "hast", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freue dich, du frommer Christ;", "tokens": ["Freu\u00b7e", "dich", ",", "du", "from\u00b7mer", "Christ", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlage nicht die Augen nieder,", "tokens": ["Schla\u00b7ge", "nicht", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Christus will vor ihnen ziehen,", "tokens": ["Chris\u00b7tus", "will", "vor", "ih\u00b7nen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Wunden rosig bl\u00fchen;", "tokens": ["Sei\u00b7ne", "Wun\u00b7den", "ro\u00b7sig", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er winkt und rufet dich;", "tokens": ["Und", "er", "winkt", "und", "ru\u00b7fet", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Br\u00fcder freuen sich,", "tokens": ["Al\u00b7le", "Br\u00fc\u00b7der", "freu\u00b7en", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihre Schaar ist noch nicht gro\u00df,", "tokens": ["Ih\u00b7re", "Schaar", "ist", "noch", "nicht", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Du bist heut ihr Schlachtgeno\u00df.", "tokens": ["Du", "bist", "heut", "ihr", "Schlacht\u00b7ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und jemehr hier fallen,", "tokens": ["Und", "je\u00b7mehr", "hier", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dort so gr\u00f6\u00dfer wird das Heer,", "tokens": ["Dort", "so", "gr\u00f6\u00b7\u00dfer", "wird", "das", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu des heil'gen Thrones Wehr,", "tokens": ["Zu", "des", "heil'\u00b7gen", "Thro\u00b7nes", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Der vom B\u00f6sen angefallen,", "tokens": ["Der", "vom", "B\u00f6\u00b7sen", "an\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wundert euch darum nicht mehr,", "tokens": ["Wun\u00b7dert", "euch", "da\u00b7rum", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Frommen hier im Heer", "tokens": ["Da\u00df", "die", "From\u00b7men", "hier", "im", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fcher sinken in dem Streit,", "tokens": ["Fr\u00fc\u00b7her", "sin\u00b7ken", "in", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie geh\u00f6ren hehrer Zeit,", "tokens": ["Sie", "ge\u00b7h\u00f6\u00b7ren", "heh\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo ihr Herz einst ganz erkannt", "tokens": ["Wo", "ihr", "Herz", "einst", "ganz", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und ihr Muth zur That entbrannt.", "tokens": ["Und", "ihr", "Muth", "zur", "That", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Klein ist irdisch Streiten", "tokens": ["Klein", "ist", "ir\u00b7disch", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADJD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gegen jene Heldenschlacht;", "tokens": ["Ge\u00b7gen", "je\u00b7ne", "Hel\u00b7den\u00b7schlacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auch in der Erdennacht", "tokens": ["Doch", "auch", "in", "der", "Er\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird ein D\u00e4mmerschein uns leiten,", "tokens": ["Wird", "ein", "D\u00e4m\u00b7mer\u00b7schein", "uns", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelszeichen retten oft,", "tokens": ["Him\u00b7mels\u00b7zei\u00b7chen", "ret\u00b7ten", "oft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer auf Himmelsgnade hofft,", "tokens": ["Wer", "auf", "Him\u00b7mels\u00b7gna\u00b7de", "hofft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steigern mu\u00df der ird'sche Streit", "tokens": ["Stei\u00b7gern", "mu\u00df", "der", "ird'\u00b7sche", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df zum H\u00f6hern wir bereit;", "tokens": ["Da\u00df", "zum", "H\u00f6\u00b7hern", "wir", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja, das Leben w\u00e4re Wahn,", "tokens": ["Ja", ",", "das", "Le\u00b7ben", "w\u00e4\u00b7re", "Wahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn es nicht des Himmels Bahn.", "tokens": ["Wenn", "es", "nicht", "des", "Him\u00b7mels", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sagt, was ist hier Lieben?", "tokens": ["Sagt", ",", "was", "ist", "hier", "Lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "ADV", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alles, was uns hier entz\u00fcckt,", "tokens": ["Al\u00b7les", ",", "was", "uns", "hier", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns der Erde frei entr\u00fcckt!", "tokens": ["Uns", "der", "Er\u00b7de", "frei", "ent\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr fragt, wo er geblieben?", "tokens": ["Und", "ihr", "fragt", ",", "wo", "er", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Droben wei\u00df das Herz die Lust,", "tokens": ["Dro\u00b7ben", "wei\u00df", "das", "Herz", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die es suchte unbewu\u00dft,", "tokens": ["Die", "es", "such\u00b7te", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Schmerz entr\u00e4thselt sich,", "tokens": ["Und", "der", "Schmerz", "ent\u00b7r\u00e4th\u00b7selt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und was mit der Zeiten wich,", "tokens": ["Und", "was", "mit", "der", "Zei\u00b7ten", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Kommt dort endlich doch zur\u00fcck.", "tokens": ["Kommt", "dort", "end\u00b7lich", "doch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Trauert nicht um Todesgl\u00fcck!", "tokens": ["Trau\u00b7ert", "nicht", "um", "To\u00b7des\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR