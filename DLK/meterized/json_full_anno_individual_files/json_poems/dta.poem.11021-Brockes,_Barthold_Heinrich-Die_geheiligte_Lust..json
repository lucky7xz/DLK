{"dta.poem.11021": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die geheiligte Lust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oherr! la\u00df mich, in deinen Werken,", "tokens": ["O\u00b7herr", "!", "la\u00df", "mich", ",", "in", "dei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in derselben Nutz und Pracht,", "tokens": ["Und", "in", "der\u00b7sel\u00b7ben", "Nutz", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nebst deiner unumschr\u00e4nkten ", "tokens": ["Nebst", "dei\u00b7ner", "un\u00b7um\u00b7schr\u00e4nk\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Proben deiner ", "tokens": ["Die", "Pro\u00b7ben", "dei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Doch auch vor allem die\u00df dabey,", "tokens": ["Doch", "auch", "vor", "al\u00b7lem", "die\u00df", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "PDS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df deine ", "tokens": ["Da\u00df", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Denkt ihr an ihn mit recht ger\u00fchrter Brust,", "tokens": ["Denkt", "ihr", "an", "ihn", "mit", "recht", "ge\u00b7r\u00fchr\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Vergn\u00fcgt ihr euch, in seiner Werke Pracht,", "tokens": ["Ver\u00b7gn\u00fcgt", "ihr", "euch", ",", "in", "sei\u00b7ner", "Wer\u00b7ke", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "An seiner ", "tokens": ["An", "sei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "So heiligt ihr, im Sch\u00f6pfer, eure Lust.", "tokens": ["So", "hei\u00b7ligt", "ihr", ",", "im", "Sch\u00f6p\u00b7fer", ",", "eu\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}