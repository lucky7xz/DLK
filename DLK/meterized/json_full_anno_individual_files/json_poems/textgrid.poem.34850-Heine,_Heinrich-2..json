{"textgrid.poem.34850": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf goldenem Stuhl, im Reiche der Schatten,", "tokens": ["Auf", "gol\u00b7de\u00b7nem", "Stuhl", ",", "im", "Rei\u00b7che", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Seite des k\u00f6niglichen Gatten,", "tokens": ["Zur", "Sei\u00b7te", "des", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sitzt Proserpine", "tokens": ["Sitzt", "Pro\u00b7ser\u00b7pi\u00b7ne"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit finstrer Miene.", "tokens": ["Mit", "finst\u00b7rer", "Mie\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und im Herzen seufzet sie traurig:", "tokens": ["Und", "im", "Her\u00b7zen", "seuf\u00b7zet", "sie", "trau\u00b7rig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbich lechze nach Rosen, nach Sangeserg\u00fcssen", "tokens": ["\u00bb", "ich", "lech\u00b7ze", "nach", "Ro\u00b7sen", ",", "nach", "San\u00b7ge\u00b7ser\u00b7g\u00fcs\u00b7sen"], "token_info": 