{"dta.poem.21338": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedwedem f\u00fchlenden Herzen bleibt", "tokens": ["Jed\u00b7we\u00b7dem", "f\u00fch\u00b7len\u00b7den", "Her\u00b7zen", "bleibt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das Vaterland ewig theuer \u2013", "tokens": ["Das", "Va\u00b7ter\u00b7land", "e\u00b7wig", "theu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich liebe auch recht braun geschmort", "tokens": ["Ich", "lie\u00b7be", "auch", "recht", "braun", "ge\u00b7schmort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die B\u00fccklinge und Eyer.", "tokens": ["Die", "B\u00fcck\u00b7lin\u00b7ge", "und", "Ey\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie jauchzten die W\u00fcrste im spritzelnden Fett!", "tokens": ["Wie", "jauchz\u00b7ten", "die", "W\u00fcrs\u00b7te", "im", "sprit\u00b7zeln\u00b7den", "Fett", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Krammetsv\u00f6gel, die frommen", "tokens": ["Die", "Kram\u00b7mets\u00b7v\u00f6\u00b7gel", ",", "die", "from\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gebratenen Englein mit Apfelmu\u00df,", "tokens": ["Ge\u00b7bra\u00b7te\u00b7nen", "En\u00b7glein", "mit", "Ap\u00b7fel\u00b7mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie zwitscherten mir: Willkommen!", "tokens": ["Sie", "zwit\u00b7scher\u00b7ten", "mir", ":", "Will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Willkommen, Landsmann, \u2013 zwitscherten sie \u2013", "tokens": ["Will\u00b7kom\u00b7men", ",", "Lands\u00b7mann", ",", "\u2013", "zwit\u00b7scher\u00b7ten", "sie", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bist lange ausgeblieben,", "tokens": ["Bist", "lan\u00b7ge", "aus\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast dich mit fremdem Gev\u00f6gel so lang", "tokens": ["Hast", "dich", "mit", "frem\u00b7dem", "Ge\u00b7v\u00f6\u00b7gel", "so", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In der Fremde herumgetrieben!", "tokens": ["In", "der", "Frem\u00b7de", "her\u00b7um\u00b7ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es stand auf dem Tische eine Gans,", "tokens": ["Es", "stand", "auf", "dem", "Ti\u00b7sche", "ei\u00b7ne", "Gans", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein stilles, gem\u00fcthliches Wesen.", "tokens": ["Ein", "stil\u00b7les", ",", "ge\u00b7m\u00fcth\u00b7li\u00b7ches", "We\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie hat vielleicht mich einst geliebt,", "tokens": ["Sie", "hat", "viel\u00b7leicht", "mich", "einst", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wir beide noch jung gewesen.", "tokens": ["Als", "wir", "bei\u00b7de", "noch", "jung", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "ADJD", "VAPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}