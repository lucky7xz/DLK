{"dta.poem.18213": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  \u201eEin Herz, das zersprungen!\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "In Sammt und in Trauer", "tokens": ["In", "Sammt", "und", "in", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Liegt einsam ein Grab,", "tokens": ["Liegt", "ein\u00b7sam", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Grab an der Mauer.", "tokens": ["Ein", "Grab", "an", "der", "Mau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Kein Marmorstein deckt", "tokens": ["Kein", "Mar\u00b7mor\u00b7stein", "deckt"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den sinkenden H\u00fcgel,", "tokens": ["Den", "sin\u00b7ken\u00b7den", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch dr\u00fcberhin reckt", "tokens": ["Doch", "dr\u00fc\u00b7berh\u00b7in", "reckt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Baum seine Fl\u00fcgel.", "tokens": ["Ein", "Baum", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Christuskreuz sieht", "tokens": ["Ein", "Chris\u00b7tus\u00b7kreuz", "sieht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Aus bl\u00fchendem Flieder", "tokens": ["Aus", "bl\u00fc\u00b7hen\u00b7dem", "Flie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und manchmal auch kniet", "tokens": ["Und", "manch\u00b7mal", "auch", "kniet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Weib davor nieder.", "tokens": ["Ein", "Weib", "da\u00b7vor", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und gestern, als sacht", "tokens": ["Und", "ge\u00b7stern", ",", "als", "sacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ich vor\u00fcbergegangen,", "tokens": ["Ich", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da gab ich drauf Acht,", "tokens": ["Da", "gab", "ich", "drauf", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "CARD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Was die V\u00f6gel dort sangen.", "tokens": ["Was", "die", "V\u00f6\u00b7gel", "dort", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ich lauschte und sieh,", "tokens": ["Ich", "lauschte", "und", "sieh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da war es die alte,", "tokens": ["Da", "war", "es", "die", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Schmerzmelodie,", "tokens": ["Die", "Schmerz\u00b7me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die noch niemals verhallte:", "tokens": ["Die", "noch", "nie\u00b7mals", "ver\u00b7hall\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Ein Baum, der verbl\u00fcht,", "tokens": ["Ein", "Baum", ",", "der", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein Ton, der verklungen,", "tokens": ["Ein", "Ton", ",", "der", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein Stern, der vergl\u00fcht,", "tokens": ["Ein", "Stern", ",", "der", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Herz, das zersprungen!", "tokens": ["Ein", "Herz", ",", "das", "zer\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}