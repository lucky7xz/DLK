{"dta.poem.17552": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "173.  GOtt hat keine Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch GOtt ged\u00e4nket nichts. Ja w\u00e4rn in Jhm", "tokens": ["Mensch", "Gott", "ge\u00b7d\u00e4n\u00b7ket", "nichts", ".", "Ja", "w\u00e4rn", "in", "Jhm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PIS", "$.", "PTKANT", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gedanken (wanken", "tokens": ["Ge\u00b7dan\u00b7ken", "(", "wan\u00b7ken"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$(", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So k\u00f6nt\u2019 Er hin und her/ welchs Jhm nicht zusteht/", "tokens": ["So", "k\u00f6nt'", "Er", "hin", "und", "her", "/", "welchs", "Jhm", "nicht", "zu\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$(", "PWS", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}