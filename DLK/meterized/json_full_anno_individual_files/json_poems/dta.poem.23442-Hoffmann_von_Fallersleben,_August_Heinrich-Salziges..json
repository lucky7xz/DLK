{"dta.poem.23442": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Salziges.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Salz ist theuer, billig sind die Z\u00e4hren!", "tokens": ["Das", "Salz", "ist", "theu\u00b7er", ",", "bil\u00b7lig", "sind", "die", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O wenn doch unsre Z\u00e4hren Salz nur w\u00e4ren!", "tokens": ["O", "wenn", "doch", "uns\u00b7re", "Z\u00e4h\u00b7ren", "Salz", "nur", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADV", "PPOSAT", "NN", "NE", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann h\u00e4tten wir in unsrer Noth", "tokens": ["Dann", "h\u00e4t\u00b7ten", "wir", "in", "uns\u00b7rer", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Salz auf unser bischen Brot.", "tokens": ["Auch", "Salz", "auf", "un\u00b7ser", "bi\u00b7schen", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum doch machen sie das Salz so theuer?", "tokens": ["Wa\u00b7rum", "doch", "ma\u00b7chen", "sie", "das", "Salz", "so", "theu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O ging' es ihnen allen doch noch heuer,", "tokens": ["O", "ging'", "es", "ih\u00b7nen", "al\u00b7len", "doch", "noch", "heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "PIAT", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Loth's Gemahlin dazumal!", "tokens": ["Wie", "Lo\u00b7th's", "Ge\u00b7mah\u00b7lin", "da\u00b7zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann brauchten wir kein Salzregal.", "tokens": ["Dann", "brauch\u00b7ten", "wir", "kein", "Salz\u00b7re\u00b7gal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}