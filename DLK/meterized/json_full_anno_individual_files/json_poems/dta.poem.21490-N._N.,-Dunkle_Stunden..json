{"dta.poem.21490": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Dunkle Stunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O wie schwer und bang\u2019 ward mir das Herz, und wie bitterweh thut", "tokens": ["O", "wie", "schwer", "und", "bang'", "ward", "mir", "das", "Herz", ",", "und", "wie", "bit\u00b7ter\u00b7weh", "thut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "PWAV", "ADJD", "VVFIN"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "doch dein Wort,", "tokens": ["doch", "dein", "Wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "All\u2019 Sonnenlicht und Sonnenglanz zieht tr\u00fcb\u2019 aus meiner Seele fort.", "tokens": ["All'", "Son\u00b7nen\u00b7licht", "und", "Son\u00b7nen\u00b7glanz", "zieht", "tr\u00fcb'", "aus", "mei\u00b7ner", "See\u00b7le", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Was ich gehofft und hei\u00df ersehnt, liegt wie ein w\u00fcstes Tr\u00fcmmerfeld,", "tokens": ["Was", "ich", "ge\u00b7hofft", "und", "hei\u00df", "er\u00b7sehnt", ",", "liegt", "wie", "ein", "w\u00fcs\u00b7tes", "Tr\u00fcm\u00b7mer\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "ADJD", "VVPP", "$,", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Der Tod schleicht durch die wundenkranke, falsche, s\u00fcndenverfallene Welt.", "tokens": ["Der", "Tod", "schleicht", "durch", "die", "wun\u00b7den\u00b7kran\u00b7ke", ",", "fal\u00b7sche", ",", "s\u00fcn\u00b7den\u00b7ver\u00b7fal\u00b7le\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Von Seufzern sch\u00fcttert deine Brust, als wollte sie zerspringen dir,", "tokens": ["Von", "Seuf\u00b7zern", "sch\u00fct\u00b7tert", "dei\u00b7ne", "Brust", ",", "als", "woll\u00b7te", "sie", "zer\u00b7sprin\u00b7gen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "O wie arm und elend, mein Liebling du, wie elend sind nun Beide wir.", "tokens": ["O", "wie", "arm", "und", "e\u00b7lend", ",", "mein", "Lieb\u00b7ling", "du", ",", "wie", "e\u00b7lend", "sind", "nun", "Bei\u00b7de", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "KON", "ADJD", "$,", "PPOSAT", "NN", "PPER", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "PIS", "PPER", "$."], "meter": "+-+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Es kommen die Nebel, die Wasser ziehn, und Finsternisse dr\u00e4uen mit Macht,", "tokens": ["Es", "kom\u00b7men", "die", "Ne\u00b7bel", ",", "die", "Was\u00b7ser", "ziehn", ",", "und", "Fins\u00b7ter\u00b7nis\u00b7se", "dr\u00e4u\u00b7en", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Licht! Licht! O s\u00e4h ich nur ein Licht in dieser todesd\u00fcst\u2019ren Nacht!", "tokens": ["Licht", "!", "Licht", "!", "O", "s\u00e4h", "ich", "nur", "ein", "Licht", "in", "die\u00b7ser", "to\u00b7des\u00b7d\u00fcst'\u00b7ren", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NE", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Was soll ich thun, was soll ich thun? Du f\u00fchre mich sicher, ewiger Geist,", "tokens": ["Was", "soll", "ich", "thun", ",", "was", "soll", "ich", "thun", "?", "Du", "f\u00fch\u00b7re", "mich", "si\u00b7cher", ",", "e\u00b7wi\u00b7ger", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "F\u00fchr\u2019 meine Seele, die durch alle Himmel und Zeiten und R\u00e4ume kreist.", "tokens": ["F\u00fchr'", "mei\u00b7ne", "See\u00b7le", ",", "die", "durch", "al\u00b7le", "Him\u00b7mel", "und", "Zei\u00b7ten", "und", "R\u00e4u\u00b7me", "kreist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+---+-+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Trage auf Adlers Fl\u00fcgeln mich gewaltig zu den Sternen hinauf,", "tokens": ["Tra\u00b7ge", "auf", "Ad\u00b7lers", "Fl\u00fc\u00b7geln", "mich", "ge\u00b7wal\u00b7tig", "zu", "den", "Ster\u00b7nen", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+-+--+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Auseinander wehen die Wolken, golden thut der Himmel sich auf.", "tokens": ["Aus\u00b7ein\u00b7an\u00b7der", "we\u00b7hen", "die", "Wol\u00b7ken", ",", "gol\u00b7den", "thut", "der", "Him\u00b7mel", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJA", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "In die bebende Seele f\u00e4llt mild eine Thr\u00e4ne aus Gottes Aug\u2019,", "tokens": ["In", "die", "be\u00b7ben\u00b7de", "See\u00b7le", "f\u00e4llt", "mild", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "aus", "Got\u00b7tes", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "--+--+-+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Um die gl\u00fchende Stirn weht\u2019s leise wie ein Fr\u00fchlingsrosenhauch.", "tokens": ["Um", "die", "gl\u00fc\u00b7hen\u00b7de", "Stirn", "weht's", "lei\u00b7se", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Nun hebe die Augen, mein Liebling du, die voll von bitt\u2019ren Thr\u00e4nen steh\u2019n,", "tokens": ["Nun", "he\u00b7be", "die", "Au\u00b7gen", ",", "mein", "Lieb\u00b7ling", "du", ",", "die", "voll", "von", "bitt'\u00b7ren", "Thr\u00e4\u00b7nen", "steh'n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "PPER", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich f\u00fchl\u2019s, ich f\u00fchl\u2019s im tiefsten Busen, nun darf ich nimmer von dir geh\u2019n.", "tokens": ["Ich", "f\u00fchl's", ",", "ich", "f\u00fchl's", "im", "tiefs\u00b7ten", "Bu\u00b7sen", ",", "nun", "darf", "ich", "nim\u00b7mer", "von", "dir", "geh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Siehe, die Welt steht wider uns auf mit Hohn und Lachen und kaltem Spott,", "tokens": ["Sie\u00b7he", ",", "die", "Welt", "steht", "wi\u00b7der", "uns", "auf", "mit", "Hohn", "und", "La\u00b7chen", "und", "kal\u00b7tem", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Trock\u2019ne die Thr\u00e4nen vom Auge dir ab, mit uns ist die Liebe, mit uns ist Gott.", "tokens": ["Trock'\u00b7ne", "die", "Thr\u00e4\u00b7nen", "vom", "Au\u00b7ge", "dir", "ab", ",", "mit", "uns", "ist", "die", "Lie\u00b7be", ",", "mit", "uns", "ist", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "+--+--+--+--+-+--+-+", "measure": "dactylic.tetra.plus"}}, "stanza.10": {"line.1": {"text": "In Feuern lodert die Seele auf, auf flammendem Wagen f\u00e4hrt sie empor", "tokens": ["In", "Feu\u00b7ern", "lo\u00b7dert", "die", "See\u00b7le", "auf", ",", "auf", "flam\u00b7men\u00b7dem", "Wa\u00b7gen", "f\u00e4hrt", "sie", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+--+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Weit \u00fcber der Erde d\u00fcsternde Nacht, und durch der Wolken schattendes Thor.", "tokens": ["Weit", "\u00fc\u00b7ber", "der", "Er\u00b7de", "d\u00fcs\u00b7tern\u00b7de", "Nacht", ",", "und", "durch", "der", "Wol\u00b7ken", "schat\u00b7ten\u00b7des", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "NN", "$,", "KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Hoch \u00fcber des Tempels Zinnen schwebt sie stark auf m\u00e4chtigem Fl\u00fcgelpaar,", "tokens": ["Hoch", "\u00fc\u00b7ber", "des", "Tem\u00b7pels", "Zin\u00b7nen", "schwebt", "sie", "stark", "auf", "m\u00e4ch\u00b7ti\u00b7gem", "Fl\u00fc\u00b7gel\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Von den Schwingen tropft die Sonne, flammenregnend, leuchtend", "tokens": ["Von", "den", "Schwin\u00b7gen", "tropft", "die", "Son\u00b7ne", ",", "flam\u00b7men\u00b7reg\u00b7nend", ",", "leuch\u00b7tend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "VVPP", "$,", "VVPP"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "und klar.", "tokens": ["und", "klar", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.12": {"line.1": {"text": "Tief liegt die Welt von Schatten bedeckt, und Thr\u00e4nen und Schmerzen", "tokens": ["Tief", "liegt", "die", "Welt", "von", "Schat\u00b7ten", "be\u00b7deckt", ",", "und", "Thr\u00e4\u00b7nen", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,", "KON", "NN", "KON", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "umh\u00fcllen sie dicht,", "tokens": ["um\u00b7h\u00fcl\u00b7len", "sie", "dicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ein Schrei voll wilder Qual aus tausend blassen Munden schrecklich bricht.", "tokens": ["Und", "ein", "Schrei", "voll", "wil\u00b7der", "Qual", "aus", "tau\u00b7send", "blas\u00b7sen", "Mun\u00b7den", "schreck\u00b7lich", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN", "APPR", "CARD", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Elend und schwach und krank und siech, wie Wasser st\u00fcrzend von Fall zu Fall,", "tokens": ["E\u00b7lend", "und", "schwach", "und", "krank", "und", "siech", ",", "wie", "Was\u00b7ser", "st\u00fcr\u00b7zend", "von", "Fall", "zu", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "ADJD", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+--+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "So sinken die Menschen fahl dahin \u2014 die S\u00fcnde jubelt \u00fcberall.", "tokens": ["So", "sin\u00b7ken", "die", "Men\u00b7schen", "fahl", "da\u00b7hin", "die", "S\u00fcn\u00b7de", "ju\u00b7belt", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PAV", "$(", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+-+--+-+-+--+", "measure": "iambic.septa.relaxed"}}, "stanza.14": {"line.1": {"text": "Und was aus Staub geboren ist, und was gezeugt vom Weibe lebt,", "tokens": ["Und", "was", "aus", "Staub", "ge\u00b7bo\u00b7ren", "ist", ",", "und", "was", "ge\u00b7zeugt", "vom", "Wei\u00b7be", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVPP", "VAFIN", "$,", "KON", "PWS", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wer ist so rein, da\u00df wider dich den ersten Stein er zornig hebt.", "tokens": ["Wer", "ist", "so", "rein", ",", "da\u00df", "wi\u00b7der", "dich", "den", "ers\u00b7ten", "Stein", "er", "zor\u00b7nig", "hebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Doch sieh im Osten gl\u00fcht es auf, und Palmen wehen im Sonnelicht,", "tokens": ["Doch", "sieh", "im", "Os\u00b7ten", "gl\u00fcht", "es", "auf", ",", "und", "Pal\u00b7men", "we\u00b7hen", "im", "Son\u00b7ne\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Heilige L\u00fcfte wandeln und flie\u00dfen um dein bla\u00dfes Angesicht.", "tokens": ["Hei\u00b7li\u00b7ge", "L\u00fcf\u00b7te", "wan\u00b7deln", "und", "flie\u00b7\u00dfen", "um", "dein", "bla\u00b7\u00dfes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN"