{"dta.poem.16460": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "121.  Durch die Menschheit zu der  \n GOttheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wiltu den Perlethau der edlen GOttheit fangen/", "tokens": ["Wil\u00b7tu", "den", "Per\u00b7let\u00b7hau", "der", "ed\u00b7len", "Got\u00b7theit", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "So mustu unverrukt an seiner Menschheit hangen.", "tokens": ["So", "mus\u00b7tu", "un\u00b7ver\u00b7rukt", "an", "sei\u00b7ner", "Menschheit", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}