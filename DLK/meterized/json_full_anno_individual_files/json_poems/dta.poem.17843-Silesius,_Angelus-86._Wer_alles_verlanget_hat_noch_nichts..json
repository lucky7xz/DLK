{"dta.poem.17843": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "86.  Wer alles verlanget/ hat noch nichts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "nl:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nichts verlangt hat alls. Wer alles thut verlan-", "tokens": ["Wer", "nichts", "ver\u00b7langt", "hat", "alls", ".", "Wer", "al\u00b7les", "thut", "ver\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "VVPP", "VAFIN", "ADV", "$.", "PWS", "PIS", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der hat in wahrheit noch nicht einen stiel empfangen.", "tokens": ["Der", "hat", "in", "wahr\u00b7heit", "noch", "nicht", "ei\u00b7nen", "stiel", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}