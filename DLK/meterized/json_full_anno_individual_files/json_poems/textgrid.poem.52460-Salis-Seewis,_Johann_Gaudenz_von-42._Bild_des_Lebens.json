{"textgrid.poem.52460": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "42. Bild des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf des Erdenlebens Steige", "tokens": ["Auf", "des", "Er\u00b7den\u00b7le\u00b7bens", "Stei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt der Freude Silberlicht,", "tokens": ["F\u00e4llt", "der", "Freu\u00b7de", "Sil\u00b7ber\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcchtig, wie durch rege Zweige", "tokens": ["Fl\u00fcch\u00b7tig", ",", "wie", "durch", "re\u00b7ge", "Zwei\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleiches Mondgeflimmer bricht;", "tokens": ["Blei\u00b7ches", "Mond\u00b7ge\u00b7flim\u00b7mer", "bricht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sich Glanz und Nacht verdr\u00e4ngen,", "tokens": ["Wie", "sich", "Glanz", "und", "Nacht", "ver\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo der Tag verlischt im Hain,", "tokens": ["Wo", "der", "Tag", "ver\u00b7lischt", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wechseln auf des Schicksals G\u00e4ngen", "tokens": ["Wech\u00b7seln", "auf", "des", "Schick\u00b7sals", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dunkle Sorg' und Wonneschein.", "tokens": ["Dunk\u00b7le", "Sor\u00b7g'", "und", "Won\u00b7ne\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wenn der Strauch am Kirchhofswege", "tokens": ["Wenn", "der", "Strauch", "am", "Kirch\u00b7hofs\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcten auf den Brautzug streut,", "tokens": ["Bl\u00fc\u00b7ten", "auf", "den", "Braut\u00b7zug", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neigt das gr\u00fcnende Gehege", "tokens": ["Neigt", "das", "gr\u00fc\u00b7nen\u00b7de", "Ge\u00b7he\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Bald sich auf ein Grabgeleit.", "tokens": ["Bald", "sich", "auf", "ein", "Grab\u00b7ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ulmen, unter deren Bl\u00e4tter", "tokens": ["Ul\u00b7men", ",", "un\u00b7ter", "de\u00b7ren", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oft die Nachtigall sich barg,", "tokens": ["Oft", "die", "Nach\u00b7ti\u00b7gall", "sich", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leihen bald des Stammes Bretter", "tokens": ["Lei\u00b7hen", "bald", "des", "Stam\u00b7mes", "Bret\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu der Dorfbewohner Sarg.", "tokens": ["Zu", "der", "Dorf\u00b7be\u00b7woh\u00b7ner", "Sarg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jener West, der auf dem Weizen", "tokens": ["Je\u00b7ner", "West", ",", "der", "auf", "dem", "Wei\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wonnetaumelnd Wogen schl\u00e4gt,", "tokens": ["Won\u00b7ne\u00b7tau\u00b7melnd", "Wo\u00b7gen", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcstert bang' an Denkmalskreuzen,", "tokens": ["Fl\u00fcs\u00b7tert", "bang'", "an", "Denk\u00b7mals\u00b7kreu\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ihr d\u00fcrrer Kranz sich regt;", "tokens": ["Wenn", "ihr", "d\u00fcr\u00b7rer", "Kranz", "sich", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute weht er Regenschauer,", "tokens": ["Heu\u00b7te", "weht", "er", "Re\u00b7gen\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgen Goldgew\u00f6lke fort;", "tokens": ["Mor\u00b7gen", "Gold\u00b7ge\u00b7w\u00f6l\u00b7ke", "fort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hebet hier den Flor der Trauer", "tokens": ["He\u00b7bet", "hier", "den", "Flor", "der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und entbl\u00e4ttert Rosen dort.", "tokens": ["Und", "ent\u00b7bl\u00e4t\u00b7tert", "Ro\u00b7sen", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn, des Reigens Platz zu hellen,", "tokens": ["Wenn", ",", "des", "Rei\u00b7gens", "Platz", "zu", "hel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich das Abendgold ergie\u00dft,", "tokens": ["Sich", "das", "A\u00b7bend\u00b7gold", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dringt es auch in Gitterzellen,", "tokens": ["Dringt", "es", "auch", "in", "Git\u00b7ter\u00b7zel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sich scheuer Gram verschlie\u00dft.", "tokens": ["Wo", "sich", "scheu\u00b7er", "Gram", "ver\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn das Meer im Fr\u00fchrot schimmert,", "tokens": ["Wenn", "das", "Meer", "im", "Fr\u00fch\u00b7rot", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00e4rbt sich auch die Klippenbank,", "tokens": ["F\u00e4rbt", "sich", "auch", "die", "Klip\u00b7pen\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo, vom Nachtorkan zertr\u00fcmmert,", "tokens": ["Wo", ",", "vom", "Nach\u00b7tor\u00b7kan", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das bemannte Schiff versank.", "tokens": ["Das", "be\u00b7mann\u00b7te", "Schiff", "ver\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wandrer, der am Strom der Zeiten", "tokens": ["Wand\u00b7rer", ",", "der", "am", "Strom", "der", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit gesenktem Blicke ruht,", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", "Bli\u00b7cke", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh! auf seiner Flut entgleiten", "tokens": ["Sieh", "!", "auf", "sei\u00b7ner", "Flut", "ent\u00b7glei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolkenschatten, Rosenglut.", "tokens": ["Wol\u00b7ken\u00b7schat\u00b7ten", ",", "Ro\u00b7seng\u00b7lut", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Natur in ihren Bildern,", "tokens": ["Die", "Na\u00b7tur", "in", "ih\u00b7ren", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steten Laufs, doch wandelbar,", "tokens": ["Ste\u00b7ten", "Laufs", ",", "doch", "wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hei\u00dft den Schmerz durch Hoffnung mildern,", "tokens": ["Hei\u00dft", "den", "Schmerz", "durch", "Hoff\u00b7nung", "mil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mahnt den Leichtsinn an Gefahr.", "tokens": ["Mahnt", "den", "Leicht\u00b7sinn", "an", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aus dem Schutte feuchter Hallen", "tokens": ["Aus", "dem", "Schut\u00b7te", "feuch\u00b7ter", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keimt die Steinlevkoje bald;", "tokens": ["Keimt", "die", "Stein\u00b7lev\u00b7ko\u00b7je", "bald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiter, neben Urnen, wallen", "tokens": ["Hei\u00b7ter", ",", "ne\u00b7ben", "Ur\u00b7nen", ",", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nymphen im Cypressenwald;", "tokens": ["Nym\u00b7phen", "im", "Cyp\u00b7res\u00b7sen\u00b7wald", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Auf der Wahlstatt singt die rasche,", "tokens": ["Auf", "der", "Wahl\u00b7statt", "singt", "die", "ra\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ahnungslose Schnitterin,", "tokens": ["Ah\u00b7nungs\u00b7lo\u00b7se", "Schnit\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00fcpft auf der verge\u00dfnen Asche", "tokens": ["H\u00fcpft", "auf", "der", "ver\u00b7ge\u00df\u00b7nen", "A\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Manches Heldenj\u00fcnglings hin.", "tokens": ["Man\u00b7ches", "Hel\u00b7den\u00b7j\u00fcng\u00b7lings", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Horch, was dir des Teiers Leier,", "tokens": ["Horch", ",", "was", "dir", "des", "Tei\u00b7ers", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleims und Flaccus' Muse r\u00e4t:", "tokens": ["Gleims", "und", "Flac\u00b7cus'", "Mu\u00b7se", "r\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weise, wer der Zukunft Schleier", "tokens": ["Wei\u00b7se", ",", "wer", "der", "Zu\u00b7kunft", "Schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur bekr\u00e4nzt und nie durchsp\u00e4ht!", "tokens": ["Nur", "be\u00b7kr\u00e4nzt", "und", "nie", "durch\u00b7sp\u00e4ht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trag ein Herz, den Freuden offen,", "tokens": ["Trag", "ein", "Herz", ",", "den", "Freu\u00b7den", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch zum Leidenskampf bereit;", "tokens": ["Doch", "zum", "Lei\u00b7dens\u00b7kampf", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lern im Mi\u00dfgeschicke hoffen;", "tokens": ["Lern", "im", "Mi\u00df\u00b7ge\u00b7schi\u00b7cke", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denk des Sturms bei heitrer Zeit!", "tokens": ["Denk", "des", "Sturms", "bei", "hei\u00b7trer", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zage nie: Den Kelch der Schmerzen", "tokens": ["Za\u00b7ge", "nie", ":", "Den", "Kelch", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrzt ein s\u00fc\u00dfes Nachgef\u00fchl;", "tokens": ["W\u00fcrzt", "ein", "s\u00fc\u00b7\u00dfes", "Nach\u00b7ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hehrer Schauer hebt die Herzen", "tokens": ["Heh\u00b7rer", "Schau\u00b7er", "hebt", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Orkan und Schlachtgew\u00fchl.", "tokens": ["Im", "Or\u00b7kan", "und"