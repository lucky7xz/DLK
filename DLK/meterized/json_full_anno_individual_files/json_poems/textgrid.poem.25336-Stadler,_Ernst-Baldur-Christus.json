{"textgrid.poem.25336": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Baldur-Christus", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder ward der zeugende Tropfen Bluts", "tokens": ["Und", "wie\u00b7der", "ward", "der", "zeu\u00b7gen\u00b7de", "Trop\u00b7fen", "Bluts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "aus Baldurs Wundenmalen", "tokens": ["aus", "Bal\u00b7durs", "Wun\u00b7den\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu roter Bl\u00fcte erl\u00f6st in der Seele eines Menschen.", "tokens": ["Zu", "ro\u00b7ter", "Bl\u00fc\u00b7te", "er\u00b7l\u00f6st", "in", "der", "See\u00b7le", "ei\u00b7nes", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das war, als der s\u00fcdliche Mittag mit gl\u00fchenden Lippen", "tokens": ["Das", "war", ",", "als", "der", "s\u00fcd\u00b7li\u00b7che", "Mit\u00b7tag", "mit", "gl\u00fc\u00b7hen\u00b7den", "Lip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.5": {"text": "Verdurstend an den Steppen sog von Pal\u00e4stina.", "tokens": ["Ver\u00b7durs\u00b7tend", "an", "den", "Step\u00b7pen", "sog", "von", "Pa\u00b7l\u00e4s\u00b7ti\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Hei\u00df g\u00e4rte ihr Blut, und von der trocknen Stra\u00dfe stieg", "tokens": ["Hei\u00df", "g\u00e4r\u00b7te", "ihr", "Blut", ",", "und", "von", "der", "trock\u00b7nen", "Stra\u00b7\u00dfe", "stieg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ein Feueratem auf", "tokens": ["Ein", "Feu\u00b7e\u00b7ra\u00b7tem", "auf"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und wirbelte in braunen Flocken", "tokens": ["Und", "wir\u00b7bel\u00b7te", "in", "brau\u00b7nen", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Um sonnverbrannte, staubstarrende Gesichter,", "tokens": ["Um", "sonn\u00b7ver\u00b7brann\u00b7te", ",", "staubs\u00b7tar\u00b7ren\u00b7de", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "VMFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Als sie ihn zum ersten Male sahen.", "tokens": ["Als", "sie", "ihn", "zum", "ers\u00b7ten", "Ma\u00b7le", "sa\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Der Sommerwind ri\u00df gierig Jubelrufe", "tokens": ["Der", "Som\u00b7mer\u00b7wind", "ri\u00df", "gie\u00b7rig", "Ju\u00b7bel\u00b7ru\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von ihrem Mund und schleifte sie die Gassen lang:", "tokens": ["Von", "ih\u00b7rem", "Mund", "und", "schleif\u00b7te", "sie", "die", "Gas\u00b7sen", "lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u00bbhosianna! Hosianna!\u00ab", "tokens": ["\u00bb", "ho\u00b7si\u00b7an\u00b7na", "!", "Ho\u00b7si\u00b7an\u00b7na", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Palmen schwankten und bunte T\u00fccher,", "tokens": ["Pal\u00b7men", "schwank\u00b7ten", "und", "bun\u00b7te", "T\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Und ein Leuchten flo\u00df", "tokens": ["Und", "ein", "Leuch\u00b7ten", "flo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.16": {"text": "Von ihm in alle Seelen", "tokens": ["Von", "ihm", "in", "al\u00b7le", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und jauchzte durch die Welt ...", "tokens": ["Und", "jauchz\u00b7te", "durch", "die", "Welt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und es sank der Mittag hin, und das Lied verschwamm", "tokens": ["Und", "es", "sank", "der", "Mit\u00b7tag", "hin", ",", "und", "das", "Lied", "ver\u00b7schwamm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In blauem D\u00e4mmern, das von den Bergen niederrollte.", "tokens": ["In", "blau\u00b7em", "D\u00e4m\u00b7mern", ",", "das", "von", "den", "Ber\u00b7gen", "nie\u00b7der\u00b7roll\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Abendgluten rankten sich um Marmors\u00e4ulen,", "tokens": ["A\u00b7bend\u00b7glu\u00b7ten", "rank\u00b7ten", "sich", "um", "Mar\u00b7mor\u00b7s\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Bluteten auf den wei\u00dfgebauschten Mantel, zuckten", "tokens": ["Blu\u00b7te\u00b7ten", "auf", "den", "wei\u00df\u00b7ge\u00b7bauschten", "Man\u00b7tel", ",", "zuck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+--+-+-++-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Um wutverzerrte, bleiche Z\u00fcge,", "tokens": ["Um", "wut\u00b7ver\u00b7zerr\u00b7te", ",", "blei\u00b7che", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um geballte F\u00e4uste,", "tokens": ["Um", "ge\u00b7ball\u00b7te", "F\u00e4us\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die sich empor warfen zur Terrasse, wo", "tokens": ["Die", "sich", "em\u00b7por", "war\u00b7fen", "zur", "Ter\u00b7ras\u00b7se", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PRF", "PTKVZ", "VVFIN", "APPRART", "NN", "$,", "PWAV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Er tr\u00e4umend \u00fcber ihre H\u00e4upter weg", "tokens": ["Er", "tr\u00e4u\u00b7mend", "\u00fc\u00b7ber", "ih\u00b7re", "H\u00e4up\u00b7ter", "weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den Tag ins blaue Meer verklingen sah \u2013", "tokens": ["Den", "Tag", "ins", "blau\u00b7e", "Meer", "ver\u00b7klin\u00b7gen", "sah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbkreuzige ihn! Kreuzige ihn!\u00ab", "tokens": ["\u00bb", "kreu\u00b7zi\u00b7ge", "ihn", "!", "Kreu\u00b7zi\u00b7ge", "ihn", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$.", "NN", "PPER", "$.", "$("], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Dumpfes H\u00e4mmern durch das schw\u00fcle Zwielicht.", "tokens": ["Dum\u00b7pfes", "H\u00e4m\u00b7mern", "durch", "das", "schw\u00fc\u00b7le", "Zwie\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Gl\u00fchend starrt die Gier.", "tokens": ["Gl\u00fc\u00b7hend", "starrt", "die", "Gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Die rostigen N\u00e4gel bei\u00dfen sich ins Fleisch.", "tokens": ["Die", "ros\u00b7ti\u00b7gen", "N\u00e4\u00b7gel", "bei\u00b7\u00dfen", "sich", "ins", "Fleisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Die Sehnen springen.", "tokens": ["Die", "Seh\u00b7nen", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Dampfend quillt das Blut.", "tokens": ["Damp\u00b7fend", "quillt", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Ein Wimmern stirbt", "tokens": ["Ein", "Wim\u00b7mern", "stirbt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Im trunknen Reigen, der von Blut und Gier berauscht", "tokens": ["Im", "trunk\u00b7nen", "Rei\u00b7gen", ",", "der", "von", "Blut", "und", "Gier", "be\u00b7rauscht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das Kreuz umrast:", "tokens": ["Das", "Kreuz", "um\u00b7rast", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "\u00bbhilf dir, K\u00f6nig der Juden!\u00ab", "tokens": ["\u00bb", "hilf", "dir", ",", "K\u00f6\u00b7nig", "der", "Ju\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "NN", "ART", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Und der Sturm st\u00f6hnt auf.", "tokens": ["Und", "der", "Sturm", "st\u00f6hnt", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Schreiend verstiebt der Schwarm.", "tokens": ["Schrei\u00b7end", "ver\u00b7stiebt", "der", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Falbe Blitze stechen nieder,", "tokens": ["Fal\u00b7be", "Blit\u00b7ze", "ste\u00b7chen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rasen durch die Stra\u00dfen der Stadt,", "tokens": ["Ra\u00b7sen", "durch", "die", "Stra\u00b7\u00dfen", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die wie von schwarzer Asche versch\u00fcttet starrt,", "tokens": ["Die", "wie", "von", "schwar\u00b7zer", "A\u00b7sche", "ver\u00b7sch\u00fct\u00b7tet", "starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPR", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Fern verdr\u00f6hnend ...", "tokens": ["Fern", "ver\u00b7dr\u00f6h\u00b7nend", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dann weicher Regen ...", "tokens": ["Dann", "wei\u00b7cher", "Re\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Atmende Stille ...", "tokens": ["At\u00b7men\u00b7de", "Stil\u00b7le", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Die Palmen schauern sich", "tokens": ["Die", "Pal\u00b7men", "schau\u00b7ern", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Den Rieseltau von feuchten Bl\u00e4ttern.", "tokens": ["Den", "Rie\u00b7sel\u00b7tau", "von", "feuch\u00b7ten", "Bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Windsto\u00df rei\u00dft die Wolken auseinander ...", "tokens": ["Ein", "Wind\u00b7sto\u00df", "rei\u00dft", "die", "Wol\u00b7ken", "aus\u00b7ein\u00b7an\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Aus grauen Nebeln wei\u00df", "tokens": ["Aus", "grau\u00b7en", "Ne\u00b7beln", "wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der Mond.", "tokens": ["Der", "Mond", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Ein bleiches Leuchten rieselt", "tokens": ["Ein", "blei\u00b7ches", "Leuch\u00b7ten", "rie\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "den schwarzen Stamm hinab,", "tokens": ["den", "schwar\u00b7zen", "Stamm", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Der j\u00e4h sich aufreckt in die Nacht auf Golgatha.", "tokens": ["Der", "j\u00e4h", "sich", "auf\u00b7reckt", "in", "die", "Nacht", "auf", "Gol\u00b7ga\u00b7tha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVPP", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zittert auf geschlossnen Lidern", "tokens": ["Zit\u00b7tert", "auf", "ge\u00b7schloss\u00b7nen", "Li\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und fahlen Wangen, \u00fcber die", "tokens": ["Und", "fah\u00b7len", "Wan\u00b7gen", ",", "\u00fc\u00b7ber", "die"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Vom Dornkranz, der mit Raubtierpranken", "tokens": ["Vom", "Dorn\u00b7kranz", ",", "der", "mit", "Raub\u00b7tier\u00b7pran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PR