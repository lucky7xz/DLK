{"dta.poem.4228": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken zur Zeit der Erndte,  \n 1735.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt, in diesem warmen Theile der Frucht-", "tokens": ["Mein", "Gott", ",", "in", "die\u00b7sem", "war\u00b7men", "Thei\u00b7le", "der", "Frucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "erf\u00fcllten Sommer-Zeit,", "tokens": ["er\u00b7f\u00fcll\u00b7ten", "Som\u00b7mer\u00b7Zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da eben Rocken, Gersten, Weizen, zu meinem Nutzen,", "tokens": ["Da", "e\u00b7ben", "Ro\u00b7cken", ",", "Gers\u00b7ten", ",", "Wei\u00b7zen", ",", "zu", "mei\u00b7nem", "Nut\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "abgemeyt,", "tokens": ["ab\u00b7ge\u00b7meyt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Betracht\u2019 ich, mit ger\u00fchrtem Geiste, die Wunder Deiner", "tokens": ["Be\u00b7tracht'", "ich", ",", "mit", "ge\u00b7r\u00fchr\u00b7tem", "Geis\u00b7te", ",", "die", "Wun\u00b7der", "Dei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "G\u00fctigkeit.", "tokens": ["G\u00fc\u00b7tig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Hier kann ich ein gem\u00e4het Feld, bedeckt mit gelben Stop-", "tokens": ["Hier", "kann", "ich", "ein", "ge\u00b7m\u00e4\u00b7het", "Feld", ",", "be\u00b7deckt", "mit", "gel\u00b7ben", "Stop"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "VVPP", "NN", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "peln, sehn;", "tokens": ["peln", ",", "sehn", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dort seh\u2019 ich ungez\u00e4hlte Garben, als w\u00e4rens kleine Zelte,", "tokens": ["Dort", "seh'", "ich", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Gar\u00b7ben", ",", "als", "w\u00e4\u00b7rens", "klei\u00b7ne", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "stehn,", "tokens": ["stehn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Die, da sie eine grosse Weite mit ihren kleinen H\u00f6hen", "tokens": ["Die", ",", "da", "sie", "ei\u00b7ne", "gros\u00b7se", "Wei\u00b7te", "mit", "ih\u00b7ren", "klei\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "zieren,", "tokens": ["zie\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ein recht nat\u00fcrlich Friedens-Lager, in langen Linien, for-", "tokens": ["Ein", "recht", "na\u00b7t\u00fcr\u00b7lich", "Frie\u00b7dens\u00b7La\u00b7ger", ",", "in", "lan\u00b7gen", "Li\u00b7ni\u00b7en", ",", "for"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "ADV", "NN", "$,", "APPR", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "mieren,", "tokens": ["mie\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "In Strichen, deren weites Ziel der schnelle Blick kaum", "tokens": ["In", "Stri\u00b7chen", ",", "de\u00b7ren", "wei\u00b7tes", "Ziel", "der", "schnel\u00b7le", "Blick", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELAT", "ADJA", "NN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "kann erreichen,", "tokens": ["kann", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Die alle fast dem Golde gleichen,", "tokens": ["Die", "al\u00b7le", "fast", "dem", "Gol\u00b7de", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Scheint zwischen gr\u00fcner Bohnen Feldern der milden Mut-", "tokens": ["Scheint", "zwi\u00b7schen", "gr\u00fc\u00b7ner", "Boh\u00b7nen", "Fel\u00b7dern", "der", "mil\u00b7den", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "ter gr\u00fcnes Kleid,", "tokens": ["ter", "gr\u00fc\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Mit g\u00fcldnen Borten als bebr\u00e4hmet, in einer pr\u00e4cht\u2019gen", "tokens": ["Mit", "g\u00fcld\u00b7nen", "Bor\u00b7ten", "als", "be\u00b7br\u00e4h\u00b7met", ",", "in", "ei\u00b7ner", "pr\u00e4cht'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Lieblichkeit,", "tokens": ["Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Die ausgespannte reine Luft, mit hellem Licht und W\u00e4rm\u2019", "tokens": ["Die", "aus\u00b7ge\u00b7spann\u00b7te", "rei\u00b7ne", "Luft", ",", "mit", "hel\u00b7lem", "Licht", "und", "W\u00e4rm'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "erf\u00fcllet,", "tokens": ["er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Ist, ganz von Duft, Gew\u00f6lk und Nebel befreyt, erheitert", "tokens": ["Ist", ",", "ganz", "von", "Duft", ",", "Ge\u00b7w\u00f6lk", "und", "Ne\u00b7bel", "be\u00b7freyt", ",", "er\u00b7hei\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "und enth\u00fcllet.", "tokens": ["und", "ent\u00b7h\u00fcl\u00b7let", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Die W\u00e4rme wird von dichten B\u00e4umen gemildert, und,", "tokens": ["Die", "W\u00e4r\u00b7me", "wird", "von", "dich\u00b7ten", "B\u00e4u\u00b7men", "ge\u00b7mil\u00b7dert", ",", "und", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,", "KON", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "durch Schatten, k\u00fchl,", "tokens": ["durch", "Schat\u00b7ten", ",", "k\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Und alles, was man sieht und f\u00fchlet, vergn\u00fcgt das Aug\u2019", "tokens": ["Und", "al\u00b7les", ",", "was", "man", "sieht", "und", "f\u00fch\u00b7let", ",", "ver\u00b7gn\u00fcgt", "das", "Aug'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "und das Gef\u00fchl.", "tokens": ["und", "das", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Ich seh\u2019, durch alle Pracht ger\u00fchrt, die Frucht, die GOtt", "tokens": ["Ich", "seh'", ",", "durch", "al\u00b7le", "Pracht", "ge\u00b7r\u00fchrt", ",", "die", "Frucht", ",", "die", "Gott"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "mir schenket, an,", "tokens": ["mir", "schen\u00b7ket", ",", "an", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Zumahlen den gem\u00e4hten Weizen, den man nicht besser", "tokens": ["Zu\u00b7mah\u00b7len", "den", "ge\u00b7m\u00e4h\u00b7ten", "Wei\u00b7zen", ",", "den", "man", "nicht", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "w\u00fcnschen kann,", "tokens": ["w\u00fcn\u00b7schen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "Von Unkraut, Brand und ", "tokens": ["Von", "Un\u00b7kraut", ",", "Brand", "und"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "einen kleinen Neid,", "tokens": ["ei\u00b7nen", "klei\u00b7nen", "Neid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.36": {"text": "Der Landmann meine Frucht nicht r\u00fchmte. Ger\u00fchrt", "tokens": ["Der", "Land\u00b7mann", "mei\u00b7ne", "Frucht", "nicht", "r\u00fchm\u00b7te", ".", "Ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "durch diese Fruchtbarkeit", "tokens": ["durch", "die\u00b7se", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Der reichen mir verliehnen Erndt\u2019, bewundert\u2019 ich die", "tokens": ["Der", "rei\u00b7chen", "mir", "ver\u00b7lieh\u00b7nen", "Erndt'", ",", "be\u00b7wun\u00b7dert'", "ich", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.39": {"text": "schwehren Aehren", "tokens": ["schweh\u00b7ren", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Derselben, und der K\u00f6rner Gr\u00f6sse. Ich raufte, mit be-", "tokens": ["Der\u00b7sel\u00b7ben", ",", "und", "der", "K\u00f6r\u00b7ner", "Gr\u00f6s\u00b7se", ".", "Ich", "rauf\u00b7te", ",", "mit", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "KON", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "gier'ger Hand,", "tokens": ["gier'\u00b7ger", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.42": {"text": "Von ihnen ein halb Dutzend aus, da ich denn halb er-", "tokens": ["Von", "ih\u00b7nen", "ein", "halb", "Dut\u00b7zend", "aus", ",", "da", "ich", "denn", "halb", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "staunet fand,", "tokens": ["stau\u00b7net", "fand", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.44": {"text": "Da\u00df mir die sechs, drey hundert K\u00f6rner, so, da\u00df nicht ei-", "tokens": ["Da\u00df", "mir", "die", "sechs", ",", "drey", "hun\u00b7dert", "K\u00f6r\u00b7ner", ",", "so", ",", "da\u00df", "nicht", "ei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "CARD", "$,", "CARD", "CARD", "NN", "$,", "ADV", "$,", "KOUS", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "ner fehlt, gew\u00e4hrt.", "tokens": ["ner", "fehlt", ",", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Ob dieses, da der grosse Geber solch einen Segen mir be-", "tokens": ["Ob", "die\u00b7ses", ",", "da", "der", "gros\u00b7se", "Ge\u00b7ber", "solch", "ei\u00b7nen", "Se\u00b7gen", "mir", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "KOUS", "ART", "ADJA", "NN", "PIAT", "ART", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.47": {"text": "schehrt,", "tokens": ["schehrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.48": {"text": "Nicht einer inniglichen Freude, nicht Dankens und Be-", "tokens": ["Nicht", "ei\u00b7ner", "in\u00b7nig\u00b7li\u00b7chen", "Freu\u00b7de", ",", "nicht", "Dan\u00b7kens", "und", "Be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "PTKNEG", "ADV", "KON", "TRUNC"], "meter": "-+--+--+--++-+", "measure": "amphibrach.tri.plus"}, "line.49": {"text": "wunderns wehrt,", "tokens": ["wun\u00b7derns", "wehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.50": {"text": "Wird kein Vern\u00fcnft\u2019ger leugnen d\u00fcrfen. Ich danke denn,", "tokens": ["Wird", "kein", "Ver\u00b7n\u00fcnft'\u00b7ger", "leug\u00b7nen", "d\u00fcr\u00b7fen", ".", "Ich", "dan\u00b7ke", "denn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "VMINF", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.51": {"text": "in meiner Lust,", "tokens": ["in", "mei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.52": {"text": "Die aus dem Schmecken Deiner Lieb\u2019, o GOtt, in meiner", "tokens": ["Die", "aus", "dem", "Schme\u00b7cken", "Dei\u00b7ner", "Lieb'", ",", "o", "Gott", ",", "in", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "frohen Brust,", "tokens": ["fro\u00b7hen", "Brust", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.54": {"text": "Indem ich es erweg\u2019, entstehet, Dir, Der Du aller guten", "tokens