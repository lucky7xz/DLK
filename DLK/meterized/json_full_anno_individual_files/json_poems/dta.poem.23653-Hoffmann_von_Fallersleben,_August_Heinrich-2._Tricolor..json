{"dta.poem.23653": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Tricolor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Blume, wie umstricket?", "tokens": ["Sch\u00f6\u00b7ne", "Blu\u00b7me", ",", "wie", "um\u00b7stri\u00b7cket", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich die w\u00e4lsche Spinne doch!", "tokens": ["Dich", "die", "w\u00e4l\u00b7sche", "Spin\u00b7ne", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du bist noch nicht zerknicket?", "tokens": ["Und", "du", "bist", "noch", "nicht", "zer\u00b7kni\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du gr\u00fcnst und bl\u00fchest noch?", "tokens": ["Und", "du", "gr\u00fcnst", "und", "bl\u00fc\u00b7hest", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eja, ich bl\u00fche, roth und golden,", "tokens": ["\u201e", "ja", ",", "ich", "bl\u00fc\u00b7he", ",", "roth", "und", "gol\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwas schwarz nur mischt sich drein,", "tokens": ["Et\u00b7was", "schwarz", "nur", "mischt", "sich", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Etwas schwarz \u2014 doch meine Dolden", "tokens": ["Et\u00b7was", "schwarz", "doch", "mei\u00b7ne", "Dol\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "KON", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Werden bald nur schwarz noch sein.\u201c", "tokens": ["Wer\u00b7den", "bald", "nur", "schwarz", "noch", "sein", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ADV", "VAINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}