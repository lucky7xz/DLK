{"textgrid.poem.48587": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19. Auf Jungfrau Elsaben Niehu\u00df mit Herren M. Salomon Matthias ihre Hochzeit, 1637 Juni 12.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Teurer Br\u00e4utgam, mir mehr nicht", "tokens": ["Teu\u00b7rer", "Br\u00e4ut\u00b7gam", ",", "mir", "mehr", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "als bekant nur vom Bericht',", "tokens": ["als", "be\u00b7kant", "nur", "vom", "Be\u00b7richt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "habt ihr das wol k\u00f6nnen wagen", "tokens": ["habt", "ihr", "das", "wol", "k\u00f6n\u00b7nen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PDS", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die mittelste der Zahl", "tokens": ["und", "die", "mit\u00b7tels\u00b7te", "der", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "dreier Schwestern nach der Wahl", "tokens": ["drei\u00b7er", "Schwes\u00b7tern", "nach", "der", "Wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "um die Ehe d\u00fcrfen fragen?", "tokens": ["um", "die", "E\u00b7he", "d\u00fcr\u00b7fen", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und getraute sies auch ihr,", "tokens": ["Und", "ge\u00b7trau\u00b7te", "sies", "auch", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "einer so zu greifen f\u00fcr,", "tokens": ["ei\u00b7ner", "so", "zu", "grei\u00b7fen", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sie, das Herzblat unter dreien?", "tokens": ["sie", ",", "das", "Herz\u00b7blat", "un\u00b7ter", "drei\u00b7en", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "APPR", "CARD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie, der Eltern wahrer Ruhm", "tokens": ["Sie", ",", "der", "El\u00b7tern", "wah\u00b7rer", "Ruhm"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und der Tugend Eigentum,", "tokens": ["und", "der", "Tu\u00b7gend", "Ei\u00b7gen\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "will und soll alleine freien?", "tokens": ["will", "und", "soll", "al\u00b7lei\u00b7ne", "frei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frei ist Freien, wie es hei\u00dft;", "tokens": ["Frei", "ist", "Frei\u00b7en", ",", "wie", "es", "hei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "frei sein will ein freier Geist.", "tokens": ["frei", "sein", "will", "ein", "frei\u00b7er", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freit denn, Freie, nach Belieben,", "tokens": ["Freit", "denn", ",", "Frei\u00b7e", ",", "nach", "Be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sie nach Wundsch' und ihr nach Lust!", "tokens": ["sie", "nach", "Wund\u00b7sch'", "und", "ihr", "nach", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Freit und liebt, dr\u00fcckt Brust an Brust!", "tokens": ["Freit", "und", "liebt", ",", "dr\u00fcckt", "Brust", "an", "Brust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe darf frei Alles \u00fcben.", "tokens": ["Lie\u00b7be", "darf", "frei", "Al\u00b7les", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df wol, da\u00df diese Schrift", "tokens": ["Ich", "wei\u00df", "wol", ",", "da\u00df", "die\u00b7se", "Schrift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PDAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die gesolte Zeit nicht trifft.", "tokens": ["die", "ge\u00b7sol\u00b7te", "Zeit", "nicht", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich hat die Begier zu reisen.", "tokens": ["Mich", "hat", "die", "Be\u00b7gier", "zu", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "allzuweit von euch gef\u00fchrt,", "tokens": ["all\u00b7zu\u00b7weit", "von", "euch", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "so da\u00df, wie sichs wol geb\u00fchrt',", "tokens": ["so", "da\u00df", ",", "wie", "sichs", "wol", "ge\u00b7b\u00fchrt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "PWAV", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ich euch nichts von mir kan weisen.", "tokens": ["ich", "euch", "nichts", "von", "mir", "kan", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als ich nachzurechnen wei\u00df,", "tokens": ["Als", "ich", "nach\u00b7zu\u00b7rech\u00b7nen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "so zerflo\u00df ich fast vor Schwei\u00df", "tokens": ["so", "zer\u00b7flo\u00df", "ich", "fast", "vor", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in dem d\u00fcrren ", "tokens": ["in", "dem", "d\u00fcr\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "da der hocherfreute ", "tokens": ["da", "der", "ho\u00b7cher\u00b7freu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "euch sah' euer Hochzeitzelt", "tokens": ["euch", "sah'", "eu\u00b7er", "Hoch\u00b7zeit\u00b7zelt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "schlagen auf an seinem Strande.", "tokens": ["schla\u00b7gen", "auf", "an", "sei\u00b7nem", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "langsam zwar und doch mit Lust.", "tokens": ["lang\u00b7sam", "zwar", "und", "doch", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit der Zeit ich das vernommen,", "tokens": ["Seit", "der", "Zeit", "ich", "das", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "reis' ich Jahr und Tag nach euch", "tokens": ["reis'", "ich", "Jahr", "und", "Tag", "nach", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "durch so manches Volk und Reich", "tokens": ["durch", "so", "man\u00b7ches", "Volk", "und", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und kan noch nicht zu Ruh' kommen.", "tokens": ["und", "kan", "noch", "nicht", "zu", "Ruh'", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Wils Gott und h\u00e4lt ", "tokens": ["Wils", "Gott", "und", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "nicht zu lange nur in sich,", "tokens": ["nicht", "zu", "lan\u00b7ge", "nur", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADV", "ADV", "APPR", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "so verhoff' ich, eh' den Fl\u00fcssen", "tokens": ["so", "ver\u00b7hoff'", "ich", ",", "eh'", "den", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihr gest\u00e4hltes Band entsteht", "tokens": ["ihr", "ge\u00b7st\u00e4hl\u00b7tes", "Band", "ent\u00b7steht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und der sp\u00e4te Schnee zergeht,", "tokens": ["und", "der", "sp\u00e4\u00b7te", "Schnee", "zer\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "euch mit Freuden zu begr\u00fc\u00dfen.", "tokens": ["euch", "mit", "Freu\u00b7den", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mittels dessen, trautes Paar,", "tokens": ["Mit\u00b7tels", "des\u00b7sen", ",", "trau\u00b7tes", "Paar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "liebt und freit euch Jahr f\u00fcr Jahr,", "tokens": ["liebt", "und", "freit", "euch", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tag f\u00fcr Tag und alle Stunden!", "tokens": ["Tag", "f\u00fcr", "Tag", "und", "al\u00b7le", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freien und gefreiet sein,", "tokens": ["Frei\u00b7en", "und", "ge\u00b7frei\u00b7et", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieb- und Gegenliebesschein", "tokens": ["Lie\u00b7b", "und", "Ge\u00b7gen\u00b7lie\u00b7bes\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "haben euch also verbunden.", "tokens": ["ha\u00b7ben", "euch", "al\u00b7so", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Schwester Braut, tut, wie ihr wi\u00dft,", "tokens": ["Schwes\u00b7ter", "Braut", ",", "tut", ",", "wie", "ihr", "wi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df sichs auf das Beste k\u00fc\u00dft!", "tokens": ["da\u00df", "sichs", "auf", "das", "Bes\u00b7te", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Juno schwerts euch zu bei Treuen:", "tokens": ["Ju\u00b7no", "schwerts", "euch", "zu", "bei", "Treu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKZU", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "euch soll der getr\u00f6st'ten Not,", "tokens": ["euch", "soll", "der", "ge\u00b7tr\u00f6st'\u00b7ten", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "die euch macht bald bla\u00df bald rot,", "tokens": ["die", "euch", "macht", "bald", "bla\u00df", "bald", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "nun und nimmermehr gereuen.", "tokens": ["nun", "und", "nim\u00b7mer\u00b7mehr", "ge\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Teurer Br\u00e4utgam, mir mehr nicht", "tokens": ["Teu\u00b7rer", "Br\u00e4ut\u00b7gam", ",", "mir", "mehr", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "als bekant nur vom Bericht',", "tokens": ["als", "be\u00b7kant", "nur", "vom", "Be\u00b7richt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "habt ihr das wol k\u00f6nnen wagen", "tokens": ["habt", "ihr", "das", "wol", "k\u00f6n\u00b7nen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PDS", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die mittelste der Zahl", "tokens": ["und", "die", "mit\u00b7tels\u00b7te", "der", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "dreier Schwestern nach der Wahl", "tokens": ["drei\u00b7er", "Schwes\u00b7tern", "nach", "der", "Wahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}