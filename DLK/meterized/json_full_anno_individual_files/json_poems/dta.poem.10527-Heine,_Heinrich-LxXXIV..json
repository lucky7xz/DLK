{"dta.poem.10527": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Halle auf dem Markt,", "tokens": ["Zu", "Hal\u00b7le", "auf", "dem", "Markt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da stehn zwei gro\u00dfe L\u00f6wen.", "tokens": ["Da", "stehn", "zwei", "gro\u00b7\u00dfe", "L\u00f6\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ei, du hallischer L\u00f6wentrotz,", "tokens": ["Ei", ",", "du", "hal\u00b7li\u00b7scher", "L\u00f6\u00b7went\u00b7rotz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wie hat man dich gez\u00e4hmet!", "tokens": ["Wie", "hat", "man", "dich", "ge\u00b7z\u00e4h\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu Halle auf dem Markt,", "tokens": ["Zu", "Hal\u00b7le", "auf", "dem", "Markt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein gro\u00dfer Riese.", "tokens": ["Da", "steht", "ein", "gro\u00b7\u00dfer", "Rie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat ein Schwert und regt sich nicht,", "tokens": ["Er", "hat", "ein", "Schwert", "und", "regt", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist vor Schreck versteinert.", "tokens": ["Er", "ist", "vor", "Schreck", "ver\u00b7stei\u00b7nert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu Halle auf dem Markt,", "tokens": ["Zu", "Hal\u00b7le", "auf", "dem", "Markt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da steht eine gro\u00dfe Kirche.", "tokens": ["Da", "steht", "ei\u00b7ne", "gro\u00b7\u00dfe", "Kir\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Burschenschaft und die Landsmannschaft,", "tokens": ["Die", "Bur\u00b7schen\u00b7schaft", "und", "die", "Lands\u00b7mann\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die haben dort Platz zum Beten.", "tokens": ["Die", "ha\u00b7ben", "dort", "Platz", "zum", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}