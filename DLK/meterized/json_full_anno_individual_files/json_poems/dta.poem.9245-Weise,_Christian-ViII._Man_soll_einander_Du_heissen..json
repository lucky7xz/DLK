{"dta.poem.9245": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n Man soll einander Du heissen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! wie geht es immer zu?", "tokens": ["Ach", "!", "wie", "geht", "es", "im\u00b7mer", "zu", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verliebten hertzen", "tokens": ["Die", "ver\u00b7lieb\u00b7ten", "hert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heissen nicht einander du/", "tokens": ["Heis\u00b7sen", "nicht", "ein\u00b7an\u00b7der", "du", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann sie freundlich schertzen.", "tokens": ["Wann", "sie", "freund\u00b7lich", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Alles heist nur er und sie/", "tokens": ["Al\u00b7les", "heist", "nur", "er", "und", "sie", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPER", "KON", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wann sie sich ehren/", "tokens": ["Und", "wann", "sie", "sich", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Kan ich kaum mit grosser m\u00fch", "tokens": ["Kan", "ich", "kaum", "mit", "gros\u00b7ser", "m\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schatz und liebgen h\u00f6ren.", "tokens": ["Schatz", "und", "lieb\u00b7gen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "2. Nein f\u00fcrwar/ die reden sind", "tokens": ["Nein", "f\u00fcr\u00b7war", "/", "die", "re\u00b7den", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$(", "ART", "VVINF", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehrentheils verlohren:", "tokens": ["Meh\u00b7ren\u00b7theils", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dann die liebe ist ein kind/", "tokens": ["Dann", "die", "lie\u00b7be", "ist", "ein", "kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wird erst gebohren/", "tokens": ["Und", "wird", "erst", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Drum/ so m\u00fcst ihr diese zeit", "tokens": ["Drum", "/", "so", "m\u00fcst", "ihr", "die\u00b7se", "zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$(", "ADV", "VMFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als wie kinder leben/", "tokens": ["Als", "wie", "kin\u00b7der", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und der s\u00fcssen freundlichkeit", "tokens": ["Und", "der", "s\u00fcs\u00b7sen", "freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kinder-nahmen geben.", "tokens": ["Kin\u00b7der\u00b7nah\u00b7men", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "3. Er und sie klingt viel zu alt/", "tokens": ["Er", "und", "sie", "klingt", "viel", "zu", "alt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Vor die jungen seelen/", "tokens": ["Vor", "die", "jun\u00b7gen", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Welche neuen auffenthalt", "tokens": ["Wel\u00b7che", "neu\u00b7en", "auf\u00b7fent\u00b7halt"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der lieb erwehlen.", "tokens": ["In", "der", "lieb", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Du und du das sch\u00f6ne wort/", "tokens": ["Du", "und", "du", "das", "sch\u00f6\u00b7ne", "wort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df die k\u00fcsse w\u00fcrtzen/", "tokens": ["Mu\u00df", "die", "k\u00fcs\u00b7se", "w\u00fcrt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und die sehnsucht fort und fort", "tokens": ["Und", "die", "sehn\u00b7sucht", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Mit der hoffnung k\u00fcrtzen.", "tokens": ["Mit", "der", "hoff\u00b7nung", "k\u00fcrt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "4. Was kan angenehmer seyn/", "tokens": ["Was", "kan", "an\u00b7ge\u00b7neh\u00b7mer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die zarten kinder/", "tokens": ["Als", "die", "zar\u00b7ten", "kin\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Drum so f\u00e4ngt die liebes-pein", "tokens": ["Drum", "so", "f\u00e4ngt", "die", "lie\u00b7bes\u00b7pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausendmahl geschwinder.", "tokens": ["Tau\u00b7send\u00b7mahl", "ge\u00b7schwin\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wann die gunst-geneigte brust", "tokens": ["Wann", "die", "gunst\u00b7ge\u00b7neig\u00b7te", "brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich darum bem\u00fchet/", "tokens": ["Sich", "da\u00b7rum", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PAV", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was sie bey der kinder-lust/", "tokens": ["Was", "sie", "bey", "der", "kin\u00b7der\u00b7lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als im bilde siehet.", "tokens": ["Als", "im", "bil\u00b7de", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "5. Und darzu so ist noch nicht", "tokens": ["Und", "dar\u00b7zu", "so", "ist", "noch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ADV", "VAFIN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was von kindern fertig/", "tokens": ["Was", "von", "kin\u00b7dern", "fer\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wann es mit der zeit geschicht/", "tokens": ["Wann", "es", "mit", "der", "zeit", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seyd ihrs kaum gew\u00e4rtig/", "tokens": ["Seyd", "ihrs", "kaum", "ge\u00b7w\u00e4r\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Drum/ eh euch die zeit was g\u00f6nnt/", "tokens": ["Drum", "/", "eh", "euch", "die", "zeit", "was", "g\u00f6nnt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "KOUS", "PPER", "ART", "NN", "PWS", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "M\u00fcst ihr euch verf\u00fchren/", "tokens": ["M\u00fcst", "ihr", "euch", "ver\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und die kinder/ wie ihr k\u00f6nnt/", "tokens": ["Und", "die", "kin\u00b7der", "/", "wie", "ihr", "k\u00f6nnt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbsten pr\u00e4sentiren.", "tokens": ["Selbs\u00b7ten", "pr\u00e4\u00b7sen\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "6. Selig/ wer so leben mag/", "tokens": ["Se\u00b7lig", "/", "wer", "so", "le\u00b7ben", "mag", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWS", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut mirs zu gefallen/", "tokens": ["Thut", "mirs", "zu", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und versuchts nur einen tag/", "tokens": ["Und", "ver\u00b7suchts", "nur", "ei\u00b7nen", "tag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so wird in allen", "tokens": ["Nun", "so", "wird", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Neue wollust/ neue ruh/", "tokens": ["Neu\u00b7e", "wol\u00b7lust", "/", "neu\u00b7e", "ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Neues wolgedeyen/", "tokens": ["Neu\u00b7es", "wol\u00b7ge\u00b7de\u00b7yen", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Unter dem verliebten du", "tokens": ["Un\u00b7ter", "dem", "ver\u00b7lieb\u00b7ten", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Euer lieb erfreuen.", "tokens": ["Eu\u00b7er", "lieb", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}