{"dta.poem.19228": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine Pflanze tr\u00e4gt im Fr\u00fchling ihren Samen,", "tokens": ["Welch", "ei\u00b7ne", "Pflan\u00b7ze", "tr\u00e4gt", "im", "Fr\u00fch\u00b7ling", "ih\u00b7ren", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ihre Bl\u00fcten erst hervor im Herbste kamen?", "tokens": ["Da", "ih\u00b7re", "Bl\u00fc\u00b7ten", "erst", "her\u00b7vor", "im", "Herbs\u00b7te", "ka\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PTKVZ", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Zeitlos' ist hierin der Blumen Widerspiel,", "tokens": ["Die", "Zeit\u00b7los'", "ist", "hie\u00b7rin", "der", "Blu\u00b7men", "Wi\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie am Anfang ist, wo jene sind am Ziel;", "tokens": ["Da\u00df", "sie", "am", "An\u00b7fang", "ist", ",", "wo", "je\u00b7ne", "sind", "am", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,", "PWAV", "PDS", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df sie am Ziel ist, wo am Anfang jene stehn;", "tokens": ["Da\u00df", "sie", "am", "Ziel", "ist", ",", "wo", "am", "An\u00b7fang", "je\u00b7ne", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,", "PWAV", "APPRART", "NN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum hat sie die Natur zum Sinnbild ausersehn,", "tokens": ["Drum", "hat", "sie", "die", "Na\u00b7tur", "zum", "Sinn\u00b7bild", "au\u00b7ser\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das aus dem Herbste, wo der Sturm das Feld erbeutet,", "tokens": ["Das", "aus", "dem", "Herbs\u00b7te", ",", "wo", "der", "Sturm", "das", "Feld", "er\u00b7beu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den kahlen Winter durch, zum Lenz hin\u00fcber deutet.", "tokens": ["Den", "kah\u00b7len", "Win\u00b7ter", "durch", ",", "zum", "Lenz", "hin\u00b7\u00fc\u00b7ber", "deu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da sie im Sommer nicht zu reifen Zeit gewann,", "tokens": ["Da", "sie", "im", "Som\u00b7mer", "nicht", "zu", "rei\u00b7fen", "Zeit", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "PTKZU", "VVINF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur die Bl\u00fctenspitz' im Herbste zeigen kann;", "tokens": ["Und", "nur", "die", "Bl\u00fc\u00b7ten\u00b7spitz'", "im", "Herbs\u00b7te", "zei\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Jenseit des Frostes tritt, geweckt von Fr\u00fchlingsluft,", "tokens": ["Jen\u00b7seit", "des", "Fros\u00b7tes", "tritt", ",", "ge\u00b7weckt", "von", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Samenkapsel samt den Bl\u00e4ttern aus der Gruft.", "tokens": ["Die", "Sa\u00b7men\u00b7kap\u00b7sel", "samt", "den", "Bl\u00e4t\u00b7tern", "aus", "der", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zeitlose hei\u00dft sie, weil sie vom Gesetz der Zeit", "tokens": ["Zeit\u00b7lo\u00b7se", "hei\u00dft", "sie", ",", "weil", "sie", "vom", "Ge\u00b7setz", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist gleichsam losgesagt, der Ewigkeit geweiht.", "tokens": ["Ist", "gleich\u00b7sam", "los\u00b7ge\u00b7sagt", ",", "der", "E\u00b7wig\u00b7keit", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}