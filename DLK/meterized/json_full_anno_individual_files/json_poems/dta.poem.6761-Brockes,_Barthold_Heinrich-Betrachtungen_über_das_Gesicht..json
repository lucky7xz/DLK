{"dta.poem.6761": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom fernen Vorwurf merckt man, da\u00df er ferne liege,", "tokens": ["Vom", "fer\u00b7nen", "Vor\u00b7wurf", "merckt", "man", ",", "da\u00df", "er", "fer\u00b7ne", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man von ihm versp\u00fcret,", "tokens": ["Wenn", "man", "von", "ihm", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df seine Farben und die Z\u00fcge", "tokens": ["Da\u00df", "sei\u00b7ne", "Far\u00b7ben", "und", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel schw\u00e4cher anzusehn,", "tokens": ["Viel", "schw\u00e4\u00b7cher", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und da\u00df er uns nicht mehr so kr\u00e4fftig r\u00fchret,", "tokens": ["Und", "da\u00df", "er", "uns", "nicht", "mehr", "so", "kr\u00e4ff\u00b7tig", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als wie von ihm, da er uns nah, geschehn.", "tokens": ["Als", "wie", "von", "ihm", ",", "da", "er", "uns", "nah", ",", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}