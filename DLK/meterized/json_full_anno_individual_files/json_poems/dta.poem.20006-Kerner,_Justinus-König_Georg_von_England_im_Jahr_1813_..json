{"dta.poem.20006": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Georg von England im Jahr 1813 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tief ergraut stieg Englands K\u00f6nig", "tokens": ["Tief", "er\u00b7graut", "stieg", "En\u00b7glands", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der V\u00e4ter hohem Thron,", "tokens": ["Von", "der", "V\u00e4\u00b7ter", "ho\u00b7hem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legte Scepter, goldne Krone", "tokens": ["Leg\u00b7te", "Scep\u00b7ter", ",", "gold\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Hand dem edlen Sohn.", "tokens": ["In", "die", "Hand", "dem", "ed\u00b7len", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald ihm Licht und Rede schwanden,", "tokens": ["Bald", "ihm", "Licht", "und", "Re\u00b7de", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam stand er in der Nacht,", "tokens": ["Ein\u00b7sam", "stand", "er", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also von der Welt geschieden", "tokens": ["Al\u00b7so", "von", "der", "Welt", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat er Jahre zugebracht.", "tokens": ["Hat", "er", "Jah\u00b7re", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich gl\u00e4nzt des Greisen Auge", "tokens": ["Pl\u00f6tz\u00b7lich", "gl\u00e4nzt", "des", "Grei\u00b7sen", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal noch im alten Licht,", "tokens": ["Ein\u00b7mal", "noch", "im", "al\u00b7ten", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die halb versunkne Sonne", "tokens": ["Wie", "die", "halb", "ver\u00b7sunk\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal noch aus Wolken bricht.", "tokens": ["Ein\u00b7mal", "noch", "aus", "Wol\u00b7ken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch die Rede kam ihm wieder,", "tokens": ["Auch", "die", "Re\u00b7de", "kam", "ihm", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klang vollstimm'ger Harfe Ton,", "tokens": ["Klang", "voll\u00b7stim\u00b7m'\u00b7ger", "Har\u00b7fe", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Treue Diener horchten staunend,", "tokens": ["Treu\u00b7e", "Die\u00b7ner", "horch\u00b7ten", "stau\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufen den geliebten Sohn.", "tokens": ["Ru\u00b7fen", "den", "ge\u00b7lieb\u00b7ten", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201eheil!\u201c so sprach der Sohn in Freude,", "tokens": ["\u201e", "heil", "!", "\u201c", "so", "sprach", "der", "Sohn", "in", "Freu\u00b7de", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eheil der himmlisch hohen Macht,", "tokens": ["\u201e", "heil", "der", "himm\u00b7lisch", "ho\u00b7hen", "Macht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dich aus des Innern N\u00e4chten,", "tokens": ["Die", "dich", "aus", "des", "In\u00b7nern", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Einmal noch zur\u00fcckgebracht!\u201c", "tokens": ["Ein\u00b7mal", "noch", "zu\u00b7r\u00fcck\u00b7ge\u00b7bracht", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eweil' bis ich dein altes Leben,", "tokens": ["\u201e", "weil'", "bis", "ich", "dein", "al\u00b7tes", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit Wein und Fr\u00fchlingsduft,", "tokens": ["Wie", "mit", "Wein", "und", "Fr\u00fch\u00b7lings\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit viel s\u00fc\u00dfer hehrer Kunde", "tokens": ["Mit", "viel", "s\u00fc\u00b7\u00dfer", "heh\u00b7rer", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Angefrischt in Kindeslust.\u201c", "tokens": ["An\u00b7ge\u00b7frischt", "in", "Kin\u00b7des\u00b7lust", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seit zur Ruhe dir vom Himmel", "tokens": ["Seit", "zur", "Ru\u00b7he", "dir", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummer auf die Sinne sank,", "tokens": ["Schlum\u00b7mer", "auf", "die", "Sin\u00b7ne", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eisenband mit wildem Donner", "tokens": ["Ei\u00b7sen\u00b7band", "mit", "wil\u00b7dem", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom bedr\u00fcckten Erdball sprang!", "tokens": ["Vom", "be\u00b7dr\u00fcck\u00b7ten", "Erd\u00b7ball", "sprang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201enordlands M\u00e4nner schwangen r\u00e4chend", "tokens": ["\u201e", "nord\u00b7lands", "M\u00e4n\u00b7ner", "schwan\u00b7gen", "r\u00e4\u00b7chend"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eisen in der starken Hand,", "tokens": ["Ei\u00b7sen", "in", "der", "star\u00b7ken", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrme brausten, Flammen tobten,", "tokens": ["St\u00fcr\u00b7me", "braus\u00b7ten", ",", "Flam\u00b7men", "tob\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00fcndeten im teutschen Land.\u201c", "tokens": ["Z\u00fcn\u00b7de\u00b7ten", "im", "teut\u00b7schen", "Land", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "\u201eunter ihren alten Eichen,", "tokens": ["\u201e", "un\u00b7ter", "ih\u00b7ren", "al\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie banger Traum umfieng,", "tokens": ["Wo", "sie", "ban\u00b7ger", "Traum", "um\u00b7fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sprangen auf die teutschen M\u00e4nner,", "tokens": ["Spran\u00b7gen", "auf", "die", "teut\u00b7schen", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprengten keck der Kette Ring.\u201c", "tokens": ["Spreng\u00b7ten", "keck", "der", "Ket\u00b7te", "Ring", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Drauf des Alten Auge gl\u00e4nzte", "tokens": ["Drauf", "des", "Al\u00b7ten", "Au\u00b7ge", "gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit des Nordsterns vollem Schein,", "tokens": ["Mit", "des", "Nords\u00b7terns", "vol\u00b7lem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Pokal ergreift er eilend,", "tokens": ["Den", "Po\u00b7kal", "er\u00b7greift", "er", "ei\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinkt in Lust viel gold'nen Wein.", "tokens": ["Trinkt", "in", "Lust", "viel", "gold'\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und er ruft in hoher Wonne,", "tokens": ["Und", "er", "ruft", "in", "ho\u00b7her", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haltend zitternd den Pokal:", "tokens": ["Hal\u00b7tend", "zit\u00b7ternd", "den", "Po\u00b7kal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201enordstern! aller Sonnen Sonne!", "tokens": ["\u201e", "nords\u00b7tern", "!", "al\u00b7ler", "Son\u00b7nen", "Son\u00b7ne", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben trink' ich deinem Strahl!\u201c", "tokens": ["Le\u00b7ben", "trink'", "ich", "dei\u00b7nem", "Strahl", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u201eleben euch, ihr alten Eichen,", "tokens": ["\u201e", "le\u00b7ben", "euch", ",", "ihr", "al\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im urfesten, teutschen Land!", "tokens": ["Im", "ur\u00b7fes\u00b7ten", ",", "teut\u00b7schen", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00e4nnern, euch, in ihrem Schatten,", "tokens": ["M\u00e4n\u00b7nern", ",", "euch", ",", "in", "ih\u00b7rem", "Schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwerdt' in der gest\u00e4hlten Hand!\u201c", "tokens": ["Schwerdt'", "in", "der", "ge\u00b7st\u00e4hl\u00b7ten", "Hand", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "\u201ebrau's, o Meer, in Harfent\u00f6nen,", "tokens": ["\u201e", "brau's", ",", "o", "Meer", ",", "in", "Har\u00b7fen\u00b7t\u00f6\u00b7nen", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singe hohen Festgesang,", "tokens": ["Sin\u00b7ge", "ho\u00b7hen", "Fest\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der H\u00f6lle Macht zerschlagen,", "tokens": ["Da\u00df", "der", "H\u00f6l\u00b7le", "Macht", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df des Erdballs Kette sprang!\u201c", "tokens": ["Da\u00df", "des", "Erd\u00b7balls", "Ket\u00b7te", "sprang", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "\u201ewas die Zeit in ihrem Laufe,", "tokens": ["\u201e", "was", "die", "Zeit", "in", "ih\u00b7rem", "Lau\u00b7fe", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich auch zur Welt gebracht,", "tokens": ["End\u00b7lich", "auch", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelte als volle Sonne", "tokens": ["Wan\u00b7del\u00b7te", "als", "vol\u00b7le", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4ngst durch meine stille Nacht.\u201c \u2014", "tokens": ["L\u00e4ngst", "durch", "mei\u00b7ne", "stil\u00b7le", "Nacht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Also sprach der Greis entz\u00fccket,", "tokens": ["Al\u00b7so", "sprach", "der", "Greis", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word"