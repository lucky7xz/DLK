{"textgrid.poem.36826": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar keinen Freund, der gleich geliebt den Musen,", "tokens": ["Zwar", "kei\u00b7nen", "Freund", ",", "der", "gleich", "ge\u00b7liebt", "den", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begeisterung entz\u00fcndend und empfangend,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "ent\u00b7z\u00fcn\u00b7dend", "und", "emp\u00b7fan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im sch\u00f6nen Taumel s\u00e4nk' an diesen Busen,", "tokens": ["Im", "sch\u00f6\u00b7nen", "Tau\u00b7mel", "s\u00e4nk'", "an", "die\u00b7sen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein M\u00e4dchen hab' ich, das am Arm mir gienge,", "tokens": ["Kein", "M\u00e4d\u00b7chen", "hab'", "ich", ",", "das", "am", "Arm", "mir", "gien\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn mich der Gott beseelt, schon auf der Lippe", "tokens": ["Wenn", "mich", "der", "Gott", "be\u00b7seelt", ",", "schon", "auf", "der", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hei\u00dfe Lied mit einem Ku\u00df empfienge.", "tokens": ["Das", "hei\u00b7\u00dfe", "Lied", "mit", "ei\u00b7nem", "Ku\u00df", "emp\u00b7fi\u00b7en\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Kaum blieb mir die Erinn'rung noch an beides,", "tokens": ["Kaum", "blieb", "mir", "die", "Er\u00b7inn'\u00b7rung", "noch", "an", "bei\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch, ach, es ist nicht der vergangnen Freuden,", "tokens": ["Doch", ",", "ach", ",", "es", "ist", "nicht", "der", "ver\u00b7gang\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur die Erinnerung vergangnen Leides.", "tokens": ["Nur", "die", "E\u00b7rin\u00b7ne\u00b7rung", "ver\u00b7gang\u00b7nen", "Lei\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Mein Umgang, meine Freunde sind die alten", "tokens": ["Mein", "Um\u00b7gang", ",", "mei\u00b7ne", "Freun\u00b7de", "sind", "die", "al\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entbl\u00f6\u00dften Felsen, der umrauschten Klippen", "tokens": ["Ent\u00b7bl\u00f6\u00df\u00b7ten", "Fel\u00b7sen", ",", "der", "um\u00b7rauschten", "Klip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schwerm\u00fcthige gigantische Gestalten.", "tokens": ["Schwer\u00b7m\u00fct\u00b7hi\u00b7ge", "gi\u00b7gan\u00b7ti\u00b7sche", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.5": {"line.1": {"text": "Denn wie die Insel fern vom festen Lande", "tokens": ["Denn", "wie", "die", "In\u00b7sel", "fern", "vom", "fes\u00b7ten", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlassen ruht, so kn\u00fcpfen mich ans Leben", "tokens": ["Ver\u00b7las\u00b7sen", "ruht", ",", "so", "kn\u00fcp\u00b7fen", "mich", "ans", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mehr begl\u00fcckende begl\u00fcckte Bande.", "tokens": ["Nicht", "mehr", "be\u00b7gl\u00fc\u00b7cken\u00b7de", "be\u00b7gl\u00fcck\u00b7te", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl bin ich einsam, bin ich abgeschlossen,", "tokens": ["Wohl", "bin", "ich", "ein\u00b7sam", ",", "bin", "ich", "ab\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein einzig Gut ist, meine einz'ge Habe,", "tokens": ["Mein", "ein\u00b7zig", "Gut", "ist", ",", "mei\u00b7ne", "einz'\u00b7ge", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich gelitten, was ich einst genossen.", "tokens": ["Was", "ich", "ge\u00b7lit\u00b7ten", ",", "was", "ich", "einst", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dem Meere gleich, seh' ich im Wellenzuge", "tokens": ["Dem", "Mee\u00b7re", "gleich", ",", "seh'", "ich", "im", "Wel\u00b7len\u00b7zu\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Menschheit Wechselstrom vor\u00fcber treiben,", "tokens": ["Der", "Menschheit", "Wech\u00b7sel\u00b7strom", "vor\u00b7\u00fc\u00b7ber", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ich folge nicht mehr seinem falschen Truge.", "tokens": ["Ich", "fol\u00b7ge", "nicht", "mehr", "sei\u00b7nem", "fal\u00b7schen", "Tru\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch wie der Fels nicht mehr im Spiel der Wogen", "tokens": ["Doch", "wie", "der", "Fels", "nicht", "mehr", "im", "Spiel", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PTKNEG", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Winde sich vergn\u00fcgt, die seine Pfeiler", "tokens": ["Und", "Win\u00b7de", "sich", "ver\u00b7gn\u00fcgt", ",", "die", "sei\u00b7ne", "Pfei\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PRF", "VVPP", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In ew'ger Wiederholung stets umzogen;", "tokens": ["In", "ew'\u00b7ger", "Wie\u00b7der\u00b7ho\u00b7lung", "stets", "um\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wie hier der Aloe stolz Gew\u00e4chs erbl\u00fchet,", "tokens": ["Wie", "hier", "der", "A\u00b7loe", "stolz", "Ge\u00b7w\u00e4chs", "er\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort Indiens Feige, Palmen und Oliven,", "tokens": ["Dort", "In\u00b7di\u00b7ens", "Fei\u00b7ge", ",", "Pal\u00b7men", "und", "O\u00b7li\u00b7ven", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier saft'gem Laubgr\u00fcn die Orang' entgl\u00fchet:", "tokens": ["Hier", "saft'\u00b7gem", "Laub\u00b7gr\u00fcn", "die", "O\u00b7rang'", "ent\u00b7gl\u00fc\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "So ist nicht unfruchtbar mein stilles Leben,", "tokens": ["So", "ist", "nicht", "un\u00b7frucht\u00b7bar", "mein", "stil\u00b7les", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In F\u00fclle reifen goldne duft'ge Fr\u00fcchte,", "tokens": ["In", "F\u00fcl\u00b7le", "rei\u00b7fen", "gold\u00b7ne", "duft'\u00b7ge", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Sonnenschein die Edelste der Reben.", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "die", "E\u00b7dels\u00b7te", "der", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "Wird sie zuletzt der sch\u00f6ne Gott bemeistern,", "tokens": ["Wird", "sie", "zu\u00b7letzt", "der", "sch\u00f6\u00b7ne", "Gott", "be\u00b7meis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird sie euch, zu reinem Wein verwandelt,", "tokens": ["So", "wird", "sie", "euch", ",", "zu", "rei\u00b7nem", "Wein", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als feuriger Gesang das Herz begeistern.", "tokens": ["Als", "feu\u00b7ri\u00b7ger", "Ge\u00b7sang", "das", "Herz", "be\u00b7geis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Zwar keinen Freund, der gleich geliebt den Musen,", "tokens": ["Zwar", "kei\u00b7nen", "Freund", ",", "der", "gleich", "ge\u00b7liebt", "den", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begeisterung entz\u00fcndend und empfangend,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "ent\u00b7z\u00fcn\u00b7dend", "und", "emp\u00b7fan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im sch\u00f6nen Taumel s\u00e4nk' an diesen Busen,", "tokens": ["Im", "sch\u00f6\u00b7nen", "Tau\u00b7mel", "s\u00e4nk'", "an", "die\u00b7sen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"]