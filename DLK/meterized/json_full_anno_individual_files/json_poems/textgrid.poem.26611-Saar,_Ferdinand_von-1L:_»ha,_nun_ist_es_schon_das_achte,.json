rd", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zimmert sie \u2013 das spart die Windeln \u2013", "tokens": ["Zim\u00b7mert", "sie", "\u2013", "das", "spart", "die", "Win\u00b7deln", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich die winzigste der Truh'n.", "tokens": ["Gleich", "die", "win\u00b7zigs\u00b7te", "der", "Truh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ART", "ADJA", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Auf der Bank in dumpfer Stube", "tokens": ["Auf", "der", "Bank", "in", "dum\u00b7pfer", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Wurm dann ausgestellt;", "tokens": ["Wird", "der", "Wurm", "dann", "aus\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei's ein M\u00e4dchen, sei's ein Bube \u2013", "tokens": ["Sei's", "ein", "M\u00e4d\u00b7chen", ",", "sei's", "ein", "Bu\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam er doch schon kalt zur Welt!", "tokens": ["Kam", "er", "doch", "schon", "kalt", "zur", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Sch\u00fcttelt auch den Kopf der Bader,", "tokens": ["Sch\u00fct\u00b7telt", "auch", "den", "Kopf", "der", "Ba\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreibt er dennoch seinen Schein;", "tokens": ["Schreibt", "er", "den\u00b7noch", "sei\u00b7nen", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gern umgeht er Streit und Hader \u2013", "tokens": ["Gern", "um\u00b7geht", "er", "Streit", "und", "Ha\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und man gr\u00e4bt das S\u00e4rglein ein.", "tokens": ["Und", "man", "gr\u00e4bt", "das", "S\u00e4rg\u00b7lein", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}