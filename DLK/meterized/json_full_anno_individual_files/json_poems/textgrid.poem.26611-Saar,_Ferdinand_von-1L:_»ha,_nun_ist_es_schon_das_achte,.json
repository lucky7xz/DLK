{"textgrid.poem.26611": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbha, nun ist es schon das achte,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbha, nun ist es schon das achte,", "tokens": ["\u00bb", "ha", ",", "nun", "ist", "es", "schon", "das", "ach\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich meinem Schoo\u00df entringt,", "tokens": ["Das", "sich", "mei\u00b7nem", "Schoo\u00df", "ent\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil der Mann, der unbedachte,", "tokens": ["Weil", "der", "Mann", ",", "der", "un\u00b7be\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets im Rausch mich wieder zwingt.\u00ab", "tokens": ["Stets", "im", "Rausch", "mich", "wie\u00b7der", "zwingt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbhungern m\u00fcssen l\u00e4ngst die andern,", "tokens": ["\u00bb", "hun\u00b7gern", "m\u00fcs\u00b7sen", "l\u00e4ngst", "die", "an\u00b7dern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn dahin sind Feld und Kuh \u2013", "tokens": ["Denn", "da\u00b7hin", "sind", "Feld", "und", "Kuh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "NN", "KON", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und wir k\u00f6nnen bettelnd wandern,", "tokens": ["Und", "wir", "k\u00f6n\u00b7nen", "bet\u00b7telnd", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt dies letzte noch hinzu.\u00ab", "tokens": ["Kommt", "dies", "letz\u00b7te", "noch", "hin\u00b7zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbs\u00e4ug' ich's auf an welken Br\u00fcsten,", "tokens": ["\u00bb", "s\u00e4ug'", "ich's", "auf", "an", "wel\u00b7ken", "Br\u00fcs\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "APPR", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fehlt mir selbst des Taglohns Brot \u2013", "tokens": ["Fehlt", "mir", "selbst", "des", "Tag\u00b7lohns", "Brot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie soll das Zeug ich r\u00fcsten? \u2013", "tokens": ["Und", "wie", "soll", "das", "Zeug", "ich", "r\u00fcs\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "W\u00e4re doch der Balg gleich todt!\u00ab", "tokens": ["W\u00e4\u00b7re", "doch", "der", "Balg", "gleich", "todt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ungeh\u00f6rt und ungesehen", "tokens": ["Un\u00b7ge\u00b7h\u00f6rt", "und", "un\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft's im \u00f6den Stall ein Weib,", "tokens": ["Ruft's", "im", "\u00f6\u00b7den", "Stall", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Greift, bedr\u00e4ngt von raschen Wehen,", "tokens": ["Greift", ",", "be\u00b7dr\u00e4ngt", "von", "ra\u00b7schen", "We\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den schmerzgesprengten Leib.", "tokens": ["In", "den", "schmerz\u00b7ge\u00b7spreng\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit der Hand, der schwielig rauhen,", "tokens": ["Mit", "der", "Hand", ",", "der", "schwie\u00b7lig", "rau\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fa\u00dft sie hart, was sie verflucht \u2013", "tokens": ["Fa\u00dft", "sie", "hart", ",", "was", "sie", "ver\u00b7flucht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PRELS", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und stumpfsinnig, ohne Grauen", "tokens": ["Und", "stumpf\u00b7sin\u00b7nig", ",", "oh\u00b7ne", "Grau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUI", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Schaut sie die entseelte Frucht.", "tokens": ["Schaut", "sie", "die", "ent\u00b7seel\u00b7te", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hastig jetzt aus morschen Schindeln,", "tokens": ["Has\u00b7tig", "jetzt", "aus", "mor\u00b7schen", "Schin\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dort in der Ecke ruh'n,", "tokens": ["Die", "dort", "in", "der", "E\u00b7cke", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zimmert sie \u2013 das spart die Windeln \u2013", "tokens": ["Zim\u00b7mert", "sie", "\u2013", "das", "spart", "die", "Win\u00b7deln", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich die winzigste der Truh'n.", "tokens": ["Gleich", "die", "win\u00b7zigs\u00b7te", "der", "Truh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ART", "ADJA", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf der Bank in dumpfer Stube", "tokens": ["Auf", "der", "Bank", "in", "dum\u00b7pfer", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Wurm dann ausgestellt;", "tokens": ["Wird", "der", "Wurm", "dann", "aus\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei's ein M\u00e4dchen, sei's ein Bube \u2013", "tokens": ["Sei's", "ein", "M\u00e4d\u00b7chen", ",", "sei's", "ein", "Bu\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam er doch schon kalt zur Welt!", "tokens": ["Kam", "er", "doch", "schon", "kalt", "zur", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sch\u00fcttelt auch den Kopf der Bader,", "tokens": ["Sch\u00fct\u00b7telt", "auch", "den", "Kopf", "der", "Ba\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreibt er dennoch seinen Schein;", "tokens": ["Schreibt", "er", "den\u00b7noch", "sei\u0