{"dta.poem.5426": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "T\u00e4glicher Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOtt, ich kann mit tausend Freuden", "tokens": ["Ach", "Gott", ",", "ich", "kann", "mit", "tau\u00b7send", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PPER", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Hertz an deinen Wundern weiden;", "tokens": ["Mein", "Hertz", "an", "dei\u00b7nen", "Wun\u00b7dern", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach la\u00df es oft von mir geschehn!", "tokens": ["Ach", "la\u00df", "es", "oft", "von", "mir", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb da\u00df ich heut und jeden Tag,", "tokens": ["Gieb", "da\u00df", "ich", "heut", "und", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "ADV", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu deiner Ehr\u2019, oft h\u00f6ren, sehen,", "tokens": ["Zu", "dei\u00b7ner", "Ehr'", ",", "oft", "h\u00f6\u00b7ren", ",", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empfinden, riechen, schmecken mag!", "tokens": ["Emp\u00b7fin\u00b7den", ",", "rie\u00b7chen", ",", "schme\u00b7cken", "mag", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df auch mein Beyspiel andre leiten,", "tokens": ["La\u00df", "auch", "mein", "Bey\u00b7spiel", "and\u00b7re", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit von deiner Herrlichkeiten", "tokens": ["Da\u00b7mit", "von", "dei\u00b7ner", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Allgegenw\u00e4rt\u2019gem Glantz und Schein", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7gem", "Glantz", "und", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Noch mancher mag ger\u00fchret seyn!", "tokens": ["Noch", "man\u00b7cher", "mag", "ge\u00b7r\u00fch\u00b7ret", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}