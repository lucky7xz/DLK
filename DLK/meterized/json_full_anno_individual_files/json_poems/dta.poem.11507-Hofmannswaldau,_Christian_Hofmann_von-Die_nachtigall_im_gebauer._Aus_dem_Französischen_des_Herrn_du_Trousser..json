{"dta.poem.11507": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die nachtigall im gebauer.  \n Aus dem Franz\u00f6sischen des Herrn  du Trousser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die muntre nachtigall, vor derer holdem singen", "tokens": ["Die", "mun\u00b7tre", "nach\u00b7ti\u00b7gall", ",", "vor", "de\u00b7rer", "hol\u00b7dem", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PDS", "ADJA", "NN"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die andern v\u00f6gel sich nicht wusten aufzuschwingen,", "tokens": ["Die", "an\u00b7dern", "v\u00f6\u00b7gel", "sich", "nicht", "wus\u00b7ten", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKNEG", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bekam in kurtzer zeit des \u00f6den waldes satt:", "tokens": ["Be\u00b7kam", "in", "kurt\u00b7zer", "zeit", "des", "\u00f6\u00b7den", "wal\u00b7des", "satt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr ticht\u2019 und trachten war von dannen in die sondt.", "tokens": ["Ihr", "ticht'", "und", "trach\u00b7ten", "war", "von", "dan\u00b7nen", "in", "die", "sondt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "VVINF", "VAFIN", "APPR", "ADV", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas? also lie\u00df sie fich in ihrer mund-art h\u00f6ren:", "tokens": ["\u201e", "was", "?", "al\u00b7so", "lie\u00df", "sie", "fich", "in", "ih\u00b7rer", "mun\u00b7dart", "h\u00f6\u00b7ren", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewas will ich doch den wald mit meinem singen ehren?", "tokens": ["\u201e", "was", "will", "ich", "doch", "den", "wald", "mit", "mei\u00b7nem", "sin\u00b7gen", "eh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eund mir fast tag und nacht den hals in st\u00fccken schreyn?", "tokens": ["\u201e", "und", "mir", "fast", "tag", "und", "nacht", "den", "hals", "in", "st\u00fc\u00b7cken", "schreyn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ADV", "NN", "KON", "NN", "ART", "NN", "APPR", "ADJD", "VAINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u201eob meine lieder noch so unvergleichlich seyn,", "tokens": ["\u201e", "ob", "mei\u00b7ne", "lie\u00b7der", "noch", "so", "un\u00b7ver\u00b7gleich\u00b7lich", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewas hab ich hier davon? Den eulen zu gefallen,", "tokens": ["\u201e", "was", "hab", "ich", "hier", "da\u00b7von", "?", "Den", "eu\u00b7len", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "PAV", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund vor das tumme wild ein k\u00fcnstlich st\u00fcck erschallen,", "tokens": ["\u201e", "und", "vor", "das", "tum\u00b7me", "wild", "ein", "k\u00fcnst\u00b7lich", "st\u00fcck", "er\u00b7schal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ehei\u00dft einen leeren schlag ins kalte wasser thun.", "tokens": ["\u201e", "hei\u00dft", "ei\u00b7nen", "lee\u00b7ren", "schlag", "ins", "kal\u00b7te", "was\u00b7ser", "thun", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ela\u00df eine hirtin hier im k\u00fchlen schatten ruhn;", "tokens": ["\u201e", "la\u00df", "ei\u00b7ne", "hir\u00b7tin", "hier", "im", "k\u00fch\u00b7len", "schat\u00b7ten", "ruhn", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "ADV", "APPRART", "ADJA", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "\u201eso h\u00f6ret sie doch mehr, was ihr der buhler faget,", "tokens": ["\u201e", "so", "h\u00f6\u00b7ret", "sie", "doch", "mehr", ",", "was", "ihr", "der", "buh\u00b7ler", "fa\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eals was ein vogel singt. Nachdem sie so geklaget,", "tokens": ["\u201e", "als", "was", "ein", "vo\u00b7gel", "singt", ".", "Nach\u00b7dem", "sie", "so", "ge\u00b7kla\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "So war ihr letzter gall: \u201cVerworffuer aufenthalt!", "tokens": ["So", "war", "ihr", "letz\u00b7ter", "gall", ":", "\u201c", "Ver\u00b7worf\u00b7fu\u00b7er", "auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$(", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "\u201ejhr p\u00fcsche! gute nacht! Dann striech sie durch den wald,", "tokens": ["\u201e", "jhr", "p\u00fc\u00b7sche", "!", "gu\u00b7te", "nacht", "!", "Dann", "striech", "sie", "durch", "den", "wald", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "$.", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Lie\u00df b\u00e4um und th\u00e4ler stehn, floh \u00fcber str\u00e4uch und hecken,", "tokens": ["Lie\u00df", "b\u00e4um", "und", "th\u00e4\u00b7ler", "stehn", ",", "floh", "\u00fc\u00b7ber", "str\u00e4uch", "und", "he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "ADJD", "VVINF", "$,", "VVFIN", "APPR", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Setzt \u00fcber d\u00f6rffer, b\u00e4ch, und lie\u00df sich nichts erschrecken,", "tokens": ["Setzt", "\u00fc\u00b7ber", "d\u00f6rf\u00b7fer", ",", "b\u00e4ch", ",", "und", "lie\u00df", "sich", "nichts", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "ADJD", "$,", "KON", "VVFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bi\u00df sie die kleine welt, das pr\u00e4chtige Pari\u00df,", "tokens": ["Bi\u00df", "sie", "die", "klei\u00b7ne", "welt", ",", "das", "pr\u00e4ch\u00b7ti\u00b7ge", "Pa\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Durch ihren flug erlangt, und sich da fangen lie\u00df.", "tokens": ["Durch", "ih\u00b7ren", "flug", "er\u00b7langt", ",", "und", "sich", "da", "fan\u00b7gen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "KON", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Man steckte sie alsbald in einen engen kercker:", "tokens": ["Man", "steck\u00b7te", "sie", "als\u00b7bald", "in", "ei\u00b7nen", "en\u00b7gen", "ker\u00b7cker", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den trug man in ein haus, und hieug ihn in dem ercker", "tokens": ["Den", "trug", "man", "in", "ein", "haus", ",", "und", "hi\u00b7eug", "ihn", "in", "dem", "er\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Des sch\u00f6nsten zimmers auf. Die zunge wurde los,", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "zim\u00b7mers", "auf", ".", "Die", "zun\u00b7ge", "wur\u00b7de", "los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von der der honigseim der reinsten stimme flos.", "tokens": ["Von", "der", "der", "ho\u00b7ni\u00b7gseim", "der", "reins\u00b7ten", "stim\u00b7me", "flos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man h\u00f6rte sie mit lust: Auf ihr so s\u00fcsses schlagen", "tokens": ["Man", "h\u00f6r\u00b7te", "sie", "mit", "lust", ":", "Auf", "ihr", "so", "s\u00fcs\u00b7ses", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "$.", "APPR", "PPER", "ADV", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Lieff alles h\u00e4uffig zu: Jhr ruhm ward fortgetragen", "tokens": ["Lieff", "al\u00b7les", "h\u00e4uf\u00b7fig", "zu", ":", "Ihr", "ruhm", "ward", "fort\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIS", "ADJD", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Noch ferner, als der schall von ihrer zunge gieng.", "tokens": ["Noch", "fer\u00b7ner", ",", "als", "der", "schall", "von", "ih\u00b7rer", "zun\u00b7ge", "gieng", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die nachtigall war froh: Weil doch ein jedes ding,", "tokens": ["Die", "nach\u00b7ti\u00b7gall", "war", "froh", ":", "Weil", "doch", "ein", "je\u00b7des", "ding", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KOUS", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was neu ist, s\u00fcsse schmeckt. Sie lie\u00df Floretten sorgen:", "tokens": ["Was", "neu", "ist", ",", "s\u00fcs\u00b7se", "schmeckt", ".", "Sie", "lie\u00df", "Flo\u00b7ret\u00b7ten", "sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ADJA", "VVFIN", "$.", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So hie\u00df das sch\u00f6ne kind, von dem sie alle morgen", "tokens": ["So", "hie\u00df", "das", "sch\u00f6\u00b7ne", "kind", ",", "von", "dem", "sie", "al\u00b7le", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mit eigner hand gespeist und auch getr\u00e4ncket ward:", "tokens": ["Mit", "eig\u00b7ner", "hand", "ge\u00b7speist", "und", "auch", "ge\u00b7tr\u00e4n\u00b7cket", "ward", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie wolt auch keine faust, so niedlich und so zart", "tokens": ["Sie", "wolt", "auch", "kei\u00b7ne", "faust", ",", "so", "nied\u00b7lich", "und", "so", "zart"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie immer mochte seyn, sonst am gebauer leiden.", "tokens": ["Sie", "im\u00b7mer", "moch\u00b7te", "seyn", ",", "sonst", "am", "ge\u00b7bau\u00b7er", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VAINF", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was that die nachtigall? Sie stiminte voller freuden", "tokens": ["Was", "that", "die", "nach\u00b7ti\u00b7gall", "?", "Sie", "sti\u00b7min\u00b7te", "vol\u00b7ler", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Ein holdes danck-lied an. Sie sang: \u201cO wie vergn\u00fcgt!", "tokens": ["Ein", "hol\u00b7des", "dan\u00b7ck\u00b7lied", "an", ".", "Sie", "sang", ":", "\u201c", "O", "wie", "ver\u00b7gn\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "$.", "$(", "NE", "KOKOM", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "\u201eo wie vergn\u00fcgt bin ich! wenn es das schicksal f\u00fcgt,", "tokens": ["\u201e", "o", "wie", "ver\u00b7gn\u00fcgt", "bin", "ich", "!", "wenn", "es", "das", "schick\u00b7sal", "f\u00fcgt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KOKOM", "VVPP", "VAFIN", "PPER", "$.", "KOUS", "PPER", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "\u201eda\u00df mein begl\u00fcckter stand so lang als ewig dauret!", "tokens": ["\u201e", "da\u00df", "mein", "be\u00b7gl\u00fcck\u00b7ter", "stand", "so", "lang", "als", "e\u00b7wig", "dau\u00b7ret", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "ADJA", "VVFIN", "ADV", "ADJD", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er daurte nicht so lang, als man bey hofe trauret;", "tokens": ["Er", "daur\u00b7te", "nicht", "so", "lang", ",", "als", "man", "bey", "ho\u00b7fe", "trau\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", 