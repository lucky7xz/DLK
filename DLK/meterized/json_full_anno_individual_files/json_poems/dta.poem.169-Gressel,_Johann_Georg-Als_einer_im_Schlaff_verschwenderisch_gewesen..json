{"dta.poem.169": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als einer im Schlaff verschwenderisch  \n gewesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dgen la\u00df hinfort mich nicht verschwendrisch seyn/", "tokens": ["Mein", "M\u00e4d\u00b7gen", "la\u00df", "hin\u00b7fort", "mich", "nicht", "ver\u00b7schwend\u00b7risch", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "ADV", "PPER", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nimm die Perlen-Milch in deine Muschel ein:", "tokens": ["Und", "nimm", "die", "Per\u00b7len\u00b7Milch", "in", "dei\u00b7ne", "Mu\u00b7schel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gro\u00df Schade/ da\u00df sie wird so liederlich verspr\u00fctzet/", "tokens": ["Gro\u00df", "Scha\u00b7de", "/", "da\u00df", "sie", "wird", "so", "lie\u00b7der\u00b7lich", "ver\u00b7spr\u00fct\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$(", "KOUS", "PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wo sie keiner Schoo\u00df/ auch nicht den T\u00fcchern n\u00fctzet.", "tokens": ["Da", "wo", "sie", "kei\u00b7ner", "Schoo\u00df", "/", "auch", "nicht", "den", "T\u00fc\u00b7chern", "n\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "PIAT", "NN", "$(", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein hart-seyn gegen mich verschwendet meinen Schatz/", "tokens": ["Dein", "har\u00b7tseyn", "ge\u00b7gen", "mich", "ver\u00b7schwen\u00b7det", "mei\u00b7nen", "Schatz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verg\u00f6nne mir hinfort in deinem Schoosse Platz/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "hin\u00b7fort", "in", "dei\u00b7nem", "Schoos\u00b7se", "Platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und la\u00df den Liebes-Than daselbsten sich ergiessen/", "tokens": ["Und", "la\u00df", "den", "Lie\u00b7bes\u00b7Than", "da\u00b7selbs\u00b7ten", "sich", "er\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo er mit gr\u00f6\u00dfrer Lust wird als im Schlaffe fliessen.", "tokens": ["Wo", "er", "mit", "gr\u00f6\u00df\u00b7rer", "Lust", "wird", "als", "im", "Schlaf\u00b7fe", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VAFIN", "KOKOM", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein d\u00fcrrer Acker wird alsdenn von Wollust feist/", "tokens": ["Dein", "d\u00fcr\u00b7rer", "A\u00b7cker", "wird", "als\u00b7denn", "von", "Wol\u00b7lust", "feist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Br\u00fcste h\u00e4rten sich/ die Lust entz\u00fcckt den Geist;", "tokens": ["Die", "Br\u00fcs\u00b7te", "h\u00e4r\u00b7ten", "sich", "/", "die", "Lust", "ent\u00b7z\u00fcckt", "den", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Anmuth/ die durchdringt des gantzen Leibes-Glieder/", "tokens": ["Die", "An\u00b7muth", "/", "die", "durch\u00b7dringt", "des", "gant\u00b7zen", "Lei\u00b7bes\u00b7Glie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In Lachen steigt man ein/ mit Kitzeln kommt man wieder/", "tokens": ["In", "La\u00b7chen", "steigt", "man", "ein", "/", "mit", "Kit\u00b7zeln", "kommt", "man", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "$(", "APPR", "NN", "VVFIN", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nichts denn Erg\u00f6tzung bringt er deiner ", "tokens": ["Nichts", "denn", "Er\u00b7g\u00f6t\u00b7zung", "bringt", "er", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "KON", "NN", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Und l\u00e4st dir alle Lust/ die sie besitzet/ schmecken/", "tokens": ["Und", "l\u00e4st", "dir", "al\u00b7le", "Lust", "/", "die", "sie", "be\u00b7sit\u00b7zet", "/", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Ach stelle doch mein Kind die Spr\u00f6digkeit nur ein!", "tokens": ["Ach", "stel\u00b7le", "doch", "mein", "Kind", "die", "Spr\u00f6\u00b7dig\u00b7keit", "nur", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "La\u00df deine Muschel mir nicht mehr verschlossen seyn/", "tokens": ["La\u00df", "dei\u00b7ne", "Mu\u00b7schel", "mir", "nicht", "mehr", "ver\u00b7schlos\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er\u00f6ffne ihren Helm die Nahrung zu empfangen/", "tokens": ["Er\u00b7\u00f6ff\u00b7ne", "ih\u00b7ren", "Helm", "die", "Nah\u00b7rung", "zu", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo in dem Liebes-Thau/ die Anmuths-Perlen prangen.", "tokens": ["Wo", "in", "dem", "Lie\u00b7bes\u00b7\u00b7T\u00b7hau", "/", "die", "An\u00b7muths\u00b7Per\u00b7len", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Sperrt nun dein Muschel Schlo\u00df die Thore willig auf/", "tokens": ["Sperrt", "nun", "dein", "Mu\u00b7schel", "Schlo\u00df", "die", "Tho\u00b7re", "wil\u00b7lig", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "NN", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und hemmt kein Widrig-seyn mir meinen Liebes-Lauff/", "tokens": ["Und", "hemmt", "kein", "Wid\u00b7rig\u00b7seyn", "mir", "mei\u00b7nen", "Lie\u00b7bes\u00b7Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So soll das Liebes-Safft mit s\u00fcssen Quellen fliessen.", "tokens": ["So", "soll", "das", "Lie\u00b7bes\u00b7\u00b7S\u00b7afft", "mit", "s\u00fcs\u00b7sen", "Quel\u00b7len", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und sich mit vollem Strohm in deine Muschel giessen.", "tokens": ["Und", "sich", "mit", "vol\u00b7lem", "Strohm", "in", "dei\u00b7ne", "Mu\u00b7schel", "gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}