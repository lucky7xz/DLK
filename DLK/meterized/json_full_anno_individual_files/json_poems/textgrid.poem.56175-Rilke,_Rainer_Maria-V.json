{"textgrid.poem.56175": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "V", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hat uns der zu weite Raum verd\u00fcnnt.", "tokens": ["Wie", "hat", "uns", "der", "zu", "wei\u00b7te", "Raum", "ver\u00b7d\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "PTKZU", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Pl\u00f6tzlich besinnen sich die \u00dcberfl\u00fcsse.", "tokens": ["Pl\u00f6tz\u00b7lich", "be\u00b7sin\u00b7nen", "sich", "die", "\u00dc\u00b7berf\u00b7l\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nun sickert durch das stille Sieb der K\u00fcsse", "tokens": ["Nun", "si\u00b7ckert", "durch", "das", "stil\u00b7le", "Sieb", "der", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "des bittren Wesens Alsem und Absynth.", "tokens": ["des", "bit\u00b7tren", "We\u00b7sens", "Al\u00b7sem", "und", "Ab\u00b7synth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Was sind wir viel, aus meinem K\u00f6rper hebt", "tokens": ["Was", "sind", "wir", "viel", ",", "aus", "mei\u00b7nem", "K\u00f6r\u00b7per", "hebt"], "token_info": ["word", "word", "word", "word", "punct