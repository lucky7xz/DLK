{"textgrid.poem.53675": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Prophezeiung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nat\u00fcrlich kommt noch mal die Stinnes-Zeit:", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "kommt", "noch", "mal", "die", "Stin\u00b7nes\u00b7Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit Streikverboten, Posten an den Ecken,", "tokens": ["mit", "Streik\u00b7ver\u00b7bo\u00b7ten", ",", "Pos\u00b7ten", "an", "den", "E\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit Schwarz-Wei\u00df-Rot und den Etappenrecken \u2013", "tokens": ["mit", "Schwa\u00b7rz\u00b7Wei\u00df\u00b7Rot", "und", "den", "E\u00b7tap\u00b7pen\u00b7re\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "das kommt bestimmt. Nur ists noch nicht so weit.", "tokens": ["das", "kommt", "be\u00b7stimmt", ".", "Nur", "ists", "noch", "nicht", "so", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$.", "ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch oben Landwirtschaft und Industrie.", "tokens": ["Hoch", "o\u00b7ben", "Land\u00b7wirt\u00b7schaft", "und", "In\u00b7dust\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Handlangerdienste tut der kleine B\u00fcrger.", "tokens": ["Hand\u00b7lan\u00b7ger\u00b7diens\u00b7te", "tut", "der", "klei\u00b7ne", "B\u00fcr\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gro\u00dfknecht war noch stets ein guter W\u00fcrger", "tokens": ["Der", "Gro\u00df\u00b7knecht", "war", "noch", "stets", "ein", "gu\u00b7ter", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "(nach unten hin) \u2013 er liebt die Monarchie.", "tokens": ["(", "nach", "un\u00b7ten", "hin", ")", "\u2013", "er", "liebt", "die", "Mon\u00b7ar\u00b7chie", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADV", "$(", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie bl\u00e4ht sich dann der kleine Mittelstand!", "tokens": ["Wie", "bl\u00e4ht", "sich", "dann", "der", "klei\u00b7ne", "Mit\u00b7tel\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschwollen blickt er auf zum Reichsverweser.", "tokens": ["Ge\u00b7schwol\u00b7len", "blickt", "er", "auf", "zum", "Reichs\u00b7ver\u00b7we\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdie Pazifisten? Und die ", "tokens": ["\u00bb", "die", "Pa\u00b7zi\u00b7fis\u00b7ten", "?", "Und", "die"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$.", "KON", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kerle m\u00fcssen alle an die Wand!\u00ab", "tokens": ["Die", "Ker\u00b7le", "m\u00fcs\u00b7sen", "al\u00b7le", "an", "die", "Wand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Potsdam steht auf. Hervor kraucht Prinz an Prinz.", "tokens": ["Pots\u00b7dam", "steht", "auf", ".", "Her\u00b7vor", "kraucht", "Prinz", "an", "Prinz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer nicht pariert, der fliegt. Und es setzt Hiebe!", "tokens": ["Wer", "nicht", "pa\u00b7riert", ",", "der", "fliegt", ".", "Und", "es", "setzt", "Hie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVPP", "$,", "PRELS", "VVFIN", "$.", "KON", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt bin ich Gottseidank Herr im Betriebe!", "tokens": ["Jetzt", "bin", "ich", "Gott\u00b7sei\u00b7dank", "Herr", "im", "Be\u00b7trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der kleinste Koofmich f\u00fchlt: Ich bins! Ich bins!", "tokens": ["Der", "kleins\u00b7te", "Koof\u00b7mich", "f\u00fchlt", ":", "Ich", "bins", "!", "Ich", "bins", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hol aus dem Mottenschrank die Uniform!", "tokens": ["Hol", "aus", "dem", "Mot\u00b7ten\u00b7schrank", "die", "U\u00b7ni\u00b7form", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00fcr Klassenurteil, Haft, f\u00fcr feiles Morden", "tokens": ["F\u00fcr", "Klas\u00b7sen\u00b7ur\u00b7teil", ",", "Haft", ",", "f\u00fcr", "fei\u00b7les", "Mor\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gibts Titel, Stellen, Rang und sch\u00f6ne Orden . . .", "tokens": ["gibts", "Ti\u00b7tel", ",", "Stel\u00b7len", ",", "Rang", "und", "sch\u00f6\u00b7ne", "Or\u00b7den", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbder Adler Erster\u00ab \u2013 so was hebt enorm!", "tokens": ["\u00bb", "der", "Ad\u00b7ler", "Ers\u00b7ter", "\u00ab", "\u2013", "so", "was", "hebt", "en\u00b7orm", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$(", "$(", "ADV", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du, Proletarier, bist der tiefste Stein.", "tokens": ["Du", ",", "Pro\u00b7le\u00b7ta\u00b7rier", ",", "bist", "der", "tiefs\u00b7te", "Stein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf dir wird immer feste druff getreten.", "tokens": ["Auf", "dir", "wird", "im\u00b7mer", "fes\u00b7te", "druff", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ADJA", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das putzt die Stiefel sich an dem Proleten \u2013", "tokens": ["Das", "putzt", "die", "Stie\u00b7fel", "sich", "an", "dem", "Pro\u00b7le\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und jeder, jeder will ein C\u00e4sar sein.", "tokens": ["Und", "je\u00b7der", ",", "je\u00b7der", "will", "ein", "C\u00e4\u00b7sar", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VMFIN", "ART", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Poincar\u00e9? Wir ziehen \u00fcbern Rhein!", "tokens": ["Poin\u00b7ca\u00b7r\u00e9", "?", "Wir", "zie\u00b7hen", "\u00fc\u00b7bern", "Rhein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und \u00fcber die Verfassung (altes M\u00f6bel!)", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Ver\u00b7fas\u00b7sung", "(", "al\u00b7tes", "M\u00f6\u00b7bel", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "grinst bayerisch-preu\u00dfischer Soldatenp\u00f6bel.", "tokens": ["grinst", "baye\u00b7rischpreu\u00b7\u00dfi\u00b7scher", "Sol\u00b7da\u00b7ten\u00b7p\u00f6\u00b7bel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und dann das sch\u00f6ne Plus, das da erzielt wird!", "tokens": ["Und", "dann", "das", "sch\u00f6\u00b7ne", "Plus", ",", "das", "da", "er\u00b7zielt", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Wann, Deutschland, siehst du ein, was hier gespielt wird?", "tokens": ["Wann", ",", "Deutschland", ",", "siehst", "du", "ein", ",", "was", "hier", "ge\u00b7spielt", "wird", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "He, Republik \u2013!", "tokens": ["He", ",", "Re\u00b7pub\u00b7lik", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["ITJ", "$,", "NN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie f\u00e4hrt empor, nickt, d\u00f6st und schlummert wieder ein.", "tokens": ["Sie", "f\u00e4hrt", "em\u00b7por", ",", "nickt", ",", "d\u00f6st", "und", "schlum\u00b7mert", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nat\u00fcrlich kommt noch mal die Stinnes-Zeit:", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "kommt", "noch", "mal", "die", "Stin\u00b7nes\u00b7Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit Streikverboten, Posten an den Ecken,", "tokens": ["mit", "Streik\u00b7ver\u00b7bo\u00b7ten", ",", "Pos\u00b7ten", "an", "den", "E\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit Schwarz-Wei\u00df-Rot und den Etappenrecken \u2013", "tokens": ["mit", "Schwa\u00b7rz\u00b7Wei\u00df\u00b7Rot", "und", "den", "E\u00b7tap\u00b7pen\u00b7re\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "das kommt bestimmt. Nur ists noch nicht so weit.", "tokens": ["das", "kommt", "be\u00b7stimmt", ".", "Nur", "ists", "noch", "nicht", "so", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$.", "ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Hoch oben