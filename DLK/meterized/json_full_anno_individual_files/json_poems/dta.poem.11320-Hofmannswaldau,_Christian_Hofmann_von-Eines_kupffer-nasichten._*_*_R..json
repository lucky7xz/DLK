{"dta.poem.11320": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines kupffer-nasichten.  \n * *  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, da\u00df kupffer nur unzeitig silber sey;", "tokens": ["Man", "sagt", ",", "da\u00df", "kupf\u00b7fer", "nur", "un\u00b7zei\u00b7tig", "sil\u00b7ber", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NN", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwundere dich nicht! Ich falle diesem bey,", "tokens": ["Ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", "!", "Ich", "fal\u00b7le", "die\u00b7sem", "bey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PDAT", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und stecke meine nas\u2019 in dieses loch voll erde;", "tokens": ["Und", "ste\u00b7cke", "mei\u00b7ne", "nas'", "in", "die\u00b7ses", "loch", "voll", "er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein leser! w\u00fcnsche nur, da\u00df sie bald silber werde!", "tokens": ["Mein", "le\u00b7ser", "!", "w\u00fcn\u00b7sche", "nur", ",", "da\u00df", "sie", "bald", "sil\u00b7ber", "wer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}