{"textgrid.poem.54276": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die plauderhaffte Dorilis", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach M\u00e4gdgen, plaudre nicht so tumm in Tag hinein,", "tokens": ["Ach", "M\u00e4gd\u00b7gen", ",", "plaud\u00b7re", "nicht", "so", "tumm", "in", "Tag", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df alles, was du wei\u00dft, gleich ausgebla\u00dfen seyn?", "tokens": ["Mu\u00df", "al\u00b7les", ",", "was", "du", "wei\u00dft", ",", "gleich", "aus\u00b7ge\u00b7bla\u00b7\u00dfen", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vertraut man etwas dir, so wei\u00df es ieder Junge,", "tokens": ["Ver\u00b7traut", "man", "et\u00b7was", "dir", ",", "so", "wei\u00df", "es", "ie\u00b7der", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "PPER", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn was im Hertzen sitzst, das schwimmt auch auf der Zunge.", "tokens": ["Denn", "was", "im", "Hert\u00b7zen", "sitz\u00b7st", ",", "das", "schwimmt", "auch", "auf", "der", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Du must, und di\u00df gewi\u00df, durch was verwahrlost seyn,", "tokens": ["Du", "must", ",", "und", "di\u00df", "ge\u00b7wi\u00df", ",", "durch", "was", "ver\u00b7wahr\u00b7lost", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KON", "PDS", "ADV", "$,", "APPR", "PRELS", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil du nicht schweigen kanst, doch ietzo f\u00e4llt mirs ein:", "tokens": ["Weil", "du", "nicht", "schwei\u00b7gen", "kanst", ",", "doch", "iet\u00b7zo", "f\u00e4llt", "mirs", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es hat die Dorilis, wie man kan leicht ermessen,", "tokens": ["Es", "hat", "die", "Do\u00b7ri\u00b7lis", ",", "wie", "man", "kan", "leicht", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "$,", "PWAV", "PIS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Hintertheil gewi\u00df den H\u00fchnern abgefressen.", "tokens": ["Das", "Hin\u00b7ter\u00b7theil", "ge\u00b7wi\u00df", "den", "H\u00fch\u00b7nern", "ab\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVPP",