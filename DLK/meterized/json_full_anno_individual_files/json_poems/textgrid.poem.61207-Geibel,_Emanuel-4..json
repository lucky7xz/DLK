{"textgrid.poem.61207": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Elsa\u00df, rot im Schmuck der Purpurtraube,", "tokens": ["Das", "El\u00b7sa\u00df", ",", "rot", "im", "Schmuck", "der", "Pur\u00b7pur\u00b7trau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Blutrubin in unsres Reichs Geschmeide,", "tokens": ["Den", "Blu\u00b7tru\u00b7bin", "in", "uns\u00b7res", "Reichs", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ausbrach der Frank' ihn mit des Schwertes Schneide,", "tokens": ["Aus\u00b7brach", "der", "Frank'", "ihn", "mit", "des", "Schwer\u00b7tes", "Schnei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er in seines K\u00f6nigs Kron' ihn schraube.", "tokens": ["Da\u00df", "er", "in", "sei\u00b7nes", "K\u00f6\u00b7nigs", "Kron'", "ihn", "schrau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch da er's tat, lag unser Volk im Staube,", "tokens": ["Doch", "da", "er's", "tat", ",", "lag", "un\u00b7ser", "Volk", "im", "S