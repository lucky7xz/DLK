{"dta.poem.14998": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.85", "sv:0.14"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' uns, Allg\u00fctiger!", "tokens": ["H\u00f6r'", "uns", ",", "All\u00b7g\u00fc\u00b7ti\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Himmlischer F\u00fchrer der Schlachten.", "tokens": ["Himm\u00b7li\u00b7scher", "F\u00fch\u00b7rer", "der", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Vater, dich preisen wir!", "tokens": ["Va\u00b7ter", ",", "dich", "prei\u00b7sen", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vater, wir danken dir,", "tokens": ["Va\u00b7ter", ",", "wir", "dan\u00b7ken", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da\u00df wir zur Freiheit erwachten!", "tokens": ["Da\u00df", "wir", "zur", "Frei\u00b7heit", "er\u00b7wach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie auch die H\u00f6lle brau\u00dft,", "tokens": ["Wie", "auch", "die", "H\u00f6l\u00b7le", "brau\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gott, deine starke Faust", "tokens": ["Gott", ",", "dei\u00b7ne", "star\u00b7ke", "Faust"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "St\u00fcrzt das Geb\u00e4ude der L\u00fcge.", "tokens": ["St\u00fcrzt", "das", "Ge\u00b7b\u00e4u\u00b7de", "der", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "F\u00fchr' uns, Herr Zebaoth,", "tokens": ["F\u00fchr'", "uns", ",", "Herr", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "F\u00fchr' uns, dreiein'ger Gott,", "tokens": ["F\u00fchr'", "uns", ",", "drei\u00b7ein'\u00b7ger", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "F\u00fchr' uns zur Schlacht und zum Siege!", "tokens": ["F\u00fchr'", "uns", "zur", "Schlacht", "und", "zum", "Sie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "F\u00fchr uns! \u2014 fall' unser Loos", "tokens": ["F\u00fchr", "uns", "!", "fall'", "un\u00b7ser", "Loos"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "$(", "VVIMP", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Auch tief in Grabes Schoo\u00df,", "tokens": ["Auch", "tief", "in", "Gra\u00b7bes", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lob doch, und Preis deinem Nahmen! \u2014", "tokens": ["Lob", "doch", ",", "und", "Preis", "dei\u00b7nem", "Nah\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$,", "KON", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Reich, Kraft und Herrlichkeit", "tokens": ["Reich", ",", "Kraft", "und", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sind dein in Ewigkeit!", "tokens": ["Sind", "dein", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fchr uns, Allm\u00e4chtiger! \u2014 Amen.", "tokens": ["F\u00fchr", "uns", ",", "All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "!", "A\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$.", "$(", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}