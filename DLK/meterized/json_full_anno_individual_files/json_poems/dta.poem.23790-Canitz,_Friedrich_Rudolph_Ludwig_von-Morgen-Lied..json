: "trochaic.tetra"}, "line.3": {"text": "Wie sich/ was nur Wachsthum sp\u00fcret/", "tokens": ["Wie", "sich", "/", "was", "nur", "Wach\u00b7sthum", "sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$(", "PWS", "ADV", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ihr Glantz die Schatten bricht.", "tokens": ["Wenn", "ihr", "Glantz", "die", "Schat\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So la\u00df dich auch fertig finden/", "tokens": ["So", "la\u00df", "dich", "auch", "fer\u00b7tig", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Deinen Weyrauch/ weil die Nacht/", "tokens": ["Dei\u00b7nen", "Wey\u00b7rauch", "/", "weil", "die", "Nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da dich GOtt f\u00fcr Ungl\u00fccks-St\u00fcrmen", "tokens": ["Da", "dich", "Gott", "f\u00fcr", "Un\u00b7gl\u00fccks\u00b7St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist so gl\u00fccklich hingebracht.", "tokens": ["Ist", "so", "gl\u00fcck\u00b7lich", "hin\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "B