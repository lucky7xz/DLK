{"dta.poem.23790": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Seele du must munter werden/\nDenn der Erden", "tokens": ["See\u00b7le", "du", "must", "mun\u00b7ter", "wer\u00b7den", "/", "Denn", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "VMFIN", "ADJD", "VAINF", "$(", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Blickt hervor ein neuer Tag", "tokens": ["Blickt", "her\u00b7vor", "ein", "neu\u00b7er", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm dem Sch\u00f6pffer dieser Straalen/", "tokens": ["Komm", "dem", "Sch\u00f6pf\u00b7fer", "die\u00b7ser", "Straa\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein schwacher Trieb vermag.", "tokens": ["Was", "dein", "schwa\u00b7cher", "Trieb", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch den grossen GOtt dort oben", "tokens": ["Doch", "den", "gros\u00b7sen", "Gott", "dort", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen nicht nur Lippen seyn/", "tokens": ["Wol\u00b7len", "nicht", "nur", "Lip\u00b7pen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "NN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein! es hat sein reines Wesen/", "tokens": ["Nein", "!", "es", "hat", "sein", "rei\u00b7nes", "We\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hertzen ohne falschen Schein.", "tokens": ["Hert\u00b7zen", "oh\u00b7ne", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Pflicht die kanst du lernen", "tokens": ["Dei\u00b7ne", "Pflicht", "die", "kanst", "du", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren Gold der Sonne weicht.", "tokens": ["De\u00b7ren", "Gold", "der", "Son\u00b7ne", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So la\u00df auch von GOtt zerrinnen/", "tokens": ["So", "la\u00df", "auch", "von", "Gott", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier im Finstern sch\u00f6ne deucht.", "tokens": ["Hier", "im", "Fins\u00b7tern", "sch\u00f6\u00b7ne