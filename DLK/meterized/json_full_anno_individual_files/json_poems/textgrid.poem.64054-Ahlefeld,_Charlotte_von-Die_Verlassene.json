{"textgrid.poem.64054": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die Verlassene", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erklinge still, du Lied der treuen Liebe,", "tokens": ["Er\u00b7klin\u00b7ge", "still", ",", "du", "Lied", "der", "treu\u00b7en", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Seufzerhauch sey deine Melodie.", "tokens": ["Nur", "Seuf\u00b7zer\u00b7hauch", "sey", "dei\u00b7ne", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein lauter Klang darf sich mit dir verm\u00e4hlen,", "tokens": ["Kein", "lau\u00b7ter", "Klang", "darf", "sich", "mit", "dir", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn leise nur spricht die Melancholie.", "tokens": ["Denn", "lei\u00b7se", "nur", "spricht", "die", "Me\u00b7lan\u00b7cho\u00b7lie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Erbebt, ihr Saiten innerer Gef\u00fchle,", "tokens": ["Er\u00b7bebt", ",", "ihr", "Sai\u00b7ten", "in\u00b7ne\u00b7rer", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei der Erinn'rung wunders\u00fcssem Schmerz,", "tokens": ["Bei", "der", "Er\u00b7inn'\u00b7rung", "wun\u00b7der\u00b7s\u00fcs\u00b7sem", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bei dem Gedanken der vergangnen Zeiten,", "tokens": ["Bei", "dem", "Ge\u00b7dan\u00b7ken", "der", "ver\u00b7gang\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wiegt in Schlummer das erregte Herz.", "tokens": ["Und", "wiegt", "in", "Schlum\u00b7mer", "das", "er\u00b7reg\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo bist du hin, du Stunde des Entz\u00fcckens,", "tokens": ["Wo", "bist", "du", "hin", ",", "du", "Stun\u00b7de", "des", "Ent\u00b7z\u00fc\u00b7ckens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als seines Auges klarer Himmelsstrahl,", "tokens": ["Als", "sei\u00b7nes", "Au\u00b7ges", "kla\u00b7rer", "Him\u00b7mels\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Sonnenlicht in eines Kerkers Dunkel,", "tokens": ["Wie", "Son\u00b7nen\u00b7licht", "in", "ei\u00b7nes", "Ker\u00b7kers", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich in die Tiefe meiner Seele stahl?", "tokens": ["Sich", "in", "die", "Tie\u00b7fe", "mei\u00b7ner", "See\u00b7le", "stahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo bist du hin, als seiner Stimme Zauber", "tokens": ["Wo", "bist", "du", "hin", ",", "als", "sei\u00b7ner", "Stim\u00b7me", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum erstenmahl mein bebend Herz durchdrang", "tokens": ["Zum", "ers\u00b7ten\u00b7mahl", "mein", "be\u00b7bend", "Herz", "durch\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVFIN", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und r\u00e4thselhafter Ahndung leises T\u00f6nen", "tokens": ["Und", "r\u00e4th\u00b7sel\u00b7haf\u00b7ter", "Ahn\u00b7dung", "lei\u00b7ses", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir im bewegten Busen tief erklang? \u2013", "tokens": ["Mir", "im", "be\u00b7weg\u00b7ten", "Bu\u00b7sen", "tief", "er\u00b7klang", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach du entflohst \u2013 doch sch\u00f6n're Stunden kamen,", "tokens": ["Ach", "du", "ent\u00b7flohst", "\u2013", "doch", "sch\u00f6n'\u00b7re", "Stun\u00b7den", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$(", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Scheu verschwand, ein ruhigeres Gl\u00fcck", "tokens": ["Die", "Scheu", "ver\u00b7schwand", ",", "ein", "ru\u00b7hi\u00b7ge\u00b7res", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Entbl\u00fchte mir in der geliebten N\u00e4he,", "tokens": ["Ent\u00b7bl\u00fch\u00b7te", "mir", "in", "der", "ge\u00b7lieb\u00b7ten", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rief den innern Frieden mir zur\u00fcck.", "tokens": ["Und", "rief", "den", "in\u00b7nern", "Frie\u00b7den", "mir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da wagt' ich's erst mir selbst es zu gestehen,", "tokens": ["Da", "wagt'", "ich's", "erst", "mir", "selbst", "es", "zu", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dass ich ihn liebte, und in seinem Blick", "tokens": ["Dass", "ich", "ihn", "lieb\u00b7te", ",", "und", "in", "sei\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lag mir, im Glanz der Hoffnung aufgeschlossen,", "tokens": ["Lag", "mir", ",", "im", "Glanz", "der", "Hoff\u00b7nung", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Zukunft, und ein l\u00e4chelndes Geschick.", "tokens": ["Die", "Zu\u00b7kunft", ",", "und", "ein", "l\u00e4\u00b7cheln\u00b7des", "Ge\u00b7schick", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und mich durchschauerte, wie eines Gottes N\u00e4he,", "tokens": ["Und", "mich", "durch\u00b7schau\u00b7er\u00b7te", ",", "wie", "ei\u00b7nes", "Got\u00b7tes", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein ernstes Wort, das Liebe zu mir sprach.", "tokens": ["Sein", "erns\u00b7tes", "Wort", ",", "das", "Lie\u00b7be", "zu", "mir", "sprach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Echo, das in meinem Innern wohnte,", "tokens": ["Ein", "E\u00b7cho", ",", "das", "in", "mei\u00b7nem", "In\u00b7nern", "wohn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klang jede Sylbe seiner Rede nach.", "tokens": ["Klang", "je\u00b7de", "Syl\u00b7be", "sei\u00b7ner", "Re\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Er musste scheiden \u2013 viele Monden schwanden,", "tokens": ["Er", "muss\u00b7te", "schei\u00b7den", "\u2013", "vie\u00b7le", "Mon\u00b7den", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein trauernd Herz erhielt sich ihm getreu.", "tokens": ["Mein", "trau\u00b7ernd", "Herz", "er\u00b7hielt", "sich", "ihm", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PRF", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Still flossen meine Tage hin, wie Thr\u00e4nen,", "tokens": ["Still", "flos\u00b7sen", "mei\u00b7ne", "Ta\u00b7ge", "hin", ",", "wie", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Liebe blieb mir immer neu.", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7be", "blieb", "mir", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Da kehrt' er wieder \u2013 zitternd ihm entgegen", "tokens": ["Da", "kehrt'", "er", "wie\u00b7der", "\u2013", "zit\u00b7ternd", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "VVPP", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu fliegen, und an der geliebten Brust", "tokens": ["Zu", "flie\u00b7gen", ",", "und", "an", "der", "ge\u00b7lieb\u00b7ten", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein freudiges Willkommen! ihm zu stammeln,", "tokens": ["Ein", "freu\u00b7di\u00b7ges", "Will\u00b7kom\u00b7men", "!", "ihm", "zu", "stam\u00b7meln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schien mir des Daseyns h\u00f6chste, reinste Lust.", "tokens": ["Schien", "mir", "des", "Da\u00b7seyns", "h\u00f6chs\u00b7te", ",", "reins\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Doch nun verstummet, leise Klaget\u00f6ne,", "tokens": ["Doch", "nun", "ver\u00b7stum\u00b7met", ",", "lei\u00b7se", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erneuert nicht den ewig heissen Schmerz.", "tokens": ["Er\u00b7neu\u00b7ert", "nicht", "den", "e\u00b7wig", "heis\u00b7sen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es kehrte die ", "tokens": ["Es", "kehr\u00b7te", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch ach \u2013 erkaltet war f\u00fcr mich sein Herz.", "tokens": ["Doch", "ach", "\u2013", "er\u00b7kal\u00b7tet", "war", "f\u00fcr", "mich", "sein", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$(", "VVPP", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Erklinge still, du Lied der treuen Liebe,", "tokens": ["Er\u00b7klin\u00b7ge", "still", ",", "du", "Lied", "der", "treu\u00b7en", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Seufzerhauch sey deine Melodie.", "tokens": ["Nur", "Seuf\u00b7zer\u00b7hauch", "sey", "dei\u00b7ne", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein lauter Klang darf sich mit dir verm\u00e4hlen,", "tokens": ["Kein", "lau\u00b7ter", "Klang", "darf", "s