{"dta.poem.4440": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte diese Nacht, da\u00df, aus der Welt'gerafft,", "tokens": ["Mir", "tr\u00e4um\u00b7te", "die\u00b7se", "Nacht", ",", "da\u00df", ",", "aus", "der", "Welt'\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "KOUS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey einem Armen ich mich eingescharret f\u00fcl\u2019te,", "tokens": ["Bey", "ei\u00b7nem", "Ar\u00b7men", "ich", "mich", "ein\u00b7ge\u00b7schar\u00b7ret", "f\u00fcl'\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ich, voll Verdru\u00df ob seiner Nachbarschaft,", "tokens": ["Und", "da\u00df", "ich", ",", "voll", "Ver\u00b7dru\u00df", "ob", "sei\u00b7ner", "Nach\u00b7bar\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "ADJD", "NN", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ein verstorb\u2019ner Herr ihm diese Rede hielte:", "tokens": ["Als", "ein", "ver\u00b7stor\u00b7b'\u00b7ner", "Herr", "ihm", "die\u00b7se", "Re\u00b7de", "hiel\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Geh, packe dich, du Schurk! verfaule weit von hier!", "tokens": ["Geh", ",", "pa\u00b7cke", "dich", ",", "du", "Schurk", "!", "ver\u00b7fau\u00b7le", "weit", "von", "hier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PPER", "NN", "$.", "VVFIN", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir kommts durchaus nicht zu, da\u00df du so nah bey mir.", "tokens": ["Dir", "kommts", "durc\u00b7haus", "nicht", "zu", ",", "da\u00df", "du", "so", "nah", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du Schurk? Sprach er darauf recht grob und freventlich,", "tokens": ["Du", "Schurk", "?", "Sprach", "er", "da\u00b7rauf", "recht", "grob", "und", "fre\u00b7vent\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "NN", "PPER", "PAV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Such deine Schurken sonst, Schurk, der du selber bist.", "tokens": ["Such", "dei\u00b7ne", "Schur\u00b7ken", "sonst", ",", "Schurk", ",", "der", "du", "sel\u00b7ber", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hier sind wir alle gleich; ich schier mich nichts um dich.", "tokens": ["Hier", "sind", "wir", "al\u00b7le", "gleich", ";", "ich", "schier", "mich", "nichts", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "$.", "PPER", "ADJD", "PPER", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich bin auf meinem, so wie du auf deinem, Mist.", "tokens": ["Ich", "bin", "auf", "mei\u00b7nem", ",", "so", "wie", "du", "auf", "dei\u00b7nem", ",", "Mist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "$,", "ADV", "KOKOM", "PPER", "APPR", "PPOSAT", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}