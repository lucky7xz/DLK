{"dta.poem.12609": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Spie\u00df, ein Schwerdt, ein sch\u00f6ner Schild,", "tokens": ["Ein", "Spie\u00df", ",", "ein", "Schwerdt", ",", "ein", "sch\u00f6\u00b7ner", "Schild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der meinen Leib besch\u00fctzet,", "tokens": ["Der", "mei\u00b7nen", "Leib", "be\u00b7sch\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind mir ein grosser Schatz.", "tokens": ["Sind", "mir", "ein", "gros\u00b7ser", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn hiedurch kann ich pfl\u00fcgen, erndten,", "tokens": ["Denn", "hie\u00b7durch", "kann", "ich", "pfl\u00fc\u00b7gen", ",", "ernd\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die s\u00fcssen Trauben keltern,", "tokens": ["Die", "s\u00fcs\u00b7sen", "Trau\u00b7ben", "kel\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Herr in meinem Hause seyn.", "tokens": ["Und", "Herr", "in", "mei\u00b7nem", "Hau\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die aber es nicht wagen,", "tokens": ["Die", "a\u00b7ber", "es", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Spie\u00df, Schwerdt und Schild zu tragen,", "tokens": ["Spie\u00df", ",", "Schwerdt", "und", "Schild", "zu", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die alle fallen vor mir nieder,", "tokens": ["Die", "al\u00b7le", "fal\u00b7len", "vor", "mir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Verehren mich, als ihren Herrn,", "tokens": ["Ver\u00b7eh\u00b7ren", "mich", ",", "als", "ih\u00b7ren", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und nennen mich gar einen K\u00f6nig,", "tokens": ["Und", "nen\u00b7nen", "mich", "gar", "ei\u00b7nen", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}