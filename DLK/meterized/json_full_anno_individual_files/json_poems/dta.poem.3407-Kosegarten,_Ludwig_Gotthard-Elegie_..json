{"dta.poem.3407": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welche fremde Gef&#252;hle durchschauern mein inner-", "tokens": ["Wel\u00b7che", "frem\u00b7de", "Gef", "&#252;", "hle", "durch\u00b7schau\u00b7ern", "mein", "in\u00b7ner"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "$(", "VVFIN", "ADV", "PPOSAT", "TRUNC"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "stes Innres!", "tokens": ["stes", "Inn\u00b7res", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Welche Bangigkeit \u00e4ngstet die k\u00e4mpfende", "tokens": ["Wel\u00b7che", "Ban\u00b7gig\u00b7keit", "\u00e4ngs\u00b7tet", "die", "k\u00e4mp\u00b7fen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Brust!", "tokens": ["Brust", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Welches Ahnen! und welches Sehnen! und welches", "tokens": ["Wel\u00b7ches", "Ah\u00b7nen", "!", "und", "wel\u00b7ches", "Seh\u00b7nen", "!", "und", "wel\u00b7ches"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$.", "KON", "PWAT", "NN", "$.", "KON", "PWS"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Verlangen!", "tokens": ["Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Welches s\u00fcsse Weh! welche zerfolternde", "tokens": ["Wel\u00b7ches", "s\u00fcs\u00b7se", "Weh", "!", "wel\u00b7che", "zer\u00b7fol\u00b7tern\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "$.", "PWAT", "ADJA"], "meter": "+-+-++--+--", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Lust!", "tokens": ["Lust", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "M\u00e4chte, die mich befehden, ihr Feindlichen, Schreck-", "tokens": ["M\u00e4ch\u00b7te", ",", "die", "mich", "be\u00b7feh\u00b7den", ",", "ihr", "Feind\u00b7li\u00b7chen", ",", "Schreck"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$,", "TRUNC"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.10": {"text": "lichen, die ihr", "tokens": ["li\u00b7chen", ",", "die", "ihr"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJA", "$,", "PRELS", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Sonder Ursach mir z\u00fcrnt, sonder Verschul-", "tokens": ["Son\u00b7der", "Ur\u00b7sach", "mir", "z\u00fcrnt", ",", "son\u00b7der", "Ver\u00b7schul"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "PPER", "VVFIN", "$,", "KON", "TRUNC"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "den mich straft,", "tokens": ["den", "mich", "straft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Redet, wer seyd ihr? \u2014 Wer bist du? \u2014 Bist du", "tokens": ["Re\u00b7det", ",", "wer", "seyd", "ihr", "?", "Wer", "bist", "du", "?", "Bist", "du"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "PPER", "$.", "$(", "PWS", "VAFIN", "PPER", "$.", "$(", "VAFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "es Liebe? \u2014 Dein Odem", "tokens": ["es", "Lie\u00b7be", "?", "Dein", "O\u00b7dem"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "NN", "$.", "$(", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wehet er so? Wie wild weht er! Wie", "tokens": ["We\u00b7het", "er", "so", "?", "Wie", "wild", "weht", "er", "!", "Wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "$.", "PWAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "st\u00fcrmt er! Es flieht", "tokens": ["st\u00fcrmt", "er", "!", "Es", "flieht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Alle Ruhe vor ihm aus meinem Herzen. In Auf-", "tokens": ["Al\u00b7le", "Ru\u00b7he", "vor", "ihm", "aus", "mei\u00b7nem", "Her\u00b7zen", ".", "In", "Auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$.", "APPR", "TRUNC"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "ruhr", "tokens": ["ruhr"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "St\u00fcrmt er diess friedliche Herz. Schone,", "tokens": ["St\u00fcrmt", "er", "diess", "fried\u00b7li\u00b7che", "Herz", ".", "Scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN", "$.", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Zerst\u00f6rende, mein!", "tokens": ["Zer\u00b7st\u00f6\u00b7ren\u00b7de", ",", "mein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Schon', o schone mein! ich mag nicht kosten des", "tokens": ["Schon'", ",", "o", "scho\u00b7ne", "mein", "!", "ich", "mag", "nicht", "kos\u00b7ten", "des"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "VVFIN", "PPOSAT", "$.", "PPER", "VMFIN", "PTKNEG", "VVFIN", "ART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Kelches,", "tokens": ["Kel\u00b7ches", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Dessen sprudelnder Schaum, dessen Ged\u00fcft", "tokens": ["Des\u00b7sen", "spru\u00b7deln\u00b7der", "Schaum", ",", "des\u00b7sen", "Ge\u00b7d\u00fcft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "mich berauscht.", "tokens": ["mich", "be\u00b7rauscht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Hemme den Fittig, und wehre dem Odem! Es weht", "tokens": ["Hem\u00b7me", "den", "Fit\u00b7tig", ",", "und", "weh\u00b7re", "dem", "O\u00b7dem", "!", "Es", "weht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.26": {"text": "so ermattend,", "tokens": ["so", "er\u00b7mat\u00b7tend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "So erkrankend darin. Schone, Zerst\u00f6ren-", "tokens": ["So", "er\u00b7kran\u00b7kend", "da\u00b7rin", ".", "Scho\u00b7ne", ",", "Zer\u00b7st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVPP", "PAV", "$.", "NN", "$,", "TRUNC"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "de, mein!", "tokens": ["de", ",", "mein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Nein, du willst nicht schonen! Mit jeglichem sie-", "tokens": ["Nein", ",", "du", "willst", "nicht", "scho\u00b7nen", "!", "Mit", "jeg\u00b7li\u00b7chem", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "APPR", "PIAT", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "genden Reize,", "tokens": ["gen\u00b7den", "Rei\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.31": {"text": "Jeder gewinnenden Huld, hast du die Fein-", "tokens": ["Je\u00b7der", "ge\u00b7win\u00b7nen\u00b7den", "Huld", ",", "hast", "du", "die", "Fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.32": {"text": "din geschm\u00fcckt,", "tokens": ["din", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Ach, die s\u00fcsse Feindin \u2014 dich ", "tokens": ["Ach", ",", "die", "s\u00fcs\u00b7se", "Fein\u00b7din", "dich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$(", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.34": {"text": "g\u00f6nne,", "tokens": ["g\u00f6n\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Dass die ", "tokens": ["Dass", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "tende Herz.", "tokens": ["ten\u00b7de", "Herz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.37": {"text": "Ida, Ida, du bist so hold, wie die Schimmer in", "tokens": ["I\u00b7da", ",", "I\u00b7da", ",", "du", "bist", "so", "hold", ",", "wie", "die", "Schim\u00b7mer", "in"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "APPR"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Osten,", "tokens": ["Os\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Wie die Wies' im Thau bl\u00fchend und duf-", "tokens": ["Wie", "die", "Wies'", "im", "Thau", "bl\u00fc\u00b7hend", "und", "duf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "KON", "TRUNC"], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}, "line.40": {"text": "tend und frisch.", "tokens": ["tend", "und", "frisch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.41": {"text": "Blau ist dein Auge; die Locken sind rollende", "tokens": ["Blau", "ist", "dein", "Au\u00b7ge", ";", "die", "Lo\u00b7cken", "sind", "rol\u00b7len\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "NN"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.42": {"text": "Strahlen; die Blicke", "tokens": ["Strah\u00b7len", ";", "die", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Freundlicher Mondenblick. Siehe! so pflegt'", "tokens": ["Freund\u00b7li\u00b7cher", "Mon\u00b7den\u00b7blick", ".", "Sie\u00b7he", "!", "so", "pflegt'"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVIMP", "$.", "ADV", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.44": {"text": "ich das Bild", "tokens": ["ich", "das", "Bild"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Meiner Erkohrnen mir vorzuzaubern. Der Flor der", "tokens": ["Mei\u00b7ner", "Er\u00b7kohr\u00b7nen", "mir", "vor\u00b7zu\u00b7zau\u00b7bern", ".", "Der", "Flor", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$.", "ART", "NN", "ART"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.46": {"text": "Unschuld", "tokens": ["Un\u00b7schuld"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "H\u00fcllet das thauende Aug', h\u00fcllet die be-", "tokens": ["H\u00fcl\u00b7let", "das", "thau\u00b7en\u00b7de", "Aug'", ",", "h\u00fcl\u00b7let", "die", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.48": {"text": "bende Brust,", "tokens": ["ben\u00b7de", "Brust", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.49": {"text": "R\u00f6thet r\u00f6ther die Rose der Wangen, und mildert", "tokens": ["R\u00f6\u00b7thet", "r\u00f6\u00b7ther", "die", "Ro\u00b7se", "der", "Wan\u00b7gen", ",", "und", "mil\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "der Lippen", "tokens": ["der", "Lip\u00b7pen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Lispelnden Liebeslaut zu noch melodischerm", "tokens": ["Lis\u00b7peln\u00b7den", "Lie\u00b7bes\u00b7laut", "zu", "noch", "me\u00b7lo\u00b7di\u00b7scherm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADV", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.52": {"text": "Klang.", "tokens": ["Klang", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.53": {"text": "Meine Ida, so hab' ich dich funden. So siegtest", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "so", "hab'", "ich", "dich", "fun\u00b7den", ".", "So", "sieg\u00b7test"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.54": {"text": "du heimlich", "tokens": ["du", "heim\u00b7lich"], "token_info": ["word", "word"], "pos": ["PPER", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.55": {"text": "In der Unschuld Gewand \u00fcber mein siche-", "tokens": ["In", "der", "Un\u00b7schuld", "Ge\u00b7wand", "\u00fc\u00b7ber", "mein", "si\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.56": {"text": "res Herz.", "tokens": ["res", "Herz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.57": {"text": "Ach, zu sicheres Herz! wird dich auch Ida wohl", "tokens": ["Ach", ",", "zu", "si\u00b7che\u00b7res", "Herz", "!", "wird", "dich", "auch", "I\u00b7da", "wohl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "NE", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.58": {"text": "lieben?", "tokens": ["lie\u00b7ben", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.59": {"text": "Wird das Ihre, wie du, z\u00e4rtlich und lie-", "tokens": ["Wird", "das", "Ih\u00b7re", ",", "wie", "du", ",", "z\u00e4rt\u00b7lich", "und", "lie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "PPOSAT", "$,", "PWAV", "PPER", "$,", "ADJD", "KON", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.60": {"text": "bevoll seyn?", "tokens": ["be\u00b7voll", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.61": {"text": "Meine Ida, wirst du mich lieben? Dein schmach-", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "wirst", "du", "mich", "lie\u00b7ben", "?", "Dein", "schmach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "VAFIN", "PPER", "PRF", "VVINF", "$.", "PPOSAT", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.62": {"text": "tendes Auge", "tokens": ["ten\u00b7des", "Au\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.63": {"text": "Blicket, dein Busen bebt, dass du zu lieben", "tokens": ["Bli\u00b7cket", ",", "dein", "Bu\u00b7sen", "bebt", ",", "dass", "du", "zu", "lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.64": {"text": "vermagst.", "tokens": ["ver\u00b7magst", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.65": {"text": "Liebe, liebe mich denn! Mein Herz ist offen und", "tokens": ["Lie\u00b7be", ",", "lie\u00b7be", "mich", "denn", "!", "Mein", "Herz", "ist", "of\u00b7fen", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.66": {"text": "redlich,", "tokens": ["red\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.67": {"text": "Geistesweihe mein Werth, Harf' und Be-", "tokens": ["Geis\u00b7tes\u00b7wei\u00b7he", "mein", "Werth", ",", "Ha\u00b7rf'", "und", "Be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "NE", "KON", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.68": {"text": "geistrung mein Ruhm.", "tokens": ["geis\u00b7trung", "mein", "Ruhm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.69": {"text": "Liebe, liebe mich, Ida. Es ist der Tugenden", "tokens": ["Lie\u00b7be", ",", "lie\u00b7be", "mich", ",", "I\u00b7da", ".", "Es", "ist", "der", "Tu\u00b7gen\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "NE", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.70": {"text": "sch\u00f6nste,", "tokens": ["sch\u00f6ns\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.71": {"text": "Rein zu lieben, sein Herz einem Verdie-", "tokens": ["Rein", "zu", "lie\u00b7ben", ",", "sein", "Herz", "ei\u00b7nem", "Ver\u00b7die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.72": {"text": "nenden weihn.", "tokens": ["nen\u00b7den", "weihn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.73": {"text": "Liebe mich, meine Erw\u00e4hlte. Es ist die h\u00f6chste", "tokens": ["Lie\u00b7be", "mich", ",", "mei\u00b7ne", "Er\u00b7w\u00e4hl\u00b7te", ".", "Es", "ist", "die", "h\u00f6chs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.74": {"text": "der Wonnen,", "tokens": ["der", "Won\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.75": {"text": "Edel zu lieben, geliebt von dem Geliebten", "tokens": ["E\u00b7del", "zu", "lie\u00b7ben", ",", "ge\u00b7liebt", "von", "dem", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.76": {"text": "zu seyn.", "tokens": ["zu", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.77": {"text": "Ach, dass du mich liebtest! Wie sollt' in s\u00fcsser", "tokens": ["Ach", ",", "dass", "du", "mich", "lieb\u00b7test", "!", "Wie", "sollt'", "in", "s\u00fcs\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$.", "PWAV", "VMFIN", "APPR", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.78": {"text": "Umarmung", "tokens": ["Um\u00b7ar\u00b7mung"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.79": {"text": "Unsre Zukunft fliehn, fliehn in elysischer", "tokens": ["Uns\u00b7re", "Zu\u00b7kunft", "fliehn", ",", "fliehn", "in", "e\u00b7ly\u00b7si\u00b7scher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VVFIN", "APPR", "ADJA"], "meter": "+-+-++-+-++", "measure": "unknown.measure.septa"}, "line.80": {"text": "Ruh!", "tokens": ["Ruh", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.81": {"text": "Jeden sonnigen Lenztag wollten wir heiter ver-", "tokens": ["Je\u00b7den", "son\u00b7ni\u00b7gen", "Lenz\u00b7tag", "woll\u00b7ten", "wir", "hei\u00b7ter", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.