, "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.97": {"text": "Beyd' in Einem keuschen Kuss, in Einer Umar-", "tokens": ["Beyd'", "in", "Ei\u00b7nem", "keu\u00b7schen", "Kuss", ",", "in", "Ei\u00b7ner", "Um\u00b7a\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.98": {"text": "Zu den Liebenden \u00fcber den Sternen ent-", "tokens": ["Zu", "den", "Lie\u00b7ben\u00b7den", "\u00fc\u00b7ber", "den", "Ster\u00b7nen", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}}}}}