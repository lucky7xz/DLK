{"textgrid.poem.24158": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Des alten Weibleins Lied vom Schwager Tod", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4hrt ein Postillion durchs Land, \u2013", "tokens": ["Es", "f\u00e4hrt", "ein", "Pos\u00b7til\u00b7li\u00b7on", "durchs", "Land", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Oh, der ist h\u00f6flich und galant!", "tokens": ["Oh", ",", "der", "ist", "h\u00f6f\u00b7lich", "und", "ga\u00b7lant", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimmt alte Leute bei der Hand,", "tokens": ["Nimmt", "al\u00b7te", "Leu\u00b7te", "bei", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hilft ihnen in den Wagen.", "tokens": ["Hilft", "ih\u00b7nen", "in", "den", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Will keinen Lohn,", "tokens": ["Will", "kei\u00b7nen", "Lohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "F\u00e4hrt schnell davon;", "tokens": ["F\u00e4hrt", "schnell", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wohin \u2013 will er nicht sagen.", "tokens": ["Wo\u00b7hin", "\u2013", "will", "er", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Die Peitsche knallt,", "tokens": ["Die", "Peit\u00b7sche", "knallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herr Schwager, halt,", "tokens": ["Herr", "Schwa\u00b7ger", ",", "halt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Seht her, hier steht noch Eine;", "tokens": ["Seht", "her", ",", "hier", "steht", "noch", "Ei\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut fahr ich mit,", "tokens": ["Heut", "fahr", "ich", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ob Trab, ob Schritt,", "tokens": ["Ob", "Trab", ",", "ob", "Schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn m\u00fcd sind meine Beine.", "tokens": ["Denn", "m\u00fcd", "sind", "mei\u00b7ne", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dank, Schwager! So! Und nun fahrt zu,", "tokens": ["Dank", ",", "Schwa\u00b7ger", "!", "So", "!", "Und", "nun", "fahrt", "zu", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADV", "$.", "KON", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blast euer Liedel trututu!", "tokens": ["Blast", "eu\u00b7er", "Lie\u00b7del", "tru\u00b7tu\u00b7tu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es geht zum Schlafen, geht zur Ruh,", "tokens": ["Es", "geht", "zum", "Schla\u00b7fen", ",", "geht", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es geht ins Endlich-gute;", "tokens": ["Es", "geht", "ins", "End\u00b7lich\u00b7gu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lebt wohl! Ade!", "tokens": ["Lebt", "wohl", "!", "A\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mir ist nicht weh,", "tokens": ["Mir", "ist", "nicht", "weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Nur wunderm\u00fcd zu Mute.", "tokens": ["Nur", "wun\u00b7der\u00b7m\u00fcd", "zu", "Mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD