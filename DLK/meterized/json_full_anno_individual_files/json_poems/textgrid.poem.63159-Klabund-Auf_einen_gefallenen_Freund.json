{"textgrid.poem.63159": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gefallenen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arm in Arm sind wir gegangen", "tokens": ["Arm", "in", "Arm", "sind", "wir", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das Himmelreich der Welt.", "tokens": ["Durch", "das", "Him\u00b7mel\u00b7reich", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Lasso haben wir gefangen", "tokens": ["Mit", "dem", "Las\u00b7so", "ha\u00b7ben", "wir", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00f6ne Frauen, die wie Rehe sprangen", "tokens": ["Sch\u00f6\u00b7ne", "Frau\u00b7en", ",", "die", "wie", "Re\u00b7he", "spran\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "KOKOM", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und wir wehten segelnd auf dem Belt.", "tokens": ["Und", "wir", "weh\u00b7ten", "se\u00b7gelnd", "auf", "dem", "Belt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und in Stunden, die wie Schleier glitten,", "tokens": ["Und", "in", "Stun\u