{"dta.poem.7580": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Weite denn des Lebens und die Enge?", "tokens": ["Was", "ist", "die", "Wei\u00b7te", "denn", "des", "Le\u00b7bens", "und", "die", "En\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weit machst du dir's allein, eng macht es dir die Menge.", "tokens": ["Weit", "machst", "du", "dir's", "al\u00b7lein", ",", "eng", "macht", "es", "dir", "die", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "ADV", "$,", "ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer aus dem dunkeln Ich noch nicht hinaus gekommen,", "tokens": ["Wer", "aus", "dem", "dun\u00b7keln", "Ich", "noch", "nicht", "hin\u00b7aus", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "PPER", "ADV", "PTKNEG", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der f\u00fchlt sich eng in sich, dem mag die Weite frommen.", "tokens": ["Der", "f\u00fchlt", "sich", "eng", "in", "sich", ",", "dem", "mag", "die", "Wei\u00b7te", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "PRF", "$,", "PDS", "VMFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch aus dem Aeu\u00dfern wer gelangt zum hellen Ich,", "tokens": ["Doch", "aus", "dem", "A\u00b7e\u00b7u\u00b7\u00dfern", "wer", "ge\u00b7langt", "zum", "hel\u00b7len", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PWS", "VVFIN", "APPRART", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dem ist die Weite leer, der f\u00fchlt sich voll in sich.", "tokens": ["Dem", "ist", "die", "Wei\u00b7te", "leer", ",", "der", "f\u00fchlt", "sich", "voll", "in", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$,", "PRELS", "VVFIN", "PRF", "ADJD", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich treibt verworrner Drang ins wirrende Gedr\u00e4nge;", "tokens": ["Dich", "treibt", "ver\u00b7worr\u00b7ner", "Drang", "ins", "wir\u00b7ren\u00b7de", "Ge\u00b7dr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Einheit nur ist viel, und wenig ist die Menge.", "tokens": ["Die", "Ein\u00b7heit", "nur", "ist", "viel", ",", "und", "we\u00b7nig", "ist", "die", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "$,", "KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Einheit ist nicht viel, sie selber ist das All;", "tokens": ["Die", "Ein\u00b7heit", "ist", "nicht", "viel", ",", "sie", "sel\u00b7ber", "ist", "das", "All", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "$,", "PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Meng' ist wenig nicht, nichts ist der ganze Schwall.", "tokens": ["Die", "Meng'", "ist", "we\u00b7nig", "nicht", ",", "nichts", "ist", "der", "gan\u00b7ze", "Schwall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "$,", "PIS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das All und Eine hat ein Weiser im Allein;", "tokens": ["Das", "All", "und", "Ei\u00b7ne", "hat", "ein", "Wei\u00b7ser", "im", "Al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Allgemeine selbst ist ohne All gemein.", "tokens": ["Das", "All\u00b7ge\u00b7mei\u00b7ne", "selbst", "ist", "oh\u00b7ne", "All", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}