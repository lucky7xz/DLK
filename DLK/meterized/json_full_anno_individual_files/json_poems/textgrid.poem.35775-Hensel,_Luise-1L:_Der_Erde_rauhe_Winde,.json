{"textgrid.poem.35775": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Erde rauhe Winde,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Erde rauhe Winde,", "tokens": ["Der", "Er\u00b7de", "rau\u00b7he", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie thun dem armen Kinde,", "tokens": ["Sie", "thun", "dem", "ar\u00b7men", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Vater! gar zu weh.", "tokens": ["O", "Va\u00b7ter", "!", "gar", "zu", "weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dort oben ist es linde,", "tokens": ["Dort", "o\u00b7ben", "ist", "es", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da ist kein Sturm, kein Schnee.", "tokens": ["Da", "ist", "kein", "Sturm", ",", "kein", "Schnee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich zieht ein stetes Sehnen", "tokens": ["Mich", "zieht", "ein", "ste\u00b7tes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach jenen reinern T\u00f6nen,", "tokens": ["Nach", "je\u00b7nen", "rei\u00b7nern", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach jenem hellern Licht;", "tokens": ["Nach", "je\u00b7nem", "hel\u00b7lern", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die schmerzenvollen Thr\u00e4nen", "tokens": ["Die", "schmer\u00b7zen\u00b7vol\u00b7len", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Versiegen ewig nicht.", "tokens": ["Ver\u00b7sie\u00b7gen", "e\u00b7wig", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das kalte Erdenleben", "tokens": ["Das", "kal\u00b7te", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann mir doch gar nicht geben,", "tokens": ["Kann", "mir", "doch", "gar", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was dieses Sehnen hemmt.", "tokens": ["Was", "die\u00b7ses", "Seh\u00b7nen", "hemmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O la\u00df mich aufw\u00e4rts schweben!", "tokens": ["O", "la\u00df", "mich", "auf\u00b7w\u00e4rts", "schwe\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier wird mir's gar zu fremd.", "tokens": ["Hier", "wird", "mir's", "gar", "zu", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Woll'st, Vater! Deinen Reinen", "tokens": ["Woll'st", ",", "Va\u00b7ter", "!", "Dei\u00b7nen", "Rei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die M\u00fcde bald vereinen;", "tokens": ["Die", "M\u00fc\u00b7de", "bald", "ver\u00b7ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier kann ich nichts mehr thun.", "tokens": ["Hier", "kann", "ich", "nichts", "mehr", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen, matt vom Weinen,", "tokens": ["Die", "Au\u00b7gen", ",", "matt", "vom", "Wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die la\u00df im Grabe ruhn!", "tokens": ["Die", "la\u00df", "im", "Gra\u00b7be", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Erde rauhe Winde,", "tokens": ["Der", "Er\u00b7de", "rau\u00b7he"