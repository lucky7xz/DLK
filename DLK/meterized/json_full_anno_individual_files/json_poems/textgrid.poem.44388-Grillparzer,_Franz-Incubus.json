{"textgrid.poem.44388": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Incubus", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fragst du mich, wie er hei\u00dft,", "tokens": ["Fragst", "du", "mich", ",", "wie", "er", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jener finstere Geist,", "tokens": ["Je\u00b7ner", "fins\u00b7te\u00b7re", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der meine Brust hat zum Reich,", "tokens": ["Der", "mei\u00b7ne", "Brust", "hat", "zum", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Davon ich so d\u00fcster und bleich?", "tokens": ["Da\u00b7von", "ich", "so", "d\u00fcs\u00b7ter", "und", "bleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Unfried ist er genennt,", "tokens": ["Un\u00b7fried", "ist", "er", "ge\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weil er den Frieden nicht kennt,", "tokens": ["Weil", "er", "den", "Frie\u00b7den", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Weil er den Frieden nicht g\u00f6nnt", "tokens": ["Weil", "er", "den", "Frie\u00b7den", "nicht", "g\u00f6nnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Jemals der Brust, wo er brennt.", "tokens": ["Je\u00b7mals", "der", "Brust", ",", "wo", "er", "brennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Der hat im Busen sein Reich,", "tokens": ["Der", "hat", "im", "Bu\u00b7sen", "sein", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der macht mich d\u00fcster und bleich,", "tokens": ["Der", "macht", "mich", "d\u00fcs\u00b7ter", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der l\u00e4\u00dft mir nimmermehr Rast,", "tokens": ["Der", "l\u00e4\u00dft", "mir", "nim\u00b7mer\u00b7mehr", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Seit er mich einmal gefa\u00dft.", "tokens": ["Seit", "er", "mich", "ein\u00b7mal", "ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau ich zum Himmel empor,", "tokens": ["Schau", "ich", "zum", "Him\u00b7mel", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Lagert er br\u00fctend sich vor,", "tokens": ["La\u00b7gert", "er", "br\u00fc\u00b7tend", "sich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Zeiget mir Wolken zur Hand,", "tokens": ["Zei\u00b7get", "mir", "Wol\u00b7ken", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wolken \u2013 und keinen Bestand.", "tokens": ["Wol\u00b7ken", "\u2013", "und", "kei\u00b7nen", "Be\u00b7stand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "PIAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Alles der Menschen Gew\u00fchl", "tokens": ["Al\u00b7les", "der", "Men\u00b7schen", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nennt er Getrieb ohne Ziel;", "tokens": ["Nennt", "er", "Ge\u00b7trieb", "oh\u00b7ne", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ob ichs auch anders gewu\u00dft,", "tokens": ["Ob", "ichs", "auch", "an\u00b7ders", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Schweigt er das Haupt durch die Brust.", "tokens": ["Schweigt", "er", "das", "Haupt", "durch", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Fl\u00fccht ich zu ihr, die mein Gl\u00fcck,", "tokens": ["Fl\u00fccht", "ich", "zu", "ihr", ",", "die", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Tadellos jeglichem Blick;", "tokens": ["Ta\u00b7del\u00b7los", "jeg\u00b7li\u00b7chem", "Blick", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Er findet Tadel mir auf,", "tokens": ["Er", "fin\u00b7det", "Ta\u00b7del", "mir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "W\u00e4rs aus der H\u00f6lle herauf;", "tokens": ["W\u00e4rs", "aus", "der", "H\u00f6l\u00b7le", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Und auf den Punkt, den er meint,", "tokens": ["Und", "auf", "den", "Punkt", ",", "den", "er", "meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "H\u00e4lt er die Lichter vereint,", "tokens": ["H\u00e4lt", "er", "die", "Lich\u00b7ter", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df es dem Aug nicht entging,", "tokens": ["Da\u00df", "es", "dem", "Aug", "nicht", "ent\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wenn es auch Blindheit umfing:", "tokens": ["Wenn", "es", "auch", "Blind\u00b7heit", "um\u00b7fing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Lacht sie \u2013 so nennt er sie leicht;", "tokens": ["Lacht", "sie", "\u2013", "so", "nennt", "er", "sie", "leicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Weint sie \u2013 von Schuld wohl erweicht!", "tokens": ["Weint", "sie", "\u2013", "von", "Schuld", "wohl", "er\u00b7weicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Spricht sie \u2013 in heuchelndem Mut,", "tokens": ["Spricht", "sie", "\u2013", "in", "heu\u00b7cheln\u00b7dem", "Mut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Schweigt sie \u2013 voll anderer Glut.", "tokens": ["Schweigt", "sie", "\u2013", "voll", "an\u00b7de\u00b7rer", "Glut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und wenns mir einmal gelang,", "tokens": ["Und", "wenns", "mir", "ein\u00b7mal", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Durchzubrechen den Drang,", "tokens": ["Durch\u00b7zu\u00b7bre\u00b7chen", "den", "Drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Frei, mit des Geistes Gewalt,", "tokens": ["Frei", ",", "mit", "des", "Geis\u00b7tes", "Ge\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Durch, bis zu Licht und Gestalt:", "tokens": ["Durch", ",", "bis", "zu", "Licht", "und", "Ge\u00b7stalt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Unter der Hand es sich bildet und hebt,", "tokens": ["Un\u00b7ter", "der", "Hand", "es", "sich", "bil\u00b7det", "und", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Lebendiges Leben das Tote belebt,", "tokens": ["Le\u00b7ben\u00b7di\u00b7ges", "Le\u00b7ben", "das", "To\u00b7te", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und es nun dasteht, ein atmendes Bild,", "tokens": ["Und", "es", "nun", "das\u00b7teht", ",", "ein", "at\u00b7men\u00b7des", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Vom Geiste des All und des Bildners erf\u00fcllt;", "tokens": ["Vom", "Geis\u00b7te", "des", "All", "und", "des", "Bild\u00b7ners", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Da stiehlt er hinein sich mit listgem Bemerk", "tokens": ["Da", "stiehlt", "er", "hin\u00b7ein", "sich", "mit", "list\u00b7gem", "Be\u00b7merk"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und grinset mich an aus dem eigenen Werk:", "tokens": ["Und", "grin\u00b7set", "mich", "an", "aus", "dem", "ei\u00b7ge\u00b7nen", "Werk", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbbins, Meister, nur ich, dem die Wohnung du w\u00f6lbst,", "tokens": ["\u00bb", "bins", ",", "Meis\u00b7ter", ",", "nur", "ich", ",", "dem", "die", "Woh\u00b7nung", "du", "w\u00f6lbst", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "NN", "$,", "ADV", "PPER", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sieh, nichtig dein Werklein und nichtig du selbst!\u00ab", "tokens": ["Sieh", ",", "nich\u00b7tig", "dein", "Wer\u00b7klein", "und", "nich\u00b7tig", "du", "selbst", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "ADJD", "PPOSAT", "NN", "KON", "ADJD", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.12": {"line.1": {"text": "Und schaudernd seh ichs, entsetzenbet\u00f6rt,", "tokens": ["Und", "schau\u00b7dernd", "seh", "ichs", ",", "ent\u00b7set\u00b7zen\u00b7be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie mein eigenes Selbst gen mich sich emp\u00f6rt,", "tokens": ["Wie", "mein", "ei\u00b7ge\u00b7nes", "Selbst", "gen", "mich", "sich", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "PRF", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Verw\u00fcnsche mein Werk und mich selber ins Grab;", "tokens": ["Ver\u00b7w\u00fcn\u00b7sche", "mein", "Werk", "und", "mich", "sel\u00b7ber", "ins", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann folgt er auch dahin wohl qu\u00e4lend hinab?", "tokens": ["Dann", "folgt", "er", "auch", "da\u00b7hin", "wohl", "qu\u00e4\u00b7lend", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.13": {"line.1": {"text": "Fragst du mich, wie er hei\u00dft,", "tokens": ["Fragst", "du", "mich", ",", "wie", "er", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Jener finstere Geist,", "tokens": ["Je\u00b7ner", "fins\u00b7te\u00b7re", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der meine Brust hat zum Reich,", "tokens": ["Der", "mei\u00b7ne", "Brust", "hat", "zum", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Davon ich so d\u00fcster und bleich?", "tokens": ["Da\u00b7von", "ich", "so", "d\u00fcs\u00b7ter", "und", "bleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Unfried ist er genennt,", "tokens": ["Un\u00b7fried", "ist", "er", "ge\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weil er den Frieden nicht kennt,", "tokens": ["Weil", "er", "den", "Frie\u00b7den", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", 