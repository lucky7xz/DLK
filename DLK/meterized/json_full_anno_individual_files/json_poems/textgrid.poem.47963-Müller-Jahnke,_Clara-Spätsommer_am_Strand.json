{"textgrid.poem.47963": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4tsommer am Strand", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da weht von S\u00fcd ein sanfter Hauch", "tokens": ["Da", "weht", "von", "S\u00fcd", "ein", "sanf\u00b7ter", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus sonnenlichten Tagen;", "tokens": ["aus", "son\u00b7nen\u00b7lich\u00b7ten", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "die goldbelaubten Aeste dehnt", "tokens": ["die", "gold\u00b7be\u00b7laub\u00b7ten", "A\u00b7es\u00b7te", "dehnt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "der Ahorn voll Behagen.", "tokens": ["der", "A\u00b7horn", "voll", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein Vogelsang, \u2013 kein Bl\u00fctenduft, \u2013", "tokens": ["Kein", "Vo\u00b7gel\u00b7sang", ",", "\u2013", "kein", "Bl\u00fc\u00b7ten\u00b7duft", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "$(", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die weiche, warme Sommerluft", "tokens": ["die", "wei\u00b7che", ",", "war\u00b7me", "Som\u00b7mer\u00b7luft"], "token_info": ["word", "word", "punct", "word", "word"], "pos":