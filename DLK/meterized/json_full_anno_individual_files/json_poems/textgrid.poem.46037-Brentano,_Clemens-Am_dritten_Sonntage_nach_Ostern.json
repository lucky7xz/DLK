{"textgrid.poem.46037": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnicht mehr, und dann wiedersehen", "tokens": ["\u00bb", "nicht", "mehr", ",", "und", "dann", "wie\u00b7der\u00b7se\u00b7hen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ADV", "$,", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollt ihr mich, nach kleiner Zeit,", "tokens": ["Sollt", "ihr", "mich", ",", "nach", "klei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich mu\u00df zum Vater gehen.\u00ab", "tokens": ["Denn", "ich", "mu\u00df", "zum", "Va\u00b7ter", "ge\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner wu\u00dft', was dies bedeut'.", "tokens": ["Kei\u00b7ner", "wu\u00dft'", ",", "was", "dies", "be\u00b7deut'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und er sprach: \u00bbIhr werdet weinen,", "tokens": ["Und", "er", "sprach", ":", "\u00bb", "Ihr", "wer\u00b7det", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Welt wird sich erfreun,", "tokens": ["Doch", "die", "Welt", "wird", "sich", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Aber Freude wird erscheinen", "tokens": ["A\u00b7ber", "Freu\u00b7de", "wird", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den betr\u00fcbten Freunden mein.", "tokens": ["Den", "be\u00b7tr\u00fcb\u00b7ten", "Freun\u00b7den", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Stunde ist gekommen,", "tokens": ["Wenn", "die", "Stun\u00b7de", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trauert die Geb\u00e4rerin;", "tokens": ["Trau\u00b7ert", "die", "Ge\u00b7b\u00e4\u00b7re\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ist das Kind gewonnen,", "tokens": ["A\u00b7ber", "ist", "das", "Kind", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllet Freude ihren Sinn.", "tokens": ["F\u00fcl\u00b7let", "Freu\u00b7de", "ih\u00b7ren", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt seid traurig, jetzt in Z\u00e4hren,", "tokens": ["Jetzt", "seid", "trau\u00b7rig", ",", "jetzt", "in", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch ich werd' euch wiedersehn,", "tokens": ["Doch", "ich", "werd'", "euch", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann wird Freude zu euch kehren,", "tokens": ["Dann", "wird", "Freu\u00b7de", "zu", "euch", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer wieder von euch gehn.\u00ab", "tokens": ["Nim\u00b7mer", "wie\u00b7der", "von", "euch", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie hat uns der Herr geliebet,", "tokens": ["Wie", "hat", "uns", "der", "Herr", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie bereitet er uns vor!", "tokens": ["Wie", "be\u00b7rei\u00b7tet", "er", "uns", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig trauert, wen betr\u00fcbet,", "tokens": ["Se\u00b7lig", "trau\u00b7ert", ",", "wen", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df den Heiland er verlor.", "tokens": ["Da\u00df", "den", "Hei\u00b7land", "er", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn nach Reue und Bekennen", "tokens": ["Denn", "nach", "Reu\u00b7e", "und", "Be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehrt er liebvoll uns zur\u00fcck;", "tokens": ["Kehrt", "er", "lieb\u00b7voll", "uns", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, sich nimmer von ihm trennen,", "tokens": ["O", ",", "sich", "nim\u00b7mer", "von", "ihm", "tren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r' das allerh\u00f6chste Gl\u00fcck!", "tokens": ["W\u00e4r'", "das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbnicht mehr, und dann wiedersehen", "tokens": ["\u00bb", "nicht", "mehr", ",", "und", "dann", "wie\u00b7