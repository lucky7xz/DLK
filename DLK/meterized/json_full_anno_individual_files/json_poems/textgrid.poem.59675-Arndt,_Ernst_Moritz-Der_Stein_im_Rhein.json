 "line.3": {"text": "\u00bbstell' auf die Knaben! Alle herbei!", "tokens": ["\u00bb", "stell'", "auf", "die", "Kna\u00b7ben", "!", "Al\u00b7le", "her\u00b7bei", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$.", "PIS", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df ich sehe, welcher der reisigste sei.\u00ab", "tokens": ["Da\u00df", "ich", "se\u00b7he", ",", "wel\u00b7cher", "der", "rei\u00b7sigs\u00b7te", "sei", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "ART", "ADJA", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie standen, und ich sprach: \u00bbEuer Rhein", "tokens": ["Sie", "stan\u00b7den", ",", "und", "ich", "sprach", ":", "\u00bb", "Eu\u00b7er", "Rhein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "$(", "PPOSAT", "NE"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mu\u00df ewig Deutschlands Herrlichkeit sein;", "tokens": ["Mu\u00df", "e\u00b7wig", "Deutschlands", "Herr\u00b7lich\u00b7keit", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihr misset's, und euer frischestes Blut", "tokens": ["Ihr", "miss\u00b7et's", ",", "und", "eu\u00b7er", "fri\u00b7sches\u00b7tes", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr solchen Preis sei es keinem zu gut.\u00ab", "tokens": ["F\u00fcr", "sol\u00b7chen", "Preis", "sei", "es", "kei\u00b7nem", "zu", "gut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "PIS", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da trat der kleinste wohl aus dem Chor,", "tokens": ["Da", "trat", "der", "kleins\u00b7te", "wohl", "aus", "dem", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein stolzer Freiwilliger, leuchtend hervor,", "tokens": ["Ein", "stol\u00b7zer", "Frei\u00b7wil\u00b7li\u00b7ger", ",", "leuch\u00b7tend", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schlug in des Feldherrn Ehrenhand", "tokens": ["Schlug", "in", "des", "Feld\u00b7herrn", "Eh\u00b7ren\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den edlen Willen rasch ein als Pfand.", "tokens": ["Den", "ed\u00b7len", "Wil\u00b7len", "rasch", "ein", "als", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "KOUS", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Er hat's gehalten, er ward der Hort,", "tokens": ["Er", "hat's", "ge\u00b7hal\u00b7ten", ",", "er", "ward", "der", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPE