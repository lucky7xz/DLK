{"textgrid.poem.53327": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin ja, Herr, in deiner Macht,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ja, Herr, in deiner Macht,", "tokens": ["Ich", "bin", "ja", ",", "Herr", ",", "in", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast mich an die\u00df Licht gebracht,", "tokens": ["Du", "hast", "mich", "an", "die\u00df", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du unterh\u00e4ltst mir auch das Leben,", "tokens": ["Du", "un\u00b7ter\u00b7h\u00e4ltst", "mir", "auch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du kennest meiner Monden-Zahl,", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7ner", "Mon\u00b7den\u00b7Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weist wenn ich diesem Jammerthal", "tokens": ["Weist", "wenn", "ich", "die\u00b7sem", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wieder gutte Nacht mus geben,", "tokens": ["Auch", "wie\u00b7der", "gut\u00b7te", "Nacht", "mus", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo, wie und wann ich sterben soll", "tokens": ["Wo", ",", "wie", "und", "wann", "ich", "ster\u00b7ben", "soll"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "KON", "PWAV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das weist du, Vater, mehr als woll.", "tokens": ["Das", "weist", "du", ",", "Va\u00b7ter", ",", "mehr", "als", "woll", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$,", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wen hab' ich nun als dich allein,", "tokens": ["Wen", "hab'", "ich", "nun", "als", "dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir in meiner letzten Pein", "tokens": ["Der", "mir", "in", "mei\u00b7ner", "letz\u00b7ten", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Trost und Raht wei\u00df zu zu springen?", "tokens": ["Mit", "Trost", "und", "Raht", "wei\u00df", "zu", "zu", "sprin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer nimmt sich meiner Seelen an", "tokens": ["Wer", "nimmt", "sich", "mei\u00b7ner", "See\u00b7len", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nun mein Leben nichts mehr kan,", "tokens": ["Wenn", "nun", "mein", "Le\u00b7ben", "nichts", "mehr", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PIS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd ich mus mit dem Tode ringen,", "tokens": ["Vnd", "ich", "mus", "mit", "dem", "To\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn aller Sinnen Krafft gebricht,", "tokens": ["Wenn", "al\u00b7ler", "Sin\u00b7nen", "Krafft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Thust du es GOTT, mein Heiland, nicht?", "tokens": ["Thust", "du", "es", "GoTT", ",", "mein", "Hei\u00b7land", ",", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NE", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich d\u00fcnckt, da lieg' ich schon vor mir", "tokens": ["Mich", "d\u00fcnckt", ",", "da", "lieg'", "ich", "schon", "vor", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In grosser Hitz ohn Krafft, ohn Zier", "tokens": ["In", "gros\u00b7ser", "Hitz", "ohn", "Krafft", ",", "ohn", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit h\u00f6chster Hertzens-Angst befallen,", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "Hert\u00b7zens\u00b7Angst", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geh\u00f6r und Rede nehmen ab,", "tokens": ["Ge\u00b7h\u00f6r", "und", "Re\u00b7de", "neh\u00b7men", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Augen werden mir ein Grab,", "tokens": ["Die", "Au\u00b7gen", "wer\u00b7den", "mir", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch kr\u00e4nckt die S\u00fcnde mich fur allen:", "tokens": ["Doch", "kr\u00e4nckt", "die", "S\u00fcn\u00b7de", "mich", "fur", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Sathans Anklag' hat nicht Rhu,", "tokens": ["Des", "Sa\u00b7thans", "An\u00b7klag'", "hat", "nicht", "Rhu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Setzt mir auch mit Versuchung zu.", "tokens": ["Setzt", "mir", "auch", "mit", "Ver\u00b7su\u00b7chung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6re der Posaunen Thon", "tokens": ["Ich", "h\u00f6\u00b7re", "der", "Po\u00b7sau\u00b7nen", "Thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd seh' auch den Gerichts-Tag schon,", "tokens": ["Vnd", "seh'", "auch", "den", "Ge\u00b7richts\u00b7Tag", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mir auch wird ein Vrtheil f\u00e4llen,", "tokens": ["Der", "mir", "auch", "wird", "ein", "Vrtheil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier weiset mein Gewissens-Buch", "tokens": ["Hier", "wei\u00b7set", "mein", "Ge\u00b7wis\u00b7sens\u00b7Buch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da aber des Gesetzes Fluch", "tokens": ["Da", "a\u00b7ber", "des", "Ge\u00b7set\u00b7zes", "Fluch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich S\u00fcnden-Kind hinab zur Hellen,", "tokens": ["Mich", "S\u00fcn\u00b7den\u00b7Kind", "hin\u00b7ab", "zur", "Hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da wo man ewig, ewig: Leid!", "tokens": ["Da", "wo", "man", "e\u00b7wig", ",", "e\u00b7wig", ":", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PWAV", "PIS", "ADJD", "$,", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mord! Jammer! Angst! vnd Zetter schreyt!", "tokens": ["Mord", "!", "Jam\u00b7mer", "!", "Angst", "!", "vnd", "Zet\u00b7ter", "schreyt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein Gold und Gut errettet mich,", "tokens": ["Kein", "Gold", "und", "Gut", "er\u00b7ret\u00b7tet", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vmbsonst erbeut ein Bruder sich", "tokens": ["Vmbsonst", "er\u00b7beut", "ein", "Bru\u00b7der", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den andern hie erst loh\u00df zu machen,", "tokens": ["Den", "an\u00b7dern", "hie", "erst", "loh\u00df", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er mus es ewig lassen stehn,", "tokens": ["Er", "mus", "es", "e\u00b7wig", "las\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir werden ewig nicht entgehn", "tokens": ["Wir", "wer\u00b7den", "e\u00b7wig", "nicht", "ent\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kriegt einmal uns des Todes Rachen,", "tokens": ["Kriegt", "ein\u00b7mal", "uns", "des", "To\u00b7des", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer hilfft mir sonst in solcher Noht,", "tokens": ["Wer", "hilfft", "mir", "sonst", "in", "sol\u00b7cher", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo du nicht GOTT, du Todes-Todt?", "tokens": ["Wo", "du", "nicht", "GoTT", ",", "du", "To\u00b7des\u00b7Todt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "NE", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Teuffel hat nicht Macht an mir,", "tokens": ["Der", "Teuf\u00b7fel", "hat", "nicht", "Macht", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe blo\u00df ges\u00fcndigt dir,", "tokens": ["Ich", "ha\u00b7be", "blo\u00df", "ge\u00b7s\u00fcn\u00b7digt", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, der du Missethat vergiebest,", "tokens": ["Dir", ",", "der", "du", "Mis\u00b7se\u00b7that", "ver\u00b7gie\u00b7best", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was masst sich Sathan dessen an", "tokens": ["Was", "masst", "sich", "Sa\u00b7than", "des\u00b7sen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "NE", "PDS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der kein Gesetz mir geben kan,", "tokens": ["Der", "kein", "Ge\u00b7setz", "mir", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts hat an dem, was du, Herr, liebest,", "tokens": ["Nichts", "hat", "an", "dem", ",", "was", "du", ",", "Herr", ",", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "$,", "PWS", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er nehme das, was sein ist, hin,", "tokens": ["Er", "neh\u00b7me", "das", ",", "was", "sein", "ist", ",", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "VAINF", "VAFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich weis da\u00df ich des Herren bin.", "tokens": ["Ich", "weis", "da\u00df", "ich", "des", "Her\u00b7ren", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Jesu, ich dein thewres Gut", "tokens": ["Herr", "Je\u00b7su", ",", "ich", "dein", "thew\u00b7res", "Gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bezeug' es mit selbs deinem Blut,", "tokens": ["Be\u00b7zeug'", "es", "mit", "selbs", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich der S\u00fcnden nicht geh\u00f6re,", "tokens": ["Da\u00df", "ich", "der", "S\u00fcn\u00b7den", "nicht", "ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was schont denn Sathan meiner nicht", "tokens": ["Was", "schont", "denn", "Sa\u00b7than", "mei\u00b7ner", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "NN", "PPOSAT", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd schreckt mich durch das Zorn-Gericht?", "tokens": ["Vnd", "schreckt", "mich", "durch", "das", "Zorn\u00b7Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, rette deines Leidens-Ehre,", "tokens": ["Komm", ",", "ret\u00b7te", "dei\u00b7nes", "Lei\u00b7dens\u00b7Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was giebest du mich frembder Handt", "tokens": ["Was", "gie\u00b7best", "du", "mich", "fremb\u00b7der", "Handt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd hast so viel an mich gewandt?", "tokens": ["Vnd", "hast", "so", "viel", "an", "mich", "ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nein, nein, ich weis gewi\u00df, mein Heil,", "tokens": ["Nein", ",", "nein", ",", "ich", "weis", "ge\u00b7wi\u00df", ",", "mein", "Heil", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "PTKVZ", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du l\u00e4ssest mich dein wahres Theil", "tokens": ["Du", "l\u00e4s\u00b7sest", "mich", "dein", "wah\u00b7res", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu tieff in deinen Wunden sitzen,", "tokens": ["Zu", "tieff", "in", "dei\u00b7nen", "Wun\u00b7den", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hie lach' ich aller Macht und Noht,", "tokens": ["Hie", "lach'", "ich", "al\u00b7ler", "Macht", "und", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es mag Gesetz, Hell' oder Todt", "tokens": ["Es", "mag", "Ge\u00b7setz", ",", "Hell'", "o\u00b7der", "Todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auff mich her donnern oder blitzen,", "tokens": ["Auff", "mich", "her", "don\u00b7nern", "o\u00b7der", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APZR", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dieweil ich lebte war ich dein,", "tokens": ["Die\u00b7weil", "ich", "leb\u00b7te", "war", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jetzt kan ich keines frembden seyn.", "tokens": ["Jetzt", "kan", "ich", "kei\u00b7nes", "fremb\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich bin ja, Herr, in deiner Macht,", "tokens": ["Ich", "bin", "ja", ",", "Herr", ",", "in", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast mich an die\u00df Licht gebracht,", "tokens": ["Du", "hast", "mich", "an", "die\u00df", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du unterh\u00e4ltst mir auch das Leben,", "tokens": ["Du", "un\u00b7ter\u00b7h\u00e4ltst", "mir", "auch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du kennest meiner Monden-Zahl,", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7ner", "Mon\u00b7den\u00b7Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weist wenn ich diesem Jammerthal", "tokens": ["Weist", "wenn", "ich", "die\u00b7sem", "Jam\u00b7mer\u00b7thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wieder gutte Nacht mus geben,", "tokens": ["Auch", "wie\u00b7der", "gut\u00b7te", "Nacht", "mus", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo, wie und wann ich sterben soll", "tokens": ["Wo", ",", "wie", "und", "wann", "ich", "ster\u00b7ben", "soll"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "KON", "PWAV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das weist du, Vater, mehr als woll.", "tokens": ["Das", "weist", "du", ",", "Va\u00b7ter", ",", "mehr", "als", "woll", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$,", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wen hab' ich nun als dich allein,", "tokens": ["Wen", "hab'", "ich", "nun", "als", "dich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir in meiner letzten Pein", "tokens": ["Der", "mir", "in", "mei\u00b7ner", "letz\u00b7ten", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Trost und Raht wei\u00df zu zu springen?", "tokens": ["Mit", "Trost", "und", "Raht", "wei\u00df", "zu", "zu", "sprin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer nimmt sich meiner Seelen an", "tokens": ["Wer", "nimmt", "sich", "mei\u00b7ner", "See\u00b7len", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nun mein Leben nichts mehr kan,", "tokens": ["Wenn", "nun", "mein", "Le\u00b7ben", "nichts", "mehr", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PIS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd ich mus mit dem Tode ringen,", "tokens": ["Vnd", "ich", "mus", "mit", "dem", "To\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn aller Sinnen Krafft gebricht,", "tokens": ["Wenn", "al\u00b7ler", "Sin\u00b7nen", "Krafft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Thust du es GOTT, mein Heiland, nicht?", "tokens": ["Thust", "du", "es", "GoTT", ",", "mein", "Hei\u00b7land", ",", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NE", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mich d\u00fcnckt, da lieg' ich schon vor mir", "tokens": ["Mich", "d\u00fcnckt", ",", "da", "lieg'", "ich", "schon", "vor", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In grosser Hitz ohn Krafft, ohn Zier", "tokens": ["In", "gros\u00b7ser", "Hitz", "ohn", "Krafft", ",", "ohn", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit h\u00f6chster Hertzens-Angst befallen,", "tokens": ["Mit", "h\u00f6chs\u00b7ter", "Hert\u00b7zens\u00b7Angst", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geh\u00f6r und Rede nehmen ab,", "tokens": ["Ge\u00b7h\u00f6r", "und", "Re\u00b7de", "neh\u00b7men", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Augen werden mir ein Grab,", "tokens": ["Die", "Au\u00b7gen", "wer\u00b7den", "mir", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch kr\u00e4nckt die S\u00fcnde mich fur allen:", "tokens": ["Doch", "kr\u00e4nckt", "die", "S\u00fcn\u00b7de", "mich", "fur", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Sathans Anklag' hat nicht Rhu,", "tokens": ["Des", "Sa\u00b7thans", "An\u00b7klag'", "hat", "nicht", "Rhu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Setzt mir auch mit Versuchung zu.", "tokens": ["Setzt", "mir", "auch", "mit", "Ver\u00b7su\u00b7chung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich h\u00f6re der Posaunen Thon", "tokens": ["Ich", "h\u00f6\u00b7re", "der", "Po\u00b7sau\u00b7nen", "Thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd seh' auch den Gerichts-Tag schon,", "tokens": ["Vnd", "seh'", "auch", "den", "Ge\u00b7richts\u00b7Tag", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mir auch wird ein Vrtheil f\u00e4llen,", "tokens": ["Der", "mir", "auch", "wird", "ein", "Vrtheil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier weiset mein Gewissens-Buch", "tokens": ["Hier", "wei\u00b7set", "mein", "Ge\u00b7wis\u00b7sens\u00b7Buch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da aber des Gesetzes Fluch", "tokens": ["Da", "a\u00b7ber", "des", "Ge\u00b7set\u00b7zes", "Fluch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich S\u00fcnden-Kind hinab zur Hellen,", "tokens": ["Mich", "S\u00fcn\u00b7den\u00b7Kind", "hin\u00b7ab", "zur", "Hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da wo man ewig, ewig: Leid!", "tokens": ["Da", "wo", "man", "e\u00b7wig", ",", "e\u00b7wig", ":", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PWAV", "PIS", "ADJD", "$,", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mord! Jammer! Angst! vnd Zetter schreyt!", "tokens": ["Mord", "!", "Jam\u00b7mer", "!", "Angst", "!", "vnd", "Zet\u00b7ter", "schreyt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Kein Gold und Gut errettet mich,", "tokens": ["Kein", "Gold", "und", "Gut", "er\u00b7ret\u00b7tet", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vmbsonst erbeut ein Bruder sich", "tokens": ["Vmbsonst", "er\u00b7beut", "ein", "Bru\u00b7der", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den andern hie erst loh\u00df zu machen,", "tokens": ["Den", "an\u00b7dern", "hie", "erst", "loh\u00df", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er mus es ewig lassen stehn,", "tokens": ["Er", "mus", "es", "e\u00b7wig", "las\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir werden ewig nicht entgehn", "tokens": ["Wir", "wer\u00b7den", "e\u00b7wig", "nicht", "ent\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kriegt einmal uns des Todes Rachen,", "tokens": ["Kriegt", "ein\u00b7mal", "uns", "des", "To\u00b7des", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer hilfft mir sonst in solcher Noht,", "tokens": ["Wer", "hilfft", "mir", "sonst", "in", "sol\u00b7cher", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo du nicht GOTT, du Todes-Todt?", "tokens": ["Wo", "du", "nicht", "GoTT", ",", "du", "To\u00b7des\u00b7Todt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "NE", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Teuffel hat nicht Macht an mir,", "tokens": ["Der", "Teuf\u00b7fel", "hat", "nicht", "Macht", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe blo\u00df ges\u00fcndigt dir,", "tokens": ["Ich", "ha\u00b7be", "blo\u00df", "ge\u00b7s\u00fcn\u00b7digt", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, der du Missethat vergiebest,", "tokens": ["Dir", ",", "der", "du", "Mis\u00b7se\u00b7that", "ver\u00b7gie\u00b7best", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was masst sich Sathan dessen an", "tokens": ["Was", "masst", "sich", "Sa\u00b7than", "des\u00b7sen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "NE", "PDS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der kein Gesetz mir geben kan,", "tokens": ["Der", "kein", "Ge\u00b7setz", "mir", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts hat an dem, was du, Herr, liebest,", "tokens": ["Nichts", "hat", "an", "dem", ",", "was", "du", ",", "Herr", ",", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "$,", "PWS", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er nehme das, was sein ist, hin,", "tokens": ["Er", "neh\u00b7me", "das", ",", "was", "sein", "ist", ",", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "VAINF", "VAFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich weis da\u00df ich des Herren bin.", "tokens": ["Ich", "weis", "da\u00df", "ich", "des", "Her\u00b7ren", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Herr Jesu, ich dein thewres Gut", "tokens": ["Herr", "Je\u00b7su", ",", "ich", "dein", "thew\u00b7res", "Gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bezeug' es mit selbs deinem Blut,", "tokens": ["Be\u00b7zeug'", "es", "mit", "selbs", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich der S\u00fcnden nicht geh\u00f6re,", "tokens": ["Da\u00df", "ich", "der", "S\u00fcn\u00b7den", "nicht", "ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was schont denn Sathan meiner nicht", "tokens": ["Was", "schont", "denn", "Sa\u00b7than", "mei\u00b7ner", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "NN", "PPOSAT", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd schreckt mich durch das Zorn-Gericht?", "tokens": ["Vnd", "schreckt", "mich", "durch", "das", "Zorn\u00b7Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, rette deines Leidens-Ehre,", "tokens": ["Komm", ",", "ret\u00b7te", "dei\u00b7nes", "Lei\u00b7dens\u00b7Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was giebest du mich frembder Handt", "tokens": ["Was", "gie\u00b7best", "du", "mich", "fremb\u00b7der", "Handt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd hast so viel an mich gewandt?", "tokens": ["Vnd", "hast", "so", "viel", "an", "mich", "ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Nein, nein, ich weis gewi\u00df, mein Heil,", "tokens": ["Nein", ",", "nein", ",", "ich", "weis", "ge\u00b7wi\u00df", ",", "mein", "Heil", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "PTKVZ", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du l\u00e4ssest mich dein wahres Theil", "tokens": ["Du", "l\u00e4s\u00b7sest", "mich", "dein", "wah\u00b7res", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu tieff in deinen Wunden sitzen,", "tokens": ["Zu", "tieff", "in", "dei\u00b7nen", "Wun\u00b7den", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hie lach' ich aller Macht und Noht,", "tokens": ["Hie", "lach'", "ich", "al\u00b7ler", "Macht", "und", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es mag Gesetz, Hell' oder Todt", "tokens": ["Es", "mag", "Ge\u00b7setz", ",", "Hell'", "o\u00b7der", "Todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auff mich her donnern oder blitzen,", "tokens": ["Auff", "mich", "her", "don\u00b7nern", "o\u00b7der", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APZR", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dieweil ich lebte war ich dein,", "tokens": ["Die\u00b7weil", "ich", "leb\u00b7te", "war", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jetzt kan ich keines frembden seyn.", "tokens": ["Jetzt", "kan", "ich", "kei\u00b7nes", "fremb\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}