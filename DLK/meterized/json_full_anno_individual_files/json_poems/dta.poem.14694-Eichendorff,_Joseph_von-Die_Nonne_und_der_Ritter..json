{"dta.poem.14694": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nonne und der Ritter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Geht ein Schiff, ein Mann stand drinne \u2014", "tokens": ["Geht", "ein", "Schiff", ",", "ein", "Mann", "stand", "drin\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Falsche Nacht, verwirrst die Sinne,", "tokens": ["Fal\u00b7sche", "Nacht", ",", "ver\u00b7wirrst", "die", "Sin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welt, Ade! Gott woll' bewahren,", "tokens": ["Welt", ",", "A\u00b7de", "!", "Gott", "woll'", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die noch irr im Dunkeln fahren.", "tokens": ["Die", "noch", "irr", "im", "Dun\u00b7keln", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}