{"dta.poem.16291": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Eigensucht ist nicht, nicht Theil an Andern nehmen;", "tokens": ["Die", "Ei\u00b7gen\u00b7sucht", "ist", "nicht", ",", "nicht", "Theil", "an", "An\u00b7dern", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "$,", "PTKNEG", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn dazu mu\u00df sich doch, wer auch nicht will, bequemen.", "tokens": ["Denn", "da\u00b7zu", "mu\u00df", "sich", "doch", ",", "wer", "auch", "nicht", "will", ",", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PRF", "ADV", "$,", "PWS", "ADV", "PTKNEG", "VMFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Eigens\u00fcchtige nimmt Theil an Gl\u00fcck und Leid;", "tokens": ["Der", "Ei\u00b7gen\u00b7s\u00fcch\u00b7ti\u00b7ge", "nimmt", "Theil", "an", "Gl\u00fcck", "und", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn dieses macht ihm Lust, und jenes macht ihm Neid.", "tokens": ["Denn", "die\u00b7ses", "macht", "ihm", "Lust", ",", "und", "je\u00b7nes", "macht", "ihm", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "NN", "$,", "KON", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Eigensucht ist nur, annehmen solchen Schein", "tokens": ["Die", "Ei\u00b7gen\u00b7sucht", "ist", "nur", ",", "an\u00b7neh\u00b7men", "sol\u00b7chen", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Theilnahmlosigkeit, als g\u00e4lt' ihr alles klein.", "tokens": ["Von", "Theil\u00b7nahm\u00b7lo\u00b7sig\u00b7keit", ",", "als", "g\u00e4lt'", "ihr", "al\u00b7les", "klein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}