{"dta.poem.23642": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nur in Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Frankreich und dem B\u00f6hmerwald,", "tokens": ["Zwi\u00b7schen", "Fran\u00b7kreich", "und", "dem", "B\u00f6h\u00b7mer\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ART", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da wachsen unsre Reben.", "tokens": ["Da", "wach\u00b7sen", "uns\u00b7re", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gr\u00fc\u00df mein Lieb am gr\u00fcnen Rhein,", "tokens": ["Gr\u00fc\u00df", "mein", "Lieb", "am", "gr\u00fc\u00b7nen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df mir meinen k\u00fchlen Wein!", "tokens": ["Gr\u00fc\u00df", "mir", "mei\u00b7nen", "k\u00fch\u00b7len", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur in Deutschland :,:", "tokens": ["Nur", "in", "Deutschland", ":", ",", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "$,", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da will ich ewig leben.", "tokens": ["Da", "will", "ich", "e\u00b7wig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fern in fremden Landen war ich auch,", "tokens": ["Fern", "in", "frem\u00b7den", "Lan\u00b7den", "war", "ich", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bald bin ich heimgegangen.", "tokens": ["Bald", "bin", "ich", "heim\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hei\u00dfe Luft und Durst dabei,", "tokens": ["Hei\u00b7\u00dfe", "Luft", "und", "Durst", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Qual und Sorgen mancherlei \u2014", "tokens": ["Qual", "und", "Sor\u00b7gen", "man\u00b7cher\u00b7lei"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIAT", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur nach Deutschland :,:", "tokens": ["Nur", "nach", "Deutschland", ":", ",", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "$,", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Th\u00e4t mein Herz verlangen.", "tokens": ["Th\u00e4t", "mein", "Herz", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ist ein Land, es hei\u00dft Italia,", "tokens": ["Ist", "ein", "Land", ",", "es", "hei\u00dft", "I\u00b7ta\u00b7lia", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Bl\u00fchn Orangen und Citronen.", "tokens": ["Bl\u00fchn", "O\u00b7ran\u00b7gen", "und", "Cit\u00b7ro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe! sprach die R\u00f6merin,", "tokens": ["Sin\u00b7ge", "!", "sprach", "die", "R\u00f6\u00b7me\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sang zum Norden hin:", "tokens": ["Und", "ich", "sang", "zum", "Nor\u00b7den", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur in Deutschland :,:", "tokens": ["Nur", "in", "Deutschland", ":", ",", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "$,", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da mu\u00df mein Sch\u00e4tzlein wohnen.", "tokens": ["Da", "mu\u00df", "mein", "Sch\u00e4tz\u00b7lein", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als ich sah die Alpen wieder gl\u00fchn", "tokens": ["Als", "ich", "sah", "die", "Al\u00b7pen", "wie\u00b7der", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hell in der Morgensonne:", "tokens": ["Hell", "in", "der", "Mor\u00b7gen\u00b7son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gr\u00fc\u00df mein Liebchen, goldner Schein,", "tokens": ["Gr\u00fc\u00df", "mein", "Lieb\u00b7chen", ",", "gold\u00b7ner", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df mir meinen gr\u00fcnen Rhein!", "tokens": ["Gr\u00fc\u00df", "mir", "mei\u00b7nen", "gr\u00fc\u00b7nen", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur in Deutschland :,:", "tokens": ["Nur", "in", "Deutschland", ":", ",", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "$,", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da wohnet Freud' und Wonne.", "tokens": ["Da", "woh\u00b7net", "Freud'", "und", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}