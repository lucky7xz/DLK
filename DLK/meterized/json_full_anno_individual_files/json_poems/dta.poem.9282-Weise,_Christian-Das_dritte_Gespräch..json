{"dta.poem.9282": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.71", "nl:0.28"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wuchert ja nicht in gedancken", "tokens": ["Wu\u00b7chert", "ja", "nicht", "in", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhr verliebten seelen ihr:", "tokens": ["Ihr", "ver\u00b7lieb\u00b7ten", "see\u00b7len", "ihr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in diesem engen schrancken", "tokens": ["Denn", "in", "die\u00b7sem", "en\u00b7gen", "schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uberk\u00f6mmt man keine zier.", "tokens": ["Ub\u00b7er\u00b7k\u00f6mmt", "man", "kei\u00b7ne", "zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weg mit diesem hertzeleide/", "tokens": ["Weg", "mit", "die\u00b7sem", "hert\u00b7ze\u00b7lei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausser uns besteht die freude.", "tokens": ["Aus\u00b7ser", "uns", "be\u00b7steht", "die", "freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die gedancken sind nur wind/", "tokens": ["Die", "ge\u00b7dan\u00b7cken", "sind", "nur", "wind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie noch so k\u00f6stlich sind.", "tokens": ["Wenn", "sie", "noch", "so", "k\u00f6st\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Manche stunde wird zu schanden/", "tokens": ["Man\u00b7che", "stun\u00b7de", "wird", "zu", "schan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancher sch\u00f6ner tag verdirbt/", "tokens": ["Man\u00b7cher", "sch\u00f6\u00b7ner", "tag", "ver\u00b7dirbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil man in den eitlen banden", "tokens": ["Weil", "man", "in", "den", "eit\u00b7len", "ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts als solchen rauch erwirbt.", "tokens": ["Nichts", "als", "sol\u00b7chen", "rauch", "er\u00b7wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da gedenckt man um die wette/", "tokens": ["Da", "ge\u00b7denckt", "man", "um", "die", "wet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich k\u00f6nte/ wenn ich h\u00e4tte/", "tokens": ["Wenn", "ich", "k\u00f6n\u00b7te", "/", "wenn", "ich", "h\u00e4t\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$(", "KOUS", "PPER", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und je mehr man denckt und dicht/", "tokens": ["Und", "je", "mehr", "man", "denckt", "und", "dicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "VVFIN", "KON", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Desto wen\u2019ger wird verricht.", "tokens": ["Des\u00b7to", "wen'\u00b7ger", "wird", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Lernt einmahl zur sache greiffen/", "tokens": ["Lernt", "ein\u00b7mahl", "zur", "sa\u00b7che", "greif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da besteht der rechte lohn.", "tokens": ["Da", "be\u00b7steht", "der", "rech\u00b7te", "lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche sich also verschweiffen/", "tokens": ["Wel\u00b7che", "sich", "al\u00b7so", "ver\u00b7schwei\u00b7ffen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sind f\u00fcrwahr noch weit davon.", "tokens": ["Sind", "f\u00fcr\u00b7wahr", "noch", "weit", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die armen patienten", "tokens": ["Wenn", "die", "ar\u00b7men", "pa\u00b7ti\u00b7en\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht den sitz der kranckheit nennten/", "tokens": ["Nicht", "den", "sitz", "der", "kran\u00b7ck\u00b7heit", "nenn\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "W\u00fcrde mancher kr\u00e4uter frucht", "tokens": ["W\u00fcr\u00b7de", "man\u00b7cher", "kr\u00e4u\u00b7ter", "frucht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch den artzt umsonst gesucht.", "tokens": ["Durch", "den", "artzt", "um\u00b7sonst", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Gibt ein kauffmann vor die waaren", "tokens": ["Gibt", "ein", "kauf\u00b7fmann", "vor", "die", "waa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht das allerbeste geld?", "tokens": ["Nicht", "das", "al\u00b7ler\u00b7bes\u00b7te", "geld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum was wolt ihr also sparen/", "tokens": ["Drum", "was", "wolt", "ihr", "al\u00b7so", "spa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn euch eine zier gef\u00e4llt.", "tokens": ["Wenn", "euch", "ei\u00b7ne", "zier", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrcklich und gewisse gaben", "tokens": ["W\u00fcrck\u00b7lich", "und", "ge\u00b7wis\u00b7se", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollen gleiches kauff-geld haben/", "tokens": ["Wol\u00b7len", "glei\u00b7ches", "kauff\u00b7geld", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die arbeit mu\u00df allein", "tokens": ["Und", "die", "ar\u00b7beit", "mu\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In der that bekr\u00e4fftigt seyn.", "tokens": ["In", "der", "that", "be\u00b7kr\u00e4ff\u00b7tigt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ach ihr ungewissen grillen", "tokens": ["Ach", "ihr", "un\u00b7ge\u00b7wis\u00b7sen", "gril\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was bem\u00fcht ihr euren sinn", "tokens": ["Was", "be\u00b7m\u00fcht", "ihr", "eu\u00b7ren", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um der allersch\u00f6nsten willen?", "tokens": ["Um", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "wil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht und suchet den gewinn", "tokens": ["Geht", "und", "su\u00b7chet", "den", "ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus besuchen/ spielen/ schertzen:", "tokens": ["Aus", "be\u00b7su\u00b7chen", "/", "spie\u00b7len", "/", "schert\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "VVINF", "$(", "VVINF", "$(", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie vergn\u00fcget eure hertzen.", "tokens": ["Sie", "ver\u00b7gn\u00fc\u00b7get", "eu\u00b7re", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer die boltzen anders richt/", "tokens": ["Wer", "die", "bolt\u00b7zen", "an\u00b7ders", "richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Siht das ziel und trifft doch nicht.", "tokens": ["Siht", "das", "ziel", "und", "trifft", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}