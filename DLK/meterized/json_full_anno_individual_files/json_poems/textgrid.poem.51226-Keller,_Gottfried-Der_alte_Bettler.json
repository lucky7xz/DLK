{"textgrid.poem.51226": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Bettler", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun legst du, alte wetterm\u00fcde F\u00f6hre,", "tokens": ["Nun", "legst", "du", ",", "al\u00b7te", "wet\u00b7ter\u00b7m\u00fc\u00b7de", "F\u00f6h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den allerletzten Jahresring dir an,", "tokens": ["Den", "al\u00b7ler\u00b7letz\u00b7ten", "Jah\u00b7res\u00b7ring", "dir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da ich im Walde schon rumoren h\u00f6re", "tokens": ["Da", "ich", "im", "Wal\u00b7de", "schon", "ru\u00b7mo\u00b7ren", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seiner Axt den grauen Zimmermann.", "tokens": ["Mit", "sei\u00b7ner", "Axt", "den", "grau\u00b7en", "Zim\u00b7mer\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er wird sowenig deinen Kopf begnaden,", "tokens": ["Er", "wird", "so\u00b7we\u00b7nig", "dei\u00b7nen", "Kopf", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als jemand \u00fcber mein Verschwinden klagt;", "tokens": ["Als", "je\u00b7mand", "\u00fc\u00b7ber", "mein", "Ver\u00b7schwin\u00b7den", "klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem armen Schelm und einem alten Schaden", "tokens": ["Dem", "ar\u00b7men", "Schelm", "und", "ei\u00b7nem", "al\u00b7ten", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur wird des Alters Ehrenzoll versagt!", "tokens": ["Nur", "wird", "des", "Al\u00b7ters", "Eh\u00b7ren\u00b7zoll", "ver\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sei's immerhin! ich liebe drum nicht minder", "tokens": ["Sei's", "im\u00b7mer\u00b7hin", "!", "ich", "lie\u00b7be", "drum", "nicht", "min\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "PPER", "VVFIN", "PAV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies alte Land, mein gutes Vaterland,", "tokens": ["Dies", "al\u00b7te", "Land", ",", "mein", "gu\u00b7tes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und segne seine lebensfrohen Kinder", "tokens": ["Und", "seg\u00b7ne", "sei\u00b7ne", "le\u00b7bens\u00b7fro\u00b7hen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit der verworfnen toten Bettlerhand!", "tokens": ["Mit", "der", "ver\u00b7worf\u00b7nen", "to\u00b7ten", "Bett\u00b7ler\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich segne euch, o Strom, Gebirg und Auen,", "tokens": ["Ich", "seg\u00b7ne", "euch", ",", "o", "Strom", ",", "Ge\u00b7birg", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ihr im Lichte heiter vor mir schwimmt!", "tokens": ["Die", "ihr", "im", "Lich\u00b7te", "hei\u00b7ter", "vor", "mir", "schwimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Reichtum ist dies selig klare Schauen,", "tokens": ["Ein", "Reich\u00b7tum", "ist", "dies", "se\u00b7lig", "kla\u00b7re", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den meinem Aug nicht Vogt noch Richter nimmt!", "tokens": ["Den", "mei\u00b7nem", "Aug", "nicht", "Vogt", "noch", "Rich\u00b7ter", "nimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als meine Br\u00fcder einst vor vierzig Jahren", "tokens": ["Als", "mei\u00b7ne", "Br\u00fc\u00b7der", "einst", "vor", "vier\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das schiefe, morsche Vaterhaus verkauft,", "tokens": ["Das", "schie\u00b7fe", ",", "mor\u00b7sche", "Va\u00b7ter\u00b7haus", "ver\u00b7kauft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um nach der fernen Neuen Welt zu fahren,", "tokens": ["Um", "nach", "der", "fer\u00b7nen", "Neu\u00b7en", "Welt", "zu", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo man sich mit der alten Erde rauft,", "tokens": ["Wo", "man", "sich", "mit", "der", "al\u00b7ten", "Er\u00b7de", "rauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da bin ich ganz allein zur\u00fcckgeblieben,", "tokens": ["Da", "bin", "ich", "ganz", "al\u00b7lein", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald war ich um mein kleines Erb geprellt;", "tokens": ["Bald", "war", "ich", "um", "mein", "klei\u00b7nes", "Erb", "ge\u00b7prellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wei\u00df nicht, wie weit sie dr\u00fcben es getrieben:", "tokens": ["Wei\u00df", "nicht", ",", "wie", "weit", "sie", "dr\u00fc\u00b7ben", "es", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich wurd ein Hauptmann in der Bettler Welt!", "tokens": ["Ich", "wurd", "ein", "Haupt\u00b7mann", "in", "der", "Bett\u00b7ler", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn weder Not noch M\u00fchsal konnten scheiden", "tokens": ["Denn", "we\u00b7der", "Not", "noch", "M\u00fch\u00b7sal", "konn\u00b7ten", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KON", "NN", "ADV", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich von den Marken meines Vaterlands \u2013", "tokens": ["Mich", "von", "den", "Mar\u00b7ken", "mei\u00b7nes", "Va\u00b7ter\u00b7lands", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer will mich zwingen, seinen Scho\u00df zu meiden,", "tokens": ["Wer", "will", "mich", "zwin\u00b7gen", ",", "sei\u00b7nen", "Scho\u00df", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu missen seiner Str\u00f6me blauen Glanz?", "tokens": ["Zu", "mis\u00b7sen", "sei\u00b7ner", "Str\u00f6\u00b7me", "blau\u00b7en", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier will ich wandeln, wo ich bin geboren,", "tokens": ["Hier", "will", "ich", "wan\u00b7deln", ",", "wo", "ich", "bin", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sei's auch in zerri\u00dfnen Bettlerschuhn!", "tokens": ["Und", "sei's", "auch", "in", "zer\u00b7ri\u00df\u00b7nen", "Bett\u00b7ler\u00b7schuhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ging drob die B\u00fcrgerehre mir verloren,", "tokens": ["Ging", "drob", "die", "B\u00fcr\u00b7ge\u00b7reh\u00b7re", "mir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich will und mu\u00df bei meinen V\u00e4tern ruhn!", "tokens": ["Ich", "will", "und", "mu\u00df", "bei", "mei\u00b7nen", "V\u00e4\u00b7tern", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dich sollt ich fliehen, trautes Netz der Wege,", "tokens": ["Dich", "sollt", "ich", "flie\u00b7hen", ",", "trau\u00b7tes", "Netz", "der", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Daran auch ich mit flei\u00df'gen F\u00fc\u00dfen spann,", "tokens": ["Da\u00b7ran", "auch", "ich", "mit", "flei\u00df'\u00b7gen", "F\u00fc\u00b7\u00dfen", "spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dich, Gebirg, wo ich des Abgrunds Stege", "tokens": ["Und", "dich", ",", "Ge\u00b7birg", ",", "wo", "ich", "des", "Ab\u00b7grunds", "Ste\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fast mit verbundnem Aug beschreiten kann?", "tokens": ["Fast", "mit", "ver\u00b7bund\u00b7nem", "Aug", "be\u00b7schrei\u00b7ten", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo ich den Fuchs und seinen Vater kenne", "tokens": ["Wo", "ich", "den", "Fuchs", "und", "sei\u00b7nen", "Va\u00b7ter", "ken\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NE", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jeden Stamm im dunklen Forst gez\u00e4hlt", "tokens": ["Und", "je\u00b7den", "Stamm", "im", "dunk\u00b7len", "Forst", "ge\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und jede Trift bei ihrem Namen nenne \u2013", "tokens": ["Und", "je\u00b7de", "Trift", "bei", "ih\u00b7rem", "Na\u00b7men", "nen\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Boden, wo mir nie ein Tritt gefehlt?", "tokens": ["Den", "Bo\u00b7den", ",", "wo", "mir", "nie", "ein", "Tritt", "ge\u00b7fehlt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O gute Scholle meiner Heimaterde,", "tokens": ["O", "gu\u00b7te", "Schol\u00b7le", "mei\u00b7ner", "Hei\u00b7ma\u00b7ter\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie kriech ich gern in deinen warmen Scho\u00df!", "tokens": ["Wie", "kriech", "ich", "gern", "in", "dei\u00b7nen", "war\u00b7men", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir ahnet schon, wie sanft ich ruhen werde,", "tokens": ["Mir", "ah\u00b7net", "schon", ",", "wie", "sanft", "ich", "ru\u00b7hen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Kaun des Brots und allem Irrsal los!", "tokens": ["Vom", "Kaun", "des", "Brots", "und", "al\u00b7lem", "Irr\u00b7sal", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie will ich meine m\u00fcden Beine strecken,", "tokens": ["Wie", "will", "ich", "mei\u00b7ne", "m\u00fc\u00b7den", "Bei\u00b7ne", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wegwerfend meines Elends d\u00fcrren Stab,", "tokens": ["Weg\u00b7wer\u00b7fend", "mei\u00b7nes", "E\u00b7lends", "d\u00fcr\u00b7ren", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie langhin mich von West nach Osten recken,", "tokens": ["Wie", "lang\u00b7hin", "mich", "von", "West", "nach", "Os\u00b7ten", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "NE", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als l\u00e4g ich stolz in eines K\u00f6nigs Grab!", "tokens": ["Als", "l\u00e4g", "ich", "stolz", "in", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch spinnt sich weiter meiner Seele Leben,", "tokens": ["Doch", "spinnt", "sich", "wei\u00b7ter", "mei\u00b7ner", "See\u00b7le", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00f6ge sie im leichten Nebelkleid,", "tokens": ["So", "m\u00f6\u00b7ge", "sie", "im", "leich\u00b7ten", "Ne\u00b7bel\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So leicht wie Luft, dies laute Volk umschweben,", "tokens": ["So", "leicht", "wie", "Luft", ",", "dies", "lau\u00b7te", "Volk", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$,", "PDS", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch immer treu in Freude, Zorn und Leid!", "tokens": ["Noch", "im\u00b7mer", "treu", "in", "Freu\u00b7de", ",", "Zorn", "und", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "M\u00f6cht meine Seligkeit darin bestehen,", "tokens": ["M\u00f6cht", "mei\u00b7ne", "Se\u00b7lig\u00b7keit", "da\u00b7rin", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Einst seines letzten Bettlers Geist zu sein,", "tokens": ["Einst", "sei\u00b7nes", "letz\u00b7ten", "Bett\u00b7lers", "Geist", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zufrieden, still und m\u00fc\u00dfig umzugehen", "tokens": ["Zu\u00b7frie\u00b7den", ",", "still", "und", "m\u00fc\u00b7\u00dfig", "um\u00b7zu\u00b7ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In seines Gl\u00fcckes hellem Sonnenschein!", "tokens": ["In", "sei\u00b7nes", "Gl\u00fc\u00b7ckes", "hel\u00b7lem", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun legst du, alte wetterm\u00fcde F\u00f6hre,", "tokens": ["Nun", "legst", "du", ",", "al\u00b7te", "wet\u00b7ter\u00b7m\u00fc\u00b7de", "F\u00f6h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den allerletzten Jahresring dir an,", "tokens": ["Den", "al\u00b7ler\u00b7letz\u00b7ten", "Jah\u00b7res\u00b7ring", "dir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da ich im Walde schon rumoren h\u00f6re", "tokens": ["Da", "ich", "im", "Wal\u00b7de", "schon", "ru\u00b7mo\u00b7ren", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seiner Axt den grauen Zimmermann.", "tokens": ["Mit", "sei\u00b7ner", "Axt", "den", "grau\u00b7en", "Zim\u00b7mer\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er wird sowenig deinen Kopf begnaden,", "tokens": ["Er", "wird", "so\u00b7we\u00b7nig", "dei\u00b7nen", "Kopf", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als jemand \u00fcber mein Verschwinden klagt;", "tokens": ["Als", "je\u00b7mand", "\u00fc\u00b7ber", "mein", "Ver\u00b7schwin\u00b7den", "klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem armen Schelm und einem alten Schaden", "tokens": ["Dem", "ar\u00b7men", "Schelm", "und", "ei\u00b7nem", "al\u00b7ten", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur wird des Alters Ehrenzoll versagt!", "tokens": ["Nur", "wird", "des", "Al\u00b7ters", "Eh\u00b7ren\u00b7zoll", "ver\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sei's immerhin! ich liebe drum nicht minder", "tokens": ["Sei's", "im\u00b7mer\u00b7hin", "!", "ich", "lie\u00b