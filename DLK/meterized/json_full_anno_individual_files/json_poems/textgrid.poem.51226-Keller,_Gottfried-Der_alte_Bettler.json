er": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fast mit verbundnem Aug beschreiten kann?", "tokens": ["Fast", "mit", "ver\u00b7bund\u00b7nem", "Aug", "be\u00b7schrei\u00b7ten", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo ich den Fuchs und seinen Vater kenne", "tokens": ["Wo", "ich", "den", "Fuchs", "und", "sei\u00b7nen", "Va\u00b7ter", "ken\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NE", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jeden Stamm im dunklen Forst gez\u00e4hlt", "tokens": ["Und", "je\u00b7den", "Stamm", "im", "dunk\u00b7len", "Forst", "ge\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und jede Trift bei ihrem Namen nenne \u2013", "tokens": ["Und", "je\u00b7de", "Trift", "bei", "ih\u00b7rem", "Na\u00b7men", "nen\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Boden, wo mir nie ein Tritt gefehlt?", "tokens": ["Den", "Bo\u00b7den", ",", "wo", "mir", "nie", "ein", "Tritt", "ge\u00b7fehlt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "O gute Scholle meiner Heimaterde,", "tokens": ["O", "gu\u00b7te", "Schol\u00b7le", "mei\u00