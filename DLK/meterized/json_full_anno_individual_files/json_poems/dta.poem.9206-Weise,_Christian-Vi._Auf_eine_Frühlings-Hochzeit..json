{"dta.poem.9206": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n Auf eine Fr\u00fchlings-Hochzeit.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Haben sich die s\u00fcssen stunden", "tokens": ["Ha\u00b7ben", "sich", "die", "s\u00fcs\u00b7sen", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem winter eingefunden/", "tokens": ["Nach", "dem", "win\u00b7ter", "ein\u00b7ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt da alles lebt und lacht/", "tokens": ["Jetzt", "da", "al\u00b7les", "lebt", "und", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solt ihr nun vergn\u00fcget werden/", "tokens": ["Solt", "ihr", "nun", "ver\u00b7gn\u00fc\u00b7get", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da die zier der gantzen erden/", "tokens": ["Da", "die", "zier", "der", "gant\u00b7zen", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allenthalben hochzeit macht?", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "hoch\u00b7zeit", "macht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Sucht einander liebzukosen/", "tokens": ["Sucht", "ein\u00b7an\u00b7der", "lieb\u00b7zu\u00b7ko\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die s\u00fcssen fr\u00fchlings-rosen", "tokens": ["Weil", "die", "s\u00fcs\u00b7sen", "fr\u00fch\u00b7lings\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem jungen stocke stehn/", "tokens": ["An", "dem", "jun\u00b7gen", "sto\u00b7cke", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ihr mit verliebten f\u00fcssen", "tokens": ["Weil", "ihr", "mit", "ver\u00b7lieb\u00b7ten", "f\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt auff lilgen und narcissen", "tokens": ["K\u00f6nnt", "auff", "lil\u00b7gen", "und", "nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu der lust spatziren gehn.", "tokens": ["Zu", "der", "lust", "spat\u00b7zi\u00b7ren", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}