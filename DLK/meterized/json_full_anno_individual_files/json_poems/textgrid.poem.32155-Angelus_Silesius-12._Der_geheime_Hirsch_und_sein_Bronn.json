{"textgrid.poem.32155": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "12. Der geheime Hirsch und sein Bronn", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hirsch, der lauft und sucht ein k\u00fchles Br\u00fcnnelein,", "tokens": ["Der", "Hirsch", ",", "der", "lauft", "und", "sucht", "ein", "k\u00fch\u00b7les", "Br\u00fcn\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit sein Herz erquickt und ruhig m\u00f6ge sein.", "tokens": ["Da\u00b7mit", "sein", "Herz", "er\u00b7quickt", "und", "ru\u00b7hig", "m\u00f6\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVPP", "KON", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seele, die Gott liebt, die eilet zu dem Bronnen,", "tokens": ["Die", "See\u00b7le", ",", "die", "Gott", "liebt", ",", "die", "ei\u00b7let", "zu", "dem", "Bron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus dem der s\u00fc\u00dfe Bach des Lebens kommt geronnen.", "tokens": ["Aus", "dem", "der", "s\u00fc\u00b7\u00dfe", "Bach", "des", "Le\u00b7bens", "kommt", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Bronn ist Jesus Christ, der uns mit seinem Quall", "tokens": ["Der", "Bronn", "ist", "Je\u00b7sus", "Christ", ",", "der", "uns", "mit", "sei\u00b7nem", "Quall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "NE", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im wahren Glauben tr\u00e4nkt und st\u00e4rkt vor S\u00fcndenfall.", "tokens": ["Im", "wah\u00b7ren", "Glau\u00b7ben", "tr\u00e4nkt", "und", "st\u00e4rkt", "vor", "S\u00fcn\u00b7den\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bleibst du bei diesem Quall und trinkst oft aus dem Bronnen,", "tokens": ["Bleibst", "du", "bei", "die\u00b7sem", "Quall", "und", "trinkst", "oft", "aus", "dem", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hast du, meine Seel, ganz seliglich gewonnen. ", "tokens": ["So", "hast", "du", ",", "mei\u00b7ne", "Seel", ",", "ganz", "se\u00b7lig\u00b7lich", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2":