{"textgrid.poem.60283": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Unentbehrliche", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt ich tausendfach mich teilen,", "tokens": ["K\u00f6nnt", "ich", "tau\u00b7send\u00b7fach", "mich", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell mit allen Winden eilen,", "tokens": ["Schnell", "mit", "al\u00b7len", "Win\u00b7den", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberall zugleich zu walten,", "tokens": ["\u00dc\u00b7be\u00b7rall", "zu\u00b7gleich", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo's die Welt gilt zu gestalten!", "tokens": ["Wo's", "die", "Welt", "gilt", "zu", "ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcrden nicht durch meine Kr\u00e4fte", "tokens": ["W\u00fcr\u00b7den", "nicht", "durch", "mei\u00b7ne", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rasch gedeihn der Zeit Gesch\u00e4fte?", "tokens": ["Rasch", "ge\u00b7deihn", "der", "Zeit", "Ge\u00b7sch\u00e4f\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch, so l\u00e4\u00dft mich mein Geschick", "tokens": ["Doch", ",", "so", "l\u00e4\u00dft", "mich", "mein", "Ge\u00b7schick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schauen nur im Zeitungsblick", "tokens": ["Schau\u00b7en", "nur", "im", "Zei\u00b7tungs\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ohne mich in fernen Reichen", "tokens": ["Oh\u00b7ne", "mich", "in", "fer\u00b7nen", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die verla\u00dfnen V\u00f6lker schleichen! \u2013", "tokens": ["Die", "ver\u00b7la\u00df\u00b7nen", "V\u00f6l\u00b7ker", "schlei\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Von den Sternen m\u00f6cht ich wissen,", "tokens": ["Von", "den", "Ster\u00b7nen", "m\u00f6cht", "ich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ob sie mich nicht schwer vermissen?", "tokens": ["Ob", "sie", "mich", "nicht", "schwer", "ver\u00b7mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter":