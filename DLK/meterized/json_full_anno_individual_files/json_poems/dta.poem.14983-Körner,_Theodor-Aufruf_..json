{"dta.poem.14983": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Hell aus dem Norden bricht der Freiheit Licht.", "tokens": ["Hell", "aus", "dem", "Nor\u00b7den", "bricht", "der", "Frei\u00b7heit", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du sollst den Stahl in Feindes Herzen tauchen,", "tokens": ["Du", "sollst", "den", "Stahl", "in", "Fein\u00b7des", "Her\u00b7zen", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Frisch auf, mein Volk! \u2014 Die Flammenzeichen rauchen,", "tokens": ["Frisch", "auf", ",", "mein", "Volk", "!", "Die", "Flam\u00b7men\u00b7zei\u00b7chen", "rau\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die Saat ist reif, ihr Schnitter, zaudert nicht!", "tokens": ["Die", "Saat", "ist", "reif", ",", "ihr", "Schnit\u00b7ter", ",", "zau\u00b7dert", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das h\u00f6chste Heil, das letzte liegt im Schwerdte!", "tokens": ["Das", "h\u00f6chs\u00b7te", "Heil", ",", "das", "letz\u00b7te", "liegt", "im", "Schwerd\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dr\u00fcck dir den Speer ins treue Herz hinein,", "tokens": ["Dr\u00fcck", "dir", "den", "Speer", "ins", "treu\u00b7e", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Der Freiheit eine Gasse! \u2014 Wasch' die Erde,", "tokens": ["Der", "Frei\u00b7heit", "ei\u00b7ne", "Gas\u00b7se", "!", "Wasch'", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$(", "PWS", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Dein deutsches Land mit deinem Blute rein!", "tokens": ["Dein", "deut\u00b7sches", "Land", "mit", "dei\u00b7nem", "Blu\u00b7te", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist kein Krieg, von dem die Kronen wissen,", "tokens": ["Es", "ist", "kein", "Krieg", ",", "von", "dem", "die", "Kro\u00b7nen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein Kreuzzug, s' ist ein heil'ger Krieg!", "tokens": ["Es", "ist", "ein", "Kreuz\u00b7zug", ",", "s'", "ist", "ein", "heil'\u00b7ger", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Recht, Sitte, Tugend, Glauben und Gewissen", "tokens": ["Recht", ",", "Sit\u00b7te", ",", "Tu\u00b7gend", ",", "Glau\u00b7ben", "und", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat der Tyrann aus deiner Brust gerissen;", "tokens": ["Hat", "der", "Ty\u00b7rann", "aus", "dei\u00b7ner", "Brust", "ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Errette sie mit deiner Freiheit Sieg!", "tokens": ["Er\u00b7ret\u00b7te", "sie", "mit", "dei\u00b7ner", "Frei\u00b7heit", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Winseln deiner Greise ruft: \u201eErwache!\u201c", "tokens": ["Das", "Win\u00b7seln", "dei\u00b7ner", "Grei\u00b7se", "ruft", ":", "\u201e", "Er\u00b7wa\u00b7che", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der H\u00fctte Schutt verflucht die R\u00e4uberbrut!", "tokens": ["Der", "H\u00fct\u00b7te", "Schutt", "ver\u00b7flucht", "die", "R\u00e4u\u00b7ber\u00b7brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Schande deiner T\u00f6chter schreit um Rache,", "tokens": ["Die", "Schan\u00b7de", "dei\u00b7ner", "T\u00f6ch\u00b7ter", "schreit", "um", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Meuchelmord der S\u00f6hne schreit nach Blut.", "tokens": ["Der", "Meu\u00b7chel\u00b7mord", "der", "S\u00f6h\u00b7ne", "schreit", "nach", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zerbrich den Pflugschaar, la\u00df den Meisel fallen,", "tokens": ["Zer\u00b7brich", "den", "Pflug\u00b7schaar", ",", "la\u00df", "den", "Mei\u00b7sel", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Leyer still, den Webstuhl ruhig stehn!", "tokens": ["Die", "Le\u00b7yer", "still", ",", "den", "Web\u00b7stuhl", "ru\u00b7hig", "stehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verlasse deine H\u00f6fe, deine Hallen! \u2014", "tokens": ["Ver\u00b7las\u00b7se", "dei\u00b7ne", "H\u00f6\u00b7fe", ",", "dei\u00b7ne", "Hal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor dessen Antlitz deine Fahnen wallen,", "tokens": ["Vor", "des\u00b7sen", "Ant\u00b7litz", "dei\u00b7ne", "Fah\u00b7nen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er will sein Volk in Waffenr\u00fcstung sehn.", "tokens": ["Er", "will", "sein", "Volk", "in", "Waf\u00b7fen\u00b7r\u00fcs\u00b7tung", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn einen gro\u00dfen Altar sollst du bauen", "tokens": ["Denn", "ei\u00b7nen", "gro\u00b7\u00dfen", "Al\u00b7tar", "sollst", "du", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "In seiner Freiheit ew'gen Morgenroth.", "tokens": ["In", "sei\u00b7ner", "Frei\u00b7heit", "ew'\u00b7gen", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit deinem Schwerdt sollst du die Steine hauen,", "tokens": ["Mit", "dei\u00b7nem", "Schwerdt", "sollst", "du", "die", "Stei\u00b7ne", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Tempel gr\u00fcnde sich auf Heldentod. \u2014", "tokens": ["Der", "Tem\u00b7pel", "gr\u00fcn\u00b7de", "sich", "auf", "Hel\u00b7den\u00b7tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was weint ihr, M\u00e4dchen, warum klagt ihr, Weiber,", "tokens": ["Was", "weint", "ihr", ",", "M\u00e4d\u00b7chen", ",", "wa\u00b7rum", "klagt", "ihr", ",", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr die der Herr die Schwerdter nicht gest\u00e4hlt,", "tokens": ["F\u00fcr", "die", "der", "Herr", "die", "Schwerd\u00b7ter", "nicht", "ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wir entz\u00fcckt die jugendlichen Leiber", "tokens": ["Wenn", "wir", "ent\u00b7z\u00fcckt", "die", "ju\u00b7gend\u00b7li\u00b7chen", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinwerfen in die Schaaren eurer R\u00e4uber,", "tokens": ["Hin\u00b7wer\u00b7fen", "in", "die", "Schaa\u00b7ren", "eu\u00b7rer", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df euch des Kampfes k\u00fchne Wollust fehlt? \u2014", "tokens": ["Da\u00df", "euch", "des", "Kamp\u00b7fes", "k\u00fch\u00b7ne", "Wol\u00b7lust", "fehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihr k\u00f6nnt ja froh zu Gottes Altar treten!", "tokens": ["Ihr", "k\u00f6nnt", "ja", "froh", "zu", "Got\u00b7tes", "Al\u00b7tar", "tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr Wunden gab er zarte Sorgsamkeit,", "tokens": ["F\u00fcr", "Wun\u00b7den", "gab", "er", "zar\u00b7te", "Sor\u00b7gsam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gab euch in Euern herzlichen Gebeten", "tokens": ["Gab", "euch", "in", "Eu\u00b7ern", "herz\u00b7li\u00b7chen", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den sch\u00f6nen reinen Sieg der Fr\u00f6mmigkeit.", "tokens": ["Den", "sch\u00f6\u00b7nen", "rei\u00b7nen", "Sieg", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So betet, da\u00df die alte Kraft erwache,", "tokens": ["So", "be\u00b7tet", ",", "da\u00df", "die", "al\u00b7te", "Kraft", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir dastehn, das alte Volk des Siegs,", "tokens": ["Da\u00df", "wir", "das\u00b7tehn", ",", "das", "al\u00b7te", "Volk", "des", "Siegs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00e4rtyrer der heil'gen deutschen Sache.", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rer", "der", "heil'\u00b7gen", "deut\u00b7schen", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O ruft sie an als Genien der Rache,", "tokens": ["O", "ruft", "sie", "an", "als", "Ge\u00b7ni\u00b7en", "der", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als gute Engel des gerechten Kriegs.", "tokens": ["Als", "gu\u00b7te", "En\u00b7gel", "des", "ge\u00b7rech\u00b7ten", "Kriegs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Luise schwebe segnend um den Gatten,", "tokens": ["Lu\u00b7i\u00b7se", "schwe\u00b7be", "seg\u00b7nend", "um", "den", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Geist unsers Ferdinands voran dem Zug!", "tokens": ["Geist", "un\u00b7sers", "Fer\u00b7di\u00b7nands", "vo\u00b7ran", "dem", "Zug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und all' ihr deutschen freien Heldenschatten,", "tokens": ["Und", "all'", "ihr", "deut\u00b7schen", "frei\u00b7en", "Hel\u00b7den\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mit uns, mit uns, und unsrer Fahnen Flug!", "tokens": ["Mit", "uns", ",", "mit", "uns", ",", "und", "uns\u00b7rer", "Fah\u00b7nen", "Flug", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Himmel hilft, die H\u00f6lle mu\u00df uns weichen!", "tokens": ["Der", "Him\u00b7mel", "hilft", ",", "die", "H\u00f6l\u00b7le", "mu\u00df", "uns", "wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drauf! wakres Volk! drauf! ruft die Freiheit, drauf!", "tokens": ["Drauf", "!", "wak\u00b7res", "Volk", "!", "drauf", "!", "ruft", "die", "Frei\u00b7heit", ",", "drauf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "wor