RT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die friedlichen Wellen ans Ohr.", "tokens": ["Die", "fried\u00b7li\u00b7chen", "Wel\u00b7len", "ans", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Die Sommernacht ist gekommen,", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Linde ragt dunkel und sieht,", "tokens": ["Die", "Lin\u00b7de", "ragt", "dun\u00b7kel", "und", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wie, Wundertiefen entglommen,", "tokens": ["Wie", ",", "Wun\u00b7der\u00b7tie\u00b7fen", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hell Gottes Sternheer zieht.", "tokens": ["Hell", "Got\u00b7tes", "Stern\u00b7heer", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "S\u00fc\u00df duftet die alte Linde,", "tokens": ["S\u00fc\u00df", "duf\u00b7tet", "die", "al\u00b7te", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die vor dem Wirtshaus bl\u00fcht,", "tokens": ["Die", "vor", "dem", "Wirts\u00b7haus", "bl\u0