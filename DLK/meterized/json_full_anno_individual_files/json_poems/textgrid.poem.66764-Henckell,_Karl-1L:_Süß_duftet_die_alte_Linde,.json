 ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die sprossenden Kindlein n\u00e4hrt ...", "tokens": ["Die", "spros\u00b7sen\u00b7den", "Kin\u00b7dlein", "n\u00e4hrt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der in der Werkstatt schmiedet", "tokens": ["Der", "in", "der", "Werk\u00b7statt", "schmie\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Pflugschar und sch\u00f6nes Ger\u00e4t,", "tokens": ["Pflug\u00b7schar", "und", "sch\u00f6\u00b7nes", "Ge\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Im Forscher unerm\u00fcdet", "tokens": ["Im", "For\u00b7scher", "un\u00b7er\u00b7m\u00fc\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Heil und Segen sp\u00e4ht ...", "tokens": ["Nach", "Heil", "und", "Se\u00b7gen", "sp\u00e4ht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du, der im K\u00fcnstler r\u00fcndet", "tokens": ["Du", ",", "der", "im", "K\u00fcnst\u00b7ler", "r\u00fcn\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die edle Krone der Kraft,", "tokens": ["Die", "ed\u00b7le", "Kro\u00b7ne", "der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Tempel baut und Br\u00fccken", "tokens": ["Der", "Tem\u00b7pel", "baut", "und", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr G\u00fcter und Geister schafft ...", "tokens": ["F\u00fcr", "G\u00fc\u00b7ter", "und", "Geis\u00b7ter", "schafft", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Nun l\u00f6sche des Hasses Feuer,", "tokens": ["Nun", "l\u00f6\u00b7sche", "des", "Has\u00b7ses", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das heiliges Leben verbrennt,", "tokens": ["Das", "hei\u00b7li\u00b7ges", "Le\u00b7ben", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nun wehre bei allem, was teuer,", "tokens": ["Nun", "weh\u00b7re", "bei", "al\u00b7lem", ",", "was", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dem h\u00f6llischen Element!", "tokens": ["Dem", "h\u00f6l\u00b7li\u00b7schen", "E\u00b7le\u00b7ment", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Den zuchtlose Frechheit entz\u00fcndet