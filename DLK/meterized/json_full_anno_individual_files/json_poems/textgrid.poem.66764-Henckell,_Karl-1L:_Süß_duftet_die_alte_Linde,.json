"word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$("], "meter": "++--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Brandet an ferner K\u00fcste", "tokens": ["Bran\u00b7det", "an", "fer\u00b7ner", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der rasende Krieg noch heut?", "tokens": ["Der", "ra\u00b7sen\u00b7de", "Krieg", "noch", "heut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Dorf verd\u00e4mmert in Frieden,", "tokens": ["Das", "Dorf", "ver\u00b7d\u00e4m\u00b7mert", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Kirchturm t\u00f6nt wie Gebet", "tokens": ["Vom", "Kirch\u00b7turm", "t\u00f6nt", "wie", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Feierglocke her\u00fcber,", "tokens": ["Die", "Fei\u00b7er\u00b7glo\u00b7cke", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Hauch des Ewigen weht.", "tokens": ["Ein", "Hauch", "des", "E\u00b7wi\u00b7gen", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Leis spielen am nahen Gel\u00e4nde", "tokens": ["Leis", "spie\u00b7len", "am", "na\u00b7hen", "Ge\u00b7l\u00e4n\