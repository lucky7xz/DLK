{"dta.poem.14036": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Tritt nicht hinaus jetzt vor die Th\u00fcr,", "tokens": ["Tritt", "nicht", "hin\u00b7aus", "jetzt", "vor", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht hat eignen Sang,", "tokens": ["Die", "Nacht", "hat", "eig\u00b7nen", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Waldhorn ruft, als rief's nach Dir,", "tokens": ["Das", "Wald\u00b7horn", "ruft", ",", "als", "rie\u00b7f's", "nach", "Dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NE", "APPR", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Betr\u00fcglich ist der irre Klang,", "tokens": ["Be\u00b7tr\u00fcg\u00b7lich", "ist", "der", "ir\u00b7re", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Endlos der W\u00e4lder Labyrinth \u2014", "tokens": ["End\u00b7los", "der", "W\u00e4l\u00b7der", "La\u00b7by\u00b7rinth"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Beh\u00fct' Dich Gott, Du sch\u00f6nes Kind!", "tokens": ["Be\u00b7h\u00fct'", "Dich", "Gott", ",", "Du", "sch\u00f6\u00b7nes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}