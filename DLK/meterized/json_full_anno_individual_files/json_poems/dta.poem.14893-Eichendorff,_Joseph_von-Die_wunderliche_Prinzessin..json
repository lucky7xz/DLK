{"dta.poem.14893": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und die N\u00e4chte von Guitarren", "tokens": ["Und", "die", "N\u00e4ch\u00b7te", "von", "Gui\u00b7tar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und Ges\u00e4ngen weich erschallen,", "tokens": ["Und", "Ge\u00b7s\u00e4n\u00b7gen", "weich", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der wunderliche Alte", "tokens": ["Da\u00df", "der", "wun\u00b7der\u00b7li\u00b7che", "Al\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie verr\u00fcckt beginnt zu tanzen.", "tokens": ["Wie", "ver\u00b7r\u00fcckt", "be\u00b7ginnt", "zu", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Prinzessin schm\u00fcckt mit Kr\u00e4nzen", "tokens": ["Die", "Prin\u00b7zes\u00b7sin", "schm\u00fcckt", "mit", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wieder sich die sch\u00f6nen Haare,", "tokens": ["Wie\u00b7der", "sich", "die", "sch\u00f6\u00b7nen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die vollen Kr\u00e4nze gl\u00e4nzen", "tokens": ["Und", "die", "vol\u00b7len", "Kr\u00e4n\u00b7ze", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie blickt verlangend nieder.", "tokens": ["Und", "sie", "blickt", "ver\u00b7lan\u00b7gend", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}