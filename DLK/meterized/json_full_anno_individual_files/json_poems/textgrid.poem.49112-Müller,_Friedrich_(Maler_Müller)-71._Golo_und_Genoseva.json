{"textgrid.poem.49112": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "71. Golo und Genoseva", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die D\u00e4mmrung sinkt gelinde", "tokens": ["Durch", "die", "D\u00e4mm\u00b7rung", "sinkt", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nun herab der Abendthau", "tokens": ["nun", "her\u00b7ab", "der", "A\u00b7bendt\u00b7hau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und die leichte Fr\u00fchlingswinde", "tokens": ["und", "die", "leich\u00b7te", "Fr\u00fch\u00b7lings\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hauchen durch die Bl\u00fcthen lau;", "tokens": ["hau\u00b7chen", "durch", "die", "Bl\u00fc\u00b7then", "lau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "und des Mondes Silberwallen,", "tokens": ["und", "des", "Mon\u00b7des", "Sil\u00b7ber\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zittert hin auf dunkler Fluth,", "tokens": ["zit\u00b7tert", "hin", "auf", "dunk\u00b7ler", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "