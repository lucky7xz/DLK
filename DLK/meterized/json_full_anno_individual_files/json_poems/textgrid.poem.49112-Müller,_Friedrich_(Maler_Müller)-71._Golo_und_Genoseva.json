d"], "pos": ["APPR", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bluhmen tausendfach zur Lufft;", "tokens": ["Bluh\u00b7men", "tau\u00b7send\u00b7fach", "zur", "Lufft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "und des Himmels klare Kerzen", "tokens": ["und", "des", "Him\u00b7mels", "kla\u00b7re", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gie\u00dfen nieder milden Schein:", "tokens": ["gie\u00b7\u00dfen", "nie\u00b7der", "mil\u00b7den", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "aber fern von holden Scherzen,", "tokens": ["a\u00b7ber", "fern", "von", "hol\u00b7den", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "klagen einsam wunde Herzen,", "tokens": ["kla\u00b7gen", "ein\u00b7sam", "wun\u00b7de", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bange sind der Liebe Schmerzen,", "tokens": ["ban\u00b7ge", "sind", "der", "Lie\u00b7be", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sie durchnagen Mark und Bein.", "tokens": ["sie", "durch\u00b7na\u00b7gen", "Mark", "und", "Bein", "."], "token_info": ["word", "w