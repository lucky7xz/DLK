{"textgrid.poem.47398": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, Israel, hat in der W\u00fcste Jehova wunderbar gef\u00fchrt,", "tokens": ["Dich", ",", "Is\u00b7rael", ",", "hat", "in", "der", "W\u00fcs\u00b7te", "Je\u00b7ho\u00b7va", "wun\u00b7der\u00b7bar", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VAFIN", "APPR", "ART", "NN", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Er hat dich zum Verhei\u00dfungslande durch Irren vierzig Jahr' gef\u00fchrt.", "tokens": ["Er", "hat", "dich", "zum", "Ver\u00b7hei\u00b7\u00dfungs\u00b7lan\u00b7de", "durch", "Ir\u00b7ren", "vier\u00b7zig", "Jahr'", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "APPR", "NN", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Er hat dich wollen altern lassen, damit verj\u00fcngt du ziehest ein;", "tokens": ["Er", "hat", "dich", "wol\u00b7len", "al\u00b7tern", "las\u00b7sen", ",", "da\u00b7mit", "ver\u00b7j\u00fcngt", "du", "zie\u00b7hest", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VMFIN", "VVINF", "VVINF", "$,", "PAV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Er hat, da unterwegs du starbest, dich heim als neue Schar gef\u00fchrt.", "tokens": ["Er", "hat", ",", "da", "un\u00b7ter\u00b7wegs", "du", "star\u00b7best", ",", "dich", "heim", "als", "neu\u00b7e", "Schar", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,", "PPER", "PTKVZ", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Er hat dich wollen dursten lassen, um dir den Quell aus Felsgestein", "tokens": ["Er", "hat", "dich", "wol\u00b7len", "durs\u00b7ten", "las\u00b7sen", ",", "um", "dir", "den", "Quell", "aus", "Fels\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VMFIN", "VVINF", "VVINF", "$,", "KOUI", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Zu schlagen: er hat tags im Donner, dich nachts in Blitzen klar gef\u00fchrt.", "tokens": ["Zu", "schla\u00b7gen", ":", "er", "hat", "tags", "im", "Don\u00b7ner", ",", "dich", "nachts", "in", "Blit\u00b7zen", "klar", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$,", "PPER", "ADV", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+--++-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Er hat dich lassen irre gehn, damit du k\u00e4mst ans rechte Ziel!", "tokens": ["Er", "hat", "dich", "las\u00b7sen", "ir\u00b7re", "gehn", ",", "da\u00b7mit", "du", "k\u00e4mst", "ans", "rech\u00b7te", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Er hat dich langsam, seltsam, aber er hat ich immerdar gef\u00fchrt.", "tokens": ["Er", "hat", "dich", "lang\u00b7sam", ",", "selt\u00b7sam", ",", "a\u00b7ber", "er", "hat", "ich", "im\u00b7mer\u00b7dar", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Und als du zum verhei\u00dfnen Lande nun hingelangt warst, riefest du:", "tokens": ["Und", "als", "du", "zum", "ver\u00b7hei\u00df\u00b7nen", "Lan\u00b7de", "nun", "hin\u00b7ge\u00b7langt", "warst", ",", "rie\u00b7fest", "du", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Er hat mich wunderbar geleitet, doch mich zurecht f\u00fcrwahr gef\u00fchrt!", "tokens": ["Er", "hat", "mich", "wun\u00b7der\u00b7bar", "ge\u00b7lei\u00b7tet", ",", "doch", "mich", "zu\u00b7recht", "f\u00fcr\u00b7wahr", "ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "KON", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "So rufet Freimund, den durch W\u00fcsten der Herr im Donner und im Blitz,", "tokens": ["So", "ru\u00b7fet", "Frei\u00b7mund", ",", "den", "durch", "W\u00fcs\u00b7ten", "der", "Herr", "im", "Don\u00b7ner", "und", "im", "Blitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "APPR", "NN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+---+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Durch L\u00e4ut'rungsfeuer hin zum Lichte, zum Liebeshochaltar gef\u00fchrt;", "tokens": ["Durch", "L\u00e4ut'\u00b7rungs\u00b7feu\u00b7er", "hin", "zum", "Lich\u00b7te", ",", "zum", "Lie\u00b7bes\u00b7hoc\u00b7hal\u00b7tar", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.13": {"text": "So rufet Freimund auch am Ziele, wo sich die Irren aufgel\u00f6st:", "tokens": ["So", "ru\u00b7fet", "Frei\u00b7mund", "auch", "am", "Zie\u00b7le", ",", "wo", "sich", "die", "Ir\u00b7ren", "auf\u00b7ge\u00b7l\u00f6st", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPRART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "Er hat f\u00fcrwahr mich recht geleitet, er hat mich wunderbar gef\u00fchrt.", "tokens": ["Er", "hat", "f\u00fcr\u00b7wahr", "mich", "recht", "ge\u00b7lei\u00b7tet", ",", "er", "hat", "mich", "wun\u00b7der\u00b7bar", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "ADJD", "VVPP", "$,", "PPER", "VAFIN", "