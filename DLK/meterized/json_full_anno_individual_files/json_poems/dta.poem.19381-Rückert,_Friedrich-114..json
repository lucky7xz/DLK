{"dta.poem.19381": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sei dankbar f\u00fcr das Gl\u00fcck, das dir der Herr bestimmt", "tokens": ["Sei", "dank\u00b7bar", "f\u00fcr", "das", "Gl\u00fcck", ",", "das", "dir", "der", "Herr", "be\u00b7stimmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gib es gern zur\u00fcck, wenn er es wieder nimmt.", "tokens": ["Und", "gib", "es", "gern", "zu\u00b7r\u00fcck", ",", "wenn", "er", "es", "wie\u00b7der", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist kein Gut so gro\u00df, er hat noch gr\u00f6\u00dfres eben,", "tokens": ["Es", "ist", "kein", "Gut", "so", "gro\u00df", ",", "er", "hat", "noch", "gr\u00f6\u00df\u00b7res", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nimmt dir eines blo\u00df um andres dir zu geben.", "tokens": ["Und", "nimmt", "dir", "ei\u00b7nes", "blo\u00df", "um", "and\u00b7res", "dir", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word