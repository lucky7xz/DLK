{"dta.poem.13194": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees hatt zwar jemand uns von dir Befehl gebracht;", "tokens": ["\u201e", "es", "hatt", "zwar", "je\u00b7mand", "uns", "von", "dir", "Be\u00b7fehl", "ge\u00b7bracht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIS", "PRF", "APPR", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewir haben auch darauf was anders ausgedacht;", "tokens": ["\u201e", "wir", "ha\u00b7ben", "auch", "da\u00b7rauf", "was", "an\u00b7ders", "aus\u00b7ge\u00b7dacht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PAV", "PWS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eallein auch dieses ist nicht ohne Streit geblieben,", "tokens": ["\u201e", "al\u00b7lein", "auch", "die\u00b7ses", "ist", "nicht", "oh\u00b7ne", "Streit", "ge\u00b7blie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PDS", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eweil uns fast jeder Saz was Neues vorgeschrieben;", "tokens": ["\u201e", "weil", "uns", "fast", "je\u00b7der", "Saz", "was", "Neu\u00b7es", "vor\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "PIAT", "NN", "PWS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "145\u201dSo da\u00df zu dem Beschlu\u00df nur noch dein Wille fehlt,", "tokens": ["\"", "So", "da\u00df", "zu", "dem", "Be\u00b7schlu\u00df", "nur", "noch", "dein", "Wil\u00b7le", "fehlt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "APPR", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund was derselbige zu der Entscheidung w\u00e4hlt.", "tokens": ["\u201e", "und", "was", "der\u00b7sel\u00b7bi\u00b7ge", "zu", "der", "Ent\u00b7schei\u00b7dung", "w\u00e4hlt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "\u201ewird die Versammlung sich der Gnade schmeicheln d\u00e4rffen,", "tokens": ["\u201e", "wird", "die", "Ver\u00b7samm\u00b7lung", "sich", "der", "Gna\u00b7de", "schmei\u00b7cheln", "d\u00e4rf\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "PRF", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eso will ich dir des Streits Beschaffenheit entwerffen.", "tokens": ["\u201e", "so", "will", "ich", "dir", "des", "Streits", "Be\u00b7schaf\u00b7fen\u00b7heit", "ent\u00b7werf\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}