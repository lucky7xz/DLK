{"textgrid.poem.35722": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "46. Rauschlied", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um zu sein, wie sich's geb\u00fchrt,", "tokens": ["Um", "zu", "sein", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VAINF", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freunde, mu\u00df man trinken;", "tokens": ["Freun\u00b7de", ",", "mu\u00df", "man", "trin\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Drum die Bole rasch geleert,", "tokens": ["Drum", "die", "Bo\u00b7le", "rasch", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist mein Bed\u00fcnken.", "tokens": ["Das", "ist", "mein", "Be\u00b7d\u00fcn\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bruder! trau dein Lebelang", "tokens": ["Bru\u00b7der", "!", "trau", "dein", "Le\u00b7be\u00b7lang"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Kerngedanken:", "tokens": ["Auf", "den", "Kern\u00b7ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sang und Klang, besonders Trank", "tokens": ["Sang", "und", "Klang", ",", "be\u00b7son\u00b7ders", "Trank"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Will nicht Ma\u00df noch Schranken.", "tokens": ["Will", "nicht", "Ma\u00df", "noch", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Einig sind wir, wie es scheint;", "tokens": ["Ei\u00b7nig", "sind", "wir", ",", "wie", "es", "scheint", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00fcchtern mu\u00df man bleiben!", "tokens": ["N\u00fcch\u00b7tern", "mu\u00df", "man", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber das Recept, mein Freund,", "tokens": ["A\u00b7ber", "das", "Re\u00b7cept", ",", "mein", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Will ich dir verschreiben;", "tokens": ["Will", "ich", "dir", "ver\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Tief ist, leider! sieh nur zu,", "tokens": ["Tief", "ist", ",", "lei\u00b7der", "!", "sieh", "nur", "zu", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADV", "$.", "VVIMP", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon der Punsch gesunken,", "tokens": ["Schon", "der", "Pun\u00b7sch", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Trunken ich, und trunken du,", "tokens": ["Trun\u00b7ken", "ich", ",", "und", "trun\u00b7ken", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir sind alle trunken.", "tokens": ["Wir", "sind", "al\u00b7le", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Flog dir etwa \u00fcbers Glas", "tokens": ["Flog", "dir", "et\u00b7wa", "\u00fc\u00b7bers", "Glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Verstand ganz leise,", "tokens": ["Der", "Ver\u00b7stand", "ganz", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und du tr\u00fcgst doch gern so was", "tokens": ["Und", "du", "tr\u00fcgst", "doch", "gern", "so", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADV", "PWS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Hirngeh\u00e4use;", "tokens": ["In", "dem", "Hirn\u00b7ge\u00b7h\u00e4u\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eh du suchest kreuz und quer", "tokens": ["Eh", "du", "su\u00b7chest", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dft du wissen, Lieber!", "tokens": ["Mu\u00dft", "du", "wis\u00b7sen", ",", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Welche Richtung ohngef\u00e4hr", "tokens": ["Wel\u00b7che", "Rich\u00b7tung", "ohn\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Flog er wohl hin\u00fcber?", "tokens": ["Flog", "er", "wohl", "hin\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Meiner war mir eben auch", "tokens": ["Mei\u00b7ner", "war", "mir", "e\u00b7ben", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Kopf entwichen;", "tokens": ["Aus", "dem", "Kopf", "ent\u00b7wi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch ich fand den kleinen Gauch", "tokens": ["Doch", "ich", "fand", "den", "klei\u00b7nen", "Gauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald auf seinen Schlichen;", "tokens": ["Bald", "auf", "sei\u00b7nen", "Schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "H\u00f6rt den weisen Rat nun an:", "tokens": ["H\u00f6rt", "den", "wei\u00b7sen", "Rat", "nun", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich meinen hole,", "tokens": ["Wo", "ich", "mei\u00b7nen", "ho\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hol' auch seinen jedermann, \u2013", "tokens": ["Hol'", "auch", "sei\u00b7nen", "je\u00b7der\u00b7mann", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PPOSAT", "PIS", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tief am Grund der Bole.", "tokens": ["Tief", "am", "Grund", "der", "Bo\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ausgetrunken! eingeschenkt!", "tokens": ["Aus\u00b7ge\u00b7trun\u00b7ken", "!", "ein\u00b7ge\u00b7schenkt", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flink nach alter Regel!", "tokens": ["Flink", "nach", "al\u00b7ter", "Re\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ha! im Winkel dort versenkt!", "tokens": ["Ha", "!", "im", "Win\u00b7kel", "dort", "ver\u00b7senkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht die losen V\u00f6gel!", "tokens": ["Seht", "die", "lo\u00b7sen", "V\u00f6\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Greift sie, greift! Viktoria!", "tokens": ["Greift", "sie", ",", "greift", "!", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$.", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Tralle, ralle, ralle!", "tokens": ["Tral\u00b7le", ",", "ral\u00b7le", ",", "ral\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Punsch ist weg, Verstand ist da,", "tokens": ["Pun\u00b7sch", "ist", "weg", ",", "Ver\u00b7stand", "ist", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "$,", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "N\u00fcchtern sind wir alle.", "tokens": ["N\u00fcch\u00b7tern", "sind", "wir", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Um zu sein, wie sich's geb\u00fchrt,", "tokens": ["Um", "zu", "sein", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "p