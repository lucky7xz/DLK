{"dta.poem.10722": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "ArISTARCHUS,  \n SIVE DE CONTEMPTU LIN-  \n GU\u00c6 TEUTONIC \u00c6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.57", "no:0.42"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "O Fortun O Fortun/ Stiessmutter aller freuden/", "tokens": ["O", "For\u00b7tun", "O", "For\u00b7tun", "/", "Stiess\u00b7mut\u00b7ter", "al\u00b7ler", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$(", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anfeinderin der lust/ erweckerin der noth/", "tokens": ["An\u00b7fein\u00b7de\u00b7rin", "der", "lust", "/", "er\u00b7we\u00b7cke\u00b7rin", "der", "noth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du todtes leben/ ja du lebendiger Todt/", "tokens": ["Du", "tod\u00b7tes", "le\u00b7ben", "/", "ja", "du", "le\u00b7ben\u00b7di\u00b7ger", "Todt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VVINF", "$(", "PTKANT", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durch welcher grim\u0303 sich mu\u00df manch trewes Hertze scheiden/", "tokens": ["Durch", "wel\u00b7cher", "grim\u0303", "sich", "mu\u00df", "manch", "tre\u00b7wes", "Hert\u00b7ze", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "PRF", "VMFIN", "PIAT", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "Soll deine grausamkeit denn auch mein umges leben/", "tokens": ["Soll", "dei\u00b7ne", "grau\u00b7sam\u00b7keit", "denn", "auch", "mein", "um\u00b7ges", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "(de\u00df allen vngeacht/ da\u00df mir Natura mehr", "tokens": ["(", "de\u00df", "al\u00b7len", "vn\u00b7ge\u00b7acht", "/", "da\u00df", "mir", "Na\u00b7tu\u00b7ra", "mehr"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "PIAT", "NN", "$(", "KOUS", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als ich auch w\u00fcrdig bin/ geschencket Gunst vnd Ehr)", "tokens": ["Als", "ich", "auch", "w\u00fcr\u00b7dig", "bin", "/", "ge\u00b7schen\u00b7cket", "Gunst", "vnd", "Ehr", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$(", "VVPP", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In tr\u00fcbni\u00df vnd gefahr so trauwrig lassen schweben?", "tokens": ["In", "tr\u00fcb\u00b7ni\u00df", "vnd", "ge\u00b7fahr", "so", "trau\u00b7wrig", "las\u00b7sen", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du scheutzliche Chimer/ sih wazu du mich bringest/", "tokens": ["Du", "scheutz\u00b7li\u00b7che", "Chi\u00b7mer", "/", "sih", "wa\u00b7zu", "du", "mich", "brin\u00b7gest", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VVIMP", "PWAV", "PPER", "PRF", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Da ich von Kindheit an mit vnverwandten sin\u0303/", "tokens": ["Da", "ich", "von", "Kind\u00b7heit", "an", "mit", "vn\u00b7ver\u00b7wand\u00b7ten", "si\u00f1", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Standhafft vnd vnverzagt allzeit gewesen bin/", "tokens": ["Stand\u00b7hafft", "vnd", "vn\u00b7ver\u00b7zagt", "all\u00b7zeit", "ge\u00b7we\u00b7sen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jetz bitter Zehren mich auch zuvergiessen dringest.", "tokens": ["Jetz", "bit\u00b7ter", "Zeh\u00b7ren", "mich", "auch", "zu\u00b7ver\u00b7gies\u00b7sen", "drin\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ey bi\u00df mit dem zufried: ey/ la\u00df dir doch gen\u00fcgen/", "tokens": ["Ey", "bi\u00df", "mit", "dem", "zu\u00b7fried", ":", "ey", "/", "la\u00df", "dir", "doch", "ge\u00b7n\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "$.", "XY", "$(", "VVIMP", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df deiner stralen brunst an meines alters blum/", "tokens": ["Da\u00df", "dei\u00b7ner", "stra\u00b7len", "brunst", "an", "mei\u00b7nes", "al\u00b7ters", "blum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die frischen bletter gantz verd\u00f6rret: diesen ruhm", "tokens": ["Die", "fri\u00b7schen", "blet\u00b7ter", "gantz", "ver\u00b7d\u00f6r\u00b7ret", ":", "die\u00b7sen", "ruhm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "La\u00df dir doch sein genug/ mit diesem Raub thu siegen.", "tokens": ["La\u00df", "dir", "doch", "sein", "ge\u00b7nug", "/", "mit", "die\u00b7sem", "Raub", "thu", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "ADV", "$(", "APPR", "PDAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein freyer Held wen er den Feind nur vberwunden/", "tokens": ["Ein", "frey\u00b7er", "Held", "wen", "er", "den", "Feind", "nur", "vber\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PWS", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Ist er gar wohl zufried\u2019: er halt es f\u00fcr r\u00fcmlich/", "tokens": ["Ist", "er", "gar", "wohl", "zu\u00b7fried'", ":", "er", "halt", "es", "f\u00fcr", "r\u00fcm\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "APPR", "ADJD", "$("], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Da\u00df/ ob ers k\u00f6nte thun/ er doch nit reche sich/", "tokens": ["Da\u00df", "/", "ob", "ers", "k\u00f6n\u00b7te", "thun", "/", "er", "doch", "nit", "re\u00b7che", "sich", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PIS", "VMFIN", "VVINF", "$(", "PPER", "ADV", "PTKNEG", "ADJA", "PRF", "$("], "meter": "--+--+--++-+", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Weil er nun allbereit/ was er gesucht/ hat funden.", "tokens": ["Weil", "er", "nun", "all\u00b7be\u00b7reit", "/", "was", "er", "ge\u00b7sucht", "/", "hat", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$(", "PWS", "PPER", "VVPP", "$(", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vnd du/ O schn\u00f6des Weib/ wilst mich so hoch verderben/", "tokens": ["Vnd", "du", "/", "O", "schn\u00f6\u00b7des", "Weib", "/", "wilst", "mich", "so", "hoch", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "NE", "ADJA", "NN", "$(", "VMFIN", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vnd dir ists nicht genug da\u00df du mich so ger\u00fchrt/", "tokens": ["Vnd", "dir", "ists", "nicht", "ge\u00b7nug", "da\u00df", "du", "mich", "so", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja durch vil Creutz vnd Leid/ durch Angst vnd Noth gef\u00fchrt/", "tokens": ["Ja", "durch", "vil", "Creutz", "vnd", "Leid", "/", "durch", "Angst", "vnd", "Noth", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PIAT", "NN", "KON", "NN", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du denckest dir auch noch bey mir vmb mehr zu werben.", "tokens": ["Du", "den\u00b7ckest", "dir", "auch", "noch", "bey", "mir", "vmb", "mehr", "zu", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Gott aber ist mein schutz/ dem will ich das vertrawen/", "tokens": ["Gott", "a\u00b7ber", "ist", "mein", "schutz", "/", "dem", "will", "ich", "das", "ver\u00b7tra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN", "$(", "PDS", "VMFIN", "PPER", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was mir noch vbrig ist: er ist mein Schirm vnd Schilt/", "tokens": ["Was", "mir", "noch", "vb\u00b7rig", "ist", ":", "er", "ist", "mein", "Schirm", "vnd", "Schilt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAFIN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wen ich jhn nur fleh\u2019 an/ gar keine noth mehr gilt.", "tokens": ["Wen", "ich", "jhn", "nur", "fleh'", "an", "/", "gar", "kei\u00b7ne", "noth", "mehr", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$(", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Auff jhn will ich allein in allen n\u00f6then bawen.", "tokens": ["Auff", "jhn", "will", "ich", "al\u00b7lein", "in", "al\u00b7len", "n\u00f6\u00b7then", "ba\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "Wer sich auff Gott verlest/ der mag wohl k\u00fcnlich dencken/", "tokens": ["Wer", "sich", "auff", "Gott", "ver\u00b7lest", "/", "der", "mag", "wohl", "k\u00fcn\u00b7lich", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVFIN", "$(", "ART", "VMFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df er alles vngl\u00fcck so vns offtmahls zusteht/", "tokens": ["Da\u00df", "er", "al\u00b7les", "vn\u00b7gl\u00fcck", "so", "vns", "offt\u00b7mahls", "zu\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "--+-+---+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "(ob es gleich in der erst schwer vnd getr\u00e4ng hergeht)", "tokens": ["(", "ob", "es", "gleich", "in", "der", "erst", "schwer", "vnd", "ge\u00b7tr\u00e4ng", "her\u00b7geht", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "APPR", "ART", "ADV", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Zu seiner Stell\u2019 vnd Stund mit frewden werde lencken.", "tokens": ["Zu", "sei\u00b7ner", "Stell'", "vnd", "Stund", "mit", "frew\u00b7den", "wer\u00b7de", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "APPR", "ADJA", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Item hoc breve Epigrammation:", "tokens": ["I\u00b7tem", "hoc", "bre\u00b7ve", "E\u00b7pig\u00b7ram\u00b7ma\u00b7ti\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.34": {"text": "Wollust vnd Vppigkeit der Welt must du vermeiden/", "tokens": ["Wol\u00b7lust", "vnd", "Vp\u00b7pig\u00b7keit", "der", "Welt", "must", "du", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Vnd tretten mit gedult der scharffen D\u00f6rner Weg/", "tokens": ["Vnd", "tret\u00b7ten", "mit", "ge\u00b7dult", "der", "scharf\u00b7fen", "D\u00f6r\u00b7ner", "Weg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So er dich tragen soll auff den lieblichen Steg/", "tokens": ["So", "er", "dich", "tra\u00b7gen", "soll", "auff", "den", "lieb\u00b7li\u00b7chen", "Steg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "VVINF", "VMFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Vnd in das sch\u00f6ne Schlo\u00df der wahren Lust vnd frewden.", "tokens": ["Vnd", "in", "das", "sch\u00f6\u00b7ne", "Schlo\u00df", "der", "wah\u00b7ren", "Lust", "vnd", "frew\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}