{"textgrid.poem.64041": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Andenken der Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Kindheit l\u00e4ngst vergangnen Tagen", "tokens": ["Auf", "der", "Kind\u00b7heit", "l\u00e4ngst", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weilet oft mein still umw\u00f6lkter Sinn,", "tokens": ["Wei\u00b7let", "oft", "mein", "still", "um\u00b7w\u00f6lk\u00b7ter", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und aus meinem Herzen brechen Klagen,", "tokens": ["Und", "aus", "mei\u00b7nem", "Her\u00b7zen", "bre\u00b7chen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dass ich nun nicht mehr so gl\u00fccklich bin!", "tokens": ["Dass", "ich", "nun", "nicht", "mehr", "so", "gl\u00fcck\u00b7lich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Angestrahlt vom Morgenroth des Lebens,", "tokens": ["An\u00b7ge\u00b7strahlt", "vom", "Mor\u00b7gen\u00b7roth", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lachte mir der Fr\u00fchling \u00fcberall!", "tokens": ["Lach\u00b7te", "mir", "der", "Fr\u00fch\u00b7ling", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keine Blume bl\u00fchte mir vergebens \u2013", "tokens": ["Kei\u00b7ne", "Blu\u00b7me", "bl\u00fch\u00b7te", "mir", "ver\u00b7ge\u00b7bens", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Melodie war mir der Wasserfall.", "tokens": ["Me\u00b7lo\u00b7die", "war", "mir", "der", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Unbekannt mit schmerzlichen Gef\u00fchlen,", "tokens": ["Un\u00b7be\u00b7kannt", "mit", "schmerz\u00b7li\u00b7chen", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rauschte harmlos, wonnevoll und frei", "tokens": ["Rauschte", "harm\u00b7los", ",", "won\u00b7ne\u00b7voll", "und", "frei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An der Seite lustiger Gespielen", "tokens": ["An", "der", "Sei\u00b7te", "lus\u00b7ti\u00b7ger", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir vor\u00fcber meiner Kindheit Mai.", "tokens": ["Mir", "vor\u00b7\u00fc\u00b7ber", "mei\u00b7ner", "Kind\u00b7heit", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aber dauernd pr\u00e4gten sich die Bilder", "tokens": ["A\u00b7ber", "dau\u00b7ernd", "pr\u00e4g\u00b7ten", "sich", "die", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seiner reinen Freuden mir in's Herz,", "tokens": ["Sei\u00b7ner", "rei\u00b7nen", "Freu\u00b7den", "mir", "in's", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und des Lebens Dunkel wird mir milder,", "tokens": ["Und", "des", "Le\u00b7bens", "Dun\u00b7kel", "wird", "mir", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denk' ich ihrer mit der Sehnsucht Schmerz.", "tokens": ["Denk'", "ich", "ih\u00b7rer", "mit", "der", "Sehn\u00b7sucht", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Darum kehr' ich gern in deine Fluren", "tokens": ["Da\u00b7rum", "kehr'", "ich", "gern", "in", "dei\u00b7ne", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stilles D\u00f6rfchen, aus dem L\u00e4rm der Welt,", "tokens": ["Stil\u00b7les", "D\u00f6rf\u00b7chen", ",", "aus", "dem", "L\u00e4rm", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn in dir begegnen mir die Spuren", "tokens": ["Denn", "in", "dir", "be\u00b7geg\u00b7nen", "mir", "die", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jener Zeit, die noch kein Gram entstellt.", "tokens": ["Je\u00b7ner", "Zeit", ",", "die", "noch", "kein", "Gram", "ent\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "L\u00e4chelnd gr\u00fcsst mich jedes Pl\u00e4tzchen wieder,", "tokens": ["L\u00e4\u00b7chelnd", "gr\u00fcs\u00b7st", "mich", "je\u00b7des", "Pl\u00e4tz\u00b7chen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$,"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wo ich mich im heitern Spiel verlohr,", "tokens": ["Wo", "ich", "mich", "im", "hei\u00b7tern", "Spiel", "ver\u00b7lohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und noch t\u00f6nt, s\u00fcss wie Sirenenlieder,", "tokens": ["Und", "noch", "t\u00f6nt", ",", "s\u00fcss", "wie", "Si\u00b7re\u00b7nen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Klarer Bach, dein Fl\u00fcstern meinem Ohr.", "tokens": ["Kla\u00b7rer", "Bach", ",", "dein", "Fl\u00fcs\u00b7tern", "mei\u00b7nem", "Ohr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Goldne Zeiten, wo ich, gleich der Biene,", "tokens": ["Gold\u00b7ne", "Zei\u00b7ten", ",", "wo", "ich", ",", "gleich", "der", "Bie\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Honig in dem kleinsten Bl\u00fcmchen fand!", "tokens": ["Ho\u00b7nig", "in", "dem", "kleins\u00b7ten", "Bl\u00fcm\u00b7chen", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die kindlich ungetr\u00fcbte Miene", "tokens": ["Wo", "die", "kind\u00b7lich", "un\u00b7ge\u00b7tr\u00fcb\u00b7te", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Noch der Flor der Wehmuth nicht umwand.", "tokens": ["Noch", "der", "Flor", "der", "Weh\u00b7muth", "nicht", "um\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Goldne Zeiten \u2013 euerm Angedenken", "tokens": ["Gold\u00b7ne", "Zei\u00b7ten", "\u2013", "eu\u00b7erm", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Werd' ich oft in stiller Einsamkeit", "tokens": ["Werd'", "ich", "oft", "in", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Augenblicke der Erinn'rung schenken,", "tokens": ["Au\u00b7gen\u00b7bli\u00b7cke", "der", "Er\u00b7inn'\u00b7rung", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Thr\u00e4nen \u2013 euerm fr\u00fchen Gl\u00fcck geweiht.", "tokens": ["Thr\u00e4\u00b7nen", "\u2013", "eu\u00b7erm", "fr\u00fc\u00b7hen", "Gl\u00fcck", "ge\u00b7weiht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "M\u00f6chte einst der Abend meiner Tage", "tokens": ["M\u00f6ch\u00b7te", "einst", "der", "A\u00b7bend", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mild und freundlich wie der Morgen seyn,", "tokens": ["Mild", "und", "freund\u00b7lich", "wie", "der", "Mor\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KOKOM", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O dann tr\u00fcg ich mit verstummter Klage", "tokens": ["O", "dann", "tr\u00fcg", "ich", "mit", "ver\u00b7stumm\u00b7ter", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jetzt des Mittags schw\u00fclen Sonnenschein.", "tokens": ["Jetzt", "des", "Mit\u00b7tags", "schw\u00fc\u00b7len", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Auf der Kindheit l\u00e4ngst vergangnen Tagen", "tokens": ["Auf", "der", "Kind\u00b7heit", "l\u00e4ngst", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weilet oft mein still umw\u00f6lkter Sinn,", "tokens": ["Wei\u00b7let", "oft", "mein", "still", "um\u00b7w\u00f6lk\u00b7ter", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und aus meinem Herzen brechen Klagen,", "tokens": ["Und", "aus", "mei\u00b7nem", "Her\u00b7zen", "bre\u00b7chen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dass ich nun nicht mehr so gl\u00fccklich bin!", "tokens": ["Dass", "ich", "nun", "nicht", "mehr", "so", "gl\u00fcck\u00b7lich", "bin", "!"], "token