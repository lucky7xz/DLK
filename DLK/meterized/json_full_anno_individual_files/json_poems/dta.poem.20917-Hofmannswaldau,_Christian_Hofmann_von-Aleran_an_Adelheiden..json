{"dta.poem.20917": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aleran an Adelheiden.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Was schreibt man mir itzund? die Rosen seyn", "tokens": ["Was", "schreibt", "man", "mir", "it\u00b7zund", "?", "die", "Ro\u00b7sen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIS", "PPER", "ADV", "$.", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verlohren/", "tokens": ["ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und Adelheidens Glantz durch mich hinweg", "tokens": ["Und", "A\u00b7del\u00b7hei\u00b7dens", "Glantz", "durch", "mich", "hin\u00b7weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "geraft?", "tokens": ["ge\u00b7raft", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Sie werden wie es scheint dir itzund neu gebohren/", "tokens": ["Sie", "wer\u00b7den", "wie", "es", "scheint", "dir", "it\u00b7zund", "neu", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deine Kummer Fluth erfrischet ihre Krafft.", "tokens": ["Und", "dei\u00b7ne", "Kum\u00b7mer", "Fluth", "er\u00b7fri\u00b7schet", "ih\u00b7re", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es scheint die Liebligkeit die kan dich nicht verlassen/", "tokens": ["Es", "scheint", "die", "Lieb\u00b7lig\u00b7keit", "die", "kan", "dich", "nicht", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie bittet allezeit dir freye Taffel an/", "tokens": ["Sie", "bit\u00b7tet", "al\u00b7le\u00b7zeit", "dir", "frey\u00b7e", "Taf\u00b7fel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Auge will mich itzt in nasse Garnen fassen/", "tokens": ["Dein", "Au\u00b7ge", "will", "mich", "itzt", "in", "nas\u00b7se", "Gar\u00b7nen", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nach dem sein Feuer mir Gewalt hat angethan.", "tokens": ["Nach", "dem", "sein", "Feu\u00b7er", "mir", "Ge\u00b7walt", "hat", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "PPER", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch weine nicht zuviel/ wir haben nichts begangen/", "tokens": ["Doch", "wei\u00b7ne", "nicht", "zu\u00b7viel", "/", "wir", "ha\u00b7ben", "nichts", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$(", "PPER", "VAFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was Folter/ Eisen/ Strang/ und Feuers w\u00fcrdig sey;", "tokens": ["Was", "Fol\u00b7ter", "/", "Ei\u00b7sen", "/", "Strang", "/", "und", "Feu\u00b7ers", "w\u00fcr\u00b7dig", "sey", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir haben keinen Krieg zusammen angefangen/", "tokens": ["Wir", "ha\u00b7ben", "kei\u00b7nen", "Krieg", "zu\u00b7sam\u00b7men", "an\u00b7ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unser B\u00fcndn\u00fc\u00df wei\u00df nichts von Verr\u00e4therey.", "tokens": ["Und", "un\u00b7ser", "B\u00fcnd\u00b7n\u00fc\u00df", "wei\u00df", "nichts", "von", "Ver\u00b7r\u00e4\u00b7the\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Schuld so uns betrifft/ besteht in Lust und lieben/", "tokens": ["Die", "Schuld", "so", "uns", "be\u00b7tr\u00b7ifft", "/", "be\u00b7steht", "in", "Lust", "und", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$(", "VVFIN", "APPR", "NN", "KON", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Es hat ja die Natur nicht Straff auf di\u00df gestellt/", "tokens": ["Es", "hat", "ja", "die", "Na\u00b7tur", "nicht", "Straff", "auf", "di\u00df", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "NN", "APPR", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Himmel lie\u00df es frey die ersten V\u00f6lcker \u00fcben;", "tokens": ["Der", "Him\u00b7mel", "lie\u00df", "es", "frey", "die", "ers\u00b7ten", "V\u00f6l\u00b7cker", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es war ein Zeitvertreib und Spiel der alten Welt;", "tokens": ["Es", "war", "ein", "Zeit\u00b7ver\u00b7treib", "und", "Spiel", "der", "al\u00b7ten", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Seyd fruchtbar hat zwar Gott in Marmel nicht gegra-", "tokens": ["Seyd", "frucht\u00b7bar", "hat", "zwar", "Gott", "in", "Mar\u00b7mel", "nicht", "ge\u00b7gra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "VAFIN", "ADV", "NN", "APPR", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Doch schrieb Er in das Bluth di\u00df Paradies Geboth/", "tokens": ["Doch", "schrieb", "Er", "in", "das", "Bluth", "di\u00df", "Pa\u00b7ra\u00b7dies", "Ge\u00b7both", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PDS", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was will man bessern Grund von dieser Sache haben?", "tokens": ["Was", "will", "man", "bes\u00b7sern", "Grund", "von", "die\u00b7ser", "Sa\u00b7che", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADJA", "NN", "APPR", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Taffel war der Mensch/ der Schreiber aber Gott.", "tokens": ["Die", "Taf\u00b7fel", "war", "der", "Mensch", "/", "der", "Schrei\u00b7ber", "a\u00b7ber", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nach diesem haben wir durch Sch\u00e4rffe der Gesetze/", "tokens": ["Nach", "die\u00b7sem", "ha\u00b7ben", "wir", "durch", "Sch\u00e4rf\u00b7fe", "der", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das schwere Joch verst\u00e4rckt: wie irrt die Sterbligkeit!", "tokens": ["Das", "schwe\u00b7re", "Joch", "ver\u00b7st\u00e4rckt", ":", "wie", "irrt", "die", "Ster\u00b7blig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie strickt ihr durch die Kunst selbst kummerreiche Ne-", "tokens": ["Sie", "strickt", "ihr", "durch", "die", "Kunst", "selbst", "kum\u00b7mer\u00b7rei\u00b7che", "Ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "tze/", "tokens": ["tze", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.28": {"text": "Und frist sich der Gestalt durch Klugheit vor der Zeit.", "tokens": ["Und", "frist", "sich", "der", "Ge\u00b7stalt", "durch", "Klug\u00b7heit", "vor", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Eh\u2019 war erstlich nur ein Schlu\u00df in dem Gem\u00fcthe/", "tokens": ["Die", "Eh'", "war", "erst\u00b7lich", "nur", "ein", "Schlu\u00df", "in", "dem", "Ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der endlich auch den Leib zu einen Zeugen nam/", "tokens": ["Der", "end\u00b7lich", "auch", "den", "Leib", "zu", "ei\u00b7nen", "Zeu\u00b7gen", "nam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer sprachte dazumahl von Stand und von Gebl\u00fc-", "tokens": ["Wer", "sprach\u00b7te", "da\u00b7zu\u00b7mahl", "von", "Stand", "und", "von", "Ge\u00b7bl\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "the/", "tokens": ["the", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Nach dem die erste Braut zu ihrem Manne kam?", "tokens": ["Nach", "dem", "die", "ers\u00b7te", "Braut", "zu", "ih\u00b7rem", "Man\u00b7ne", "kam", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Ehberedung war geschrieben in den Hertzen/", "tokens": ["Die", "Eh\u00b7be\u00b7re\u00b7dung", "war", "ge\u00b7schrie\u00b7ben", "in", "den", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Tinte war das Bluth/ das Siegel war ein Ku\u00df/", "tokens": ["Die", "Tin\u00b7te", "war", "das", "Bluth", "/", "das", "Sie\u00b7gel", "war", "ein", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie hatten sonst kein Licht/ als nur des Himmels Ker-", "tokens": ["Sie", "hat\u00b7ten", "sonst", "kein", "Licht", "/", "als", "nur", "des", "Him\u00b7mels", "Ker"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$(", "KOKOM", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "tzen/", "tokens": ["tzen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.38": {"text": "Und liebten keine Pracht bey diesem Uberflu\u00df;", "tokens": ["Und", "lieb\u00b7ten", "kei\u00b7ne", "Pracht", "bey", "die\u00b7sem", "U\u00b7ber\u00b7flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Braut- und auch Trauring kam aus eines Meisters", "tokens": ["Braut", "und", "auch", "Trau\u00b7ring", "kam", "aus", "ei\u00b7nes", "Meis\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADV", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.40": {"text": "H\u00e4nden/", "tokens": ["H\u00e4n\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Denn Gold lag dazumahl noch in der Mutter Scho\u00df/", "tokens": ["Denn", "Gold", "lag", "da\u00b7zu\u00b7mahl", "noch", "in", "der", "Mut\u00b7ter", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Jhr Bette knackte nicht und war nicht umzuwenden/", "tokens": ["Ihr", "Bet\u00b7te", "knack\u00b7te", "nicht", "und", "war", "nicht", "um\u00b7zu\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "KON", "VAFIN", "PTKNEG", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der Himmel war die Deck\u2019/ ihr Pf\u00fchl der Erdenklo\u00df.", "tokens": ["Der", "Him\u00b7mel", "war", "die", "Deck'", "/", "ihr", "Pf\u00fchl", "der", "Er\u00b7den\u00b7klo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Doch kan man freylich nicht Gebr\u00e4uche hintertreiben/", "tokens": ["Doch", "kan", "man", "frey\u00b7lich", "nicht", "Ge\u00b7br\u00e4u\u00b7che", "hin\u00b7ter\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sie meistern die Natur und seyn der hohe Rath;", "tokens": ["Sie", "meis\u00b7tern", "die", "Na\u00b7tur", "und", "seyn", "der", "ho\u00b7he", "Rath", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Sie seyn fast M\u00fcntzen Arth/ ihr Werth der mu\u00df ver-", "tokens": ["Sie", "seyn", "fast", "M\u00fcnt\u00b7zen", "Ar\u00b7th", "/", "ihr", "Werth", "der", "