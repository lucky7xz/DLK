{"textgrid.poem.37099": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ku\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ward Unsterblichkeit mir? Stieg ein Olympier", "tokens": ["Ward", "U\u00b7nsterb\u00b7lich\u00b7keit", "mir", "?", "Stieg", "ein", "O\u00b7lym\u00b7pier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPER", "$.", "NN", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit der Schale herab? Bebte sein goldner Kelch,", "tokens": ["Mit", "der", "Scha\u00b7le", "her\u00b7ab", "?", "Beb\u00b7te", "sein", "gold\u00b7ner", "Kelch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Voll der Trauben des Himmels,", "tokens": ["Voll", "der", "Trau\u00b7ben", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Um die Lippe des Taumelnden?", "tokens": ["Um", "die", "Lip\u00b7pe", "des", "Tau\u00b7meln\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wehe K\u00fchlung mir zu, wann du mir wiederum", "tokens": ["We\u00b7he", "K\u00fch\u00b7lung", "mir", "zu", ",", "wann", "du", "mir", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Reichst den gl\u00fchenden Kelch, da\u00df mir die Seele nicht", "tokens": ["Reichst", "den", "gl\u00fc\u00b7hen\u00b7den", "Kelch", ",", "da\u00df", "mir", "die", "See\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ganz im Feuer zerflie\u00dfe;", "tokens": ["Ganz", "im", "Feu\u00b7er", "zer\u00b7flie\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wehe, wehe mir K\u00fchlung zu!", "tokens": ["We\u00b7he", ",", "we\u00b7he", "mir", "K\u00fch\u00b7lung", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Unter Bl\u00fcthen des Mays spielt' ich mit ihrer Hand;", "tokens": ["Un\u00b7ter", "Bl\u00fc\u00b7then", "des", "Mays", "spielt'", "ich", "mit", "ih\u00b7rer", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Kos'te liebelnd mit ihr, schaute mein schwebendes", "tokens": ["Kos'\u00b7te", "lie\u00b7belnd", "mit", "ihr", ",", "schau\u00b7te", "mein", "schwe\u00b7ben\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPER", "$,", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bild im Auge des M\u00e4dchens;", "tokens": ["Bild", "im", "Au\u00b7ge", "des", "M\u00e4d\u00b7chens", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Raubt' ihr bebend den ersten Ku\u00df!", "tokens": ["Raubt'", "ihr", "be\u00b7bend", "den", "ers\u00b7ten", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ewig strahlt die Gestalt mir in der Seel' herauf;", "tokens": ["E\u00b7wig", "strahlt", "die", "Ge\u00b7stalt", "mir", "in", "der", "Seel'", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ewig flieget der Ku\u00df, wie ein versengend Feur,", "tokens": ["E\u00b7wig", "flie\u00b7get", "der", "Ku\u00df", ",", "wie", "ein", "ver\u00b7sen\u00b7gend", "Feur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "VVPP", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mir durch Mark und Gebeine;", "tokens": ["Mir", "durch", "Mark", "und", "Ge\u00b7bei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ewig zittert mein Herz nach ihr!", "tokens": ["E\u00b7wig", "zit\u00b7tert", "mein", "Herz", "nach", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["A