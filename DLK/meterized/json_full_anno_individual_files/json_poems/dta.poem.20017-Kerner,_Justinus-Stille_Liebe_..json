{"dta.poem.20017": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Stille Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich dich in Liedern preisen,", "tokens": ["K\u00f6nnt'", "ich", "dich", "in", "Lie\u00b7dern", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4ng' ich dir das l\u00e4ngste Lied,", "tokens": ["S\u00e4ng'", "ich", "dir", "das", "l\u00e4ngs\u00b7te", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja! ich w\u00fcrd' in allen Weisen", "tokens": ["Ja", "!", "ich", "w\u00fcrd'", "in", "al\u00b7len", "Wei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu singen nimmer m\u00fcd.", "tokens": ["Dich", "zu", "sin\u00b7gen", "nim\u00b7mer", "m\u00fcd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch was immer mich betr\u00fcbte", "tokens": ["Doch", "was", "im\u00b7mer", "mich", "be\u00b7tr\u00fcb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist, da\u00df ich nur immer stumm,", "tokens": ["Ist", ",", "da\u00df", "ich", "nur", "im\u00b7mer", "stumm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tragen kann dich Herzgeliebte!", "tokens": ["Tra\u00b7gen", "kann", "dich", "Herz\u00b7ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Busens Heiligthum.", "tokens": ["In", "des", "Bu\u00b7sens", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und da\u00df du, was laut ich sage,", "tokens": ["Und", "da\u00df", "du", ",", "was", "laut", "ich", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Oder preis in Sangeslust,", "tokens": ["O\u00b7der", "preis", "in", "San\u00b7ges\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinest, da\u00df ich tiefer trage", "tokens": ["Mei\u00b7nest", ",", "da\u00df", "ich", "tie\u00b7fer", "tra\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als dich, Herz, in warmer Brust.", "tokens": ["Als", "dich", ",", "Herz", ",", "in", "war\u00b7mer", "Brust", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieser Schmerz hat mich bezwungen,", "tokens": ["Die\u00b7ser", "Schmerz", "hat", "mich", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich sang die\u00df kleine Lied,", "tokens": ["Da\u00df", "ich", "sang", "die\u00df", "klei\u00b7ne", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch von bittrem Leid durchdrungen,", "tokens": ["Doch", "von", "bitt\u00b7rem", "Leid", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df noch keins auf dich gerieth.", "tokens": ["Da\u00df", "noch", "keins", "auf", "dich", "ge\u00b7rieth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}