{"textgrid.poem.66306": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Todesgedanken im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche Stimme schallet", "tokens": ["Wel\u00b7che", "Stim\u00b7me", "schal\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vom Gebirg und wallet", "tokens": ["Vom", "Ge\u00b7birg", "und", "wal\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Um mein lauschend Ohr;", "tokens": ["Um", "mein", "lau\u00b7schend", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Welche Silbert\u00f6ne", "tokens": ["Wel\u00b7che", "Sil\u00b7ber\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rufen: \u00bbMeine Sch\u00f6ne,", "tokens": ["Ru\u00b7fen", ":", "\u00bb", "Mei\u00b7ne", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auf! und tritt hervor!", "tokens": ["Auf", "!", "und", "tritt", "her\u00b7vor", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Schaue nur,", "tokens": ["Schau\u00b7e", "nur", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wie die Natur", "tokens": ["Wie", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Sich in ihrer Pracht erhebet", "tokens": ["Sich", "in", "ih\u00b7rer", "Pracht", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und auf's neue lebet.", "tokens": ["Und", "auf's", "neu\u00b7e", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schnee und Regeng\u00fcsse", "tokens": ["Schnee", "und", "Re\u00b7gen\u00b7g\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind dahin. Die Fl\u00fcsse", "tokens": ["Sind", "da\u00b7hin", ".", "Die", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PAV", "$.", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wandeln ihren Lauf.", "tokens": ["Wan\u00b7deln", "ih\u00b7ren", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Komm aus deiner H\u00fctte,", "tokens": ["Komm", "aus", "dei\u00b7ner", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unter deinem Schritte", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "Schrit\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sprossen Blumen auf.", "tokens": ["Spros\u00b7sen", "Blu\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Komm und schau", "tokens": ["Komm", "und", "schau"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Den Morgenthau", "tokens": ["Den", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Tausend goldne Sonnenstrahlen", "tokens": ["Tau\u00b7send", "gold\u00b7ne", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf die Veilchen malen.", "tokens": ["Auf", "die", "Veil\u00b7chen", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Balsamreiche D\u00fcfte", "tokens": ["Bal\u00b7sam\u00b7rei\u00b7che", "D\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwimmen durch die L\u00fcfte;", "tokens": ["Schwim\u00b7men", "durch", "die", "L\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn der Weinstock bl\u00fcht.", "tokens": ["Denn", "der", "Wein\u00b7stock", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00f6r! die Turteltaube", "tokens": ["H\u00f6r", "!", "die", "Tur\u00b7tel\u00b7tau\u00b7be"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Girrt aus jener Laube", "tokens": ["Girrt", "aus", "je\u00b7ner", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dir ein Fr\u00fchlingslied.", "tokens": ["Dir", "ein", "Fr\u00fch\u00b7lings\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Auf! der Mai", "tokens": ["Auf", "!", "der", "Mai"], "token_info": ["word", "punct", "word", "word"], "pos": ["APPR", "$.", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Fliegt sonst vorbei.", "tokens": ["Fliegt", "sonst", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sieh, die Feigenb\u00e4ume zeigen", "tokens": ["Sieh", ",", "die", "Fei\u00b7gen\u00b7b\u00e4u\u00b7me", "zei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Knoten an den Zweigen.\u00ab", "tokens": ["Kno\u00b7ten", "an", "den", "Zwei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Meiner Jugend Leiter,", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freund, o rede weiter;", "tokens": ["Freund", ",", "o", "re\u00b7de", "wei\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn ich h\u00f6re gern. \u2013", "tokens": ["Denn", "ich", "h\u00f6\u00b7re", "gern", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Doch die Stimme schweiget", "tokens": ["Doch", "die", "Stim\u00b7me", "schwei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und der Fr\u00fchling zeiget", "tokens": ["Und", "der", "Fr\u00fch\u00b7ling", "zei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Spuren seines Herrn.", "tokens": ["Spu\u00b7ren", "sei\u00b7nes", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wo Er war,", "tokens": ["Wo", "Er", "war", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Seh' ich ein Paar", "tokens": ["Seh'", "ich", "ein", "Paar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Junge Fr\u00fchlingsrosen bl\u00fchen,", "tokens": ["Jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die wie Sterne gl\u00fchen.", "tokens": ["Die", "wie", "Ster\u00b7ne", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aus dem Erdenscho\u00dfe", "tokens": ["Aus", "dem", "Er\u00b7den\u00b7scho\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schallt von jeder Rose", "tokens": ["Schallt", "von", "je\u00b7der", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gottes Ruhm hinauf.", "tokens": ["Got\u00b7tes", "Ruhm", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kleine S\u00e4nger schl\u00fcpfen", "tokens": ["Klei\u00b7ne", "S\u00e4n\u00b7ger", "schl\u00fcp\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In den Busch und h\u00fcpfen", "tokens": ["In", "den", "Busch", "und", "h\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Jubilirend auf.", "tokens": ["Ju\u00b7bi\u00b7li\u00b7rend", "auf", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "----+", "measure": "unknown.measure.single"}, "line.7": {"text": "Wo die Pracht", "tokens": ["Wo", "die", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Des Fr\u00fchlings lacht,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Auf dem Schauplatz vom Vergn\u00fcgen", "tokens": ["Auf", "dem", "Schau\u00b7platz", "vom", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sollen ", "tokens": ["Sol\u00b7len"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Grabgedanken, h\u00e4rter,", "tokens": ["Grab\u00b7ge\u00b7dan\u00b7ken", ",", "h\u00e4r\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schneidender, als Schwerter,", "tokens": ["Schnei\u00b7den\u00b7der", ",", "als", "Schwer\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fahrt ihr durch mein Herz.", "tokens": ["Fahrt", "ihr", "durch", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Arme Fr\u00fchlingsscenen,", "tokens": ["Ar\u00b7me", "Fr\u00fch\u00b7lings\u00b7sce\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hemmt ihr meine Thr\u00e4nen?", "tokens": ["Hemmt", "ihr", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Stillt ihr meinen Schmerz?", "tokens": ["Stillt", "ihr", "mei\u00b7nen", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nur das Wort", "tokens": ["Nur", "das", "Wort"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ist schon ein Mord:", "tokens": ["Ist", "schon", "ein", "Mord", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Unter jenem gr\u00fcnen Haine", "tokens": ["Un\u00b7ter", "je\u00b7nem", "gr\u00fc\u00b7nen", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Liegen Todtenbeine.", "tokens": ["Lie\u00b7gen", "Tod\u00b7ten\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Alles um mich lebet,", "tokens": ["Al\u00b7les", "um", "mich", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jener Baum erhebet", "tokens": ["Je\u00b7ner", "Baum", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6n sein Bl\u00fcthenhaupt.", "tokens": ["Sch\u00f6n", "sein", "Bl\u00fc\u00b7then\u00b7haupt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber seine Kr\u00e4fte", "tokens": ["A\u00b7ber", "sei\u00b7ne", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und sein Schmuck sind S\u00e4fte,", "tokens": ["Und", "sein", "Schmuck", "sind",