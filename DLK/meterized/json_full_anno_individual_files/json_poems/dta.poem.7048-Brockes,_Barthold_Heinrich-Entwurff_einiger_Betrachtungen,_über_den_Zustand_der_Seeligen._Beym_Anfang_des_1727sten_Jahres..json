{"dta.poem.7048": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du aber, grosses ALL! erbarm\u2019 dich mein, und lencke", "tokens": ["Du", "a\u00b7ber", ",", "gros\u00b7ses", "AlL", "!", "er\u00b7barm'", "dich", "mein", ",", "und", "len\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Sinnen dergestalt,", "tokens": ["Mein", "Sin\u00b7nen", "der\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich von Deiner ", "tokens": ["Da\u00df", "ich", "von", "Dei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nichts niedertr\u00e4chtiges, nichts unanst\u00e4ndigs dencke.", "tokens": ["Nichts", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ges", ",", "nichts", "un\u00b7an\u00b7st\u00e4n\u00b7digs", "den\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "$,", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gieb, da\u00df ich auch in dem nicht fehle,", "tokens": ["Gieb", ",", "da\u00df", "ich", "auch", "in", "dem", "nicht", "feh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADV", "APPR", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich zu diesem ", "tokens": ["Was", "ich", "zu", "die\u00b7sem"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}}}}}