{"dta.poem.19531": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der r\u00f6mische Brunnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Aufsteigt der Strahl und fallend gie\u00dft", "tokens": ["Auf\u00b7steigt", "der", "Strahl", "und", "fal\u00b7lend", "gie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er voll der Marmorschaale Rund,", "tokens": ["Er", "voll", "der", "Mar\u00b7mor\u00b7schaa\u00b7le", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, sich verschleiernd, \u00fcberflie\u00dft", "tokens": ["Die", ",", "sich", "ver\u00b7schlei\u00b7ernd", ",", "\u00fc\u00b7berf\u00b7lie\u00dft"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "$,", "PRF", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einer zweiten Schaale Grund;", "tokens": ["In", "ei\u00b7ner", "zwei\u00b7ten", "Schaa\u00b7le", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die zweite giebt, sie wird zu reich,", "tokens": ["Die", "zwei\u00b7te", "giebt", ",", "sie", "wird", "zu", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der dritten wallend ihre Flut,", "tokens": ["Der", "drit\u00b7ten", "wal\u00b7lend", "ih\u00b7re", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jede nimmt und giebt zugleich", "tokens": ["Und", "je\u00b7de", "nimmt", "und", "giebt", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und str\u00f6mt und ruht.", "tokens": ["Und", "str\u00f6mt", "und", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}