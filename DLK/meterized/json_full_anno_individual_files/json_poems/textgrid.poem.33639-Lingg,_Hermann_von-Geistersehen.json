{"textgrid.poem.33639": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Geistersehen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu lautes Klagen um die Toten", "tokens": ["Zu", "lau\u00b7tes", "Kla\u00b7gen", "um", "die", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verst\u00f6re, sagt man, ihre Ruh.", "tokens": ["Ver\u00b7st\u00f6\u00b7re", ",", "sagt", "man", ",", "ih\u00b7re", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PIS", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schweben dann wie Friedensboten", "tokens": ["Sie", "schwe\u00b7ben", "dann", "wie", "Frie\u00b7dens\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lager der Verlassnen zu;", "tokens": ["Dem", "La\u00b7ger", "der", "Ver\u00b7lass\u00b7nen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und unsrer Tr\u00e4nen \u00fcberdr\u00fcssig", "tokens": ["Und", "uns\u00b7rer", "Tr\u00e4\u00b7nen", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erscheinen sie zwar bleich und kalt,", "tokens": ["Er\u00b7schei\u00b7nen", "sie", "zwar", "bleich", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch strahlend und wie Nebel fl\u00fcssig,", "tokens": ["Doch", "strah\u00b7lend", "und", "wie", "Ne\u00b7bel", "fl\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In \u00e4therhafter Lichtgestalt.", "tokens": ["In", "\u00e4t\u00b7her\u00b7haf\u00b7ter", "Licht\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, w\u00e4r' es so, und lebt' ein Leben,", "tokens": ["Ach", ",", "w\u00e4r'", "es", "so", ",", "und", "lebt'", "ein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das aus Verwesung sich entrei\u00dft,", "tokens": ["Das", "aus", "Ver\u00b7we\u00b7sung", "sich", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, keine Fiber sollte beben,", "tokens": ["Nein", ",", "kei\u00b7ne", "Fi\u00b7ber", "soll\u00b7te", "be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4t' in der Nacht zu mir dein Geist!", "tokens": ["Tr\u00e4t'", "in", "der", "Nacht", "zu", "mir", "dein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wenn mich die milden Augen gr\u00fc\u00dften,", "tokens": ["Wenn", "mich", "die", "mil\u00b7den", "Au\u00b7gen", "gr\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In denen sich ein Jenseits malt,", "tokens": ["In", "de\u00b7nen", "sich", "ein", "Jen\u00b7seits", "malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir w\u00e4r' es, wie ein Tau den W\u00fcsten,", "tokens": ["Mir", "w\u00e4r'", "es", ",", "wie", "ein", "Tau", "den", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Licht, das einem Kerker strahlt!", "tokens": ["Wie", "Licht", ",", "das", "ei\u00b7nem", "Ker\u00b7ker", "strahlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu lautes Klagen um die Toten", "tokens": ["Zu", "lau\u00b7tes", "Kla\u00b7gen", "um", "die", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verst\u00f6re, sagt man, i