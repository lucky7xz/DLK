{"dta.poem.17597": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "219.  Der Mensch sol nicht ein Mensch  \n bleiben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch bleib doch nicht ein Mensch: man mu\u00df aufs", "tokens": ["Mensch", "bleib", "doch", "nicht", "ein", "Mensch", ":", "man", "mu\u00df", "aufs"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$.", "PIS", "VMFIN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00f6chste kommen.", "tokens": ["h\u00f6chs\u00b7te", "kom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bey Gotte werden nur die G\u00f6tter angenommen.", "tokens": ["Bey", "Got\u00b7te", "wer\u00b7den", "nur", "die", "G\u00f6t\u00b7ter", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}