{"dta.poem.20092": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ein J\u00e4ger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn einen J\u00e4ger, man hei\u00dft ihn \u201e", "tokens": ["Ich", "kenn", "ei\u00b7nen", "J\u00e4\u00b7ger", ",", "man", "hei\u00dft", "ihn", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PIS", "VVFIN", "PPER", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Seine Wang ist bla\u00df, sein Speer ist roth,", "tokens": ["Sei\u00b7ne", "Wang", "ist", "bla\u00df", ",", "sein", "Speer", "ist", "roth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sein Forst ist die Welt, er zieht auf die Pirsch,", "tokens": ["Sein", "Forst", "ist", "die", "Welt", ",", "er", "zieht", "auf", "die", "Pirsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und jaget Elenn und Edelhirsch.", "tokens": ["Und", "ja\u00b7get", "E\u00b7lenn", "und", "E\u00b7del\u00b7hirsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Im V\u00f6lkerkrieg, auf blutigem Feld,", "tokens": ["Im", "V\u00f6l\u00b7ker\u00b7krieg", ",", "auf", "blu\u00b7ti\u00b7gem", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ist\u2019s wo er sein ", "tokens": ["Ist's", "wo", "er", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "PPOSAT"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ha\u00df, Ehrsucht und Geizen nach Ruhmesschall", "tokens": ["Ha\u00df", ",", "Ehr\u00b7sucht", "und", "Gei\u00b7zen", "nach", "Ruh\u00b7mes\u00b7schall"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Nicht f\u00fcrcht ich ihn selber, wie nah er auch droht,", "tokens": ["Nicht", "f\u00fcrcht", "ich", "ihn", "sel\u00b7ber", ",", "wie", "nah", "er", "auch", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Doch wohl seine R\u00fcden: Gram, Krankheit und", "tokens": ["Doch", "wohl", "sei\u00b7ne", "R\u00fc\u00b7den", ":", "Gram", ",", "Krank\u00b7heit", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$.", "NE", "$,", "NN", "KON"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noth,", "tokens": ["Noth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die Meute, die st\u00fcckweis das Leben zerfetzt,", "tokens": ["Die", "Meu\u00b7te", ",", "die", "st\u00fcck\u00b7weis", "das", "Le\u00b7ben", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}