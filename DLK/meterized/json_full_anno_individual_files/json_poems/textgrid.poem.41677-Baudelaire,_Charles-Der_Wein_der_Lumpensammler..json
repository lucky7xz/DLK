{"textgrid.poem.41677": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Der Wein der Lumpensammler.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft schauen wir, wie in der Flammen rotem Flirren,", "tokens": ["Oft", "schau\u00b7en", "wir", ",", "wie", "in", "der", "Flam\u00b7men", "ro\u00b7tem", "Flir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im wehnden Flackerschein, bei der Laternen Klirren,", "tokens": ["Im", "wehn\u00b7den", "Fla\u00b7cker\u00b7schein", ",", "bei", "der", "La\u00b7ter\u00b7nen", "Klir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Scho\u00df der alten Stadt, von Schmutz und Elend voll,", "tokens": ["Im", "Scho\u00df", "der", "al\u00b7ten", "Stadt", ",", "von", "Schmutz", "und", "E\u00b7lend", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort, wo die Menschheit st\u00f6hnt in wetterschwangrem Groll,", "tokens": ["Dort", ",", "wo", "die", "Menschheit", "st\u00f6hnt", "in", "wet\u00b7ter\u00b7schwang\u00b7rem", "Groll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Lumpensammler kommt, der, wie ein Dichter schwankend,", "tokens": ["Ein", "Lum\u00b7pen\u00b7samm\u00b7ler", "kommt", ",", "der", ",", "wie", "ein", "Dich\u00b7ter", "schwan\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wild sch\u00fcttelt mit dem Kopf, an alte Mauern wankend;", "tokens": ["Wild", "sch\u00fct\u00b7telt", "mit", "dem", "Kopf", ",", "an", "al\u00b7te", "Mau\u00b7ern", "wan\u00b7kend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und voll Verachtung f\u00fcr der Sp\u00e4her feilen Hauf", "tokens": ["Und", "voll", "Ver\u00b7ach\u00b7tung", "f\u00fcr", "der", "Sp\u00e4\u00b7her", "fei\u00b7len", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "L\u00e4\u00dft seinem Hoffen er im Rausche freien Lauf.", "tokens": ["L\u00e4\u00dft", "sei\u00b7nem", "Hof\u00b7fen", "er", "im", "Rau\u00b7sche", "frei\u00b7en", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er schw\u00f6rt Gel\u00fcbde, gibt erhabene Gesetze,", "tokens": ["Er", "schw\u00f6rt", "Ge\u00b7l\u00fcb\u00b7de", ",", "gibt", "er\u00b7ha\u00b7be\u00b7ne", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hebt Gest\u00fcrzte auf, zerrei\u00dft der B\u00f6sen Netze,", "tokens": ["Er", "hebt", "Ge\u00b7st\u00fcrz\u00b7te", "auf", ",", "zer\u00b7rei\u00dft", "der", "B\u00f6\u00b7sen", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Himmel \u00fcberw\u00f6lbt ihn wie ein Baldachin,", "tokens": ["Der", "Him\u00b7mel", "\u00fc\u00b7berw\u00b7\u00f6lbt", "ihn", "wie", "ein", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie trunken macht der Glanz der eignen Tugend ihn.", "tokens": ["Wie", "trun\u00b7ken", "macht", "der", "Glanz", "der", "eig\u00b7nen", "Tu\u00b7gend", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ja, diese Leute, die in Sorgen niederbrechen,", "tokens": ["Ja", ",", "die\u00b7se", "Leu\u00b7te", ",", "die", "in", "Sor\u00b7gen", "nie\u00b7der\u00b7bre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Arbeit schier zermalmt, die lange Jahre schw\u00e4chen,", "tokens": ["Die", "Ar\u00b7beit", "schier", "zer\u00b7malmt", ",", "die", "lan\u00b7ge", "Jah\u00b7re", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gel\u00e4hmt, sich b\u00fcckend vor der Last geh\u00e4uften Schutts,", "tokens": ["Ge\u00b7l\u00e4hmt", ",", "sich", "b\u00fc\u00b7ckend", "vor", "der", "Last", "ge\u00b7h\u00e4uf\u00b7ten", "Schutts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ausgespien Paris, ein wirr Gew\u00fchl von Schmutz,", "tokens": ["Die", "aus\u00b7ge\u00b7spi\u00b7en", "Pa\u00b7ris", ",", "ein", "wirr", "Ge\u00b7w\u00fchl", "von", "Schmutz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie kommen vom Geruch der F\u00e4sser wie umflossen,", "tokens": ["Sie", "kom\u00b7men", "vom", "Ge\u00b7ruch", "der", "F\u00e4s\u00b7ser", "wie", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit kampfergrauter Schar, mit jubelnden Genossen,", "tokens": ["Mit", "kamp\u00b7fer\u00b7grau\u00b7ter", "Schar", ",", "mit", "ju\u00b7beln\u00b7den", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Schnurrbart h\u00e4ngt herab wie Fahnen greisen Ruhms,", "tokens": ["Ihr", "Schnurr\u00b7bart", "h\u00e4ngt", "her\u00b7ab", "wie", "Fah\u00b7nen", "grei\u00b7sen", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KOKOM", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u2013 Siegbogen, Blumen, all der Glanz des Heldentums", "tokens": ["\u2013", "Sieg\u00b7bo\u00b7gen", ",", "Blu\u00b7men", ",", "all", "der", "Glanz", "des", "Hel\u00b7den\u00b7tums"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "$,", "NN", "$,", "PIAT", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Erhebt vor ihnen sich wie eine Zaubersonne,", "tokens": ["Er\u00b7hebt", "vor", "ih\u00b7nen", "sich", "wie", "ei\u00b7ne", "Zau\u00b7ber\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie bringen, ganz bet\u00e4ubt vom Festl\u00e4rm und der Wonne", "tokens": ["Sie", "brin\u00b7gen", ",", "ganz", "be\u00b7t\u00e4ubt", "vom", "Fest\u00b7l\u00e4rm", "und", "der", "Won\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "ADV", "VVPP", "APPRART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Trommeln, des Geschreis, des Horns, der Strahlenpracht,", "tokens": ["Der", "Trom\u00b7meln", ",", "des", "Ge\u00b7schreis", ",", "des", "Horns", ",", "der", "Strah\u00b7len\u00b7pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Glorie ihrem Volk, das Liebe trunken macht,", "tokens": ["Die", "Glo\u00b7rie", "ih\u00b7rem", "Volk", ",", "das", "Lie\u00b7be", "trun\u00b7ken", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So rollt, auf da\u00df er all die nichtge Menschheit letze,", "tokens": ["So", "rollt", ",", "auf", "da\u00df", "er", "all", "die", "nicht\u00b7ge", "Menschheit", "let\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "KOUS", "PPER", "PIAT", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der Wein sein reiches Gold, ein Paktolos der Sch\u00e4tze;", "tokens": ["Der", "Wein", "sein", "rei\u00b7ches", "Gold", ",", "ein", "Pak\u00b7to\u00b7los", "der", "Sch\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Mund des Menschen singt sein Tun er, siegeshehr,", "tokens": ["Im", "Mund", "des", "Men\u00b7schen", "singt", "sein", "Tun", "er", ",", "sie\u00b7ges\u00b7hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich wahren K\u00f6ngen herrscht durch seine Gaben er.", "tokens": ["Gleich", "wah\u00b7ren", "K\u00f6n\u00b7gen", "herrscht", "durch", "sei\u00b7ne", "Ga\u00b7ben", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Den Gram zu tilgen und die Gleichmut sanft zu wiegen", "tokens": ["Den", "Gram", "zu", "til\u00b7gen", "und", "die", "Gleich\u00b7mut", "sanft", "zu", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "KON", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All der Versto\u00dfnen, die ergreist und stumm erliegen,", "tokens": ["All", "der", "Ver\u00b7sto\u00df\u00b7nen", ",", "die", "er\u00b7greist", "und", "stumm", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Gab reuig Gott den Schlaf, der tr\u00f6stlich und gelind.", "tokens": ["Gab", "reu\u00b7ig", "Gott", "den", "Schlaf", ",", "der", "tr\u00f6st\u00b7lich", "und", "ge\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mensch erschuf den Wein, der Sonne heilges Kind!", "tokens": ["Der", "Mensch", "er\u00b7schuf", "den", "Wein", ",", "der", "Son\u00b7ne", "heil\u00b7ges", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Oft schauen wir, wie in der Flammen rotem Flirren,", "tokens": ["Oft", "schau\u00b7en", "wir", ",", "wie", "in", "der", "Flam\u00b7men", "ro\u00b7tem", "Flir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im wehnden Flackerschein, bei der Laternen Klirren,", "tokens": ["Im", "wehn\u00b7den", "Fla\u00b7cker\u00b7schein", ",", "bei", "der", "La\u00b7ter\u00b7nen", "Klir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Scho\u00df der alten Stadt, von Schmutz und Elend voll,", "tokens": ["Im", "Scho\u00df", "der", "al\u00b7ten", "Stadt", ",", "von", "Schmutz", "und", "E\u00b7lend", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.he