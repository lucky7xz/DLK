{"dta.poem.2309": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Sternseher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Abends funkeln Sterne;", "tokens": ["Des", "A\u00b7bends", "fun\u00b7keln", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ist der Himmel helle:", "tokens": ["Und", "ist", "der", "Him\u00b7mel", "hel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So seh\u2019 ich gern ihr Funkeln.", "tokens": ["So", "seh'", "ich", "gern", "ihr", "Fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch seh\u2019 ich meines M\u00e4dchens", "tokens": ["Doch", "seh'", "ich", "mei\u00b7nes", "M\u00e4d\u00b7chens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Recht feuervolle Augen,", "tokens": ["Recht", "feu\u00b7er\u00b7vol\u00b7le", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zugleich im Fenster funkeln:", "tokens": ["Zu\u00b7gleich", "im", "Fens\u00b7ter", "fun\u00b7keln", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So lenk ich schnell mein Auge", "tokens": ["So", "lenk", "ich", "schnell", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom Himmel nach dem Fenster.", "tokens": ["Vom", "Him\u00b7mel", "nach", "dem", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da seh ich be\u00dfre Sterne;", "tokens": ["Da", "seh", "ich", "be\u00df\u00b7re", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da schimmert meinen Augen,", "tokens": ["Da", "schim\u00b7mert", "mei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die allersch\u00f6nste Venus;", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ve\u00b7nus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Da seh\u2019 ich, in der N\u00e4he,", "tokens": ["Da", "seh'", "ich", ",", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Den Glanz der rechten Henne,", "tokens": ["Den", "Glanz", "der", "rech\u00b7ten", "Hen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und einen bessern Wagen.", "tokens": ["Und", "ei\u00b7nen", "bes\u00b7sern", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}