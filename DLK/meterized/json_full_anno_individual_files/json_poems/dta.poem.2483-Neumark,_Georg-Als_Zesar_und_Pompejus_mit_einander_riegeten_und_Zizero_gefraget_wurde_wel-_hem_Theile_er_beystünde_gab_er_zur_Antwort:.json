{"dta.poem.2483": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als Zesar und Pompejus mit einander  \n riegeten/ und Zizero gefraget wurde/ wel-  \n hem Theile er beyst\u00fcnde/ gab er zur Antwort:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Nur der Mensch stehet wol und ist ein reicher Mann/", "tokens": ["Nur", "der", "Mensch", "ste\u00b7het", "wol", "und", "ist", "ein", "rei\u00b7cher", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "KON", "VAFIN", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sich mit seinem Gl\u00fckk\u2019 allzeit vergn\u00fcgen kan.", "tokens": ["Der", "sich", "mit", "sei\u00b7nem", "Gl\u00fc\u00b7kk'", "all\u00b7zeit", "ver\u00b7gn\u00fc\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}