{"dta.poem.19102": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIV.   Auf Herrn Brumhards   Installa-  \n tion   zum   Diaconat   in Jena.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Herr JEsu, ewiger Prophet,", "tokens": ["Herr", "Je\u00b7su", ",", "e\u00b7wi\u00b7ger", "Pro\u00b7phet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarm dich aller, die da lehren,", "tokens": ["Er\u00b7barm", "dich", "al\u00b7ler", ",", "die", "da", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welcher Hertz dein Creutze steht,", "tokens": ["In", "wel\u00b7cher", "Hertz", "dein", "Creut\u00b7ze", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die andrer falche Ruhen st\u00f6hren!", "tokens": ["Die", "an\u00b7drer", "fal\u00b7che", "Ru\u00b7hen", "st\u00f6h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So sehen wir im Geist voraus,", "tokens": ["So", "se\u00b7hen", "wir", "im", "Geist", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sich noch mancher Gnaden-Segen", "tokens": ["Da\u00df", "sich", "noch", "man\u00b7cher", "Gna\u00b7den\u00b7Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ins ordentliche Kirchen-Hau\u00df", "tokens": ["Ins", "or\u00b7dent\u00b7li\u00b7che", "Kir\u00b7chen\u00b7Hau\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Auf deinen Diener werde legen.", "tokens": ["Auf", "dei\u00b7nen", "Die\u00b7ner", "wer\u00b7de", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df ihn in Eingesunckenheit", "tokens": ["La\u00df", "ihn", "in", "Ein\u00b7ge\u00b7sun\u00b7cken\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor seines Volckes S\u00fcnde beten:", "tokens": ["Vor", "sei\u00b7nes", "Vol\u00b7ckes", "S\u00fcn\u00b7de", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df deines Knechtes Freudigkeit", "tokens": ["La\u00df", "dei\u00b7nes", "Knech\u00b7tes", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor aller Menschen Augen treten;", "tokens": ["Vor", "al\u00b7ler", "Men\u00b7schen", "Au\u00b7gen", "tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Angst und Hertzens-Bangigkeit", "tokens": ["Mit", "Angst", "und", "Hert\u00b7zens\u00b7Ban\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Sacrament zum Fluche brauchen,", "tokens": ["Das", "Sa\u00b7cra\u00b7ment", "zum", "Flu\u00b7che", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In heiliger Erschrockenheit", "tokens": ["In", "hei\u00b7li\u00b7ger", "Er\u00b7schro\u00b7cken\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von heissen Bannes-Kr\u00e4ften rauchen.", "tokens": ["Von", "heis\u00b7sen", "Ban\u00b7nes\u00b7Kr\u00e4f\u00b7ten", "rau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df aber auch die s\u00fcsse Frucht", "tokens": ["La\u00df", "a\u00b7ber", "auch", "die", "s\u00fcs\u00b7se", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Amts, das die Vers\u00f6hnung predigt,", "tokens": ["Des", "Amts", ",", "das", "die", "Ver\u00b7s\u00f6h\u00b7nung", "pre\u00b7digt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Seele werden, die sie sucht:", "tokens": ["Der", "See\u00b7le", "wer\u00b7den", ",", "die", "sie", "sucht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh hin, sey deiner Last entledigt.", "tokens": ["Geh", "hin", ",", "sey", "dei\u00b7ner", "Last", "ent\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und das geehrte Testament,", "tokens": ["Und", "das", "ge\u00b7ehr\u00b7te", "Tes\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Blut und Fleisch vom ewgen Leben,", "tokens": ["Das", "Blut", "und", "Fleisch", "vom", "ew\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df ieglichen, ders recht erkennt,", "tokens": ["La\u00df", "ieg\u00b7li\u00b7chen", ",", "ders", "recht", "er\u00b7kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit inniger Bewegung geben.", "tokens": ["Mit", "in\u00b7ni\u00b7ger", "Be\u00b7we\u00b7gung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Tauffe werde hie und dar,", "tokens": ["Die", "Tauf\u00b7fe", "wer\u00b7de", "hie", "und", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der durch dein Blut erl\u00f6\u00dfter Kinder,", "tokens": ["Der", "durch", "dein", "Blut", "er\u00b7l\u00f6\u00df\u00b7ter", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So seligen, so lieben Schaar,", "tokens": ["So", "se\u00b7li\u00b7gen", ",", "so", "lie\u00b7ben", "Schaar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem ewgen Uberwinder;", "tokens": ["Zu", "ei\u00b7nem", "ew\u00b7gen", "U\u00b7ber\u00b7win\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kurtz: Gieb auch diesem noch allhier,", "tokens": ["Kurtz", ":", "Gieb", "auch", "die\u00b7sem", "noch", "all\u00b7hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVIMP", "ADV", "PDAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey Angst und Last unendlich vieles,", "tokens": ["Bey", "Angst", "und", "Last", "un\u00b7end\u00b7lich", "vie\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu deiner Selgen Lehre Zier,", "tokens": ["Zu", "dei\u00b7ner", "Sel\u00b7gen", "Leh\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und zur Erjagung seines Zieles.", "tokens": ["Und", "zur", "Er\u00b7ja\u00b7gung", "sei\u00b7nes", "Zie\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}