{"dta.poem.966": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Spr\u00fcche f\u00fcr die geladenen in T . . .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Bei seiner reise mittag bald zur&#252;ck                 ", "tokens": ["Bei", "sei\u00b7ner", "rei\u00b7se", "mit\u00b7tag", "bald", "zur", "&#252;", "ck"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bald vor sich zum gew\u00f6lke bangen fragens                 ", "tokens": ["Bald", "vor", "sich", "zum", "ge\u00b7w\u00f6l\u00b7ke", "ban\u00b7gen", "fra\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat lange sich der rastende gedreht &#183;                 ", "tokens": ["Hat", "lan\u00b7ge", "sich", "der", "ras\u00b7ten\u00b7de", "ge\u00b7dreht", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["VAFIN", "ADV", "PRF", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durchwallt ist ganzer erden berg und thal                 ", "tokens": ["Durch\u00b7wallt", "ist", "gan\u00b7zer", "er\u00b7den", "berg", "und", "thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADJA", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Soviel an gl&#252;ck und thr&#228;nen hinter ihm &#183;                 ", "tokens": ["So\u00b7viel", "an", "gl", "&#252;", "ck", "und", "thr", "&#228;", "nen", "hin\u00b7ter", "ihm", "&#183;"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity"], "pos": ["PIS", "APPR", "NE", "$(", "NE", "KON", "NN", "$(", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Was kann noch sein? soll er das haupt hier betten                 ", "tokens": ["Was", "kann", "noch", "sein", "?", "soll", "er", "das", "haupt", "hier", "bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "VAINF", "$.", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als an des weges marken oder soll er                 ", "tokens": ["Als", "an", "des", "we\u00b7ges", "mar\u00b7ken", "o\u00b7der", "soll", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVINF", "KON", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In helleren h\u00f6hen lauter noch frohlocken                 ", "tokens": ["In", "hel\u00b7le\u00b7ren", "h\u00f6\u00b7hen", "lau\u00b7ter", "noch", "froh\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "In wildern schluchten tiefer noch erst\u00f6hnen &#183; &#183;                 ", "tokens": ["In", "wil\u00b7dern", "schluch\u00b7ten", "tie\u00b7fer", "noch", "er\u00b7st\u00f6h\u00b7nen", "&#183;", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "XML_entity"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ADV", "VVINF", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So war dies alles erst der morgengang?               ", "tokens": ["So", "war", "dies", "al\u00b7les", "erst", "der", "mor\u00b7gen\u00b7gang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}