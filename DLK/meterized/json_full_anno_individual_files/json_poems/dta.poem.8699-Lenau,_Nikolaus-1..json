{"dta.poem.8699": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Friedlicher Abend senkt sich auf's Gefilde,", "tokens": ["Fried\u00b7li\u00b7cher", "A\u00b7bend", "senkt", "sich", "auf's", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sanft entschlummert Natur, um ihre Z\u00fcge", "tokens": ["Sanft", "ent\u00b7schlum\u00b7mert", "Na\u00b7tur", ",", "um", "ih\u00b7re", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "$,", "KOUI", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schwebt der D\u00e4mm'rung zarte Verh\u00fcllung, und sie", "tokens": ["Schwebt", "der", "D\u00e4m\u00b7m'\u00b7rung", "zar\u00b7te", "Ver\u00b7h\u00fcl\u00b7lung", ",", "und", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,", "KON", "PPER"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "L\u00e4chelt, die holde,", "tokens": ["L\u00e4\u00b7chelt", ",", "die", "hol\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "L\u00e4chelt, ein schlummernd Kind in Vaters Armen,", "tokens": ["L\u00e4\u00b7chelt", ",", "ein", "schlum\u00b7mernd", "Kind", "in", "Va\u00b7ters", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJD", "NN", "APPR", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der voll Liebe zu ihr sich neigt, sein g\u00f6ttlich", "tokens": ["Der", "voll", "Lie\u00b7be", "zu", "ihr", "sich", "neigt", ",", "sein", "g\u00f6tt\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "PRF", "VVFIN", "$,", "PPOSAT", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auge weilt auf ihr, und es weht sein Odem", "tokens": ["Au\u00b7ge", "weilt", "auf", "ihr", ",", "und", "es", "weht", "sein", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ueber ihr Antlitz.", "tokens": ["Ue\u00b7ber", "ihr", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}