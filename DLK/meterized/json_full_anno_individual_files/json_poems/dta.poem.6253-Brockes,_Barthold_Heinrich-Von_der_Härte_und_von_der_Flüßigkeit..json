{"dta.poem.6253": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch ihre Eigenschafft der F\u00fchlbarkeit allein,", "tokens": ["Durch", "ih\u00b7re", "Ei\u00b7gen\u00b7schafft", "der", "F\u00fchl\u00b7bar\u00b7keit", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man dieselbige wol \u00fcberleget,", "tokens": ["Wenn", "man", "die\u00b7sel\u00b7bi\u00b7ge", "wol", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird sie am sichersten von uns erweget,", "tokens": ["Wird", "sie", "am", "si\u00b7chers\u00b7ten", "von", "uns", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am besten untersuchet seyn.", "tokens": ["Am", "bes\u00b7ten", "un\u00b7ter\u00b7su\u00b7chet", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In unsrer luckren Erd\u2019, in welcher Thon und Sand", "tokens": ["In", "uns\u00b7rer", "luck\u00b7ren", "Erd'", ",", "in", "wel\u00b7cher", "Thon", "und", "Sand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PWAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf unterschiedne Art gekr\u00fcmmte Wege finden,", "tokens": ["Auf", "un\u00b7ter\u00b7schied\u00b7ne", "Art", "ge\u00b7kr\u00fcmm\u00b7te", "We\u00b7ge", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sucht Schwefel, Saltz, und sonsten allerhand", "tokens": ["Sucht", "Schwe\u00b7fel", ",", "Saltz", ",", "und", "sons\u00b7ten", "al\u00b7ler\u00b7hand"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "KON", "ADV", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gantz kleine C\u00f6rperchen, geformt verschiedentlich,", "tokens": ["Gantz", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7chen", ",", "ge\u00b7formt", "ver\u00b7schie\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVPP", "ADJD", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Verschiedlich durchgesiebt, in jener sich", "tokens": ["Ver\u00b7schied\u00b7lich", "durch\u00b7ge\u00b7siebt", ",", "in", "je\u00b7ner", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "PDAT", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als fortgezogen, zu verbinden.", "tokens": ["Als", "fort\u00b7ge\u00b7zo\u00b7gen", ",", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie dringen, da sie erst von ihr getrennet seyn,", "tokens": ["Sie", "drin\u00b7gen", ",", "da", "sie", "erst", "von", "ihr", "ge\u00b7tren\u00b7net", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich in der Erden L\u00f6cher ein:", "tokens": ["Sich", "in", "der", "Er\u00b7den", "L\u00f6\u00b7cher", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie f\u00fcgen, r\u00fccken sich, verwickeln sich. Die Last,", "tokens": ["Sie", "f\u00fc\u00b7gen", ",", "r\u00fc\u00b7cken", "sich", ",", "ver\u00b7wi\u00b7ckeln", "sich", ".", "Die", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Druck, die Ungleichheit, verursacht, da\u00df das Regen", "tokens": ["Der", "Druck", ",", "die", "Un\u00b7gleich\u00b7heit", ",", "ver\u00b7ur\u00b7sacht", ",", "da\u00df", "das", "Re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In ihnen bald vergeht, und sie sich nicht bewegen.", "tokens": ["In", "ih\u00b7nen", "bald", "ver\u00b7geht", ",", "und", "sie", "sich", "nicht", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$,", "KON", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es h\u00e4ngen sich die kleinen Aest\u2019", "tokens": ["Es", "h\u00e4n\u00b7gen", "sich", "die", "klei\u00b7nen", "A\u00b7est'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Und Winckel in einander fest,", "tokens": ["Und", "Win\u00b7ckel", "in", "ein\u00b7an\u00b7der", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Daher kommt die Beschaffenheit", "tokens": ["Da\u00b7her", "kommt", "die", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.19": {"text": "Der Dichte, wodurch in der Erden", "tokens": ["Der", "Dich\u00b7te", ",", "wo\u00b7durch", "in", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "So von Metall, als Stein, die Dick\u2019und H\u00e4rtigkeit", "tokens": ["So", "von", "Me\u00b7tall", ",", "als", "Stein", ",", "die", "Dick'und", "H\u00e4r\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "KOUS", "NN", "$,", "ART", "NN", "NN"], "meter": "--++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Entstehen und verursacht werden.", "tokens": ["Ent\u00b7ste\u00b7hen", "und", "ver\u00b7ur\u00b7sacht", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}