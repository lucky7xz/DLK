{"dta.poem.3917": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 1.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seine Liebste k\u00fc\u00dft, und hat sich nicht barbiert,", "tokens": ["Wer", "sei\u00b7ne", "Liebs\u00b7te", "k\u00fc\u00dft", ",", "und", "hat", "sich", "nicht", "bar\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VAFIN", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinen Tobacks-Mund nicht reinlich aus-", "tokens": ["Und", "sei\u00b7nen", "To\u00b7backs\u00b7Mund", "nicht", "rein\u00b7lich", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "purgiert;", "tokens": ["pur\u00b7giert", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wen\u0303 er ihr auf dem Arm ein blaues Maal geknippen,", "tokens": ["We\u00f1", "er", "ihr", "auf", "dem", "Arm", "ein", "blau\u00b7es", "Maal", "ge\u00b7knip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und bisse sie noch wohl im K\u00fcssen in die Lippen,", "tokens": ["Und", "bis\u00b7se", "sie", "noch", "wohl", "im", "K\u00fcs\u00b7sen", "in", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja, unterl\u00e4st er gar vor ihr sein Compliment,", "tokens": ["Ja", ",", "un\u00b7ter\u00b7l\u00e4st", "er", "gar", "vor", "ihr", "sein", "Com\u00b7pli\u00b7ment", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So hilfft da nichts daf\u00fcr, es kostet ein Pr\u00e4sent.", "tokens": ["So", "hilfft", "da", "nichts", "da\u00b7f\u00fcr", ",", "es", "kos\u00b7tet", "ein", "Pr\u00e4\u00b7sent", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "PAV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}