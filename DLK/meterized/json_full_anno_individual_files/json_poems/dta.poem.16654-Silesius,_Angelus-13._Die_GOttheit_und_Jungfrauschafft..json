{"dta.poem.16654": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "13.  Die GOttheit und Jungfrauschafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die GOttheit ist so nah der Jungfrauschafft verwandt/", "tokens": ["Die", "Got\u00b7theit", "ist", "so", "nah", "der", "Jung\u00b7frausc\u00b7hafft", "ver\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie auch ohne die nicht GOttheit wird erkandt.", "tokens": ["Da\u00df", "sie", "auch", "oh\u00b7ne", "die", "nicht", "Got\u00b7theit", "wird", "er\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRELS", "PTKNEG", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}