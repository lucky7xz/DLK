{"dta.poem.21720": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Mondnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehe, wie die Mondenstrahlen", "tokens": ["Sie\u00b7he", ",", "wie", "die", "Mon\u00b7dens\u00b7trah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Busch und Flur in Silber mahlen!", "tokens": ["Busch", "und", "Flur", "in", "Sil\u00b7ber", "mah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das B\u00e4chlein rollt und flimmt!", "tokens": ["Wie", "das", "B\u00e4ch\u00b7lein", "rollt", "und", "flimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Strahlen regnen, Funken schmettern", "tokens": ["Strah\u00b7len", "reg\u00b7nen", ",", "Fun\u00b7ken", "schmet\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den sanftgeregten Bl\u00e4ttern,", "tokens": ["Von", "den", "sanft\u00b7ge\u00b7reg\u00b7ten", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Thauflur gl\u00e4nzt und glimmt.", "tokens": ["Und", "die", "Thauf\u00b7lur", "gl\u00e4nzt", "und", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Gl\u00e4nzend erd\u00e4mmern der Berge Gipfel,", "tokens": ["Gl\u00e4n\u00b7zend", "er\u00b7d\u00e4m\u00b7mern", "der", "Ber\u00b7ge", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Gl\u00e4nzend der Pappeln wogende Wipfel.", "tokens": ["Gl\u00e4n\u00b7zend", "der", "Pap\u00b7peln", "wo\u00b7gen\u00b7de", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Durch die glanzberauschten R\u00e4ume", "tokens": ["Durch", "die", "glanz\u00b7be\u00b7rauschten", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Fl\u00fcstern Stimmen, gaukeln Tr\u00e4ume,", "tokens": ["Fl\u00fcs\u00b7tern", "Stim\u00b7men", ",", "gau\u00b7keln", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprechen mir vertraulich zu.", "tokens": ["Spre\u00b7chen", "mir", "ver\u00b7trau\u00b7lich", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seligkeit, die mich gemahnet,", "tokens": ["Se\u00b7lig\u00b7keit", ",", "die", "mich", "ge\u00b7mah\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6chste Lust, die s\u00fcss mich schwanet,", "tokens": ["H\u00f6chs\u00b7te", "Lust", ",", "die", "s\u00fcss", "mich", "schwa\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprich, wo bl\u00fchst, wo zeitigst du?", "tokens": ["Sprich", ",", "wo", "bl\u00fchst", ",", "wo", "zei\u00b7tigst", "du", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sprenge die Brust nicht, m\u00e4chtiges Dehnen;", "tokens": ["Spren\u00b7ge", "die", "Brust", "nicht", ",", "m\u00e4ch\u00b7ti\u00b7ges", "Deh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "L\u00f6schet die Wehmuth, labende Thr\u00e4nen.", "tokens": ["L\u00f6\u00b7schet", "die", "Weh\u00b7muth", ",", "la\u00b7ben\u00b7de", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wie, ach wie der Qual genesen?", "tokens": ["Wie", ",", "ach", "wie", "der", "Qual", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo, ach wo ein liebend Wesen,", "tokens": ["Wo", ",", "ach", "wo", "ein", "lie\u00b7bend", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "XY", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das die s\u00fcssen Qualen stillt.", "tokens": ["Das", "die", "s\u00fcs\u00b7sen", "Qua\u00b7len", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eins ins andre gar versunken,", "tokens": ["Eins", "ins", "and\u00b7re", "gar", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gar verloren, gar ertrunken,", "tokens": ["Gar", "ver\u00b7lo\u00b7ren", ",", "gar", "er\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis sich jede \u00d6de f\u00fcllt \u2014", "tokens": ["Bis", "sich", "je\u00b7de", "\u00d6\u00b7de", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Solches, ach, w\u00e4hn' ich, k\u00fchlte das Sehnen;", "tokens": ["Sol\u00b7ches", ",", "ach", ",", "w\u00e4hn'", "ich", ",", "k\u00fchl\u00b7te", "das", "Seh\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ITJ", "$,", "VVIMP", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "L\u00f6schte die Wehmuth mit k\u00f6stlichen Thr\u00e4nen", "tokens": ["L\u00f6schte", "die", "Weh\u00b7muth", "mit", "k\u00f6st\u00b7li\u00b7chen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine weiss ich, ach nur Eine,", "tokens": ["Ei\u00b7ne", "weiss", "ich", ",", "ach", "nur", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "$,", "ITJ", "ADV", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich nur weiss ich, dich, o Reine,", "tokens": ["Dich", "nur", "weiss", "ich", ",", "dich", ",", "o", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die des Herzens Wehmuth meint.", "tokens": ["Die", "des", "Her\u00b7zens", "Weh\u00b7muth", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich umringend, von dir umrungen,", "tokens": ["Dich", "um\u00b7rin\u00b7gend", ",", "von", "dir", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dich umschlingend, von dir umschlungen,", "tokens": ["Dich", "um\u00b7schlin\u00b7gend", ",", "von", "dir", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "PPER", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gar in Eins mit dir geeint \u2014 \u2014", "tokens": ["Gar", "in", "Eins", "mit", "dir", "ge\u00b7eint"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "VVPP", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon', ach schone den Wonneversunknen.", "tokens": ["Schon'", ",", "ach", "scho\u00b7ne", "den", "Won\u00b7ne\u00b7ver\u00b7sun\u00b7knen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Himmel und Erde verschwinden dem Trunknen,", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7schwin\u00b7den", "dem", "Trunk\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}