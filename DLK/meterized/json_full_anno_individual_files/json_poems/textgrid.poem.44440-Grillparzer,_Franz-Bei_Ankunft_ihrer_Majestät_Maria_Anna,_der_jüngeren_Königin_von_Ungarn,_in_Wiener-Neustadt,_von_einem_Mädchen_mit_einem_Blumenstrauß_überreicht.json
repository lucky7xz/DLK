{"textgrid.poem.44440": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Bei Ankunft ihrer Majest\u00e4t Maria Anna, der j\u00fcngeren K\u00f6nigin von Ungarn, in Wiener-Neustadt, von einem M\u00e4dchen mit einem Blumenstrau\u00df \u00fcberreicht", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O F\u00fcrstin, du, dem sch\u00f6nen Land entnommen,", "tokens": ["O", "F\u00fcrs\u00b7tin", ",", "du", ",", "dem", "sch\u00f6\u00b7nen", "Land", "ent\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Myrt und Lorbeer steht, wo die Orangen bl\u00fchn,", "tokens": ["Wo", "Myrt", "und", "Lor\u00b7beer", "steht", ",", "wo", "die", "O\u00b7ran\u00b7gen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sei du auf deutschem Boden uns willkommen,", "tokens": ["Sei", "du", "auf", "deut\u00b7schem", "Bo\u00b7den", "uns", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Land der Eichen, die nicht minder gr\u00fcn.", "tokens": ["Im", "Land", "der", "Ei\u00b7chen", ",", "die", "nicht", "min\u00b7der", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und sahst du, kommend, schneebedeckte Fl\u00e4chen,", "tokens": ["Und", "sahst", "du", ",", "kom\u00b7mend", ",", "schnee\u00b7be\u00b7deck\u00b7te", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Baches Lauf gehemmt von starrem Eis;", "tokens": ["Des", "Ba\u00b7ches", "Lauf", "ge\u00b7hemmt", "von", "star\u00b7rem", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir konnten ", "tokens": ["Wir", "konn\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Was dort Natur, gibt Neigung hier und Flei\u00df.", "tokens": ["Was", "dort", "Na\u00b7tur", ",", "gibt", "Nei\u00b7gung", "hier", "und", "Flei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "$,", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und so auch! Harre nur noch kurze Stunden!", "tokens": ["Und", "so", "auch", "!", "Har\u00b7re", "nur", "noch", "kur\u00b7ze", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "NE", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir haben einen Lenz, und er ist sch\u00f6n;", "tokens": ["Wir", "ha\u00b7ben", "ei\u00b7nen", "Lenz", ",", "und", "er", "ist", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat erst die Flur des Fr\u00fchlings Hauch empfunden,", "tokens": ["Hat", "erst", "die", "Flur", "des", "Fr\u00fch\u00b7lings", "Hauch", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird prangend sie, gleich deiner Heimat stehn.", "tokens": ["Wird", "pran\u00b7gend", "sie", ",", "gleich", "dei\u00b7ner", "Hei\u00b7mat", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sei du die Sonne! la\u00df die Decke schwinden,", "tokens": ["Sei", "du", "die", "Son\u00b7ne", "!", "la\u00df", "die", "De\u00b7cke", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "VVIMP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die unsrer Zukunft Boden halb verh\u00fcllt,", "tokens": ["Die", "uns\u00b7rer", "Zu\u00b7kunft", "Bo\u00b7den", "halb", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in Hesperien sollst du froh dich finden,", "tokens": ["Und", "in", "Hes\u00b7pe\u00b7ri\u00b7en", "sollst", "du", "froh"