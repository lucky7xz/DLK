{"dta.poem.2100": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einleitung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der angestralten Welt, zeigt uns, jede sch\u00f6-", "tokens": ["Auf", "der", "an\u00b7ge\u00b7stral\u00b7ten", "Welt", ",", "zeigt", "uns", ",", "je\u00b7de", "sch\u00f6"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "PIAT", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "ne Stelle,", "tokens": ["ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Aller Sch\u00f6nheit Quell, die Sonne, und zu-", "tokens": ["Al\u00b7ler", "Sch\u00f6n\u00b7heit", "Quell", ",", "die", "Son\u00b7ne", ",", "und", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "ART", "NN", "$,", "KON", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "gleich derselben Quelle,", "tokens": ["gleich", "der\u00b7sel\u00b7ben", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aller Sonnen Sonne, Gott! Wie gesegnet, wie be-", "tokens": ["Al\u00b7ler", "Son\u00b7nen", "Son\u00b7ne", ",", "Gott", "!", "Wie", "ge\u00b7seg\u00b7net", ",", "wie", "be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "NN", "$.", "PWAV", "VVPP", "$,", "PWAV", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "gl\u00fcckt", "tokens": ["gl\u00fcckt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ist, der, da Gott allenthalben, allenthalben Gott erblickt!", "tokens": ["Ist", ",", "der", ",", "da", "Gott", "al\u00b7len\u00b7thal\u00b7ben", ",", "al\u00b7len\u00b7thal\u00b7ben", "Gott", "er\u00b7blickt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "$,", "KOUS", "NN", "ADV", "$,", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}