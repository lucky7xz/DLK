{"textgrid.poem.58060": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "98. Eins ist not", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen suchen viel und finden nimmer g'nug,", "tokens": ["Die", "Men\u00b7schen", "su\u00b7chen", "viel", "und", "fin\u00b7den", "nim\u00b7mer", "g'\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ich aber bin vergn\u00fcgt, weil ich nur eines such';", "tokens": ["Ich", "a\u00b7ber", "bin", "ver\u00b7gn\u00fcgt", ",", "weil", "ich", "nur", "ei\u00b7nes", "such'", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie haben viel zu tun, ich nur ein einzig Werk,", "tokens": ["Sie", "ha\u00b7ben", "viel", "zu", "tun", ",", "ich", "nur", "ein", "ein\u00b7zig", "Werk", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$,", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich beim Stillesein auf Jesu Reden merk'.", "tokens": ["Da\u00df", "ich", "beim", "Stil\u00b7le\u00b7sein", "auf", "Je\u00b7su", "Re\u00b7den", "merk'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+