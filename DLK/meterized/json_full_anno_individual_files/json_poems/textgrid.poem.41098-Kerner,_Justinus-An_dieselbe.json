{"textgrid.poem.41098": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich Dir in Versen schreibe,", "tokens": ["Da\u00df", "ich", "Dir", "in", "Ver\u00b7sen", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist, um Dir zu zeigen nur,", "tokens": ["Ist", ",", "um", "Dir", "zu", "zei\u00b7gen", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich bis zum Tode bleibe", "tokens": ["Wie", "ich", "bis", "zum", "To\u00b7de", "blei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein getreuer Troubadour.", "tokens": ["Dein", "ge\u00b7treu\u00b7er", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ich sing', sind schwache Worte,", "tokens": ["Was", "ich", "sing'", ",", "sind", "schwa\u00b7che", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh! mein Herz ist krank und wund,", "tokens": ["Sieh", "!", "mein", "Herz", "ist", "krank", "und", "wund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es holt sich die Akkorde", "tokens": ["Und", "es", "holt", "sich", "die", "Ak\u00b7kor\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur aus dem zerri\u00dfnen Grund.", "tokens": ["Nur", "aus", "dem", "zer\u00b7ri\u00df\u00b7nen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einmal doch wird's anders werden,", "tokens": ["Ein\u00b7mal", "doch", "wird's", "an\u00b7ders", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Geist vom Leibe schied.", "tokens": ["Wenn", "der", "Geist", "vom", "Lei\u00b7be", "schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abgestreift den Staub der Erden", "tokens": ["Ab\u00b7ge\u00b7streift", "den", "Staub", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sing' ich Geist Dir geist'ges Lied.", "tokens": ["Sing'", "ich", "Geist", "Dir", "geist'\u00b7ges", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schaust Du dann nach sel'gen Sternen,", "tokens": ["Schaust", "Du", "dann", "nach", "sel'\u00b7gen", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf nach Deines Schwaigerns Flur,", "tokens": ["Auf", "nach", "Dei\u00b7nes", "Schwai\u00b7gerns", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6nt es Dir aus blauen Fernen", "tokens": ["T\u00f6nt", "es", "Dir", "aus", "blau\u00b7en", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Gr\u00fc\u00dfen der Natur.", "tokens": ["Wie", "ein", "Gr\u00fc\u00b7\u00dfen", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ruhst Du am Platanenbaume", "tokens": ["Ruhst", "Du", "am", "Pla\u00b7ta\u00b7nen\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Nachtviole Duft,", "tokens": ["In", "der", "Nacht\u00b7vi\u00b7o\u00b7le", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieget Dich zum s\u00fc\u00dfsten Traume", "tokens": ["Wie\u00b7get", "Dich", "zum", "s\u00fc\u00df\u00b7sten", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geisterlaut aus stiller Luft.", "tokens": ["Geis\u00b7ter\u00b7laut", "aus", "stil\u00b7ler", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und im Mondlicht, wenn durch Ranken", "tokens": ["Und", "im", "Mond\u00b7licht", ",", "wenn", "durch", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Balkons ein Westhauch zieht,", "tokens": ["Des", "Bal\u00b7kons", "ein", "West\u00b7hauch", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wirst Du fragen in Gedanken:", "tokens": ["Wirst", "Du", "fra\u00b7gen", "in", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt nicht meines Kerners Lied?", "tokens": ["T\u00f6nt", "nicht", "mei\u00b7nes", "Ker\u00b7ners", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tauben mit melod'schen Schwingen,", "tokens": ["Tau\u00b7ben", "mit", "me\u00b7lo\u00b7d'\u00b7schen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bienen, summend durch die Flur,", "tokens": ["Bie\u00b7nen", ",", "sum\u00b7mend", "durch", "die", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werden Dir mein Lied dann bringen,", "tokens": ["Wer\u00b7den", "Dir", "mein", "Lied", "dann", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nahm mich zu sich die Natur.", "tokens": ["Nahm", "mich", "zu", "sich", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Doch, indem ich Dir will klagen,", "tokens": ["Doch", ",", "in\u00b7dem", "ich", "Dir", "will", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich noch auf Erden bin,", "tokens": ["Da\u00df", "ich", "noch", "auf", "Er\u00b7den", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mein Singen, wo mein Sagen", "tokens": ["Wo", "mein", "Sin\u00b7gen", ",", "wo", "mein", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleicht, ein tr\u00fcber Bach, dahin,", "tokens": ["Schleicht", ",", "ein", "tr\u00fc\u00b7ber", "Bach", ",", "da\u00b7hin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Rufen Geister: La\u00df dein Sorgen,", "tokens": ["Ru\u00b7fen", "Geis\u00b7ter", ":", "La\u00df", "dein", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liederquelle! springe klar!", "tokens": ["Lie\u00b7der\u00b7quel\u00b7le", "!", "sprin\u00b7ge", "klar", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut ja ist der frohe Morgen,", "tokens": ["Heut", "ja", "ist", "der", "fro\u00b7he", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die Lieblichste gebar.", "tokens": ["Der", "die", "Lieb\u00b7lichs\u00b7te", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gerne nun m\u00f6cht' ich Dir schreiben,", "tokens": ["Ger\u00b7ne", "nun", "m\u00f6cht'", "ich", "Dir", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wie mein Gr\u00e4men T\u00e4uschung nur:", "tokens": ["Wie", "mein", "Gr\u00e4\u00b7men", "T\u00e4u\u00b7schung", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn auf einmal m\u00f6cht' ich bleiben", "tokens": ["Denn", "auf", "ein\u00b7mal", "m\u00f6cht'", "ich", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebend, lang Dein Troubadour.", "tokens": ["Le\u00b7bend", ",", "lang", "Dein", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Da\u00df ich Dir in Versen schreibe,", "tokens": ["Da\u00df", "ich", "Dir", "in", "Ver\u00b7sen", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist, um Dir zu zeigen nur,", "tokens": ["Ist", ",", "um", "Dir", "zu", "zei\u00b7gen", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich bis zum Tode bleibe", "tokens": ["Wie", "ich", "bis", "zum", "To\u00b7de", "blei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein getreuer Troubadour.", "tokens": ["Dein", "ge\u00b7treu\u00b7er", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Was ich sing',