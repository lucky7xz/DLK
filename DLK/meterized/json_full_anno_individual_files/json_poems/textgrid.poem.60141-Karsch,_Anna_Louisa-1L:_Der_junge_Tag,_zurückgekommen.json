{"textgrid.poem.60141": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der junge Tag, zur\u00fcckgekommen", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge Tag, zur\u00fcckgekommen", "tokens": ["Der", "jun\u00b7ge", "Tag", ",", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit neugeschaffnem Angesicht,", "tokens": ["Mit", "neu\u00b7ge\u00b7schaff\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat halb die Freundlichkeit des Gottes angenommen,", "tokens": ["Hat", "halb", "die", "Freund\u00b7lich\u00b7keit", "des", "Got\u00b7tes", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ihn bekleidet mit Licht!", "tokens": ["Der", "ihn", "be\u00b7klei\u00b7det", "mit", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Du, Seele! bist nicht fortgerissen", "tokens": ["Du", ",", "See\u00b7le", "!", "bist", "nicht", "fort\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$.", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus mir, durch irgend eine Macht;", "tokens": ["Aus", "mir", ",", "durch", "ir\u00b7gend", "ei\u00b7ne", "Macht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O dem, auf dessen Wort die Himmel horchen m\u00fcssen,", "tokens": ["O", "dem", ",", "auf", "des\u00b7sen", "Wort", "die", "Him\u00b7mel", "hor\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Sey neues Opfer gebracht!", "tokens": ["Sey", "neu\u00b7es", "Op\u00b7fer", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Er durfte sprechen, durfte winken,", "tokens": ["Er", "durf\u00b7te", "spre\u00b7chen", ",", "durf\u00b7te", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schlug der Todes-Engel mich,", "tokens": ["So", "schlug", "der", "To\u00b7des\u00b7En\u00b7gel", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00dft ich pl\u00f6zlich hin in ewgen Schlaf versinken", "tokens": ["So", "mu\u00dft", "ich", "pl\u00f6z\u00b7lich", "hin", "in", "ew\u00b7gen", "Schlaf", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Luft bekleidete dich!", "tokens": ["Und", "Luft", "be\u00b7klei\u00b7de\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Er hie\u00df mich leben, hie\u00df dich bleiben,", "tokens": ["Er", "hie\u00df", "mich", "le\u00b7ben", ",", "hie\u00df", "dich", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, die vom Himmel niederfuhr;", "tokens": ["Dich", ",", "die", "vom", "Him\u00b7mel", "nie\u00b7der\u00b7fuhr", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey Funken oder Hauch, ich kann dich nicht beschreiben,", "tokens": ["Sey", "Fun\u00b7ken", "o\u00b7der", "Hauch", ",", "ich", "kann", "dich", "nicht", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Empfinden kann ich dich nur!", "tokens": ["Emp\u00b7fin\u00b7den", "kann", "ich", "dich", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Du denkst in mir, du kannst dich schwingen", "tokens": ["Du", "denkst", "in", "mir", ",", "du", "kannst", "dich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem unsichtbaren Winde gleich,", "tokens": ["Dem", "un\u00b7sicht\u00b7ba\u00b7ren", "Win\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem Augenblick dahin, wo Engel singen,", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "da\u00b7hin", ",", "wo", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und singst mit ihnen zugleich!", "tokens": ["Und", "singst", "mit", "ih\u00b7nen", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Du \u00fcbersteigest Mond und Sterne", "tokens": ["Du", "\u00fc\u00b7bers\u00b7tei\u00b7gest", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fliehst schnell zur\u00fcck, du schweifst umher", "tokens": ["Fliehst", "schnell", "zu\u00b7r\u00fcck", ",", "du", "schweifst", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Gottes Blitz, und schwebst in ungeme\u00dfner Ferne", "tokens": ["Wie", "Got\u00b7tes", "Blitz", ",", "und", "schwebst", "in", "un\u00b7ge\u00b7me\u00df\u00b7ner", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hoch \u00fcber H\u00fcgel und Meer!", "tokens": ["Hoch", "\u00fc\u00b7ber", "H\u00fc\u00b7gel", "und", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Du drengest dich durch dicke Mauren,", "tokens": ["Du", "dren\u00b7gest", "dich", "durch", "di\u00b7cke", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du achtest feste Schl\u00f6sser nichts;", "tokens": ["Du", "ach\u00b7test", "fes\u00b7te", "Schl\u00f6s\u00b7ser", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl es, da\u00df du strebst der Gottheit gleich zu dauren,", "tokens": ["Ich", "f\u00fchl", "es", ",", "da\u00df", "du", "strebst", "der", "Got\u00b7theit", "gleich", "zu", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu trinken Str\u00f6me des Lichts.", "tokens": ["Zu", "trin\u00b7ken", "Str\u00f6\u00b7me", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Dein nahmenloser Geiz begehret", "tokens": ["Dein", "nah\u00b7men\u00b7lo\u00b7ser", "Geiz", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mehr, als die Welt zu geben wei\u00df;", "tokens": ["Mehr", ",", "als", "die", "Welt", "zu", "ge\u00b7ben", "wei\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Wollust oder Gold und Ehre nicht gen\u00e4hret,", "tokens": ["Von", "Wol\u00b7lust", "o\u00b7der", "Gold", "und", "Eh\u00b7re", "nicht", "ge\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleibt stets dein Hunger noch hei\u00df,", "tokens": ["Bleibt", "stets", "dein", "Hun\u00b7ger", "noch", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Bis du zum Seraph wirst erhoben.", "tokens": ["Bis", "du", "zum", "Se\u00b7raph", "wirst", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O f\u00fchle deine W\u00fcrde ganz,", "tokens": ["O", "f\u00fch\u00b7le", "dei\u00b7ne", "W\u00fcr\u00b7de", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unsterbliche! dir gab der, den die Sterne loben", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", "!", "dir", "gab", "der", ",", "den", "die", "Ster\u00b7ne", "lo\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Ein Theil vom himmlischen Glanz.", "tokens": ["Ein", "Theil", "vom", "himm\u00b7li\u00b7schen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Der junge Tag, zur\u00fcckgekommen", "tokens": ["Der", "jun\u00b7ge", "Tag", ",", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit neugeschaffnem Angesicht,", "tokens": ["Mit", "neu\u00b7ge\u00b7schaff\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat halb die Freundlichkeit des Gottes angenommen,", "tokens": ["Hat", "halb", "die", "Freund\u00b7lich\u00b7keit", "des", "Got\u00b7tes", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ihn bekleidet mit Licht!", "tokens": ["Der", "ihn", "be\u00b7klei\u00b7det", "mit", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Du, Seele! bist nicht fortgerissen", "tokens": ["Du", ",", "See\u00b7le", "!", "bist", "nicht", "fort\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$.", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus mir, durch irgend eine Macht;", "tokens": ["Aus", "mir", ",", "durch", "ir\u00b7gend", "ei\u00b7ne", "Macht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O dem, auf dessen Wort die Himmel horchen m\u00fcssen,", "tokens": ["O", "dem", ",", "auf", "des\u00b7sen", "Wort", "die", "Him\u00b7mel", "hor\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Sey neues Opfer gebracht!", "tokens": ["Sey", "neu\u00b7es", "Op\u00b7fer", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Er durfte sprechen, durfte winken,", "tokens": ["Er", "durf\u00b7te", "spre\u00b7chen", ",", "durf\u00b7te", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schlug der Todes-Engel mich,", "tokens": ["So", "schlug", "der", "To\u00b7des\u00b7En\u00b7gel", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00dft ich pl\u00f6zlich hin in ewgen Schlaf versinken", "tokens": ["So", "mu\u00dft", "ich", "pl\u00f6z\u00b7lich", "hin", "in", "ew\u00b7gen", "Schlaf", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Luft bekleidete dich!", "tokens": ["Und", "Luft", "be\u00b7klei\u00b7de\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Er hie\u00df mich leben, hie\u00df dich bleiben,", "tokens": ["Er", "hie\u00df", "mich", "le\u00b7ben", ",", "hie\u00df", "dich", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$,", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, die vom Himmel niederfuhr;", "tokens": ["Dich", ",", "die", "vom", "Him\u00b7mel", "nie\u00b7der\u00b7fuhr", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey Funken oder Hauch, ich kann dich nicht beschreiben,", "tokens": ["Sey", "Fun\u00b7ken", "o\u00b7der", "Hauch", ",", "ich", "kann", "dich", "nicht", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Empfinden kann ich dich nur!", "tokens": ["Emp\u00b7fin\u00b7den", "kann", "ich", "dich", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Du denkst in mir, du kannst dich schwingen", "tokens": ["Du", "denkst", "in", "mir", ",", "du", "kannst", "dich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem unsichtbaren Winde gleich,", "tokens": ["Dem", "un\u00b7sicht\u00b7ba\u00b7ren", "Win\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem Augenblick dahin, wo Engel singen,", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", "da\u00b7hin", ",", "wo", "En\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und singst mit ihnen zugleich!", "tokens": ["Und", "singst", "mit", "ih\u00b7nen", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Du \u00fcbersteigest Mond und Sterne", "tokens": ["Du", "\u00fc\u00b7bers\u00b7tei\u00b7gest", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fliehst schnell zur\u00fcck, du schweifst umher", "tokens": ["Fliehst", "schnell", "zu\u00b7r\u00fcck", ",", "du", "schweifst", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Gottes Blitz, und schwebst in ungeme\u00dfner Ferne", "tokens": ["Wie", "Got\u00b7tes", "Blitz", ",", "und", "schwebst", "in", "un\u00b7ge\u00b7me\u00df\u00b7ner", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hoch \u00fcber H\u00fcgel und Meer!", "tokens": ["Hoch", "\u00fc\u00b7ber", "H\u00fc\u00b7gel", "und", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Du drengest dich durch dicke Mauren,", "tokens": ["Du", "dren\u00b7gest", "dich", "durch", "di\u00b7cke", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du achtest feste Schl\u00f6sser nichts;", "tokens": ["Du", "ach\u00b7test", "fes\u00b7te", "Schl\u00f6s\u00b7ser", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl es, da\u00df du strebst der Gottheit gleich zu dauren,", "tokens": ["Ich", "f\u00fchl", "es", ",", "da\u00df", "du", "strebst", "der", "Got\u00b7theit", "gleich", "zu", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu trinken Str\u00f6me des Lichts.", "tokens": ["Zu", "trin\u00b7ken", "Str\u00f6\u00b7me", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.17": {"line.1": {"text": "Dein nahmenloser Geiz begehret", "tokens": ["Dein", "nah\u00b7men\u00b7lo\u00b7ser", "Geiz", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mehr, als die Welt zu geben wei\u00df;", "tokens": ["Mehr", ",", "als", "die", "Welt", "zu", "ge\u00b7ben", "wei\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Wollust oder Gold und Ehre nicht gen\u00e4hret,", "tokens": ["Von", "Wol\u00b7lust", "o\u00b7der", "Gold", "und", "Eh\u00b7re", "nicht", "ge\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleibt stets dein Hunger noch hei\u00df,", "tokens": ["Bleibt", "stets", "dein", "Hun\u00b7ger", "noch", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.18": {"line.1": {"text": "Bis du zum Seraph wirst erhoben.", "tokens": ["Bis", "du", "zum", "Se\u00b7raph", "wirst", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O f\u00fchle deine W\u00fcrde ganz,", "tokens": ["O", "f\u00fch\u00b7le", "dei\u00b7ne", "W\u00fcr\u00b7de", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unsterbliche! dir gab der, den die Sterne loben", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", "!", "dir", "gab", "der", ",", "den", "die", "Ster\u00b7ne", "lo\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Ein Theil vom himmlischen Glanz.", "tokens": ["Ein", "Theil", "vom", "himm\u00b7li\u00b7schen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}