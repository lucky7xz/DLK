{"dta.poem.8920": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du mich liebst; es kann dich nicht erniedern:", "tokens": ["Wenn", "du", "mich", "liebst", ";", "es", "kann", "dich", "nicht", "er\u00b7nie\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlieren w\u00fcrden in der Gunst der Biedern,", "tokens": ["Ver\u00b7lie\u00b7ren", "w\u00fcr\u00b7den", "in", "der", "Gunst", "der", "Bie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die meine Gunst mir vor die F\u00fc\u00dfe w\u00fcrfen.", "tokens": ["Die", "mei\u00b7ne", "Gunst", "mir", "vor", "die", "F\u00fc\u00b7\u00dfe", "w\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich w\u00fcrde viele Freunde z\u00e4hlen d\u00fcrfen,", "tokens": ["Ich", "w\u00fcr\u00b7de", "vie\u00b7le", "Freun\u00b7de", "z\u00e4h\u00b7len", "d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich die Freundschaft Aller k\u00f6nnt' erwiedern,", "tokens": ["Wenn", "ich", "die", "Freund\u00b7schaft", "Al\u00b7ler", "k\u00f6nnt'", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch der Entfernten, welche blos aus Liedern", "tokens": ["Auch", "der", "Ent\u00b7fern\u00b7ten", ",", "wel\u00b7che", "blos", "aus", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die ganze Flamme meiner Seele schl\u00fcrfen.", "tokens": ["Die", "gan\u00b7ze", "Flam\u00b7me", "mei\u00b7ner", "See\u00b7le", "schl\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein warmes Herz, und wenn auch du mit herben,", "tokens": ["Ein", "war\u00b7mes", "Herz", ",", "und", "wenn", "auch", "du", "mit", "her\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "KOUS", "ADV", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00e4ssigen Geschossen nach ihm zielest,", "tokens": ["Ge\u00b7h\u00e4s\u00b7si\u00b7gen", "Ge\u00b7schos\u00b7sen", "nach", "ihm", "zie\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df doch sich manchen warmen Freund erwerben!", "tokens": ["Mu\u00df", "doch", "sich", "man\u00b7chen", "war\u00b7men", "Freund", "er\u00b7wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du aber, der du jezt den Harten spielest,", "tokens": ["Du", "a\u00b7ber", ",", "der", "du", "jezt", "den", "Har\u00b7ten", "spie\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df einst mich nur an deinem Busen sterben,", "tokens": ["La\u00df", "einst", "mich", "nur", "an", "dei\u00b7nem", "Bu\u00b7sen", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schlie\u00df' ein Auge, dem du wohlgefielest!", "tokens": ["Und", "schlie\u00df'", "ein", "Au\u00b7ge", ",", "dem", "du", "wohl\u00b7ge\u00b7fie\u00b7lest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}