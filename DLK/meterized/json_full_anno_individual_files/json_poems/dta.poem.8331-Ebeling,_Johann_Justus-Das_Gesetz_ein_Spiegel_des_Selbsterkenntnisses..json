{"dta.poem.8331": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das Gesetz ein Spiegel des  \n Selbsterkenntnisses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch! wilt du dich recht kennen lernen,", "tokens": ["Mensch", "!", "wilt", "du", "dich", "recht", "ken\u00b7nen", "ler\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So mu\u00df das Blendglas sich entfernen,", "tokens": ["So", "mu\u00df", "das", "Blend\u00b7glas", "sich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df deine Eigenliebe ehrt,", "tokens": ["Da\u00df", "dei\u00b7ne", "Ei\u00b7gen\u00b7lie\u00b7be", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin sie stets die Augen kehrt.", "tokens": ["Da\u00b7hin", "sie", "stets", "die", "Au\u00b7gen", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du suchest dich gern zu vergr\u00f6ssern,", "tokens": ["Du", "su\u00b7chest", "dich", "gern", "zu", "ver\u00b7gr\u00f6s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein du wilt dich doch nicht bessern,", "tokens": ["Al\u00b7lein", "du", "wilt", "dich", "doch", "nicht", "bes\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer sich bem\u00fcht recht sch\u00f6n zu seyn,", "tokens": ["Wer", "sich", "be\u00b7m\u00fcht", "recht", "sch\u00f6n", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der mu\u00df nicht seine Flekken scheun.", "tokens": ["Der", "mu\u00df", "nicht", "sei\u00b7ne", "Flek\u00b7ken", "scheun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer seine eigne Fehler fliehet,", "tokens": ["Wer", "sei\u00b7ne", "eig\u00b7ne", "Feh\u00b7ler", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dieselbe blindlings \u00fcbersiehet,", "tokens": ["Die\u00b7sel\u00b7be", "blind\u00b7lings", "\u00fc\u00b7ber\u00b7sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Beh\u00e4lt ein flekkigt Angesicht,", "tokens": ["Be\u00b7h\u00e4lt", "ein", "flek\u00b7kigt", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und s\u00e4ubert sich vom Schmuzze nicht:", "tokens": ["Und", "s\u00e4u\u00b7bert", "sich", "vom", "Schmuz\u00b7ze", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wer aber solche recht erweget,", "tokens": ["Wer", "a\u00b7ber", "sol\u00b7che", "recht", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Bemerkt was er vor M\u00e4hler tr\u00e4get:", "tokens": ["Be\u00b7merkt", "was", "er", "vor", "M\u00e4h\u00b7ler", "tr\u00e4\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der bringt sich durch die Reinigung,", "tokens": ["Der", "bringt", "sich", "durch", "die", "Rei\u00b7ni\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.16": {"text": "Zu einer sch\u00f6nen Aenderung.", "tokens": ["Zu", "ei\u00b7ner", "sch\u00f6\u00b7nen", "A\u00b7en\u00b7de\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Wer sich zum Spiegel der da blendet,", "tokens": ["Wer", "sich", "zum", "Spie\u00b7gel", "der", "da", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mit seinem Angesichte wendet,", "tokens": ["Mit", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der bildet sich im Gegenschein,", "tokens": ["Der", "bil\u00b7det", "sich", "im", "Ge\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn er gleich he\u00dflich, sch\u00f6ne ein.", "tokens": ["Wenn", "er", "gleich", "he\u00df\u00b7lich", ",", "sch\u00f6\u00b7ne", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Hingegen wer den Spiegel w\u00e4hlet,", "tokens": ["Hin\u00b7ge\u00b7gen", "wer", "den", "Spie\u00b7gel", "w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Der nicht in seiner Bildung fehlet,", "tokens": ["Der", "nicht", "in", "sei\u00b7ner", "Bil\u00b7dung", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der siehet sich in der Gest\u00e4lt,", "tokens": ["Der", "sie\u00b7het", "sich", "in", "der", "Ge\u00b7st\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Wie sie hinein, zur\u00fckke prallt.", "tokens": ["Wie", "sie", "hin\u00b7ein", ",", "zu\u00b7r\u00fck\u00b7ke", "prallt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "$,", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ein Spiegel der uns nicht flattiret,", "tokens": ["Ein", "Spie\u00b7gel", "der", "uns", "nicht", "flat\u00b7ti\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.26": {"text": "Was heslich ist, nicht herrlich zieret,", "tokens": ["Was", "hes\u00b7lich", "ist", ",", "nicht", "herr\u00b7lich", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ist das Gesez, wer darauf blikt,", "tokens": ["Ist", "das", "Ge\u00b7sez", ",", "wer", "da\u00b7rauf", "blikt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWS", "PAV", "VVFIN", "$,"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Sieht sein Gem\u00fcthe abgedr\u00fckt.", "tokens": ["Sieht", "sein", "Ge\u00b7m\u00fc\u00b7the", "ab\u00b7ge\u00b7dr\u00fckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Drum kan es woll ein Spiegel heissen,", "tokens": ["Drum", "kan", "es", "woll", "ein", "Spie\u00b7gel", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und wer sich dabei wird befleissen,", "tokens": ["Und", "wer", "sich", "da\u00b7bei", "wird", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PAV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Mit Achtsamkeit hinein zu sehn,", "tokens": ["Mit", "Acht\u00b7sam\u00b7keit", "hin\u00b7ein", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Der sieht sein Bild darinnen stehn.", "tokens": ["Der", "sieht", "sein", "Bild", "da\u00b7rin\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Da k\u00f6nnen wir die schwarzen Flekken", "tokens": ["Da", "k\u00f6n\u00b7nen", "wir", "die", "schwar\u00b7zen", "Flek\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Der S\u00fcnde, kl\u00e4rlich gnug entdekken.", "tokens": ["Der", "S\u00fcn\u00b7de", ",", "kl\u00e4r\u00b7lich", "gnug", "ent\u00b7dek\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Jhr Menschen! spiegelt euren Sin,", "tokens": ["Ihr", "Men\u00b7schen", "!", "spie\u00b7gelt", "eu\u00b7ren", "Sin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und die Gestalt der Seelen drin;", "tokens": ["Und", "die", "Ge\u00b7stalt", "der", "See\u00b7len", "drin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "So werdet ihr ger\u00fchrt erkennen,", "tokens": ["So", "wer\u00b7det", "ihr", "ge\u00b7r\u00fchrt", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df ihr sehr elend seid zu nennen.", "tokens": ["Da\u00df", "ihr", "sehr", "e\u00b7lend", "seid", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Beschaut ein jegliches Gebot,", "tokens": ["Be\u00b7schaut", "ein", "jeg\u00b7li\u00b7ches", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Das macht euch warlich blas und roth,", "tokens": ["Das", "macht", "euch", "war\u00b7lich", "blas", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Weil es euch solche Runzeln zeiget,", "tokens": ["Weil", "es", "euch", "sol\u00b7che", "Run\u00b7zeln", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Davon ein andrer Spiegel schweiget.", "tokens": ["Da\u00b7von", "ein", "an\u00b7drer", "Spie\u00b7gel", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Wie k\u00f6nnt ihr ohne innres Graun,", "tokens": ["Wie", "k\u00f6nnt", "ihr", "oh\u00b7ne", "inn\u00b7res", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "In den Gesezzes Spiegel schaun?", "tokens": ["In", "den", "Ge\u00b7sez\u00b7zes", "Spie\u00b7gel", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "O! nein! ihr lernet nun gestehen,", "tokens": ["O", "!", "nein", "!", "ihr", "ler\u00b7net", "nun", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Jhr h\u00e4ttet nie den Greul gesehen,", "tokens": ["Ihr", "h\u00e4t\u00b7tet", "nie", "den", "Greul", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Als nunmehr, da ihr euch erkennt,", "tokens": ["Als", "nun\u00b7mehr", ",", "da", "ihr", "euch", "er\u00b7kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Eur Tichten, Trachten b\u00f6se nennt.", "tokens": ["Eur", "Tich\u00b7ten", ",", "Trach\u00b7ten", "b\u00f6\u00b7se", "nennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Was seid ihr nun, betrogne Seelen!", "tokens": ["Was", "seid", "ihr", "nun", ",", "be\u00b7trog\u00b7ne", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "K\u00f6nnt ihr der Flekken Menge z\u00e4hlen,", "tokens": ["K\u00f6nnt", "ihr", "der", "Flek\u00b7ken", "Men\u00b7ge", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Die bei des Spiegels Gegenschein,", "tokens": ["Die", "bei", "des", "Spie\u00b7gels", "Ge\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "In dem Gewissen sichtbahr seyn?", "tokens": ["In", "dem", "Ge\u00b7wis\u00b7sen", "sicht\u00b7bahr", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Jhr habt gemeint bei andrer Tadel,", "tokens": ["Ihr", "habt", "ge\u00b7meint", "bei", "an\u00b7drer", "Ta\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Jhr w\u00e4rt mit einen wahren Adel,", "tokens": ["Ihr", "w\u00e4rt", "mit", "ei\u00b7nen", "wah\u00b7ren", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "An eurer Seele ausgeziert,", "tokens": ["An", "eu\u00b7rer", "See\u00b7le", "aus\u00b7ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Doch diese Einbildung verliert", "tokens": ["Doch", "die\u00b7se", "Ein\u00b7bil\u00b7dung", "ver\u00b7liert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.57": {"text": "Das Herz, das sich nur recht betrachet,", "tokens": ["Das", "Herz", ",", "das", "sich", "nur", "recht", "be\u00b7tra\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Nicht besser, als die andern achtet.", "tokens": ["Nicht", "bes\u00b7ser", ",", "als", "die", "an\u00b7dern", "ach\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.59": {"text": "Wer diesen Spiegel flei\u00dfig n\u00fczt,", "tokens": ["Wer", "die\u00b7sen", "Spie\u00b7gel", "flei\u00b7\u00dfig", "n\u00fczt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Woraus das wahre Bildnis blizt,", "tokens": ["Wo\u00b7raus", "das", "wah\u00b7re", "Bild\u00b7nis", "blizt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Von unsrer Seelen rechten Wesen,", "tokens": ["Von", "uns\u00b7rer", "See\u00b7len", "rech\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Der wird, wie er gestaltet, lesen.", "tokens": ["Der", "wird", ",", "wie", "er", "ge\u00b7stal\u00b7tet", ",", "le\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.63": {"text": "Drum Menschen! die ihr, wie ihr solt,", "tokens": ["Drum", "Men\u00b7schen", "!", "die", "ihr", ",", "wie", "ihr", "solt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$.", "PRELS", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Euch recht im Herzen kennen wolt,", "tokens": ["Euch", "recht", "im", "Her\u00b7zen", "ken\u00b7nen", "wolt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Beschauet darin eure Seelen,", "tokens": ["Be\u00b7schau\u00b7et", "da\u00b7rin", "eu\u00b7re", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Der wird euch nichts vom Fehl verhelen:", "tokens": ["Der", "wird", "euch", "nichts", "vom", "Fehl", "ver\u00b7he\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PIS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.67": {"text": "Wischt durch des Heilands heiligs Blut,", "tokens": ["Wischt", "durch", "des", "Hei\u00b7lands", "hei\u00b7ligs", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Die M\u00e4hler ab; so seid ihr gut.", "tokens": ["Die", "M\u00e4h\u00b7ler", "ab", ";", "so", "seid", "ihr", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}