{"dta.poem.21066": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Jubelgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oselig, wer der m&#252;tterlichen Erde,               ", "tokens": ["O\u00b7se\u00b7lig", ",", "wer", "der", "m", "&#252;", "tter\u00b7li\u00b7chen", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ART", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Die ihn gebar und freundlich hegt und pflegt,", "tokens": ["Die", "ihn", "ge\u00b7bar", "und", "freund\u00b7lich", "hegt", "und", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die s\u00fcsse Schuld bezahlt, und Arbeit und Beschwerde", "tokens": ["Die", "s\u00fcs\u00b7se", "Schuld", "be\u00b7zahlt", ",", "und", "Ar\u00b7beit", "und", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In ihrem Dienste rastlos tr\u00e4gt;", "tokens": ["In", "ih\u00b7rem", "Diens\u00b7te", "rast\u00b7los", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer seinen Br\u00fcdern Recht, das Recht ist, spendet,", "tokens": ["Wer", "sei\u00b7nen", "Br\u00fc\u00b7dern", "Recht", ",", "das", "Recht", "ist", ",", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Unschuld schirmt, das kecke Laster schreckt,", "tokens": ["Die", "Un\u00b7schuld", "schirmt", ",", "das", "ke\u00b7cke", "Las\u00b7ter", "schreckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des J\u00fcnglings Herz gewinnt, der Thorheit ihn ent-", "tokens": ["Des", "J\u00fcng\u00b7lings", "Herz", "ge\u00b7winnt", ",", "der", "Thor\u00b7heit", "ihn", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wendet,", "tokens": ["wen\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und f\u00fcr die Tugend fr\u00fch ihn weckt;", "tokens": ["Und", "f\u00fcr", "die", "Tu\u00b7gend", "fr\u00fch", "ihn", "weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer aller Narrheit fr\u00fchvers\u00f6hnter Hasser,", "tokens": ["Wer", "al\u00b7ler", "Nar\u00b7rheit", "fr\u00fch\u00b7ver\u00b7s\u00f6hn\u00b7ter", "Has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich ihrem Andrang stark entgegenstemmt,", "tokens": ["Sich", "ih\u00b7rem", "An\u00b7drang", "stark", "ent\u00b7ge\u00b7gens\u00b7temmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sittenf\u00e4ulniss wehrt, der Frechheit wilde Wasser", "tokens": ["Der", "Sit\u00b7ten\u00b7f\u00e4ul\u00b7niss", "wehrt", ",", "der", "Frech\u00b7heit", "wil\u00b7de", "Was\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In engre Ufer sorgsam d\u00e4mmt;", "tokens": ["In", "en\u00b7gre", "U\u00b7fer", "sorg\u00b7sam", "d\u00e4mmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer so durch manche m\u00fchevolle Jahre", "tokens": ["Wer", "so", "durch", "man\u00b7che", "m\u00fc\u00b7he\u00b7vol\u00b7le", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Wohlthuns Lust in vollen Z\u00fcgen trinkt,", "tokens": ["Des", "Wohl\u00b7thuns", "Lust", "in", "vol\u00b7len", "Z\u00fc\u00b7gen", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und endlich thatensatt und in schneeweissem Haare", "tokens": ["Und", "end\u00b7lich", "tha\u00b7ten\u00b7satt", "und", "in", "schnee\u00b7weis\u00b7sem", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zufrieden in die Grube sinkt \u2014", "tokens": ["Zu\u00b7frie\u00b7den", "in", "die", "Gru\u00b7be", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So wandelt' einst auf ruhmumstrahlten Pfaden", "tokens": ["So", "wan\u00b7delt'", "einst", "auf", "ruh\u00b7mums\u00b7trahl\u00b7ten", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aristides die hohe Bahn hinab;               ", "tokens": ["A\u00b7ris\u00b7ti\u00b7des", "die", "ho\u00b7he", "Bahn", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sank er allbeweint, an deinen Ulmgestaden,", "tokens": ["So", "sank", "er", "all\u00b7be\u00b7weint", ",", "an", "dei\u00b7nen", "Ulm\u00b7ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ilyssus, in sein duftend Grab.", "tokens": ["I\u00b7lys\u00b7sus", ",", "in", "sein", "duf\u00b7tend", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.5": {"text": "So stand, umschwemmt von der Verderbniss", "tokens": ["So", "stand", ",", "um\u00b7schwemmt", "von", "der", "Ver\u00b7derb\u00b7niss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Fluthen,", "tokens": ["Flut\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Gleich fern von Sklaverey und Tyranney,", "tokens": ["Gleich", "fern", "von", "Skla\u00b7ve\u00b7rey", "und", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Roms ", "tokens": ["Roms"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Guten", "tokens": ["Gu\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Und hoher V\u00e4tertugend treu.", "tokens": ["Und", "ho\u00b7her", "V\u00e4\u00b7ter\u00b7tu\u00b7gend", "treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So standst du, Trefflicher, seit funfzig Jahren,", "tokens": ["So", "standst", "du", ",", "Treff\u00b7li\u00b7cher", ",", "seit", "funf\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Weisheit gross, und gross durch Thatenkraft;", "tokens": ["Durch", "Weis\u00b7heit", "gross", ",", "und", "gross", "durch", "Tha\u00b7ten\u00b7kraft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So stehst du noch, umkr\u00e4nzt mit silberweissen", "tokens": ["So", "stehst", "du", "noch", ",", "um\u00b7kr\u00e4nzt", "mit", "sil\u00b7ber\u00b7weis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Haaren,", "tokens": ["Haa\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hochfreudig noch und unerschlafft.", "tokens": ["Hoch\u00b7freu\u00b7dig", "noch", "und", "un\u00b7er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es huldigt dir die Unschuld, die du sch\u00fctztest;", "tokens": ["Es", "hul\u00b7digt", "dir", "die", "Un\u00b7schuld", ",", "die", "du", "sch\u00fctz\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es feyert dich das nie gebogne Recht.", "tokens": ["Es", "fe\u00b7yert", "dich", "das", "nie", "ge\u00b7bog\u00b7ne", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ehrt die Bosheit dich, die du zu Boden blitztest,", "tokens": ["Es", "ehrt", "die", "Bos\u00b7heit", "dich", ",", "die", "du", "zu", "Bo\u00b7den", "blitz\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nennt dich knirschend recht und schlecht.", "tokens": ["Und", "nennt", "dich", "knir\u00b7schend", "recht", "und", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du standst allein in der Verderbniss Mitte,", "tokens": ["Du", "standst", "al\u00b7lein", "in", "der", "Ver\u00b7derb\u00b7niss", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihre Strudel rissen dich nicht hin.", "tokens": ["Und", "ih\u00b7re", "Stru\u00b7del", "ris\u00b7sen", "dich", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fest hieltest du und steif an angestammter Sitte", "tokens": ["Fest", "hiel\u00b7test", "du", "und", "steif", "an", "an\u00b7ge\u00b7stamm\u00b7ter", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und an der V\u00e4ter gradem Sinn.", "tokens": ["Und", "an", "der", "V\u00e4\u00b7ter", "gra\u00b7dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es prallten ab von deiner rauhen Tugend", "tokens": ["Es", "prall\u00b7ten", "ab", "von", "dei\u00b7ner", "rau\u00b7hen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Pfeile geckischer Verfeinerung.", "tokens": ["Die", "Pfei\u00b7le", "ge\u00b7cki\u00b7scher", "Ver\u00b7fei\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Des Auslands Flittergold, der Firniss deutscher", "tokens": ["Des", "Aus\u00b7lands", "Flit\u00b7ter\u00b7gold", ",", "der", "Fir\u00b7niss", "deut\u00b7scher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jugend", "tokens": ["Ju\u00b7gend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Bed\u00fcnkte dich Verweichlichung.", "tokens": ["Be\u00b7d\u00fcnk\u00b7te", "dich", "Ver\u00b7weich\u00b7li\u00b7chung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der bunten Thorheit gaukelndes Gepr\u00e4nge", "tokens": ["Der", "bun\u00b7ten", "Thor\u00b7heit", "gau\u00b7keln\u00b7des", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerstob besch\u00e4mt vor deinem finstern Blick.", "tokens": ["Zer\u00b7stob", "be\u00b7sch\u00e4mt", "vor", "dei\u00b7nem", "fins\u00b7tern", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Laster zitterte vor deines Spruches Strenge,", "tokens": ["Das", "Las\u00b7ter", "zit\u00b7ter\u00b7te", "vor", "dei\u00b7nes", "Spru\u00b7ches", "Stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und floh in sein Verliess zur\u00fcck.", "tokens": ["Und", "floh", "in", "sein", "Ver\u00b7liess", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Im Felsengrund auf ehrnen S\u00e4ulen baute", "tokens": ["Im", "Fel\u00b7sen\u00b7grund", "auf", "ehr\u00b7nen", "S\u00e4u\u00b7len", "bau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deiner Brust sich einen goldnen Thron", "tokens": ["In", "dei\u00b7ner", "Brust", "sich", "ei\u00b7nen", "gold\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschheit Tr\u00f6sterin, der Edleren Ver-", "tokens": ["Der", "Menschheit", "Tr\u00f6s\u00b7te\u00b7rin", ",", "der", "Ed\u00b7le\u00b7ren", "Ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-", "measure": 