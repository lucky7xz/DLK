{"textgrid.poem.41363": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Wolf und der Hund", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein abgezehrter Wolf, ein Bild der D\u00fcrftigkeit,", "tokens": ["Ein", "ab\u00b7ge\u00b7zehr\u00b7ter", "Wolf", ",", "ein", "Bild", "der", "D\u00fcrf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah einen feisten Hund bei Nacht umherspazieren.", "tokens": ["Sah", "ei\u00b7nen", "feis\u00b7ten", "Hund", "bei", "Nacht", "um\u00b7her\u00b7spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Wanst gefiel ihm sehr; drum hielt ers f\u00fcr gescheidt,", "tokens": ["Sein", "Wanst", "ge\u00b7fiel", "ihm", "sehr", ";", "drum", "hielt", "ers", "f\u00fcr", "ge\u00b7scheidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "PAV", "VVFIN", "PIS", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei diesem Fremden sich manierlich aufzuf\u00fchren.", "tokens": ["Bei", "die\u00b7sem", "Frem\u00b7den", "sich", "ma\u00b7nier\u00b7lich", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er schien, vor gro\u00dfer Lust, ganz au\u00dfer sich zu sein,", "tokens": ["Er", "schien", ",", "vor", "gro\u00b7\u00dfer", "Lust", ",", "ganz", "au\u00b7\u00dfer", "sich", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "PRF", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gesellschaft solcher Art im Felde vorzufinden,", "tokens": ["Ge\u00b7sell\u00b7schaft", "sol\u00b7cher", "Art", "im", "Fel\u00b7de", "vor\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sprach: Wann wird auch mich ein kleines Gl\u00fcck erfreun?", "tokens": ["Und", "sprach", ":", "Wann", "wird", "auch", "mich", "ein", "klei\u00b7nes", "Gl\u00fcck", "er\u00b7freun", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ach! wie k\u00f6nnte mich ein guter Rath verbinden!", "tokens": ["Und", "ach", "!", "wie", "k\u00f6nn\u00b7te", "mich", "ein", "gu\u00b7ter", "Rath", "ver\u00b7bin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An G\u00f6nnern fehlt es nur; die Zeiten sind nicht gut.", "tokens": ["An", "G\u00f6n\u00b7nern", "fehlt", "es", "nur", ";", "die", "Zei\u00b7ten", "sind", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein Blutsfreund ladet uns mit andern lieben G\u00e4sten.", "tokens": ["Kein", "Bluts\u00b7freund", "la\u00b7det", "uns", "mit", "an\u00b7dern", "lie\u00b7ben", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir k\u00e4mpfen um den Fra\u00df; wann, mit vergn\u00fcgtem Muth,", "tokens": ["Wir", "k\u00e4mp\u00b7fen", "um", "den", "Fra\u00df", ";", "wann", ",", "mit", "ver\u00b7gn\u00fcg\u00b7tem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Herren Hunde sich in vollen K\u00fcchen m\u00e4sten.", "tokens": ["Die", "Her\u00b7ren", "Hun\u00b7de", "sich", "in", "vol\u00b7len", "K\u00fc\u00b7chen", "m\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Melamp erwidert drauf: Freund! wir beklagen dich;", "tokens": ["Me\u00b7lamp", "er\u00b7wi\u00b7dert", "drauf", ":", "Freund", "!", "wir", "be\u00b7kla\u00b7gen", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir glauben's, dort im Wald ist oft nicht viel zu fressen.", "tokens": ["Wir", "glau\u00b7ben's", ",", "dort", "im", "Wald", "ist", "oft", "nicht", "viel", "zu", "fres\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPRART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch willst du mit mir gehn, so wirst du, so wie ich,", "tokens": ["Doch", "willst", "du", "mit", "mir", "gehn", ",", "so", "wirst", "du", ",", "so", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "ADV", "VAFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Wunsch verpfleget sein, und aller Noth vergessen.", "tokens": ["Nach", "Wunsch", "ver\u00b7pfle\u00b7get", "sein", ",", "und", "al\u00b7ler", "Noth", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$,", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich liebet Herr und Frau; mein Amt f\u00e4llt gar nicht schwer.", "tokens": ["Mich", "lie\u00b7bet", "Herr", "und", "Frau", ";", "mein", "Amt", "f\u00e4llt", "gar", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich h\u00fcte Haus und Hof, und halte n\u00e4chtlich Wache.", "tokens": ["Ich", "h\u00fc\u00b7te", "Haus", "und", "Hof", ",", "und", "hal\u00b7te", "n\u00e4cht\u00b7lich", "Wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch du scheinst mir geschickt zur Hut und Gegenwehr;", "tokens": ["Auch", "du", "scheinst", "mir", "ge\u00b7schickt", "zur", "Hut", "und", "Ge\u00b7gen\u00b7wehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und mehr bedarf es nicht, da\u00df man dich gl\u00fccklich mache.", "tokens": ["Und", "mehr", "be\u00b7darf", "es", "nicht", ",", "da\u00df", "man", "dich", "gl\u00fcck\u00b7lich", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Wolf umhalset ihn; und als er hurtig trabt,", "tokens": ["Der", "Wolf", "um\u00b7hal\u00b7set", "ihn", ";", "und", "als", "er", "hur\u00b7tig", "trabt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "$.", "KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Stelle vorzustehn, die man ihm angetragen,", "tokens": ["Der", "Stel\u00b7le", "vor\u00b7zu\u00b7stehn", ",", "die", "man", "ihm", "an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sieht er des Hundes Hals enthaart und abgeschabt,", "tokens": ["Sieht", "er", "des", "Hun\u00b7des", "Hals", "ent\u00b7haart", "und", "ab\u00b7ge\u00b7schabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wird aus F\u00fcrwitz k\u00fchn, ihn desfalls zu befragen.", "tokens": ["Und", "wird", "aus", "F\u00fcr\u00b7witz", "k\u00fchn", ",", "ihn", "des\u00b7falls", "zu", "be\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADJD", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mich d\u00fcnkt, versetzt sein Freund, mir f\u00e4llt die Ursach' ein:", "tokens": ["Mich", "d\u00fcnkt", ",", "ver\u00b7setzt", "sein", "Freund", ",", "mir", "f\u00e4llt", "die", "Ur\u00b7sach'", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Tages legt man mich mit Schmeicheln an die Kette;", "tokens": ["Des", "Ta\u00b7ges", "legt", "man", "mich", "mit", "Schmei\u00b7cheln", "an", "die", "Ket\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PRF", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Furcht, ich m\u00f6chte sonst falsch oder bei\u00dfig sein,", "tokens": ["Aus", "Furcht", ",", "ich", "m\u00f6ch\u00b7te", "sonst", "falsch", "o\u00b7der", "bei\u00b7\u00dfig", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VMFIN", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dafern ein Held, wie ich, stets seinen Willen h\u00e4tte.", "tokens": ["Da\u00b7fern", "ein", "Held", ",", "wie", "ich", ",", "stets", "sei\u00b7nen", "Wil\u00b7len", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber schadet dies? Ich liege warm und still;", "tokens": ["Was", "a\u00b7ber", "scha\u00b7det", "dies", "?", "Ich", "lie\u00b7ge", "warm", "und", "still", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PDS", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Herr besuchet mich; der Knecht bringt Trank und Speise.", "tokens": ["Mein", "Herr", "be\u00b7su\u00b7chet", "mich", ";", "der", "Knecht", "bringt", "Trank", "und", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Wolf, der weiter nicht den Hund begleiten will,", "tokens": ["Der", "Wolf", ",", "der", "wei\u00b7ter", "nicht", "den", "Hund", "be\u00b7glei\u00b7ten", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sucht seinen R\u00fcckweg bald, und dankt ihm f\u00fcr die Reise", "tokens": ["Sucht", "sei\u00b7nen", "R\u00fcck\u00b7weg", "bald", ",", "und", "dankt", "ihm", "f\u00fcr", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nein! ruft er: auf der Welt ist nichts der Freiheit gleich.", "tokens": ["Nein", "!", "ruft", "er", ":", "auf", "der", "Welt", "ist", "nichts", "der", "Frei\u00b7heit", "gleich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "$.", "APPR", "ART", "NN", "VAFIN", "PIS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sollt' ich mir einen Stand, den sie nicht schm\u00fcckt, erw\u00e4hlen?", "tokens": ["Sollt'", "ich", "mir", "ei\u00b7nen", "Stand", ",", "den", "sie", "nicht", "schm\u00fcckt", ",", "er\u00b7w\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Weisen gilt sie mehr als Thron und K\u00f6nigreich:", "tokens": ["Dem", "Wei\u00b7sen", "gilt", "sie", "mehr", "als", "Thron", "und", "K\u00f6\u00b7nig\u00b7reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihm die Freiheit fehlt, so wird ihm alles fehlen.", "tokens": ["Wenn", "ihm", "die", "Frei\u00b7heit", "fehlt", ",", "so", "wird", "ihm", "al\u00b7les", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein abgezehrter Wolf, ein Bild der D\u00fcrftigkeit,", "tokens": ["Ein", "ab\u00b7ge\u00b7zehr\u00b7ter", "Wolf", ",", "ein", "Bild", "der", "D\u00fcrf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah einen feisten Hund bei Nacht umherspazieren.", "tokens": ["Sah", "ei\u00b7nen", "feis\u00b7ten", "Hund", "bei", "Nacht", "um\u00b7her\u00b7spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Wanst gefiel ihm sehr; drum hielt ers f\u00fcr gescheidt,", "tokens": ["Sein", "Wanst", "ge\u00b7fiel", "ihm", "sehr", ";", "drum", "hielt", "ers", "f\u00fcr", "ge\u00b7scheidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "PAV", "VVFIN", "PIS", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei diesem Fremden sich manierlich aufzuf\u00fchren.", "tokens": ["Bei", "die\u00b7sem", "Frem\u00b7den", "sich", "ma\u00b7nier\u00b7lich", "auf\u00b7zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er schien, vor gro\u00dfer Lust, ganz au\u00dfer sich zu sein,", "tokens": ["Er", "schien", ",", "vor", "gro\u00b7\u00dfer", "Lust", ",", "ganz", "au\u00b7\u00dfer", "sich", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "PRF", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gesellschaft solcher Art im Felde vorzufinden,", "tokens": ["Ge\u00b7sell\u00b7schaft", "sol\u00b7cher", "Art", "im", "Fel\u00b7de", "vor\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sprach: Wann wird auch mich ein kleines Gl\u00fcck erfreun?", "tokens": ["Und", "sprach", ":", "Wann", "wird", "auch", "mich", "ein", "klei\u00b7nes", "Gl\u00fcck", "er\u00b7freun", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ach! wie k\u00f6nnte mich ein guter Rath verbinden!", "tokens": ["Und", "ach", "!", "wie", "k\u00f6nn\u00b7te", "mich", "ein", "gu\u00b7ter", "Rath", "ver\u00b7bin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An G\u00f6nnern fehlt es nur; die Zeiten sind nicht gut.", "tokens": ["An", "G\u00f6n\u00b7nern", "fehlt", "es", "nur", ";", "die", "Zei\u00b7ten", "sind", "nicht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein Blutsfreund ladet uns mit andern lieben G\u00e4sten.", "tokens": ["Kein", "Bluts\u00b7freund", "la\u00b7det", "uns", "mit", "an\u00b7dern", "lie\u00b7ben", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wir k\u00e4mpfen um den Fra\u00df; wann, mit vergn\u00fcgtem Muth,", "tokens": ["Wir", "k\u00e4mp\u00b7fen", "um", "den", "Fra\u00df", ";", "wann", ",", "mit", "ver\u00b7gn\u00fcg\u00b7tem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Herren Hunde sich in vollen K\u00fcchen m\u00e4sten.", "tokens": ["Die", "Her\u00b7ren", "Hun\u00b7de", "sich", "in", "vol\u00b7len", "K\u00fc\u00b7chen", "m\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Melamp erwidert drauf: Freund! wir beklagen dich;", "tokens": ["Me\u00b7lamp", "er\u00b7wi\u00b7dert", "drauf", ":", "Freund", "!", "wir", "be\u00b7kla\u00b7gen", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir glauben's, dort im Wald ist oft nicht viel zu fressen.", "tokens": ["Wir", "glau\u00b7ben's", ",", "dort", "im", "Wald", "ist", "oft", "nicht", "viel", "zu", "fres\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPRART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch willst du mit mir gehn, so wirst du, so wie ich,", "tokens": ["Doch", "willst", "du", "mit", "mir", "gehn", ",", "so", "wirst", "du", ",", "so", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "ADV", "VAFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Wunsch verpfleget sein, und aller Noth vergessen.", "tokens": ["Nach", "Wunsch", "ver\u00b7pfle\u00b7get", "sein", ",", "und", "al\u00b7ler", "Noth", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$,", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich liebet Herr und Frau; mein Amt f\u00e4llt gar nicht schwer.", "tokens": ["Mich", "lie\u00b7bet", "Herr", "und", "Frau", ";", "mein", "Amt", "f\u00e4llt", "gar", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich h\u00fcte Haus und Hof, und halte n\u00e4chtlich Wache.", "tokens": ["Ich", "h\u00fc\u00b7te", "Haus", "und", "Hof", ",", "und", "hal\u00b7te", "n\u00e4cht\u00b7lich", "Wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch du scheinst mir geschickt zur Hut und Gegenwehr;", "tokens": ["Auch", "du", "scheinst", "mir", "ge\u00b7schickt", "zur", "Hut", "und", "Ge\u00b7gen\u00b7wehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und mehr bedarf es nicht, da\u00df man dich gl\u00fccklich mache.", "tokens": ["Und", "mehr", "be\u00b7darf", "es", "nicht", ",", "da\u00df", "man", "dich", "gl\u00fcck\u00b7lich", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Wolf umhalset ihn; und als er hurtig trabt,", "tokens": ["Der", "Wolf", "um\u00b7hal\u00b7set", "ihn", ";", "und", "als", "er", "hur\u00b7tig", "trabt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "$.", "KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Stelle vorzustehn, die man ihm angetragen,", "tokens": ["Der", "Stel\u00b7le", "vor\u00b7zu\u00b7stehn", ",", "die", "man", "ihm", "an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sieht er des Hundes Hals enthaart und abgeschabt,", "tokens": ["Sieht", "er", "des", "Hun\u00b7des", "Hals", "ent\u00b7haart", "und", "ab\u00b7ge\u00b7schabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wird aus F\u00fcrwitz k\u00fchn, ihn desfalls zu befragen.", "tokens": ["Und", "wird", "aus", "F\u00fcr\u00b7witz", "k\u00fchn", ",", "ihn", "des\u00b7falls", "zu", "be\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", 