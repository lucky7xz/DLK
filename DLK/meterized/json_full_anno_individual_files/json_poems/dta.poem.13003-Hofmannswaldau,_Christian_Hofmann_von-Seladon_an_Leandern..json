{"dta.poem.13003": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seladon an Leandern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So nimm, Leander! hin, was ich im gr\u00fcnen schreibe,", "tokens": ["So", "nimm", ",", "Le\u00b7an\u00b7der", "!", "hin", ",", "was", "ich", "im", "gr\u00fc\u00b7nen", "schrei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "NE", "$.", "PTKVZ", "$,", "PWS", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm gr\u00fcnen, wo mich n\u00e4chst dein beyseyn hat erg\u00f6tzt.", "tokens": ["Jm", "gr\u00fc\u00b7nen", ",", "wo", "mich", "n\u00e4chst", "dein", "bey\u00b7seyn", "hat", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und glaube: da\u00df ich dir, wie sonst, verbunden bleibe,", "tokens": ["Und", "glau\u00b7be", ":", "da\u00df", "ich", "dir", ",", "wie", "sonst", ",", "ver\u00b7bun\u00b7den", "blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "PPER", "$,", "PWAV", "ADV", "$,", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn dieses schlechte blat dich in vergn\u00fcgung setzt.", "tokens": ["Wenn", "die\u00b7ses", "schlech\u00b7te", "blat", "dich", "in", "ver\u00b7gn\u00fc\u00b7gung", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schaue nichts um mir, als blumen, gras und b\u00e4ume,", "tokens": ["Ich", "schau\u00b7e", "nichts", "um", "mir", ",", "als", "blu\u00b7men", ",", "gras", "und", "b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$,", "KOUS", "NN", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du aber bist entfernt, und doch betracht ich dich.", "tokens": ["Du", "a\u00b7ber", "bist", "ent\u00b7fernt", ",", "und", "doch", "be\u00b7tracht", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$,", "KON", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So ists, wir schmeucheln uns durch angenehme tr\u00e4ume,", "tokens": ["So", "ists", ",", "wir", "schmeu\u00b7cheln", "uns", "durch", "an\u00b7ge\u00b7neh\u00b7me", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie aber geht es dir? gedenckst du auch an mich?", "tokens": ["Wie", "a\u00b7ber", "geht", "es", "dir", "?", "ge\u00b7denckst", "du", "auch", "an", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gewi\u00df, ich w\u00fcnsche dir so viel vergn\u00fcgte stunden,", "tokens": ["Ge\u00b7wi\u00df", ",", "ich", "w\u00fcn\u00b7sche", "dir", "so", "viel", "ver\u00b7gn\u00fcg\u00b7te", "stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als jener gro\u00dfe baum beliebte bl\u00e4tter n\u00e4hrt.", "tokens": ["Als", "je\u00b7ner", "gro\u00b7\u00dfe", "baum", "be\u00b7lieb\u00b7te", "bl\u00e4t\u00b7ter", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du hast den wermuth-safft schon allzu wohl empfunden,", "tokens": ["Du", "hast", "den", "wer\u00b7muth\u00b7\u00b7safft", "schon", "all\u00b7zu", "wohl", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum hoff auch, da\u00df das gl\u00fcck dir honigseim beschert.", "tokens": ["Drum", "hoff", "auch", ",", "da\u00df", "das", "gl\u00fcck", "dir", "ho\u00b7ni\u00b7gseim", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wohl dem, der sich zuvor rechtschaffen wei\u00df zu schmiegen.", "tokens": ["Wohl", "dem", ",", "der", "sich", "zu\u00b7vor", "recht\u00b7schaf\u00b7fen", "wei\u00df", "zu", "schmie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "ADV", "VVINF", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zufriedenheit bleibt doch der edlen tugend lohn.", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "bleibt", "doch", "der", "ed\u00b7len", "tu\u00b7gend", "lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wie dieselbe dich vollkommen wird vergn\u00fcgen;", "tokens": ["Und", "wie", "die\u00b7sel\u00b7be", "dich", "voll\u00b7kom\u00b7men", "wird", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDAT", "PRF", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So bleib ich unverr\u00fcckt dein treuer Seladon.", "tokens": ["So", "bleib", "ich", "un\u00b7ver\u00b7r\u00fcckt", "dein", "treu\u00b7er", "Se\u00b7la\u00b7don", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}