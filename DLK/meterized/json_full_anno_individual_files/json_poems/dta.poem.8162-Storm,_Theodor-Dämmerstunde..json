{"dta.poem.8162": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mmerstunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Das Haupt zu dir gewendet, sa\u00dfen wir;", "tokens": ["Das", "Haupt", "zu", "dir", "ge\u00b7wen\u00b7det", ",", "sa\u00b7\u00dfen", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sanfter f\u00fchlten wir die Stunden flie\u00dfen,", "tokens": ["Und", "sanf\u00b7ter", "f\u00fchl\u00b7ten", "wir", "die", "Stun\u00b7den", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und stiller ward es zwischen mir und dir;", "tokens": ["Und", "stil\u00b7ler", "ward", "es", "zwi\u00b7schen", "mir", "und", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis unsre Augen in einander sanken", "tokens": ["Bis", "uns\u00b7re", "Au\u00b7gen", "in", "ein\u00b7an\u00b7der", "san\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wir berauscht der Seele Athmen tranken.", "tokens": ["Und", "wir", "be\u00b7rauscht", "der", "See\u00b7le", "Ath\u00b7men", "tran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}