{"dta.poem.12814": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  N. N. Medicin\u00e6 Licentiat  \n wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Njmm hin, Gelehrter Freund! den wohlverdienten prei\u00df:", "tokens": ["Njmm", "hin", ",", "Ge\u00b7lehr\u00b7ter", "Freund", "!", "den", "wohl\u00b7ver\u00b7dien\u00b7ten", "prei\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm hin vor deine m\u00fch der ehre lorbeer-rei\u00df,", "tokens": ["Nimm", "hin", "vor", "dei\u00b7ne", "m\u00fch", "der", "eh\u00b7re", "lor\u00b7beer\u00b7rei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So Meditrinens hand um deine schl\u00e4fe windet.", "tokens": ["So", "Me\u00b7di\u00b7tri\u00b7nens", "hand", "um", "dei\u00b7ne", "schl\u00e4\u00b7fe", "win\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was mancher nur durch geld und fremde mittel findet,", "tokens": ["Was", "man\u00b7cher", "nur", "durch", "geld", "und", "frem\u00b7de", "mit\u00b7tel", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gew\u00e4hrt dir dein verdienst. Du hast von jugend auf", "tokens": ["Ge\u00b7w\u00e4hrt", "dir", "dein", "ver\u00b7dienst", ".", "Du", "hast", "von", "ju\u00b7gend", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die wissenschafft geliebt. Dein gantzer lebens-lauff", "tokens": ["Die", "wis\u00b7sen\u00b7schafft", "ge\u00b7liebt", ".", "Dein", "gant\u00b7zer", "le\u00b7bens\u00b7lauff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist tugend, flei\u00df und witz. Man kan es zeitig mercken,", "tokens": ["Ist", "tu\u00b7gend", ",", "flei\u00df", "und", "witz", ".", "Man", "kan", "es", "zei\u00b7tig", "mer\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVFIN", "KON", "NN", "$.", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was eine rose wird. Es lie\u00df in deinen wercken", "tokens": ["Was", "ei\u00b7ne", "ro\u00b7se", "wird", ".", "Es", "lie\u00df", "in", "dei\u00b7nen", "wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch in der kindheit schon sich etwas kluges sehn.", "tokens": ["Auch", "in", "der", "kind\u00b7heit", "schon", "sich", "et\u00b7was", "klu\u00b7ges", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "PRF", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den faulen m\u00fc\u00dfiggang ohn allen zwang verschm\u00e4hn,", "tokens": ["Den", "fau\u00b7len", "m\u00fc\u00b7\u00dfig\u00b7gang", "ohn", "al\u00b7len", "zwang", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zeigt nichts gemeines an. Du bist nicht umgeschlagen,", "tokens": ["Zeigt", "nichts", "ge\u00b7mei\u00b7nes", "an", ".", "Du", "bist", "nicht", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "PTKVZ", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie viele, die erhitzt erst nach viel b\u00fcchern fragen,", "tokens": ["Wie", "vie\u00b7le", ",", "die", "er\u00b7hitzt", "erst", "nach", "viel", "b\u00fc\u00b7chern", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "VVFIN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und doch in kurtzer zeit derselben feinde seyn.", "tokens": ["Und", "doch", "in", "kurt\u00b7zer", "zeit", "der\u00b7sel\u00b7ben", "fein\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Eltern konten sich an deinem flei\u00df erfreun,", "tokens": ["Die", "El\u00b7tern", "kon\u00b7ten", "sich", "an", "dei\u00b7nem", "flei\u00df", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil er best\u00e4ndig war. Du hast das gold der stunden", "tokens": ["Weil", "er", "be\u00b7st\u00e4n\u00b7dig", "war", ".", "Du", "hast", "das", "gold", "der", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nicht liederlich verpra\u00dft. Du hast den trieb empfunden,", "tokens": ["Nicht", "lie\u00b7der\u00b7lich", "ver\u00b7pra\u00dft", ".", "Du", "hast", "den", "trieb", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$.", "PPER", "VAFIN", "ART", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der zum Parnassus f\u00fchrt, und ihn nicht unterdr\u00fcckt.", "tokens": ["Der", "zum", "Par\u00b7nas\u00b7sus", "f\u00fchrt", ",", "und", "ihn", "nicht", "un\u00b7ter\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,", "KON", "PPER", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Die hohe schule hat schon manchen heimgeschickt,", "tokens": ["Die", "ho\u00b7he", "schu\u00b7le", "hat", "schon", "man\u00b7chen", "heim\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VAFIN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der nicht so viel gewust, als du bereits verstanden,", "tokens": ["Der", "nicht", "so", "viel", "ge\u00b7wust", ",", "als", "du", "be\u00b7reits", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als du nach Jena kamst. Du warst in allen landen", "tokens": ["Als", "du", "nach", "Je\u00b7na", "kamst", ".", "Du", "warst", "in", "al\u00b7len", "lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein schon bekanter gast, dieweil dein muntrer witz", "tokens": ["Ein", "schon", "be\u00b7kan\u00b7ter", "gast", ",", "die\u00b7weil", "dein", "mun\u00b7trer", "witz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie flei\u00dfig durchstudirt. Der alten R\u00f6mer sitz,", "tokens": ["Sie", "flei\u00b7\u00dfig", "durch\u00b7stu\u00b7dirt", ".", "Der", "al\u00b7ten", "R\u00f6\u00b7mer", "sitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$.", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Griechen kluges thun, die n\u00fctzlichsten geschichte,", "tokens": ["Der", "Grie\u00b7chen", "klu\u00b7ges", "thun", ",", "die", "n\u00fctz\u00b7lichs\u00b7ten", "ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVINF", "$,", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Die theure rede-kunst, was der vernunfft zum lichte,", "tokens": ["Die", "theu\u00b7re", "re\u00b7de\u00b7kunst", ",", "was", "der", "ver\u00b7nunfft", "zum", "lich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zur regel dienen kan, dis alles hattest du", "tokens": ["Zur", "re\u00b7gel", "die\u00b7nen", "kan", ",", "dis", "al\u00b7les", "hat\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$,", "PDS", "PIS", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In deinen kopff gebracht. Ein freund beqvemer ruh", "tokens": ["In", "dei\u00b7nen", "kopff", "ge\u00b7bracht", ".", "Ein", "freund", "be\u00b7qve\u00b7mer", "ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bringt nicht so viel mit sich auf universit\u00e4ten,", "tokens": ["Bringt", "nicht", "so", "viel", "mit", "sich", "auf", "u\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "APPR", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Drum kontest du begl\u00fcckt die sch\u00f6ne bahn betreten,", "tokens": ["Drum", "kon\u00b7test", "du", "be\u00b7gl\u00fcckt", "die", "sch\u00f6\u00b7ne", "bahn", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auf welcher Treuner dich zur tugend angef\u00fchrt.", "tokens": ["Auf", "wel\u00b7cher", "Treu\u00b7ner", "dich", "zur", "tu\u00b7gend", "an\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Hamberger lehrte dich, was so wol n\u00fctzt als ziert,", "tokens": ["Ham\u00b7ber\u00b7ger", "lehr\u00b7te", "dich", ",", "was", "so", "wol", "n\u00fctzt", "als", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Indem er dir den grund von der natur erkl\u00e4rte,", "tokens": ["In\u00b7dem", "er", "dir", "den", "grund", "von", "der", "na\u00b7tur", "er\u00b7kl\u00e4r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und durch den augenschein der s\u00e4tze schlu\u00df bew\u00e4hrte.", "tokens": ["Und", "durch", "den", "au\u00b7gen\u00b7schein", "der", "s\u00e4t\u00b7ze", "schlu\u00df", "be\u00b7w\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wer so ger\u00fcstet ist, der scheuet keinen sand,", "tokens": ["Wer", "so", "ge\u00b7r\u00fcs\u00b7tet", "ist", ",", "der", "scheu\u00b7et", "kei\u00b7nen", "sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "PRELS", "VVFIN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Kein irrlicht, keinen sturm, er wagt sich mit verstand", "tokens": ["Kein", "irr\u00b7licht", ",", "kei\u00b7nen", "sturm", ",", "er", "wagt", "sich", "mit", "ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "In die geheime burg der hohen Meditrine,", "tokens": ["In", "die", "ge\u00b7hei\u00b7me", "burg", "der", "ho\u00b7hen", "Me\u00b7di\u00b7tri\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Allwo dir Krausens witz den weg zur heilungs-b\u00fchne", "tokens": ["All\u00b7wo", "dir", "Krau\u00b7sens", "witz", "den", "weg", "zur", "hei\u00b7lungs\u00b7b\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "APPR", "ART", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Aufrichtig kund gethan. Drauf stellte Schlevogt dir", "tokens": ["Auf\u00b7rich\u00b7tig", "kund", "ge\u00b7than", ".", "Drauf", "stell\u00b7te", "Schle\u00b7vogt", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "VVPP", "$.", "PAV", "VVFIN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der c\u00f6rper \u00f6ffnung vor, und \u00f6ffnete die th\u00fcr", "tokens": ["Der", "c\u00f6r\u00b7per", "\u00f6ff\u00b7nung", "vor", ",", "und", "\u00f6ff\u00b7ne\u00b7te", "die", "th\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zur edlen scheide-kunst. Wie du noch mehr begriffen", "tokens": ["Zur", "ed\u00b7len", "schei\u00b7de\u00b7kunst", ".", "Wie", "du", "noch", "mehr", "be\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PWAV", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Morbonens syrten, b\u00e4nck und strudel zu umschiffen:", "tokens": ["Mor\u00b7bo\u00b7nens", "syr\u00b7ten", ",", "b\u00e4nck", "und", "stru\u00b7del", "zu", "um\u00b7schif\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.41": {"text": "Was Wedels kluger mund, was dich sein Sohn gelehrt,", "tokens": ["Was", "We\u00b7dels", "klu\u00b7ger", "mund", ",", "was", "dich", "sein", "Sohn", "