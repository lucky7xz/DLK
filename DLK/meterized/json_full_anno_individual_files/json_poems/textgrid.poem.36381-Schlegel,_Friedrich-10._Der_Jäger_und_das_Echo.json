INF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Bande der S\u00fcnden?", "tokens": ["Vom", "Ban\u00b7de", "der", "S\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Was gibt dieser Lieb'", "tokens": ["Was", "gibt", "die\u00b7ser", "Lieb'"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Erde zu lieb?", "tokens": ["Die", "Er\u00b7de", "zu", "lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Lieb!", "tokens": ["Lieb", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.18": {"line.1": {"text": "All Liebe dir weicht,", "tokens": ["All", "Lie\u00b7be", "dir", "weicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der deinen nicht gleicht,", "tokens": ["Der", "dei\u00b7nen", "nicht", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und nimmer auf Erd'", "tokens": ["Und", "nim\u00b7mer", "auf", "Erd'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wird Dank dir gew\u00e4hrt.", "tokens": ["Wird", "Dank", "dir", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der S