{"textgrid.poem.36381": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10. Der J\u00e4ger und das Echo", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Hitze, in K\u00e4lt',", "tokens": ["In", "Hit\u00b7ze", ",", "in", "K\u00e4lt'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Im offenen Feld,", "tokens": ["Im", "of\u00b7fe\u00b7nen", "Feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im d\u00fcsteren Wald", "tokens": ["Im", "d\u00fcs\u00b7te\u00b7ren", "Wald"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mein Jagdhorn erschallt.", "tokens": ["Mein", "Jagd\u00b7horn", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit Kummer und Plage", "tokens": ["Mit", "Kum\u00b7mer", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dem Wild ich nachjage,", "tokens": ["Dem", "Wild", "ich", "nach\u00b7ja\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bleibe dabei", "tokens": ["Und", "blei\u00b7be", "da\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Diana dir treu.", "tokens": ["Di\u00b7a\u00b7na", "dir", "treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.9": {"text": "Reu!", "tokens": ["Reu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Wer spricht hier von Reu?", "tokens": ["Wer", "spricht", "hier", "von", "Reu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich sage es frei,", "tokens": ["Ich", "sa\u00b7ge", "es", "frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es findet das Herz", "tokens": ["Es", "fin\u00b7det", "das", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wohl Freude im Schmerz;", "tokens": ["Wohl", "Freu\u00b7de", "im", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie soll in den W\u00e4ldern", "tokens": ["Wie", "soll", "in", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auf lustigen Feldern", "tokens": ["Auf", "lus\u00b7ti\u00b7gen", "Fel\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Freude nicht sein?", "tokens": ["Die", "Freu\u00b7de", "nicht", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sprich Ja oder Nein.", "tokens": ["Sprich", "Ja", "o\u00b7der", "Nein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKANT", "KON", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Nein!", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Wer ist, der die Jagd", "tokens": ["Wer", "ist", ",", "der", "die", "Jagd"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So trotzig veracht?", "tokens": ["So", "trot\u00b7zig", "ver\u00b7acht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sag' Echo, sag' an,", "tokens": ["Sag'", "E\u00b7cho", ",", "sag'", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Bist du es etwann?", "tokens": ["Bist", "du", "es", "et\u00b7wann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Geschw\u00e4tzige Nymphe", "tokens": ["Ge\u00b7schw\u00e4t\u00b7zi\u00b7ge", "Nym\u00b7phe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Flieh weiter und schimpfe;", "tokens": ["Flieh", "wei\u00b7ter", "und", "schimp\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Unsichtbare Eul'", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "Eul'"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Nicht l\u00e4nger verweil.", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "ver\u00b7weil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Eil!", "tokens": ["Eil", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "O G\u00f6ttin verweile,", "tokens": ["O", "G\u00f6t\u00b7tin", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wohin soll ich eilen?", "tokens": ["Wo\u00b7hin", "soll", "ich", "ei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ist etwa ein Wild,", "tokens": ["Ist", "et\u00b7wa", "ein", "Wild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Worauf ich gezielt?", "tokens": ["Wo\u00b7rauf", "ich", "ge\u00b7zielt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich seh' nichts dergleichen,", "tokens": ["Ich", "seh'", "nichts", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So weit ich kann reichen,", "tokens": ["So", "weit", "ich", "kann", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sprich, G\u00f6ttin, ein Wort,", "tokens": ["Sprich", ",", "G\u00f6t\u00b7tin", ",", "ein", "Wort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ist's hier oder dort?", "tokens": ["Ist's", "hier", "o\u00b7der", "dort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Dort!", "tokens": ["Dort", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Dort seh' ich ein' H\u00fctt',", "tokens": ["Dort", "seh'", "ich", "ein'", "H\u00fctt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was willst du damit?", "tokens": ["Was", "willst", "du", "da\u00b7mit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich folg' deinem Sinn,", "tokens": ["Ich", "fol\u00b7g'", "dei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich gehe dahin.", "tokens": ["Ich", "ge\u00b7he", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ein Kindlein ich finde", "tokens": ["Ein", "Kin\u00b7dlein", "ich", "fin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Beim Esel, beim Rinde,", "tokens": ["Beim", "E\u00b7sel", ",", "beim", "Rin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In Jammer und Not,", "tokens": ["In", "Jam\u00b7mer", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Was ist das mein Gott?", "tokens": ["Was", "ist", "das", "mein", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Gott!", "tokens": ["Gott", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Ja wohl, ich das Kind", "tokens": ["Ja", "wohl", ",", "ich", "das", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Auch g\u00f6ttergleich find';", "tokens": ["Auch", "g\u00f6t\u00b7ter\u00b7gleich", "find'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sein himmlischer Glanz", "tokens": ["Sein", "himm\u00b7li\u00b7scher", "Glanz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bew\u00e4hret es ganz.", "tokens": ["Be\u00b7w\u00e4h\u00b7ret", "es", "ganz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Was mag dich wohl zwingen", "tokens": ["Was", "mag", "dich", "wohl", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So elender Dingen,", "tokens": ["So", "e\u00b7len\u00b7der", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Da\u00df dir in der Kripp'", "tokens": ["Da\u00df", "dir", "in", "der", "Kripp'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Zu liegen belieb?", "tokens": ["Zu", "lie\u00b7gen", "be\u00b7lieb", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Lieb!", "tokens": ["Lieb", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "O himmlischer Trieb", "tokens": ["O", "himm\u00b7li\u00b7scher", "Trieb"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der g\u00f6ttlichen Lieb!", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7chen", "Lieb", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hat denn deine Macht", "tokens": ["Hat", "denn", "dei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Zum Mensch' dich gemacht,", "tokens": ["Zum", "Men\u00b7sch'", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welt zu entbinden", "tokens": ["Die", "Welt", "zu", "ent\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Bande der S\u00fcnden?", "tokens": ["Vom", "Ban\u00b7de", "der", "S\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Was gibt dieser Lieb'", "tokens": ["Was", "gibt", "die\u00b7ser", "Lieb'"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Erde zu lieb?", "tokens": ["Die", "Er\u00b7de", "zu", "lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Lieb!", "tokens": ["Lieb", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "All Liebe dir weicht,", "tokens": ["All", "Lie\u00b7be", "dir", "weicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der deinen nicht gleicht,", "tokens": ["Der", "dei\u00b7nen", "nicht", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und nimmer auf Erd'", "tokens": ["Und", "nim\u00b7mer", "auf", "Erd'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wird Dank dir gew\u00e4hrt.", "tokens": ["Wird", "Dank", "dir", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Sch\u00f6pfer der Erden", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Knechte will werden", "tokens": ["Zum", "Knech\u00b7te", "will", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "VAINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In der Sterblichkeit,", "tokens": ["In", "der", "Sterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In Jammer und Leid.", "tokens": ["In", "Jam\u00b7mer", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Leid!", "tokens": ["Leid", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.9": {"line.1": {"text": "Ich hab' es geh\u00f6rt,", "tokens": ["Ich", "hab'", "es", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mein Sch\u00f6pfer begehrt", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", "be\u00b7gehrt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nur Lieb' und Geduld", "tokens": ["Nur", "Lieb'", "und", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "F\u00fcr all seine Huld.", "tokens": ["F\u00fcr", "all", "sei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nun soll mich nichts scheiden", "tokens": ["Nun", "soll", "mich", "nichts", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Von Lieben, von Leiden,", "tokens": ["Von", "Lie\u00b7ben", ",", "von", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O da\u00df ich nur m\u00f6cht'", "tokens": ["O", "da\u00df", "ich", "nur", "m\u00f6cht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Dich lieben so recht.", "tokens": ["Dich", "lie\u00b7ben", "so", "recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Recht!", "tokens": ["Recht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.10": {"line.1": {"text": "Nun h\u00f6chste Gewalt,", "tokens": ["Nun", "h\u00f6chs\u00b7te", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In Kindes Gestalt,", "tokens": ["In", "Kin\u00b7des", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nimm Herze und Sinn", "tokens": ["Nimm", "Her\u00b7ze", "und", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zum Opfer dir hin.", "tokens": ["Zum", "Op\u00b7fer", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nimm hin all' mein Wesen,", "tokens": ["Nimm", "hin", "all'", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du willst mich erl\u00f6sen,", "tokens": ["Du", "willst", "mich", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was immer war mein,", "tokens": ["Was", "im\u00b7mer", "war", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPOSAT", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sei zwiefach jetzt dein.", "tokens": ["Sei", "zwie\u00b7fach", "jetzt", "dein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Dein!", "tokens": ["Dein", "!"], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.11": {"line.1": {"text": "In Hitze, in K\u00e4lt',", "tokens": ["In", "Hit\u00b7ze", ",", "in", "K\u00e4lt'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Im offenen Feld,", "tokens": ["Im", "of\u00b7fe\u00b7nen", "Feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im d\u00fcsteren Wald", "tokens": ["Im", "d\u00fcs\u00b7te\u00b7ren", "Wald"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mein Jagdhorn erschallt.", "tokens": ["Mein", "Jagd\u00b7horn", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mit Kummer und Plage", "tokens": ["Mit", "Kum\u00b7mer", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dem Wild ich nachjage,", "tokens": ["Dem", "Wild", "ich", "nach\u00b7ja\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bleibe dabei", "tokens": ["Und", "blei\u00b7be", "da\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Diana dir treu.", "tokens": ["Di\u00b7a\u00b7na", "dir", "treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.9": {"text": "Reu!", "tokens": ["Reu", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.12": {"line.1": {"text": "Wer spricht hier von Reu?", "tokens": ["Wer", "spricht", "hier", "von", "Reu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich sage es frei,", "tokens": ["Ich", "sa\u00b7ge", "es", "frei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es findet das Herz", "tokens": ["Es", "fin\u00b7det", "das", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wohl Freude im Schmerz;", "tokens": ["Wohl", "Freu\u00b7de", "im", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie soll in den W\u00e4ldern", "tokens": ["Wie", "soll", "in", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auf lustigen Feldern", "tokens": ["Auf", "lus\u00b7ti\u00b7gen", "Fel\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die Freude nicht sein?", "tokens": ["Die", "Freu\u00b7de", "nicht", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sprich Ja oder Nein.", "tokens": ["Sprich", "Ja", "o\u00b7der", "Nein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKANT", "KON", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Nein!", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.13": {"line.1": {"text": "Wer ist, der die Jagd", "tokens": ["Wer", "ist", ",", "der", "die", "Jagd"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So trotzig veracht?", "tokens": ["So", "trot\u00b7zig", "ver\u00b7acht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sag' Echo, sag' an,", "tokens": ["Sag'", "E\u00b7cho", ",", "sag'", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Bist du es etwann?", "tokens": ["Bist", "du", "es", "et\u00b7wann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Geschw\u00e4tzige Nymphe", "tokens": ["Ge\u00b7schw\u00e4t\u00b7zi\u00b7ge", "Nym\u00b7phe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Flieh weiter und schimpfe;", "tokens": ["Flieh", "wei\u00b7ter", "und", "schimp\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Unsichtbare Eul'", "tokens": ["Un\u00b7sicht\u00b7ba\u00b7re", "Eul'"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Nicht l\u00e4nger verweil.", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "ver\u00b7weil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Eil!", "tokens": ["Eil", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.14": {"line.1": {"text": "O G\u00f6ttin verweile,", "tokens": ["O", "G\u00f6t\u00b7tin", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wohin soll ich eilen?", "tokens": ["Wo\u00b7hin", "soll", "ich", "ei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ist etwa ein Wild,", "tokens": ["Ist", "et\u00b7wa", "ein", "Wild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Worauf ich gezielt?", "tokens": ["Wo\u00b7rauf", "ich", "ge\u00b7zielt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ich seh' nichts dergleichen,", "tokens": ["Ich", "seh'", "nichts", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So weit ich kann reichen,", "tokens": ["So", "weit", "ich", "kann", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sprich, G\u00f6ttin, ein Wort,", "tokens": ["Sprich", ",", "G\u00f6t\u00b7tin", ",", "ein", "Wort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ist's hier oder dort?", "tokens": ["Ist's", "hier", "o\u00b7der", "dort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Dort!", "tokens": ["Dort", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}}, "stanza.15": {"line.1": {"text": "Dort seh' ich ein' H\u00fctt',", "tokens": ["Dort", "seh'", "ich", "ein'", "H\u00fctt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Was willst du damit?", "tokens": ["Was", "willst", "du", "da\u00b7mit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich folg' deinem Sinn,", "tokens": ["Ich", "fol\u00b7g'", "dei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich gehe dahin.", "tokens": ["Ich", "ge\u00b7he", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ein Kindlein ich finde", "tokens": ["Ein", "Kin\u00b7dlein", "ich", "fin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Beim Esel, beim Rinde,", "tokens": ["Beim", "E\u00b7sel", ",", "beim", "Rin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In Jammer und Not,", "tokens": ["In", "Jam\u00b7mer", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Was ist das mein Gott?", "tokens": ["Was", "ist", "das", "mein", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Gott!", "tokens": ["Gott", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.16": {"line.1": {"text": "Ja wohl, ich das Kind", "tokens": ["Ja", "wohl", ",", "ich", "das", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "$,", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Auch g\u00f6ttergleich find';", "tokens": ["Auch", "g\u00f6t\u00b7ter\u00b7gleich", "find'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sein himmlischer Glanz", "tokens": ["Sein", "himm\u00b7li\u00b7scher", "Glanz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bew\u00e4hret es ganz.", "tokens": ["Be\u00b7w\u00e4h\u00b7ret", "es", "ganz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Was mag dich wohl zwingen", "tokens": ["Was", "mag", "dich", "wohl", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So e