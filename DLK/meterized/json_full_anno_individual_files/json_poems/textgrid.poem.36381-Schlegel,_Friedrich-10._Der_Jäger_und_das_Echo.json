os": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nun soll mich nichts scheiden", "tokens": ["Nun", "soll", "mich", "nichts", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Von Lieben, von Leiden,", "tokens": ["Von", "Lie\u00b7ben", ",", "von", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O da\u00df ich nur m\u00f6cht'", "tokens": ["O", "da\u00df", "ich", "nur", "m\u00f6cht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Dich lieben so recht.", "tokens": ["Dich", "lie\u00b7ben", "so", "recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Recht!", "tokens": ["Recht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.20": {"line.1": {"text": "Nun h\u00f6chste Gewalt,", "tokens": ["Nun", "h\u00f6chs\u00b7te", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In Kindes Gestalt,", "tokens": ["In", "Kin\u00b7des", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nimm Herze und Sinn", "tokens": ["Nimm", "Her\u00b7ze", "und", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zum Opfer dir hin.", "tokens": ["Zum", "Op\u00b7fer", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nimm hin all' mein Wesen,", "tokens": ["Nimm", "hin", "all'", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du willst mich erl\u00f6sen,", "tokens": ["Du", "willst", "mich", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was immer war mein,", "tokens": ["Was", "im\u00b7mer", "war", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPOSAT", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sei zwiefach jetzt dein.", "tokens": ["Sei", "zwie\u00b7fach", "jetzt", "dein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Dein!", "tokens": ["Dein", "!"], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$."], "meter": "+", "measure": "single.up"}}}}}