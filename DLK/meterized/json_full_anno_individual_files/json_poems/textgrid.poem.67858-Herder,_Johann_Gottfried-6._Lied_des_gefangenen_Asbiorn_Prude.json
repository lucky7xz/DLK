asure": "trochaic.tetra"}, "line.4": {"text": "Svanhid' im sch\u00f6nen D\u00e4nnmark,", "tokens": ["Svan\u00b7hid'", "im", "sch\u00f6\u00b7nen", "D\u00e4nn\u00b7mark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Ich hatts ihr zugesaget,", "tokens": ["Ich", "hatts", "ihr", "zu\u00b7ge\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu ihr bald heimzukommen, \u2013", "tokens": ["Zu", "ihr", "bald", "heim\u00b7zu\u00b7kom\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun seh' ich, wird das Schwert wohl", "tokens": ["Nun", "seh'", "ich", ",", "wird", "das", "Schwert", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Seite mir durchboren.", "tokens": ["Die", "Sei\u00b7te", "mir", "durch\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bier sassen wir trinken,", "tokens": ["Bier", "sas\u00b7sen", "wir", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", 