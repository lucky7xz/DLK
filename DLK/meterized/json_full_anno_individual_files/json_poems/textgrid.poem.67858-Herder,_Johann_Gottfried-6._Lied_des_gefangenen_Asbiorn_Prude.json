{"textgrid.poem.67858": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6. Lied des gefangenen Asbiorn Prude", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagets meiner Mutter:", "tokens": ["Sa\u00b7gets", "mei\u00b7ner", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sie wird den Sommer heurig", "tokens": ["Sie", "wird", "den", "Som\u00b7mer", "heu\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr's Sohnes Haar nicht k\u00e4mmen.", "tokens": ["Ih\u00b7r's", "Soh\u00b7nes", "Haar", "nicht", "k\u00e4m\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Svanhid' im sch\u00f6nen D\u00e4nnmark,", "tokens": ["Svan\u00b7hid'", "im", "sch\u00f6\u00b7nen", "D\u00e4nn\u00b7mark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.5": {"text": "Ich hatts ihr zugesaget,", "tokens": ["Ich", "hatts", "ihr", "zu\u00b7ge\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu ihr bald heimzukommen, \u2013", "tokens": ["Zu", "ihr", "bald", "heim\u00b7zu\u00b7kom\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun seh' ich, wird das Schwert wohl", "tokens": ["Nun", "seh'", "ich", ",", "wird", "das", "Schwert", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Seite mir durchboren.", "tokens": ["Die", "Sei\u00b7te", "mir", "durch\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bier sassen wir trinken,", "tokens": ["Bier", "sas\u00b7sen", "wir", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fuhren mit Freuden", "tokens": ["Fuh\u00b7ren", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Furt nach Hordland,", "tokens": ["Die", "Furt", "nach", "Hord\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Meeth wir tranken, schwazten;", "tokens": ["Meeth", "wir", "tran\u00b7ken", ",", "schwaz\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lachten viel beisammen. \u2013", "tokens": ["Lach\u00b7ten", "viel", "bei\u00b7sam\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nun lieg' ich beklommen", "tokens": ["Nun", "lieg'", "ich", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In der engen Riesenkluft hier.", "tokens": ["In", "der", "en\u00b7gen", "Rie\u00b7sen\u00b7kluft", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wir all beisammen waren,", "tokens": ["Da", "wir", "all", "bei\u00b7sam\u00b7men", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fuhren pr\u00e4chtig, vorne", "tokens": ["Fuh\u00b7ren", "pr\u00e4ch\u00b7tig", ",", "vor\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "$,", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Storolfs Sohn vor allen,", "tokens": ["Sto\u00b7rolfs", "Sohn", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Landte mit den langen", "tokens": ["Land\u00b7te", "mit", "den", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schiffen im Oresunde. \u2013", "tokens": ["Schif\u00b7fen", "im", "O\u00b7re\u00b7sun\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Nun mu\u00df ich hier sch\u00e4ndlich", "tokens": ["Nun", "mu\u00df", "ich", "hier", "sch\u00e4nd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Riesenst\u00e4tte schauen.", "tokens": ["Die", "Rie\u00b7sen\u00b7st\u00e4t\u00b7te", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Orm, im Schlachtensturme,", "tokens": ["Orm", ",", "im", "Schlach\u00b7ten\u00b7stur\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Str\u00f6mt den durstigen Raben", "tokens": ["Str\u00f6mt", "den", "durs\u00b7ti\u00b7gen", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Manches reiche Mahl.", "tokens": ["Man\u00b7ches", "rei\u00b7che", "Mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Manche wackre M\u00e4nner", "tokens": ["Man\u00b7che", "wack\u00b7re", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gab er den gier'gen W\u00f6lfen,", "tokens": ["Gab", "er", "den", "gier'\u00b7gen", "W\u00f6l\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Treflich an der Ifa", "tokens": ["Tref\u00b7lich", "an", "der", "I\u00b7fa"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Traf er Todeshieb.", "tokens": ["Traf", "er", "To\u00b7des\u00b7hieb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da auch ich, mit scharfem Schwerte,", "tokens": ["Da", "auch", "ich", ",", "mit", "schar\u00b7fem", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Warm von harten Hieben,", "tokens": ["Warm", "von", "har\u00b7ten", "Hie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00e4nnerhaufen m\u00e4ht'.", "tokens": ["M\u00e4n\u00b7ner\u00b7hau\u00b7fen", "m\u00e4ht'", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "'s war am Elfers Eiland", "tokens": ["'s", "war", "am", "El\u00b7fers", "Ei\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Entgegen dem schw\u00fclen Mittag,", "tokens": ["Ent\u00b7ge\u00b7gen", "dem", "schw\u00fc\u00b7len", "Mit\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Orm hagelt herrlich", "tokens": ["Orm", "ha\u00b7gelt", "herr\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Pfeil' auf die R\u00e4uber,", "tokens": ["Pfeil'", "auf", "die", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Auf die er traf.", "tokens": ["Auf", "die", "er", "traf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War'n alle noch bei'nander,", "tokens": ["Wa\u00b7r'n", "al\u00b7le", "noch", "bei'\u00b7nan\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gaut'r und Geiri,", "tokens": ["Gaut'r", "und", "Gei\u00b7ri", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Glum'r und Stari,", "tokens": ["Glum'r", "und", "Sta\u00b7ri", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sam'r und Seming'r,", "tokens": ["Sam'r", "und", "Se\u00b7ming'r", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Oddvarars S\u00f6hne,", "tokens": ["O\u00b7ddva\u00b7rars", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Haukr und Hoki,", "tokens": ["Haukr", "und", "Ho\u00b7ki", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Hroko (lies: Hrokr) und Tocki.", "tokens": ["Hro\u00b7ko", "(", "lies", ":", "Hrokr", ")", "und", "To\u00b7cki", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "$.", "NE", "$(", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wir oft zusammen schifften,", "tokens": ["Da", "wir", "oft", "zu\u00b7sam\u00b7men", "schiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hrani und Hogei (lies: Hogni),", "tokens": ["Hra\u00b7ni", "und", "Ho\u00b7gei", "(", "lies", ":", "Hog\u00b7ni", ")", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "KON", "NE", "$(", "VVFIN", "$.", "NE", "$(", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hialmr und Stafnir,", "tokens": ["Hi\u00b7almr", "und", "Staf\u00b7nir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Grani und Gunnar,", "tokens": ["Gra\u00b7ni", "und", "Gun\u00b7nar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+-+++", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Grimr und Sorkvir,", "tokens": ["Grimr", "und", "Sork\u00b7vir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Tumi, Torfvi,", "tokens": ["Tu\u00b7mi", ",", "Torf\u00b7vi", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Teite (lies: Teitr) und Geitir.", "tokens": ["Tei\u00b7te", "(", "lies", ":", "Tei\u00b7tr", ")", "und", "Gei\u00b7tir", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "$.", "NE", "$(", "KON", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Anders war's dort dr\u00fcben!", "tokens": ["An\u00b7ders", "wa\u00b7r's", "dort", "dr\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Selten wirs ausschlugen", "tokens": ["Sel\u00b7ten", "wirs", "aus\u00b7schlu\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIS", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Uns zu schlagen; selten", "tokens": ["Uns", "zu", "schla\u00b7gen", ";", "sel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Rieth ich's ab, mit Schwerte", "tokens": ["Rieth", "ich's", "ab", ",", "mit", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Scharfes Schwert zu sprechen.", "tokens": ["Schar\u00b7fes", "Schwert", "zu", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Doch Orm war immer", "tokens": ["Doch", "Orm", "war", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Unser der Erste.", "tokens": ["Un\u00b7ser", "der", "Ers\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "W\u00fcste Orm", "tokens": ["W\u00fcs\u00b7te", "Orm"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hier meine Qualen;", "tokens": ["Hier", "mei\u00b7ne", "Qua\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Stirne falten", "tokens": ["Die", "Stir\u00b7ne", "fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.