{"textgrid.poem.41493": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den verlornen Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du hin, du Tr\u00f6ster in Beschwerde,", "tokens": ["Wo", "bist", "du", "hin", ",", "du", "Tr\u00f6s\u00b7ter", "in", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein g\u00fcldner Schlaf?", "tokens": ["Mein", "g\u00fcld\u00b7ner", "Schlaf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "An dem ich sonst die Gr\u00f6\u00dfesten der Erde", "tokens": ["An", "dem", "ich", "sonst", "die", "Gr\u00f6\u00b7\u00dfes\u00b7ten", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Weit \u00fcbertraf.", "tokens": ["Weit", "\u00fc\u00b7bert\u00b7raf", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du hast mich oft an Wassern und an B\u00fcschen", "tokens": ["Du", "hast", "mich", "oft", "an", "Was\u00b7sern", "und", "an", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sanft \u00fcbereilt,", "tokens": ["Sanft", "\u00fc\u00b7be\u00b7reilt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und konntest mich mit be\u00dfrer Rast erfrischen,", "tokens": ["Und", "konn\u00b7test", "mich", "mit", "be\u00df\u00b7rer", "Rast", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als mir vorjetzt der weiche Pf\u00fchl ertheilt.", "tokens": ["Als", "mir", "vor\u00b7jetzt", "der", "wei\u00b7che", "Pf\u00fchl", "er\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein bedeckt vom himmlischen Gew\u00f6lbe", "tokens": ["Al\u00b7lein", "be\u00b7deckt", "vom", "himm\u00b7li\u00b7schen", "Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlief ich dann ein.", "tokens": ["Schlief", "ich", "dann", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die stolze Thems, die Saal und Hamburgs Elbe", "tokens": ["Die", "stol\u00b7ze", "Thems", ",", "die", "Saal", "und", "Ham\u00b7burgs", "El\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann Zeugin sein.", "tokens": ["Kann", "Zeu\u00b7gin", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dort hab' ich oft, in l\u00e4ngstvergr\u00fcnten Jahren,", "tokens": ["Dort", "hab'", "ich", "oft", ",", "in", "l\u00e4ngst\u00b7ver\u00b7gr\u00fcn\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich hingelegt,", "tokens": ["Mich", "hin\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und hoffnungsreich, in Sorgen unerfahren,", "tokens": ["Und", "hoff\u00b7nungs\u00b7reich", ",", "in", "Sor\u00b7gen", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der freien Ruh' um ihren Strand gepflegt.", "tokens": ["Der", "frei\u00b7en", "Ruh'", "um", "ih\u00b7ren", "Strand", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie s\u00e4uselten die L\u00fcfte so gelinde", "tokens": ["Wie", "s\u00e4u\u00b7sel\u00b7ten", "die", "L\u00fcf\u00b7te", "so", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Zu jener Ruh'!", "tokens": ["Zu", "je\u00b7ner", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie spielten mir die Wellen und die Winde", "tokens": ["Wie", "spiel\u00b7ten", "mir", "die", "Wel\u00b7len", "und", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Schlummer zu!", "tokens": ["Den", "Schlum\u00b7mer", "zu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mich st\u00f6rte nicht der Ehrsucht reger Kummer,", "tokens": ["Mich", "st\u00f6r\u00b7te", "nicht", "der", "Ehr\u00b7sucht", "re\u00b7ger", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der vielen droht;", "tokens": ["Der", "vie\u00b7len", "droht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich war, vertieft im angenehmsten Schlummer,", "tokens": ["Ich", "war", ",", "ver\u00b7tieft", "im", "an\u00b7ge\u00b7nehms\u00b7ten", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr alle Welt, nur nicht f\u00fcr Phyllis, todt.", "tokens": ["F\u00fcr", "al\u00b7le", "Welt", ",", "nur", "nicht", "f\u00fcr", "Phyl\u00b7lis", ",", "todt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "PTKNEG", "APPR", "NE", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie eilte dort, in jugendlichen Tr\u00e4umen,", "tokens": ["Sie", "eil\u00b7te", "dort", ",", "in", "ju\u00b7gend\u00b7li\u00b7chen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir immer nach;", "tokens": ["Mir", "im\u00b7mer", "nach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bald in der Flur, bald unter hohen B\u00e4umen,", "tokens": ["Bald", "in", "der", "Flur", ",", "bald", "un\u00b7ter", "ho\u00b7hen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald an dem Bach.", "tokens": ["Bald", "an", "dem", "Bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Oft stolz im Putz, oft leicht im Sch\u00e4ferkleide,", "tokens": ["Oft", "stolz", "im", "Putz", ",", "oft", "leicht", "im", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit offner Brust,", "tokens": ["Mit", "off\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Stets l\u00e4chelnd hold im Ueberflu\u00df der Freude:", "tokens": ["Stets", "l\u00e4\u00b7chelnd", "hold", "im", "Ue\u00b7berf\u00b7lu\u00df", "der", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sch\u00f6n von Gestalt, noch sch\u00f6ner durch die Lust.", "tokens": ["Sch\u00f6n", "von", "Ge\u00b7stalt", ",", "noch", "sch\u00f6\u00b7ner", "durch", "die", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Mein alter Freund, mein Schlaf, erscheine wieder!", "tokens": ["Mein", "al\u00b7ter", "Freund", ",", "mein", "Schlaf", ",", "er\u00b7schei\u00b7ne", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie w\u00fcnsch ich dich!", "tokens": ["Wie", "w\u00fcnsch", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du Sohn der Nacht, o breite dein Gefieder", "tokens": ["Du", "Sohn", "der", "Nacht", ",", "o", "brei\u00b7te", "dein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "FM", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch \u00fcber mich!", "tokens": ["Auch", "\u00fc\u00b7ber", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Verla\u00df daf\u00fcr den Wuchrer, ihn zu strafen,", "tokens": ["Ver\u00b7la\u00df", "da\u00b7f\u00fcr", "den", "Wuch\u00b7rer", ",", "ihn", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Trug erg\u00f6tzt:", "tokens": ["Den", "Trug", "er\u00b7g\u00f6tzt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hingegen la\u00df den wachen Codrus schlafen,", "tokens": ["Hin\u00b7ge\u00b7gen", "la\u00df", "den", "wa\u00b7chen", "Cod\u00b7rus", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Der immer reimt und immer \u00fcbersetzt.", "tokens": ["Der", "im\u00b7mer", "reimt", "und", "im\u00b7mer", "\u00fc\u00b7bers\u00b7etzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo bist du hin, du Tr\u00f6ster in Beschwerde,", "tokens": ["Wo", "bist", "du", "hin", ",", "du", "Tr\u00f6s\u00b7ter", "in", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein g\u00fcldner Schlaf?", "tokens": ["Mein", "g\u00fcld\u00b7ner", "Schlaf", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+