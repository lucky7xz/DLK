{"dta.poem.13903": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Der alte Held .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich habe geforscht und ergr\u00fcndet", "tokens": ["\u201e", "ich", "ha\u00b7be", "ge\u00b7forscht", "und", "er\u00b7gr\u00fcn\u00b7det"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und that es euch treulich kund:", "tokens": ["Und", "that", "es", "euch", "treu\u00b7lich", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was das Leben dunkel verk\u00fcndet,", "tokens": ["Was", "das", "Le\u00b7ben", "dun\u00b7kel", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die heilige Schrift, die entz\u00fcndet,", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Schrift", ",", "die", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Herr in der Seelen Grund.\u201c", "tokens": ["Der", "Herr", "in", "der", "See\u00b7len", "Grund", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}