{"dta.poem.9705": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal.  \n Einer verwundert sich \u00fcber  \u2012 \u2012  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wundre mich/ da\u00df hier die universit\u00e4t", "tokens": ["Ich", "wund\u00b7re", "mich", "/", "da\u00df", "hier", "die", "u\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In solchem flore steht.", "tokens": ["In", "sol\u00b7chem", "flo\u00b7re", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wundre mich/ da\u00df hier in allen st\u00e4nden", "tokens": ["Ich", "wund\u00b7re", "mich", "/", "da\u00df", "hier", "in", "al\u00b7len", "st\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ieglich ding recht ordentlich bestellt.", "tokens": ["Ein", "ieg\u00b7lich", "ding", "recht", "or\u00b7dent\u00b7lich", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und will ich mich nach sch\u00f6nen sachen wenden/", "tokens": ["Und", "will", "ich", "mich", "nach", "sch\u00f6\u00b7nen", "sa\u00b7chen", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So sind ich hier die sch\u00f6nsten von der welt.", "tokens": ["So", "sind", "ich", "hier", "die", "sch\u00f6ns\u00b7ten", "von", "der", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich will itzt nicht von sch\u00f6nen h\u00e4usern sagen/", "tokens": ["Ich", "will", "itzt", "nicht", "von", "sch\u00f6\u00b7nen", "h\u00e4u\u00b7sern", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Man sehe nur das frauenzimmer an/", "tokens": ["Man", "se\u00b7he", "nur", "das", "frau\u00b7en\u00b7zim\u00b7mer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So will ich ieden fragen/", "tokens": ["So", "will", "ich", "ie\u00b7den", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ob er sich wol genug verwundern kan?", "tokens": ["Ob", "er", "sich", "wol", "ge\u00b7nug", "ver\u00b7wun\u00b7dern", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Kurtz: alles ist zu \u2012 \u2012 wunderns werth.", "tokens": ["Kurtz", ":", "al\u00b7les", "ist", "zu", "\u2012", "\u2012", "wun\u00b7derns", "werth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIS", "VAFIN", "PTKZU", "$(", "$(", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch gibt mir di\u00df den gr\u00f6sten wunder ein/", "tokens": ["Doch", "gibt", "mir", "di\u00df", "den", "gr\u00f6s\u00b7ten", "wun\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df gleichwohl hier die schinder ehrlich seyn.", "tokens": ["Da\u00df", "gleich\u00b7wohl", "hier", "die", "schin\u00b7der", "ehr\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}