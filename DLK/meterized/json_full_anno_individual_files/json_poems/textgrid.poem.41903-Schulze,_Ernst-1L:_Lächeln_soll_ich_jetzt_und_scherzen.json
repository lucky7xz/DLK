{"textgrid.poem.41903": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4cheln soll ich jetzt und scherzen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4cheln soll ich jetzt und scherzen", "tokens": ["L\u00e4\u00b7cheln", "soll", "ich", "jetzt", "und", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verweintem Angesicht,", "tokens": ["Mit", "ver\u00b7wein\u00b7tem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll mit leichten Worten spielen,", "tokens": ["Soll", "mit", "leich\u00b7ten", "Wor\u00b7ten", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn von k\u00e4mpfenden Gef\u00fchlen,", "tokens": ["Wenn", "von", "k\u00e4mp\u00b7fen\u00b7den", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn von tiefverborgnen Schmerzen", "tokens": ["Wenn", "von", "tief\u00b7ver\u00b7borg\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laut im Herzen", "tokens": ["Laut", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Jeder rasche Puls mir spricht.", "tokens": ["Je\u00b7der", "ra\u00b7sche", "Puls", "mir", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebe, kannst du mir's vergeben?", "tokens": ["Lie\u00b7be", ",", "kannst", "du", "mir's", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ring' ich nicht mit deiner Macht?", "tokens": ["Ring'", "ich", "nicht", "mit", "dei\u00b7ner", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frevel ist's, dein heil'ges Feuer", "tokens": ["Fre\u00b7vel", "ist's", ",", "dein", "heil'\u00b7ges", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu umziehn mit dunkelm Schleyer.", "tokens": ["Zu", "um\u00b7ziehn", "mit", "dun\u00b7kelm", "Schle\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die G\u00f6tter herrschend schweben,", "tokens": ["Wo", "die", "G\u00f6t\u00b7ter", "herr\u00b7schend", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ihr Leben", "tokens": ["Will", "ihr", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Leuchten durch die ird'sche Nacht.", "tokens": ["Leuch\u00b7ten", "durch", "die", "ird'\u00b7sche", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenig r\u00fchrt's die stolzen M\u00e4chte,", "tokens": ["We\u00b7nig", "r\u00fchrt's", "die", "stol\u00b7zen", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sie Schmerz, ob Lust verleihn;", "tokens": ["Ob", "sie", "Schmerz", ",", "ob", "Lust", "ver\u00b7leihn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rastlos wollen sie sich regen,", "tokens": ["Rast\u00b7los", "wol\u00b7len", "sie", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die ew'ge Kraft bewegen,", "tokens": ["Nur", "die", "ew'\u00b7ge", "Kraft", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem sterblichen Geschlechte", "tokens": ["Und", "dem", "sterb\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll das Rechte,", "tokens": ["Soll", "das", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Was der Starke fordert, seyn.", "tokens": ["Was", "der", "Star\u00b7ke", "for\u00b7dert", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber wie von Blitzespfeilen", "tokens": ["A\u00b7ber", "wie", "von", "Blit\u00b7ze\u00b7spfei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell der Eiche Haupt entgl\u00fcht", "tokens": ["Hell", "der", "Ei\u00b7che", "Haupt", "ent\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Strahl, der sie entz\u00fcndet,", "tokens": ["Und", "dem", "Strahl", ",", "der", "sie", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt die eigne Kraft verb\u00fcndet", "tokens": ["Jetzt", "die", "eig\u00b7ne", "Kraft", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im wilden Sturmesheulen", "tokens": ["Und", "im", "wil\u00b7den", "Stur\u00b7mes\u00b7heu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flammens\u00e4ulen", "tokens": ["Flam\u00b7men\u00b7s\u00e4u\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihrem Feind entgegenspr\u00fcht:", "tokens": ["Ih\u00b7rem", "Feind", "ent\u00b7ge\u00b7gen\u00b7spr\u00fcht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So soll auch das Herz nicht zagen", "tokens": ["So", "soll", "auch", "das", "Herz", "nicht", "za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor der G\u00f6tter gl\u00fch'ndem Wehn,", "tokens": ["Vor", "der", "G\u00f6t\u00b7ter", "gl\u00fch'n\u00b7dem", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr Wandeln und ihr Walten", "tokens": ["Denn", "ihr", "Wan\u00b7deln", "und", "ihr", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird auch ihm die Kraft entfalten.", "tokens": ["Wird", "auch", "ihm", "die", "Kraft", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ihr m\u00e4cht'ges Nahn ertragen,", "tokens": ["Wer", "ihr", "m\u00e4cht'\u00b7ges", "Nahn", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Darf es wagen,", "tokens": ["Darf", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Selbst die Sieger zu bestehn.", "tokens": ["Selbst", "die", "Sie\u00b7ger", "zu", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wilde Gluth, ich will dich zwingen,", "tokens": ["Wil\u00b7de", "Gluth", ",", "ich", "will", "dich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will nur l\u00e4cheln, klagen nie,", "tokens": ["Will", "nur", "l\u00e4\u00b7cheln", ",", "kla\u00b7gen", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die S\u00fc\u00dfe, die ich liebe,", "tokens": ["Da\u00df", "die", "S\u00fc\u00b7\u00dfe", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht im Stillen sich betr\u00fcbe;", "tokens": ["Nicht", "im", "Stil\u00b7len", "sich", "be\u00b7tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag mein Herz im Kampfesringen", "tokens": ["Mag", "mein", "Herz", "im", "Kamp\u00b7fes\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch zerspringen;", "tokens": ["Auch", "zer\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Tod und Leben sind f\u00fcr sie!", "tokens": ["Tod", "und", "Le\u00b7ben", "sind", "f\u00fcr", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "L\u00e4cheln soll ich jetzt und scherzen", "tokens": ["L\u00e4\u00b7cheln", "soll", "ich", "jetzt", "und", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verweintem Angesicht,", "tokens": ["Mit", "ver\u00b7wein\u00b7tem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll mit leichten Worten spielen,", "tokens": ["Soll", "mit", "leich\u00b7ten", "Wor\u00b7ten", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn von k\u00e4mpfenden Gef\u00fchlen,", "tokens": ["Wenn", "von", "k\u00e4mp\u00b7fen\u00b7den", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn von tiefverborgnen Schmerzen", "tokens": ["Wenn", "von", "tief\u00b7ver\u00b7borg\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laut im Herzen", "tokens": ["Laut", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Jeder rasche Puls mir spricht.", "tokens": ["Je\u00b7der", "ra\u00b7sche", "Puls", "mir", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebe, kannst du mir's vergeben?", "tokens": ["Lie\u00b7be", ",", "kannst", "du", "mir's", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ring' ich nicht mit deiner Macht?", "tokens": ["Ring'", "ich", "nicht", "mit", "dei\u00b7ner", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frevel ist's, dein heil'ges Feuer", "tokens": ["Fre\u00b7vel", "ist's", ",", "dein", "heil'\u00b7ges", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu umziehn mit dunkelm Schleyer.", "tokens": ["Zu", "um\u00b7ziehn", "mit", "dun\u00b7kelm", "Schle\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die G\u00f6tter herrschend schweben,", "tokens": ["Wo", "die", "G\u00f6t\u00b7ter", "herr\u00b7schend", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ihr Leben", "tokens": ["Will", "ihr", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Leuchten durch die ird'sche Nacht.", "tokens": ["Leuch\u00b7ten", "durch", "die", "ird'\u00b7sche", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenig r\u00fchrt's die stolzen M\u00e4chte,", "tokens": ["We\u00b7nig", "r\u00fchrt's", "die", "stol\u00b7zen", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sie Schmerz, ob Lust verleihn;", "tokens": ["Ob", "sie", "Schmerz", ",", "ob", "Lust", "ver\u00b7leihn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rastlos wollen sie sich regen,", "tokens": ["Rast\u00b7los", "wol\u00b7len", "sie", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die ew'ge Kraft bewegen,", "tokens": ["Nur", "die", "ew'\u00b7ge", "Kraft", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem sterblichen Geschlechte", "tokens": ["Und", "dem", "sterb\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll das Rechte,", "tokens": ["Soll", "das", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Was der Starke fordert, seyn.", "tokens": ["Was", "der", "Star\u00b7ke", "for\u00b7dert", ",", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber wie von Blitzespfeilen", "tokens": ["A\u00b7ber", "wie", "von", "Blit\u00b7ze\u00b7spfei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hell der Eiche Haupt entgl\u00fcht", "tokens": ["Hell", "der", "Ei\u00b7che", "Haupt", "ent\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Strahl, der sie entz\u00fcndet,", "tokens": ["Und", "dem", "Strahl", ",", "der", "sie", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt die eigne Kraft verb\u00fcndet", "tokens": ["Jetzt", "die", "eig\u00b7ne", "Kraft", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im wilden Sturmesheulen", "tokens": ["Und", "im", "wil\u00b7den", "Stur\u00b7mes\u00b7heu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flammens\u00e4ulen", "tokens": ["Flam\u00b7men\u00b7s\u00e4u\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihrem Feind entgegenspr\u00fcht:", "tokens": ["Ih\u00b7rem", "Feind", "ent\u00b7ge\u00b7gen\u00b7spr\u00fcht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "So soll auch das Herz nicht zagen", "tokens": ["So", "soll", "auch", "das", "Herz", "nicht", "za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor der G\u00f6tter gl\u00fch'ndem Wehn,", "tokens": ["Vor", "der", "G\u00f6t\u00b7ter", "gl\u00fch'n\u00b7dem", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr Wandeln und ihr Walten", "tokens": ["Denn", "ihr", "Wan\u00b7deln", "und", "ihr", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird auch ihm die Kraft entfalten.", "tokens": ["Wird", "auch", "ihm", "die", "Kraft", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ihr m\u00e4cht'ges Nahn ertragen,", "tokens": ["Wer", "ihr", "m\u00e4cht'\u00b7ges", "Nahn", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Darf es wagen,", "tokens": ["Darf", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Selbst die Sieger zu bestehn.", "tokens": ["Selbst", "die", "Sie\u00b7ger", "zu", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wilde Gluth, ich will dich zwingen,", "tokens": ["Wil\u00b7de", "Gluth", ",", "ich", "will", "dich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will nur l\u00e4cheln, klagen nie,", "tokens": ["Will", "nur", "l\u00e4\u00b7cheln", ",", "kla\u00b7gen", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die S\u00fc\u00dfe, die ich liebe,", "tokens": ["Da\u00df", "die", "S\u00fc\u00b7\u00dfe", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht im Stillen sich betr\u00fcbe;", "tokens": ["Nicht", "im", "Stil\u00b7len", "sich", "be\u00b7tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag mein Herz im Kampfesringen", "tokens": ["Mag", "mein", "Herz", "im", "Kamp\u00b7fes\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch zerspringen;", "tokens": ["Auch", "zer\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Tod und Leben sind f\u00fcr sie!", "tokens": ["Tod", "und", "Le\u00b7ben", "sind", "f\u00fcr", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}