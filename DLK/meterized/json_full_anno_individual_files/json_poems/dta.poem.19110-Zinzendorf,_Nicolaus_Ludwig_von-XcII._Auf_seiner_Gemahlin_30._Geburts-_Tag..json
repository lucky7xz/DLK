{"dta.poem.19110": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XcII.   Auf seiner Gemahlin 30. Geburts-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebe Fran! ich bitte dich um des ewgen Felsens willen,", "tokens": ["Lje\u00b7be", "Fran", "!", "ich", "bit\u00b7te", "dich", "um", "des", "ew\u00b7gen", "Fel\u00b7sens", "wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Draus wir beyderseits gehauen, wo auch unser Steinritz ist:", "tokens": ["Draus", "wir", "bey\u00b7der\u00b7seits", "ge\u00b7hau\u00b7en", ",", "wo", "auch", "un\u00b7ser", "Stein\u00b7ritz", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "La\u00dft uns in Verbundenheit in die Gnaden-Fl\u00fcgel h\u00fcllen,", "tokens": ["La\u00dft", "uns", "in", "Ver\u00b7bun\u00b7den\u00b7heit", "in", "die", "Gna\u00b7den\u00b7F\u00b7l\u00fc\u00b7gel", "h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Unser Haupt, das uns regieret, unser Mann ist JEsus Christ.", "tokens": ["Un\u00b7ser", "Haupt", ",", "das", "uns", "re\u00b7gie\u00b7ret", ",", "un\u00b7ser", "Mann", "ist", "Je\u00b7sus", "Christ", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "NE", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Drey\u00dfig Jahre hat er dich auf den H\u00e4nden hingetragen,", "tokens": ["Drey\u00b7\u00dfig", "Jah\u00b7re", "hat", "er", "dich", "auf", "den", "H\u00e4n\u00b7den", "hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und nicht ", "tokens": ["Und", "nicht"], "token_info": ["word", "word"], "pos": ["KON", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Manche Ungem\u00e4chlichkeit zeigte sich in unsern Tagen,", "tokens": ["Man\u00b7che", "Un\u00b7ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "zeig\u00b7te", "sich", "in", "un\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Aber, wie sie sich gezeiget, eben so ist sie verraucht.", "tokens": ["A\u00b7ber", ",", "wie", "sie", "sich", "ge\u00b7zei\u00b7get", ",", "e\u00b7ben", "so", "ist", "sie", "ver\u00b7raucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Meine Schwester, liebe Frau! JEsus hat es uns vor \u00fcbel,", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", ",", "lie\u00b7be", "Frau", "!", "Je\u00b7sus", "hat", "es", "uns", "vor", "\u00fc\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "NE", "VAFIN", "PPER", "PRF", "APPR", "ADJD", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Wenn wir nicht mit gantzer Seele und mit aller unsrer Kraft,", "tokens": ["Wenn", "wir", "nicht", "mit", "gant\u00b7zer", "See\u00b7le", "und", "mit", "al\u00b7ler", "uns\u00b7rer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "KON", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Uns in seine Liebe ziehn: unsre Regel sey die Bibel,", "tokens": ["Uns", "in", "sei\u00b7ne", "Lie\u00b7be", "ziehn", ":", "uns\u00b7re", "Re\u00b7gel", "sey", "die", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Unser Dollmetsch sey der Wille, unsre Kraft seins Lebens Saft.", "tokens": ["Un\u00b7ser", "Doll\u00b7metsch", "sey", "der", "Wil\u00b7le", ",", "uns\u00b7re", "Kraft", "seins", "Le\u00b7bens", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "H\u00fcter des Vollendungs-Saals, wo so viele Braut-Gem\u00e4cher,", "tokens": ["H\u00fc\u00b7ter", "des", "Voll\u00b7en\u00b7dungs\u00b7Saals", ",", "wo", "so", "vie\u00b7le", "Braut\u00b7Ge\u00b7m\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "ADV", "PIAT", "NN", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.14": {"text": "Drinnen sich die Seelen schm\u00fccken, nimm dich meiner Schwe-", "tokens": ["Drin\u00b7nen", "sich", "die", "See\u00b7len", "schm\u00fc\u00b7cken", ",", "nimm", "dich", "mei\u00b7ner", "Schwe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ART", "NN", "VVINF", "$,", "VVIMP", "PPER", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.15": {"text": "Ich will Mardachai seyn, sey du Werber und Versprecher,", "tokens": ["Ich", "will", "Mar\u00b7dac\u00b7hai", "seyn", ",", "sey", "du", "Wer\u00b7ber", "und", "Ver\u00b7spre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "VAINF", "$,", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Und der grosse Sohn des K\u00f6nigs sey der Esther Ehe-Mann!", "tokens": ["Und", "der", "gros\u00b7se", "Sohn", "des", "K\u00f6\u00b7nigs", "sey", "der", "E\u00b7sther", "E\u00b7he\u00b7Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}