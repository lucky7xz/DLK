{"dta.poem.15906": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "111.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr vielleicht vermi\u00dft in diesem Buch die Einheit,", "tokens": ["Wenn", "ihr", "viel\u00b7leicht", "ver\u00b7mi\u00dft", "in", "die\u00b7sem", "Buch", "die", "Ein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt gro\u00dfes Ganzen seht der Einzelheiten Kleinheit;", "tokens": ["Statt", "gro\u00b7\u00dfes", "Gan\u00b7zen", "seht", "der", "Ein\u00b7zel\u00b7hei\u00b7ten", "Klein\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch eine Einheit ist, und doppelte, darinn:", "tokens": ["Doch", "ei\u00b7ne", "Ein\u00b7heit", "ist", ",", "und", "dop\u00b7pel\u00b7te", ",", "da\u00b7rinn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "KON", "ADJA", "$,", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Einheit in der Form, die Einheit auch im Sinn.", "tokens": ["Die", "Ein\u00b7heit", "in", "der", "Form", ",", "die", "Ein\u00b7heit", "auch", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf wieviel Stoff nun angewandt die Einheit sei,", "tokens": ["Auf", "wie\u00b7viel", "Stoff", "nun", "an\u00b7ge\u00b7wandt", "die", "Ein\u00b7heit", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "VVPP", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das lenkt der Zufall, und ist wirklich einerlei.", "tokens": ["Das", "lenkt", "der", "Zu\u00b7fall", ",", "und", "ist", "wirk\u00b7lich", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}