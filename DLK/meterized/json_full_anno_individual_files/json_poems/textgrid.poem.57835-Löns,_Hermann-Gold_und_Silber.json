{"textgrid.poem.57835": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Gold und Silber", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Sonne, liebe Sonne,", "tokens": ["Ach", "Son\u00b7ne", ",", "lie\u00b7be", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hast du in dem Sinn?", "tokens": ["Was", "hast", "du", "in", "dem", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich stehe an dem Fenster", "tokens": ["Ich", "ste\u00b7he", "an", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weine vor mich hin.", "tokens": ["Und", "wei\u00b7ne", "vor", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Ringelein von Silber,", "tokens": ["Ein", "Rin\u00b7ge\u00b7lein", "von", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das gab er mir zum Pfand;", "tokens": ["Das", "gab", "er", "mir", "zum", "Pfand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ring von rotem Golde", "tokens": ["Ein", "Ring", "von", "ro\u00b7tem", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den trag ich an der Hand.", "tokens": ["Den", "trag", "ich", "an", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Myrtenstock am Fenster", "tokens": ["Der", "Myr\u00b7ten\u00b7stock", "am", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der dauert mich so sehr;", "tokens": ["Der", "dau\u00b7ert", "mich", "so", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seine Zweige sind gefallen,", "tokens": ["Sei\u00b7ne", "Zwei\u00b7ge", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun ist er kahl und leer.", "tokens": ["Nun", "ist", "er", "kahl", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der eine kriegt das Silber,", "tokens": ["Der", "ei\u00b7ne", "kriegt", "das", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Gold der andre hat;", "tokens": ["Das", "Gold", "der", "and\u00b7re", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn alle Leute schlafen,", "tokens": ["Wenn", "al\u00b7le", "Leu\u00b7te", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann komm und k\u00fcss' dich satt.", "tokens": ["Dann", "komm", "und", "k\u00fcss'", "dich", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die gr\u00fcnen Myrtenzweige,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Myr\u00b7ten\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sind das allerbest;", "tokens": ["Die", "sind", "das", "al\u00b7ler\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDS", "VVFIN", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "Du sollst das Kr\u00e4nzlein haben,", "tokens": ["Du", "sollst", "das", "Kr\u00e4nz\u00b7lein", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der andre kriegt den Rest.", "tokens": ["Der", "and\u00b7re", "kriegt", "den", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach Sonne, liebe Sonne,", "tokens": ["Ach", "Son\u00b7ne", ",", "lie\u00b7be", "Son\u00b7ne