{"textgrid.poem.61299": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Wald sind keine Vogelch\u00f6re,", "tokens": ["In", "mei\u00b7nem", "Wald", "sind", "kei\u00b7ne", "Vo\u00b7gelc\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da nur verlorne Schimmer drinnen wanken;", "tokens": ["Da", "nur", "ver\u00b7lor\u00b7ne", "Schim\u00b7mer", "drin\u00b7nen", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Stamm zu Stamme wuchern dichte Ranken,", "tokens": ["Von", "Stamm", "zu", "Stam\u00b7me", "wu\u00b7chern", "dich\u00b7te", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und d\u00fcster schatten dr\u00fcber Buch' und F\u00f6hre.", "tokens": ["Und", "d\u00fcs\u00b7ter", "schat\u00b7ten", "dr\u00fc\u00b7ber", "Buch'", "und", "F\u00f6h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum ruft ein Hirsch, da\u00df er das Schweigen st\u00f6re,", "tokens": ["Kaum", "ruft", "ein", "Hirsch", ",", "da\u00df", "er", "das", "Schwei\u00b7gen", "st\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaum rauscht ein welkes Blatt im Niederschwanken;", "tokens": ["Kaum", "rauscht", "ein", "wel\u00b7kes", "Blatt", "im", "Nie\u00b7der\u00b7schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So stille wird es, da\u00df ich die Gedanken", "tokens": ["So", "stil\u00b7le", "wird", "es", ",", "da\u00df", "ich", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meiner eignen Seele wandeln h\u00f6re.", "tokens": ["In", "mei\u00b7ner", "eig\u00b7nen", "See\u00b7le", "wan\u00b7deln", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da will ein Schauder oft ins Herz mir gleiten", "tokens": ["Da", "will", "ein", "Schau\u00b7der", "oft", "ins", "Herz", "mir", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit leisem Frost, als st\u00fcnd' ich an den T\u00fcren,", "tokens": ["Mit", "lei\u00b7sem", "Frost", ",", "als", "st\u00fcnd'", "ich", "an", "den", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den eh'rnen, die ins Reich der Wunder gleiten.", "tokens": ["Den", "eh'r\u00b7nen", ",", "die", "ins", "Reich", "der", "Wun\u00b7der", "glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir ist's \u2013 beginnt sich's dann im Laub zu r\u00fchren \u2013", "tokens": ["Mir", "ist's", "\u2013", "be\u00b7ginnt", "sich's", "dann", "im", "Laub", "zu", "r\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "VVFIN", "PIS", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es m\u00fcss' hervor Virgil, der Hohe, schreiten,", "tokens": ["Es", "m\u00fcss'", "her\u00b7vor", "Vir\u00b7gil", ",", "der", "Ho\u00b7he", ",", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NE", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch H\u00f6lle mich und Paradies zu f\u00fchren.", "tokens": ["Durch", "H\u00f6l\u00b7le", "mich", "und", "Pa\u00b7ra\u00b7dies", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In meinem Wald sind keine Vogelch\u00f6re,", "tokens": ["In", "mei\u00b7nem"