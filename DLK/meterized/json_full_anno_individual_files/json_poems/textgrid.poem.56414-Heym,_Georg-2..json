{"textgrid.poem.56414": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll brauner Zelte liegt der ganze Strand", "tokens": ["Voll", "brau\u00b7ner", "Zel\u00b7te", "liegt", "der", "gan\u00b7ze", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Heuschrecken gleich, die auf die Felder fielen.", "tokens": ["Heu\u00b7schre\u00b7cken", "gleich", ",", "die", "auf", "die", "Fel\u00b7der", "fie\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und tausend Schiffe mit den schwarzen Kielen", "tokens": ["Und", "tau\u00b7send", "Schif\u00b7fe", "mit", "den", "schwar\u00b7zen", "Kie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stehn, hochgezogen auf den Ufersand.", "tokens": ["Stehn", ",", "hoch\u00b7ge\u00b7zo\u00b7gen", "auf", "den", "U\u00b7fer\u00b7sand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie sehn der Griechenpanzer Sonnenbrand:", "tokens": ["Sie", "sehn", "der", "Grie\u00b7chen\u00b7pan\u00b7zer", "Son\u00b7nen\u00b7brand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."]