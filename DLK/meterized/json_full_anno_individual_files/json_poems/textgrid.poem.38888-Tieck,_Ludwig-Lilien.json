{"textgrid.poem.38888": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lilien", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wende dich zu unsern wei\u00dfen Sternen,", "tokens": ["Wen\u00b7de", "dich", "zu", "un\u00b7sern", "wei\u00b7\u00dfen", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mondschein sind sie in der Sonne,", "tokens": ["Mond\u00b7schein", "sind", "sie", "in", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahndung unbekannter Wonne,", "tokens": ["Ahn\u00b7dung", "un\u00b7be\u00b7kann\u00b7ter", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freud' und Leid, doch in der Ferne,", "tokens": ["Freud'", "und", "Leid", ",", "doch", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur Erinn'rung, man hegt sie gerne.", "tokens": ["Nur", "Er\u00b7inn'\u00b7rung", ",", "man", "hegt", "sie", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Unser Lieben, unser Dichten,", "tokens": ["Un\u