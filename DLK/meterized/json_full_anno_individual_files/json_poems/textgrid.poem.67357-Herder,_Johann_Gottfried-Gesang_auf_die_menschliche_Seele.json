{"textgrid.poem.67357": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Gesang auf die menschliche Seele", "genre": "verse", "period": "N.A.", "pub_year": 1768, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Statt Luft- und Himmelswesen", "tokens": ["Statt", "Luft", "und", "Him\u00b7mels\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Preis' ich die Menschenseele,", "tokens": ["Preis'", "ich", "die", "Men\u00b7schen\u00b7see\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sch\u00f6pferin! Erlesen", "tokens": ["Die", "Sch\u00f6p\u00b7fe\u00b7rin", "!", "Er\u00b7le\u00b7sen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat sie in dunkler Zauberh\u00f6hle", "tokens": ["Hat", "sie", "in", "dunk\u00b7ler", "Zau\u00b7ber\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich einen Erdenleib und ward,", "tokens": ["Sich", "ei\u00b7nen", "Er\u00b7den\u00b7leib", "und", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sch\u00f6pferin, mit regen Kr\u00e4ften", "tokens": ["Die", "Sch\u00f6p\u00b7fe\u00b7rin", ",", "mit", "re\u00b7gen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Der G\u00f6tter offenbart.", "tokens": ["Der", "G\u00f6t\u00b7ter", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nimm, was die Menschenseele", "tokens": ["Nimm", ",", "was", "die", "Men\u00b7schen\u00b7see\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "F\u00fcr neuen Sinn ersonnen,", "tokens": ["F\u00fcr", "neu\u00b7en", "Sinn", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fleuch aus Deiner H\u00f6hle,", "tokens": ["Und", "fleuch", "aus", "Dei\u00b7ner", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu wandeln unter Stern- und Sonnen,", "tokens": ["Zu", "wan\u00b7deln", "un\u00b7ter", "Stern", "und", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kehr hinab und sieh und sprich,", "tokens": ["Und", "kehr", "hin\u00b7ab", "und", "sieh", "und", "sprich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVIMP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was gr\u00f6\u00dfer Du gesehen,", "tokens": ["Was", "gr\u00f6\u00b7\u00dfer", "Du", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Sonnen oder Dich.", "tokens": ["Die", "Son\u00b7nen", "o\u00b7der", "Dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was Dir ein Erdball d\u00fcnket,", "tokens": ["Was", "Dir", "ein", "Erd\u00b7ball", "d\u00fcn\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Leib, den Du ern\u00e4hrest,", "tokens": ["Der", "Leib", ",", "den", "Du", "er\u00b7n\u00e4h\u00b7rest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der todt zu Staube sinket,", "tokens": ["Der", "todt", "zu", "Stau\u00b7be", "sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn Du in neue Reiche kehrest \u2013", "tokens": ["Wenn", "Du", "in", "neu\u00b7e", "Rei\u00b7che", "keh\u00b7rest", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist eine Welt, von ihr vereint,", "tokens": ["Ist", "ei\u00b7ne", "Welt", ",", "von", "ihr", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gewebt und kunstgebildet,", "tokens": ["Ge\u00b7webt", "und", "kunst\u00b7ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ob's Dir ein Erdklo\u00df scheint.", "tokens": ["Ob's", "Dir", "ein", "Erd\u00b7klo\u00df", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wer ist's, der sie z\u00e4hle,", "tokens": ["Und", "wer", "ist's", ",", "der", "sie", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Erdbau noch durchstrebend,", "tokens": ["Den", "Erd\u00b7bau", "noch", "durch\u00b7stre\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kr\u00e4fte Deiner Seele?", "tokens": ["Die", "Kr\u00e4f\u00b7te", "Dei\u00b7ner", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allgegenw\u00e4rtig, allbelebend,", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ",", "all\u00b7be\u00b7le\u00b7bend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Als eine Sonn', als K\u00f6nigin", "tokens": ["Als", "ei\u00b7ne", "Sonn'", ",", "als", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herrscht sie und f\u00fchlt und langet", "tokens": ["Herrscht", "sie", "und", "f\u00fchlt", "und", "lan\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ans Weltenende hin.", "tokens": ["Ans", "Wel\u00b7te\u00b7nen\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jetzt, wenn sie g\u00f6ttlich f\u00fchlet", "tokens": ["Jetzt", ",", "wenn", "sie", "g\u00f6tt\u00b7lich", "f\u00fch\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des hohen Ursprungs Feier,", "tokens": ["Des", "ho\u00b7hen", "Ur\u00b7sprungs", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu hohem Ziele zielet,", "tokens": ["Zu", "ho\u00b7hem", "Zie\u00b7le", "zie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie K\u00f6nig Adler; jetzt als Ungeheuer", "tokens": ["Wie", "K\u00f6\u00b7nig", "Ad\u00b7ler", ";", "jetzt", "als", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "$.", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Entflammet, wo sie Zug und Wuth", "tokens": ["Ent\u00b7flam\u00b7met", ",", "wo", "sie", "Zug", "und", "Wuth"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Raubbegier hinabrei\u00dft,", "tokens": ["Und", "Raub\u00b7be\u00b7gier", "hi\u00b7na\u00b7brei\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Gleich einem Pfeil, nach Blut \u2013", "tokens": ["Gleich", "ei\u00b7nem", "Pfeil", ",", "nach", "Blut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie da Blutstr\u00f6me wallen", "tokens": ["Wie", "da", "Blut\u00b7str\u00f6\u00b7me", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NN", "VVINF"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.2": {"text": "Von Herzen und zu Herzen!", "tokens": ["Von", "Her\u00b7zen", "und", "zu", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in den Str\u00f6men allen", "tokens": ["Und", "in", "den", "Str\u00f6\u00b7men", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wallt Leben, wallen Wonn' und Schmerzen,", "tokens": ["Wallt", "Le\u00b7ben", ",", "wal\u00b7len", "Wonn'", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Kugeln, unerforscht und viel,", "tokens": ["In", "Ku\u00b7geln", ",", "un\u00b7er\u00b7forscht", "und", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Welt- und Erdenb\u00e4lle,", "tokens": ["Wie", "Welt", "und", "Er\u00b7den\u00b7b\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer, der sie z\u00e4hlen will?", "tokens": ["Wer", ",", "der", "sie", "z\u00e4h\u00b7len", "will", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sieh, mit den Lebensb\u00e4llen", "tokens": ["Sieh", ",", "mit", "den", "Le\u00b7bens\u00b7b\u00e4l\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Gehn Deiner Seele Bilder.", "tokens": ["Gehn", "Dei\u00b7ner", "See\u00b7le", "Bil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt, wie aus Abgrundsquellen", "tokens": ["Jetzt", ",", "wie", "aus", "Ab\u00b7grunds\u00b7quel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Weltenwirbel, jetzt und milder", "tokens": ["Die", "Wel\u00b7ten\u00b7wir\u00b7bel", ",", "jetzt", "und", "mil\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schmeichelnd sanft, wie sich die Thr\u00e4n'", "tokens": ["Und", "schmei\u00b7chelnd", "sanft", ",", "wie", "sich", "die", "Thr\u00e4n'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Blick der Liebe sammelt", "tokens": ["Im", "Blick", "der", "Lie\u00b7be", "sam\u00b7melt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und abrinnt sterbend sch\u00f6n.", "tokens": ["Und", "ab\u00b7rinnt", "ster\u00b7bend", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gro\u00dfe Gedanken streben", "tokens": ["Gro\u00b7\u00dfe", "Ge\u00b7dan\u00b7ken", "stre\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da auf wie Flammenmeere!", "tokens": ["Da", "auf", "wie", "Flam\u00b7men\u00b7mee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf ihren Schilden heben", "tokens": ["Auf", "ih\u00b7ren", "Schil\u00b7den", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie Dich empor, Gedankenheere!", "tokens": ["Sie", "Dich", "em\u00b7por", ",", "Ge\u00b7dan\u00b7ken\u00b7hee\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ist, der denkend je umfing", "tokens": ["Wer", "ist", ",", "der", "den\u00b7kend", "je", "um\u00b7fing"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Weg, den Menschenseele", "tokens": ["Den", "Weg", ",", "den", "Men\u00b7schen\u00b7see\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zum Thron der Gottheit ging?", "tokens": ["Zum", "Thron", "der", "Got\u00b7theit", "ging", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wo, Lorbeern statt, sie Kreise", "tokens": ["Wo", ",", "Lor\u00b7beern", "statt", ",", "sie", "Krei\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "NN", "PTKVZ", "$,", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hellen Stern' umgl\u00e4nzen,", "tokens": ["Der", "hel\u00b7len", "Stern'", "um\u00b7gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesetz und Kraft und Lauf im ew'gen Gleise,", "tokens": ["Ge\u00b7setz", "und", "Kraft", "und", "Lauf", "im", "ew'\u00b7gen", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Boten ihres Sinns, die Welt umgrenzen,", "tokens": ["Die", "Bo\u00b7ten", "ih\u00b7res", "Sinns", ",", "die", "Welt", "um\u00b7gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"