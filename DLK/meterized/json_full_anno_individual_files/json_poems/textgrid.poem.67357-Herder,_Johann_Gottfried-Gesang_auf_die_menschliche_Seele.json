 "tokens": ["Auf", "ih\u00b7ren", "Schil\u00b7den", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie Dich empor, Gedankenheere!", "tokens": ["Sie", "Dich", "em\u00b7por", ",", "Ge\u00b7dan\u00b7ken\u00b7hee\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ist, der denkend je umfing", "tokens": ["Wer", "ist", ",", "der", "den\u00b7kend", "je", "um\u00b7fing"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Weg, den Menschenseele", "tokens": ["Den", "Weg", ",", "den", "Men\u00b7schen\u00b7see\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zum Thron der Gottheit ging?", "tokens": ["Zum", "Thron", "der", "Got\u00b7theit", "ging", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Wo, Lorbeern statt, sie Kreise", "tokens": ["Wo", ",", "Lor\u00b7beern", "statt", ",", "sie", "Krei\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "NN", "PTKVZ", "$,", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hellen Stern' umgl\u00e4nzen,", "tokens": ["Der", "hel\u00b7len", "Stern'", "um\u00b7gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"