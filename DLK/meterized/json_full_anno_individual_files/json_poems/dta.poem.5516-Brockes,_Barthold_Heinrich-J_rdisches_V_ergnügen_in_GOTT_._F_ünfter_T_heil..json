{"dta.poem.5516": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des alle Dinge f\u00e4rbenden und selbst so bunt-gef\u00e4rbten", "tokens": ["Des", "al\u00b7le", "Din\u00b7ge", "f\u00e4r\u00b7ben\u00b7den", "und", "selbst", "so", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF", "KON", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Lentzen,", "tokens": ["Lent\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In mehr als hundert tausend Farben, zumahl in Gr\u00fcnen,", "tokens": ["In", "mehr", "als", "hun\u00b7dert", "tau\u00b7send", "Far\u00b7ben", ",", "zu\u00b7mahl", "in", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "CARD", "CARD", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "sehe gl\u00e4ntzen;", "tokens": ["se\u00b7he", "gl\u00e4nt\u00b7zen", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "So seh ich so viel Wunder-Dinge, aus allen Orten jetzt", "tokens": ["So", "seh", "ich", "so", "viel", "Wun\u00b7der\u00b7Din\u00b7ge", ",", "aus", "al\u00b7len", "Or\u00b7ten", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "entspriessen,", "tokens": ["ent\u00b7spries\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "(da recht ein w\u00fcrcklich Wunder-Heer, aus Wasser, Luft", "tokens": ["(", "da", "recht", "ein", "w\u00fcrck\u00b7lich", "Wun\u00b7der\u00b7Heer", ",", "aus", "Was\u00b7ser", ",", "Luft"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "ADV", "ART", "ADJD", "NN", "$,", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und Erde qvillt)", "tokens": ["und", "Er\u00b7de", "qvillt", ")"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Das ihre mannigfache Zier und Pracht mein gantzes", "tokens": ["Das", "ih\u00b7re", "man\u00b7nig\u00b7fa\u00b7che", "Zier", "und", "Pracht", "mein", "gant\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "KON", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wesen f\u00fcllt.", "tokens": ["We\u00b7sen", "f\u00fcllt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ich f\u00fchle mein gef\u00fclltes Hertz von Anmuth gleichsam", "tokens": ["Ich", "f\u00fch\u00b7le", "mein", "ge\u00b7f\u00fcll\u00b7tes", "Hertz", "von", "An\u00b7muth", "gleich\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u00fcberfliessen.", "tokens": ["\u00fc\u00b7berf\u00b7lies\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Dabey f\u00e4llt, mitten in der Lust, der wohl gemeinte Wunsch", "tokens": ["Da\u00b7bey", "f\u00e4llt", ",", "mit\u00b7ten", "in", "der", "Lust", ",", "der", "wohl", "ge\u00b7mein\u00b7te", "Wunsch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "mir ein:", "tokens": ["mir", "ein", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Ach, w\u00e4r bey allgemeiner Sch\u00f6nheit der Welt, die Lust auch", "tokens": ["Ach", ",", "w\u00e4r", "bey", "all\u00b7ge\u00b7mei\u00b7ner", "Sch\u00f6n\u00b7heit", "der", "Welt", ",", "die", "Lust", "auch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "allgemein!", "tokens": ["all\u00b7ge\u00b7mein", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Ich wiederhohle denn zum Schlu\u00df, bey meiner oft gef\u00fchr-", "tokens": ["Ich", "wie\u00b7der\u00b7hoh\u00b7le", "denn", "zum", "Schlu\u00df", ",", "bey", "mei\u00b7ner", "oft", "ge\u00b7f\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,", "APPR", "PPOSAT", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "ten Klage", "tokens": ["ten", "Kla\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Ob unsrer Unempfindlichkeit, die euch vielleicht verhasste", "tokens": ["Ob", "uns\u00b7rer", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", ",", "die", "euch", "viel\u00b7leicht", "ver\u00b7hass\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "Frage:", "tokens": ["Fra\u00b7ge", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "\u201eda die Natur an tausend Orten, wie ihr es allenthalben", "tokens": ["\u201e", "da", "die", "Na\u00b7tur", "an", "tau\u00b7send", "Or\u00b7ten", ",", "wie", "ihr", "es", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "APPR", "CARD", "NN", "$,", "PWAV", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "h\u00f6ret,", "tokens": ["h\u00f6\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "\u201ezugleich auch allenthalben seht, da\u00df in der Wercke Schmuck", "tokens": ["\u201e", "zu\u00b7gleich", "auch", "al\u00b7len\u00b7thal\u00b7ben", "seht", ",", "da\u00df", "in", "der", "Wer\u00b7cke", "Schmuck"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "ADV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "und Schein,", "tokens": ["und", "Schein", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "\u201esie unsern Sch\u00f6pfer deutlich zeigt, auch da\u00df, ja was er ist,", "tokens": ["\u201e", "sie", "un\u00b7sern", "Sch\u00f6p\u00b7fer", "deut\u00b7lich", "zeigt", ",", "auch", "da\u00df", ",", "ja", "was", "er", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "ADV", "KOUS", "$,", "ADV", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "uns lehrt;", "tokens": ["uns", "lehrt", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "\u201ewie kann man blind bey ihrer Sch\u00f6nheit, und taub bey ihrer", "tokens": ["\u201e", "wie", "kann", "man", "blind", "bey", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", ",", "und", "taub", "bey", "ih\u00b7rer"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "$,", "KON", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Lehre seyn?", "tokens": ["Leh\u00b7re", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "EnDE.", "tokens": ["En\u00b7DE", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}}}}}