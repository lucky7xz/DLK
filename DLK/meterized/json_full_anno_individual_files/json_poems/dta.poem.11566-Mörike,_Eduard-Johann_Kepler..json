{"dta.poem.11566": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Johann Kepler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Gestern, als ich vom n\u00e4chtlichen Lager den Stern mir", "tokens": ["Ge\u00b7stern", ",", "als", "ich", "vom", "n\u00e4cht\u00b7li\u00b7chen", "La\u00b7ger", "den", "Stern", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "PPER"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Lang' betrachtete, den dort mit dem r\u00f6thlichen Licht,", "tokens": ["Lang'", "be\u00b7trach\u00b7te\u00b7te", ",", "den", "dort", "mit", "dem", "r\u00f6th\u00b7li\u00b7chen", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und des Mannes gedachte, der seine Bahnen zu messen,", "tokens": ["Und", "des", "Man\u00b7nes", "ge\u00b7dach\u00b7te", ",", "der", "sei\u00b7ne", "Bah\u00b7nen", "zu", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Von dem Gotte gereizt, himmlischer Pflicht sich ergab,", "tokens": ["Von", "dem", "Got\u00b7te", "ge\u00b7reizt", ",", "himm\u00b7li\u00b7scher", "Pflicht", "sich", "er\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Durch beharrlichen Flei\u00df der Armuth grimmigen Stachel", "tokens": ["Durch", "be\u00b7harr\u00b7li\u00b7chen", "Flei\u00df", "der", "Ar\u00b7muth", "grim\u00b7mi\u00b7gen", "Sta\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Zu vers\u00f6hnen, umsonst, und zu verachten bem\u00fcht:", "tokens": ["Zu", "ver\u00b7s\u00f6h\u00b7nen", ",", "um\u00b7sonst", ",", "und", "zu", "ver\u00b7ach\u00b7ten", "be\u00b7m\u00fcht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "$,", "KON", "PTKZU", "VVFIN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Mir entbrannte mein Herz von inniger Wehmuth; ach!", "tokens": ["Mir", "ent\u00b7brann\u00b7te", "mein", "Herz", "von", "in\u00b7ni\u00b7ger", "Weh\u00b7muth", ";", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$.", "XY", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Wu\u00dften die Himmlischen dir, Meister, kein besseres", "tokens": ["Wu\u00df\u00b7ten", "die", "Himm\u00b7li\u00b7schen", "dir", ",", "Meis\u00b7ter", ",", "kein", "bes\u00b7se\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "NN", "$,", "PIAT", "ADJA"], "meter": "+--+-+-+-++--", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Wie ein Dichter den Helden sich w\u00e4hlt, wie Homer von", "tokens": ["Wie", "ein", "Dich\u00b7ter", "den", "Hel\u00b7den", "sich", "w\u00e4hlt", ",", "wie", "Ho\u00b7mer", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,", "PWAV", "NE", "APPR"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "G\u00f6ttlichem Adel ger\u00fchrt, sch\u00f6n im Gesang ihn erhob.", "tokens": ["G\u00f6tt\u00b7li\u00b7chem", "A\u00b7del", "ge\u00b7r\u00fchrt", ",", "sch\u00f6n", "im", "Ge\u00b7sang", "ihn", "er\u00b7hob", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Also wandtest du ganz die Kr\u00e4fte nach jenem Gestirne,", "tokens": ["Al\u00b7so", "wand\u00b7test", "du", "ganz", "die", "Kr\u00e4f\u00b7te", "nach", "je\u00b7nem", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Sein gewaltiger Gang war dir ein ewiges Lied.", "tokens": ["Sein", "ge\u00b7wal\u00b7ti\u00b7ger", "Gang", "war", "dir", "ein", "e\u00b7wi\u00b7ges", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Doch so bewegt sich kein Gott von seinem goldenen Sitze,", "tokens": ["Doch", "so", "be\u00b7wegt", "sich", "kein", "Gott", "von", "sei\u00b7nem", "gol\u00b7de\u00b7nen", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Holdem Gesange geneigt, den zu erretten, herab,", "tokens": ["Hol\u00b7dem", "Ge\u00b7san\u00b7ge", "ge\u00b7neigt", ",", "den", "zu", "er\u00b7ret\u00b7ten", ",", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "ART", "PTKZU", "VVINF", "$,", "ADV", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Dem die h\u00f6here Macht die dunkeln Tage bestimmt hat,", "tokens": ["Dem", "die", "h\u00f6\u00b7he\u00b7re", "Macht", "die", "dun\u00b7keln", "Ta\u00b7ge", "be\u00b7stimmt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Und euch Sterne ber\u00fchrt nimmer ein Menschengeschick;", "tokens": ["Und", "euch", "Ster\u00b7ne", "be\u00b7r\u00fchrt", "nim\u00b7mer", "ein", "Men\u00b7schen\u00b7ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Ihr geht \u00fcber dem Haupte des Weisen oder des Thoren", "tokens": ["Ihr", "geht", "\u00fc\u00b7ber", "dem", "Haup\u00b7te", "des", "Wei\u00b7sen", "o\u00b7der", "des", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Euern seligen Weg ewig gelassen dahin!", "tokens": ["Eu\u00b7ern", "se\u00b7li\u00b7gen", "Weg", "e\u00b7wig", "ge\u00b7las\u00b7sen", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "PAV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}