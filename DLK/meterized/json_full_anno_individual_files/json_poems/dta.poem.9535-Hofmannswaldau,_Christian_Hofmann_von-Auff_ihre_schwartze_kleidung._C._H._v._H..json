{"dta.poem.9535": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre schwartze kleidung.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe Lesbien umschlossen und umh\u00fcllt", "tokens": ["Ich", "sa\u00b7he", "Les\u00b7bi\u00b7en", "um\u00b7schlos\u00b7sen", "und", "um\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit kleidung/ welche selbst die traurigkeit erfunden/", "tokens": ["Mit", "klei\u00b7dung", "/", "wel\u00b7che", "selbst", "die", "trau\u00b7rig\u00b7keit", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es war ein tr\u00fcbes tuch ihr um den schlaff gebunden/", "tokens": ["Es", "war", "ein", "tr\u00fc\u00b7bes", "tuch", "ihr", "um", "den", "schlaff", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre stirne war mit wehmuth angef\u00fcllt:", "tokens": ["Und", "ih\u00b7re", "stir\u00b7ne", "war", "mit", "weh\u00b7muth", "an\u00b7ge\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch hat die traurigkeit das himmel-reine bild", "tokens": ["Doch", "hat", "die", "trau\u00b7rig\u00b7keit", "das", "him\u00b7mel\u00b7rei\u00b7ne", "bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier g\u00e4ntzlich abzuthun sich niemahls unterwunden.", "tokens": ["Hier", "g\u00e4ntz\u00b7lich", "ab\u00b7zu\u00b7thun", "sich", "nie\u00b7mahls", "un\u00b7ter\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der blitz/ so unvermerckt aus diesen n\u00e4chten quillt/", "tokens": ["Der", "blitz", "/", "so", "un\u00b7ver\u00b7merckt", "aus", "die\u00b7sen", "n\u00e4ch\u00b7ten", "quillt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "APPR", "PDAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vermehret meine brunst fast alle tag und sinnden.", "tokens": ["Ver\u00b7meh\u00b7ret", "mei\u00b7ne", "brunst", "fast", "al\u00b7le", "tag", "und", "sinn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es hat ja Venus selbst sich sch\u00f6ner nicht gezeiget/", "tokens": ["Es", "hat", "ja", "Ve\u00b7nus", "selbst", "sich", "sch\u00f6\u00b7ner", "nicht", "ge\u00b7zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADV", "PRF", "ADJD", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als da sie in dem schos des tr\u00fcben Aetnens sa\u00df", "tokens": ["Als", "da", "sie", "in", "dem", "schos", "des", "tr\u00fc\u00b7ben", "A\u00b7e\u00b7tnens", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Und mit der weissen hand die schwartzen kohlen la\u00df/", "tokens": ["Und", "mit", "der", "weis\u00b7sen", "hand", "die", "schwart\u00b7zen", "koh\u00b7len", "la\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man schaut wie da\u00df ein stern aus schwartze\u0303 wolcken steiget/", "tokens": ["Man", "schaut", "wie", "da\u00df", "ein", "stern", "aus", "schwartz\u1ebd", "wol\u00b7cken", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "KOUS", "ART", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ja selbst die sonne zeigt kein angenehmer licht/", "tokens": ["Ja", "selbst", "die", "son\u00b7ne", "zeigt", "kein", "an\u00b7ge\u00b7neh\u00b7mer", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als wenn sie unvermerckt durch tr\u00fcbe wolcken bricht.", "tokens": ["Als", "wenn", "sie", "un\u00b7ver\u00b7merckt", "durch", "tr\u00fc\u00b7be", "wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}