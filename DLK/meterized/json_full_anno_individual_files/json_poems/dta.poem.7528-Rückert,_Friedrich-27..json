{"dta.poem.7528": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungl\u00fccklich bist du nicht, wie unbegl\u00fcckt du seist;", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "bist", "du", "nicht", ",", "wie", "un\u00b7be\u00b7gl\u00fcckt", "du", "seist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Schicksal nur begl\u00fcckt, doch gl\u00fccklich macht der Geist.", "tokens": ["Das", "Schick\u00b7sal", "nur", "be\u00b7gl\u00fcckt", ",", "doch", "gl\u00fcck\u00b7lich", "macht", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denkst du, wie sch\u00f6n es w\u00e4r', ob du ein Gut gewannst;", "tokens": ["Denkst", "du", ",", "wie", "sch\u00f6n", "es", "w\u00e4r'", ",", "ob", "du", "ein", "Gut", "ge\u00b7wannst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk auch, noch sch\u00f6ner ists, da\u00df du's entbehren kannst.", "tokens": ["Denk", "auch", ",", "noch", "sch\u00f6\u00b7ner", "ists", ",", "da\u00df", "du's", "ent\u00b7beh\u00b7ren", "kannst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob auszutheilen du nicht Sch\u00e4tze hast im Haus,", "tokens": ["Ob", "aus\u00b7zu\u00b7thei\u00b7len", "du", "nicht", "Sch\u00e4t\u00b7ze", "hast", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVIZU", "PPER", "PTKNEG", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So theile, die du hast, die goldnen Lieder aus.", "tokens": ["So", "thei\u00b7le", ",", "die", "du", "hast", ",", "die", "gold\u00b7nen", "Lie\u00b7der", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "VAFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich gebe, was ich hab', und hab' um nur zu geben;", "tokens": ["Ich", "ge\u00b7be", ",", "was", "ich", "hab'", ",", "und", "hab'", "um", "nur", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VAFIN", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu geben samml' ich ein, dis Sammeln ist mein Leben.", "tokens": ["Zu", "ge\u00b7ben", "samml'", "ich", "ein", ",", "dis", "Sam\u00b7meln", "ist", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "PTKVZ", "$,", "PDS", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den K\u00f6nig wollt' ich sehn, der in Freigebigkeit", "tokens": ["Den", "K\u00f6\u00b7nig", "wollt'", "ich", "sehn", ",", "der", "in", "Frei\u00b7ge\u00b7big\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit mir wetteiferte! wer, F\u00fcrsten, wagt den Streit?", "tokens": ["Mit", "mir", "wett\u00b7ei\u00b7fer\u00b7te", "!", "wer", ",", "F\u00fcrs\u00b7ten", ",", "wagt", "den", "Streit", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "PWS", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-++-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Dazu aus Ost und West erheb' ich Geisteszehnten,", "tokens": ["Da\u00b7zu", "aus", "Ost", "und", "West", "er\u00b7heb'", "ich", "Geis\u00b7tes\u00b7zehn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu lohnen k\u00f6niglich all meinen Kronbelehnten.", "tokens": ["Zu", "loh\u00b7nen", "k\u00f6\u00b7nig\u00b7lich", "all", "mei\u00b7nen", "Kron\u00b7be\u00b7lehn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So zieht die Sonne wol das Wasser auf mit Stralen,", "tokens": ["So", "zieht", "die", "Son\u00b7ne", "wol", "das", "Was\u00b7ser", "auf", "mit", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibts der Welt zur\u00fcck in Regenbogenschalen.", "tokens": ["Und", "gibts", "der", "Welt", "zu\u00b7r\u00fcck", "in", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}