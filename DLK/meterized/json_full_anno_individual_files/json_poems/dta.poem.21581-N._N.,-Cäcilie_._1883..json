{"dta.poem.21581": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "C\u00e4cilie .  \n 1883.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Du es w\u00fc\u00dftest,", "tokens": ["Wenn", "Du", "es", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was tr\u00e4umen hei\u00dft", "tokens": ["Was", "tr\u00e4u\u00b7men", "hei\u00dft"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVINF", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von brennenden K\u00fcssen,", "tokens": ["Von", "bren\u00b7nen\u00b7den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vom Wandern und Ruhen", "tokens": ["Vom", "Wan\u00b7dern", "und", "Ru\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit der Geliebten,", "tokens": ["Mit", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Aug\u2019 in Auge", "tokens": ["Aug'", "in", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und kosend und plaudernd \u2014", "tokens": ["Und", "ko\u00b7send", "und", "plau\u00b7dernd"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wenn Du es w\u00fc\u00dftest,", "tokens": ["Wenn", "Du", "es", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Du neigtest Dein Herz.", "tokens": ["Du", "neig\u00b7test", "Dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wenn Du es w\u00fc\u00dftest,", "tokens": ["Wenn", "Du", "es", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was bangen hei\u00dft", "tokens": ["Was", "ban\u00b7gen", "hei\u00dft"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In einsamen N\u00e4chten,", "tokens": ["In", "ein\u00b7sa\u00b7men", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Umschauert vom Sturm,", "tokens": ["Um\u00b7schau\u00b7ert", "vom", "Sturm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da Niemand tr\u00f6stet", "tokens": ["Da", "Nie\u00b7mand", "tr\u00f6s\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Milden Mundes", "tokens": ["Mil\u00b7den", "Mun\u00b7des"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die kampfm\u00fcde Seele \u2014", "tokens": ["Die", "kampf\u00b7m\u00fc\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wenn Du es w\u00fc\u00dftest,", "tokens": ["Wenn", "Du", "es", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Du k\u00e4mest zu mir.", "tokens": ["Du", "k\u00e4\u00b7mest", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wenn Du es w\u00fc\u00dftest,", "tokens": ["Wenn", "Du", "es", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was leben hei\u00dft", "tokens": ["Was", "le\u00b7ben", "hei\u00dft"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVINF", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Umhaucht von der Gottheit", "tokens": ["Um\u00b7haucht", "von", "der", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weltschaffendem Athem,", "tokens": ["Welt\u00b7schaf\u00b7fen\u00b7dem", "A\u00b7them", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Zu schweben empor", "tokens": ["Zu", "schwe\u00b7ben", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["PTKZU", "VVINF", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Lichtgetragen", "tokens": ["Licht\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Zu seligen H\u00f6hen \u2014", "tokens": ["Zu", "se\u00b7li\u00b7gen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wenn Du es w\u00fc\u00dftest,", "tokens": ["Wenn", "Du", "es", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Du lebtest mit mir.", "tokens": ["Du", "leb\u00b7test", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}