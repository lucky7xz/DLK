{"textgrid.poem.58732": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine Frucht ist meiner Kehle s\u00fc\u00df. Hoheslied 2, 3", "tokens": ["Sei\u00b7ne", "Frucht", "ist", "mei\u00b7ner", "Keh\u00b7le", "s\u00fc\u00df", ".", "Ho\u00b7hes\u00b7lied", "2", ",", "3"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "So s\u00fc\u00dfe Liebe, pfropf dein Kreuz ins Herze ein,", "tokens": ["So", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", ",", "pfropf", "dein", "Kreuz", "ins", "Her\u00b7ze", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja, la\u00df es drin gewurzelt sein!", "tokens": ["Ja", ",", "la\u00df", "es", "drin", "ge\u00b7wur\u00b7zelt", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVIMP", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich w\u00e4hl' ", "tokens": ["Ich", "w\u00e4hl'"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Aus ihm viel Gottesgnaden keimen.", "tokens": ["Aus", "ihm", "viel", "Got\u00b7tes\u00b7gna\u00b7den", "kei\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die N\u00e4gel, Dornen, Schmerz' und Leiden, die ich find',", "tokens": ["Die", "N\u00e4\u00b7gel", ",", "Dor\u00b7nen", ",", "Schmer\u00b7z'", "und", "Lei\u00b7den", ",", "die", "ich", "find'", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Des Lebensbaumes Fr\u00fcchte sind;", "tokens": ["Des", "Le\u00b7bens\u00b7bau\u00b7mes", "Fr\u00fcch\u00b7te", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schmecken sie nur deinen Lieben,", "tokens": ["Doch", "schme\u00b7cken", "sie", "nur", "dei\u00b7nen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihre Seel' dir ganz verschrieben.", "tokens": ["Die", "ih\u00b7re", "Seel'", "dir", "ganz", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du, Gottesliebe, machst, da\u00df ihnen schmeckt ihr Leid", "tokens": ["Du", ",", "Got\u00b7tes\u00b7lie\u00b7be", ",", "machst", ",", "da\u00df", "ih\u00b7nen", "schmeckt", "ihr", "Leid"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "mete