{"textgrid.poem.55161": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Anakreons Grab", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Rose hier bl\u00fcht, wo Reben um Lorbeer sich schlingen,", "tokens": ["Wo", "die", "Ro\u00b7se", "hier", "bl\u00fcht", ",", "wo", "Re\u00b7ben", "um", "Lor\u00b7beer", "sich", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,", "PWAV", "NN", "APPR", "NE", "PRF", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo das Turtelchen lockt, wo sich das Grillchen ergetzt,", "tokens": ["Wo", "das", "Tur\u00b7tel\u00b7chen", "lockt", ",", "wo", "sich", "das", "Grill\u00b7chen", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Welch ein Grab ist hier, das alle G\u00f6tter mit Leben", "tokens": ["Welch", "ein", "Grab", "ist", "hier", ",", "das", "al\u00b7le", "G\u00f6t\u00b7ter", "mit", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ADV", "$,", "PRELS", "PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sch\u00f6n bepflanzt und geziert? Es ist Anakreons Ruh.", "tokens": ["Sch\u00f6n", "be\u00b7pflanzt", "und", "ge\u00b7ziert", "?", "Es", "ist", "A\u00b7nak\u00b7re\u00b7ons", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word