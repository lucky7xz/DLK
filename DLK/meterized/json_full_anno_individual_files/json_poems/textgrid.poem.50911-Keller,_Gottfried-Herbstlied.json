{"textgrid.poem.50911": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Herbstlied", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns auf alle Berge gehen,", "tokens": ["La\u00dft", "uns", "auf", "al\u00b7le", "Ber\u00b7ge", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo jetzt der Wein in Str\u00f6men flie\u00dft,", "tokens": ["Wo", "jetzt", "der", "Wein", "in", "Str\u00f6\u00b7men", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcberall am n\u00e4chsten stehen,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "am", "n\u00e4chs\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sich der Freude Quell ergie\u00dft,", "tokens": ["Wo", "sich", "der", "Freu\u00b7de", "Quell", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns tief in allen Augen spiegeln,", "tokens": ["Uns", "tief", "in", "al\u00b7len", "Au\u00b7gen", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die durch das Rebenlaub ergl\u00fchn,", "tokens": ["Die", "durch", "das", "Re\u00b7ben\u00b7laub", "er\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00dft uns das letzte Lied entriegeln,", "tokens": ["La\u00dft", "uns", "das", "letz\u00b7te", "Lied", "ent\u00b7rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo noch zwei rote Lippen bl\u00fchn!", "tokens": ["Wo", "noch", "zwei", "ro\u00b7te", "Lip\u00b7pen", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht, wie des Mondes Antlitz gl\u00fchend", "tokens": ["Seht", ",", "wie", "des", "Mon\u00b7des", "Ant\u00b7litz", "gl\u00fc\u00b7hend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Rosenscheine aufersteht,", "tokens": ["Im", "Ro\u00b7sen\u00b7schei\u00b7ne", "auf\u00b7er\u00b7steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indes die Sonne, freudespr\u00fchend,", "tokens": ["In\u00b7des", "die", "Son\u00b7ne", ",", "freu\u00b7des\u00b7pr\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Leib im Westmeer baden geht!", "tokens": ["Den", "Leib", "im", "West\u00b7meer", "ba\u00b7den", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und an der Jungfraun einer Wange", "tokens": ["Und", "an", "der", "Jung\u00b7fraun", "ei\u00b7ner", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bricht sich des Mondes blasse Glut,", "tokens": ["Bricht", "sich", "des", "Mon\u00b7des", "blas\u00b7se", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Indes, erh\u00f6ht vom Niedergange,", "tokens": ["In\u00b7des", ",", "er\u00b7h\u00f6ht", "vom", "Nie\u00b7der\u00b7gan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ergl\u00e4nzt der andern Purpurblut.", "tokens": ["Er\u00b7gl\u00e4nzt", "der", "an\u00b7dern", "Pur\u00b7pur\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O k\u00fcsset schnell die Himmelszeichen,", "tokens": ["O", "k\u00fcs\u00b7set", "schnell", "die", "Him\u00b7mels\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh sich verdunkelt die Natur!", "tokens": ["Eh", "sich", "ver\u00b7dun\u00b7kelt", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mag dann der Abglanz auch erbleichen:", "tokens": ["Mag", "dann", "der", "Ab\u00b7glanz", "auch", "er\u00b7blei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen glimmt die sch\u00f6nre Spur!", "tokens": ["Im", "Her\u00b7zen", "glimmt", "die", "sch\u00f6n\u00b7re", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mag sich, wer zu dem s\u00fc\u00dfen Leben", "tokens": ["Mag", "sich", ",", "wer", "zu", "dem", "s\u00fc\u00b7\u00dfen", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "$,", "PWS", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der Lieb im Lenz das Wort nicht fand,", "tokens": ["Der", "Lieb", "im", "Lenz", "das", "Wort", "nicht", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der holden Torheit nun ergeben,", "tokens": ["Der", "hol\u00b7den", "Tor\u00b7heit", "nun", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Brausebecher in der Hand!", "tokens": ["Den", "Brau\u00b7se\u00b7be\u00b7cher", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl wird man edler durch das Leiden", "tokens": ["Wohl", "wird", "man", "ed\u00b7ler", "durch", "das", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und strenger durch die herbe Qual;", "tokens": ["Und", "stren\u00b7ger", "durch", "die", "her\u00b7be", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hoch ergl\u00fchn in hei\u00dfen Freuden,", "tokens": ["Doch", "hoch", "er\u00b7gl\u00fchn", "in", "hei\u00b7\u00dfen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das adelt Seel und Leib zumal!", "tokens": ["Das", "a\u00b7delt", "Seel", "und", "Leib", "zu\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und liebt der Himmel seine Kinder,", "tokens": ["Und", "liebt", "der", "Him\u00b7mel", "sei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Tr\u00e4nen er durch Leid erpre\u00dft,", "tokens": ["Wo", "Tr\u00e4\u00b7nen", "er", "durch", "Leid", "er\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So liebt er jene drum nicht minder,", "tokens": ["So", "liebt", "er", "je\u00b7ne", "drum", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PAV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die er vor Freuden weinen l\u00e4\u00dft.", "tokens": ["Die", "er", "vor", "Freu\u00b7den", "wei\u00b7nen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sehnen bleiche Gramgenossen", "tokens": ["Und", "seh\u00b7nen", "blei\u00b7che", "Gram\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich nach dem Grab in ihrer Not:", "tokens": ["Sich", "nach", "dem", "Grab", "in", "ih\u00b7rer", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wem hell des Lebens Born geflossen,", "tokens": ["Wem", "hell", "des", "Le\u00b7bens", "Born", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der scheut noch weniger den Tod!", "tokens": ["Der", "scheut", "noch", "we\u00b7ni\u00b7ger", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Taucht euch ins Bad der Lust, ins klare,", "tokens": ["Taucht", "euch", "ins", "Bad", "der", "Lust", ",", "ins", "kla\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NE", "ART", "NN", "$,", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das euch die kurze Stunde g\u00f6nnt,", "tokens": ["Das", "euch", "die", "kur\u00b7ze", "Stun\u00b7de", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf da\u00df f\u00fcr alles heilig Wahre", "tokens": ["Auf", "da\u00df", "f\u00fcr", "al\u00b7les", "hei\u00b7lig", "Wah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "APPR", "PIS", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr jede Stunde sterben k\u00f6nnt!", "tokens": ["Ihr", "je\u00b7de", "Stun\u00b7de", "ster\u00b7ben", "k\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00dft uns auf alle Berge gehen,", "tokens": ["La\u00dft", "uns", "auf", "al\u00b7le", "Ber\u00b7ge", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo jetzt der Wein in Str\u00f6men flie\u00dft,", "tokens": ["Wo", "jetzt", "der", "Wein", "in", "Str\u00f6\u00b7men", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "