{"textgrid.poem.53045": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Sterne, darf ich euch auch wohl noch Sterne nennen,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Sterne, darf ich euch auch wohl noch Sterne nennen,", "tokens": ["Ihr", "Ster\u00b7ne", ",", "darf", "ich", "euch", "auch", "wohl", "noch", "Ster\u00b7ne", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn jetzt ein Nebel euch umw\u00f6lket Flamm' und Licht,", "tokens": ["Wenn", "jetzt", "ein", "Ne\u00b7bel", "euch", "um\u00b7w\u00f6l\u00b7ket", "Flamm'", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Hermione doch am himmlischen Gesicht", "tokens": ["Da", "Her\u00b7mio\u00b7ne", "doch", "am", "himm\u00b7li\u00b7schen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Keinmal nicht minder l\u00e4\u00dft, als zwei Gestirne, brennen?", "tokens": ["Kein\u00b7mal", "nicht", "min\u00b7der", "l\u00e4\u00dft", ",", "als", "zwei", "Ge\u00b7stir\u00b7ne", ",", "bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVFIN", "$,", "KOUS", "CARD", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du g\u00fcldne Sternenburg, du, Himmel, mu\u00dft's bekennen,", "tokens": ["Du", "g\u00fcld\u00b7ne", "Ster\u00b7nen\u00b7burg", ",", "du", ",", "Him\u00b7mel", ",", "mu\u00dft's", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "$,", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein blaugew\u00f6lbtes Dach wei\u00df von zwei Sonnen nicht,", "tokens": ["Dein", "blau\u00b7ge\u00b7w\u00f6lb\u00b7tes", "Dach", "wei\u00df", "von", "zwei", "Son\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "CARD", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da, wenn die Morgenr\u00f6th' auf ihrem Mund' anbricht,", "tokens": ["Da", ",", "wenn", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7th'", "auf", "ih\u00b7rem", "Mund'", "an\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zwei Sonnen ihr allzeit der Stirne Thron umbrennen.", "tokens": ["Zwei", "Son\u00b7nen", "ihr", "all\u00b7zeit", "der", "Stir\u00b7ne", "Thron", "um\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jedoch du magst dich noch mit hundert Sonnen schm\u00