{"dta.poem.11814": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jemehr du aus ihm nimmst, je gr\u00f6\u00dfer wird der Graben;", "tokens": ["Je\u00b7mehr", "du", "aus", "ihm", "nimmst", ",", "je", "gr\u00f6\u00b7\u00dfer", "wird", "der", "Gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Freigebigkeit, das ist ein Bild von deinen Gaben.", "tokens": ["Frei\u00b7ge\u00b7big\u00b7keit", ",", "das", "ist", "ein", "Bild", "von", "dei\u00b7nen", "Ga\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Dem edlen Sinn ist kein geringes Bild zu klein,", "tokens": ["Dem", "ed\u00b7len", "Sinn", "ist", "kein", "ge\u00b7rin\u00b7ges", "Bild", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er macht es sich zurecht, und legt sich selbst hinein.", "tokens": ["Er", "macht", "es", "sich", "zu\u00b7recht", ",", "und", "legt", "sich", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei du der Sch\u00f6pfbrunn, der gern allen Nachbarn borgt,", "tokens": ["Sei", "du", "der", "Sch\u00f6pf\u00b7brunn", ",", "der", "gern", "al\u00b7len", "Nach\u00b7barn", "borgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor Ersch\u00f6pfung ist am wenigsten besorgt.", "tokens": ["Und", "vor", "Er\u00b7sch\u00f6p\u00b7fung", "ist", "am", "we\u00b7nigs\u00b7ten", "be\u00b7sorgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Er hat stets frische F\u00fcll', erh\u00e4lt man ihn im Zug;", "tokens": ["Er", "hat", "stets", "fri\u00b7sche", "F\u00fcll'", ",", "er\u00b7h\u00e4lt", "man", "ihn", "im", "Zug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,", "VVFIN", "PIS", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht, so \u00fcberzieht ihn Schimmel bald genug.", "tokens": ["Wo", "nicht", ",", "so", "\u00fc\u00b7berz\u00b7ieht", "ihn", "Schim\u00b7mel", "bald", "ge\u00b7nug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sei du das Licht im Haus, vom Scheffel unverdeckt,", "tokens": ["Sei", "du", "das", "Licht", "im", "Haus", ",", "vom", "Schef\u00b7fel", "un\u00b7ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das gl\u00e4nzt, wenn an ihm wird ein andres angesteckt.", "tokens": ["Das", "gl\u00e4nzt", ",", "wenn", "an", "ihm", "wird", "ein", "and\u00b7res", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "APPR", "PPER", "VAFIN", "ART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es geht davon nicht aus, und seinen Widerschein", "tokens": ["Es", "geht", "da\u00b7von", "nicht", "aus", ",", "und", "sei\u00b7nen", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "PTKVZ", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht es im Nachbarhaus, kein Stern gl\u00e4nzt gern allein.", "tokens": ["Sieht", "es", "im", "Nach\u00b7bar\u00b7haus", ",", "kein", "Stern", "gl\u00e4nzt", "gern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PIAT", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wir alle sind nur Stern' in einer Erdennacht,", "tokens": ["Wir", "al\u00b7le", "sind", "nur", "Stern'", "in", "ei\u00b7ner", "Er\u00b7den\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehn aus wie Lampen gern, wann unser Tag erwacht.", "tokens": ["Gehn", "aus", "wie", "Lam\u00b7pen", "gern", ",", "wann", "un\u00b7ser", "Tag", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "NN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}