{"dta.poem.21873": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Diane wusche sich in reinem Bade/", "tokens": ["Di\u00b7a\u00b7ne", "wu\u00b7sche", "sich", "in", "rei\u00b7nem", "Ba\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein junger J\u00e4ger kahm darzu gerade/", "tokens": ["ein", "jun\u00b7ger", "J\u00e4\u00b7ger", "kahm", "dar\u00b7zu", "ge\u00b7ra\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den halben Leib bedekkt die Silber-Fluht", "tokens": ["den", "hal\u00b7ben", "Leib", "be\u00b7dekkt", "die", "Sil\u00b7ber\u00b7Fluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die Helffte sah\u2019 er blo\u00df ob Tetis Wellen.", "tokens": ["die", "Helff\u00b7te", "sah'", "er", "blo\u00df", "ob", "Te\u00b7tis", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KOUS", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hie war der Klippen Bild/ und dort der hellen", "tokens": ["Hie", "war", "der", "Klip\u00b7pen", "Bild", "/", "und", "dort", "der", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$(", "KON", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein ander trug darvon Akteons Hut", "tokens": ["Ein", "an\u00b7der", "trug", "dar\u00b7von", "Ak\u00b7te\u00b7ons", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "PAV", "NN", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}