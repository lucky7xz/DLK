{"textgrid.poem.43405": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngst kam ein K\u00f6nig vor das Himmelsthor", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst kam ein K\u00f6nig vor das Himmelsthor", "tokens": ["J\u00fcngst", "kam", "ein", "K\u00f6\u00b7nig", "vor", "das", "Him\u00b7mel\u00b7sthor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schien in voller Zuversicht zu hoffen,", "tokens": ["Und", "schien", "in", "vol\u00b7ler", "Zu\u00b7ver\u00b7sicht", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn eine Majest\u00e4t nur kommt davor,", "tokens": ["Wenn", "ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "nur", "kommt", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So steh' ihr gleich der ganze Himmel offen.", "tokens": ["So", "steh'", "ihr", "gleich", "der", "gan\u00b7ze", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der K\u00f6nig h\u00f6rt St. Peters Wort:", "tokens": ["Der", "K\u00f6\u00b7nig", "h\u00f6rt", "St.", "Pe\u00b7ters", "Wort", ":"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NE", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Du darfst hier keine Hoffnung fassen!", "tokens": ["Du", "darfst", "hier", "kei\u00b7ne", "Hoff\u00b7nung", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bleib drau\u00dfen stehn nur immerfort,", "tokens": ["Bleib", "drau\u00b7\u00dfen", "stehn", "nur", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du wirst f\u00fcrwahr nie eingelassen.", "tokens": ["Du", "wirst", "f\u00fcr\u00b7wahr", "nie", "ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Darauf erscheint ein deutscher Jesuit", "tokens": ["Da\u00b7rauf", "er\u00b7scheint", "ein", "deut\u00b7scher", "Je\u00b7su\u00b7it"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und spricht um Einla\u00df an den heil'gen Peter.", "tokens": ["Und", "spricht", "um", "Ein\u00b7la\u00df", "an", "den", "heil'\u00b7gen", "Pe\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sobald St. Petrus ihn nur eben sieht,", "tokens": ["So\u00b7bald", "St.", "Pet\u00b7rus", "ihn", "nur", "e\u00b7ben", "sieht", ","], "token_info": ["word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da schreit er ihm entgegen Mord und Zeter.", "tokens": ["Da", "schreit", "er", "ihm", "ent\u00b7ge\u00b7gen", "Mord", "und", "Ze\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was willst du hier? auf! mach dich fort!", "tokens": ["Was", "willst", "du", "hier", "?", "auf", "!", "mach", "dich", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "PTKVZ", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann euch Heuchler nicht vertragen;", "tokens": ["Ich", "kann", "euch", "Heuch\u00b7ler", "nicht", "ver\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Geh, setz dich zu dem K\u00f6nig dort!", "tokens": ["Geh", ",", "setz", "dich", "zu", "dem", "K\u00f6\u00b7nig", "dort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Himmel darfst du uns nicht plagen.", "tokens": ["Im", "Him\u00b7mel", "darfst", "du", "uns", "nicht", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zum K\u00f6nig setzt sich da der Jesuit", "tokens": ["Zum", "K\u00f6\u00b7nig", "setzt", "sich", "da", "der", "Je\u00b7su\u00b7it"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tr\u00f6stet ihn mit manchem frommen Worte:", "tokens": ["Und", "tr\u00f6s\u00b7tet", "ihn", "mit", "man\u00b7chem", "from\u00b7men", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbich wei\u00df gewi\u00df, auch unsre Qual entflieht,", "tokens": ["\u00bb", "ich", "wei\u00df", "ge\u00b7wi\u00df", ",", "auch", "uns\u00b7re", "Qual", "ent\u00b7flieht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald \u00f6ffnet sich auch uns die Himmelspforte;", "tokens": ["Bald", "\u00f6ff\u00b7net", "sich", "auch", "uns", "die", "Him\u00b7mel\u00b7spfor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bald findet sich Gelegenheit,", "tokens": ["Bald", "fin\u00b7det", "sich", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann werden wir auch eingelassen,", "tokens": ["Dann", "wer\u00b7den", "wir", "auch", "ein\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann ist vorbei auch unser Leid \u2013", "tokens": ["Dann", "ist", "vor\u00b7bei", "auch", "un\u00b7ser", "Leid", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum la\u00df uns ruhig Hoffnung fassen.\u00ab", "tokens": ["Drum", "la\u00df", "uns", "ru\u00b7hig", "Hoff\u00b7nung", "fas\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Jesuit wei\u00df die Gelegenheit", "tokens": ["Der", "Je\u00b7su\u00b7it", "wei\u00df", "die", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So ganz und gar f\u00fcrtrefflich abzupassen:", "tokens": ["So", "ganz", "und", "gar", "f\u00fcr\u00b7treff\u00b7lich", "ab\u00b7zu\u00b7pas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gefahren kommt des Papstes Heiligkeit,", "tokens": ["Ge\u00b7fah\u00b7ren", "kommt", "des", "Paps\u00b7tes", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die wird sogleich von Petrus eingelassen.", "tokens": ["Die", "wird", "sog\u00b7leich", "von", "Pet\u00b7rus", "ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbjetzt, ruft er, Majest\u00e4t mit mir!", "tokens": ["\u00bb", "jetzt", ",", "ruft", "er", ",", "Ma\u00b7jes\u00b7t\u00e4t", "mit", "mir", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "$,", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt ist es Zeit: nur frisch, Courage!\u00ab", "tokens": ["Jetzt", "ist", "es", "Zeit", ":", "nur", "frisch", ",", "Cou\u00b7ra\u00b7ge", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ADV", "ADJD", "$,", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Doch Petrus fragt: wer seid denn ihr?", "tokens": ["Doch", "Pet\u00b7rus", "fragt", ":", "wer", "seid", "denn", "ihr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "VVFIN", "$.", "PWS", "VAFIN", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwir sind die p\u00e4pstliche Bagage.\u00ab", "tokens": ["\u00bb", "wir", "sind", "die", "p\u00e4pst\u00b7li\u00b7che", "Ba\u00b7ga\u00b7ge", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "J\u00fcngst kam ein K\u00f6nig vor das Himmelsthor", "tokens": ["J\u00fcngst", "kam", "ein", "K\u00f6\u00b7nig", "vor", "das", "Him\u00b7mel\