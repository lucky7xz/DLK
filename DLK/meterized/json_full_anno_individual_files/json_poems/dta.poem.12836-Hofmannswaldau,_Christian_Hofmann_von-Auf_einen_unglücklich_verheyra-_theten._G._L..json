{"dta.poem.12836": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ungl\u00fccklich verheyra-  \n theten.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Harte, derbe p\u00fcffe,", "tokens": ["Har\u00b7te", ",", "der\u00b7be", "p\u00fcf\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenig gute s\u00fcffe,", "tokens": ["We\u00b7nig", "gu\u00b7te", "s\u00fcf\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Machen trefflich matt,", "tokens": ["Ma\u00b7chen", "treff\u00b7lich", "matt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber selten satt.", "tokens": ["A\u00b7ber", "sel\u00b7ten", "satt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Halb-gekochte speise,", "tokens": ["Halb\u00b7ge\u00b7koch\u00b7te", "spei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ungeheure l\u00e4use,", "tokens": ["Un\u00b7ge\u00b7heu\u00b7re", "l\u00e4u\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eine b\u00f6se frau,", "tokens": ["Ei\u00b7ne", "b\u00f6\u00b7se", "frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Machen zeitig grau.", "tokens": ["Ma\u00b7chen", "zei\u00b7tig", "grau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schwere h\u00f6rner tragen,", "tokens": ["Schwe\u00b7re", "h\u00f6r\u00b7ner", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und mit eseln jagen,", "tokens": ["Und", "mit", "e\u00b7seln", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVINF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Machet h\u00e4uffig schwei\u00df,", "tokens": ["Ma\u00b7chet", "h\u00e4uf\u00b7fig", "schwei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und den r\u00fccken hei\u00df.", "tokens": ["Und", "den", "r\u00fc\u00b7cken", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auf den b\u00e4ncken liegen,", "tokens": ["Auf", "den", "b\u00e4n\u00b7cken", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fremde kinder wiegen,", "tokens": ["Frem\u00b7de", "kin\u00b7der", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist verzweiffelt schwer,", "tokens": ["Ist", "ver\u00b7zweif\u00b7felt", "schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und vexiret sehr.", "tokens": ["Und", "ve\u00b7xi\u00b7ret", "sehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ey so mag die ehe", "tokens": ["Ey", "so", "mag", "die", "e\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "ART", "KOUS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wol ein schlimmes wehe", "tokens": ["Wol", "ein", "schlim\u00b7mes", "we\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ein saurer wein", "tokens": ["Und", "ein", "sau\u00b7rer", "wein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Armer schelmen seyn.", "tokens": ["Ar\u00b7mer", "schel\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}