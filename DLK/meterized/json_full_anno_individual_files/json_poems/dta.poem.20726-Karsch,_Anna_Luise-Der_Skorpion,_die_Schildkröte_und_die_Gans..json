{"dta.poem.20726": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Skorpion,  \n die Schildkr\u00f6te und die Gans.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am weidenreichen Spreegestade,               ", "tokens": ["Am", "wei\u00b7den\u00b7rei\u00b7chen", "Spree\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die gesicherte Najade", "tokens": ["Wo", "die", "ge\u00b7si\u00b7cher\u00b7te", "Na\u00b7ja\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr lockigt Haupt noch stolzer trug,", "tokens": ["Ihr", "lo\u00b7ckigt", "Haupt", "noch", "stol\u00b7zer", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seitdem in Sachsenland Held Heinrich Feinde schlug;", "tokens": ["Seit\u00b7dem", "in", "Sach\u00b7sen\u00b7land", "Held", "Hein\u00b7rich", "Fein\u00b7de", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NE", "NE", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Am Spreegestade kroch aus einem holen Baum", "tokens": ["Am", "Spree\u00b7ge\u00b7sta\u00b7de", "kroch", "aus", "ei\u00b7nem", "ho\u00b7len", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Skorpion, das glaubt man kaum.", "tokens": ["Ein", "Skor\u00b7pi\u00b7on", ",", "das", "glaubt", "man", "kaum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Giebts zu Berlin auch Skorpionen?", "tokens": ["Giebts", "zu", "Ber\u00b7lin", "auch", "Skor\u00b7pi\u00b7o\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADV", "NN", "$."], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Ich dachte, da\u00df sie nur in hei\u00dfen L\u00e4ndern wohnen.", "tokens": ["Ich", "dach\u00b7te", ",", "da\u00df", "sie", "nur", "in", "hei\u00b7\u00dfen", "L\u00e4n\u00b7dern", "woh\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Leser, h\u00f6re doch, ich sah ihn nur im Traum.", "tokens": ["Mein", "Le\u00b7ser", ",", "h\u00f6\u00b7re", "doch", ",", "ich", "sah", "ihn", "nur", "im", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er kroch am Ufer hin und wieder,", "tokens": ["Er", "kroch", "am", "U\u00b7fer", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und sah, von bittern Neid bewegt,", "tokens": ["Und", "sah", ",", "von", "bit\u00b7tern", "Neid", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ins gr\u00fcne Schilf scheels\u00fcchtig nieder", "tokens": ["Ins", "gr\u00fc\u00b7ne", "Schilf", "scheel\u00b7s\u00fcch\u00b7tig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Auf ein Gesch\u00f6pf, das sich mit breitem Schilde tr\u00e4gt,", "tokens": ["Auf", "ein", "Ge\u00b7sch\u00f6pf", ",", "das", "sich", "mit", "brei\u00b7tem", "Schil\u00b7de", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und schmackhaft ist am Fleisch, und nach dem Tode", "tokens": ["Und", "schmack\u00b7haft", "ist", "am", "Fleisch", ",", "und", "nach", "dem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "gl\u00e4nzet", "tokens": ["gl\u00e4n\u00b7zet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "In seinem Deckel sch\u00f6n polirt.", "tokens": ["In", "sei\u00b7nem", "De\u00b7ckel", "sch\u00f6n", "po\u00b7lirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Skorpion mit Gift zum Schadenthun geschw\u00e4nzet,", "tokens": ["Der", "Skor\u00b7pi\u00b7on", "mit", "Gift", "zum", "Scha\u00b7den\u00b7thun", "ge\u00b7schw\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von der Natur, und nicht geziert", "tokens": ["Von", "der", "Na\u00b7tur", ",", "und", "nicht", "ge\u00b7ziert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mit bunten Flecken, wie die Schlangen,", "tokens": ["Mit", "bun\u00b7ten", "Fle\u00b7cken", ",", "wie", "die", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der Skorpion kroch an das Schilf", "tokens": ["Der", "Skor\u00b7pi\u00b7on", "kroch", "an", "das", "Schilf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und sprach: dir Freundin sey geklaget mein Verlangen,", "tokens": ["Und", "sprach", ":", "dir", "Freun\u00b7din", "sey", "ge\u00b7kla\u00b7get", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dort \u00fcbern Strome will mein Bruder mich umfangen,", "tokens": ["Dort", "\u00fc\u00b7bern", "Stro\u00b7me", "will", "mein", "Bru\u00b7der", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und schwimmen kann ich nicht; du aber kannst, ach hilf", "tokens": ["Und", "schwim\u00b7men", "kann", "ich", "nicht", ";", "du", "a\u00b7ber", "kannst", ",", "ach", "hilf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "ADV", "VMFIN", "$,", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit deinen Rudern mir her\u00fcber!", "tokens": ["Mit", "dei\u00b7nen", "Ru\u00b7dern", "mir", "her\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die Kr\u00f6te mit dem Schilde spricht:", "tokens": ["Die", "Kr\u00f6\u00b7te", "mit", "dem", "Schil\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Gef\u00e4lligkeit ist meine Pflicht,", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "ist", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und kein Gesch\u00e4fte war mir lieber", "tokens": ["Und", "kein", "Ge\u00b7sch\u00e4f\u00b7te", "war", "mir", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Als dies; mein Schild ist breit genug.", "tokens": ["Als", "dies", ";", "mein", "Schild", "ist", "breit", "ge\u00b7nug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Sie sprichts: er setzt sich auf und da sie nun getreulich", "tokens": ["Sie", "sprichts", ":", "er", "setzt", "sich", "auf", "und", "da", "sie", "nun", "ge\u00b7treu\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "PTKVZ", "KON", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den giftigen Verr\u00e4ther trug,", "tokens": ["Den", "gif\u00b7ti\u00b7gen", "Ver\u00b7r\u00e4\u00b7ther", "trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Schwamm eine Gans daher und schlug", "tokens": ["Schwamm", "ei\u00b7ne", "Gans", "da\u00b7her", "und", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PAV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Mit beiden Fl\u00fcgeln auf, und schrie: das ist abscheulich!", "tokens": ["Mit", "bei\u00b7den", "Fl\u00fc\u00b7geln", "auf", ",", "und", "schrie", ":", "das", "ist", "ab\u00b7scheu\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$.", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Jetzt fl\u00f6\u00dft dir guten Schwimmerin", "tokens": ["Jetzt", "fl\u00f6\u00dft", "dir", "gu\u00b7ten", "Schwim\u00b7me\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der, den du tr\u00e4gst, das Gift im R\u00fccken.", "tokens": ["Der", ",", "den", "du", "tr\u00e4gst", ",", "das", "Gift", "im", "R\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Verdammter! sprach hierauf die treue Tr\u00e4gerin,", "tokens": ["Ver\u00b7damm\u00b7ter", "!", "sprach", "hier\u00b7auf", "die", "treu\u00b7e", "Tr\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mich panzert die Natur zu sehr vor deinen T\u00fccken,", "tokens": ["Mich", "pan\u00b7zert", "die", "Na\u00b7tur", "zu", "sehr", "vor", "dei\u00b7nen", "T\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dein Gift flo\u00df schadlos in den Flu\u00df;", "tokens": ["Dein", "Gift", "flo\u00df", "schad\u00b7los", "in", "den", "Flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Sey du ihm nachgest\u00fcrzt! Hier tauchte sie ihn nieder \u2014", "tokens": ["Sey", "du", "ihm", "nach\u00b7ge\u00b7st\u00fcrzt", "!", "Hier", "tauch\u00b7te", "sie", "ihn", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Skorpion hat noch viel Schwestern und viel", "tokens": ["Der", "Skor\u00b7pi\u00b7on", "hat", "noch", "viel", "Schwes\u00b7tern", "und", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "KON", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.40": {"text": "Br\u00fcder.", "tokens": ["Br\u00fc\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "O da\u00df nicht jeder Mensch nach dem Verr\u00e4ther Ku\u00df,", "tokens": ["O", "da\u00df", "nicht", "je\u00b7der", "Mensch", "nach", "dem", "Ver\u00b7r\u00e4\u00b7ther", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PTKNEG", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Den er gegeben hat, also ersaufen mu\u00df!", "tokens": ["Den", "er", "ge\u00b7ge\u00b7ben", "hat", ",", "al\u00b7so", "er\u00b7sau\u00b7fen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}