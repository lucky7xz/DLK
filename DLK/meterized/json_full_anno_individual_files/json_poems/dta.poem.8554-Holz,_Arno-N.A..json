{"dta.poem.8554": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unter weissen Sommerwolken \u2014", "tokens": ["Un\u00b7ter", "weis\u00b7sen", "Som\u00b7mer\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blumen und Gr\u00e4ser wiegen sich, ich bin so wunderbar m\u00fcde.", "tokens": ["Blu\u00b7men", "und", "Gr\u00e4\u00b7ser", "wie\u00b7gen", "sich", ",", "ich", "bin", "so", "wun\u00b7der\u00b7bar", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "$,", "PPER", "VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}}, "stanza.3": {"line.1": {"text": "Aus einer Welt, die unterging, ruft der Vogel B\u00fclow,", "tokens": ["Aus", "ei\u00b7ner", "Welt", ",", "die", "un\u00b7ter\u00b7ging", ",", "ruft", "der", "Vo\u00b7gel", "B\u00fc\u00b7low", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "ART", "NE", "NE", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "in meinen Traum", "tokens": ["in", "mei\u00b7nen", "Traum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "flammt Mohn, wogt ein Kornfeld.", "tokens": ["flammt", "Mohn", ",", "wogt", "ein", "Korn\u00b7feld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-++-+-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Durch riesige Korallenw\u00e4lder", "tokens": ["Durch", "rie\u00b7si\u00b7ge", "Ko\u00b7ral\u00b7len\u00b7w\u00e4l\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sinke ich immer tiefer.", "tokens": ["sin\u00b7ke", "ich", "im\u00b7mer", "tie\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Seesterne rollen in mir und alte Kronen.", "tokens": ["Sees\u00b7ter\u00b7ne", "rol\u00b7len", "in", "mir", "und", "al\u00b7te", "Kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "PPER", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Meine gr\u00fcnen T\u00f6chter,", "tokens": ["Mei\u00b7ne", "gr\u00fc\u00b7nen", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tang im Haar,", "tokens": ["Tang", "im", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "tanzen.", "tokens": ["tan\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Ich bin die Flut, ich bin die Finsterniss.", "tokens": ["Ich", "bin", "die", "Flut", ",", "ich", "bin", "die", "Fins\u00b7ter\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Glocken!", "tokens": ["Glo\u00b7cken", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Durch die h\u00e4ngenden Zweige eines Birkenw\u00e4ldchens", "tokens": ["Durch", "die", "h\u00e4n\u00b7gen\u00b7den", "Zwei\u00b7ge", "ei\u00b7nes", "Bir\u00b7ken\u00b7w\u00e4ld\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "glitzert ein Goldhimmel.", "tokens": ["glit\u00b7zert", "ein", "Gold\u00b7him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}}}}