{"dta.poem.19818": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schn\u00fctzelputz - H\u00e4usel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So geht es in Schn\u00fctzelputz H\u00e4usel,               ", "tokens": ["So", "geht", "es", "in", "Schn\u00fct\u00b7zel\u00b7putz", "H\u00e4u\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da singen und tanzen die M\u00e4usel,", "tokens": ["Da", "sin\u00b7gen", "und", "tan\u00b7zen", "die", "M\u00e4u\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und bellen die Schnecken im H\u00e4usel.", "tokens": ["Und", "bel\u00b7len", "die", "Schne\u00b7cken", "im", "H\u00e4u\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In Schn\u00fctzelputz H\u00e4usel da geht es sehr toll,", "tokens": ["In", "Schn\u00fct\u00b7zel\u00b7putz", "H\u00e4u\u00b7sel", "da", "geht", "es", "sehr", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da saufen sich Tisch und B\u00e4nke voll,", "tokens": ["Da", "sau\u00b7fen", "sich", "Tisch", "und", "B\u00e4n\u00b7ke", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Pantoffeln unter dem Bette,", "tokens": ["Pan\u00b7tof\u00b7feln", "un\u00b7ter", "dem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "So geht es in Schn\u00fctzelputz H\u00e4usel etc.", "tokens": ["So", "geht", "es", "in", "Schn\u00fct\u00b7zel\u00b7putz", "H\u00e4u\u00b7sel", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es sassen zwei Ochsen im Storchenest.", "tokens": ["Es", "sas\u00b7sen", "zwei", "Och\u00b7sen", "im", "Stor\u00b7che\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die hatten einander gar lieblich getr\u00f6st't,", "tokens": ["Die", "hat\u00b7ten", "ein\u00b7an\u00b7der", "gar", "lieb\u00b7lich", "ge\u00b7tr\u00f6st't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und wollten die Eier ausbr\u00fcten.", "tokens": ["Und", "woll\u00b7ten", "die", "Ei\u00b7er", "aus\u00b7br\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "So geht es in Schn\u00fctzelputz H\u00e4usel etc.", "tokens": ["So", "geht", "es", "in", "Schn\u00fct\u00b7zel\u00b7putz", "H\u00e4u\u00b7sel", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es zogen zwei St\u00f6rche wohl auf die Wacht,", "tokens": ["Es", "zo\u00b7gen", "zwei", "St\u00f6r\u00b7che", "wohl", "auf", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die hatten ihre Sache gar wohl bedacht,", "tokens": ["Die", "hat\u00b7ten", "ih\u00b7re", "Sa\u00b7che", "gar", "wohl", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit ihren gro\u00dfm\u00e4chtigen Spiessen.", "tokens": ["Mit", "ih\u00b7ren", "gro\u00df\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Spies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "So geht es in Schn\u00fctzelputz H\u00e4usel etc.", "tokens": ["So", "geht", "es", "in", "Schn\u00fct\u00b7zel\u00b7putz", "H\u00e4u\u00b7sel", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NN", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich w\u00fc\u00dfte der Dinge noch mehr zu sagen,", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "der", "Din\u00b7ge", "noch", "mehr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die sich in Schn\u00fctzelputz H\u00e4usel zutragen,", "tokens": ["Die", "sich", "in", "Schn\u00fct\u00b7zel\u00b7putz", "H\u00e4u\u00b7sel", "zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gar l\u00e4cherlich \u00fcber die Ma\u00dfen.", "tokens": ["Gar", "l\u00e4\u00b7cher\u00b7lich", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}