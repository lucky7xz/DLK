{"dta.poem.2855": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du thust, da du dir sollst die Unart abgew\u00f6hnen,", "tokens": ["Du", "thust", ",", "da", "du", "dir", "sollst", "die", "Un\u00b7art", "ab\u00b7ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sollte dir entgehn das Sch\u00f6nste von dem Sch\u00f6nen.", "tokens": ["Als", "soll\u00b7te", "dir", "ent\u00b7gehn", "das", "Sch\u00f6ns\u00b7te", "von", "dem", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sch\u00f6n sie d\u00fcnke dir, doch gib die Unart mir,", "tokens": ["Wie", "sch\u00f6n", "sie", "d\u00fcn\u00b7ke", "dir", ",", "doch", "gib", "die", "Un\u00b7art", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "PPER", "$,", "KON", "VVIMP", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zum Ersatze geb' ich meine Liebe dir.", "tokens": ["Und", "zum", "Er\u00b7sat\u00b7ze", "geb'", "ich", "mei\u00b7ne", "Lie\u00b7be", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bist du's zufrieden? Gut! geschlossen ist der Kauf,", "tokens": ["Bist", "du's", "zu\u00b7frie\u00b7den", "?", "Gut", "!", "ge\u00b7schlos\u00b7sen", "ist", "der", "Kauf", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$.", "NN", "$.", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Unart ist nun mein, du hast die Liebe drauf.", "tokens": ["Die", "Un\u00b7art", "ist", "nun", "mein", ",", "du", "hast", "die", "Lie\u00b7be", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "$,", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Handel freut mich sehr; m\u00f6g' er dich auch erfreun!", "tokens": ["Der", "Han\u00b7del", "freut", "mich", "sehr", ";", "m\u00f6g'", "er", "dich", "auch", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bedenke dieses nur, und nie wird es dich reun:", "tokens": ["Be\u00b7den\u00b7ke", "die\u00b7ses", "nur", ",", "und", "nie", "wird", "es", "dich", "reun", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADV", "$,", "KON", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Einst n\u00e4hme doch die Welt die Unart nach Geb\u00fchr", "tokens": ["Einst", "n\u00e4h\u00b7me", "doch", "die", "Welt", "die", "Un\u00b7art", "nach", "Ge\u00b7b\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir ab und g\u00e4be nichts als ihren Spott daf\u00fcr.", "tokens": ["Dir", "ab", "und", "g\u00e4\u00b7be", "nichts", "als", "ih\u00b7ren", "Spott", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "PIS", "KOKOM", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}