etra"}, "line.3": {"text": "Ach! geschmolzen flie\u00dft in Tropfen", "tokens": ["Ach", "!", "ge\u00b7schmol\u00b7zen", "flie\u00dft", "in", "Trop\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVPP", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Herd mein armes Herz!", "tokens": ["Auf", "den", "Herd", "mein", "ar\u00b7mes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "M\u00e4dchen, deiner Stimme Lachen,", "tokens": ["M\u00e4d\u00b7chen", ",", "dei\u00b7ner", "Stim\u00b7me", "La\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Wangen Rosenlicht,", "tokens": ["Dei\u00b7ner", "Wan\u00b7gen", "Ro\u00b7sen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei's im Schlummer, sei's im Wachen,", "tokens": ["Sei's", "im", "Schlum\u00b7mer", ",", "sei's", "im", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andres tr\u00e4um' und denk' ich nicht.", "tokens": ["And\u00b7res", "tr\u00e4um'", "und", "denk'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bei der Kastagnetten Schmettern,", "tokens": ["Bei", "der", "Kas\