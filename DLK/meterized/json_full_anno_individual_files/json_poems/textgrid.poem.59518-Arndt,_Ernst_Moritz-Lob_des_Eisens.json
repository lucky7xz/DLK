{"textgrid.poem.59518": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Lob des Eisens", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgold\u00ab schreit die feige Welt,", "tokens": ["\u00bb", "gold", "\u00ab", "schreit", "die", "fei\u00b7ge", "Welt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und Gold macht feige Knechte,", "tokens": ["Und", "Gold", "macht", "fei\u00b7ge", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Tapfern Herz verstellt", "tokens": ["Des", "Tap\u00b7fern", "Herz", "ver\u00b7stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und schw\u00e4cht des Starken Rechte;", "tokens": ["Und", "schw\u00e4cht", "des", "Star\u00b7ken", "Rech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Gold mag keiner sterben,", "tokens": ["F\u00fcr", "Gold", "mag", "kei\u00b7ner", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der nicht mehr leben darf,", "tokens": ["Der", "nicht", "mehr", "le\u00b7ben", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und edlen Ruhm zu werben", "tokens": ["Und", "ed\u00b7len", "Ruhm", "zu", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Macht's nie den Degen scharf.", "tokens": ["Macht's", "nie", "den", "De\u00b7gen", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drum preis' ich das Metall,", "tokens": ["Drum", "preis'", "ich", "das", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das schwarze, braune Eisen,", "tokens": ["Das", "schwar\u00b7ze", ",", "brau\u00b7ne", "Ei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn ohne Glanz und Schall", "tokens": ["Denn", "oh\u00b7ne", "Glanz", "und", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es tut sich herrlich weisen,", "tokens": ["Es", "tut", "sich", "herr\u00b7lich", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heilt m\u00e4chtig alle Wunden,", "tokens": ["Heilt", "m\u00e4ch\u00b7tig", "al\u00b7le", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die jenes blanke macht;", "tokens": ["Die", "je\u00b7nes", "blan\u00b7ke", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00e4r' Eisen nicht gefunden,", "tokens": ["W\u00e4r'", "Ei\u00b7sen", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch tappten wir in Nacht.", "tokens": ["Noch", "tapp\u00b7ten", "wir", "in", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es stellt den Pflug ins Land,", "tokens": ["Es", "stellt", "den", "Pflug", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde zu bezwingen,", "tokens": ["Die", "Er\u00b7de", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es l\u00e4\u00dft das Schiff vom Strand", "tokens": ["Es", "l\u00e4\u00dft", "das", "Schiff", "vom", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf schnellen Windesschwingen,", "tokens": ["Auf", "schnel\u00b7len", "Win\u00b7des\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Baut Menschen feste Sitze", "tokens": ["Baut", "Men\u00b7schen", "fes\u00b7te", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und f\u00fchrt die Kunst ins Haus", "tokens": ["Und", "f\u00fchrt", "die", "Kunst", "ins", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und l\u00f6scht des Donners Blitze", "tokens": ["Und", "l\u00f6scht", "des", "Don\u00b7ners", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit einer Stange aus.", "tokens": ["Mit", "ei\u00b7ner", "Stan\u00b7ge", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wann die Sitte flieht", "tokens": ["Und", "wann", "die", "Sit\u00b7te", "flieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und M\u00e4nnerarm' erschlaffen,", "tokens": ["Und", "M\u00e4n\u00b7ne\u00b7rarm'", "er\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann Trug f\u00fcr Ehre bl\u00fcht", "tokens": ["Wann", "Trug", "f\u00fcr", "Eh\u00b7re", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Gold gebeut f\u00fcr Waffen,", "tokens": ["Und", "Gold", "ge\u00b7beut", "f\u00fcr", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wann Despotismusjammer", "tokens": ["Wann", "Des\u00b7po\u00b7tis\u00b7mus\u00b7jam\u00b7mer"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Welt mit Schmach bedroht,", "tokens": ["Die", "Welt", "mit", "Schmach", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann schl\u00e4gt aus ihm der Hammer", "tokens": ["Dann", "schl\u00e4gt", "aus", "ihm", "der", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sieg und Tyrannentod.", "tokens": ["Sieg", "und", "Ty\u00b7ran\u00b7nen\u00b7tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Dann wird es sch\u00f6ne Wehr,", "tokens": ["Dann", "wird", "es", "sch\u00f6\u00b7ne", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Mannes Heil und Freude,", "tokens": ["Des", "Man\u00b7nes", "Heil", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Schwert, als Schild, als Speer,", "tokens": ["Als", "Schwert", ",", "als", "Schild", ",", "als", "Speer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als festes Brustgeschmeide", "tokens": ["Als", "fes\u00b7tes", "Brust\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Macht es den Tritt der Braven", "tokens": ["Macht", "es", "den", "Tritt", "der", "Bra\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Den Knechten f\u00fcrchterlich,", "tokens": ["Den", "Knech\u00b7ten", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir w\u00e4ren alle Sklaven", "tokens": ["Wir", "w\u00e4\u00b7ren", "al\u00b7le", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ohn' Eisen ewiglich.", "tokens": ["Ohn'", "Ei\u00b7sen", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und sieget Tyrannei", "tokens": ["Und", "sie\u00b7get", "Ty\u00b7ran\u00b7nei"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und sinkt des Gl\u00fcckes Wage,", "tokens": ["Und", "sinkt", "des", "Gl\u00fc\u00b7ckes", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So macht es blutig frei", "tokens": ["So", "macht", "es", "blu\u00b7tig", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem tapfern Schlage,", "tokens": ["Mit", "ei\u00b7nem", "tap\u00b7fern", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zerhaut die Schlangenknoten,", "tokens": ["Zer\u00b7haut", "die", "Schlan\u00b7gen\u00b7kno\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Trug und Feigheit flicht,", "tokens": ["Die", "Trug", "und", "Feig\u00b7heit", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schickt die tapfern Toten", "tokens": ["Und", "schickt", "die", "tap\u00b7fern", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Empor zu Recht und Licht.", "tokens": ["Em\u00b7por", "zu", "Recht", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bleib, Eisen, M\u00e4nnern hold,", "tokens": ["Bleib", ",", "Ei\u00b7sen", ",", "M\u00e4n\u00b7nern", "hold", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df Knechte Gold begehren.", "tokens": ["La\u00df", "Knech\u00b7te", "Gold", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer deine Kraft gewollt,", "tokens": ["Wer", "dei\u00b7ne", "Kraft", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der wollte hohe Ehren,", "tokens": ["Der", "woll\u00b7te", "ho\u00b7he", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der wollte herrlich leben", "tokens": ["Der", "woll\u00b7te", "herr\u00b7lich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und herrlich untergehn.", "tokens": ["Und", "herr\u00b7lich", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum sei dir Preis gegeben,", "tokens": ["Drum", "sei", "dir", "Preis", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Eisen schwarz und sch\u00f6n!", "tokens": ["O", "Ei\u00b7sen", "schwarz", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbgold\u00ab schreit die feige Welt,", "tokens": ["\u00bb", "gold", "\u00ab", "schreit", "die", "fei\u00b7ge", "Welt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und Gold macht feige Knechte,", "tokens": ["Und", "Gold", "macht", "fei\u00b7ge", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Tapfern Herz verstellt", "tokens": ["Des", "Tap\u00b7fern", "Herz", "ver\u00b7stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und schw\u00e4cht des Starken Rechte;", "tokens": ["Und", "schw\u00e4cht", "des", "Star\u00b7ken", "Rech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Gold mag keiner sterben,", "tokens": ["F\u00fcr", "Gold", "mag", "kei\u00b7ner", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der nicht mehr leben darf,", "tokens": ["Der", "nicht", "mehr", "le\u00b7ben", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und edlen Ruhm zu werben", "tokens": ["Und", "ed\u00b7len", "Ruhm", "zu", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Macht's nie den Degen scharf.", "tokens": ["Macht's", "nie", "den", "De\u00b7gen", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Drum