{"textgrid.poem.61305": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Bildung Fluch, darin wir leben,", "tokens": ["Das", "ist", "der", "Bil\u00b7dung", "Fluch", ",", "da\u00b7rin", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr das Beste untergeht im Vielen;", "tokens": ["Da\u00df", "ihr", "das", "Bes\u00b7te", "un\u00b7ter\u00b7geht", "im", "Vie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit jedem Elemente will sie spielen", "tokens": ["Mit", "je\u00b7dem", "E\u00b7le\u00b7men\u00b7te", "will", "sie", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wagt sich keinem voll dahinzugeben.", "tokens": ["Und", "wagt", "sich", "kei\u00b7nem", "voll", "da\u00b7hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum winkt ihr rechts ein Kranz, darnach zu streben,", "tokens": ["Kaum", "winkt", "ihr", "rechts", "ein", "Kranz", ",", "dar\u00b7nach", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "