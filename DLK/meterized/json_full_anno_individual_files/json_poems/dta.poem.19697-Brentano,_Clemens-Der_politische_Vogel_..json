{"dta.poem.19697": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der politische Vogel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich einmahl spazieren ging,               ", "tokens": ["Als", "ich", "ein\u00b7mahl", "spa\u00b7zie\u00b7ren", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einen Lustgarten hinein,", "tokens": ["In", "ei\u00b7nen", "Lust\u00b7gar\u00b7ten", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zu \u00fcberdenken, was ich meint,", "tokens": ["Zu", "\u00fc\u00b7ber\u00b7den\u00b7ken", ",", "was", "ich", "meint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sch\u00f6ne Lusth\u00e4user sein,", "tokens": ["Wo", "sch\u00f6\u00b7ne", "Lust\u00b7h\u00e4u\u00b7ser", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "H\u00f6rt ich es ein V\u00f6glein singen,", "tokens": ["H\u00f6rt", "ich", "es", "ein", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Verstund es auch gar wohl,", "tokens": ["Ver\u00b7stund", "es", "auch", "gar", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von unbekannten Dingen,", "tokens": ["Von", "un\u00b7be\u00b7kann\u00b7ten", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was dieses Jahr geschehen soll,", "tokens": ["Was", "die\u00b7ses", "Jahr", "ge\u00b7sche\u00b7hen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was dieses Jahr geschehen soll.", "tokens": ["Was", "die\u00b7ses", "Jahr", "ge\u00b7sche\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das V\u00f6glein in dem Strausse sa\u00df,", "tokens": ["Das", "V\u00f6\u00b7glein", "in", "dem", "Straus\u00b7se", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schauet den Adler an:", "tokens": ["Und", "schau\u00b7et", "den", "Ad\u00b7ler", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach Adler es wird dieses Jahr,", "tokens": ["Ach", "Ad\u00b7ler", "es", "wird", "die\u00b7ses", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Ein fremder G\u00e4rtners Mann,", "tokens": ["Ein", "frem\u00b7der", "G\u00e4rt\u00b7ners", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich aus dem Garten vertreiben,", "tokens": ["Dich", "aus", "dem", "Gar\u00b7ten", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Sammt deinen Gesellen all,", "tokens": ["Sammt", "dei\u00b7nen", "Ge\u00b7sel\u00b7len", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nicht l\u00e4nger sollt du es verbleiben,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "sollt", "du", "es", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Adlers wartet Gewalt,", "tokens": ["Des", "Ad\u00b7lers", "war\u00b7tet", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Des Adlers wartet Gewalt.", "tokens": ["Des", "Ad\u00b7lers", "war\u00b7tet", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "So bald der Adler dies vernahm,", "tokens": ["So", "bald", "der", "Ad\u00b7ler", "dies", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwingt er sich in die Luft,", "tokens": ["Schwingt", "er", "sich", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu schauen was auf mehr umschwebt", "tokens": ["Zu", "schau\u00b7en", "was", "auf", "mehr", "um\u00b7schwebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PRELS", "APPR", "ADV", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Franz\u00f6sisch und Spanischer Luft,", "tokens": ["Fran\u00b7z\u00f6\u00b7sisch", "und", "Spa\u00b7ni\u00b7scher", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "In Garten will eindringen", "tokens": ["In", "Gar\u00b7ten", "will", "ein\u00b7drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein fremder G\u00e4rtners Mann,", "tokens": ["Ein", "frem\u00b7der", "G\u00e4rt\u00b7ners", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und will einpflanzen die Lilien,", "tokens": ["Und", "will", "ein\u00b7pflan\u00b7zen", "die", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Anstatt der Tulipan,", "tokens": ["An\u00b7statt", "der", "Tu\u00b7li\u00b7pan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Anstatt der Tulipan.", "tokens": ["An\u00b7statt", "der", "Tu\u00b7li\u00b7pan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bei Parmen und Castellen", "tokens": ["Bei", "Par\u00b7men", "und", "Cas\u00b7tel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Hat man die Kugel gesp\u00fcrt,", "tokens": ["Hat", "man", "die", "Ku\u00b7gel", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Stein m\u00f6gt es erbarmen,", "tokens": ["Ein", "Stein", "m\u00f6gt", "es", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie man auf uns hat zielt,", "tokens": ["Wie", "man", "auf", "uns", "hat", "zielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-----+", "measure": "unknown.measure.single"}, "line.5": {"text": "Drei Prinzen und neun Generalen,", "tokens": ["Drei", "Prin\u00b7zen", "und", "neun", "Ge\u00b7ne\u00b7ra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie auch der Feldmarschall,", "tokens": ["Wie", "auch", "der", "Feld\u00b7mar\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sind schon zu Boden gefallen,", "tokens": ["Sind", "schon", "zu", "Bo\u00b7den", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der Gemeinen ohne Zahl,", "tokens": ["Der", "Ge\u00b7mei\u00b7nen", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Der Gemeinen ohne Zahl.", "tokens": ["Der", "Ge\u00b7mei\u00b7nen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Safoier schlug ein Rebell,", "tokens": ["Der", "Sa\u00b7foi\u00b7er", "schlug", "ein", "Re\u00b7bell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Vermeinte sicher zu sein,", "tokens": ["Ver\u00b7mein\u00b7te", "si\u00b7cher", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Deutschen aber Pif, Paf, Puf,", "tokens": ["Die", "Deut\u00b7schen", "a\u00b7ber", "Pif", ",", "Paf", ",", "Puf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fielen ins Lager hinein,", "tokens": ["Und", "fie\u00b7len", "ins", "La\u00b7ger", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dort hatten sie alles verlassen,", "tokens": ["Dort", "hat\u00b7ten", "sie", "al\u00b7les", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Sammt Lager, Stuck und Zelt,", "tokens": ["Sammt", "La\u00b7ger", ",", "Stuck", "und", "Zelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Hemd davon geloffen,", "tokens": ["Im", "Hemd", "da\u00b7von", "ge\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Deutschen bleibt das Feld,", "tokens": ["Den", "Deut\u00b7schen", "bleibt", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Den Deutschen bleibt das Feld.", "tokens": ["Den", "Deut\u00b7schen", "bleibt", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So bald der Franzos nach Safoien kam,", "tokens": ["So", "bald", "der", "Fran\u00b7zos", "nach", "Sa\u00b7foi\u00b7en", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da war der Safoier so froh,", "tokens": ["Da", "war", "der", "Sa\u00b7foi\u00b7er", "so", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da zog er erst sein Hosen an,", "tokens": ["Da", "zog", "er", "erst", "sein", "Ho\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kammpelte die Maus das Stroh,", "tokens": ["Da", "kamm\u00b7pel\u00b7te", "die", "Maus", "das", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Er th\u00e4t sich resolviren,", "tokens": ["Er", "th\u00e4t", "sich", "re\u00b7sol\u00b7vi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schwur bei seinem Gott,", "tokens": ["Und", "schwur", "bei", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sollt er auch alles verlieren,", "tokens": ["Sollt", "er", "auch", "al\u00b7les", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Wollt r\u00e4chen diesen Spott,", "tokens": ["Wollt", "r\u00e4\u00b7chen", "die\u00b7sen", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wollt r\u00e4chen diesen Spott.", "tokens": ["Wollt", "r\u00e4\u00b7chen", "die\u00b7sen", "Spott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}