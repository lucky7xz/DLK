{"dta.poem.20776": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die gro\u00dfe That des Julius C\u00e4sars.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df den Pompejus C\u00e4sar \u00fcberwunden:               ", "tokens": ["Da\u00df", "den", "Pom\u00b7pe\u00b7jus", "C\u00e4\u00b7sar", "\u00fc\u00b7berw\u00b7un\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das war ein Werk des Gl\u00fccks und war der G\u00f6tter", "tokens": ["Das", "war", "ein", "Werk", "des", "Gl\u00fccks", "und", "war", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rath.", "tokens": ["Rath", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Doch da\u00df er nach dem Sieg die Menschlichkeit em-", "tokens": ["Doch", "da\u00df", "er", "nach", "dem", "Sieg", "die", "Menschlich\u00b7keit", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "pfunden,", "tokens": ["pfun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und jeden Tag sich selbst vor seine Feinde bat:", "tokens": ["Und", "je\u00b7den", "Tag", "sich", "selbst", "vor", "sei\u00b7ne", "Fein\u00b7de", "bat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dis war des Helden gro\u00dfe That.", "tokens": ["Dis", "war", "des", "Hel\u00b7den", "gro\u00b7\u00dfe", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}