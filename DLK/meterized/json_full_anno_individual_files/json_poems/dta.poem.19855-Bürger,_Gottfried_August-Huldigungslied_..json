{"dta.poem.19855": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Huldigungslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.71", "pl:0.14", "sv:0.14"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich doch so hold, wie jener               ", "tokens": ["W\u00e4r'", "ich", "doch", "so", "hold", ",", "wie", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "PWAV", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freund der Liebesk\u00f6nigin;", "tokens": ["Freund", "der", "Lie\u00b7bes\u00b7k\u00f6\u00b7ni\u00b7gin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder nur ein bischen sch\u00f6ner,", "tokens": ["O\u00b7der", "nur", "ein", "bi\u00b7schen", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich Armer izo bin!", "tokens": ["Als", "ich", "Ar\u00b7mer", "i\u00b7zo", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn von einem h\u00fcbschen Knaben", "tokens": ["Denn", "von", "ei\u00b7nem", "h\u00fcb\u00b7schen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchltest du vielleicht den Schmerz,", "tokens": ["F\u00fchl\u00b7test", "du", "viel\u00b7leicht", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verschm\u00e4htest nicht die Gaben,", "tokens": ["Und", "ver\u00b7schm\u00e4h\u00b7test", "nicht", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich biete: Hand und Herz.", "tokens": ["Die", "ich", "bie\u00b7te", ":", "Hand", "und", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$.", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00fchrt dich auch aus blassem Munde", "tokens": ["R\u00fchrt", "dich", "auch", "aus", "blas\u00b7sem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebevolle Huldigung;", "tokens": ["Lie\u00b7be\u00b7vol\u00b7le", "Hul\u00b7di\u00b7gung", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "O so heile meine Wunde,", "tokens": ["O", "so", "hei\u00b7le", "mei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder gib ihr Linderung!", "tokens": ["O\u00b7der", "gib", "ihr", "Lin\u00b7de\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dienen kan dir Niemand treuer,", "tokens": ["Die\u00b7nen", "kan", "dir", "Nie\u00b7mand", "treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als dein frommer Agathon.", "tokens": ["Als", "dein", "from\u00b7mer", "A\u00b7gat\u00b7hon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese huldigende Leier", "tokens": ["Die\u00b7se", "hul\u00b7di\u00b7gen\u00b7de", "Lei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt die H\u00e4lfte nicht davon.", "tokens": ["Sagt", "die", "H\u00e4lf\u00b7te", "nicht", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unerm\u00fcdet wil er dienen,", "tokens": ["Un\u00b7er\u00b7m\u00fc\u00b7det", "wil", "er", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Lebens Genius,", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und erforschen aus den Mienen", "tokens": ["Und", "er\u00b7for\u00b7schen", "aus", "den", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolgefallen und Verdrus.", "tokens": ["Wol\u00b7ge\u00b7fal\u00b7len", "und", "Ver\u00b7drus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles, Kind, was dir behagte,", "tokens": ["Al\u00b7les", ",", "Kind", ",", "was", "dir", "be\u00b7hag\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt\u2019 ichs, alles g\u00e4b\u2019 ich dir.", "tokens": ["H\u00e4tt'", "ichs", ",", "al\u00b7les", "g\u00e4b'", "ich", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PIS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schande, wenn ich was versagte,", "tokens": ["Schan\u00b7de", ",", "wenn", "ich", "was", "ver\u00b7sag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohe Schande w\u00e4r\u2019 es mir!", "tokens": ["Ho\u00b7he", "Schan\u00b7de", "w\u00e4r'", "es", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Felen solt\u2019 es nie an Schaaren", "tokens": ["Fe\u00b7len", "solt'", "es", "nie", "an", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holder Spiele, dir zur Lust,", "tokens": ["Hol\u00b7der", "Spie\u00b7le", ",", "dir", "zur", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie an Blumen zu den Haaren,", "tokens": ["Nie", "an", "Blu\u00b7men", "zu", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie an Blumen vor die Brust.", "tokens": ["Nie", "an", "Blu\u00b7men", "vor", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aemsig warten jeder Rebe,", "tokens": ["A\u00b7em\u00b7sig", "war\u00b7ten", "je\u00b7der", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pflegen wolt\u2019 ich jeden Baum,", "tokens": ["Pfle\u00b7gen", "wolt'", "ich", "je\u00b7den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er s\u00fcsse Fr\u00fcchte g\u00e4be,", "tokens": ["Da\u00df", "er", "s\u00fcs\u00b7se", "Fr\u00fcch\u00b7te", "g\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur f\u00fcr deinen zarten Gaum.", "tokens": ["Nur", "f\u00fcr", "dei\u00b7nen", "zar\u00b7ten", "Gaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Schatteng\u00e4nge, Sommerlauben", "tokens": ["Schat\u00b7ten\u00b7g\u00e4n\u00b7ge", ",", "Som\u00b7mer\u00b7lau\u00b7ben"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00f6lbt\u2019 ich dir, zu k\u00fchler Ruh,", "tokens": ["W\u00f6lbt'", "ich", "dir", ",", "zu", "k\u00fch\u00b7ler", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcge Beeren, N\u00fcss\u2019 und Trauben", "tokens": ["Tr\u00fc\u00b7ge", "Bee\u00b7ren", ",", "N\u00fcss'", "und", "Trau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir in Binsenk\u00f6rbchen zu.", "tokens": ["Dir", "in", "Bin\u00b7sen\u00b7k\u00f6rb\u00b7chen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Neben deinem Lager stehen,", "tokens": ["Ne\u00b7ben", "dei\u00b7nem", "La\u00b7ger", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann du lauschtest, wolt\u2019 ich hier.", "tokens": ["Wann", "du", "lauschtest", ",", "wolt'", "ich", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Angenehme K\u00fchlung wehen", "tokens": ["An\u00b7ge\u00b7neh\u00b7me", "K\u00fch\u00b7lung", "we\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solt\u2019 ein Myrtenf\u00e4cher dir. \u2014", "tokens": ["Solt'", "ein", "Myr\u00b7ten\u00b7f\u00e4\u00b7cher", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Alles Leid und Misbehagen,", "tokens": ["Al\u00b7les", "Leid", "und", "Mis\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Sorge, jede Last", "tokens": ["Je\u00b7de", "Sor\u00b7ge", ",", "je\u00b7de", "Last"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r\u2019 ich ganz allein zu tragen", "tokens": ["W\u00e4r'", "ich", "ganz", "al\u00b7lein", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun und immerdar gefast.", "tokens": ["Nun", "und", "im\u00b7mer\u00b7dar", "ge\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nimmer, Liebchen, wolt\u2019 ich tr\u00fcben", "tokens": ["Nim\u00b7mer", ",", "Lieb\u00b7chen", ",", "wolt'", "ich", "tr\u00fc\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Lebens Heiterkeit.", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "Hei\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle deine Launen lieben", "tokens": ["Al\u00b7le", "dei\u00b7ne", "Lau\u00b7nen", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolt\u2019 ich mit Vertr\u00e4glichkeit.", "tokens": ["Wolt'", "ich", "mit", "Ver\u00b7tr\u00e4g\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Sey es Liebes oder Leides!", "tokens": ["Sey", "es", "Lie\u00b7bes", "o\u00b7der", "Lei\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00e4m\u2019 es nur von deiner Huld,", "tokens": ["K\u00e4m'", "es", "nur", "von", "dei\u00b7ner", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So erwiedert ich auf beides", "tokens": ["So", "er\u00b7wie\u00b7dert", "ich", "auf", "bei\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald Entz\u00fccken, bald Gedult.", "tokens": ["Bald", "Ent\u00b7z\u00fc\u00b7cken", ",", "bald", "Ge\u00b7dult", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Fl\u00fcgelschl\u00e4ge von dem Weibchen", "tokens": ["Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7ge", "von", "dem", "Weib\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt des Taubers frommer Sin,", "tokens": ["Tr\u00e4gt", "des", "Tau\u00b7bers", "from\u00b7mer", "Sin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch von dir, geliebtes T\u00e4ubchen,", "tokens": ["Auch", "von", "dir", ",", "ge\u00b7lieb\u00b7tes", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4hm\u2019 ich alles willig hin.", "tokens": ["N\u00e4hm'", "ich", "al\u00b7les", "wil\u00b7lig", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Hiesse mich dein Blik entweichen,", "tokens": ["Hies\u00b7se", "mich", "dein", "Blik", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00fcrnte mir dein Angesicht,", "tokens": ["Z\u00fcrn\u00b7te", "mir", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd\u2019 ich traurend von dir schleichen.", "tokens": ["W\u00fcrd'", "ich", "trau\u00b7rend", "von", "dir", "schlei\u00b7chen", "."], "token_info"