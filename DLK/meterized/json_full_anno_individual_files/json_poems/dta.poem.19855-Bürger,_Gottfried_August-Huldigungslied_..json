ure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Sch\u00f6ne Buler werden kommen,", "tokens": ["Sch\u00f6\u00b7ne", "Bu\u00b7ler", "wer\u00b7den", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden dich um Liebe flehn,", "tokens": ["Wer\u00b7den", "dich", "um", "Lie\u00b7be", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und du wirst von deinem Frommen", "tokens": ["Und", "du", "wirst", "von", "dei\u00b7nem", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem Sch\u00f6nern \u00fcbergehn.", "tokens": ["Zu", "dem", "Sch\u00f6\u00b7nern", "\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Leicht gen\u00fcgen sich die Sinnen", "tokens": ["Leicht", "ge\u00b7n\u00fc\u00b7gen", "sich", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Sch\u00f6nheit T\u00fcncherei,", "tokens": ["An", "der", "Sch\u00f6n\u00b7heit", "T\u00fcn\u00b7che\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbek\u00fcmmert, ob darinnen", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", ",", "ob", "da\u00b7rin\u00b7nen"], "token_info": ["word", "punct", "word", "