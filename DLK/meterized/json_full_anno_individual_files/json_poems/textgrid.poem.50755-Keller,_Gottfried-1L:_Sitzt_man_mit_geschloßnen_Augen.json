{"textgrid.poem.50755": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sitzt man mit geschlo\u00dfnen Augen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sitzt man mit geschlo\u00dfnen Augen", "tokens": ["Sitzt", "man", "mit", "ge\u00b7schlo\u00df\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam in dem dunklen Zimmer,", "tokens": ["Ein\u00b7sam", "in", "dem", "dunk\u00b7len", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzt oft durch die zarten Lider", "tokens": ["Blitzt", "oft", "durch", "die", "zar\u00b7ten", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich roter Kerzenschimmer;", "tokens": ["Pl\u00f6tz\u00b7lich", "ro\u00b7ter", "Ker\u00b7zen\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df ich doch, da\u00df Sonnenstrahlen", "tokens": ["Wei\u00df", "ich", "doch", ",", "da\u00df", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Augendeckel dringen", "tokens": ["Durch", "die", "Au\u00b7gen\u00b7de\u00b7ckel", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in flimmernden Gebilden", "tokens": ["Und", "in", "flim\u00b7mern\u00b7den", "Ge\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich um unsre Seele schlingen.", "tokens": ["Sich", "um", "uns\u00b7re", "See\u00b7le", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also sa\u00df ich in der D\u00e4mmrung,", "tokens": ["Al\u00b7so", "sa\u00df", "ich", "in", "der", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcd von Erdenl\u00e4rm und Staube,", "tokens": ["M\u00fcd", "von", "Er\u00b7den\u00b7l\u00e4rm", "und", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingelullt vom Abendrote,", "tokens": ["Ein\u00b7ge\u00b7lullt", "vom", "A\u00b7ben\u00b7dro\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummernd in der gr\u00fcnen Laube:", "tokens": ["Schlum\u00b7mernd", "in", "der", "gr\u00fc\u00b7nen", "Lau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da begann von Licht und Blumen", "tokens": ["Da", "be\u00b7gann", "von", "Licht", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar ein seltsam schimmernd Weben", "tokens": ["Gar", "ein", "selt\u00b7sam", "schim\u00b7mernd", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Ranken um die Augen", "tokens": ["Und", "ein", "Ran\u00b7ken", "um", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie von goldnen Zauberreben.", "tokens": ["Wie", "von", "gold\u00b7nen", "Zau\u00b7ber\u00b7re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rote Rosen, wei\u00dfe Rosen,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "wei\u00b7\u00dfe", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Primeln, Tulpen und Narzissen,", "tokens": ["Pri\u00b7meln", ",", "Tul\u00b7pen", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dahlien von hundert Farben", "tokens": ["Da\u00b7hli\u00b7en", "von", "hun\u00b7dert", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sah ich durcheinander sprie\u00dfen!", "tokens": ["Sah", "ich", "durch\u00b7ein\u00b7an\u00b7der", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Purpur, Gold, Azur und Silber", "tokens": ["Pur\u00b7pur", ",", "Gold", ",", "A\u00b7zur", "und", "Sil\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flimmerten in Wechselt\u00f6nen,", "tokens": ["Flim\u00b7mer\u00b7ten", "in", "Wech\u00b7sel\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lila, Rosa, heitres Meergr\u00fcn", "tokens": ["Li\u00b7la", ",", "Ro\u00b7sa", ",", "heit\u00b7res", "Meer\u00b7gr\u00fcn"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dften Glanz mit Glanz vers\u00f6hnen!", "tokens": ["Mu\u00df\u00b7ten", "Glanz", "mit", "Glanz", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O das war ein pr\u00e4cht'ger Reigen,", "tokens": ["O", "das", "war", "ein", "pr\u00e4cht'\u00b7ger", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Farben all ihn tanzten,", "tokens": ["Wie", "die", "Far\u00b7ben", "all", "ihn", "tanz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Bt\u00fctenstern' und -glocken", "tokens": ["Wie", "die", "Bt\u00fc\u00b7tens\u00b7tern'", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ringelnd sich in Beete pflanzten! \u2013", "tokens": ["Rin\u00b7gelnd", "sich", "in", "Bee\u00b7te", "pflanz\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "APPR", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber in den Wundergarten", "tokens": ["A\u00b7ber", "in", "den", "Wun\u00b7der\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Senkte eine Jakobsleiter", "tokens": ["Senk\u00b7te", "ei\u00b7ne", "Ja\u00b7kobs\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von zwei Strahlen sanft sich nieder", "tokens": ["Von", "zwei", "Strah\u00b7len", "sanft", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus zwei Sternen, bl\u00e4ulich heiter!", "tokens": ["Aus", "zwei", "Ster\u00b7nen", ",", "bl\u00e4u\u00b7lich", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kleine blonde Liebesengel", "tokens": ["Klei\u00b7ne", "blon\u00b7de", "Lie\u00b7be\u00b7sen\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebten daran auf und nieder,", "tokens": ["Schweb\u00b7ten", "da\u00b7ran", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiegen in den Sternenhimmel,", "tokens": ["Stie\u00b7gen", "in", "den", "Ster\u00b7nen\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrten in mein Herze wieder;", "tokens": ["Kehr\u00b7ten", "in", "mein", "Her\u00b7ze", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weckten andre h\u00fcbsche Knaben,", "tokens": ["Weck\u00b7ten", "and\u00b7re", "h\u00fcb\u00b7sche", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die darinnen tr\u00e4umend schliefen", "tokens": ["Die", "da\u00b7rin\u00b7nen", "tr\u00e4u\u00b7mend", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und darauf mit ihnen spielend,", "tokens": ["Und", "da\u00b7rauf", "mit", "ih\u00b7nen", "spie\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kosend durch die Blumen liefen.", "tokens": ["Ko\u00b7send", "durch", "die", "Blu\u00b7men", "lie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die aus dem Himmel kamen,", "tokens": ["Und", "die", "aus", "dem", "Him\u00b7mel", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wollten meines Herzens Kinder", "tokens": ["Woll\u00b7ten", "mei\u00b7nes", "Her\u00b7zens", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringend mit sich aufw\u00e4rts ziehen;", "tokens": ["Rin\u00b7gend", "mit", "sich", "auf\u00b7w\u00e4rts", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber diese auch nicht minder", "tokens": ["A\u00b7ber", "die\u00b7se", "auch", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hielten stand und k\u00e4mpften wacker,", "tokens": ["Hiel\u00b7ten", "stand", "und", "k\u00e4mpf\u00b7ten", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als sie jene dicht umschlangen,", "tokens": ["Als", "sie", "je\u00b7ne", "dicht", "um\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hielten sie in meines Herzens", "tokens": ["Hiel\u00b7ten", "sie", "in", "mei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tiefstem Grunde bald gefangen!", "tokens": ["Tiefs\u00b7tem", "Grun\u00b7de", "bald", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Oben an der Himmelsleiter", "tokens": ["O\u00b7ben", "an", "der", "Him\u00b7mels\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine klare Seele schwebte,", "tokens": ["Ei\u00b7ne", "kla\u00b7re", "See\u00b7le", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die halb zornig, halb mit L\u00e4cheln", "tokens": ["Die", "halb", "zor\u00b7nig", ",", "halb", "mit", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "$,", "ADJD", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie zur\u00fcckzulocken strebte;", "tokens": ["Sie", "zu\u00b7r\u00fcck\u00b7zu\u00b7lo\u00b7cken", "streb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch es schien mir im Gef\u00e4ngnis", "tokens": ["Doch", "es", "schien", "mir", "im", "Ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihnen leidlich zu gefallen;", "tokens": ["Ih\u00b7nen", "leid\u00b7lich", "zu", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich sah, der Herrin trotzend,", "tokens": ["Denn", "ich", "sah", ",", "der", "Her\u00b7rin", "trot\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bunt sie durcheinanderwallen!", "tokens": ["Bunt", "sie", "durch\u00b7ein\u00b7an\u00b7der\u00b7wal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sie mu\u00dfte sich bequemen,", "tokens": ["Und", "sie", "mu\u00df\u00b7te", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich selbst herabzusteigen,", "tokens": ["End\u00b7lich", "selbst", "her\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sich pl\u00f6tzlich dann gefangen", "tokens": ["Sah", "sich", "pl\u00f6tz\u00b7lich", "dann", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitten in dem frohen Reigen.", "tokens": ["Mit\u00b7ten", "in", "dem", "fro\u00b7hen", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch f\u00fcr all den Liebesjubel", "tokens": ["Doch", "f\u00fcr", "all", "den", "Lie\u00b7bes\u00b7ju\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward mein Herz zu eng und nieder:", "tokens": ["Ward", "mein", "Herz", "zu", "eng", "und", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Klingend sprangen auf die Pforten,", "tokens": ["Klin\u00b7gend", "spran\u00b7gen", "auf", "die", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprangen auf die Augenlider!", "tokens": ["Spran\u00b7gen", "auf", "die", "Au\u00b7gen\u00b7li\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sieh! da standest ", "tokens": ["Sieh", "!", "da", "stan\u00b7dest"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schl\u00e4feraugen schweigsam schauend,", "tokens": ["Schl\u00e4\u00b7fe\u00b7rau\u00b7gen", "schweig\u00b7sam", "schau\u00b7end", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorgebogen, unbefangen,", "tokens": ["Vor\u00b7ge\u00b7bo\u00b7gen", ",", "un\u00b7be\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den festen Schlaf vertrauend;", "tokens": ["Auf", "den", "fes\u00b7ten", "Schlaf", "ver\u00b7trau\u00b7end", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wurdest rot und flohst vor\u00fcber,", "tokens": ["Wur\u00b7dest", "rot", "und", "flohst", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ungeschickt ein Liedlein summend", "tokens": ["Un\u00b7ge\u00b7schickt", "ein", "Lied\u00b7lein", "sum\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vergeblich dein Geheimnis", "tokens": ["Und", "ver\u00b7geb\u00b7lich", "dein", "Ge\u00b7heim\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der D\u00e4mmerung vermummend!", "tokens": ["In", "der", "D\u00e4m\u00b7me\u00b7rung", "ver\u00b7mum\u00b7mend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Fliehe nur, verratne Seele,", "tokens": ["Flie\u00b7he", "nur", ",", "ver\u00b7rat\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trostlos durch des Gartens Bl\u00fcten!", "tokens": ["Trost\u00b7los", "durch", "des", "Gar\u00b7tens", "Bl\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Such dir be\u00dfre Zauberdrachen,", "tokens": ["Such", "dir", "be\u00df\u00b7re", "Zau\u00b7ber\u00b7dra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Busens Schatz zu h\u00fcten!", "tokens": ["Dei\u00b7nes", "Bu\u00b7sens", "Schatz", "zu", "h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00f6richt Kind! nun magst du immer", "tokens": ["T\u00f6\u00b7richt", "Kind", "!", "nun", "magst", "du", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreifach mir dein Herz verschlie\u00dfen:", "tokens": ["Drei\u00b7fach", "mir", "dein", "Herz", "ver\u00b7schlie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unerbittlich seh ich innen", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "seh", "ich", "in\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr mich rote Rosen sprie\u00dfen!", "tokens": ["F\u00fcr", "mich", "ro\u00b7te", "Ro\u00b7sen", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sitzt man mit geschlo\u00dfnen Augen", "tokens": ["Sitzt", "man", "mit", "ge\u00b7schlo\u00df\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam in dem dunklen Zimmer,", "tokens": ["Ein\u00b7sam", "in", "dem", "dunk\u00b7len", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzt oft durch die zarten Lider", "tokens": ["Blitzt", "oft", "durch", "die", "zar\u00b7ten", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich roter Kerzenschimmer;", "tokens": ["Pl\u00f6tz\u00b7lich", "ro\u00b7ter", "Ker\u00b7zen\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df ich doch, da\u00df Sonnenstrahlen", "tokens": ["Wei\u00df", "ich", "doch", ",", "da\u00df", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Augendeckel dringen", "tokens": ["Durch", "die", "Au\u00b7gen\u00b7de\u00b7ckel", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in flimmernden Gebilden", "tokens": ["Und", "in", "flim\u00b7mern\u00b7den", "Ge\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich um unsre Seele schlingen.", "tokens": ["Sich", "um", "uns\u00b7re", "See\u00b7le", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Also sa\u00df ich in der D\u00e4mmrung,", "tokens": ["Al\u00b7so", "sa\u00df", "ich", "in", "der", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcd von Erdenl\u00e4rm und Staube,", "tokens": ["M\u00fcd", "von", "Er\u00b7den\u00b7l\u00e4rm", "und", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingelullt vom Abendrote,", "tokens": ["Ein\u00b7ge\u00b7lullt", "vom", "A\u00b7ben\u00b7dro\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummernd in der gr\u00fcnen Laube:", "tokens": ["Schlum\u00b7mernd", "in", "der", "gr\u00fc\u00b7nen", "Lau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da begann von Licht und Blumen", "tokens": ["Da", "be\u00b7gann", "von", "Licht", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar ein seltsam schimmernd Weben", "tokens": ["Gar", "ein", "selt\u00b7sam", "schim\u00b7mernd", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Ranken um die Augen", "tokens": ["Und", "ein", "Ran\u00b7ken", "um", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie von goldnen Zauberreben.", "tokens": ["Wie", "von", "gold\u00b7nen", "Zau\u00b7ber\u00b7re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Rote Rosen, wei\u00dfe Rosen,", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", ",", "wei\u00b7\u00dfe", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Primeln, Tulpen und Narzissen,", "tokens": ["Pri\u00b7meln", ",", "Tul\u00b7pen", "und", "Nar\u00b7zis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dahlien von hundert Farben", "tokens": ["Da\u00b7hli\u00b7en", "von", "hun\u00b7dert", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sah ich durcheinander sprie\u00dfen!", "tokens": ["Sah", "ich", "durch\u00b7ein\u00b7an\u00b7der", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Purpur, Gold, Azur und Silber", "tokens": ["Pur\u00b7pur", ",", "Gold", ",", "A\u00b7zur", "und", "Sil\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flimmerten in Wechselt\u00f6nen,", "tokens": ["Flim\u00b7mer\u00b7ten", "in", "Wech\u00b7sel\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lila, Rosa, heitres Meergr\u00fcn", "tokens": ["Li\u00b7la", ",", "Ro\u00b7sa", ",", "heit\u00b7res", "Meer\u00b7gr\u00fcn"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dften Glanz mit Glanz vers\u00f6hnen!", "tokens": ["Mu\u00df\u00b7ten", "Glanz", "mit", "Glanz", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "O das war ein pr\u00e4cht'ger Reigen,", "tokens": ["O", "das", "war", "ein", "pr\u00e4cht'\u00b7ger", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Farben all ihn tanzten,", "tokens": ["Wie", "die", "Far\u00b7ben", "all", "ihn", "tanz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Bt\u00fctenstern' und -glocken", "tokens": ["Wie", "die", "Bt\u00fc\u00b7tens\u00b7tern'", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ringelnd sich in Beete pflanzten! \u2013", "tokens": ["Rin\u00b7gelnd", "sich", "in", "Bee\u00b7te", "pflanz\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "APPR", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber in den Wundergarten", "tokens": ["A\u00b7ber", "in", "den", "Wun\u00b7der\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Senkte eine Jakobsleiter", "tokens": ["Senk\u00b7te", "ei\u00b7ne", "Ja\u00b7kobs\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von zwei Strahlen sanft sich nieder", "tokens": ["Von", "zwei", "Strah\u00b7len", "sanft", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus zwei Sternen, bl\u00e4ulich heiter!", "tokens": ["Aus", "zwei", "Ster\u00b7nen", ",", "bl\u00e4u\u00b7lich", "hei\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Kleine blonde Liebesengel", "tokens": ["Klei\u00b7ne", "blon\u00b7de", "Lie\u00b7be\u00b7sen\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebten daran auf und nieder,", "tokens": ["Schweb\u00b7ten", "da\u00b7ran", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiegen in den Sternenhimmel,", "tokens": ["Stie\u00b7gen", "in", "den", "Ster\u00b7nen\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrten in mein Herze wieder;", "tokens": ["Kehr\u00b7ten", "in", "mein", "Her\u00b7ze", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weckten andre h\u00fcbsche Knaben,", "tokens": ["Weck\u00b7ten", "and\u00b7re", "h\u00fcb\u00b7sche", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die darinnen tr\u00e4umend schliefen", "tokens": ["Die", "da\u00b7rin\u00b7nen", "tr\u00e4u\u00b7mend", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und darauf mit ihnen spielend,", "tokens": ["Und", "da\u00b7rauf", "mit", "ih\u00b7nen", "spie\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kosend durch die Blumen liefen.", "tokens": ["Ko\u00b7send", "durch", "die", "Blu\u00b7men", "lie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und die aus dem Himmel kamen,", "tokens": ["Und", "die", "aus", "dem", "Him\u00b7mel", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wollten meines Herzens Kinder", "tokens": ["Woll\u00b7ten", "mei\u00b7nes", "Her\u00b7zens", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringend mit sich aufw\u00e4rts ziehen;", "tokens": ["Rin\u00b7gend", "mit", "sich", "auf\u00b7w\u00e4rts", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber diese auch nicht minder", "tokens": ["A\u00b7ber", "die\u00b7se", "auch", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hielten stand und k\u00e4mpften wacker,", "tokens": ["Hiel\u00b7ten", "stand", "und", "k\u00e4mpf\u00b7ten", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als sie jene dicht umschlangen,", "tokens": ["Als", "sie", "je\u00b7ne", "dicht", "um\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hielten sie in meines Herzens", "tokens": ["Hiel\u00b7ten", "sie", "in", "mei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tiefstem Grunde bald gefangen!", "tokens": ["Tiefs\u00b7tem", "Grun\u00b7de", "bald", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Oben an der Himmelsleiter", "tokens": ["O\u00b7ben", "an", "der", "Him\u00b7mels\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine klare Seele schwebte,", "tokens": ["Ei\u00b7ne", "kla\u00b7re", "See\u00b7le", "schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die halb zornig, halb mit L\u00e4cheln", "tokens": ["Die", "halb", "zor\u00b7nig", ",", "halb", "mit", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "$,", "ADJD", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie zur\u00fcckzulocken strebte;", "tokens": ["Sie", "zu\u00b7r\u00fcck\u00b7zu\u00b7lo\u00b7cken", "streb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch es schien mir im Gef\u00e4ngnis", "tokens": ["Doch", "es", "schien", "mir", "im", "Ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihnen leidlich zu gefallen;", "tokens": ["Ih\u00b7nen", "leid\u00b7lich", "zu", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich sah, der Herrin trotzend,", "tokens": ["Denn", "ich", "sah", ",", "der", "Her\u00b7rin", "trot\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bunt sie durcheinanderwallen!", "tokens": ["Bunt", "sie", "durch\u00b7ein\u00b7an\u00b7der\u00b7wal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Und sie mu\u00dfte sich bequemen,", "tokens": ["Und", "sie", "mu\u00df\u00b7te", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich selbst herabzusteigen,", "tokens": ["End\u00b7lich", "selbst", "her\u00b7ab\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sich pl\u00f6tzlich dann gefangen", "tokens": ["Sah", "sich", "pl\u00f6tz\u00b7lich", "dann", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitten in dem frohen Reigen.", "tokens": ["Mit\u00b7ten", "in", "dem", "fro\u00b7hen", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch f\u00fcr all den Liebesjubel", "tokens": ["Doch", "f\u00fcr", "all", "den", "Lie\u00b7bes\u00b7ju\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ward mein Herz zu eng und nieder:", "tokens": ["Ward", "mein", "Herz", "zu", "eng", "und", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Klingend sprangen auf die Pforten,", "tokens": ["Klin\u00b7gend", "spran\u00b7gen", "auf", "die", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprangen auf die Augenlider!", "tokens": ["Spran\u00b7gen", "auf", "die", "Au\u00b7gen\u00b7li\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Sieh! da standest ", "tokens": ["Sieh", "!", "da", "stan\u00b7dest"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schl\u00e4feraugen schweigsam schauend,", "tokens": ["Schl\u00e4\u00b7fe\u00b7rau\u00b7gen", "schweig\u00b7sam", "schau\u00b7end", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorgebogen, unbefangen,", "tokens": ["Vor\u00b7ge\u00b7bo\u00b7gen", ",", "un\u00b7be\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den festen Schlaf vertrauend;", "tokens": ["Auf", "den", "fes\u00b7ten", "Schlaf", "ver\u00b7trau\u00b7end", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wurdest rot und flohst vor\u00fcber,", "tokens": ["Wur\u00b7dest", "rot", "und", "flohst", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ungeschickt ein Liedlein summend", "tokens": ["Un\u00b7ge\u00b7schickt", "ein", "Lied\u00b7lein", "sum\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und vergeblich dein Geheimnis", "tokens": ["Und", "ver\u00b7geb\u00b7lich", "dein", "Ge\u00b7heim\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der D\u00e4mmerung vermummend!", "tokens": ["In", "der", "D\u00e4m\u00b7me\u00b7rung", "ver\u00b7mum\u00b7mend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.20": {"line.1": {"text": "Fliehe nur, verratne Seele,", "tokens": ["Flie\u00b7he", "nur", ",", "ver\u00b7rat\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trostlos durch des Gartens Bl\u00fcten!", "tokens": ["Trost\u00b7los", "durch", "des", "Gar\u00b7tens", "Bl\u00fc\u0