{"dta.poem.20552": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "11.  \n  Zusammenkunft Kaiser Aleranders mit K\u00f6nig  \n Friedrich Wilhelm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Das Dorf hat wenig H&#228;user,", "tokens": ["Das", "Dorf", "hat", "we\u00b7nig", "H", "&#228;", "u\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "XY", "$(", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bei Oels liegt\u2019s, hei\u00dft Stapitz.", "tokens": ["Bei", "O\u00b7els", "liegt's", ",", "hei\u00dft", "Sta\u00b7pitz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorthin kam Ru\u00dflands Kaiser.", "tokens": ["Dor\u00b7thin", "kam", "Ru\u00df\u00b7lands", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Selbst f\u00fchrt ihn hin zum Sitz;", "tokens": ["Selbst", "f\u00fchrt", "ihn", "hin", "zum", "Sitz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sa\u00df neben ihm der K\u00f6nig", "tokens": ["Sa\u00df", "ne\u00b7ben", "ihm", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Preu\u00dfens, reicht ihm die Hand. \u2014", "tokens": ["Preu\u00b7\u00dfens", ",", "reicht", "ihm", "die", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Jhr Zwiegespr\u00e4ch erw\u00e4hn\u2019 ich", "tokens": ["Ihr", "Zwie\u00b7ge\u00b7spr\u00e4ch", "er\u00b7w\u00e4hn'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht, es blieb unbekannt. \u2014", "tokens": ["Nicht", ",", "es", "blieb", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "$,", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wohl aber mag man denken,", "tokens": ["Wohl", "a\u00b7ber", "mag", "man", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Was Beider Freundschaft sprach.", "tokens": ["Was", "Bei\u00b7der", "Freund\u00b7schaft", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die Blicke mochten lenken", "tokens": ["Die", "Bli\u00b7cke", "moch\u00b7ten", "len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sie auf Europa\u2019s Schmach.", "tokens": ["Sie", "auf", "Eu\u00b7ro\u00b7pa's", "Schmach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Hier sprach kein falsches Glei\u00dfen,", "tokens": ["Hier", "sprach", "kein", "fal\u00b7sches", "Glei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wort drang von Mann zu Mann!", "tokens": ["Wort", "drang", "von", "Mann", "zu", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dem alten Freund bot Preu\u00dfen", "tokens": ["Dem", "al\u00b7ten", "Freund", "bot", "Preu\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die alte Freundschaft an;", "tokens": ["Die", "al\u00b7te", "Freund\u00b7schaft", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Eilt, statt erst lang zu kl\u00fcgeln,", "tokens": ["Eilt", ",", "statt", "erst", "lang", "zu", "kl\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Den Bund neu zu besiegeln.", "tokens": ["Den", "Bund", "neu", "zu", "be\u00b7sie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Denn Ru\u00dflands Pr\u00fcfestunden", "tokens": ["Denn", "Ru\u00df\u00b7lands", "Pr\u00fc\u00b7fes\u00b7tun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Hat Preu\u00dfen mit empfunden,", "tokens": ["Hat", "Preu\u00b7\u00dfen", "mit", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Des Drangsals tr\u00fcbe Nacht,", "tokens": ["Des", "Drangs\u00b7als", "tr\u00fc\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Was immer Ru\u00dfland litt,", "tokens": ["Was", "im\u00b7mer", "Ru\u00df\u00b7land", "litt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Die Opfer, die\u2019s gebracht,", "tokens": ["Die", "Op\u00b7fer", ",", "die's", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Preu\u00dfen f\u00fchlt tief sie mit.", "tokens": ["Preu\u00b7\u00dfen", "f\u00fchlt", "tief", "sie", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.25": {"text": "Allein ri\u00df Preu\u00dfen los", "tokens": ["Al\u00b7lein", "ri\u00df", "Preu\u00b7\u00dfen", "los"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Sich aus dem Herrscherschoo\u00df", "tokens": ["Sich", "aus", "dem", "Herr\u00b7scher\u00b7schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Frankreichs, vor allen M\u00e4chten", "tokens": ["Fran\u00b7kreichs", ",", "vor", "al\u00b7len", "M\u00e4ch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Zuerst bereit zu fechten.", "tokens": ["Zu\u00b7erst", "be\u00b7reit", "zu", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Sein edler K\u00f6nig war", "tokens": ["Sein", "ed\u00b7ler", "K\u00f6\u00b7nig", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Der Held, dem immerdar", "tokens": ["Der", "Held", ",", "dem", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Herz, das in Brust er trug,", "tokens": ["Herz", ",", "das", "in", "Brust", "er", "trug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.32": {"text": "F\u00fcr wahre Ehre schlug! \u2014", "tokens": ["F\u00fcr", "wah\u00b7re", "Eh\u00b7re", "schlug", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Ru\u00dfland vergi\u00df drum nicht", "tokens": ["Ru\u00df\u00b7land", "ver\u00b7gi\u00df", "drum", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PAV", "PTKNEG"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.34": {"text": "Des Dankes heil\u2019ge Pflicht!", "tokens": ["Des", "Dan\u00b7kes", "heil'\u00b7ge", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}