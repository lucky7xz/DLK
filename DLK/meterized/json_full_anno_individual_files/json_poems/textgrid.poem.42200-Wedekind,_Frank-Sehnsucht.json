{"textgrid.poem.42200": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und sei mir noch so traurig auch zu Sinn,", "tokens": ["Und", "sei", "mir", "noch", "so", "trau\u00b7rig", "auch", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich will's nicht glauben, da\u00df ich elend bin.", "tokens": ["Ich", "will's", "nicht", "glau\u00b7ben", ",", "da\u00df", "ich", "e\u00b7lend", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Fluch, das Leid, das mich zugrund gerichtet,", "tokens": ["Der", "Fluch", ",", "das", "Leid", ",", "das", "mich", "zu\u00b7grund", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Ende war ja alles nur erdichtet.", "tokens": ["Am", "En\u00b7de", "war", "ja", "al\u00b7les", "nur", "er\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Phantasie treibt oft ihr Possenspiel.", "tokens": ["Die", "Phan\u00b7ta\u00b7sie", "treibt", "oft", "ihr", "Pos\u00b7sen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schon manchen hob sie, der zu Boden fiel,", "tokens": ["Schon", "man\u00b7chen", "hob", "sie", ",", "der", "zu", "Bo\u00b7den", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Geist empor. Schon manchen aus den H\u00f6hen", "tokens": ["Im", "Geist", "em\u00b7por", ".", "Schon", "man\u00b7chen", "aus", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "ADV", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Himmels lie\u00df sie Schreck und Unheil sehen.", "tokens": ["Des", "Him\u00b7mels", "lie\u00df", "sie", "Schreck", "und", "Un\u00b7heil", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df ab von mir, du gro\u00dfe Zauberin!", "tokens": ["La\u00df", "ab", "von", "mir", ",", "du", "gro\u00b7\u00dfe", "Zau\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erbarm dich mein, entschleire meinen Sinn!", "tokens": ["Er\u00b7barm", "dich", "mein", ",", "ent\u00b7schlei\u00b7re", "mei\u00b7nen", "Sinn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerteil die Nacht, mit der du mich geschlagen \u2013", "tokens": ["Zer\u00b7teil", "die", "Nacht", ",", "mit", "der", "du", "mich", "ge\u00b7schla\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Sonnenglanz des Gl\u00fccks, wann wirst du tagen!", "tokens": ["O", "Son\u00b7nen\u00b7glanz", "des", "Gl\u00fccks", ",", "wann", "wirst", "du", "ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sei mir noch so traurig auch zu Sinn,", "tokens": ["Und", "sei", "mir", "noch", "so", "trau\u00b7rig", "auch", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich will's nicht glauben, da\u00df ich elend bin.", "tokens": ["Ich", "will's", "nicht", "glau\u00b7ben", ",", "da\u00df", "ich", "e\u00b7lend", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Fluch, das Leid, das mich zugrund gerichtet,", "tokens": ["Der", "Fluch", ",", "das", "Leid", ",", "das", "mich