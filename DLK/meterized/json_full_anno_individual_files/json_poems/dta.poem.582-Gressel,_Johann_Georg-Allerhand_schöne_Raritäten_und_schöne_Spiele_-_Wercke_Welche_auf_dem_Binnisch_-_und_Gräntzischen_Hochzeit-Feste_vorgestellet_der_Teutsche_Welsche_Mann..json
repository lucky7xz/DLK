{"dta.poem.582": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Seht wie der ", "tokens": ["Seht", "wie", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Mit dem ", "tokens": ["Mit", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Hier kegeln ", "tokens": ["Hier", "ke\u00b7geln"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und fangen allesamt von Hertzen an zu lachen.", "tokens": ["Und", "fan\u00b7gen", "al\u00b7le\u00b7samt", "von", "Hert\u00b7zen", "an", "zu", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun h\u00f6ret die ", "tokens": ["Nun", "h\u00f6\u00b7ret", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens": ["Sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4t", "/", "sch\u00f6\u00b7ne", "Spie\u00b7le\u00b7\u00b7Wer\u00b7ckerc", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}}}}