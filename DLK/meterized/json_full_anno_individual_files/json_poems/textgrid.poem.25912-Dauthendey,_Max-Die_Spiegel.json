{"textgrid.poem.25912": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum werden Spiegel im Alter matt?", "tokens": ["Wa\u00b7rum", "wer\u00b7den", "Spie\u00b7gel", "im", "Al\u00b7ter", "matt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Weil jeder ma\u00dflos genossen hat.", "tokens": ["Weil", "je\u00b7der", "ma\u00df\u00b7los", "ge\u00b7nos\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Denn sind sie gl\u00fccklich, die sch\u00f6nen Frauen,", "tokens": ["Denn", "sind", "sie", "gl\u00fcck\u00b7lich", ",", "die", "sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tun sie geschwind zu dem Spiegel schauen.", "tokens": ["Tun", "sie", "ge\u00b7schwind", "zu", "dem", "Spie\u00b7gel", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und mu\u00dften zarte Frauen mal weinen,", "tokens": ["Und", "mu\u00df\u00b7ten", "zar\u00b7te", "Frau\u00b7en", "mal", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Trocknen am Spiegel die Tr\u00e4nen, die feinen.", "tokens": ["Trock\u00b7nen", "am", "Spie\u00b7gel", "die", "Tr\u00e4\u00b7nen", ",", "die", "fei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Immer m\u00fcssen die heimlichsten Frauen", "tokens": ["Im\u00b7mer", "m\u00fcs\u00b7sen", "die", "heim\u00b7lichs\u00b7ten", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Herzensw\u00fcnsche dem Spiegel vertrauen.", "tokens": ["Her\u00b7zens\u00b7w\u00fcn\u00b7sche", "dem", "Spie\u00b7gel", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Wie oft habe ich einen Spiegel beneidet,", "tokens": ["Wie", "oft", "ha\u00b7be", "ich", "ei\u00b7nen", "Spie\u00b7gel", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Weil meine Liebste sich an ihm weidet!", "tokens": ["Weil", "mei\u00b7ne", "Liebs\u00b7te", "sich", "an", "ihm", "wei\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wie oft habe ich ihren Spiegel verflucht,", "tokens": ["Wie", "oft", "ha\u0