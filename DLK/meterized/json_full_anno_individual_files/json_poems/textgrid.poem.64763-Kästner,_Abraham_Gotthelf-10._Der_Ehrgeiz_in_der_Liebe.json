{"textgrid.poem.64763": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "10. Der Ehrgeiz in der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst du, mein Herze zu bestreiten:", "tokens": ["Kommst", "du", ",", "mein", "Her\u00b7ze", "zu", "be\u00b7strei\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So komm, o Liebe, nicht allein,", "tokens": ["So", "komm", ",", "o", "Lie\u00b7be", ",", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df edlen Ehrgeiz dich begleiten,", "tokens": ["La\u00df", "ed\u00b7len", "Ehr\u00b7geiz", "dich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihn nimmst du vielleicht mich ein.", "tokens": ["Durch", "ihn", "nimmst", "du", "viel\u00b7leicht", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Zu sanfter Triebe matte Lust", "tokens": ["Zu", "sanf\u00b7ter", "Trie\u00b7be", "mat\u00b7te", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verachtet meine muntre Brust.", "tokens": ["Ver\u00b7ach\u00b7tet", "mei\u00b7ne", "mun\u00b7tre", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn nur ein unschuldsvoller Knabe,", "tokens": ["Wenn", "nur", "ein", "un\u00b7schulds\u00b7vol\u00b7ler", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den erst mein Auge lieben lehrt,", "tokens": ["Den", "erst", "mein", "Au\u00b7ge", "lie\u00b7ben", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie tief ich ihn verwundet habe,", "tokens": ["Wie", "tief", "ich", "ihn", "ver\u00b7wun\u00b7det", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir stammelnd und voll Furcht erkl\u00e4rt:", "tokens": ["Mir", "stam\u00b7melnd", "und", "voll", "Furcht", "er\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So scheint er nur f\u00fcr mich entbrannt,", "tokens": ["So", "scheint", "er", "nur", "f\u00fcr", "mich", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil er nichts Sch\u00f6nes noch gekannt.", "tokens