{"textgrid.poem.67840": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12. Morgengesang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, horch die Lerch' am Himm'lsthor singt;", "tokens": ["Horch", ",", "horch", "die", "Ler\u00b7ch'", "am", "Him\u00b7m'lst\u00b7hor", "singt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Die liebe Sonn wacht auf!", "tokens": ["Die", "lie\u00b7be", "Sonn", "wacht", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von allen Blumenkelchen trinkt", "tokens": ["Von", "al\u00b7len", "Blu\u00b7men\u00b7kel\u00b7chen", "trinkt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schon ihr Opfer auf.", "tokens": ["Sie", "schon", "ihr", "Op\u00b7fer", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Hochzeitkn\u00f6spchen freundlich winkt,", "tokens": ["Das", "Hoch\u00b7zeit\u00b7kn\u00f6spc\u00b7hen", "freund\u00b7lich", "winkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und thut sein' Aeuglein auf;", "tokens": ["Und", "thut", "sein'", "A\u00b7e\u00b7u\u00b7glein", "auf", ";"], "token_info": ["word", "word", "word", "word",