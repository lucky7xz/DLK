{"textgrid.poem.42609": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bertran de Born", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Droben auf dem schroffen Steine", "tokens": ["Dro\u00b7ben", "auf", "dem", "schrof\u00b7fen", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Raucht in Tr\u00fcmmern Autafort,", "tokens": ["Raucht", "in", "Tr\u00fcm\u00b7mern", "Au\u00b7ta\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Burgherr steht gefesselt", "tokens": ["Und", "der", "Burg\u00b7herr", "steht", "ge\u00b7fes\u00b7selt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Vor des K\u00f6nigs Zelte dort:", "tokens": ["Vor", "des", "K\u00f6\u00b7nigs", "Zel\u00b7te", "dort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbkamst du, der mit Schwert und Liedern", "tokens": ["\u00bb", "kamst", "du", ",", "der", "mit", "Schwert", "und", "Lie\u00b7dern"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aufruhr trug von Ort zu Ort,", "tokens": ["Auf\u00b7ruhr", "trug", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der die Kinder aufgewiegelt", "tokens": ["Der", "die", "Kin\u00b7der", "auf\u00b7ge\u00b7wie\u00b7gelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gegen ihres Vaters Wort?", "tokens": ["Ge\u00b7gen", "ih\u00b7res", "Va\u00b7ters", "Wort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steht vor mir, der sich ger\u00fchmet", "tokens": ["Steht", "vor", "mir", ",", "der", "sich", "ge\u00b7r\u00fch\u00b7met"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PRELS", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In verme\u00dfner Prahlerei,", "tokens": ["In", "ver\u00b7me\u00df\u00b7ner", "Prah\u00b7le\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihm nie mehr als die H\u00e4lfte", "tokens": ["Da\u00df", "ihm", "nie", "mehr", "als", "die", "H\u00e4lf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Geistes n\u00f6tig sei?", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "n\u00f6\u00b7tig", "sei", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun der halbe dich nicht rettet,", "tokens": ["Nun", "der", "hal\u00b7be", "dich", "nicht", "ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruf den ganzen doch herbei,", "tokens": ["Ruf", "den", "gan\u00b7zen", "doch", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er neu dein Schlo\u00df dir baue,", "tokens": ["Da\u00df", "er", "neu", "dein", "Schlo\u00df", "dir", "bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Ketten brech entzwei!\u00ab", "tokens": ["Dei\u00b7ne", "Ket\u00b7ten", "brech", "ent\u00b7zwei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwie du sagst, mein Herr und K\u00f6nig!", "tokens": ["\u00bb", "wie", "du", "sagst", ",", "mein", "Herr", "und", "K\u00f6\u00b7nig", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht vor dir Bertran de Born,", "tokens": ["Steht", "vor", "dir", "Ber\u00b7tran", "de", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NE", "NE", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der mit einem Lied entflammte", "tokens": ["Der", "mit", "ei\u00b7nem", "Lied", "ent\u00b7flamm\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Perigord und Ventadorn,", "tokens": ["Pe\u00b7ri\u00b7gord", "und", "Ven\u00b7ta\u00b7dorn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der dem m\u00e4chtigen Gebieter", "tokens": ["Der", "dem", "m\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7bie\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stets im Auge war ein Dorn,", "tokens": ["Stets", "im", "Au\u00b7ge", "war", "ein", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem zuliebe K\u00f6nigskinder", "tokens": ["Dem", "zu\u00b7lie\u00b7be", "K\u00f6\u00b7nigs\u00b7kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trugen ihres Vaters Zorn.", "tokens": ["Tru\u00b7gen", "ih\u00b7res", "Va\u00b7ters", "Zorn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Tochter sa\u00df im Saale,", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "sa\u00df", "im", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festlich, eines Herzogs Braut,", "tokens": ["Fest\u00b7lich", ",", "ei\u00b7nes", "Her\u00b7zogs", "Braut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da sang vor ihr mein Bote,", "tokens": ["Und", "da", "sang", "vor", "ihr", "mein", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dem ein Lied ich anvertraut,", "tokens": ["Dem", "ein", "Lied", "ich", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sang, was einst ihr Stolz gewesen,", "tokens": ["Sang", ",", "was", "einst", "ihr", "Stolz", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihres Dichters Sehnsuchtlaut,", "tokens": ["Ih\u00b7res", "Dich\u00b7ters", "Sehn\u00b7sucht\u00b7laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ihr leuchtend Brautgeschmeide", "tokens": ["Bis", "ihr", "leuch\u00b7tend", "Braut\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ganz von Tr\u00e4nen war betaut.", "tokens": ["Ganz", "von", "Tr\u00e4\u00b7nen", "war", "be\u00b7taut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus des \u00d6lbaums Schlummerschatten", "tokens": ["Aus", "des", "\u00d6l\u00b7baums", "Schlum\u00b7mer\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fuhr dein bester Sohn empor,", "tokens": ["Fuhr", "dein", "bes\u00b7ter", "Sohn", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als mit zorn'gen Schlachtges\u00e4ngen", "tokens": ["Als", "mit", "zorn'\u00b7gen", "Schlacht\u00b7ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich best\u00fcrmen lie\u00df sein Ohr.", "tokens": ["Ich", "be\u00b7st\u00fcr\u00b7men", "lie\u00df", "sein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schnell war ihm das Ro\u00df geg\u00fcrtet,", "tokens": ["Schnell", "war", "ihm", "das", "Ro\u00df", "ge\u00b7g\u00fcr\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und ich trug das Banner vor,", "tokens": ["Und", "ich", "trug", "das", "Ban\u00b7ner", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jenem Todespfeil entgegen,", "tokens": ["Je\u00b7nem", "To\u00b7des\u00b7pfeil", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der ihn traf vor Montforts Tor.", "tokens": ["Der", "ihn", "traf", "vor", "Mont\u00b7forts", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blutend lag er mir im Arme;", "tokens": ["Blu\u00b7tend", "lag", "er", "mir", "im", "Ar\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht der scharfe, kalte Stahl \u2013", "tokens": ["Nicht", "der", "schar\u00b7fe", ",", "kal\u00b7te", "Stahl", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er sterb in deinem Fluche,", "tokens": ["Da\u00df", "er", "sterb", "in", "dei\u00b7nem", "Flu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das war seines Sterbens Qual.", "tokens": ["Das", "war", "sei\u00b7nes", "Ster\u00b7bens", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strecken wollt er dir die Rechte", "tokens": ["Stre\u00b7cken", "wollt", "er", "dir", "die", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber Meer, Gebirg und Tal,", "tokens": ["\u00dc\u00b7ber", "Meer", ",", "Ge\u00b7birg", "und", "Tal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als er deine nicht erreichet,", "tokens": ["Als", "er", "dei\u00b7ne", "nicht", "er\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcckt' er meine noch einmal.", "tokens": ["Dr\u00fcckt'", "er", "mei\u00b7ne", "noch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da, wie Autafort dort oben,", "tokens": ["Da", ",", "wie", "Au\u00b7ta\u00b7fort", "dort", "o\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward gebrochen meine Kraft;", "tokens": ["Ward", "ge\u00b7bro\u00b7chen", "mei\u00b7ne", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht die ganze, nicht die halbe", "tokens": ["Nicht", "die", "gan\u00b7ze", ",", "nicht", "die", "hal\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "$,", "PTKNEG", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb mir, Saite nicht noch Schaft.", "tokens": ["Blieb", "mir", ",", "Sai\u00b7te", "nicht", "noch", "Schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PTKNEG", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leicht hast du den Arm gebunden,", "tokens": ["Leicht", "hast", "du", "den", "Arm", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seit der Geist mir liegt in Haft;", "tokens": ["Seit", "der", "Geist", "mir", "liegt", "in", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur zu einem Trauerliede", "tokens": ["Nur", "zu", "ei\u00b7nem", "Trau\u00b7er\u00b7lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat er sich noch aufgerafft.\u00ab", "tokens": ["Hat", "er", "sich", "noch", "auf\u00b7ge\u00b7rafft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der K\u00f6nig senkt die Stirne:", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "senkt", "die", "Stir\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u00bbmeinen Sohn hast du verf\u00fchrt,", "tokens": ["\u00bb", "mei\u00b7nen", "Sohn", "hast", "du", "ver\u00b7f\u00fchrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"