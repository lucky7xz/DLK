: ["Blu\u00b7tend", "lag", "er", "mir", "im", "Ar\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht der scharfe, kalte Stahl \u2013", "tokens": ["Nicht", "der", "schar\u00b7fe", ",", "kal\u00b7te", "Stahl", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er sterb in deinem Fluche,", "tokens": ["Da\u00df", "er", "sterb", "in", "dei\u00b7nem", "Flu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das war seines Sterbens Qual.", "tokens": ["Das", "war", "sei\u00b7nes", "Ster\u00b7bens", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strecken wollt er dir die Rechte", "tokens": ["Stre\u00b7cken", "wollt", "er", "dir", "die", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber Meer, Gebirg und Tal,", "tokens": ["\u00dc\u00b7ber", "Meer", ",", "Ge\u00b7birg", "und", "Tal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als er deine nicht erreichet,", "tokens": ["Als", "er", "dei\u00b7ne", "nicht", "er\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcckt' er meine noch einmal.", "tokens": ["Dr\u00fcckt'", "er", "mei\u00b7ne", "noch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Da, wie Autafort dort oben,", "tokens": ["Da", ",", "wie", "Au\u00b7ta\u00b7fort", "dort", "o\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward gebrochen meine Kraft;", "tokens": ["Ward", "ge\u00b7bro\u00b7chen", "mei\u00b7ne", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht die ganze, nicht die halbe", "tokens": ["Nicht", "die", "gan\u00b7ze", ",", "nicht", "die", "hal\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "$,", "PTKNEG", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb mir, Saite nicht noch Schaft.", "tokens": ["Blieb", "mir", ",", "Sai\u00b7te", "nicht", "noch", "Schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PTKNEG", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leicht hast du den Arm gebunden,", "tokens": ["Leicht", "hast", "du", "den", "Arm", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seit der Geist mir liegt in Haft;", "tokens": ["Seit", "der", "Geist", "mir", "liegt", "in", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur zu einem Trauerliede", "tokens": ["Nur", "zu", "ei\u00b7nem", "Trau\u00b7er\u00b7lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat er sich noch aufgerafft.\u00ab", "tokens": ["Hat", "er", "sich", "noch", "auf\u00b7ge\u00b7rafft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und der K\u00f6nig senkt die Stirne:", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "senkt", "die", "Stir\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u00bbmeinen Sohn hast du verf\u00fchrt,", "tokens": ["\u00bb", "mei\u00b7nen", "Sohn", "hast", "du", "ver\u00b7f\u00fchrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast der Tochter Herz verzaubert,", "tokens": ["Hast", "der", "Toch\u00b7ter", "Herz", "ver\u00b7zau\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast auch meines nun ger\u00fchrt.", "tokens": ["Hast", "auch", "mei\u00b7nes", "nun", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm die Hand, du Freund des Toten!", "tokens": ["Nimm", "die", "Hand", ",", "du", "Freund", "des", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die verzeihend ihm geb\u00fchrt.", "tokens": ["Die", "ver\u00b7zei\u00b7hend", "ihm", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weg die Fesseln! Deines Geistes", "tokens": ["Weg", "die", "Fes\u00b7seln", "!", "Dei\u00b7nes", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hab ich einen Hauch versp\u00fcrt.\u00ab", "tokens": ["Hab", "ich", "ei\u00b7nen", "Hauch", "ver\u00b7sp\u00fcrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}