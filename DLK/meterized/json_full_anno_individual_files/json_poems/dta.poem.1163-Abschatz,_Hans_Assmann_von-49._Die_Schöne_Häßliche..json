{"dta.poem.1163": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "49. Die Sch\u00f6ne H\u00e4\u00dfliche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wo nehm' ich Farben her/ dich zierlich auszuschm\u00fccken? ", "tokens": ["Wo", "nehm'", "ich", "Far\u00b7ben", "her", "/", "dich", "zier\u00b7lich", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "PTKVZ", "$(", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo find\u2019 ich Worte/ die genugsam kr\u00e4fftig seyn/", "tokens": ["Wo", "find'", "ich", "Wor\u00b7te", "/", "die", "ge\u00b7nug\u00b7sam", "kr\u00e4ff\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$(", "ART", "ADJD", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schwachen Geister Schaar von dir zu bilden ein", "tokens": ["Der", "schwa\u00b7chen", "Geis\u00b7ter", "Schaar", "von", "dir", "zu", "bil\u00b7den", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPER", "PTKZU", "VVINF", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sch\u00f6nheit/ die sie nicht mit bl\u00f6dem Aug\u2019 erblicken?", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "die", "sie", "nicht", "mit", "bl\u00f6\u00b7dem", "Aug'", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie stellen M\u00e4ngel aus an Nase/ Schos und R\u00fccken:", "tokens": ["Sie", "stel\u00b7len", "M\u00e4n\u00b7gel", "aus", "an", "Na\u00b7se", "/", "Schos", "und", "R\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "APPR", "NN", "$(", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ist nicht lang genung/ die schwartz und jene klein/", "tokens": ["Der", "ist", "nicht", "lang", "ge\u00b7nung", "/", "die", "schwartz", "und", "je\u00b7ne", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "ADV", "$(", "ART", "ADJD", "KON", "PDS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was die Sp\u00f6tter mehr zu deinem Schimpff ausschreyn;", "tokens": ["Und", "was", "die", "Sp\u00f6t\u00b7ter", "mehr", "zu", "dei\u00b7nem", "Schimpff", "aus\u00b7schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dennoch m\u00fcssen sich f\u00fcr dir viel Hertzen b\u00fccken.", "tokens": ["Und", "den\u00b7noch", "m\u00fcs\u00b7sen", "sich", "f\u00fcr", "dir", "viel", "Hert\u00b7zen", "b\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PRF", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein\u2019 unbekannte Krafft/ di\u00df Blicken/ dieser Geist/", "tokens": ["Ein'", "un\u00b7be\u00b7kann\u00b7te", "Krafft", "/", "di\u00df", "Bli\u00b7cken", "/", "die\u00b7ser", "Geist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "NN", "$(", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein uns verborgner Zug/ den ich nicht nennen kan", "tokens": ["Ein", "uns", "ver\u00b7borg\u00b7ner", "Zug", "/", "den", "ich", "nicht", "nen\u00b7nen", "kan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN", "$(", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und dennoch f\u00fchl\u2019/ ists/ der die Hertzen zu dir reist.", "tokens": ["Und", "den\u00b7noch", "f\u00fchl'", "/", "ists", "/", "der", "die", "Hert\u00b7zen", "zu", "dir", "reist", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$(", "VAFIN", "$(", "ART", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der finstere Magnet zieht blanckes Eisen an/", "tokens": ["Der", "fins\u00b7te\u00b7re", "Mag\u00b7net", "zieht", "blan\u00b7ckes", "Ei\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Aus schwartzen Mumien/ aus Gifft-gef\u00fcrchten Sachen", "tokens": ["Aus", "schwart\u00b7zen", "Mu\u00b7mi\u00b7en", "/", "aus", "Gifft\u00b7ge\u00b7f\u00fcrch\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kan Klugheit und Verstand ein heilsam Labsal machen.", "tokens": ["Kan", "Klug\u00b7heit", "und", "Ver\u00b7stand", "ein", "heil\u00b7sam", "Lab\u00b7sal", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}