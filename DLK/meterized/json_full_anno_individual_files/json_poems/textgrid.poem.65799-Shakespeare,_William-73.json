{"textgrid.poem.65799": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "73", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, kaum mit wenig gelbem Laub behangen,", "tokens": ["Wenn", ",", "kaum", "mit", "we\u00b7nig", "gel\u00b7bem", "Laub", "be\u00b7han\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Zweige zittern in der Fr\u00f6ste Wehn,", "tokens": ["Die", "Zwei\u00b7ge", "zit\u00b7tern", "in", "der", "Fr\u00f6s\u00b7te", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verfallnen Ch\u00f6ren gleich, wo einst die V\u00f6gel sangen.", "tokens": ["Ver\u00b7fall\u00b7nen", "Ch\u00f6\u00b7ren", "gleich", ",", "wo", "einst", "die", "V\u00f6\u00b7gel", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein solches D\u00e4mmerlicht stell' ich dir vor,", "tokens": ["Ein", "sol\u00b7ches", "D\u00e4m\u00b7mer\u00b7licht", "stell'", "ich", "dir", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie, wenn die Sonne sank, im Westen bleichet;", "tokens": ["Wie", ",", "wenn", "die", "Son\u00b7ne", "sank", ",", "im", "Wes\u00b7ten", "blei\u00b7chet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Allm\u00e4hlich h\u00fcllt's die Nacht in tr\u00fcben Flor,", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "h\u00fcllt's", "die", "Nacht", "in", "tr\u00fc\u00b7ben", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Todes Schein, der alles Leben scheuchet.", "tokens": ["In", "To\u00b7des", "Schein", ",", "der", "al\u00b7les", "Le\u00b7ben", "scheu\u00b7chet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du siehst in mir des Feuers \u00dcberdru\u00df,", "tokens": ["Du", "siehst", "in", "mir", "des", "Feu\u00b7ers", "\u00dc\u00b7berd\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das auf der Asche seiner Jugend liegt", "tokens": ["Das", "auf", "der", "A\u00b7sche", "sei\u00b7ner", "Ju\u00b7gend", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie auf dem Todbett, wo es sterben mu\u00df,", "tokens": ["Wie", "auf", "dem", "Tod\u00b7bett", ",", "wo", "es", "ster\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "w