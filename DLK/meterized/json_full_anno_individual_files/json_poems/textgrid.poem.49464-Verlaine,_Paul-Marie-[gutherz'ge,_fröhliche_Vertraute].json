{"textgrid.poem.49464": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[gutherz'ge, fr\u00f6hliche Vertraute]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gutherz'ge, fr\u00f6hliche Vertraute,", "tokens": ["Gut\u00b7her\u00b7z'\u00b7ge", ",", "fr\u00f6h\u00b7li\u00b7che", "Ver\u00b7trau\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf die ich meines Herzens Ruh,", "tokens": ["Auf", "die", "ich", "mei\u00b7nes", "Her\u00b7zens", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf die ich ganz mein Leben baute,", "tokens": ["Auf", "die", "ich", "ganz", "mein", "Le\u00b7ben", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein letzter, einz'ger Zeuge du,", "tokens": ["Mein", "letz\u00b7ter", ",", "einz'\u00b7ger", "Zeu\u00b7ge", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, Schatz, dass ich dich an mich dr\u00fccke,", "tokens": ["Komm", ",", "Schatz", ",", "dass", "ich", "dich", "an", "mich", "dr\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du k\u00fcsse lang und innig mich,", "tokens": ["Du", "k\u00fcs\u00b7se", "lang", "und", "in\u00b7nig", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So schl\u00e4gt mein Herz in heiterm Gl\u00fccke", "tokens": ["So", "schl\u00e4gt", "mein", "Herz", "in", "hei\u00b7term", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Liebe bis zum Tod f\u00fcr dich!", "tokens": ["Und", "Lie\u00b7be", "bis", "zum", "Tod", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Liebe mich,", "tokens": ["Lie\u00b7be", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Ohne dich", "tokens": ["Oh\u00b7ne", "dich"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ist dahin,", "tokens": ["Ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Was ich bin.", "tokens": ["Was", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dein Reichtum, Kind, sind deine H\u00e4nde,", "tokens": ["Dein", "Reich\u00b7tum", ",", "Kind", ",", "sind", "dei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich bin wie 'ne Kirchenmaus;", "tokens": ["Und", "ich", "bin", "wie", "'ne", "Kir\u00b7chen\u00b7maus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit unserm Tisch sieht's tr\u00fcb am Ende", "tokens": ["Mit", "un\u00b7serm", "Tisch", "sieht's", "tr\u00fcb", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Dachverschlag und Keller aus,", "tokens": ["In", "Dach\u00b7ver\u00b7schlag", "und", "Kel\u00b7ler", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch unser Lager, unser weiches,", "tokens": ["Doch", "un\u00b7ser", "La\u00b7ger", ",", "un\u00b7ser", "wei\u00b7ches", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Fehlt nie, vergn\u00fcgt und dauerhaft,", "tokens": ["Fehlt", "nie", ",", "ver\u00b7gn\u00fcgt", "und", "dau\u00b7er\u00b7haft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und K\u00f6nig ich des K\u00f6nigreiches,", "tokens": ["Und", "K\u00f6\u00b7nig", "ich", "des", "K\u00f6\u00b7nig\u00b7rei\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "All deiner Heiterkeit und Kraft.", "tokens": ["All", "dei\u00b7ner", "Hei\u00b7ter\u00b7keit", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Liebe mich,", "tokens": ["Lie\u00b7be", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Ohne dich", "tokens": ["Oh\u00b7ne", "dich"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ist dahin,", "tokens": ["Ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Was ich bin.", "tokens": ["Was", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Aus der Umarmung unsrer N\u00e4chte", "tokens": ["Aus", "der", "Um\u00b7ar\u00b7mung", "uns\u00b7rer", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebe ich mich neugest\u00e4hlt,", "tokens": ["Er\u00b7he\u00b7be", "ich", "mich", "neu\u00b7ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn deine S\u00fcsse ist das rechte,", "tokens": ["Denn", "dei\u00b7ne", "S\u00fcs\u00b7se", "ist", "das", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da meinem armen Fleisch gefehlt.", "tokens": ["Da", "mei\u00b7nem", "ar\u00b7men", "Fleisch", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Kuss giesst Mut gleich starkem Weine", "tokens": ["Dein", "Kuss", "giesst", "Mut", "gleich", "star\u00b7kem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In meine Brust mit Lieb und Scherz,", "tokens": ["In", "mei\u00b7ne", "Brust", "mit", "Lieb", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du kennst die Kunst, ja, du alleine,", "tokens": ["Du", "kennst", "die", "Kunst", ",", "ja", ",", "du", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PTKANT", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu schwellen mir ein g\u00f6ttlich Herz.", "tokens": ["Zu", "schwel\u00b7len", "mir", "ein", "g\u00f6tt\u00b7lich", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Liebe mich,", "tokens": ["Lie\u00b7be", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Ohne dich", "tokens": ["Oh\u00b7ne", "dich"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ist dahin,", "tokens": ["Ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Was ich bin.", "tokens": ["Was", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Was schadet, Schatz, dein einst'ges Leben?", "tokens": ["Was", "scha\u00b7det", ",", "Schatz", ",", "dein", "einst'\u00b7ges", "Le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was k\u00fcmmert wohl das meine mich?", "tokens": ["Was", "k\u00fcm\u00b7mert", "wohl", "das", "mei\u00b7ne", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin dir treu in Lieb ergeben,", "tokens": ["Ich", "bin", "dir", "treu", "in", "Lieb", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Gutes tatest du f\u00fcr mich.", "tokens": ["Nur", "Gu\u00b7tes", "ta\u00b7test", "du", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lass im Umarmen uns vergessen,", "tokens": ["Lass", "im", "Um\u00b7ar\u00b7men", "uns", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dass man uns Armen nicht verzieh,", "tokens": ["Dass", "man", "uns", "Ar\u00b7men", "nicht", "ver\u00b7zieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Lass gl\u00fchend Herz an Herz uns pressen,", "tokens": ["Lass", "gl\u00fc\u00b7hend", "Herz", "an", "Herz", "uns", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was schert die Welt uns? \u2013 H\u00f6hne sie!", "tokens": ["Was", "schert", "die", "Welt", "uns", "?", "\u2013", "H\u00f6h\u00b7ne", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$.", "$(", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Liebe mich,", "tokens": ["Lie\u00b7be", "mich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Ohne dich", "tokens": ["Oh\u00b7ne", "dich"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ist dahin,", "tokens": ["Ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Was ich bin.", "tokens": ["Was", "ich", "bin", "."], "token_info": ["word", "word"