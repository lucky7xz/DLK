{"dta.poem.14044": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Stille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So still ist's nicht drau\u00dfen im Schnee,", "tokens": ["So", "still", "ist's", "nicht", "drau\u00b7\u00dfen", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So stumm und verschwiegen sind", "tokens": ["So", "stumm", "und", "ver\u00b7schwie\u00b7gen", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVPP", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Sterne nicht in der H\u00f6he,", "tokens": ["Die", "Ster\u00b7ne", "nicht", "in", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als meine Gedanken sind.", "tokens": ["Als", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}