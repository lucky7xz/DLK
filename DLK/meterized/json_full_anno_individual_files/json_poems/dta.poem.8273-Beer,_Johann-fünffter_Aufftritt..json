{"dta.poem.8273": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "f\u00fcnffter Aufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird schon die Rosen-Lippen", "tokens": ["Es", "wird", "schon", "die", "Ro\u00b7sen\u00b7\u00b7Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Neuer Purpur \u00fcberziehn;", "tokens": ["Neu\u00b7er", "Pur\u00b7pur", "\u00fc\u00b7ber\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die wei\u00dfen Marmel-Klippen", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "Mar\u00b7mel\u00b7Klip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In gekr\u00f6hnten Schmucke gl\u00fchn/", "tokens": ["In", "ge\u00b7kr\u00f6hn\u00b7ten", "Schmu\u00b7cke", "gl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die neue Liebes-Lust", "tokens": ["Wann", "die", "neu\u00b7e", "Lie\u00b7bes\u00b7Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcllet die Rubinen-Brust.", "tokens": ["F\u00fcl\u00b7let", "die", "Ru\u00b7bi\u00b7nen\u00b7Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}