{"dta.poem.13205": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr stimmt zwar \u00fcber eins, und sagt mir insgesamt:", "tokens": ["\u201e", "jhr", "stimmt", "zwar", "\u00fc\u00b7ber", "eins", ",", "und", "sagt", "mir", "ins\u00b7ge\u00b7samt", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "PIS", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eich herrsche; die\u00df sey mein, das Rathen euer Amt.", "tokens": ["\u201e", "ich", "herr\u00b7sche", ";", "die\u00df", "sey", "mein", ",", "das", "Ra\u00b7then", "eu\u00b7er", "Amt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "275\u201dDie\u00df ists, we\u00dfwegen ich bin in den Saal gekommen,", "tokens": ["\"", "Die\u00df", "ists", ",", "we\u00df\u00b7we\u00b7gen", "ich", "bin", "in", "den", "Saal", "ge\u00b7kom\u00b7men", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "$,", "PWAV", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "\u201ewe\u00dfwegen ich der Frag Entscheidung vorgenommen.", "tokens": ["\u201e", "we\u00df\u00b7we\u00b7gen", "ich", "der", "Frag", "Ent\u00b7schei\u00b7dung", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewie konnt\u2019 ich sonder euch den Feinden wiederstehn?", "tokens": ["\u201e", "wie", "konnt'", "ich", "son\u00b7der", "euch", "den", "Fein\u00b7den", "wie\u00b7ders\u00b7tehn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ees mu\u00dften Kron und Thron und Zepter untergehn.", "tokens": ["\u201e", "es", "mu\u00df\u00b7ten", "Kron", "und", "Thron", "und", "Zep\u00b7ter", "un\u00b7ter\u00b7gehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eder Gegner war am Thor; was konnt\u2019 ich da befehlen?", "tokens": ["\u201e", "der", "Geg\u00b7ner", "war", "am", "Thor", ";", "was", "konnt'", "ich", "da", "be\u00b7feh\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "280\u201dWas hatt ich sonder euch zu lassen und zu w\u00e4hlen?", "tokens": ["\"", "Was", "hatt", "ich", "son\u00b7der", "euch", "zu", "las\u00b7sen", "und", "zu", "w\u00e4h\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edie Rettung gr\u00fcndte sich in nichts, als in dem Rath", "tokens": ["\u201e", "die", "Ret\u00b7tung", "gr\u00fcnd\u00b7te", "sich", "in", "nichts", ",", "als", "in", "dem", "Rath"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "APPR", "PIS", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eden jede Tugend mir damahls gegeben hat.", "tokens": ["\u201e", "den", "je\u00b7de", "Tu\u00b7gend", "mir", "da\u00b7mahls", "ge\u00b7ge\u00b7ben", "hat", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edurch euch hab ich des Feinds Ma\u00df-nehmungen zernichtet;", "tokens": ["\u201e", "durch", "euch", "hab", "ich", "des", "Feinds", "Ma\u00df\u00b7neh\u00b7mun\u00b7gen", "zer\u00b7nich\u00b7tet", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "\u201emithin seyd ihr nicht mir, nein: ich bin euch verpflichtet.", "tokens": ["\u201e", "mi\u00b7thin", "seyd", "ihr", "nicht", "mir", ",", "nein", ":", "ich", "bin", "euch", "ver\u00b7pflich\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PTKNEG", "PPER", "$,", "PTKANT", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}