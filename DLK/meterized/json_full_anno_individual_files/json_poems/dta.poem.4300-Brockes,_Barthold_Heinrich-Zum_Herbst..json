{"dta.poem.4300": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum hat die Erndte sich geendet,", "tokens": ["Kaum", "hat", "die", "Ernd\u00b7te", "sich", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wird des Feldes ebner Grund", "tokens": ["So", "wird", "des", "Fel\u00b7des", "eb\u00b7ner", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Pfl\u00fcgen wieder umgewendet.", "tokens": ["Durch", "Pfl\u00fc\u00b7gen", "wie\u00b7der", "um\u00b7ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was erst an Kraut und Stoppeln bunt,", "tokens": ["Was", "erst", "an", "Kraut", "und", "Stop\u00b7peln", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird pl\u00f6tzlich braun. Ein dunkler Sammt,", "tokens": ["Wird", "pl\u00f6tz\u00b7lich", "braun", ".", "Ein", "dunk\u00b7ler", "Sammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem, voller dunklern Regel-rechten darinn gezognen lan-", "tokens": ["Dem", ",", "vol\u00b7ler", "dunk\u00b7lern", "Re\u00b7gel\u00b7rech\u00b7ten", "da\u00b7rinn", "ge\u00b7zog\u00b7nen", "lan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "ADJA", "ADJA", "NN", "PAV", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "gen Strichen,", "tokens": ["gen", "Stri\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Die Aecker, voller Furchen, glichen,", "tokens": ["Die", "A\u00b7e\u00b7cker", ",", "vol\u00b7ler", "Fur\u00b7chen", ",", "gli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Scheint alles itzo zu bedecken.", "tokens": ["Scheint", "al\u00b7les", "it\u00b7zo", "zu", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Absicht dieser Arbeit schien", "tokens": ["Die", "Ab\u00b7sicht", "die\u00b7ser", "Ar\u00b7beit", "schien"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich auf drey St\u00fccke zu erstrecken,", "tokens": ["Sich", "auf", "drey", "St\u00fc\u00b7cke", "zu", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Des Unkrauts gar zu fruchtbar Gr\u00fcn", "tokens": ["Des", "Un\u00b7krauts", "gar", "zu", "frucht\u00b7bar", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu unterdr\u00fccken, zu begraben,", "tokens": ["Zu", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", ",", "zu", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Um eine D\u00fcnge draus zu haben,", "tokens": ["Um", "ei\u00b7ne", "D\u00fcn\u00b7ge", "draus", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PAV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und aus dem sch\u00e4dlichen selbst Nutzen noch zu zieh\u2019n.", "tokens": ["Und", "aus", "dem", "sch\u00e4d\u00b7li\u00b7chen", "selbst", "Nut\u00b7zen", "noch", "zu", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "NN", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der andre Zweck, damit die umgest\u00fcrzte Erde", "tokens": ["Der", "and\u00b7re", "Zweck", ",", "da\u00b7mit", "die", "um\u00b7ge\u00b7st\u00fcrz\u00b7te", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ge\u00f6ffnet, Himmel-werts gekehrt,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", ",", "Him\u00b7mel\u00b7werts", "ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Regen, Thau und Luft befeuchtet und gen\u00e4hrt,", "tokens": ["Mit", "Re\u00b7gen", ",", "Thau", "und", "Luft", "be\u00b7feuch\u00b7tet", "und", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Sonnen-Licht durchdrungen werde,", "tokens": ["Vom", "Son\u00b7nen\u00b7Licht", "durch\u00b7drun\u00b7gen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch die Fruchtbarkeit, wie die Erfahrung lehrt,", "tokens": ["Wo\u00b7durch", "die", "Frucht\u00b7bar\u00b7keit", ",", "wie", "die", "Er\u00b7fah\u00b7rung", "lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In einen Acker wird gebracht.", "tokens": ["In", "ei\u00b7nen", "A\u00b7cker", "wird", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie aber eigentlich, auf welche Weis\u2019 und Art,", "tokens": ["Wie", "a\u00b7ber", "ei\u00b7gent\u00b7lich", ",", "auf", "wel\u00b7che", "Weis'", "und", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "$,", "APPR", "PWAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Himmel hier sich mit der Erde paart,", "tokens": ["Der", "Him\u00b7mel", "hier", "sich", "mit", "der", "Er\u00b7de", "paart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird wohl so leichtlich nicht erkl\u00e4rt.", "tokens": ["Wird", "wohl", "so", "leicht\u00b7lich", "nicht", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vermuhtlich ist, da\u00df in dem Reich der L\u00fcfte,", "tokens": ["Ver\u00b7muht\u00b7lich", "ist", ",", "da\u00df", "in", "dem", "Reich", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In welches aller C\u00f6rper D\u00fcfte,", "tokens": ["In", "wel\u00b7ches", "al\u00b7ler", "C\u00f6r\u00b7per", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So bald sie aufgel\u00f6set, quillen,", "tokens": ["So", "bald", "sie", "auf\u00b7ge\u00b7l\u00f6\u00b7set", ",", "quil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und es als einen Chaos f\u00fcllen,", "tokens": ["Und", "es", "als", "ei\u00b7nen", "Chaos", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Auch Saamen-Theile sich erh\u00f6h\u2019n,", "tokens": ["Auch", "Saa\u00b7men\u00b7T\u00b7hei\u00b7le", "sich", "er\u00b7h\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nebst schweflichten und salzen Theilen,", "tokens": ["Nebst", "schwef\u00b7lich\u00b7ten", "und", "sal\u00b7zen", "Thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Die mit den D\u00fcnsten aufwerts eilen,", "tokens": ["Die", "mit", "den", "D\u00fcns\u00b7ten", "auf\u00b7werts", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, durch der Sonnen Zug, sich in die H\u00f6he lenken,", "tokens": ["Und", ",", "durch", "der", "Son\u00b7nen", "Zug", ",", "sich", "in", "die", "H\u00f6\u00b7he", "len\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch, wenn sie weicht, sich wieder nieder senken.", "tokens": ["Auch", ",", "wenn", "sie", "weicht", ",", "sich", "wie\u00b7der", "nie\u00b7der", "sen\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRF", "ADV", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch diesen sanften Zug und stetiges Bewegen", "tokens": ["Durch", "die\u00b7sen", "sanf\u00b7ten", "Zug", "und", "ste\u00b7ti\u00b7ges", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Scheint \u00fcberall die Fruchtbarkeit und Segen", "tokens": ["Scheint", "\u00fc\u00b7be\u00b7rall", "die", "Frucht\u00b7bar\u00b7keit", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gen\u00e4hrt durch sanfte W\u00e4rm\u2019, getr\u00e4nkt durch lauen Regen.", "tokens": ["Ge\u00b7n\u00e4hrt", "durch", "sanf\u00b7te", "W\u00e4rm'", ",", "ge\u00b7tr\u00e4nkt", "durch", "lau\u00b7en", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der dritte Zweck scheint darinn zu besteh\u2019n,", "tokens": ["Der", "drit\u00b7te", "Zweck", "scheint", "da\u00b7rinn", "zu", "be\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, in den tief-gezognen Furchen die Segen-reiche Saat", "tokens": ["Da\u00df", ",", "in", "den", "tie\u00b7fge\u00b7zo\u00b7gnen", "Fur\u00b7chen", "die", "Se\u00b7gen\u00b7rei\u00b7che", "Saat"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "zu s\u00e4'n,", "tokens": ["zu", "s\u00e4'n", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "Man alles kl\u00fcglich zubereite, damit, wenn sich die K\u00f6rner", "tokens": ["Man", "al\u00b7les", "kl\u00fcg\u00b7lich", "zu\u00b7be\u00b7rei\u00b7te", ",", "da\u00b7mit", ",", "wenn", "sich", "die", "K\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PIS", "ADJD", "VVFIN", "$,", "PAV", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "heben,", "tokens": ["he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Vermehret durch den Saft der Erde, sie uns, im Ueberflu\u00df,", "tokens": ["Ver\u00b7meh\u00b7ret", "durch", "den", "Saft", "der", "Er\u00b7de", ",", "sie", "uns", ",", "im", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "im Leben,", "tokens": ["im", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Zur Erndte-Zeit, die Nahrungs-Sch\u00e4tze geben.", "tokens": ["Zur", "Ernd\u00b7te\u00b7Zeit", ",", "die", "Nah\u00b7rungs\u00b7Sch\u00e4t\u00b7ze", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Brunquell\u2019 aller Fruchtbarkeit! O einz\u2019ger Ursprung", "tokens": ["O", "Brun\u00b7quell'", "al\u00b7ler", "Frucht\u00b7bar\u00b7keit", "!", "O", "einz'\u00b7ger", "Ur\u00b7sprung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PIAT", "NN", "$.", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "aller Kr\u00e4fte!", "tokens": ["al\u00b7ler", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "O weiser Stifter aller Ordnung! Der Du, da\u00df man ver-", "tokens": ["O", "wei\u00b7ser", "Stif\u00b7ter", "al\u00b7ler", "Ord\u00b7nung", "!", "Der", "Du", ",", "da\u00df", "man", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "$.", "ART", "PPER", "$,", "KOUS", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "n\u00fcnftig denkt,", "tokens": ["n\u00fcnf\u00b7tig", "denkt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df unser Geist die Regeln merkt, die Du in die Natur", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "die", "Re\u00b7geln", "merkt", ",", "die", "Du", "in", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gesenkt,", "tokens": ["ge\u00b7senkt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und sie zum Nutzen brauchen kann, den Menschen blo\u00df", "tokens": ["Und", "sie", "zum", "Nut\u00b7zen", "brau\u00b7chen", "kann", ",", "den", "Men\u00b7schen", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "allein geschenkt,", "tokens": ["al\u00b7lein", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "---+", "measure": "unknown.measure.single"}, "line.9": {"text": "Ach, segne doch des Ackersmanns der Welt so n\u00fctzliches", "tokens": ["Ach", ",", "seg\u00b7ne", "doch", "des", "A\u00b7ckers\u00b7manns", "der", "Welt", "so", "n\u00fctz\u00b7li\u00b7ches"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Gesch\u00e4fte!", "tokens": ["Ge\u00b7sch\u00e4f\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ach la\u00df, da das, was er gekonnt, von ihm mit allem Flei\u00df", "tokens": ["Ach", "la\u00df", ",", "da", "das", ",", "was", "er", "ge\u00b7konnt", ",", "von", "ihm", "mit", "al\u00b7lem", "Flei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "KOUS", "PDS", "$,", "PWS", "PPER", "ADJD", "$,", "APPR", "PPER", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "gescheh'n,", "tokens": ["ge\u00b7scheh'n", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Und er nun weiter nichts vermag, ihn seiner Arbeit Fr\u0