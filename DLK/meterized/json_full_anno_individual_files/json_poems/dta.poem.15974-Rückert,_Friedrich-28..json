{"dta.poem.15974": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob eine Wahrheit ist in dieser falschen Welt,", "tokens": ["Ob", "ei\u00b7ne", "Wahr\u00b7heit", "ist", "in", "die\u00b7ser", "fal\u00b7schen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df nicht; minder noch, wo sie versteckt sich h\u00e4lt.", "tokens": ["Ich", "wei\u00df", "nicht", ";", "min\u00b7der", "noch", ",", "wo", "sie", "ver\u00b7steckt", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df eine Wahrheit war, schlie\u00df' ich aus ihrem Namen,", "tokens": ["Da\u00df", "ei\u00b7ne", "Wahr\u00b7heit", "war", ",", "schlie\u00df'", "ich", "aus", "ih\u00b7rem", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn war und Wahrheit scheint erspro\u00dft aus gleichem Samen.", "tokens": ["Denn", "war", "und", "Wahr\u00b7heit", "scheint", "ers\u00b7pro\u00dft", "aus", "glei\u00b7chem", "Sa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KON", "NN", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn sie einmal war, wird sie dort ewig seyn,", "tokens": ["Doch", "wenn", "sie", "ein\u00b7mal", "war", ",", "wird", "sie", "dort", "e\u00b7wig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo alles ist was war, dort geht sie aus und ein.", "tokens": ["Wo", "al\u00b7les", "ist", "was", "war", ",", "dort", "geht", "sie", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PIS", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dort werd' ich einst sie sehn in eigenster Gestalt:", "tokens": ["Dort", "werd'", "ich", "einst", "sie", "sehn", "in", "ei\u00b7gens\u00b7ter", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt scheint ihr Licht von dort herab durch Wolkenspalt.", "tokens": ["Jetzt", "scheint", "ihr", "Licht", "von", "dort", "her\u00b7ab", "durch", "Wol\u00b7ken\u00b7spalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie ist die Sonne, die nicht selbst zur Erde kommt,", "tokens": ["Sie", "ist", "die", "Son\u00b7ne", ",", "die", "nicht", "selbst", "zur", "Er\u00b7de", "kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist in ihrem Schein, was uns zum Leben frommt.", "tokens": ["Doch", "ist", "in", "ih\u00b7rem", "Schein", ",", "was", "uns", "zum", "Le\u00b7ben", "frommt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie ist der Wahrheit Schein genannt? Wahrscheinlichkeit,", "tokens": ["Wie", "ist", "der", "Wahr\u00b7heit", "Schein", "ge\u00b7nannt", "?", "Wahr\u00b7schein\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "VVPP", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit behelfen wir uns vorderhand zur Zeit.", "tokens": ["Da\u00b7mit", "be\u00b7hel\u00b7fen", "wir", "uns", "vor\u00b7der\u00b7hand", "zur", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}