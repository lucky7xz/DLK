{"textgrid.poem.44271": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[anjezo thu ich was, das mir kaum m\u00f6glich ist]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anjezo thu ich was, das mir kaum m\u00f6glich ist,", "tokens": ["An\u00b7je\u00b7zo", "thu", "ich", "was", ",", "das", "mir", "kaum", "m\u00f6g\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIS", "$,", "PRELS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich reime, da mir doch kein einzger Vers nicht fliest;", "tokens": ["Ich", "rei\u00b7me", ",", "da", "mir", "doch", "kein", "einz\u00b7ger", "Vers", "nicht", "fliest", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der muntre Dichtergeist sizt mir nicht im Gehirne.", "tokens": ["Der", "mun\u00b7tre", "Dich\u00b7ter\u00b7geist", "sizt", "mir", "nicht", "im", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bald wisch ich mir verwirrt den Angstschwei\u00df von der Stirne,", "tokens": ["Bald", "wisch", "ich", "mir", "ver\u00b7wirrt", "den", "A\u00b7ngstschwei\u00df", "von", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald stampf ich meinen Kiel, bald such ichs hinterm Ohr,", "tokens": ["Bald", "stampf", "ich", "mei\u00b7nen", "Kiel", ",", "bald", "such", "ichs", "hin\u00b7term", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NE", "$,", "ADV", "ADV", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bald streich ich alles aus und bringe nichts hervor.", "tokens": ["Bald", "streich", "ich", "al\u00b7les", "aus", "und", "brin\u00b7ge", "nichts", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Pegasus hat fast Bucephals seine T\u00fccke;", "tokens": ["Der", "Pe\u00b7ga\u00b7sus", "hat", "fast", "Bu\u00b7ce\u00b7phals", "sei\u00b7ne", "T\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Er springt, er schl\u00e4gt, er bei\u00dft und jaget mich zur\u00fccke,", "tokens": ["Er", "springt", ",", "er", "schl\u00e4gt", ",", "er", "bei\u00dft", "und", "ja\u00b7get", "mich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So oft ich mich auf ihn poetisch sezen will.", "tokens": ["So", "oft", "ich", "mich", "auf", "ihn", "po\u00b7e\u00b7tisch", "se\u00b7zen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "APPR", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ihn ein Neukirch braucht, so steht er fromm und still.", "tokens": ["Wenn", "ihn", "ein", "Neu\u00b7kirch", "braucht", ",", "so", "steht", "er", "fromm", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und gleichwohl soll ich jezt zu einem Freundschaftszeichen", "tokens": ["Und", "gleich\u00b7wohl", "soll", "ich", "jezt", "zu", "ei\u00b7nem", "Freund\u00b7schafts\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein neues Ehrenlied euch Freunden \u00fcberreichen.", "tokens": ["Ein", "neu\u00b7es", "Eh\u00b7ren\u00b7lied", "euch", "Freun\u00b7den", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und hab ich ohngefehr ein Verschen, das noch taugt,", "tokens": ["Und", "hab", "ich", "ohn\u00b7ge\u00b7fehr", "ein", "Ver\u00b7schen", ",", "das", "noch", "taugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus meinem Finger gleich mit K\u00fcmmern\u00fc\u00df gesaugt,", "tokens": ["Aus", "mei\u00b7nem", "Fin\u00b7ger", "gleich", "mit", "K\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "ge\u00b7saugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So traut ihr mirs nicht zu und dencket: Ein Ducaten", "tokens": ["So", "traut", "ihr", "mirs", "nicht", "zu", "und", "den\u00b7cket", ":", "Ein", "Du\u00b7ca\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "PTKNEG", "PTKVZ", "KON", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Trift einen Dichter an und kan in N\u00f6then rathen.", "tokens": ["Trift", "ei\u00b7nen", "Dich\u00b7ter", "an", "und", "kan", "in", "N\u00f6\u00b7then", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum glaubt mir, was ihr wollt; bin ich kein Dichter nicht,", "tokens": ["Drum", "glaubt", "mir", ",", "was", "ihr", "wollt", ";", "bin", "ich", "kein", "Dich\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$.", "VAFIN", "PPER", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So zeuget doch dies Blat von meiner Treu und Pflicht.", "tokens": ["So", "zeu\u00b7get", "doch", "dies", "Blat", "von", "mei\u00b7ner", "Treu", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich mu\u00df die Redligkeit, ich mu\u00df die Freundschaftsproben,", "tokens": ["Ich", "mu\u00df", "die", "Red\u00b7lig\u00b7keit", ",", "ich", "mu\u00df", "die", "Freund\u00b7schafts\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ich von euch gesehn, nach meinen Kr\u00e4ften loben.", "tokens": ["Die", "ich", "von", "euch", "ge\u00b7sehn", ",", "nach", "mei\u00b7nen", "Kr\u00e4f\u00b7ten", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie sollt ich euch nun nicht an eurem Ehrenfest,", "tokens": ["Wie", "sollt", "ich", "euch", "nun", "nicht", "an", "eu\u00b7rem", "Eh\u00b7ren\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das euch Minervens Huld vergn\u00fcget feyren l\u00e4st,", "tokens": ["Das", "euch", "Mi\u00b7ner\u00b7vens", "Huld", "ver\u00b7gn\u00fc\u00b7get", "fey\u00b7ren", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NE", "NE", "VVFIN", "VVINF", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Mit einem treuen Wuntsch vergn\u00fcgt entgegen gehen", "tokens": ["Mit", "ei\u00b7nem", "treu\u00b7en", "Wunt\u00b7sch", "ver\u00b7gn\u00fcgt", "ent\u00b7ge\u00b7gen", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "PTKVZ", "VVINF"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und euren klugen Flei\u00df besingen und erh\u00f6hen?", "tokens": ["Und", "eu\u00b7ren", "klu\u00b7gen", "Flei\u00df", "be\u00b7sin\u00b7gen", "und", "er\u00b7h\u00f6\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ihr habts verdient, da\u00df euch Apoll in Tempel f\u00fchrt", "tokens": ["Ihr", "habts", "ver\u00b7dient", ",", "da\u00df", "euch", "A\u00b7poll", "in", "Tem\u00b7pel", "f\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "NE", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und euch vor seltne M\u00fch mit seinem Lorbeer ziert.", "tokens": ["Und", "euch", "vor", "selt\u00b7ne", "M\u00fch", "mit", "sei\u00b7nem", "Lor\u00b7beer", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wir k\u00f6nnen euch mit Recht, gelehrt- und muntre Preu\u00dfen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "euch", "mit", "Recht", ",", "ge\u00b7lehr\u00b7t", "und", "mun\u00b7tre", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Der r\u00fchmlichen Sophie getreue Buhlen hei\u00dfen.", "tokens": ["Der", "r\u00fchm\u00b7li\u00b7chen", "So\u00b7phie", "ge\u00b7treu\u00b7e", "Buh\u00b7len", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allein wer Rachelchen und Dorchen blos erwehlt,", "tokens": ["Al\u00b7lein", "wer", "Ra\u00b7chel\u00b7chen", "und", "Dor\u00b7chen", "blos", "er\u00b7wehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.30": {"text": "Auch ihnen Zeit und Geld verschwendrisch zugezehlt,", "tokens": ["Auch", "ih\u00b7nen", "Zeit", "und", "Geld", "ver\u00b7schwend\u00b7risch", "zu\u00b7ge\u00b7zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sophien aber nur wie ", "tokens": ["So\u00b7phi\u00b7en", "a\u00b7ber", "nur", "wie"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "KOKOM"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Vor solche hat der Zweig des Lorbeers nicht gegr\u00fcnet.", "tokens": ["Vor", "sol\u00b7che", "hat", "der", "Zweig", "des", "Lor\u00b7beers", "nicht", "ge\u00b7gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch Hummeln schleichen sich oftmahls bey Bienen ein,", "tokens": ["Doch", "Hum\u00b7meln", "schlei\u00b7chen", "sich", "oft\u00b7mahls", "bey", "Bie\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "So kan auch der Parna\u00df nicht ohne R\u00e4uber seyn.", "tokens": ["So", "kan", "auch", "der", "Par\u00b7na\u00df", "nicht", "oh\u00b7ne", "R\u00e4u\u00b7ber", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach, folgten jene doch dem r\u00fchmlichen Exempel,", "tokens": ["Ach", ",", "folg\u00b7ten", "je\u00b7ne", "doch", "dem", "r\u00fchm\u00b7li\u00b7chen", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Das man an euch gesehn, so w\u00e4r der Ehrentempel", "tokens": ["Das", "man", "an", "euch", "ge\u00b7sehn", ",", "so", "w\u00e4r", "der", "Eh\u00b7ren\u00b7tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "PPER", "VVPP", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Von der verhasten Zunft der Ignoranten frey", "tokens": ["Von", "der", "ver\u00b7has\u00b7ten", "Zunft", "der", "Ig\u00b7no\u00b7ran\u00b7ten", "frey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und sie entweihten nicht der Pallas Sacristey.", "tokens": ["Und", "sie", "ent\u00b7weih\u00b7ten", "nicht", "der", "Pal\u00b7las", "Sa\u00b7cris\u00b7tey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ihr la\u00dfet euch dahin auf die erh\u00f6hten Stufen", "tokens": ["Ihr", "la\u00b7\u00dfet", "euch", "da\u00b7hin", "auf", "die", "er\u00b7h\u00f6h\u00b7ten", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Zum Preis und Ehrenkranz gerecht und w\u00fcrdig rufen.", "tokens": ["Zum", "Preis", "und", "Eh\u00b7ren\u00b7kranz", "ge\u00b7recht", "und", "w\u00fcr\u00b7dig", "ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ihr tugendhaften Zwey, selbst euer Vaterland", "tokens": ["Ihr", "tu\u00b7gend\u00b7haf\u00b7ten", "Zwey", ",", "selbst", "eu\u00b7er", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wies euch vorl\u00e4ngst den Ring mit ungezwungner Hand;", "tokens": ["Wies", "euch", "vor\u00b7l\u00e4ngst", "den", "Ring", "mit", "un\u00b7ge\u00b7zwung\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nun wird es neidisch seyn, da\u00df unsre Castalinnen", "tokens": ["Nun", "wird", "es", "nei\u00b7disch", "seyn", ",", "da\u00df", "uns\u00b7re", "Cas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Euch mit dem Violet frohlockende bedienen.", "tokens": ["Euch", "mit", "dem", "Vi\u00b7o\u00b7let", "froh\u00b7lo\u00b7cken\u00b7de", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Zwar K\u00f6nigsberg sey nun zu eures Flei\u00dfes Lohn", "tokens": ["Zwar", "K\u00f6\u00b7nigs\u00b7berg", "sey", "nun", "zu", "eu\u00b7res", "Flei\u00b7\u00dfes", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ein neuer Ehrenberg und eures Gl\u00fcckes Thron;", "tokens": ["Ein", "neu\u00b7er", "Eh\u00b7ren\u00b7berg", "und", "eu\u00b7res", "Gl\u00fc\u00b7ckes", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Doch liebt ihr fremde Luft, so steht, wir wollens hofen,", "tokens": ["Doch", "liebt", "ihr", "frem\u00b7de", "Luft", ",", "so", "steht", ",", "wir", "wol\u00b7lens", "ho\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Des Gl\u00fcckes Ehrenthor euch hier noch ferner ofen.", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "Eh\u00b7ren\u00b7thor", "euch", "hier", "noch", "fer\u00b7ner", "o\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Und wenn ihr einst nach Wuntsch in sch\u00f6nen \u00c4mtern sizt,", "tokens": ["Und", "wenn", "ihr", "einst", "nach", "Wunt\u00b7sch", "in", "sch\u00f6\u00b7nen", "\u00c4m\u00b7tern", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.50": {"text": "Euch auch ein lieber Schaz vonn\u00f6then ist und n\u00fczt,", "tokens": ["Euch", "auch", "ein", "lie\u00b7ber", "Schaz", "von\u00b7n\u00f6\u00b7then", "ist", "und", "n\u00fczt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": 