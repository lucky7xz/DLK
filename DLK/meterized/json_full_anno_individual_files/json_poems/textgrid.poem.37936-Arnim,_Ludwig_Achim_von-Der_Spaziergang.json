{"textgrid.poem.37936": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Spaziergang", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt la\u00dft uns aus spazieren,", "tokens": ["Kommt", "la\u00dft", "uns", "aus", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVIMP", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu h\u00f6ren durch den Wald,", "tokens": ["Zu", "h\u00f6\u00b7ren", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gel musiziren,", "tokens": ["Die", "V\u00f6\u00b7gel", "mu\u00b7si\u00b7zi\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Berg und Thal erschallt.", "tokens": ["Da\u00df", "Berg", "und", "Thal", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl dem der frey kann singen,", "tokens": ["Wohl", "dem", "der", "frey", "kann", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie du, du Volk der Luft,", "tokens": ["Wie", "du", ",", "du", "Volk", "der", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPER", "NN", "ART", "NN", "$