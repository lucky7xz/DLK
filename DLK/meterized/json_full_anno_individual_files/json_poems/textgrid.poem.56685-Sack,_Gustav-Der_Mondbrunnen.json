{"textgrid.poem.56685": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Mondbrunnen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch als sie wieder sich in Tr\u00e4umen wiegte", "tokens": ["Doch", "als", "sie", "wie\u00b7der", "sich", "in", "Tr\u00e4u\u00b7men", "wieg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schwer an meiner m\u00fcden Schulter lag,", "tokens": ["und", "schwer", "an", "mei\u00b7ner", "m\u00fc\u00b7den", "Schul\u00b7ter", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "gedachte ich, da leuchtend wie der Tag", "tokens": ["ge\u00b7dach\u00b7te", "ich", ",", "da", "leuch\u00b7tend", "wie", "der", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVPP", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der Mond sich an die hohen D\u00e4cher schmiegte,", "tokens": ["der", "Mond", "sich", "an", "die", "ho\u00b7hen", "D\u00e4\u00b7cher", "schmieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "der Lust, wie sie seit Jahren mich bekriegte", "tokens": ["der", "Lust", ",", "wie", "sie", "seit", "Jah\u00b7ren", "mich", "be\u00b7krieg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und meinen Stolz mit weichem Wellenschlag", "tokens": ["und", "mei\u00b7nen", "Stolz", "mit", "wei\u00b7chem", "Wel\u00b7len\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und tausend Armen immer tiefer brach \u2013", "tokens": ["und", "tau\u00b7send", "Ar\u00b7men", "im\u00b7mer", "tie\u00b7fer", "brach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "oh da\u00df sie doch gleich einem Quell versiegte,", "tokens": ["oh", "da\u00df", "sie", "doch", "gleich", "ei\u00b7nem", "Quell", "ver\u00b7sieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "auf den man einen Block aus schwerem Golde", "tokens": ["auf", "den", "man", "ei\u00b7nen", "Block", "aus", "schwe\u00b7rem", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "w\u00e4lzt! Da, ohne Ende unaufhaltsam rollte", "tokens": ["w\u00e4lzt", "!", "Da", ",", "oh\u00b7ne", "En\u00b7de", "un\u00b7auf\u00b7halt\u00b7sam", "roll\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "$,", "KOUI", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-+-", "me