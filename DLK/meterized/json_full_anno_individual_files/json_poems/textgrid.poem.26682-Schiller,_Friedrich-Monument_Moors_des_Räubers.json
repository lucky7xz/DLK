{"textgrid.poem.26682": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Monument Moors des R\u00e4ubers", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vollendet!", "tokens": ["Voll\u00b7en\u00b7det", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Heil dir! Vollendet!", "tokens": ["Heil", "dir", "!", "Voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Majest\u00e4tischer S\u00fcnder!", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7scher", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deine furchtbare Rolle vollbracht.", "tokens": ["Dei\u00b7ne", "furcht\u00b7ba\u00b7re", "Rol\u00b7le", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Hoher Gefallener!", "tokens": ["Ho\u00b7her", "Ge\u00b7fal\u00b7le\u00b7ner", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Deines Geschlechts Beginner und Ender!", "tokens": ["Dei\u00b7nes", "Ge\u00b7schlechts", "Be\u00b7gin\u00b7ner", "und", "En\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Seltner Sohn ihrer schr\u00f6cklichsten Laune,", "tokens": ["Selt\u00b7ner", "Sohn", "ih\u00b7rer", "schr\u00f6ck\u00b7lichs\u00b7ten", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Erhabner Versto\u00df der Mutter Natur!", "tokens": ["Er\u00b7hab\u00b7ner", "Ver\u00b7sto\u00df", "der", "Mut\u00b7ter", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Durch wolkigte Nacht ein pr\u00e4chtiger Blitz!", "tokens": ["Durch", "wol\u00b7kig\u00b7te", "Nacht", "ein", "pr\u00e4ch\u00b7ti\u00b7ger", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hui! hinter ihm schlagen die Pforten zusammen!", "tokens": ["Hui", "!", "hin\u00b7ter", "ihm", "schla\u00b7gen", "die", "Pfor\u00b7ten", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$.", "APPR", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Geizig schlingt ihn der Rachen der Nacht!", "tokens": ["Gei\u00b7zig", "schlingt", "ihn", "der", "Ra\u00b7chen", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zucken die V\u00f6lker", "tokens": ["Zu\u00b7cken", "die", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Unter seiner verderbenden Pracht!", "tokens": ["Un\u00b7ter", "sei\u00b7ner", "ver\u00b7der\u00b7ben\u00b7den", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Aber Heil dir! vollendet!", "tokens": ["A\u00b7ber", "Heil", "dir", "!", "voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "PPER", "$.", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Majest\u00e4tischer S\u00fcnder!", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7scher", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Deine furchtbare Rolle vollbracht!", "tokens": ["Dei\u00b7ne", "furcht\u00b7ba\u00b7re", "Rol\u00b7le", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Modre \u2013 verstieb", "tokens": ["Mod\u00b7re", "\u2013", "ver\u00b7stieb"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "VVIMP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "In der Wiege des offnen Himmels!", "tokens": ["In", "der", "Wie\u00b7ge", "des", "off\u00b7nen", "Him\u00b7mels", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "F\u00fcrchterlich jedem S\u00fcnder zur Schau,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "je\u00b7dem", "S\u00fcn\u00b7der", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wo dem ", "tokens": ["Wo", "dem"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Hei\u00dfer Ruhmsucht ", "tokens": ["Hei\u00b7\u00dfer", "Ruhm\u00b7sucht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Siehe! der Ewigkeit \u00fcbergibt dich die Schande!", "tokens": ["Sie\u00b7he", "!", "der", "E\u00b7wig\u00b7keit", "\u00fc\u00b7berg\u00b7ibt", "dich", "die", "Schan\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu den Sternen des Ruhms", "tokens": ["Zu", "den", "Ster\u00b7nen", "des", "Ruhms"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Klimmst du auf den Schultern der Schande!", "tokens": ["Klimmst", "du", "auf", "den", "Schul\u00b7tern", "der", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Einst wird unter dir auch die Schande zerstieben,", "tokens": ["Einst", "wird", "un\u00b7ter", "dir", "auch", "die", "Schan\u00b7de", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und dich reicht \u2013 die Bewunderung.", "tokens": ["Und", "dich", "reicht", "\u2013", "die", "Be\u00b7wun\u00b7de\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Nassen Auges an deinem schauernden Grabe", "tokens": ["Nas\u00b7sen", "Au\u00b7ges", "an", "dei\u00b7nem", "schau\u00b7ern\u00b7den", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "M\u00e4nner vor\u00fcber \u2013", "tokens": ["M\u00e4n\u00b7ner", "vor\u00b7\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Freue dich der Tr\u00e4ne der M\u00e4nner,", "tokens": ["Freu\u00b7e", "dich", "der", "Tr\u00e4\u00b7ne", "der", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Des Gerichteten Geist!", "tokens": ["Des", "Ge\u00b7rich\u00b7te\u00b7ten", "Geist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nassen Auges an deinem schauernden Grabe", "tokens": ["Nas\u00b7sen", "Au\u00b7ges", "an", "dei\u00b7nem", "schau\u00b7ern\u00b7den", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "J\u00fcngst ein M\u00e4dchen vor\u00fcber,", "tokens": ["J\u00fcngst", "ein", "M\u00e4d\u00b7chen", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "H\u00f6rte die furchtbare Kunde", "tokens": ["H\u00f6r\u00b7te", "die", "furcht\u00b7ba\u00b7re", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Deiner Taten vom steinernen Herold,", "tokens": ["Dei\u00b7ner", "Ta\u00b7ten", "vom", "stei\u00b7ner\u00b7nen", "He\u00b7rold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und das M\u00e4dchen \u2013 freue dich! freue dich!", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "\u2013", "freu\u00b7e", "dich", "!", "freu\u00b7e", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wischte die Tr\u00e4ne nicht ab.", "tokens": ["Wischte", "die", "Tr\u00e4\u00b7ne", "nicht", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Ferne stand ich \u2013 sah die Perle fallen,", "tokens": ["Fer\u00b7ne", "stand", "ich", "\u2013", "sah", "die", "Per\u00b7le", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und ich rief ihr: Amalia!", "tokens": ["Und", "ich", "rief", "ihr", ":", "A\u00b7ma\u00b7lia", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "J\u00fcnglinge! J\u00fcnglinge!", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "!", "J\u00fcng\u00b7lin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "++-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mit des ", "tokens": ["Mit", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Lernt behutsamer spielen.", "tokens": ["Lernt", "be\u00b7hut\u00b7sa\u00b7mer", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "St\u00f6rrig knirscht in den Z\u00fcgel das Sonnenro\u00df,", "tokens": ["St\u00f6r\u00b7rig", "knirscht", "in", "den", "Z\u00fc\u00b7gel", "das", "Son\u00b7nen\u00b7ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wie's am Seile des Meisters", "tokens": ["Wie's", "am", "Sei\u00b7le", "des", "Meis\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Erd und Himmel in sanfterem Schwunge wiegt,", "tokens": ["Erd", "und", "Him\u00b7mel", "in", "sanf\u00b7te\u00b7rem", "Schwun\u00b7ge", "wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Flammts am kindischen Zaume", "tokens": ["Flammts", "am", "kin\u00b7di\u00b7schen", "Zau\u00b7me"], "token_info": ["word", "wo