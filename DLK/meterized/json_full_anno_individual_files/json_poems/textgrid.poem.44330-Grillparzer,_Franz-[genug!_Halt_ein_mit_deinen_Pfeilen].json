{"textgrid.poem.44330": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[genug! Halt ein mit deinen Pfeilen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genug! Halt ein mit deinen Pfeilen,", "tokens": ["Ge\u00b7nug", "!", "Halt", "ein", "mit", "dei\u00b7nen", "Pfei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "grausames, h\u00f6hnisches Geschick,", "tokens": ["grau\u00b7sa\u00b7mes", ",", "h\u00f6h\u00b7ni\u00b7sches", "Ge\u00b7schick", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hei\u00df mich den Pfad des Todes eilen,", "tokens": ["Hei\u00df", "mich", "den", "Pfad", "des", "To\u00b7des", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "doch deine H\u00e4nde halt zur\u00fcck!", "tokens": ["doch", "dei\u00b7ne", "H\u00e4n\u00b7de", "halt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lang hab ich deine Wut empfunden", "tokens": ["Lang", "hab", "ich", "dei\u00b7ne", "Wut", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und schwieg; noch bluten sie, die Wunden,", "tokens": ["und", "schwieg", ";", "noch", "blu\u00b7ten", "sie", ",", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "die dein Hy\u00e4nengrimm mir schlug,", "tokens": ["die", "dein", "Hy\u00b7\u00e4\u00b7nen\u00b7grimm", "mir", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch nun, beim Himmel, nun genug!", "tokens": ["Doch", "nun", ",", "beim", "Him\u00b7mel", ",", "nun", "ge\u00b7nug", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPRART", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum bin ich allein versto\u00dfen,", "tokens": ["Wa\u00b7rum", "bin", "ich", "al\u00b7lein", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von all den deinen, ich, Natur?", "tokens": ["von", "all", "den", "dei\u00b7nen", ",", "ich", ",", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "PPOSAT", "$,", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Segens Kelch ward ausgegossen", "tokens": ["Des", "Se\u00b7gens", "Kelch", "ward", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "auf alles, was da lebet, nur", "tokens": ["auf", "al\u00b7les", ",", "was", "da", "le\u00b7bet", ",", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "von mir ist deine Hand gewendet.", "tokens": ["von", "mir", "ist", "dei\u00b7ne", "Hand", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du denn alles Gl\u00fcck verschwendet", "tokens": ["Hast", "du", "denn", "al\u00b7les", "Gl\u00fcck", "ver\u00b7schwen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "an andere stiefm\u00fctterlich,", "tokens": ["an", "an\u00b7de\u00b7re", "stief\u00b7m\u00fct\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "da\u00df dir kein Tr\u00f6pfchen blieb f\u00fcr mich?", "tokens": ["da\u00df", "dir", "kein", "Tr\u00f6pf\u00b7chen", "blieb", "f\u00fcr", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du ha\u00dftest mich schon in der Wiege,", "tokens": ["Du", "ha\u00df\u00b7test", "mich", "schon", "in", "der", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da waffnete dein t\u00fcckscher Grimm", "tokens": ["da", "waff\u00b7ne\u00b7te", "dein", "t\u00fcck\u00b7scher", "Grimm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "schon gegen mich zu ewgem Kriege", "tokens": ["schon", "ge\u00b7gen", "mich", "zu", "ew\u00b7gem", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Krankheit bleiches Unget\u00fcm,", "tokens": ["der", "Krank\u00b7heit", "blei\u00b7ches", "Un\u00b7ge\u00b7t\u00fcm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der Kindheit s\u00fc\u00dfe Blumen alle", "tokens": ["der", "Kind\u00b7heit", "s\u00fc\u00b7\u00dfe", "Blu\u00b7men", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "erstickte seine d\u00fcrre Kralle,", "tokens": ["er\u00b7stick\u00b7te", "sei\u00b7ne", "d\u00fcr\u00b7re", "Kral\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "und jeden keimenden Genu\u00df", "tokens": ["und", "je\u00b7den", "kei\u00b7men\u00b7den", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "zertrat des Ungeheuers Fu\u00df.", "tokens": ["zer\u00b7trat", "des", "Un\u00b7ge\u00b7heu\u00b7ers", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df nichts von des Lebens Lenze,", "tokens": ["Ich", "wei\u00df", "nichts", "von", "des", "Le\u00b7bens", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich hat kein Jugendtraum entz\u00fcckt,", "tokens": ["mich", "hat", "kein", "Ju\u00b7gend\u00b7traum", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Kindheit frische Bl\u00fctenkr\u00e4nze,", "tokens": ["der", "Kind\u00b7heit", "fri\u00b7sche", "Bl\u00fc\u00b7ten\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sie haben nie mein Haupt geschm\u00fcckt,", "tokens": ["sie", "ha\u00b7ben", "nie", "mein", "Haupt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "das Ungl\u00fcck wand dem kaum Gebornen", "tokens": ["das", "Un\u00b7gl\u00fcck", "wand", "dem", "kaum", "Ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "schon seine martervollen Dornen", "tokens": ["schon", "sei\u00b7ne", "mar\u00b7ter\u00b7vol\u00b7len", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ums matte welke Knabenhaupt,", "tokens": ["ums", "mat\u00b7te", "wel\u00b7ke", "Kna\u00b7ben\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "das nie der Jugend Schmuck umlaubt.", "tokens": ["das", "nie", "der", "Ju\u00b7gend", "Schmuck", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00fcr alles, was sich ausw\u00e4rts waget,", "tokens": ["F\u00fcr", "al\u00b7les", ",", "was", "sich", "aus\u00b7w\u00e4rts", "wa\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu allem, was nach au\u00dfen wirkt,", "tokens": ["Zu", "al\u00b7lem", ",", "was", "nach", "au\u00b7\u00dfen", "wirkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du die Schwungkraft mir versaget,", "tokens": ["Hast", "du", "die", "Schwung\u00b7kraft", "mir", "ver\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf innen all mein Tun bezirkt.", "tokens": ["Auf", "in\u00b7nen", "all", "mein", "Tun", "be\u00b7zirkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bl\u00f6d stand ich vor des Wirkens Pforten", "tokens": ["Bl\u00f6d", "stand", "ich", "vor", "des", "Wir\u00b7kens", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Taten mangelten den Worten,", "tokens": ["Und", "Ta\u00b7ten", "man\u00b7gel\u00b7ten", "den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Was ich bestimmt und hell gedacht,", "tokens": ["Was", "ich", "be\u00b7stimmt", "und", "hell", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bedeckte beim Vollbringen Nacht.", "tokens": ["Be\u00b7deck\u00b7te", "beim", "Voll\u00b7brin\u00b7gen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}, "stanza.6": {"line.1": {"text": "Genug! Halt ein mit deinen Pfeilen,", "tokens": ["Ge\u00b7nug", "!", "Halt", "ein", "mit", "dei\u00b7nen", "Pfei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "grausames, h\u00f6hnisches Geschick,", "tokens": ["grau\u00b7sa\u00b7mes", ",", "h\u00f6h\u00b7ni\u00b7sches", "Ge\u00b7schick", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3