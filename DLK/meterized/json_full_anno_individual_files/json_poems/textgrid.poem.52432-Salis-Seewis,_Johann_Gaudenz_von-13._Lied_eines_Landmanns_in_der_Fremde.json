{"textgrid.poem.52432": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "13. Lied eines Landmanns in der Fremde", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traute Heimat meiner Lieben,", "tokens": ["Trau\u00b7te", "Hei\u00b7mat", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinn' ich still an dich zur\u00fcck,", "tokens": ["Sinn'", "ich", "still", "an", "dich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird mir wohl, und dennoch tr\u00fcben", "tokens": ["Wird", "mir", "wohl", ",", "und", "den\u00b7noch", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnsuchtsthr\u00e4nen meinen Blick.", "tokens": ["Sehn\u00b7suchtst\u00b7hr\u00e4\u00b7nen", "mei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stiller Weiler, gr\u00fcn umfangen", "tokens": ["Stil\u00b7ler", "Wei\u00b7ler", ",", "gr\u00fcn", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von beschirmendem Gestr\u00e4uch,", "tokens": ["Von", "be\u00b7schir\u00b7men\u00b7dem", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleine H\u00fctte, voll Verlangen", "tokens": ["Klei\u00b7ne", "H\u00fct\u00b7te", ",", "voll", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denk' ich immer noch an euch!", "tokens": ["Denk'", "ich", "im\u00b7mer", "noch", "an", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An die Fenster, die mit Reben", "tokens": ["An", "die", "Fens\u00b7ter", ",", "die", "mit", "Re\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst mein Vater selbst umzog;", "tokens": ["Einst", "mein", "Va\u00b7ter", "selbst", "um\u00b7zog", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Birnbaum, der daneben", "tokens": ["An", "den", "Birn\u00b7baum", ",", "der", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PAV"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Auf das niedre Dach sich bog;", "tokens": ["Auf", "das", "nied\u00b7re", "Dach", "sich", "bog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"te