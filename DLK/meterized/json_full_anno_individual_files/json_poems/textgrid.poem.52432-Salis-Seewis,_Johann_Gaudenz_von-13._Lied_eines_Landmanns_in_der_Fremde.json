-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiederhallt in meinem Ohr.", "tokens": ["Wie\u00b7der\u00b7hallt", "in", "mei\u00b7nem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Selbst des Nachts in meinen Tr\u00e4umen", "tokens": ["Selbst", "des", "Nachts", "in", "mei\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schiff' ich auf der Heimat See;", "tokens": ["Schiff'", "ich", "auf", "der", "Hei\u00b7mat", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcttle \u00c4pfel von den B\u00e4umen,", "tokens": ["Sch\u00fctt\u00b7le", "\u00c4p\u00b7fel", "von", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4ss're ihrer Wiesen Klee;", "tokens": ["W\u00e4ss'\u00b7re", "ih\u00b7rer", "Wie\u00b7sen", "Klee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "L\u00f6sch' aus ihres Brunnens R\u00f6hren", "tokens": ["L\u00f6sch'", "aus", "ih\u00b7res", "Brun\u00b7nens", "R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Durst am schw\u00fclen Tag,", "tokens": ["Mei\u00b7nen", "Durst", "am", "schw\u00fc\u00b7len", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fcck' im Walde Heidelbeeren,", "tokens": ["Pfl\u00fcck", "im", "Wal\u00b7de", "Hei\u00b7del\u00b7bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ich einst im Schatten lag.", "tokens": ["Wo", "ich", "einst", "im", "Schat\u00b7ten", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Wann erblick' ich selbst die Linde", "tokens": ["Wann", "er\u00b7blick'", "ich", "selbst", "die", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Kirchenplatz bepflanzt,", "tokens": ["Auf", "den", "Kir\u00b7chen\u00b7platz", "be\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo gek\u00fchlt im Abendwinde", "tokens": ["Wo", "ge\u00b7k\u00fchlt", "im", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre frohe Jugend tanzt?", "tokens": ["Uns\u00b7re", "fro\u00b7he", "Ju\u00b7gend", "tanzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wann des Kirchturms Giebe