{"dta.poem.10959": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ver\u00e4nderung des Menschen  \n in Ver\u00e4nderung seines Alters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Fleischnarr ist ein junger Mensch, ein Narr vom\nEhrenwind allein,", "tokens": ["Ein", "Fleischnarr", "ist", "ein", "jun\u00b7ger", "Mensch", ",", "ein", "Narr", "vom", "Eh\u00b7ren\u00b7wind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wenn er ein Mann; und wenn er alt, ist er ein ", "tokens": ["Wenn", "er", "ein", "Mann", ";", "und", "wenn", "er", "alt", ",", "ist", "er", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$.", "KON", "KOUS", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}