{"textgrid.poem.57768": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem Gr\u00fcnebusch, in dem Gr\u00fcnebusch", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Gr\u00fcnebusch, in dem Gr\u00fcnebusch", "tokens": ["In", "dem", "Gr\u00fc\u00b7ne\u00b7busch", ",", "in", "dem", "Gr\u00fc\u00b7ne\u00b7busch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Singt die Nachtigall die ganze Nacht;", "tokens": ["Singt", "die", "Nach\u00b7ti\u00b7gall", "die", "gan\u00b7ze", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Singt mit lautem Schall, singt mit lautem Schall,", "tokens": ["Singt", "mit", "lau\u00b7tem", "Schall", ",", "singt", "mit", "lau\u00b7tem", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Da\u00df ich davon bin vom Schlaf erwacht.", "tokens": ["Da\u00df", "ich", "da\u00b7von", "bin", "vom", "Schlaf", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Singst ja viel zu s\u00fc\u00df, singst ja viel zu s\u00fc\u00df,", "tokens": ["Singst", "ja", "viel", "zu", "s\u00fc\u00df", ",", "singst", "ja", "viel",