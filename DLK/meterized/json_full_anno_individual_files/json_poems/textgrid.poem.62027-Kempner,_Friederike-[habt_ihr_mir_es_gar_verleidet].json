{"textgrid.poem.62027": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[habt ihr mir es gar verleidet]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr mir es gar verleidet,", "tokens": ["Habt", "ihr", "mir", "es", "gar", "ver\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses kleine Leben, ach,", "tokens": ["Die\u00b7ses", "klei\u00b7ne", "Le\u00b7ben", ",", "ach", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "ITJ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mein Geist einst von euch scheidet,", "tokens": ["Wenn", "mein", "Geist", "einst", "von", "euch", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sag' ich euch nichts Gutes nach.", "tokens": ["Sag'", "ich", "euch", "nichts", "Gu\u00b7tes", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PIS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn das Aller-Allerbeste", "tokens": ["Denn", "das", "Al\u00b7ler\u00b7Al\u00b7ler\u00b7bes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug ich gl\u00fccklich in der Brust,", "tokens": ["Trug", "ich", "gl\u00fcck\u00b7lich", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig glich sie einem Feste,", "tokens": ["Freu\u00b7dig", "glich", "sie", "ei\u00b7nem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich feiernd neue Lust!", "tokens": ["T\u00e4g\u00b7lich", "fei\u00b7ernd", "neu\u00b7e", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die frohen Bl\u00fcten alle", "tokens": ["Und", "die", "fro\u00b7hen", "Bl\u00fc\u00b7ten", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breitete ich vor euch aus:", "tokens": ["Brei\u00b7te\u00b7te", "ich", "vor", "euch", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder gabt ihr mir nur Galle,", "tokens": ["Wie\u00b7der", "gabt", "ihr", "mir", "nur", "Gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machtet traurig, ach, mein Los!", "tokens": ["Mach\u00b7tet", "trau\u00b7rig", ",", "ach", ",", "mein", "Los", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ITJ", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.