{"textgrid.poem.61555": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Valet mu\u00df ich dir geben,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Valet mu\u00df ich dir geben,", "tokens": ["Va\u00b7let", "mu\u00df", "ich", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du alte Lindenstadt;", "tokens": ["Du", "al\u00b7te", "Lin\u00b7den\u00b7stadt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon gl\u00fcht an deinen Reben", "tokens": ["Schon", "gl\u00fcht", "an", "dei\u00b7nen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Purpur Blatt um Blatt;", "tokens": ["Wie", "Pur\u00b7pur", "Blatt", "um", "Blatt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon stiebt es von den Wipfeln,", "tokens": ["Schon", "stiebt", "es", "von", "den", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und dunkler treibt der See,", "tokens": ["Und", "dunk\u00b7ler", "treibt", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auf der Berge Gipfeln", "tokens": ["Und", "auf", "der", "Ber\u00b7ge", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ergl\u00e4nzt der erste Schnee.", "tokens": ["Er\u00b7gl\u00e4nzt", "der", "ers\u00b7te", "Schnee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist mir hold gewesen;", "tokens": ["Du", "bist", "mir", "hold", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So nimm des Gastes Dank,", "tokens": ["So", "nimm", "des", "Gas\u00b7tes", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hoffnungsvoll Genesen", "tokens": ["Der", "hoff\u00b7nungs\u00b7voll", "Ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Aus deinen L\u00fcften trank,", "tokens": ["Aus", "dei\u00b7nen", "L\u00fcf\u00b7ten", "trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den nach verj\u00e4hrter Plage", "tokens": ["Den", "nach", "ver\u00b7j\u00e4hr\u00b7ter", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am gr\u00fcnen Flutenring", "tokens": ["Am", "gr\u00fc\u00b7nen", "Flu\u00b7ten\u00b7ring"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durchsonnter Fr\u00fchherbsttage", "tokens": ["Durch\u00b7sonn\u00b7ter", "Fr\u00fch\u00b7herbst\u00b7ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fcckte Rast umfing.", "tokens": ["Be\u00b7gl\u00fcck\u00b7te", "Rast", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da lernt' ich fromm aufs neue", "tokens": ["Da", "lernt'", "ich", "fromm", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stimmen all verstehn,", "tokens": ["Die", "Stim\u00b7men", "all", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die durch des Himmels Bl\u00e4ue", "tokens": ["Die", "durch", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Zug des Windes gehn;", "tokens": ["Im", "Zug", "des", "Win\u00b7des", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was in den Wellen schauert,", "tokens": ["Was", "in", "den", "Wel\u00b7len", "schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was in des Waldes Grund", "tokens": ["Was", "in", "des", "Wal\u00b7des", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sehns\u00fcchtig gl\u00e4nzt und trauert,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "gl\u00e4nzt", "und", "trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch einmal ward's mir kund.", "tokens": ["Noch", "ein\u00b7mal", "ward's", "mir", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sah, wenn l\u00e4ngst versunken", "tokens": ["Ich", "sah", ",", "wenn", "l\u00e4ngst", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Schwarz der T\u00e4ler Gr\u00fcn,", "tokens": ["In", "Schwarz", "der", "T\u00e4\u00b7ler", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Schneehorn purpurtrunken", "tokens": ["Am", "Schnee\u00b7horn", "pur\u00b7pur\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein hei\u00df Erinnern gl\u00fchn;", "tokens": ["Ein", "hei\u00df", "E\u00b7rin\u00b7nern", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo grimm durch Klippenbogen", "tokens": ["Wo", "grimm", "durch", "Klip\u00b7pen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Gie\u00dfbach Bahn sich schuf,", "tokens": ["Der", "Gie\u00df\u00b7bach", "Bahn", "sich", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erscholl mir's aus den Wogen", "tokens": ["Er\u00b7scholl", "mir's", "aus", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie trotz'ger Jubelruf.", "tokens": ["Wie", "trotz'\u00b7ger", "Ju\u00b7bel\u00b7ruf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie im segelhellen,", "tokens": ["Und", "wie", "im", "se\u00b7gel\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Besonnten Griechenschiff", "tokens": ["Be\u00b7sonn\u00b7ten", "Grie\u00b7chen\u00b7schiff"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich einst auf blauen Wellen", "tokens": ["Mich", "einst", "auf", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Lied Homers ergriff,", "tokens": ["Das", "Lied", "Ho\u00b7mers", "er\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sprach hier in dunklen Zungen", "tokens": ["Sprach", "hier", "in", "dunk\u00b7len", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus Felsgekl\u00fcft und Tann", "tokens": ["Aus", "Fels\u00b7ge\u00b7kl\u00fcft", "und", "Tann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Geist der Nibelungen", "tokens": ["Der", "Geist", "der", "Ni\u00b7be\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geheimnisvoll mich an.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "mich", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Versenkt in tiefes Lauschen", "tokens": ["Ver\u00b7senkt", "in", "tie\u00b7fes", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oft sa\u00df ich bis zur Nacht;", "tokens": ["Oft", "sa\u00df", "ich", "bis", "zur", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam's wie Adlersrauschen", "tokens": ["Da", "kam's", "wie", "Ad\u00b7ler\u00b7srau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf mich herab mit Macht;", "tokens": ["Auf", "mich", "her\u00b7ab", "mit", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch meinen Busen z\u00fcckte", "tokens": ["Durch", "mei\u00b7nen", "Bu\u00b7sen", "z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verwandter Drang und Klang,", "tokens": ["Ver\u00b7wand\u00b7ter", "Drang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was mich hob und dr\u00fcckte,", "tokens": ["Und", "was", "mich", "hob", "und", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ward flutender Gesang.", "tokens": ["Ward", "flu\u00b7ten\u00b7der", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O stillvertiefte Stunden,", "tokens": ["O", "still\u00b7ver\u00b7tief\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Labsal der S\u00e4ngerbrust,", "tokens": ["Lab\u00b7sal", "der", "S\u00e4n\u00b7ger\u00b7brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl seid ihr hingeschwunden", "tokens": ["Wohl", "seid", "ihr", "hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rasch mit des Sommers Lust.", "tokens": ["Rasch", "mit", "des", "Som\u00b7mers", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Doch wallt das Herz lebendig", "tokens": ["Doch", "wallt", "das", "Herz", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir auf nach eurer Ruh',", "tokens": ["Mir", "auf", "nach", "eu\u00b7rer", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und frohgekr\u00e4ftigt wend' ich", "tokens": ["Und", "froh\u00b7ge\u00b7kr\u00e4f\u00b7tigt", "wend'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Heimat heut mich zu.", "tokens": ["Der", "Hei\u00b7mat", "heut", "mich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dort winkt mir nach der Mu\u00dfe", "tokens": ["Dort", "winkt", "mir", "nach", "der", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Manch liebgewordne Pflicht;", "tokens": ["Manch", "lieb\u00b7ge\u00b7word\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es winkt mit hohem Gru\u00dfe", "tokens": ["Es", "winkt", "mit", "ho\u00b7hem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Herrschers Angesicht,", "tokens": ["Des", "Herr\u00b7schers", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der, jedem Fl\u00fcgelschlage", "tokens": ["Der", ",", "je\u00b7dem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["ART", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des deutschen Geistes hold,", "tokens": ["Des", "deut\u00b7schen", "Geis\u00b7tes", "hold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Hoffnung k\u00fcnft'ger Tage", "tokens": ["Der", "Hoff\u00b7nung", "k\u00fcnft'\u00b7ger", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein licht Panier entrollt.", "tokens": ["Ein", "licht", "Pa\u00b7nier", "ent\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Kunst in Laub und Blume", "tokens": ["Die", "Kunst", "in", "Laub", "und", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umwob des Vaters Thron;", "tokens": ["Um\u00b7wob", "des", "Va\u00b7ters", "Thron", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun r