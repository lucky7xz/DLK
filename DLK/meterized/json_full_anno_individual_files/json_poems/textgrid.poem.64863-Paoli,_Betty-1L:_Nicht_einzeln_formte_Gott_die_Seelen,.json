{"textgrid.poem.64863": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht einzeln formte Gott die Seelen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht einzeln formte Gott die Seelen,", "tokens": ["Nicht", "ein\u00b7zeln", "form\u00b7te", "Gott", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als er sie sandt' in's Erdenland;", "tokens": ["Als", "er", "sie", "sandt'", "in's", "Er\u00b7den\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gingen, hier sich zu verm\u00e4hlen,", "tokens": ["Es", "gin\u00b7gen", ",", "hier", "sich", "zu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwei gleiche stets aus seiner Hand.", "tokens": ["Zwei", "glei\u00b7che", "stets", "aus", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch das Geschick in seinem Neide,", "tokens": ["Doch", "das", "Ge\u00b7schick", "in", "sei\u00b7nem", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seines Hasses Ironie,", "tokens": ["In", "sei\u00b7nes", "Has\u00b7ses", "I\u00b7ro\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirft oft den Ocean als Scheide", "tokens": ["Wirft", "oft", "den", "O\u00b7cean", "als", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KOUS", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und dunkle Schranke zwischen sie.", "tokens": ["Und", "dunk\u00b7le", "Schran\u00b7ke", "zwi\u00b7schen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die\u00df Schicksal denk' ich, ist das meine,", "tokens": ["Die\u00df", "Schick\u00b7sal", "denk'", "ich", ",", "ist", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$,", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum breit' ich oft, von Schmerz durchgraut,", "tokens": ["Drum", "breit'", "ich", "oft", ",", "von", "Schmerz", "durch\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Arme sehnend aus und weine", "tokens": ["Die", "Ar\u00b7me", "seh\u00b7nend", "aus", "und", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rufe ungest\u00fcm und laut:", "tokens": ["Und", "ru\u00b7fe", "un\u00b7ge\u00b7st\u00fcm", "und", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Wesen, das in gleichen Tagen", "tokens": ["Du", "We\u00b7sen", ",", "das", "in", "glei\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gleicher Gotteshauch belebt,", "tokens": ["Ein", "glei\u00b7cher", "Got\u00b7tes\u00b7hauch", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "De\u00df Pulse wie die meinen schlagen,", "tokens": ["De\u00df", "Pul\u00b7se", "wie", "die", "mei\u00b7nen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df Herz so wie das meine bebt!", "tokens": ["De\u00df", "Herz", "so", "wie", "das", "mei\u00b7ne", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOKOM", "PDS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das gleiche W\u00fcnsche und Gebete", "tokens": ["Das", "glei\u00b7che", "W\u00fcn\u00b7sche", "und", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich entsendet himmelan,", "tokens": ["Wie", "ich", "ent\u00b7sen\u00b7det", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O sage mir, wo ist die St\u00e4tte,", "tokens": ["O", "sa\u00b7ge", "mir", ",", "wo", "ist", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich dich endlich finden kann?", "tokens": ["Wo", "ich", "dich", "end\u00b7lich", "fin\u00b7den", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Lebst du an ferner Nordenk\u00fcste,", "tokens": ["Lebst", "du", "an", "fer\u00b7ner", "Nor\u00b7den\u00b7k\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wo eisbedeckt die Str\u00f6me zieh'n!", "tokens": ["Wo", "eis\u00b7be\u00b7deckt", "die", "Str\u00f6\u00b7me", "zieh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fliegst du in der arab'schen W\u00fcste", "tokens": ["Fliegst", "du", "in", "der", "a\u00b7rab'\u00b7schen", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Auf windesschnellem Ro\u00df dahin?", "tokens": ["Auf", "win\u00b7des\u00b7schnel\u00b7lem", "Ro\u00df", "da\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Neigst du am sch\u00f6nen Gangesstrande", "tokens": ["Neigst", "du", "am", "sch\u00f6\u00b7nen", "Gan\u00b7ges\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Lotosblumen still dein Haupt?", "tokens": ["Vor", "Lo\u00b7tos\u00b7blu\u00b7men", "still", "dein", "Haupt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steht an der Andes dunklem Rande", "tokens": ["Steht", "an", "der", "An\u00b7des", "dunk\u00b7lem", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Haus von frischem Gr\u00fcn umlaubt?", "tokens": ["Dein", "Haus", "von", "fri\u00b7schem", "Gr\u00fcn", "um\u00b7laubt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Weilst du vielleicht in meiner N\u00e4he", "tokens": ["Weilst", "du", "viel\u00b7leicht", "in", "mei\u00b7ner", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaust mit mir dasselbe Licht,", "tokens": ["Und", "schaust", "mit", "mir", "das\u00b7sel\u00b7be", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchlst dasselbe bittre Wehe,", "tokens": ["Und", "f\u00fchlst", "das\u00b7sel\u00b7be", "bitt\u00b7re", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mein verzagend Herz umflicht?", "tokens": ["Das", "mein", "ver\u00b7za\u00b7gend", "Herz", "um\u00b7flicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVPP", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und suchst mich an jedweder Stelle,", "tokens": ["Und", "suchst", "mich", "an", "jed\u00b7we\u00b7der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wie auch ich dich suchen mu\u00df,", "tokens": ["So", "wie", "auch", "ich", "dich", "su\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schickest mir durch jede Welle", "tokens": ["Und", "schi\u00b7ckest", "mir", "durch", "je\u00b7de", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch jedes L\u00fcftchen einen Gru\u00df?", "tokens": ["Durch", "je\u00b7des", "L\u00fcft\u00b7chen", "ei\u00b7nen", "Gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und klagst wie ich: \u00bbwas mu\u00df ich missen", "tokens": ["Und", "klagst", "wie", "ich", ":", "\u00bb", "was", "mu\u00df", "ich", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "$.", "$(", "PWS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, meines Geist's erw\u00e4hlte Braut?", "tokens": ["Dich", ",", "mei\u00b7nes", "Geist's", "er\u00b7w\u00e4hl\u00b7te", "Braut", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll sich mein Aug' im Tode schlie\u00dfen,", "tokens": ["Soll", "sich", "mein", "Aug'", "im", "To\u00b7de", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' es im Leben ", "tokens": ["Eh'", "es", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Nicht einzeln formte Gott die Seelen,", "tokens": ["Nicht", "ein\u00b7zeln", "form\u00b7te", "Gott", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als er sie sandt' in's Erdenland;", "tokens": ["Als", "er", "sie", "sandt'", "in's", "Er\u00b7den\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra