{"dta.poem.4309": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst in Ritzeb\u00fcttel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! ich seh, mit vielen Freuden, aufs neu, im", "tokens": ["Gott", "Lob", "!", "ich", "seh", ",", "mit", "vie\u00b7len", "Freu\u00b7den", ",", "aufs", "neu", ",", "im"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,", "APPRART", "ADJD", "$,", "APPRART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Herbst, mein fruchtbar Feld", "tokens": ["Herbst", ",", "mein", "frucht\u00b7bar", "Feld"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Begraben, wohl bed\u00fcngt, gepfl\u00fcgt, bes\u00e4t, beeget und bestellt.", "tokens": ["Be\u00b7gra\u00b7ben", ",", "wohl", "be\u00b7d\u00fcngt", ",", "ge\u00b7pfl\u00fcgt", ",", "be\u00b7s\u00e4t", ",", "bee\u00b7get", "und", "be\u00b7stellt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "$,", "VVPP", "$,", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Man sieht, wenn man bedachtsam sieht, anitzt, so weit das", "tokens": ["Man", "sieht", ",", "wenn", "man", "be\u00b7dacht\u00b7sam", "sieht", ",", "a\u00b7nitzt", ",", "so", "weit", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,", "VVFIN", "$,", "ADV", "ADJD", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auge tr\u00e4get,", "tokens": ["Au\u00b7ge", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ein angenehmes Dunkelbraun von Aeckern, welche theils", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mes", "Dun\u00b7kel\u00b7braun", "von", "A\u00b7e\u00b7ckern", ",", "wel\u00b7che", "theils"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "geeget,", "tokens": ["gee\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Theils aufgebrochen, theils gepfl\u00fcgt, das uns, wenn wir es", "tokens": ["Theils", "auf\u00b7ge\u00b7bro\u00b7chen", ",", "theils", "ge\u00b7pfl\u00fcgt", ",", "das", "uns", ",", "wenn", "wir", "es"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "ADV", "VVPP", "$,", "PRELS", "PPER", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "recht betrachten,", "tokens": ["recht", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Und auf die gleich-geformte Fl\u00e4ch\u2019, und gleich-gemischte", "tokens": ["Und", "auf", "die", "gleich\u00b7ge\u00b7form\u00b7te", "Fl\u00e4ch'", ",", "und", "gleich\u00b7ge\u00b7mischte"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Farben achten,", "tokens": ["Far\u00b7ben", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Uns eine sanft\u2019 und fremde Lust in der ger\u00fchrten Brust", "tokens": ["Uns", "ei\u00b7ne", "sanft'", "und", "frem\u00b7de", "Lust", "in", "der", "ge\u00b7r\u00fchr\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJD", "KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "erregt.", "tokens": ["er\u00b7regt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Der Himmel, den man, meist bedeckt, in einer klaren D\u00e4mm-", "tokens": ["Der", "Him\u00b7mel", ",", "den", "man", ",", "meist", "be\u00b7deckt", ",", "in", "ei\u00b7ner", "kla\u00b7ren", "D\u00e4m\u00b7m"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "$,", "ADV", "VVPP", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "rung sieht,", "tokens": ["rung", "sieht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Erreget, nebst dem dunklen Vorwurf der Erden, itzo dem", "tokens": ["Er\u00b7re\u00b7get", ",", "nebst", "dem", "dunk\u00b7len", "Vor\u00b7wurf", "der", "Er\u00b7den", ",", "it\u00b7zo", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADV", "ART"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gem\u00fcht", "tokens": ["Ge\u00b7m\u00fcht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ein\u2019 Art von angenehmer Schwehrmuht, zufriedner Unzu-", "tokens": ["Ein'", "Art", "von", "an\u00b7ge\u00b7neh\u00b7mer", "Schwehr\u00b7muht", ",", "zu\u00b7fried\u00b7ner", "Un\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "friedenheit,", "tokens": ["frie\u00b7den\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ein melancholisches Vergn\u00fcgen, und eine s\u00fcsse Trau-", "tokens": ["Ein", "me\u00b7lan\u00b7cho\u00b7li\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen", ",", "und", "ei\u00b7ne", "s\u00fcs\u00b7se", "Trau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "rigkeit.", "tokens": ["rig\u00b7keit", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Mich deucht, da\u00df wenn ich itzt bedachtsam, bey stiller Luft,", "tokens": ["Mich", "deucht", ",", "da\u00df", "wenn", "ich", "itzt", "be\u00b7dacht\u00b7sam", ",", "bey", "stil\u00b7ler", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "KOUS", "PPER", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "spatzieren gehe,", "tokens": ["spat\u00b7zie\u00b7ren", "ge\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und rings umher, in solcher Ordnung, von der bestellten", "tokens": ["Und", "rings", "um\u00b7her", ",", "in", "sol\u00b7cher", "Ord\u00b7nung", ",", "von", "der", "be\u00b7stell\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "$,", "APPR", "PIAT", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Felder Flur", "tokens": ["Fel\u00b7der", "Flur"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Die dunkel-braune Weite schau, ich von der emsigen Natur", "tokens": ["Die", "dun\u00b7kel\u00b7brau\u00b7ne", "Wei\u00b7te", "schau", ",", "ich", "von", "der", "em\u00b7si\u00b7gen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Ein ernsthaft majest\u00e4tisch Wesen, mit einer Art von Ehr-", "tokens": ["Ein", "ernst\u00b7haft", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "We\u00b7sen", ",", "mit", "ei\u00b7ner", "Art", "von", "Ehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,", "APPR", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "furcht, sehe.", "tokens": ["furcht", ",", "se\u00b7he", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Es ist auf solcher flachen Ebne nun alles leer und nichts", "tokens": ["Es", "ist", "auf", "sol\u00b7cher", "fla\u00b7chen", "Eb\u00b7ne", "nun", "al\u00b7les", "leer", "und", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "ADV", "PIS", "ADJD", "KON", "PIS"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "zu sehn;", "tokens": ["zu", "sehn", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Doch scheinet mir die flache Weite, recht wie ein neuer", "tokens": ["Doch", "schei\u00b7net", "mir", "die", "fla\u00b7che", "Wei\u00b7te", ",", "recht", "wie", "ein", "neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schauplatz, sch\u00f6n.", "tokens": ["Schau\u00b7platz", ",", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Man sieht agirende Personen zwar itzt nicht auf demselben", "tokens": ["Man", "sieht", "a\u00b7gi\u00b7ren\u00b7de", "Per\u00b7so\u00b7nen", "zwar", "itzt", "nicht", "auf", "dem\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "ADV", "ADV", "PTKNEG", "APPR", "PDAT"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "stehn;", "tokens": ["stehn", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Doch zeigt die allgemeine Stille, auf der sonst nimmer stillen", "tokens": ["Doch", "zeigt", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Stil\u00b7le", ",", "auf", "der", "sonst", "nim\u00b7mer", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-++-+-", "measure": "unknown.measure.octa.plus"}, "line.8": {"text": "Erde,", "tokens": ["Er\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dem, der auf ihren Zustand achtet, und ihr Betragen kennt,", "tokens": ["Dem", ",", "der", "auf", "ih\u00b7ren", "Zu\u00b7stand", "ach\u00b7tet", ",", "und", "ihr", "Be\u00b7tra\u00b7gen", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "es werde,", "tokens": ["es", "wer\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Zu neuen Handlungen, von ihr itzt hintern Scenen was", "tokens": ["Zu", "neu\u00b7en", "Hand\u00b7lun\u00b7gen", ",", "von", "ihr", "itzt", "hin\u00b7tern", "Sce\u00b7nen", "was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PPER", "ADV", "ADJA", "NN", "PWS"], "meter": "-+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.12": {"text": "geschehn.", "tokens": ["ge\u00b7schehn", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Ja wohl geschicht was sonderlichs, es wirket die Natur", "tokens": ["Ja", "wohl", "ge\u00b7schicht", "was", "son\u00b7der\u00b7lichs", ",", "es", "wir\u00b7ket", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VVPP", "PWS", "PIS", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "von innen,", "tokens": ["von", "in\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "(obgleich das, so sie itzt verrichtet und treibt, kein Vorwurf", "tokens": ["(", "ob\u00b7gleich", "das", ",", "so", "sie", "itzt", "ver\u00b7rich\u00b7tet", "und", "treibt", ",", "kein", "Vor\u00b7wurf"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KOUS", "PDS", "$,", "ADV", "PPER", "ADV", "VVPP", "KON", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "unsrer Sinnen)", "tokens": ["uns\u00b7rer", "Sin\u00b7nen", ")"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mit vielem Flei\u00df, und in der Stille, das allern\u00fctzlichste", "tokens": ["Mit", "vie\u00b7lem", "Flei\u00df", ",", "und", "in", "der", "Stil\u00b7le", ",", "das", "al\u00b7ler\u00b7n\u00fctz\u00b7lichs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "NN", "$,", "KON", "APPR", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Gesch\u00e4fte.", "tokens": ["Ge\u00b7sch\u00e4f\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sie l\u00e4sset itzt den Saamen keimen, hierzu gebraucht sie", "tokens": ["Sie", "l\u00e4s\u00b7set", "itzt", "den", "Saa\u00b7men", "kei\u00b7men", ",", "hier\u00b7zu", "ge\u00b7braucht", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,", "PAV", "VVFIN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "ihre Kr\u00e4fte,", "tokens": ["ih\u00b7re", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und wendet sie, ohn\u2019 unser Zuthun, so ernstlich und so emsig", "tokens": ["Und", "wen\u00b7det", "sie", ",", "ohn'", "un\u00b7ser", "Zu\u00b7thun", ",", "so", "ernst\u00b7lich", "und", "so", "em\u00b7sig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+--+--+--", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "an,", "tokens": ["an", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Da\u00df man in kurzer Zeit bereits der Arbeit Fr\u00fcchte sp\u00fchren", "tokens": ["Da\u00df", "man", "in", "kur\u00b7zer", "Zeit", "be\u00b7reits", "der", "Ar\u00b7beit", "Fr\u00fcch\u00b7te", "sp\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "kann.", "tokens": ["kann", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Aus den geraden Furchen dringet die gr\u00fcne Saat gemach", "tokens": ["Aus", "den