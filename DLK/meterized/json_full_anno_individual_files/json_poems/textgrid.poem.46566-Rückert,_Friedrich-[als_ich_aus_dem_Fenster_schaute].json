{"textgrid.poem.46566": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als ich aus dem Fenster schaute]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich aus dem Fenster schaute", "tokens": ["Als", "ich", "aus", "dem", "Fens\u00b7ter", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem wintergrauen Himmel,", "tokens": ["Nach", "dem", "win\u00b7ter\u00b7grau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein einz'ger Streifen Lichtes", "tokens": ["Wo", "ein", "einz'\u00b7ger", "Strei\u00b7fen", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mir die Bahnen schien zu zeichnen,", "tokens": ["Mir", "die", "Bah\u00b7nen", "schien", "zu", "zeich\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mein Engel angeflogen;", "tokens": ["Die", "mein", "En\u00b7gel", "an\u00b7ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fielen meine Thr\u00e4nentropfen,", "tokens": ["Fie\u00b7len", "mei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich merkte, da\u00df sie fielen,", "tokens": ["Und", "ich", "merk\u00b7te", ",", "da\u00df", "sie", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur weil sie auf Gl\u00e4ser klangen,", "tokens": ["Nur", "weil", "sie", "auf", "Gl\u00e4\u00b7ser", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die da vor dem Fenster standen.", "tokens": ["Die", "da", "vor", "dem", "Fens\u00b7ter", "stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Soviel Arzeneiengl\u00e4ser,", "tokens": ["So\u00b7viel", "Ar\u00b7ze\u00b7nei\u00b7en\u00b7gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit den myst'schen Signaturen,", "tokens": ["Mit", "den", "myst'\u00b7schen", "Sig\u00b7na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zugez\u00e4hlt nach Stund' und Tropfen,", "tokens": ["Zu\u00b7ge\u00b7z\u00e4hlt", "nach", "Stund'", "und", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Konnten nicht ein Leben fristen.", "to