{"textgrid.poem.55139": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fc\u00dfe Sorgen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weichet, Sorgen, von mir! \u2013 Doch ach! den sterblichen Menschen", "tokens": ["Wei\u00b7chet", ",", "Sor\u00b7gen", ",", "von", "mir", "!", "\u2013", "Doch", "ach", "!", "den", "sterb\u00b7li\u00b7chen", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPER", "$.", "$(", "KON", "XY", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "L\u00e4sset die Sorge nicht los, eh ihn das Leben verl\u00e4\u00dft.", "tokens": ["L\u00e4s\u00b7set", "die", "Sor\u00b7ge", "nicht", "los", ",", "eh", "ihn", "das", "Le\u00b7ben", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Soll es einmal denn sein, so kommt ihr, Sorgen der Liebe,", "tokens": ["Soll", "es", "ein\u00b7mal", "denn", "sein", ",", "so", "kommt", "ihr", ",", "Sor\u00b7gen", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Treibt die Geschwister hinaus, nehmt und behauptet mein Herz!", "tokens": ["Treibt", "die", "Ge\u00b7schwis\u00b7ter", "hin\u00b7aus", ",", "nehmt", "und", "be\u00b7haup\u00b7tet", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APZR", "$,", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Weichet, Sorgen, von mir! \u2013 Doch ach! den sterblichen Menschen", "tokens": ["Wei\u00b7chet", ",", "Sor\u00b7gen", ",", "von", "mir", "!", "\u2013", "Doch", "ach", "!", "den", "sterb\u00b7li\u00b7chen", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPER", "$.", "$(", "KON", "XY", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "L\u00e4sset die Sorge nicht los, eh ihn das Leben verl\u00e4\u00dft.", "tokens": ["L\u00e4s\u00b7set", "die", "Sor\u00b7ge", "nicht", "los", ",", "eh", "ihn", "das", "Le\u00b7ben", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Soll es einmal denn sein, so kommt ihr, Sorgen der Liebe,", "tokens": ["Soll", "es", "ein\u00b7mal", "denn", "sein", ",", "so", "kommt", "ihr", ",", "Sor\u00b7gen", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Treibt die Geschwister hinaus, nehmt und behauptet mein Herz!",