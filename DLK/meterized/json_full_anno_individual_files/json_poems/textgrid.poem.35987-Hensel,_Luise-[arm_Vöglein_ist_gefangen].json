{"textgrid.poem.35987": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[arm V\u00f6glein ist gefangen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arm V\u00f6glein ist gefangen,", "tokens": ["Arm", "V\u00f6\u00b7glein", "ist", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein H\u00e4uslein ist eng und klein,", "tokens": ["Sein", "H\u00e4us\u00b7lein", "ist", "eng", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Freuden sind alle vergangen,", "tokens": ["Die", "Freu\u00b7den", "sind", "al\u00b7le", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du armes, buntes lieb's V\u00f6gelein.", "tokens": ["Du", "ar\u00b7mes", ",", "bun\u00b7tes", "lie\u00b7b's", "V\u00f6\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, w\u00e4r' nur mein Kerkerlein weiter,", "tokens": ["Ach", ",", "w\u00e4r'", "nur", "mein", "Ker\u00b7ker\u00b7lein", "wei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ach, nur ein Spannelein breit,", "tokens": ["Ach", ",", "nur", "ein", "Span\u00b7nel\u00b7ein", "breit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Dann k\u00f6nnt' ich mein Beinchen doch r\u00fchren,", "tokens": ["Dann", "k\u00f6nnt'", "ich", "mein", "Be\u00b7in\u00b7chen", "doch", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn einmal die Sonne scheint.", "tokens": ["Wenn", "ein\u00b7mal", "die", "Son\u00b7ne", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach, bitt' euch, wenn nun ist gekommen", "tokens": ["Ach", ",", "bitt'", "euch", ",", "wenn", "nun", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "KOUS", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der bitterlich eiskalte Tod,", "tokens": ["Der", "bit\u00b7ter\u00b7lich", "eis\u00b7kal\u00b7te", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerbrecht mir mein Kerkerlein eilig,", "tokens": ["Zer\u00b7brecht", "mir", "mein", "Ker\u00b7ker\u00b7lein", "ei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dann fliegt doch mein Seelchen zu Gott.", "tokens": ["Dann", "fliegt", "doch", "mein", "Seel\u00b7chen", "zu", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es soll ihm gewi\u00dflich nichts sagen", "tokens": ["Es", "soll", "ihm", "ge\u00b7wi\u00df\u00b7lich", "nichts", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von wegen dem Kerkerlein;", "tokens": ["Von", "we\u00b7gen", "dem", "Ker\u00b7ker\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich will euch herunter auch schmei\u00dfen", "tokens": ["Ich", "will", "euch", "her\u00b7un\u00b7ter", "auch", "schmei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Rot \u00c4ppel und Bl\u00fcmelein fein.", "tokens": ["Rot", "\u00c4p\u00b7pel", "und", "Bl\u00fc\u00b7me\u00b7lein", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Arm V\u00f6glein ist gefangen,", "tokens": ["Arm", "V\u00f6\u00b7glein", "ist", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein H\u00e4uslein ist eng und klein,", "tokens": ["Sein", "H\u00e4us\u00b7lein", "ist", "eng", "und", "klein", ","], "token_info": ["word", "word", "w