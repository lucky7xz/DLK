{"textgrid.poem.45856": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[auf Dornen oder Rosen hingesunken?]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Dornen oder Rosen hingesunken? \u2013", "tokens": ["Auf", "Dor\u00b7nen", "o\u00b7der", "Ro\u00b7sen", "hin\u00b7ge\u00b7sun\u00b7ken", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Ob leiser Atem von den Lippen flie\u00dft \u2013", "tokens": ["\u2013", "Ob", "lei\u00b7ser", "A\u00b7tem", "von", "den", "Lip\u00b7pen", "flie\u00dft", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Ob ihr der Krampf den kleinen Mund verschlie\u00dft \u2013", "tokens": ["\u2013", "Ob", "ihr", "der", "Krampf", "den", "klei\u00b7nen", "Mund", "ver\u00b7schlie\u00dft", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 Kein \u00d6l die Lampe? \u2013 oder keinen Funken? \u2013", "tokens": ["\u2013", "Kein", "\u00d6l", "die", "Lam\u00b7pe", "?", "\u2013", "o\u00b7der", "kei\u00b7nen", "Fun\u00b7ken", "?", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "ART", "NN", "$.", "$(", "KON", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "s