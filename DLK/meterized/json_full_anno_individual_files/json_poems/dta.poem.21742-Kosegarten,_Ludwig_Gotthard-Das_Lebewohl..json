{"dta.poem.21742": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Lebewohl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fahret wohl, ihr gr&#252;nen Matten,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "gr", "&#252;", "nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Murmelbach durchrollt.", "tokens": ["Die", "der", "Mur\u00b7mel\u00b7bach", "durch\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahret wohl, ihr trauten Schatten,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "trau\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ihr zwiefach Labsal zollt.", "tokens": ["Die", "ihr", "zwie\u00b7fach", "Lab\u00b7sal", "zollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fahret wohl, bebl\u00fcmte Triften,", "tokens": ["Fah\u00b7ret", "wohl", ",", "be\u00b7bl\u00fcm\u00b7te", "Trif\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ein ew'ger Fr\u00fchling schm\u00fcckt,", "tokens": ["Die", "ein", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Die ihr, reich an Schmelz und D\u00fcften", "tokens": ["Die", "ihr", ",", "reich", "an", "Schmelz", "und", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr denn einen Sinn erquickt.", "tokens": ["Mehr", "denn", "ei\u00b7nen", "Sinn", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fromme Kinder dieser Fluren,", "tokens": ["From\u00b7me", "Kin\u00b7der", "die\u00b7ser", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr freundlich mich umfingt,", "tokens": ["Die", "ihr", "freund\u00b7lich", "mich", "um\u00b7fingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr, liebende Naturen,", "tokens": ["Die", "ihr", ",", "lie\u00b7ben\u00b7de", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Euch vertraulich an mich hingt;", "tokens": ["Euch", "ver\u00b7trau\u00b7lich", "an", "mich", "hingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O ihr Guten, o ihr Frommen,", "tokens": ["O", "ihr", "Gu\u00b7ten", ",", "o", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denket mein, und lebet wohl:", "tokens": ["Den\u00b7ket", "mein", ",", "und", "le\u00b7bet", "wohl", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn das St\u00fcndlein ist gekommen,", "tokens": ["Denn", "das", "St\u00fcnd\u00b7lein", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo ich euch verlassen soll.", "tokens": ["Wo", "ich", "euch", "ver\u00b7las\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der du meine Seele schm\u00fccktest,", "tokens": ["Der", "du", "mei\u00b7ne", "See\u00b7le", "schm\u00fcck\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edler Theodosius,", "tokens": ["Ed\u00b7ler", "Theo\u00b7do\u00b7si\u00b7us", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mich mit manchem Trunk entz\u00fccktest", "tokens": ["Mich", "mit", "man\u00b7chem", "Trunk", "ent\u00b7z\u00fcck\u00b7test"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Sch\u00f6nheit Silberfluss.", "tokens": ["Aus", "der", "Sch\u00f6n\u00b7heit", "Sil\u00b7ber\u00b7fluss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem f\u00fcr Tugend, dem f\u00fcr Wahrheit", "tokens": ["Dem", "f\u00fcr", "Tu\u00b7gend", ",", "dem", "f\u00fcr", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmelhoch die Seele schwoll,", "tokens": ["Him\u00b7mel\u00b7hoch", "die", "See\u00b7le", "schwoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "J\u00fcngling, reich an Sinn und Klarheit,", "tokens": ["J\u00fcng\u00b7ling", ",", "reich", "an", "Sinn", "und", "Klar\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Edler J\u00fcngling, fahre wohl.", "tokens": ["Ed\u00b7ler", "J\u00fcng\u00b7ling", ",", "fah\u00b7re", "wohl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dorothea, zarte Rose,", "tokens": ["Do\u00b7ro\u00b7thea", ",", "zar\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlummre s\u00fcss, Unschuldige,", "tokens": ["Schlumm\u00b7re", "s\u00fcss", ",", "Un\u00b7schul\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keusche z\u00fcchtige Mimose,", "tokens": ["Keu\u00b7sche", "z\u00fcch\u00b7ti\u00b7ge", "Mi\u00b7mo\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unentweihte Lilie,", "tokens": ["Un\u00b7ent\u00b7weih\u00b7te", "Li\u00b7lie", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Luft und Licht und Thau entfalte", "tokens": ["Luft", "und", "Licht", "und", "Thau", "ent\u00b7fal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebend deines Kelches Zier.", "tokens": ["Lie\u00b7bend", "dei\u00b7nes", "Kel\u00b7ches", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlummre s\u00fcss und schirmend walte", "tokens": ["Schlumm\u00b7re", "s\u00fcss", "und", "schir\u00b7mend", "wal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gottes Engel \u00fcber dir!", "tokens": ["Got\u00b7tes", "En\u00b7gel", "\u00fc\u00b7ber", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Holde Fluren, traute Matten,", "tokens": ["Hol\u00b7de", "Flu\u00b7ren", ",", "trau\u00b7te", "Mat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Murmelbach voll Melodie,", "tokens": ["Mur\u00b7mel\u00b7bach", "voll", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bunte Triften, gr\u00fcne Schatten,", "tokens": ["Bun\u00b7te", "Trif\u00b7ten", ",", "gr\u00fc\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch vergisst Bianka nie.", "tokens": ["Euch", "ver\u00b7gisst", "Bi\u00b7an\u00b7ka", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Theure Menschen, euer denken", "tokens": ["Theu\u00b7re", "Men\u00b7schen", ",", "eu\u00b7er", "den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird Bianka lebenslang.", "tokens": ["Wird", "Bi\u00b7an\u00b7ka", "le\u00b7bens\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Bis sie in die Gruft sie senken,", "tokens": ["Bis", "sie", "in", "die", "Gruft", "sie", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fchlt sich nie Biankens Dank.", "tokens": ["K\u00fchlt", "sich", "nie", "Bi\u00b7an\u00b7kens", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}