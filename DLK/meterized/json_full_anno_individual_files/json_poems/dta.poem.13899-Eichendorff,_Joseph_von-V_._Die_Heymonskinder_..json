{"dta.poem.13899": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Die Heymonskinder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "af:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und was sie ersonnen nun, singen die Drei'.", "tokens": ["Und", "was", "sie", "er\u00b7son\u00b7nen", "nun", ",", "sin\u00b7gen", "die", "Drei'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201eo weh!\u201c ruft ein S\u00e4nger herauf,", "tokens": ["\u201e", "o", "weh", "!", "\u201c", "ruft", "ein", "S\u00e4n\u00b7ger", "her\u00b7auf", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u201eihr schreit ja die k\u00f6stlichsten Noten entzwei!\u201c", "tokens": ["\u201e", "ihr", "schreit", "ja", "die", "k\u00f6st\u00b7lichs\u00b7ten", "No\u00b7ten", "ent\u00b7zwei", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und schwingt zu den Dreien sich auf.", "tokens": ["Und", "schwingt", "zu", "den", "Drei\u00b7en", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "CARD", "PRF", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}