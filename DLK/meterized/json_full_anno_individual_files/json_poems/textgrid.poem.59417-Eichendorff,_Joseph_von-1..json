{"textgrid.poem.59417": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit tiefe, bleiche, stille Felder \u2013", "tokens": ["Weit", "tie\u00b7fe", ",", "blei\u00b7che", ",", "stil\u00b7le", "Fel\u00b7der", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O wie mich das freut,", "tokens": ["O", "wie", "mich", "das", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcber alle, alle T\u00e4ler, W\u00e4lder", "tokens": ["\u00dc\u00b7ber", "al\u00b7le", ",", "al\u00b7le", "T\u00e4\u00b7ler", ",", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PIS", "$,", "PIAT", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die pr\u00e4chtige Einsamkeit!", "tokens": ["Die", "pr\u00e4ch\u00b7ti\u00b7ge", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus der Stadt nur schlagen die Glocken", "tokens": ["Aus", "der", "Stadt", "nur", "schla\u00b7gen", "die", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00dcber die Wipfel herein,", "tokens": ["\u00dc\u00b7ber", "die", "Wip\u00b7fel", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ein Reh hebt den Kopf erschrocken", "tokens": ["Ein", "Reh", "hebt", "den", "Kopf", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schlummert gleich wieder ein.", "tokens": ["Und", "schlum\u00b7mert", "gleich", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Wald aber r\u00fchret die Wipfel", "tokens": ["Der", "Wald", "a\u00b7ber", "r\u00fch\u00b7ret", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Schlaf von der Felsenwand,", "tokens": ["Im", "Schlaf", "von", "der", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn der Herr geht \u00fcber die Gipfel", "tokens": ["Denn", "der", "Herr", "geht", "\u00fc\u00b7ber", "die", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und segnet das stille Land.", "tokens": ["Und", "seg\u00b7net", "das", "stil\u00b7le", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure