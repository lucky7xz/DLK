{"textgrid.poem.32613": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "In der Wildnis.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht senkt sich auf die tiefe Wildnis,", "tokens": ["Nacht", "senkt", "sich", "auf", "die", "tie\u00b7fe", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ziellos irr' ich f\u00fcr und f\u00fcr,", "tokens": ["Und", "ziel\u00b7los", "irr'", "ich", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach rechts und links zweigt sich die Stra\u00dfe,", "tokens": ["Nach", "rechts", "und", "links", "zweigt", "sich", "die", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo wird ein treuer F\u00fchrer mir?", "tokens": ["Wo", "wird", "ein", "treu\u00b7er", "F\u00fch\u00b7rer", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl seh' ich Sterne niederleuchten", "tokens": ["Wohl", "seh'", "ich", "Ster\u00b7ne", "nie\u00b7der\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Himmelsdome, ohne Zahl,", "tokens": ["Vom", "Him\u00b7mels\u00b7do\u00b7me", ",", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch werd' ich auch mein Ziel erreichen,", "tokens": ["Doch", "werd'", "ich", "auch", "mein", "Ziel", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich vertraue ihrem Strahl?", "tokens": ["Wenn", "ich", "ver\u00b7trau\u00b7e", "ih\u00b7rem", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir strahlte einst ein M\u00e4dchenauge,", "tokens": ["Mir", "strahl\u00b7te", "einst", "ein", "M\u00e4d\u00b7chen\u00b7au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem der hellste Stern erblich,", "tokens": ["Vor", "dem", "der", "hells\u00b7te", "Stern", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als ich diesem Strahl vertraute,", "tokens": ["Und", "als", "ich", "die\u00b7sem", "Strahl", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da fand i