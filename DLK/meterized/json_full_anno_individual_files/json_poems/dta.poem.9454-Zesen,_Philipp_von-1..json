{"dta.poem.9454": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unter dem \u00e4pfelbaum/ da Du gebohren", "tokens": ["Un\u00b7ter", "dem", "\u00e4p\u00b7fel\u00b7baum", "/", "da", "Du", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "KOUS", "PPER", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weckt' ich vom Schlaaffe der Eytelkeit dich/", "tokens": ["Weckt'", "ich", "vom", "Schlaaf\u00b7fe", "der", "Ey\u00b7tel\u00b7keit", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da dich die Mutter zur Tochter erkohren/", "tokens": ["Da", "dich", "die", "Mut\u00b7ter", "zur", "Toch\u00b7ter", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freundin/ ach! Schwester/ ach! setze doch mich", "tokens": ["Freun\u00b7din", "/", "ach", "!", "Schwes\u00b7ter", "/", "ach", "!", "set\u00b7ze", "doch", "mich"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "XY", "$.", "NN", "$(", "XY", "$.", "VVFIN", "ADV", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wie sonsten ein Siegel/", "tokens": ["Wie", "sons\u00b7ten", "ein", "Sie\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du lieblicher Spiegel/", "tokens": ["Du", "lieb\u00b7li\u00b7cher", "Spie\u00b7gel", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Aufs Hertze mit Lust;", "tokens": ["Aufs", "Hert\u00b7ze", "mit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "und la\u00df mich erwarmen", "tokens": ["und", "la\u00df", "mich", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "und schlaaffen in Armen/", "tokens": ["und", "schlaaf\u00b7fen", "in", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Halse mich/ Schwester/ ich k\u00fcsse die Brust.", "tokens": ["Hal\u00b7se", "mich", "/", "Schwes\u00b7ter", "/", "ich", "k\u00fcs\u00b7se", "die", "Brust", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Lieben ist st\u00e4rcker als t\u00f6dtliche schmertzen/", "tokens": ["Lie\u00b7ben", "ist", "st\u00e4r\u00b7cker", "als", "t\u00f6dt\u00b7li\u00b7che", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ADJD", "KOKOM", "ADJA", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Brennet wie feuer/ wie hellische gluth/", "tokens": ["Bren\u00b7net", "wie", "feu\u00b7er", "/", "wie", "hel\u00b7li\u00b7sche", "gluth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "$(", "KOKOM", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Funckelt und flammet und gl\u00fcet im Hertzen/", "tokens": ["Fun\u00b7ckelt", "und", "flam\u00b7met", "und", "gl\u00fcet", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lieben ist st\u00e4rcker als Wasser und fluth:", "tokens": ["Lie\u00b7ben", "ist", "st\u00e4r\u00b7cker", "als", "Was\u00b7ser", "und", "fluth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Das Silber vergehet/", "tokens": ["Das", "Sil\u00b7ber", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Liebe bestehet/", "tokens": ["Die", "Lie\u00b7be", "be\u00b7ste\u00b7het", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die flamme des ", "tokens": ["Die", "flam\u00b7me", "des"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Es m\u00fcssen verfallen", "tokens": ["Es", "m\u00fcs\u00b7sen", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Die Edlen metallen/", "tokens": ["Die", "Ed\u00b7len", "me\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Demand und Jaspis mu\u00df bleiben von fern.", "tokens": ["De\u00b7mand", "und", "Jas\u00b7pis", "mu\u00df", "blei\u00b7ben", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VMFIN", "VVINF", "APPR", "ADJD", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}}}}}