{"dta.poem.6852": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Schwindel, in der Trunckenheit,", "tokens": ["Jm", "Schwin\u00b7del", ",", "in", "der", "Trun\u00b7cken\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie auch durch starcker D\u00fcnste Macht,", "tokens": ["Wie", "auch", "durch", "star\u00b7cker", "D\u00fcns\u00b7te", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind unsre Geister aufgebracht:", "tokens": ["Sind", "uns\u00b7re", "Geis\u00b7ter", "auf\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wodurch, wenn sie mit grosser Hefftigkeit", "tokens": ["Wo\u00b7durch", ",", "wenn", "sie", "mit", "gros\u00b7ser", "Heff\u00b7tig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Unordentlich durch unsre Nerven irren;", "tokens": ["Un\u00b7or\u00b7dent\u00b7lich", "durch", "uns\u00b7re", "Ner\u00b7ven", "ir\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sie im Gehirn die Z\u00fcg\u2019 und Spuren gantz verwirren.", "tokens": ["Sie", "im", "Ge\u00b7hirn", "die", "Z\u00fcg'", "und", "Spu\u00b7ren", "gantz", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Bilder zittern sehr, sind doppelt, und verkehrt.", "tokens": ["Die", "Bil\u00b7der", "zit\u00b7tern", "sehr", ",", "sind", "dop\u00b7pelt", ",", "und", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "VAFIN", "ADJD", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch ihren Blick, der gantz verstohrt,", "tokens": ["Durch", "ih\u00b7ren", "Blick", ",", "der", "gantz", "ver\u00b7stohrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und durch ihr wild Geschrey, stellt die Bacchantin vor,", "tokens": ["Und", "durch", "ihr", "wild", "Ge\u00b7schrey", ",", "stellt", "die", "Bac\u00b7chan\u00b7tin", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie nebst den Sinnen sie auch die Vernunfft verlohr,", "tokens": ["Wie", "nebst", "den", "Sin\u00b7nen", "sie", "auch", "die", "Ver\u00b7nunfft", "ver\u00b7lohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Pentheus, der durch solche Wuth", "tokens": ["Und", "Pen\u00b7theus", ",", "der", "durch", "sol\u00b7che", "Wuth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Erschrecket, umgejagt und an zu rasen finge,", "tokens": ["Er\u00b7schre\u00b7cket", ",", "um\u00b7ge\u00b7jagt", "und", "an", "zu", "ra\u00b7sen", "fin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "APPR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sah\u2019 stolpernd lauter Wunder-Dinge", "tokens": ["Sah'", "stol\u00b7pernd", "lau\u00b7ter", "Wun\u00b7der\u00b7Din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zwey Theben und zugleich auch zweyer Son\u0303en Gluht.", "tokens": ["Zwey", "The\u00b7ben", "und", "zu\u00b7gleich", "auch", "zwey\u00b7er", "So\u00f1en", "Gluht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ADV", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}