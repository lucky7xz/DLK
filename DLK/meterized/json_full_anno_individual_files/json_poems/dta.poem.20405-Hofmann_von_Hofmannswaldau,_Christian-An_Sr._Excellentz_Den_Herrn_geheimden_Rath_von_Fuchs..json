{"dta.poem.20405": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Sr. Excellentz/  \n Den Herrn geheimden Rath  \n von Fuchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn eine wolcke glantz aus sonnen-strahlen zieht/", "tokens": ["Wenn", "ei\u00b7ne", "wol\u00b7cke", "glantz", "aus", "son\u00b7nen\u00b7strah\u00b7len", "zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die spreu den diamant/ die ulme reben liebet/", "tokens": ["Die", "spreu", "den", "di\u00b7a\u00b7mant", "/", "die", "ul\u00b7me", "re\u00b7ben", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geringer majoran bey k\u00e4yserkronen bl\u00fcht/", "tokens": ["Ge\u00b7rin\u00b7ger", "ma\u00b7jo\u00b7ran", "bey", "k\u00e4y\u00b7ser\u00b7kro\u00b7nen", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein hoher cederbaum auch pappeln schatten giebet;", "tokens": ["Ein", "ho\u00b7her", "ce\u00b7der\u00b7baum", "auch", "pap\u00b7peln", "schat\u00b7ten", "gie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wundere dich nicht/ du wunder kluger welt/", "tokens": ["So", "wun\u00b7de\u00b7re", "dich", "nicht", "/", "du", "wun\u00b7der", "klu\u00b7ger", "welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sich mein finsterni\u00df zu deinem lichte stellt/", "tokens": ["Da\u00df", "sich", "mein", "fins\u00b7ter\u00b7ni\u00df", "zu", "dei\u00b7nem", "lich\u00b7te", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und seinen schimmer will aus deinen holden augen/", "tokens": ["Und", "sei\u00b7nen", "schim\u00b7mer", "will", "aus", "dei\u00b7nen", "hol\u00b7den", "au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie muscheln ihre krafft aus k\u00fchlen morgen saugen.", "tokens": ["Wie", "mu\u00b7scheln", "ih\u00b7re", "krafft", "aus", "k\u00fch\u00b7len", "mor\u00b7gen", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn was auch die natur vor riesen-wercke zeigt/", "tokens": ["Denn", "was", "auch", "die", "na\u00b7tur", "vor", "rie\u00b7sen\u00b7\u00b7wer\u00b7cke", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kleben wir doch nur/ wie schnecken/ an der erden;", "tokens": ["So", "kle\u00b7ben", "wir", "doch", "nur", "/", "wie", "schne\u00b7cken", "/", "an", "der", "er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOKOM", "VVINF", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo unsre jugend nicht durch fiemde fl\u00fcgel steigt/", "tokens": ["Wo", "uns\u00b7re", "ju\u00b7gend", "nicht", "durch", "fiem\u00b7de", "fl\u00fc\u00b7gel", "steigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und uns ein D\u00e4dalus kan lehren kl\u00fcger werden.", "tokens": ["Und", "uns", "ein", "D\u00e4\u00b7da\u00b7lus", "kan", "leh\u00b7ren", "kl\u00fc\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VMFIN", "VVINF", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum mu\u00df ein junger mensch/ der in den fr\u00fchlings-schein", "tokens": ["Drum", "mu\u00df", "ein", "jun\u00b7ger", "mensch", "/", "der", "in", "den", "fr\u00fch\u00b7lings\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des gl\u00fcckes treten will/ wie balsam-b\u00e4ume seyn/", "tokens": ["Des", "gl\u00fc\u00b7ckes", "tre\u00b7ten", "will", "/", "wie", "bal\u00b7sam\u00b7b\u00e4u\u00b7me", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VMFIN", "$(", "KOKOM", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und gleich wie diese bald ihm einen platz erw\u00e4hlen/", "tokens": ["Und", "gleich", "wie", "die\u00b7se", "bald", "ihm", "ei\u00b7nen", "platz", "er\u00b7w\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "PDS", "ADV", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da es ihm nimmer kan an licht und sonne fehlen.", "tokens": ["Da", "es", "ihm", "nim\u00b7mer", "kan", "an", "licht", "und", "son\u00b7ne", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie aber solt\u2019 ich wohl mein gl\u00fccke/ grosser Rath/", "tokens": ["Wie", "a\u00b7ber", "solt'", "ich", "wohl", "mein", "gl\u00fc\u00b7cke", "/", "gros\u00b7ser", "Rath", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "VVFIN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff einen bessern platz als deine klugheit gr\u00fcnden?", "tokens": ["Auff", "ei\u00b7nen", "bes\u00b7sern", "platz", "als", "dei\u00b7ne", "klug\u00b7heit", "gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Friedrichs hohen geist zur sonne selber hat/", "tokens": ["Die", "Fried\u00b7richs", "ho\u00b7hen", "geist", "zur", "son\u00b7ne", "sel\u00b7ber", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "APPRART", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und also keine noth an strahlen darff empfinden.", "tokens": ["Und", "al\u00b7so", "kei\u00b7ne", "noth", "an", "strah\u00b7len", "darff", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gantz Deutschland wei\u00df bereit/ was deine feder kan/", "tokens": ["Gantz", "Deutschland", "wei\u00df", "be\u00b7reit", "/", "was", "dei\u00b7ne", "fe\u00b7der", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADJD", "$(", "PWS", "PPOSAT", "NN", "VMFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Die rechte schauen dich als einen Solon an/", "tokens": ["Die", "rech\u00b7te", "schau\u00b7en", "dich", "als", "ei\u00b7nen", "So\u00b7lon", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "KOUS", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Marck wird aber bald die wunder deiner gaben/", "tokens": ["Die", "Marck", "wird", "a\u00b7ber", "bald", "die", "wun\u00b7der", "dei\u00b7ner", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie Rom des Cato ruhm/ in ertz und marmol graben.", "tokens": ["Wie", "Rom", "des", "Ca\u00b7to", "ruhm", "/", "in", "ertz", "und", "mar\u00b7mol", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NE", "NN", "$(", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn was vor witz und kunst im Janus nur erdacht/", "tokens": ["Denn", "was", "vor", "witz", "und", "kunst", "im", "Ja\u00b7nus", "nur", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "KON", "ADV", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ihm das alterthum lie\u00df zwey gesichter schnitzen/", "tokens": ["Wann", "ihm", "das", "al\u00b7ter\u00b7thum", "lie\u00df", "zwey", "ge\u00b7sich\u00b7ter", "schnit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "CARD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat dein gelehrter kopff nunmehro wahr gemacht/", "tokens": ["Hat", "dein", "ge\u00b7lehr\u00b7ter", "kopff", "nun\u00b7meh\u00b7ro", "wahr", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn seiner augenlicht kan vor- und r\u00fcckw\u00e4rts plitzen.", "tokens": ["Wenn", "sei\u00b7ner", "au\u00b7gen\u00b7licht", "kan", "vor", "und", "r\u00fcck\u00b7w\u00e4rts", "plit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "TRUNC", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das ist: Wenn sein verstand in das vergangne blickt/", "tokens": ["Das", "ist", ":", "Wenn", "sein", "ver\u00b7stand", "in", "das", "ver\u00b7gang\u00b7ne", "blickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "KOUS", "PPOSAT", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und als ein Hercules die sorgen unterdr\u00fcckt.", "tokens": ["Und", "als", "ein", "Her\u00b7cu\u00b7les", "die", "sor\u00b7gen", "un\u00b7ter\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was aber hie und da vor schaden will geschehen/", "tokens": ["Was", "a\u00b7ber", "hie", "und", "da", "vor", "scha\u00b7den", "will", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "KON", "ADV", "APPR", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie Campanella/ bald kan in gedancken sehen.", "tokens": ["Wie", "Cam\u00b7pa\u00b7nel\u00b7la", "/", "bald", "kan", "in", "ge\u00b7dan\u00b7cken", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wo ein rechter rath dem zarten flachse gleicht/", "tokens": ["Doch", "wo", "ein", "rech\u00b7ter", "rath", "dem", "zar\u00b7ten", "flach\u00b7se", "gleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in der jugend schon so wie smaragden gr\u00fcnet/", "tokens": ["Der", "in", "der", "ju\u00b7gend", "schon", "so", "wie", "sma\u00b7rag\u00b7den", "gr\u00fc\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADV", "KOKOM", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit himmels-farbe bl\u00fcht/ von sonnen-hitze bleicht/", "tokens": ["Mit", "him\u00b7mels\u00b7fa\u00b7rbe", "bl\u00fcht", "/", "von", "son\u00b7nen\u00b7hit\u00b7ze", "bleicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$(", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und endlich aller welt zu weisser leinwand dienet:", "tokens": ["Und", "end\u00b7lich", "al\u00b7ler", "welt", "zu", "weis\u00b7ser", "lein\u00b7wand", "die\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So geust ihm die natur zwar milch und klugheit ein/", "tokens": ["So", "geust", "ihm", "die", "na\u00b7tur", "zwar", "milch", "und", "klug\u00b7heit", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch mu\u00df sein absehn auch dem himmel \u00e4hnlich seyn;", "tokens": ["Doch", "mu\u00df", "sein", "ab\u00b7sehn", "auch", "dem", "him\u00b7mel", "\u00e4hn\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "VVFIN", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und letzlich/ wie der flachs im schoo\u00dfe tieffer erden/", "tokens": ["Und", "letz\u00b7lich", "/", "wie", "der", "flachs", "im", "schoo\u00b7\u00dfe", "tief\u00b7fer", "er\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KOKOM", "ART", "ADJA", "APPRART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch hohe sonnen reiff/ durch m\u00fche nutzbar werden.", "tokens": ["Durch", "ho\u00b7he", "son\u00b7nen", "reiff", "/", "durch", "m\u00fc\u00b7he", "nutz\u00b7bar", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "APPR", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hochwohlgebohrner Herr/ was dein verstand gebiert/", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ner", "Herr", "/", "was", "dein", "ver\u00b7stand", "ge\u00b7biert", "/"], "tok