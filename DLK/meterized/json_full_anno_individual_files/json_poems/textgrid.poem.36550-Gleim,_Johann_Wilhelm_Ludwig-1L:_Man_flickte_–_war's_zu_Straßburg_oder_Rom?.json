{"textgrid.poem.36550": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man flickte \u2013 war's zu Stra\u00dfburg oder Rom?", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man flickte \u2013 war's zu Stra\u00dfburg oder Rom?", "tokens": ["Man", "flick\u00b7te", "\u2013", "wa\u00b7r's", "zu", "Stra\u00df\u00b7burg", "o\u00b7der", "Rom", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "VAFIN", "APPR", "NE", "KON", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich wei\u00df es nicht \u2013 an einem Dom,", "tokens": ["Ich", "wei\u00df", "es", "nicht", "\u2013", "an", "ei\u00b7nem", "Dom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jagte Mutter, Br\u00fcder, Schwestern", "tokens": ["Und", "jag\u00b7te", "Mut\u00b7ter", ",", "Br\u00fc\u00b7der", ",", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sperlingvolks aus ihren Nestern;", "tokens": ["Des", "Sper\u00b7ling\u00b7volks", "aus", "ih\u00b7ren", "Nes\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und als die Flickerei zu Ende war,", "tokens": ["Und", "als", "die", "Fli\u00b7cke\u00b7rei", "zu", "En\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da kam, bei Tausenden, die Schar", "tokens": ["Da", "kam", ",", "bei", "Tau\u00b7sen\u00b7den", ",", "die", "Schar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Fl\u00fcchtigen zur\u00fcck geflogen;", "tokens": ["Der", "Fl\u00fcch\u00b7ti\u00b7gen", "zu\u00b7r\u00fcck", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und freudig h\u00e4tte jedes Paar", "tokens": ["Und", "freu\u00b7dig", "h\u00e4t\u00b7te", "je\u00b7des", "Paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sein Nestchen wieder gern bezogen;", "tokens": ["Sein", "Nest\u00b7chen", "wie\u00b7der", "gern", "be\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Allein man sah betr\u00fcbt, da\u00df keins gelassen war.", "tokens": ["Al\u00b7lein", "man", "sah", "be\u00b7tr\u00fcbt", ",", "da\u00df", "keins", "ge\u00b7las\u00b7sen", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "VVPP", "$,", "KOUS", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und: Gott! was hat sie doch bewogen,", "tokens": ["Und", ":", "Gott", "!", "was", "hat", "sie", "doch", "be\u00b7wo\u00b7gen", ","], "to