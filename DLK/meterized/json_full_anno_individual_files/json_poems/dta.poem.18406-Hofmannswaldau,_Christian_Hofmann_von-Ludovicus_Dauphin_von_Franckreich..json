{"dta.poem.18406": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ludovicus Dauphin von Franckreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dj\u00df ist ein junger Mars/ der offt ins feld ger\u00fccket/", "tokens": ["Dj\u00df", "ist", "ein", "jun\u00b7ger", "Mars", "/", "der", "offt", "ins", "feld", "ge\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und manchen festen platz von weiten angeblicket;", "tokens": ["Und", "man\u00b7chen", "fes\u00b7ten", "platz", "von", "wei\u00b7ten", "an\u00b7ge\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein heldenmuth bezeigt/ da\u00df nicht die meinung fehlt/", "tokens": ["Sein", "hel\u00b7den\u00b7muth", "be\u00b7zeigt", "/", "da\u00df", "nicht", "die", "mei\u00b7nung", "fehlt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$(", "KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wodurch man den Delphin zu zahmen fischen zehlt.", "tokens": ["Wo\u00b7durch", "man", "den", "Del\u00b7phin", "zu", "zah\u00b7men", "fi\u00b7schen", "zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PTKZU", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}