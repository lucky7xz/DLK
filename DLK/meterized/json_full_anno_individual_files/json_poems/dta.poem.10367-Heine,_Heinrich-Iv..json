{"dta.poem.10367": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im Hirn spukt mir ein M\u00e4hrchen wunderfein,", "tokens": ["Im", "Hirn", "spukt", "mir", "ein", "M\u00e4hr\u00b7chen", "wun\u00b7der\u00b7fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und in dem M\u00e4hrchen klingt ein feines Lied,", "tokens": ["Und", "in", "dem", "M\u00e4hr\u00b7chen", "klingt", "ein", "fei\u00b7nes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in dem Liede lebt und webt und bl\u00fcht", "tokens": ["Und", "in", "dem", "Lie\u00b7de", "lebt", "und", "webt", "und", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein wundersch\u00f6nes, zartes M\u00e4gdelein.", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", ",", "zar\u00b7tes", "M\u00e4g\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in dem M\u00e4gdlein wohnt ein Herzchen klein,", "tokens": ["Und", "in", "dem", "M\u00e4gd\u00b7lein", "wohnt", "ein", "Herz\u00b7chen", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch in dem Herzchen keine Liebe gl\u00fcht;", "tokens": ["Doch", "in", "dem", "Herz\u00b7chen", "kei\u00b7ne", "Lie\u00b7be", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In dieses lieblos frostige Gem\u00fcth", "tokens": ["In", "die\u00b7ses", "lieb\u00b7los", "fros\u00b7ti\u00b7ge", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kam Hochmuth nur und Uebermuth hinein.", "tokens": ["Kam", "Hoch\u00b7muth", "nur", "und", "Ue\u00b7ber\u00b7muth", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "H\u00f6rst du wie mir im Kopf' das M\u00e4hrchen klinget?", "tokens": ["H\u00f6rst", "du", "wie", "mir", "im", "Kopf'", "das", "M\u00e4hr\u00b7chen", "klin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wie das Liedchen summet ernst und schaurig?", "tokens": ["Und", "wie", "das", "Lied\u00b7chen", "sum\u00b7met", "ernst", "und", "schau\u00b7rig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und wie das M\u00e4gdlein kichert leise, leise?", "tokens": ["Und", "wie", "das", "M\u00e4gd\u00b7lein", "ki\u00b7chert", "lei\u00b7se", ",", "lei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ich f\u00fcrchte nur, da\u00df mir der Kopf zerspringet:", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nur", ",", "da\u00df", "mir", "der", "Kopf", "zer\u00b7sprin\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und, ach! da w\u00e4r's doch gar entsetzlich traurig,", "tokens": ["Und", ",", "ach", "!", "da", "w\u00e4r's", "doch", "gar", "ent\u00b7setz\u00b7lich", "trau\u00b7rig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ADV", "VAFIN", "ADV", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "K\u00e4m' der Verstand mir aus dem alten Gleise.", "tokens": ["K\u00e4m'", "der", "Ver\u00b7stand", "mir", "aus", "dem", "al\u00b7ten", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}