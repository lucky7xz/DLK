{"textgrid.poem.34611": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihre ver\u00e4nderung", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast/ o Sylvia! mein treues hertz besiegt/", "tokens": ["Du", "hast", "/", "o", "Syl\u00b7via", "!", "mein", "treu\u00b7es", "hertz", "be\u00b7siegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "FM", "NE", "$.", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und liessest neulich selbst mich deine neigung wissen:", "tokens": ["Und", "lies\u00b7sest", "neu\u00b7lich", "selbst", "mich", "dei\u00b7ne", "nei\u00b7gung", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch da das arme ding in deinen ketten liegt/", "tokens": ["Doch", "da", "das", "ar\u00b7me", "ding", "in", "dei\u00b7nen", "ket\u00b7ten", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So jagest du mich fort/ und st\u00f6ssest mich mit f\u00fcssen.", "tokens": ["So", "ja\u00b7gest", "du", "mich", "fort", "/", "und", "st\u00f6s\u00b7sest", "mich", "mit", "f\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$(", "KON", "VVFIN", "PRF", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach allzugrausame! was qu\u00e4lstu meine glieder?", "tokens": ["Ach", "all\u00b7zu\u00b7grau\u00b7sa\u00b7me", "!", "was", "qu\u00e4l\u00b7stu", "mei\u00b7ne", "glie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Behalt dir die person/ gieb mir das hertze wieder.", "tokens": ["Be\u00b7halt", "dir", "die", "per\u00b7son", "/", "gieb", "mir", "das", "hert\u00b7ze", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "$(", "VVIMP", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du hast/ o Sylvia! mein treues hertz besiegt/", "tokens": ["Du", "hast", "/", "o", "Syl\u00b7via", "!", "mein", "treu\u00b7es", "hertz", "be\u00b7siegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "FM", "NE", "$.", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und liessest neulich selbst mich deine neigung wissen:", "tokens": ["Und", "lies\u00b7sest", "neu\u00b7lich", "selbst", "mich", "dei\u00b7ne", "nei\u00b7gung", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch da das arme ding in deinen ketten liegt/", "tokens": ["Doch", "da", "das", "ar\u00b7me", "ding", "in", "dei\u00b7nen", "ket\u00b7ten", "liegt",