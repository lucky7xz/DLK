{"dta.poem.5380": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich thue denn, o HErr, allhier so viel ich kann,", "tokens": ["Ich", "thue", "denn", ",", "o", "Herr", ",", "all\u00b7hier", "so", "viel", "ich", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "FM", "NN", "$,", "ADV", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und bete dich, mit Lust, in deinen Wercken an.", "tokens": ["Und", "be\u00b7te", "dich", ",", "mit", "Lust", ",", "in", "dei\u00b7nen", "Wer\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn aber doch, wenn etwas soll gelingen,", "tokens": ["Wenn", "a\u00b7ber", "doch", ",", "wenn", "et\u00b7was", "soll", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "$,", "KOUS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du so das Wollen, als Vollbringen,", "tokens": ["Du", "so", "das", "Wol\u00b7len", ",", "als", "Voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns geben must, und wir vor uns allein", "tokens": ["Uns", "ge\u00b7ben", "must", ",", "und", "wir", "vor", "uns", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "KON", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu keinem Guten f\u00e4hig seyn;", "tokens": ["Zu", "kei\u00b7nem", "Gu\u00b7ten", "f\u00e4\u00b7hig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bitt ich, grosser GOtt vermehre,", "tokens": ["So", "bitt", "ich", ",", "gros\u00b7ser", "Gott", "ver\u00b7meh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu deiner Ehre,", "tokens": ["Zu", "dei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Um deines Nahmens Ruhm auch hier stets zu verneuen,", "tokens": ["Um", "dei\u00b7nes", "Nah\u00b7mens", "Ruhm", "auch", "hier", "stets", "zu", "ver\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In mir die F\u00e4higkeit, mich w\u00fcrdig dein zu freuen!", "tokens": ["In", "mir", "die", "F\u00e4\u00b7hig\u00b7keit", ",", "mich", "w\u00fcr\u00b7dig", "dein", "zu", "freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PPER", "ADJD", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}