{"dta.poem.13273": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egenug! es ist genug\u201e, so widersprache sie", "tokens": ["\u201e", "ge\u00b7nug", "!", "es", "ist", "ge\u00b7nug", "\u201e", ",", "so", "wi\u00b7der\u00b7spra\u00b7che", "sie"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "$.", "PPER", "VAFIN", "ADV", "$(", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weisheit Unterricht, du nimmst zu viele M\u00fch,", "tokens": ["Der", "Weis\u00b7heit", "Un\u00b7ter\u00b7richt", ",", "du", "nimmst", "zu", "vie\u00b7le", "M\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder Unterthanen Lust mit Tummheit zu vergleichen,", "tokens": ["\u201e", "der", "Un\u00b7ter\u00b7tha\u00b7nen", "Lust", "mit", "Tumm\u00b7heit", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eh\u00f6r! ich will deinen Gram mit G\u00fctigkeit erweichen.", "tokens": ["\u201e", "h\u00f6r", "!", "ich", "will", "dei\u00b7nen", "Gram", "mit", "G\u00fc\u00b7tig\u00b7keit", "er\u00b7wei\u00b7chen", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos":