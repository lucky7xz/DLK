{"textgrid.poem.48553": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Auf des Wolgebornen Fr\u00e4uleins, Fr\u00e4ulein Agnesen von Sch\u00f6nburg u.s.w. Beisetzung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ja zu beklagen:", "tokens": ["Es", "ist", "ja", "zu", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "auf eins in zweien Tagen", "tokens": ["auf", "eins", "in", "zwei\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "sind zwei Geschwister hin.", "tokens": ["sind", "zwei", "Ge\u00b7schwis\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zwei junge Stammessprossen", "tokens": ["Zwei", "jun\u00b7ge", "Stam\u00b7mes\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sind nun auf eins verschossen;", "tokens": ["sind", "nun", "auf", "eins", "ver\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "di\u00df schmerzet unsren Sin.", "tokens": ["di\u00df", "schmer\u00b7zet", "un\u00b7sren", "Sin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Um was wir gestern weinten", "tokens": ["Um", "was", "wir", "ge\u00b7stern", "wein\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und nicht zu tr\u00f6sten scheinten,", "tokens": ["und", "nicht", "zu", "tr\u00f6s\u00b7ten", "schein\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das machte Held ", "tokens": ["das", "mach\u00b7te", "Held"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Heut' ursachst du, du Liebe,", "tokens": ["Heut'", "ur\u00b7sachst", "du", ",", "du", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "das wir so sehen tr\u00fcbe", "tokens": ["das", "wir", "so", "se\u00b7hen", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und schlagen an die Brust.", "tokens": ["und", "schla\u00b7gen", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er war der Helden Herze,", "tokens": ["Er", "war", "der", "Hel\u00b7den", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "du aller Sch\u00f6nen Kerze,", "tokens": ["du", "al\u00b7ler", "Sch\u00f6\u00b7nen", "Ker\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "so ie gewesen sein.", "tokens": ["so", "ie", "ge\u00b7we\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dich preist man unter Deinen.", "tokens": ["Dich", "preist", "man", "un\u00b7ter", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihn r\u00fchmt man unter Seinen,", "tokens": ["Ihn", "r\u00fchmt", "man", "un\u00b7ter", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "weil keins nicht war gemein'.", "tokens": ["weil", "keins", "nicht", "war", "ge\u00b7mein'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Nymphe, deine Bl\u00fcte,", "tokens": ["O", "Nym\u00b7phe", ",", "dei\u00b7ne", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dein himmlisches Gem\u00fcte,", "tokens": ["dein", "himm\u00b7li\u00b7sches", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "dein tugendvoller Geist", "tokens": ["dein", "tu\u00b7gend\u00b7vol\u00b7ler", "Geist"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "macht, da\u00df wir duppelt m\u00fcssen", "tokens": ["macht", ",", "da\u00df", "wir", "dup\u00b7pelt", "m\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "VMFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "auf Tr\u00e4nen sein beflissen", "tokens": ["auf", "Tr\u00e4\u00b7nen", "sein", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und was uns Trauren hei\u00dft.", "tokens": ["und", "was", "uns", "Trau\u00b7ren", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was man an dir nur sahe,", "tokens": ["Was", "man", "an", "dir", "nur", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das war dem Himmel nahe,", "tokens": ["das", "war", "dem", "Him\u00b7mel", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "daher du warest auch.", "tokens": ["da\u00b7her", "du", "wa\u00b7rest", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nichts mochte dir belieben,", "tokens": ["Nichts", "moch\u00b7te", "dir", "be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was dunkeln kan und tr\u00fcben", "tokens": ["was", "dun\u00b7keln", "kan", "und", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der Eitelkeiten Rauch.", "tokens": ["der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Rauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dein Leben war ein Leben,", "tokens": ["Dein", "Le\u00b7ben", "war", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das stets dem Tod' ergeben", "tokens": ["das", "stets", "dem", "Tod'", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und willig kunte sein,", "tokens": ["und", "wil\u00b7lig", "kun\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wenn einst der Sch\u00f6pfer k\u00e4me", "tokens": ["wenn", "einst", "der", "Sch\u00f6p\u00b7fer", "k\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und wieder zu sich n\u00e4me", "tokens": ["und", "wie\u00b7der", "zu", "sich", "n\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "was er dir vor blies ein.", "tokens": ["was", "er", "dir", "vor", "blies", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "APPR", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nur uns deucht es zu schnelle,", "tokens": ["Nur", "uns", "deucht", "es", "zu", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKZU", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "da\u00df du von deiner Stelle", "tokens": ["da\u00df", "du", "von", "dei\u00b7ner", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und uns gewichen bist.", "tokens": ["und", "uns", "ge\u00b7wi\u00b7chen", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch war es selbst dein Wille,", "tokens": ["Doch", "war", "es", "selbst", "dein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "da\u00df du Gott hieltest stille,", "tokens": ["da\u00df", "du", "Gott", "hiel\u00b7test", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wie tut ein wahrer Christ.", "tokens": ["wie", "tut", "ein", "wah\u00b7rer", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du fromme Menschg\u00f6ttinne,", "tokens": ["Du", "from\u00b7me", "Menschg\u00f6t\u00b7tin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "nun hastu v\u00f6llig inne,", "tokens": ["nun", "has\u00b7tu", "v\u00f6l\u00b7lig", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wornach du hier gestrebt.", "tokens": ["wor\u00b7nach", "du", "hier", "ge\u00b7strebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Worauf du bist gestorben,", "tokens": ["Wo\u00b7rauf", "du", "bist", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "das hastu nun erworben", "tokens": ["das", "has\u00b7tu", "nun", "er\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und tot uns \u00fcberlebt.", "tokens": ["und", "tot", "uns", "\u00fc\u00b7ber\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Itzt m\u00fcssen deine Stralen", "tokens": ["Itzt", "m\u00fcs\u00b7sen", "dei\u00b7ne", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das Blaue sch\u00f6ner malen,", "tokens": ["das", "Blau\u00b7e", "sch\u00f6\u00b7ner", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "uns rauben des Gesichts.", "tokens": ["uns", "rau\u00b7ben", "des", "Ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du f\u00fcnkelst in den Sternen", "tokens": ["Du", "f\u00fcn\u00b7kelst", "in", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und blickest her von Fernen", "tokens": ["und", "bli\u00b7ckest", "her", "von", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "auf dieses gro\u00dfe Nichts.", "tokens": ["auf", "die\u00b7ses", "gro\u00b7\u00dfe", "Nichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die g\u00fcldnen Cherubinnen", "tokens": ["Die", "g\u00fcld\u00b7nen", "Che\u00b7ru\u00b7bin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "bem\u00fchn sich, was sie k\u00f6nnen,", "tokens": ["be\u00b7m\u00fchn", "sich", ",", "was", "sie", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und dienen stets dir auf.", "tokens": ["und", "die\u00b7nen", "stets", "dir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Engel edle Scharen", "tokens": ["Der", "En\u00b7gel", "ed\u00b7le", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "zu hunderttausent Paren", "tokens": ["zu", "hun\u00b7dert\u00b7tau\u00b7sent", "Pa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "begleiten deinen Lauf.", "tokens": ["be\u00b7glei\u00b7ten", "dei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn wird es doch geschehen,", "tokens": ["Wenn", "wird", "es", "doch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da\u00df wir auch k\u00f6nnen sehen,", "tokens": ["da\u00df", "wir", "auch", "k\u00f6n\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "was du schon siehest itzt,", "tokens": ["was", "du", "schon", "sie\u00b7hest", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wo jene drei Personen,", "tokens": ["wo", "je\u00b7ne", "drei", "Per\u00b7so\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "die doch nur Eins sind, wonen,", "tokens": ["die", "doch", "nur", "Eins", "sind", ",", "wo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VAFIN", "$,", "PWAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und wo Gott selber sitzt?", "tokens": ["und", "wo", "Gott", "sel\u00b7ber", "sitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Sei, seelge Seele, seelig", "tokens": ["Sei", ",", "seel\u00b7ge", "See\u00b7le", ",", "see\u00b7lig"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und zeuch auch uns allm\u00e4lig", "tokens": ["und", "zeuch", "auch", "uns", "all\u00b7m\u00e4\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "dir nach und Himmel an!", "tokens": ["dir", "nach", "und", "Him\u00b7mel", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was uns von dir verbleibet,", "tokens": ["Was", "uns", "von", "dir", "ver\u00b7blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "mit dem du warst umleibet,", "tokens": ["mit", "dem", "du", "warst", "um\u00b7lei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sei ehrlich beigetan!", "tokens": ["sei", "ehr\u00b7lich", "bei\u00b7ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Di\u00df, was wir hier verscharren,", "tokens": ["Di\u00df", ",", "was", "wir", "hier", "ver\u00b7schar\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "will deiner Ankunft harren", "tokens": ["will", "dei\u00b7ner", "An\u00b7kunft", "har\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "auf jenen gro\u00dfen Tag.", "tokens": ["auf", "je\u00b7nen", "gro\u00b7\u00dfen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Erde sei ihm leichte,", "tokens": ["Die", "Er\u00b7de", "sei", "ihm", "leich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "so dar ein Ieder reichte,", "tokens": ["so", "dar", "ein", "Ie\u00b7der", "reich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der ihn zu lieben pflag!", "tokens": ["der", "ihn", "zu", "lie\u00b7ben", "pflag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dein Grab, das m\u00fcsse bl\u00fchen,", "tokens": ["Dein", "Grab", ",", "das", "m\u00fcs\u00b7se", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit Lilgen ganz verschnien", "tokens": ["mit", "Lil\u00b7gen", "ganz", "ver\u00b7schni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und aller Blumen Zier!", "tokens": ["und", "al\u00b7ler", "Blu\u00b7men", "Zier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kein Wind, kein Schnee, kein Regen", "tokens": ["Kein", "Wind", ",", "kein", "Schnee", ",", "kein", "Re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "soll deiner Gruft entgegen", "tokens": ["soll", "dei\u00b7ner", "Gruft", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und sein beschwerlich dir.", "tokens": ["und", "sein", "be\u00b7schwer\u00b7lich", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es ist ja zu beklagen:", "tokens": ["Es", "ist", "ja", "zu", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "auf eins in zweien Tagen", "tokens": ["auf", "eins", "in", "zwei\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "sind zwei Geschwister hin.", "tokens": ["sind", "zwei", "Ge\u00b7schwis\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zwei junge Stammessprossen", "tokens": ["Zwei", "jun\u00b7ge", "Stam\u00b7mes\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "sind nun auf eins verschossen;", "tokens": ["sind", "nun", "auf", "eins", "ver\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "di\u00df schmerzet unsren Sin.", "tokens": ["di\u00df", "schmer\u00b7zet", "un\u00b7sren", "Sin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Um was wir gestern weinten", "tokens": ["Um", "was", "wir", "ge\u00b7stern", "wein\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und nicht zu tr\u00f6sten scheinten,", "tokens": ["und", "nicht", "zu", "tr\u00f6s\u00b7ten", "schein\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "das machte Held ", "tokens": ["das", "mach\u00b7te", "Held"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "