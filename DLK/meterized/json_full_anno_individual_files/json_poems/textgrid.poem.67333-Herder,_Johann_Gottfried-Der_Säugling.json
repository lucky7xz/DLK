{"textgrid.poem.67333": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4ugling", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der kleine Sklave, der in Banden", "tokens": ["Wer", "ist", "der", "klei\u00b7ne", "Skla\u00b7ve", ",", "der", "in", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus diesem fr\u00fchen Sarge Klagen weint?", "tokens": ["Aus", "die\u00b7sem", "fr\u00fc\u00b7hen", "Sar\u00b7ge", "Kla\u00b7gen", "weint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Mensch? O l\u00f6set ihn, macht frei ihn von den Banden!", "tokens": ["Ein", "Mensch", "?", "O", "l\u00f6\u00b7set", "ihn", ",", "macht", "frei", "ihn", "von", "den", "Ban\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NE", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Wer Seufzer hemmet, ist ein Menschenfeind.", "tokens": ["Wer", "Seuf\u00b7zer", "hem\u00b7met", ",", "ist", "ein", "Men\u00b7schen\u00b7feind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Wurm darf sich im Staube winden,", "tokens": ["Der", "Wurm", "darf", "sich", "im", "Stau\u00b7be", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Lamm h\u00fcpft um die Mutter her;", "tokens": ["Das", "Lamm", "h\u00fcpft", "um", "die", "Mut\u00b7ter", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihn umh\u00fcllen Binden,", "tokens": ["Und", "ihn", "um\u00b7h\u00fcl\u00b7len", "Bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zwangfesseln, eng und schwer.", "tokens": ["Zwang\u00b7fes\u00b7seln", ",", "eng", "und", "schwer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Du Weltank\u00f6mmling, Deinen zarten H\u00e4nden", "tokens": ["Du", "Wel\u00b7tan\u00b7k\u00f6mm\u00b7ling", ",", "Dei\u00b7nen", "zar\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pr\u00e4gt dies Geschenk Dein Gl\u00fcck des Lebens ein;", "tokens": ["Pr\u00e4gt", "dies", "Ge\u00b7schenk", "Dein", "Gl\u00fcck", "des", "Le\u00b7bens", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um einen Pilgrimsweg von Sarg zu Sarg zu enden,", "tokens": ["Um", "ei\u00b7nen", "Pil\u00b7grims\u00b7weg", "von", "Sarg", "zu", "Sarg", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sollst Du der Sklaven ew'ger Sklave sein.", "tokens": ["Sollst", "Du", "der", "Skla\u00b7ven", "ew'\u00b7ger", "Skla\u00b7ve", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So h\u00f6rt' ich es und singe bebend", "tokens": ["So", "h\u00f6rt'", "ich", "es", "und", "sin\u00b7ge", "be\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Lied, das Dir die Parze sang,", "tokens": ["Das", "Lied", ",", "das", "Dir", "die", "Par\u00b7ze", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als sie den Faden webend", "tokens": ["Als", "sie", "den", "Fa\u00b7den", "we\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Kette um Dich schlang.", "tokens": ["Zur", "Ket\u00b7te", "um", "Dich", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sang: \u00bbO Du im Chaos von Ideen", "tokens": ["Sie", "sang", ":", "\u00bb", "O", "Du", "im", "Chaos", "von", "I\u00b7deen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "PPER", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geborner, wenn Du einst mit Fesseln ringst,", "tokens": ["Ge\u00b7bor\u00b7ner", ",", "wenn", "Du", "einst", "mit", "Fes\u00b7seln", "ringst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie im Schiffbruch dann, um Sonn' und Tag zu sehen,", "tokens": ["Und", "wie", "im", "Schiff\u00b7bruch", "dann", ",", "um", "Sonn'", "und", "Tag", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADV", "$,", "KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Abgrund auf, doch schwer beladen dringst;", "tokens": ["Vom", "Ab\u00b7grund", "auf", ",", "doch", "schwer", "be\u00b7la\u00b7den", "dringst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du h\u00f6rst das Chor der Sterne droben", "tokens": ["Du", "h\u00f6rst", "das", "Chor", "der", "Ster\u00b7ne", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf ewig unverr\u00fcckter Bahn", "tokens": ["Auf", "e\u00b7wig", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "Bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Weltgebieter loben", "tokens": ["Den", "Welt\u00b7ge\u00b7bie\u00b7ter", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und schaust sie liebend an.", "tokens": ["Und", "schaust", "sie", "lie\u00b7bend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dich weckt ihr Hochgesang, und aus der Seele", "tokens": ["Dich", "weckt", "ihr", "Hoch\u00b7ge\u00b7sang", ",", "und", "aus", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00fcrmt in die Fl\u00fcgel Dir des Adlers Muth;", "tokens": ["St\u00fcrmt", "in", "die", "Fl\u00fc\u00b7gel", "Dir", "des", "Ad\u00b7lers", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du w\u00e4gst den schweren Leib, entschwingst den Staub der H\u00f6hle", "tokens": ["Du", "w\u00e4gst", "den", "schwe\u00b7ren", "Leib", ",", "ent\u00b7schwingst", "den", "Staub", "der", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und trinkst im Geiste schon der Sonne Gluth.", "tokens": ["Und", "trinkst", "im", "Geis\u00b7te", "schon", "der", "Son\u00b7ne", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, nicht vom ersten Morgensterne,", "tokens": ["Ach", ",", "nicht", "vom", "ers\u00b7ten", "Mor\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Felsen blickst Du bald hinab,", "tokens": ["Vom", "Fel\u00b7sen", "blickst", "Du", "bald", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schaust in naher Ferne", "tokens": ["Und", "schaust", "in", "na\u00b7her", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Erdenball, Dein Grab.", "tokens": ["Den", "Er\u00b7den\u00b7ball", ",", "Dein", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann klagt Dein Herz, da\u00df, die im Staube wohnen,", "tokens": ["Dann", "klagt", "Dein", "Herz", ",", "da\u00df", ",", "die", "im", "Stau\u00b7be", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Erdenvolk sich lab' an Finsterni\u00df.", "tokens": ["Das", "Er\u00b7den\u00b7volk", "sich", "lab'", "an", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O, Dir zu eigner Ruh, Dein bestes Selbst zu schonen,", "tokens": ["O", ",", "Dir", "zu", "eig\u00b7ner", "Ruh", ",", "Dein", "bes\u00b7tes", "Selbst", "zu", "scho\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPR", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War's, da\u00df ich gr\u00f6\u00dferm Lichte Dich entri\u00df,", "tokens": ["Wa\u00b7r's", ",", "da\u00df", "ich", "gr\u00f6\u00b7\u00dferm", "Lich\u00b7te", "Dich", "ent\u00b7ri\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bis bald der sanfte Schwung der Wiege", "tokens": ["Bis", "bald", "der", "sanf\u00b7te", "Schwung", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Lethe's Welle Dich besprengt", "tokens": ["Mit", "Lethe's", "Wel\u00b7le", "Dich", "be\u00b7sprengt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Dir zum Thorenkriege", "tokens": ["Und", "Dir", "zum", "Tho\u00b7ren\u00b7krie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein weises Phlegma schenkt.\u00ab", "tokens": ["Ein", "wei\u00b7ses", "Phleg\u00b7ma", "schenkt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Parze sprach's. Da trat zu seiner Wiege", "tokens": ["Die", "Par\u00b7ze", "sprach'", "s.", "Da", "trat", "zu", "sei\u00b7ner", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVIMP", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein lichter, leichter Lebensgenius", "tokens": ["Ein", "lich\u00b7ter", ",", "leich\u00b7ter", "Le\u00b7bens\u00b7ge\u00b7nius"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und gab, da\u00df er im Kampf der Thoren nicht erliege,", "tokens": ["Und", "gab", ",", "da\u00df", "er", "im", "Kampf", "der", "Tho\u00b7ren", "nicht", "er\u00b7lie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit seinem Segen ihm den Friedensku\u00df,", "tokens": ["Mit", "sei\u00b7nem", "Se\u00b7gen", "ihm", "den", "Frie\u00b7dens\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gab ihn der Unschuld Mutterh\u00e4nden", "tokens": ["Gab", "ihn", "der", "Un\u00b7schuld", "Mut\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und, sehet! hat sein zartes Haupt,", "tokens": ["Und", ",", "se\u00b7het", "!", "hat", "sein", "zar\u00b7tes", "Haupt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "$.", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den D\u00e4mon abzuwenden,", "tokens": ["Den", "D\u00e4\u00b7mon", "ab\u00b7zu\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit einem Kranz umlaubt.", "tokens": ["Mit", "ei\u00b7nem", "Kranz", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein Kranz der Blume, die verborgen bl\u00fchet", "tokens": ["Ein", "Kranz", "der", "Blu\u00b7me", ",", "die", "ver\u00b7bor\u00b7gen", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schm\u00fcckt ihr sch\u00f6nes Thal auch ungesehn,", "tokens": ["Und", "schm\u00fcckt", "ihr", "sch\u00f6\u00b7nes", "Thal", "auch", "un\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfreut, wenn sie den Blick der Liebe zu sich ziehet,", "tokens": ["Er\u00b7freut", ",", "wenn", "sie", "den", "Blick", "der", "Lie\u00b7be", "zu", "sich", "zie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergn\u00fcgt, wenn keine Blicke sie ersp\u00e4hn.", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "wenn", "kei\u00b7ne", "Bli\u00b7cke", "sie", "er\u00b7sp\u00e4hn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Knabe mit dem Veilchenkranze,", "tokens": ["O", "Kna\u00b7be", "mit", "dem", "Veil\u00b7chen\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sei wie die Blume, die im Gru\u00df", "tokens": ["Sei", "wie", "die", "Blu\u00b7me", ",", "die", "im", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Friedens Dir mit stillem Glanze", "tokens": ["Des", "Frie\u00b7dens", "Dir", "mit", "stil\u00b7lem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Umwand Dein Genius.", "tokens": ["Um\u00b7wand", "Dein", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."