{"textgrid.poem.57526": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich segele k\u00fchn durch den tobenden See,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich segele k\u00fchn durch den tobenden See,", "tokens": ["Ich", "se\u00b7ge\u00b7le", "k\u00fchn", "durch", "den", "to\u00b7ben\u00b7den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durchfahre die b\u00e4umende Flut", "tokens": ["Durch\u00b7fah\u00b7re", "die", "b\u00e4u\u00b7men\u00b7de", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit blo\u00dfen Armen und wehendem Haar", "tokens": ["Mit", "blo\u00b7\u00dfen", "Ar\u00b7men", "und", "we\u00b7hen\u00b7dem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und lebensverachtendem Mut;", "tokens": ["Und", "le\u00b7bens\u00b7ver\u00b7ach\u00b7ten\u00b7dem", "Mut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wohl springen die Wellen am Bord in die H\u00f6h',", "tokens": ["Wohl", "sprin\u00b7gen", "die", "Wel\u00b7len", "am", "Bord", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Bespritzen mir Backen und F\u00fc\u00dfe,", "tokens": ["Be\u00b7sprit\u00b7zen", "mir", "Ba\u00b7cken", "und", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Doch ich achte es nicht", "tokens": ["Doch", "ich", "ach\u00b7te", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und ich rede mir ein,", "tokens": ["Und", "ich", "re\u00b7de", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Es sei'n nur der Seejungfern Gr\u00fc\u00dfe.", "tokens": ["Es", "sei'n", "nur", "der", "See\u00b7jung\u00b7fern", "Gr\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Es w\u00fchlt der Wind in dem straffen Gezeug", "tokens": ["Es", "w\u00fchlt", "der", "Wind", "in", "dem", "straf\u00b7fen", "Ge\u00b7zeug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sprengt fast die st\u00f6hnenden Laken,", "tokens": ["Und", "sprengt", "fast", "die", "st\u00f6h\u00b7nen\u00b7den", "La\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es knastern die Klammern vom zerrenden Seil,", "tokens": ["Es", "knas\u00b7tern", "die", "Klam\u00b7mern", "vom", "zer\u00b7ren\u00b7den", "Seil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es kreischet das Steuer am Haken.", "tokens": ["Es", "krei\u00b7schet", "das", "Steu\u00b7er", "am", "Ha\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wohl spritzt mir der Gischt \u00fcber Stirne und Rock,", "tokens": ["Wohl", "spritzt", "mir", "der", "Gischt", "\u00fc\u00b7ber", "Stir\u00b7ne", "und", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Im Haar die Flocken mir kleben,", "tokens": ["Im", "Haar", "die", "Flo\u00b7cken", "mir", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Um die Stirne mir hell", "tokens": ["Um", "die", "Stir\u00b7ne", "mir", "hell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PPER", "ADJD"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Einen Siegeskranz", "tokens": ["Ei\u00b7nen", "Sie\u00b7ges\u00b7kranz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Die verliebten Seejungfern weben.", "tokens": ["Die", "ver\u00b7lieb\u00b7ten", "See\u00b7jung\u00b7fern", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Von Westen, zu dunkelem Kn\u00e4uel geballt,", "tokens": ["Von", "Wes\u00b7ten", ",", "zu", "dun\u00b7ke\u00b7lem", "Kn\u00e4u\u00b7el", "ge\u00b7ballt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zieht die Wetterwolke herauf,", "tokens": ["Zieht", "die", "Wet\u00b7ter\u00b7wol\u00b7ke", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Von schwefligfalbem Ged\u00fcnste umzuckt,", "tokens": ["Von", "schwef\u00b7lig\u00b7fal\u00b7bem", "Ge\u00b7d\u00fcns\u00b7te", "um\u00b7zuckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beginnt sie den rasenden Lauf.", "tokens": ["Be\u00b7ginnt", "sie", "den", "ra\u00b7sen\u00b7den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Hoioi, mein Schiffchen, nun zeig deine Kraft,", "tokens": ["Ho\u00b7io\u00b7i", ",", "mein", "Schiff\u00b7chen", ",", "nun", "zeig", "dei\u00b7ne", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Jetzt geht es um Leben und Sterben,", "tokens": ["Jetzt", "geht", "es", "um", "Le\u00b7ben", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Doch mich k\u00fcmmert es nicht", "tokens": ["Doch", "mich", "k\u00fcm\u00b7mert", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und ich denke nicht dran,", "tokens": ["Und", "ich", "den\u00b7ke", "nicht", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ob uns Rettung bl\u00fcht oder Verderben.", "tokens": ["Ob", "uns", "Ret\u00b7tung", "bl\u00fcht", "o\u00b7der", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Auf demselbigen Fleck, wo mein Boot grade \u00e4chzt,", "tokens": ["Auf", "dem\u00b7sel\u00b7bi\u00b7gen", "Fleck", ",", "wo", "mein", "Boot", "gra\u00b7de", "\u00e4chzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Sind am gestrigen Tage versunken \u2013", "tokens": ["Sind", "am", "ge\u00b7stri\u00b7gen", "Ta\u00b7ge", "ver\u00b7sun\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von dem Sturme verweht, von der Woge errafft,", "tokens": ["Von", "dem", "Stur\u00b7me", "ver\u00b7weht", ",", "von", "der", "Wo\u00b7ge", "er\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Zwei junge Gesellen ertrunken.", "tokens": ["Zwei", "jun\u00b7ge", "Ge\u00b7sel\u00b7len", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die liegen nun weich in der Seejungfern Arm,", "tokens": ["Die", "lie\u00b7gen", "nun", "weich", "in", "der", "See\u00b7jung\u00b7fern", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Vor Ungl\u00fcck und Kummer geborgen,", "tokens": ["Vor", "Un\u00b7gl\u00fcck", "und", "Kum\u00b7mer", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ich beneide sie nicht,", "tokens": ["Ich", "be\u00b7nei\u00b7de", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Denn ich wei\u00df es ja nicht,", "tokens": ["Denn", "ich", "wei\u00df", "es", "ja", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ob ich selber nicht schlafe dort morgen.", "tokens": ["Ob", "ich", "sel\u00b7ber", "nicht", "schla\u00b7fe", "dort", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "ADV", "ADV", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Dicht \u00fcber der Welle wei\u00dfsch\u00e4umendem Kamm", "tokens": ["Dicht", "\u00fc\u00b7ber", "der", "Wel\u00b7le", "wei\u00df\u00b7sch\u00e4u\u00b7men\u00b7dem", "Kamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die hungernden Seeschwalben kreischen,", "tokens": ["Die", "hun\u00b7gern\u00b7den", "See\u00b7schwal\u00b7ben", "krei\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als wollten sie aus dem gr\u00fcnklaffenden Schlund", "tokens": ["Als", "woll\u00b7ten", "sie", "aus", "dem", "gr\u00fcn\u00b7klaf\u00b7fen\u00b7den", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die gestrigen Opfer erheischen;", "tokens": ["Die", "ge\u00b7stri\u00b7gen", "Op\u00b7fer", "er\u00b7hei\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sie gellen und kreischen dem Sturme zum Trotz,", "tokens": ["Sie", "gel\u00b7len", "und", "krei\u00b7schen", "dem", "Stur\u00b7me", "zum", "Trotz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "VVFIN", "ART", "NN", "APPRART", "N