{"textgrid.poem.38241": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwalben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fliegen zwei Schwalben ins Nachbar sein Haus,", "tokens": ["Es", "flie\u00b7gen", "zwei", "Schwal\u00b7ben", "ins", "Nach\u00b7bar", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie fliegen bald hoch und bald nieder;", "tokens": ["Sie", "flie\u00b7gen", "bald", "hoch", "und", "bald", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Aufs Jahr, da kommen sie wieder,", "tokens": ["Aufs", "Jahr", ",", "da", "kom\u00b7men", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und suchen ihr voriges Haus.", "tokens": ["Und", "su\u00b7chen", "ihr", "vo\u00b7ri\u00b7ges", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie gehen jezt fort ins neue Land,", "tokens": ["Sie", "ge\u00b7hen", "jezt", "fort", "ins", "neu\u00b7e", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ziehen jezt eilig hin\u00fcber;", "tokens": ["Und", "zie\u00b7hen", "jezt", "ei\u00b7lig", "hin\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch kommen sie wieder her\u00fcber,", "tokens": ["Doch", "kom\u00b7men", "sie", "wie\u00b7der", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das ist einem jeden bekannt.", "tokens": ["Das", "ist", "ei\u00b7nem", "je\u00b7den", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PIAT", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und kommen sie wieder zu uns zur\u00fcck,", "tokens": ["Und", "kom\u00b7men", "sie", "wie\u00b7der", "zu", "uns", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Baur geht ihnen entgegen;", "tokens": ["Der", "Baur", "geht", "ih\u00b7nen", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie bringen ihm vielmahl den Segen,", "tokens": ["Sie", "brin\u00b7gen", "ihm", "viel\u00b7mahl", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measur