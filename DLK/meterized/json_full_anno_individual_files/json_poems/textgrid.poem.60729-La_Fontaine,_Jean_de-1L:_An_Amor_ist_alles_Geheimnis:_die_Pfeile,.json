{"textgrid.poem.60729": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: An Amor ist alles Geheimnis: die Pfeile,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An Amor ist alles Geheimnis: die Pfeile,", "tokens": ["An", "A\u00b7mor", "ist", "al\u00b7les", "Ge\u00b7heim\u00b7nis", ":", "die", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIAT", "NN", "$.", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der K\u00f6cher, die Fackel, die Jugendlichkeit.", "tokens": ["Der", "K\u00f6\u00b7cher", ",", "die", "Fa\u00b7ckel", ",", "die", "Ju\u00b7gend\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wer wollte ersch\u00f6pfen in Tageseile", "tokens": ["Wer", "woll\u00b7te", "er\u00b7sch\u00f6p\u00b7fen", "in", "Ta\u00b7ge\u00b7sei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was alles Gott Amor geh\u00f6rt und geweiht!", "tokens": ["Was", "al\u00b7les", "Gott", "A\u00b7mor", "ge\u00b7h\u00f6rt", "und", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "NE", "VVFIN", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ihn v\u00f6llig erkl\u00e4ren, ich k\u00f6nnt es mit nichten;", "tokens": ["Ihn", "v\u00f6l\u00b7lig", "er\u00b7kl\u00e4\u00b7ren", ",", "ich", "k\u00f6nnt", "es", "mit", "nich\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "PPER", "VVFIN", "PPER", "APPR", "PIS", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mein Ziel ist geringer: will eins nur berichten,", "tokens": ["Mein", "Ziel", "ist", "ge\u00b7rin\u00b7ger", ":", "will", "eins", "nur", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wie hier dieser Blinde aus G\u00f6tterchor", "tokens": ["Wie", "hier", "die\u00b7ser", "Blin\u00b7de", "aus", "G\u00f6t\u00b7ter\u00b7chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PDAT", "NN", "APPR", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Das kostbare Licht seiner Augen verlor,", "tokens": ["Das", "kost\u00b7ba\u00b7re", "Licht", "sei\u00b7ner", "Au\u00b7gen", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und wie der Verlust eine Folge gebar \u2013", "tokens": ["Und", "wie", "der", "Ver\u00b7lust", "ei\u00b7ne", "Fol\u00b7ge", "ge\u00b7bar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Die schlie\u00dflich zu Amors Bestem war.", "tokens": ["Die", "schlie\u00df\u00b7lich", "zu", "A\u00b7mors", "Bes\u00b7tem", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "NN", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "(ein Liebender richte \u2013 ich sage nichts.)", "tokens": ["(", "ein", "Lie\u00b7ben\u00b7der", "rich\u00b7te", "\u2013", "ich", "sa\u00b7ge", "nichts", ".", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "PIS", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Als Amor noch nicht seines Augenlichts", "tokens": ["Als", "A\u00b7mor", "noch", "nicht", "sei\u00b7nes", "Au\u00b7gen\u00b7lichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beraubt war, spielte er gern und oft", "tokens": ["Be\u00b7raubt", "war", ",", "spiel\u00b7te", "er", "gern", "und", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit Torheit, die gleichfalls ein rechtes Kind.", "tokens": ["Mit", "Tor\u00b7heit", ",", "die", "gleich\u00b7falls", "ein", "rech\u00b7tes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und wie so spielende Kinder sind,", "tokens": ["Und", "wie", "so", "spie\u00b7len\u00b7de", "Kin\u00b7der", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Geschah es einmal unverhofft,", "tokens": ["Ge\u00b7schah", "es", "ein\u00b7mal", "un\u00b7ver\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df beide heftig stritten.", "tokens": ["Da\u00df", "bei\u00b7de", "hef\u00b7tig", "strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zum Rate der G\u00f6tter will Amor geschwind,", "tokens": ["Zum", "Ra\u00b7te", "der", "G\u00f6t\u00b7ter", "will", "A\u00b7mor", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gerechtes Urteil erbitten.", "tokens": ["Ge\u00b7rech\u00b7tes", "Ur\u00b7teil", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Doch Torheit schnellere Rache sinnt.", "tokens": ["Doch", "Tor\u00b7heit", "schnel\u00b7le\u00b7re", "Ra\u00b7che", "sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Mit zornig kr\u00e4ftigem Schlage", "tokens": ["Mit", "zor\u00b7nig", "kr\u00e4f\u00b7ti\u00b7gem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Entgilt sie dem andern die Plage \u2013", "tokens": ["Ent\u00b7gilt", "sie", "dem", "an\u00b7dern", "die", "Pla\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Und Amor verlor das Augenlicht.", "tokens": ["Und", "A\u00b7mor", "ver\u00b7lor", "das", "Au\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Frau Venus, die Mutter, f\u00fchrt Klage,", "tokens": ["Frau", "Ve\u00b7nus", ",", "die", "Mut\u00b7ter", ",", "f\u00fchrt", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Sie weint und jammert und ruhet nicht", "tokens": ["Sie", "weint", "und", "jam\u00b7mert", "und", "ru\u00b7het", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und fordert ein g\u00f6ttliches Strafgericht.", "tokens": ["Und", "for\u00b7dert", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Straf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Ein Weib und Mutter \u2013 ihr k\u00f6nnt euch denken,", "tokens": ["Ein", "Weib", "und", "Mut\u00b7ter", "\u2013", "ihr", "k\u00f6nnt", "euch", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Da\u00df ihr Geschrei recht l\u00e4stig war;", "tokens": ["Da\u00df", "ihr", "Ge\u00b7schrei", "recht", "l\u00e4s\u00b7tig", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Es st\u00f6rte die ganze himmlische Schar", "tokens": ["Es", "st\u00f6r\u00b7te", "die", "gan\u00b7ze", "himm\u00b7li\u00b7sche", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und mu\u00dfte besonders Jupiter kr\u00e4nken.", "tokens": ["Und", "mu\u00df\u00b7te", "be\u00b7son\u00b7ders", "Ju\u00b7pi\u00b7ter", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Wie stellte Venus das Schreckliche dar!", "tokens": ["Wie", "stell\u00b7te", "Ve\u00b7nus", "das", "Schreck\u00b7li\u00b7che", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Ihr Sohn k\u00f6nne nur am Stock noch gehn,", "tokens": ["Ihr", "Sohn", "k\u00f6n\u00b7ne", "nur", "am", "Stock", "noch", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Als blinder J\u00e4ger kein Ziel mehr sehn,", "tokens": ["Als", "blin\u00b7der", "J\u00e4\u00b7ger", "kein", "Ziel", "mehr", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und Nemesis m\u00fcsse, das sei klar,", "tokens": ["Und", "Ne\u00b7me\u00b7sis", "m\u00fcs\u00b7se", ",", "das", "sei", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Der T\u00e4terin S\u00fchne auferlegen,", "tokens": ["Der", "T\u00e4\u00b7te\u00b7rin", "S\u00fch\u00b7ne", "auf\u00b7er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Und keine Strafe und keine Pein", "tokens": ["Und", "kei\u00b7ne", "Stra\u00b7fe", "und", "kei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "K\u00f6nne zu schwer und schmerzlich sein!", "tokens": ["K\u00f6n\u00b7ne", "zu", "schwer", "und", "schmerz\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKA", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Nach vielem Pr\u00fcfen und Erw\u00e4gen,", "tokens": ["Nach", "vie\u00b7lem", "Pr\u00fc\u00b7fen", "und", "Er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Besorgt ums Wohl der Menschen alle \u2013", "tokens": ["Be\u00b7sorgt", "ums", "Wohl", "der", "Men\u00b7schen", "al\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PIAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und Amors auch in diesem Falle \u2013", "tokens": ["Und", "A\u00b7mors", "auch", "in", "die\u00b7sem", "Fal\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Beschlossen die Richter: Die Torheit n\u00fctze", "tokens": ["Be\u00b7schlos\u00b7sen", "die", "Rich\u00b7ter", ":", "Die", "Tor\u00b7heit", "n\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Zur S\u00fchne dem Amor als Stab und St\u00fctze,", "tokens": ["Zur", "S\u00fch\u00b7ne", "dem", "A\u00b7mor", "als", "Stab", "und", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Sie habe ihm immer zur Seite zu schreiten", "tokens": ["Sie", "ha\u00b7be", "ihm", "im\u00b7mer", "zur", "Sei\u00b7te", "zu", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.33": {"text": "Und jeden seiner Schritte zu leiten.", "tokens": ["Und", "je\u00b7den", "sei\u00b7ner", "Schrit\u00b7te", "zu", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "An Amor ist alles Geheimnis: die Pfeile,", "tokens": ["An", "A\u00b7mor", "ist", "al\u00b7les", "Ge\u00b7heim\u00b7nis", ":", "die", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIAT", "NN", "$.", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der K\u00f6cher, die Fackel, die Jugendlichkeit.", "tokens": ["Der", "K\u00f6\u00b7cher", ",", "die", "Fa\u00b7ckel", ",", "die", "Ju\u00b7gend\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wer wollte ersch\u00f6pfen in Tageseile", "tokens": ["Wer", "woll\u00b7te", "er\u00b7sch\u00f6p\u00b7fen", "in", "Ta\u00b7ge\u00b7sei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was alles Gott Amor geh\u00f6rt und geweiht!", "tokens": ["Was", "al\u00b7les", "Gott", "A\u00b7mor", "ge\u00b7h\u00f6rt", "und", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "NN", "NE", "VVFIN", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ihn v\u00f6llig erkl\u00e4ren, ich k\u00f6nnt es mit nichten;", "tokens": ["Ihn", "v\u00f6l\u00b7lig", "er\u00b7kl\u00e4\u00b7ren", ",", "ich", "k\u00f6nnt", "es", "mit", "nich\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "PPER", "VVFIN", "PPER", "APPR", "PIS", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mein Ziel ist geringer: will eins nur berichten,", "tokens": ["M