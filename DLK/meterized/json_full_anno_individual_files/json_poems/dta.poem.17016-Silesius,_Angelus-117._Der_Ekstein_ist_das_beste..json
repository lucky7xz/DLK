{"dta.poem.17016": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "117.  Der Ekstein ist das beste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Den Goldstein suchet man/ und l\u00e4st den Ekkestein/", "tokens": ["Den", "Gold\u00b7stein", "su\u00b7chet", "man", "/", "und", "l\u00e4st", "den", "Ek\u00b7kes\u00b7tein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch den man ewig reich/ gesund/ und klug kan seyn!", "tokens": ["Durch", "den", "man", "e\u00b7wig", "reich", "/", "ge\u00b7sund", "/", "und", "klug", "kan", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADJD", "ADJD", "$(", "ADJD", "$(", "KON", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}