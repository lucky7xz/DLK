{"dta.poem.11397": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien, wie er ohne sie leben k\u00f6nne?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist, o Sylvia! mein leben,", "tokens": ["Du", "bist", ",", "o", "Syl\u00b7via", "!", "mein", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NE", "$.", "PPOSAT", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und gleichwohl leb ich ohne dich;", "tokens": ["Und", "gleich\u00b7wohl", "leb", "ich", "oh\u00b7ne", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll ich nun dir, mein ander ich!", "tokens": ["Soll", "ich", "nun", "dir", ",", "mein", "an\u00b7der", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "$,", "PPOSAT", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00dfwegen red und antwort geben?", "tokens": ["De\u00df\u00b7we\u00b7gen", "red", "und", "ant\u00b7wort", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wi\u00df\u2019, es mu\u00df dein contrefait", "tokens": ["So", "wi\u00df'", ",", "es", "mu\u00df", "dein", "con\u00b7tre\u00b7fait"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meinem hertzen dich vertreten:", "tokens": ["In", "mei\u00b7nem", "hert\u00b7zen", "dich", "ver\u00b7tre\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn Seladon ist auch erfreut,", "tokens": ["Denn", "Se\u00b7la\u00b7don", "ist", "auch", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur deinen schatten anzubeten.", "tokens": ["Nur", "dei\u00b7nen", "schat\u00b7ten", "an\u00b7zu\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "L\u00e4st nun der holden liebe gunst", "tokens": ["L\u00e4st", "nun", "der", "hol\u00b7den", "lie\u00b7be", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ihres zarten pinsels kunst", "tokens": ["Und", "ih\u00b7res", "zar\u00b7ten", "pin\u00b7sels", "kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In mir dein bildni\u00df nicht verderben;", "tokens": ["In", "mir", "dein", "bild\u00b7ni\u00df", "nicht", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "O Sylvia! wie kan ich sterben?", "tokens": ["O", "Syl\u00b7via", "!", "wie", "kan", "ich", "ster\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}