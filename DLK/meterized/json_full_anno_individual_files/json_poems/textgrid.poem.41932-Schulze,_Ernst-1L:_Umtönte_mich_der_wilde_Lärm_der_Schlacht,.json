{"textgrid.poem.41932": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Umt\u00f6nte mich der wilde L\u00e4rm der Schlacht,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umt\u00f6nte mich der wilde L\u00e4rm der Schlacht,", "tokens": ["Um\u00b7t\u00f6n\u00b7te", "mich", "der", "wil\u00b7de", "L\u00e4rm", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann sah ich dich an meiner Seite stehen;", "tokens": ["Dann", "sah", "ich", "dich", "an", "mei\u00b7ner", "Sei\u00b7te", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast mit mir auf wald'gen Bergesh\u00f6hen,", "tokens": ["Du", "hast", "mit", "mir", "auf", "wald'\u00b7gen", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Feinde nah, bei n\u00e4cht'ger Gluth gewacht.", "tokens": ["Dem", "Fein\u00b7de", "nah", ",", "bei", "n\u00e4cht'\u00b7ger", "Gluth", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und lockte mich des Lenzes bl\u00fch'nde Pracht,", "tokens": ["Und", "lock\u00b7te", "mich", "des", "Len\u00b7zes", "bl\u00fch'n\u00b7de", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der reife Herbst, durch Berg und Thal zu gehen,", "tokens": ["Der", "rei\u00b7fe", "Herbst", ",", "durch", "Berg", "und", "Thal", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets hab' ich nur dein holdes Bild gesehen,", "tokens": ["Stets", "hab'", "ich", "nur", "dein", "hol\u00b7des", "Bild", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein hab' ich stets in Freud' und Leid gedacht.", "tokens": ["Dein", "hab'", "ich", "stets", "in", "Freud'", "und", "Leid", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So nimm auch jetzt, was aus dem bunten Leben", "tokens": ["So", "nimm", "auch", "jetzt", ",", "was", "aus", "dem", "bun\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf irrer Fahrt die Muse mir gegeben,", "tokens": ["Auf", "ir\u00b7rer", "Fahrt", "die", "Mu\u00b7se", 