{"textgrid.poem.41412": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wein und Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, Liebe, nein! dir gilt nicht dieses Lied;", "tokens": ["Nein", ",", "Lie\u00b7be", ",", "nein", "!", "dir", "gilt", "nicht", "die\u00b7ses", "Lied", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es soll mit Bacchus Ruhme prangen,", "tokens": ["Es", "soll", "mit", "Bac\u00b7chus", "Ruh\u00b7me", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was mich erweckt, und was man hier ersieht,", "tokens": ["Was", "mich", "er\u00b7weckt", ",", "und", "was", "man", "hier", "er\u00b7sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "KON", "PWS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist wichtiger als wei\u00df' und rothe Wangen.", "tokens": ["Ist", "wich\u00b7ti\u00b7ger", "als", "wei\u00df'", "und", "ro\u00b7the", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein jedes Glas, das diese Tafel ziert,", "tokens": ["Ein", "je\u00b7des", "Glas", ",", "das", "die\u00b7se", "Ta\u00b7fel", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verbannt das blinde Kind, und macht aus Freunden Br\u00fcder,", "tokens": ["Ver\u00b7bannt", "das", "blin\u00b7de", "Kind", ",", "und", "macht", "aus", "Freun\u00b7den", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wer bei dir oft Herz und Witz verliert,", "tokens": ["Und", "wer", "bei", "dir", "oft", "Herz", "und", "Witz", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem gibt der Wein Verstand und Freiheit wieder.", "tokens": ["Dem", "gibt", "der", "Wein", "Ver\u00b7stand", "und", "Frei\u00b7heit", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was hat vordem die Deutschen gro\u00df gemacht,", "tokens": ["Was", "hat", "vor\u00b7dem", "die", "Deut\u00b7schen", "gro\u00df", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von deren Muth auch Feinde melden?", "tokens": ["Von", "de\u00b7ren", "Muth", "auch", "Fein\u00b7de", "mel\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie flohen dich, und zechten vor der Schlacht:", "tokens": ["Sie", "flo\u00b7hen", "dich", ",", "und", "zech\u00b7ten", "vor", "der", "Schlacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und die\u00df allein, die\u00df machte sie zu Helden.", "tokens": ["Und", "die\u00df", "al\u00b7lein", ",", "die\u00df", "mach\u00b7te", "sie", "zu", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "$,", "PDS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Alter selbst verj\u00fcnget sich durch Wein,", "tokens": ["Das", "Al\u00b7ter", "selbst", "ver\u00b7j\u00fcn\u00b7get", "sich", "durch", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wann Eintracht, Lust und Durst mit vollen Stutzern winken;", "tokens": ["Wann", "Ein\u00b7tracht", ",", "Lust", "und", "Durst", "mit", "vol\u00b7len", "Stut\u00b7zern", "win\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und w\u00fcrden nicht auch G\u00f6tter sterblich sein,", "tokens": ["Und", "w\u00fcr\u00b7den", "nicht", "auch", "G\u00f6t\u00b7ter", "sterb\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn G\u00f6tter nicht stets ihren Nectar tr\u00fcnken?", "tokens": ["Wenn", "G\u00f6t\u00b7ter", "nicht", "stets", "ih\u00b7ren", "Nec\u00b7tar", "tr\u00fcn\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was macht gelehrt? Was nutzet einem Staat?", "tokens": ["Was", "macht", "ge\u00b7lehrt", "?", "Was", "nut\u00b7zet", "ei\u00b7nem", "Staat", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVPP", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was suchen alt' und neue Weisen?", "tokens": ["Was", "su\u00b7chen", "alt'", "und", "neu\u00b7e", "Wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was fehlt dem Hof, der so viel Edles hat?", "tokens": ["Was", "fehlt", "dem", "Hof", ",", "der", "so", "viel", "Ed\u00b7les", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was m\u00fc\u00dften auch die gr\u00f6\u00dften Dichter preisen?", "tokens": ["Was", "m\u00fc\u00df\u00b7ten", "auch", "die", "gr\u00f6\u00df\u00b7ten", "Dich\u00b7ter", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Wahrheit ist's. Man trifft sie selten an;", "tokens": ["Die", "Wahr\u00b7heit", "ist'", "s.", "Man", "trifft", "sie", "sel\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch wird sie dir gewi\u00df ein \u00e4chter S\u00e4ufer sagen:", "tokens": ["Doch", "wird", "sie", "dir", "ge\u00b7wi\u00df", "ein", "\u00e4ch\u00b7ter", "S\u00e4u\u00b7fer", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wer sie nicht beim Trunk entdecken kann,", "tokens": ["Und", "wer", "sie", "nicht", "beim", "Trunk", "ent\u00b7de\u00b7cken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sucht sie umsonst den Sch\u00f6nen abzufragen.", "tokens": ["Sucht", "sie", "um\u00b7sonst", "den", "Sch\u00f6\u00b7nen", "ab\u00b7zu\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sch\u00f6nheit ist der Falschheit stolzer Sitz,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "der", "Falschheit", "stol\u00b7zer", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und jedes Jahr schw\u00e4cht ihre St\u00e4rke.", "tokens": ["Und", "je\u00b7des", "Jahr", "schw\u00e4cht", "ih\u00b7re", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch thut der Wein durch eingefl\u00f6\u00dften Witz,", "tokens": ["Doch", "thut", "der", "Wein", "durch", "ein\u00b7ge\u00b7fl\u00f6\u00df\u00b7ten", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Alter erst die gr\u00f6\u00dften Wunderwerke.", "tokens": ["Im", "Al\u00b7ter", "erst", "die", "gr\u00f6\u00df\u00b7ten", "Wun\u00b7der\u00b7wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie oftmals t\u00e4uscht das Schmeicheln die Vernunft?", "tokens": ["Wie", "oft\u00b7mals", "t\u00e4uscht", "das", "Schmei\u00b7cheln", "die", "Ver\u00b7nunft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie sklavisch wird ein Mund, der l\u00e4chelnd tr\u00fcgt, verehret?", "tokens": ["Wie", "skla\u00b7visch", "wird", "ein", "Mund", ",", "der", "l\u00e4\u00b7chelnd", "tr\u00fcgt", ",", "ver\u00b7eh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch dieser Wahn verschont die freie Zunft,", "tokens": ["Doch", "die\u00b7ser", "Wahn", "ver\u00b7schont", "die", "frei\u00b7e", "Zunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die stets ihr Glas in Einem Zuge leeret.", "tokens": ["Die", "stets", "ihr", "Glas", "in", "Ei\u00b7nem", "Zu\u00b7ge", "lee\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So wollt' ich einst, bei jubelvoller Lust,", "tokens": ["So", "wollt'", "ich", "einst", ",", "bei", "ju\u00b7bel\u00b7vol\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Weines Lob der Welt erz\u00e4hlen;", "tokens": ["Des", "Wei\u00b7nes", "Lob", "der", "Welt", "er\u00b7z\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch r\u00fchrte bald ein andrer Trieb die Brust,", "tokens": ["Doch", "r\u00fchr\u00b7te", "bald", "ein", "an\u00b7drer", "Trieb", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch mu\u00dften bald die besten Worte fehlen.", "tokens": ["Doch", "mu\u00df\u00b7ten", "bald", "die", "bes\u00b7ten", "Wor\u00b7te", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nein, Bacchus, nein! dir galt nicht mehr mein Lied;", "tokens": ["Nein", ",", "Bac\u00b7chus", ",", "nein", "!", "dir", "galt", "nicht", "mehr", "mein", "Lied", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die junge Phyllis kam gegangen;", "tokens": ["Die", "jun\u00b7ge", "Phyl\u00b7lis", "kam", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und man erblickt, wo so viel Liebreiz bl\u00fcht,", "tokens": ["Und", "man", "er\u00b7blickt", ",", "wo", "so", "viel", "Lieb\u00b7reiz", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$,", "PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nichts Wichtigers, als ihre sch\u00f6ne Wangen.", "tokens": ["Nichts", "Wich\u00b7ti\u00b7gers", ",", "als", "ih\u00b7re", "sch\u00f6\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nein, Liebe, nein! dir gilt nicht dieses Lied;", "tokens": ["Nein", ",", "Lie\u00b7be", ",", "nein", "!", "dir", "gilt", "nicht", "die\u00b7ses", "Lied", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es soll mit Bacchus Ruhme prangen,", "tokens": ["Es", "soll", "mit", "Bac\u00b7chus", "Ruh\u00b7me", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was mich erweckt, und was man hier ersieht,", "tokens": ["Was", "mich", "er\u00b7weckt", ",", "und", "was", "man", "hier", "er\u00b7sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "KON", "PWS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist wichtiger als wei\u00df' und rothe Wangen.", "tokens": ["Ist", "wich\u00b7ti\u00b7ger", "als", "wei\u00df'", "und", "ro\u00b7the", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein jedes Glas, das diese Tafel ziert,", "tokens": ["Ein", "je\u00b7des", "Glas", ",", "das", "die\u00b7se", "Ta\u00b7fel", "ziert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verbannt das blinde Kind, und macht