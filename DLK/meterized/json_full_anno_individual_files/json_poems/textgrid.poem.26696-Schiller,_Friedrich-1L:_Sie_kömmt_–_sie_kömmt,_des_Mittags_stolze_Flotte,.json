{"textgrid.poem.26696": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie k\u00f6mmt \u2013 sie k\u00f6mmt, des Mittags stolze Flotte,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie k\u00f6mmt \u2013 sie k\u00f6mmt, des Mittags stolze Flotte,", "tokens": ["Sie", "k\u00f6mmt", "\u2013", "sie", "k\u00f6mmt", ",", "des", "Mit\u00b7tags", "stol\u00b7ze", "Flot\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Weltmeer wimmert unter ihr,", "tokens": ["Das", "Welt\u00b7meer", "wim\u00b7mert", "un\u00b7ter", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Kettenklang und einem neuen Gotte", "tokens": ["Mit", "Ket\u00b7ten\u00b7klang", "und", "ei\u00b7nem", "neu\u00b7en", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tausend Donnern naht sie dir \u2013", "tokens": ["Und", "tau\u00b7send", "Don\u00b7nern", "naht", "sie", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein schwimmend Heer furchtbarer Zitadellen,", "tokens": ["Ein", "schwim\u00b7mend", "Heer", "furcht\u00b7ba\u00b7rer", "Zi\u00b7ta\u00b7del\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.6": {"text": "(der Ozean sah ihresgleichen nie)", "tokens": ["(", "der", "O\u00b7ze\u00b7an", "sah", "ih\u00b7res\u00b7glei\u00b7chen", "nie", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Un\u00fcberwindlich nennt man sie,", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "nennt", "man", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zieht sie einher auf den erschrocknen Wellen;", "tokens": ["Zieht", "sie", "ein\u00b7her", "auf", "den", "er\u00b7schrock\u00b7nen", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.9": {"text": "Den stolzen Namen weiht", "tokens": ["Den", "stol\u00b7zen", "Na\u00b7men", "weiht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der Schrecken, den sie um sich speit.", "tokens": ["Der", "Schre\u00b7cken", ",", "den", "sie", "um", "sich", "speit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit majest\u00e4tisch-stillem Schritte", "tokens": ["Mit", "ma\u00b7je\u00b7st\u00e4\u00b7tischstil\u00b7lem", "Schrit\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt seine Last der zitternde Neptun,", "tokens": ["Tr\u00e4gt", "sei\u00b7ne", "Last", "der", "zit\u00b7tern\u00b7de", "Nep\u00b7tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weltuntergang in ihrer Mitte,", "tokens": ["Welt\u00b7un\u00b7ter\u00b7gang", "in", "ih\u00b7rer", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Naht sie heran, und alle St\u00fcrme ruhn.", "tokens": ["Naht", "sie", "he\u00b7ran", ",", "und", "al\u00b7le", "St\u00fcr\u00b7me", "ruhn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir gegen\u00fcber steht sie da,", "tokens": ["Dir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "steht", "sie", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00fcckselge Insel \u2013 Herrscherin der Meere,", "tokens": ["Gl\u00fcck\u00b7sel\u00b7ge", "In\u00b7sel", "\u2013", "Herr\u00b7sche\u00b7rin", "der", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Dir drohen diese Gallionenheere,", "tokens": ["Dir", "dro\u00b7hen", "die\u00b7se", "Gal\u00b7li\u00b7o\u00b7nen\u00b7hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gro\u00dfherzige Britannia.", "tokens": ["Gro\u00df\u00b7her\u00b7zi\u00b7ge", "Bri\u00b7tan\u00b7nia", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Weh deinem freigebornen Volke!", "tokens": ["Weh", "dei\u00b7nem", "frei\u00b7ge\u00b7bor\u00b7nen", "Vol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da steht sie, eine wetterschwangre Wolke.", "tokens": ["Da", "steht", "sie", ",", "ei\u00b7ne", "wet\u00b7ter\u00b7schwang\u00b7re", "Wol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer hat das hohe Kleinod dir errungen,", "tokens": ["Wer", "hat", "das", "ho\u00b7he", "Klei\u00b7nod", "dir", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das zu der L\u00e4nder F\u00fcrstin dich gemacht?", "tokens": ["Das", "zu", "der", "L\u00e4n\u00b7der", "F\u00fcrs\u00b7tin", "dich", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du nicht selbst, von stolzen K\u00f6nigen gezwungen,", "tokens": ["Hast", "du", "nicht", "selbst", ",", "von", "stol\u00b7zen", "K\u00f6\u00b7ni\u00b7gen", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Reichsgesetze weisestes erdacht,", "tokens": ["Der", "Reichs\u00b7ge\u00b7set\u00b7ze", "wei\u00b7ses\u00b7tes", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Zu F\u00fcrsten deine B\u00fcrger macht?", "tokens": ["Zu", "F\u00fcrs\u00b7ten", "dei\u00b7ne", "B\u00fcr\u00b7ger", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Segel stolze Obermacht,", "tokens": ["Der", "Se\u00b7gel", "stol\u00b7ze", "O\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hast du sie nicht von Millionen W\u00fcrgern", "tokens": ["Hast", "du", "sie", "nicht", "von", "Mil\u00b7lion\u00b7en", "W\u00fcr\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "APPR", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Erstritten in der Wasserschlacht?", "tokens": ["Er\u00b7strit\u00b7ten", "in", "der", "Was\u00b7ser\u00b7schlacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wem dankst du sie \u2013 err\u00f6tet, V\u00f6lker dieser Erde \u2013", "tokens": ["Wem", "dankst", "du", "sie", "\u2013", "er\u00b7r\u00f6\u00b7tet", ",", "V\u00f6l\u00b7ker", "die\u00b7ser", "Er\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$(", "VVPP", "$,", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wem sonst als deinem Geist und deinem Schwerte?", "tokens": ["Wem", "sonst", "als", "dei\u00b7nem", "Geist", "und", "dei\u00b7nem", "Schwer\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ungl\u00fcckliche \u2013 blick hin auf diese feuerwerfenden Kolossen,", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7che", "\u2013", "blick", "hin", "auf", "die\u00b7se", "feu\u00b7er\u00b7wer\u00b7fen\u00b7den", "Ko\u00b7los\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Blick hin und ahnde deines Ruhmes Fall!", "tokens": ["Blick", "hin", "und", "ahn\u00b7de", "dei\u00b7nes", "Ruh\u00b7mes", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bang schaut auf dich der Erdenball,", "tokens": ["Bang", "schaut", "auf", "dich", "der", "Er\u00b7den\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und aller freien M\u00e4nner Herzen schlagen,", "tokens": ["Und", "al\u00b7ler", "frei\u00b7en", "M\u00e4n\u00b7ner", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und alle gute sch\u00f6ne Seelen klagen", "tokens": ["Und", "al\u00b7le", "gu\u00b7te", "sch\u00f6\u00b7ne", "See\u00b7len", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Teilnehmend deines Ruhmes Fall.", "tokens": ["Teil\u00b7neh\u00b7mend", "dei\u00b7nes", "Ruh\u00b7mes", "Fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott der Allm\u00e4chtge sah herab,", "tokens": ["Gott", "der", "All\u00b7m\u00e4cht\u00b7ge", "sah", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sah deines Feindes stolze L\u00f6wenflaggen wehen,", "tokens": ["Sah", "dei\u00b7nes", "Fein\u00b7des", "stol\u00b7ze", "L\u00f6\u00b7wen\u00b7flag\u00b7gen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sah drohend offen dein gewisses Grab \u2013", "tokens": ["Sah", "dro\u00b7hend", "of\u00b7fen", "dein", "ge\u00b7wis\u00b7ses", "Grab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll, sprach er, soll mein Albion vergehen,", "tokens": ["Soll", ",", "sprach", "er", ",", "soll", "mein", "Al\u00b7bi\u00b7on", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erl\u00f6schen meiner Helden Stamm,", "tokens": ["Er\u00b7l\u00f6\u00b7schen", "mei\u00b7ner", "Hel\u00b7den", "Stamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Unterdr\u00fcckung letzter Felsendamm", "tokens": ["Der", "Un\u00b7ter\u00b7dr\u00fc\u00b7ckung", "letz\u00b7ter", "Fel\u00b7sen\u00b7damm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zusammenst\u00fcrzen, die ", "tokens": ["Zu\u00b7sam\u00b7men\u00b7st\u00fcr\u00b7zen", ",", "die"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vernichtet sein von dieser Hemisph\u00e4re?", "tokens": ["Ver\u00b7nich\u00b7tet", "sein", "von", "die\u00b7ser", "He\u00b7misp\u00b7h\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nie, rief er, soll der Freiheit Paradies,", "tokens": ["Nie", ",", "rief", "er", ",", "soll", "der", "Frei\u00b7heit", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Menschenw\u00fcrde starker Schirm verschwinden!", "tokens": ["Der", "Men\u00b7schen\u00b7w\u00fcr\u00b7de", "star\u00b7ker", "Schirm", "ver\u00b7schwin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gott der Allm\u00e4chtge blies,", "tokens": ["Gott", "der", "All\u00b7m\u00e4cht\u00b7ge", "blies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Und die Armada flog nach allen Winden.", "tokens": ["Und", "die", "Ar\u00b7ma\u00b7da", "flog", "nach", "al\u00b7len", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie k\u00f6mmt \u2013 sie k\u00f6mmt, des Mittags stolze Flotte,", "tokens": ["Sie", "k\u00f6mmt", "\u2013", "sie", "k\u00f6mmt", ",", "des", "Mit\u00b7tags", "stol\u00b7ze", "Flot\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Weltmeer wimmert unter ihr,", "tokens": ["Das", "Welt\u00b7meer", "wim\u00b7mert", "un\u00b7ter", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Kettenklang und einem neuen Gotte", "tokens": ["Mit", "Ket\u00b7ten\u00b7klang", "und", "ei\u00b7nem", "neu\u00b7en", "Got\u00b7te"], "token_info": ["word", "word", "word