{"dta.poem.17481": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "102.  Warumb niemand vonn Engeln  \n besessen wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie da\u00df keln heilges Hertz vonn Engeln wird besessen?", "tokens": ["Wie", "da\u00df", "keln", "heil\u00b7ges", "Hertz", "vonn", "En\u00b7geln", "wird", "be\u00b7ses\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "NN", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie thuns nicht weil es GOtt f\u00fcr sich hat abgemessen.", "tokens": ["Sie", "thuns", "nicht", "weil", "es", "Gott", "f\u00fcr", "sich", "hat", "ab\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "NN", "APPR", "PRF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}