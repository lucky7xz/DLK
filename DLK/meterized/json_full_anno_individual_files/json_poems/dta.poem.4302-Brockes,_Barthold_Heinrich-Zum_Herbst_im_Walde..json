{"dta.poem.4302": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Herbst im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzt sieht man in den tiefen G\u00e4ngen", "tokens": ["Jtzt", "sieht", "man", "in", "den", "tie\u00b7fen", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich viele Farben lieblich mengen,", "tokens": ["Sich", "vie\u00b7le", "Far\u00b7ben", "lieb\u00b7lich", "men\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verschiedne Stellen sch\u00f6n sich r\u00f6hten,", "tokens": ["Ver\u00b7schied\u00b7ne", "Stel\u00b7len", "sch\u00f6n", "sich", "r\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verschiedne gleichsam sich verg\u00fclden,", "tokens": ["Ver\u00b7schied\u00b7ne", "gleich\u00b7sam", "sich", "ver\u00b7g\u00fcl\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch sie Persische Tapeten,", "tokens": ["Wo\u00b7durch", "sie", "Per\u00b7si\u00b7sche", "Ta\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Golde reich durchwirket, bilden.", "tokens": ["Mit", "Gol\u00b7de", "reich", "durch\u00b7wir\u00b7ket", ",", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel\u2019 gelb gewordne Zweige schienen,", "tokens": ["Viel'", "gelb", "ge\u00b7word\u00b7ne", "Zwei\u00b7ge", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dem noch unverwelkten Gr\u00fcnen,", "tokens": ["In", "dem", "noch", "un\u00b7ver\u00b7welk\u00b7ten", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jm Sonnen-Licht, auf manchen Stellen,", "tokens": ["Jm", "Son\u00b7nen\u00b7Licht", ",", "auf", "man\u00b7chen", "Stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den Zweig Aeneas vorzustellen,", "tokens": ["Den", "Zweig", "A\u00b7e\u00b7neas", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der ihm selbst, durch Cocytus Schwellen,", "tokens": ["Der", "ihm", "selbst", ",", "durch", "Co\u00b7cy\u00b7tus", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Ein sicheres Geleite gab.", "tokens": ["Ein", "si\u00b7che\u00b7res", "Ge\u00b7lei\u00b7te", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Die bunten Steige selber liessen,", "tokens": ["Die", "bun\u00b7ten", "Stei\u00b7ge", "sel\u00b7ber", "lies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von Bl\u00e4ttern, die von oben ab", "tokens": ["Von", "Bl\u00e4t\u00b7tern", ",", "die", "von", "o\u00b7ben", "ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gefallen, selber unsern F\u00fcssen", "tokens": ["Ge\u00b7fal\u00b7len", ",", "sel\u00b7ber", "un\u00b7sern", "F\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Gef\u00e4rbte Teppich\u2019 abzugeben.", "tokens": ["Ge\u00b7f\u00e4rb\u00b7te", "Tep\u00b7pich'", "ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Fu\u00df, um sie nicht zu verderben, scheut oftermahls sich", "tokens": ["Der", "Fu\u00df", ",", "um", "sie", "nicht", "zu", "ver\u00b7der\u00b7ben", ",", "scheut", "of\u00b7ter\u00b7mahls", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "PPER", "PTKNEG", "PTKZU", "VVFIN", "$,", "VVFIN", "ADV", "PRF"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.18": {"text": "aufzuheben.", "tokens": ["auf\u00b7zu\u00b7he\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Die Farben scheinen auf den B\u00e4umen itzt gleichsam, uns zur", "tokens": ["Die", "Far\u00b7ben", "schei\u00b7nen", "auf", "den", "B\u00e4u\u00b7men", "itzt", "gleich\u00b7sam", ",", "uns", "zur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "PPER", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Lust, zu k\u00e4mpfen,", "tokens": ["Lust", ",", "zu", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Die Gelbe sucht das nahe Gr\u00fcn, durch ihr erhabner Licht,", "tokens": ["Die", "Gel\u00b7be", "sucht", "das", "na\u00b7he", "Gr\u00fcn", ",", "durch", "ihr", "er\u00b7hab\u00b7ner", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "zu d\u00e4mpfen,", "tokens": ["zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Das Rohte d\u00e4mpft das Gelbe wieder, und bey derselben", "tokens": ["Das", "Roh\u00b7te", "d\u00e4mpft", "das", "Gel\u00b7be", "wie\u00b7der", ",", "und", "bey", "der\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "APPR", "PDAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "hellen Pracht,", "tokens": ["hel\u00b7len", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Nimmt man zugleich die Mittel-Farben, im mannigfachen", "tokens": ["Nimmt", "man", "zu\u00b7gleich", "die", "Mit\u00b7tel\u00b7Fa\u00b7rben", ",", "im", "man\u00b7nig\u00b7fa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Grad', in Acht.", "tokens": ["Grad'", ",", "in", "Acht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "CARD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Dieselben sind fast nicht zu z\u00e4hlen, und ihr gebrochenes", "tokens": ["Die\u00b7sel\u00b7ben", "sind", "fast", "nicht", "zu", "z\u00e4h\u00b7len", ",", "und", "ihr", "ge\u00b7bro\u00b7che\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "KON", "PPOSAT", "ADJA"], "meter": "-+--+--+--+-+--", "measure": "amphibrach.tetra.plus"}, "line.28": {"text": "Gemische", "tokens": ["Ge\u00b7mi\u00b7sche"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Ziert, in ged\u00e4mpfter Harmonie, itzt \u00fcberall so B\u00e4um\u2019 als", "tokens": ["Ziert", ",", "in", "ge\u00b7d\u00e4mpf\u00b7ter", "Har\u00b7mo\u00b7nie", ",", "itzt", "\u00fc\u00b7be\u00b7rall", "so", "B\u00e4um'", "als"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "NN", "KOKOM"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "B\u00fcsche,", "tokens": ["B\u00fc\u00b7sche", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Und diese zieren Luft und Erde. Es sieht ein achtsames", "tokens": ["Und", "die\u00b7se", "zie\u00b7ren", "Luft", "und", "Er\u00b7de", ".", "Es", "sieht", "ein", "acht\u00b7sa\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Gesicht,", "tokens": ["Ge\u00b7sicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "So weit es itzo sehen kann, ein buntes allgemeines Licht.", "tokens": ["So", "weit", "es", "it\u00b7zo", "se\u00b7hen", "kann", ",", "ein", "bun\u00b7tes", "all\u00b7ge\u00b7mei\u00b7nes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVINF", "VMFIN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Die mit den welken Bl\u00e4ttern gleichsam mit-abgefallne", "tokens": ["Die", "mit", "den", "wel\u00b7ken", "Bl\u00e4t\u00b7tern", "gleich\u00b7sam", "mit\u00b7ab\u00b7ge\u00b7fall\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schatten sind", "tokens": ["Schat\u00b7ten", "sind"], "token_info": ["word", "word"], "pos": ["NN", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Fast itzo nirgend mehr zu finden, im ganzen Walde sehr", "tokens": ["Fast", "it\u00b7zo", "nir\u00b7gend", "mehr", "zu", "fin\u00b7den", ",", "im", "gan\u00b7zen", "Wal\u00b7de", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "verd\u00fcnnt,", "tokens": ["ver\u00b7d\u00fcnnt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wodurch das zwar gemilderte, doch itzt fast allgemeine", "tokens": ["Wo\u00b7durch", "das", "zwar", "ge\u00b7mil\u00b7der\u00b7te", ",", "doch", "itzt", "fast", "all\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADV", "ADJA", "$,", "ADV", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Licht", "tokens": ["Licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ein fanft Gemisch von Farben zeigt, das dem betrachtenden", "tokens": ["Ein", "fanft", "Ge\u00b7misch", "von", "Far\u00b7ben", "zeigt", ",", "das", "dem", "be\u00b7trach\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Gesicht", "tokens": ["Ge\u00b7sicht"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Auf eine neue Weise schmeichelt. Man kann, auf neue", "tokens": ["Auf", "ei\u00b7ne", "neu\u00b7e", "Wei\u00b7se", "schmei\u00b7chelt", ".", "Man", "kann", ",", "auf", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "PIS", "VMFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Weise, seh'n,", "tokens": ["Wei\u00b7se", ",", "seh'n", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Wie die sich schildernde Natur, auch \u00f6fters ohne Schatten,", "tokens": ["Wie", "die", "sich", "schil\u00b7dern\u00b7de", "Na\u00b7tur", ",", "auch", "\u00f6f\u00b7ters", "oh\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRELS", "PRF", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "sch\u00f6n.", "tokens": ["sch\u00f6n", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Ja, wenn der Sonnen-Strahl von oben, durch die gef\u00e4rbte", "tokens": ["Ja", ",", "wenn", "der", "Son\u00b7nen\u00b7Strahl", "von", "o\u00b7ben", ",", "durch", "die", "ge\u00b7f\u00e4rb\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "APPR", "ADV", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Bl\u00e4tter, strahlet,", "tokens": ["Bl\u00e4t\u00b7ter", ",", "strah\u00b7let", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Scheint alles in dem ganzen Walde nicht bunt allein, und", "tokens": ["Scheint", "al\u00b7les", "in", "dem", "gan\u00b7zen", "Wal\u00b7de", "nicht", "bunt", "al\u00b7lein", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "ADV", "$,", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "sch\u00f6n gemahlet,", "tokens": ["sch\u00f6n", "ge\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Es scheint vielmehr der ganze Wald mit einem bunten Glanz", "tokens": ["Es", "scheint", "viel\u00b7mehr", "der", "gan\u00b7ze", "Wald", "mit", "ei\u00b7nem", "bun\u00b7ten", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "gezieret,", "tokens": ["ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Und alles um und \u00fcber uns recht in der That illuminiret.", "tokens": ["Und", "al\u00b7les", "um", "und", "\u00fc\u00b7ber", "uns", "recht", "in", "der", "That", "il\u00b7lu\u00b7mi\u00b7ni\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "KON", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.20": {"text": "O, welche Lust! wenn man in solcher Jllumination", "tokens": ["O", ",", "wel\u00b7che", "Lust", "!", "wenn", "man", "in", "sol\u00b7cher", "Jllu\u00b7mi\u00b7na\u00b7ti\u00b7on"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAT", "NN", "$.", "KOUS", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "spatziret,", "tokens": ["spat\u00b7zi\u00b7re