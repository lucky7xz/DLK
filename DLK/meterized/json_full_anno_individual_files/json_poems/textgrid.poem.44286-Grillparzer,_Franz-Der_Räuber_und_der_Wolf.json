{"textgrid.poem.44286": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der R\u00e4uber und der Wolf", "genre": "verse", "period": "N.A.", "pub_year": 1805, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wolf, ein grauses Scheusal der Natur,", "tokens": ["Ein", "Wolf", ",", "ein", "grau\u00b7ses", "Scheu\u00b7sal", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Schrecken aller Sch\u00e4fer auf der Flur,", "tokens": ["Das", "Schre\u00b7cken", "al\u00b7ler", "Sch\u00e4\u00b7fer", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hielt, hingestreckt auf gr\u00fcne Matte,", "tokens": ["Hielt", ",", "hin\u00b7ge\u00b7streckt", "auf", "gr\u00fc\u00b7ne", "Mat\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Lamm, das er zerissen hatte,", "tokens": ["Ein", "Lamm", ",", "das", "er", "ze\u00b7ris\u00b7sen", "hat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, unger\u00fchrt von herben Klagen", "tokens": ["Und", ",", "un\u00b7ge\u00b7r\u00fchrt", "von", "her\u00b7ben", "Kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mutter, er davongetragen,", "tokens": ["Der", "Mut\u00b7ter", ",", "er", "da\u00b7von\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In seiner Klau und fra\u00df. Ein R\u00e4uber sah", "tokens": ["In", "sei\u00b7ner", "Klau", "und", "fra\u00df", ".", "Ein", "R\u00e4u\u00b7ber", "sah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das blutge Paar. Raubgierig schrie er, ha!", "tokens": ["Das", "blut\u00b7ge", "Paar", ".", "Raub\u00b7gie\u00b7rig", "schrie", "er", ",", "ha", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PPER", "$,", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Schmeckts, guter Freund? \u2013 Mit seinem Schwerte", "tokens": ["Schmeckts", ",", "gu\u00b7ter", "Freund", "?", "\u2013", "Mit", "sei\u00b7nem", "Schwer\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bohrt er den Wolfen hin zur Erde.", "tokens": ["Bohrt", "er", "den", "Wol\u00b7fen", "hin", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da st\u00f6hnt der matt: Du bist so b\u00f6s wie ich,", "tokens": ["Da", "st\u00f6hnt", "der", "matt", ":", "Du", "bist", "so", "b\u00f6s", "wie", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und doch, du Bruderm\u00f6rder, t\u00f6test mich!", "tokens": ["Und", "doch", ",", "du", "Bru\u00b7der\u00b7m\u00f6r\u00b7der", ",", "t\u00f6\u00b7test", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der nimmt das Lamm. Mein Bruder, h\u00f6re,", "tokens": ["Der", "nimmt", "das", "Lamm", ".", "Mein", "Bru\u00b7der", ",", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Spricht er, zu sp\u00e4t nun diese Lehre.", "tokens": ["Spricht", "er", ",", "zu", "sp\u00e4t", "nun", "die\u00b7se", "Leh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PTKA", "ADJD", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Kein arger B\u00f6swicht ist des andern Freund,", "tokens": ["Kein", "ar\u00b7ger", "B\u00f6s\u00b7wicht", "ist", "des", "an\u00b7dern", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und selbst, Freund, merke dirs, sein \u00e4rgster Feind.", "tokens": ["Und", "selbst