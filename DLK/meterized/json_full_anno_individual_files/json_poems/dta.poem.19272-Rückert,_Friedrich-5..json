{"dta.poem.19272": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt hat solche Sch\u00e4tz' im Innern aufgethan,", "tokens": ["Die", "Welt", "hat", "sol\u00b7che", "Sch\u00e4tz'", "im", "In\u00b7nern", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie der Dichtkunst Form nun nicht mehr fassen kan;", "tokens": ["Da\u00df", "sie", "der", "Dicht\u00b7kunst", "Form", "nun", "nicht", "mehr", "fas\u00b7sen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sonst die Dichtkunst wol, was ist und war, umfa\u00dfte,", "tokens": ["Wie", "sonst", "die", "Dicht\u00b7kunst", "wol", ",", "was", "ist", "und", "war", ",", "um\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "$,", "PWS", "VAFIN", "KON", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als noch ihr Ma\u00df mit dem der Welt zusammen pa\u00dfte.", "tokens": ["Als", "noch", "ihr", "Ma\u00df", "mit", "dem", "der", "Welt", "zu\u00b7sam\u00b7men", "pa\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PRELS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nun begn\u00fcgt sie sich, was sie nicht auszubeuten", "tokens": ["Doch", "nun", "be\u00b7gn\u00fcgt", "sie", "sich", ",", "was", "sie", "nicht", "aus\u00b7zu\u00b7beu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "PRELS", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermag, mit fl\u00fccht'gem Schlag der Wunschruth' anzudeuten.", "tokens": ["Ver\u00b7mag", ",", "mit", "fl\u00fccht'\u00b7gem", "Schlag", "der", "Wunschruth'", "an\u00b7zu\u00b7deu\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn auch den Vollgehalt die Form nicht in sich h\u00e4lt,", "tokens": ["Wenn", "auch", "den", "Voll\u00b7ge\u00b7halt", "die", "Form", "nicht", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch im Bewu\u00dftseyn ruht die F\u00fclle dieser Welt.", "tokens": ["Doch", "im", "Be\u00b7wu\u00df\u00b7tseyn", "ruht", "die", "F\u00fcl\u00b7le", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}