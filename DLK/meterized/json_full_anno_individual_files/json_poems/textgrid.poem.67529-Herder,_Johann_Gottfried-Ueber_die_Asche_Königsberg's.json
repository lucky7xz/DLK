"], "pos": ["APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und h\u00fcpfen froh ums Grab in Strahlenr\u00fcstung;", "tokens": ["Und", "h\u00fcp\u00b7fen", "froh", "ums", "Grab", "in", "Strah\u00b7len\u00b7r\u00fcs\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So h\u00fcpft \u2013 der Seher sieht's! \u2013 so h\u00fcpfst Du Stadt", "tokens": ["So", "h\u00fcpft", "\u2013", "der", "Se\u00b7her", "sieht's", "!", "\u2013", "so", "h\u00fcpfst", "Du", "Stadt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "ART", "NN", "NE", "$.", "$(", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald, statt des Gr\u00e4uels der Verw\u00fcstung,", "tokens": ["Bald", ",", "statt", "des", "Gr\u00e4u\u00b7els", "der", "Ver\u00b7w\u00fcs\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Um eine neue Stadt!", "tokens": ["Um", "ei\u00b7ne", "neu\u00b7e", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich sah! Der Seher bebt, es anzusagen;", "tokens": ["Ich", "sah", "!", "Der", "Se\u00b7her", "bebt", ",", "es", "an\u00b7zu\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch ist sein Auge Nacht! \u2013 Ist Volk um mich,", "tokens": ["Noch", "ist", "sein", "Au\u00b7ge", "Nacht", "!", "\u2013", "Ist", "Volk", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$.", "$(", "VAFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das h\u00f6r' und heul' den Tr\u00fcmmern Klagen,", "tokens": ["Das", "h\u00f6r'", "und", "heul'", "den", "Tr\u00fcm\u00b7mern", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beasch' und b\u00fccke sich!", "tokens": ["Be\u00b7asch'", "und", "b\u00fc\u00b7cke", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Denn ein Gesicht zur Zeit der Sabbathsstille", "tokens": ["Denn", "ein", "Ge\u00b7sicht", "zur", "Zeit", "der", "Sab\u00b7baths\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich, entz\u00fcckt den Blick emporgewandt,", "tokens": ["Sah", "ich", ",", "ent\u00b7z\u00fcckt", "den", "Blick", "em\u00b7por\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sah in Abbadon's Wolkenh\u00fclle,", "tokens": ["Sah", "in", "Ab\u00b7ba\u00b7don's", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le", ","], "token_info"