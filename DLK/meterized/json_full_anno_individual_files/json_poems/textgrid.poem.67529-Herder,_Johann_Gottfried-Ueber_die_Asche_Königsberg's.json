{"textgrid.poem.67529": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ueber die Asche K\u00f6nigsberg's", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah! Der Seher bebt, es anzusagen;", "tokens": ["Ich", "sah", "!", "Der", "Se\u00b7her", "bebt", ",", "es", "an\u00b7zu\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch ist sein Auge Nacht! \u2013 Ist Volk um mich,", "tokens": ["Noch", "ist", "sein", "Au\u00b7ge", "Nacht", "!", "\u2013", "Ist", "Volk", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$.", "$(", "VAFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das h\u00f6r' und heul' den Tr\u00fcmmern Klagen,", "tokens": ["Das", "h\u00f6r'", "und", "heul'", "den", "Tr\u00fcm\u00b7mern", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beasch' und b\u00fccke sich!", "tokens": ["Be\u00b7asch'", "und", "b\u00fc\u00b7cke", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn ein Gesicht zur Zeit der Sabbathsstille", "tokens": ["Denn", "ein", "Ge\u00b7sicht", "zur", "Zeit", "der", "Sab\u00b7baths\u00b7stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich, entz\u00fcckt den Blick emporgewandt,", "tokens": ["Sah", "ich", ",", "ent\u00b7z\u00fcckt", "den", "Blick", "em\u00b7por\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sah in Abbadon's Wolkenh\u00fclle,", "tokens": ["Sah", "in", "Ab\u00b7ba\u00b7don's", "Wol\u00b7ken\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das Feu'rschwert in der Hand;", "tokens": ["Das", "Feu'r\u00b7schwert", "in", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sank auf des Sturmes Fl\u00fcgelwagen nieder", "tokens": ["Sank", "auf", "des", "Stur\u00b7mes", "Fl\u00fc\u00b7gel\u00b7wa\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Todesengel! und es schrieb sein Stab", "tokens": ["Ein", "To\u00b7de\u00b7sen\u00b7gel", "!", "und", "es", "schrieb", "sein", "Stab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(wie Belsazern, so zittern mir die Glieder!),", "tokens": ["(", "wie", "Bel\u00b7sa\u00b7zern", ",", "so", "zit\u00b7tern", "mir", "die", "Glie\u00b7der", "!", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KOKOM", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er schrieb zum Flammengrab", "tokens": ["Er", "schrieb", "zum", "Flam\u00b7men\u00b7grab"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Jehovah's Wohnungen, seid, Tempel, heut", "tokens": ["Je\u00b7ho\u00b7vah's", "Woh\u00b7nun\u00b7gen", ",", "seid", ",", "Tem\u00b7pel", ",", "heut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "$,", "NN", "$,", "ADV"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Herd! \u2013 Dein Palastgold die Beute", "tokens": ["Sein", "Herd", "!", "\u2013", "Dein", "Pa\u00b7last\u00b7gold", "die", "Beu\u00b7te"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Gluthstroms! \u2013 Asche seid!\u00ab", "tokens": ["Des", "Gluth\u00b7stroms", "!", "\u2013", "A\u00b7sche", "seid", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da flo\u00df von Cherubs Schwert ein Funke nieder", "tokens": ["Da", "flo\u00df", "von", "Che\u00b7rubs", "Schwert", "ein", "Fun\u00b7ke", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "(wie jener Stern Abbadon), und sein Fu\u00df", "tokens": ["(", "wie", "je\u00b7ner", "Stern", "Ab\u00b7ba\u00b7don", ")", ",", "und", "sein", "Fu\u00df"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "KOKOM", "PDAT", "NN", "NE", "$(", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sandt' Sturm ihm nach, schwarz am Gefieder;", "tokens": ["Sandt'", "Sturm", "ihm", "nach", ",", "schwarz", "am", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PTKVZ", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da ward der Funk' ein Flu\u00df", "tokens": ["Da", "ward", "der", "Funk'", "ein", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von Wirbeln himmelhoher Aetnaswellen", "tokens": ["Von", "Wir\u00b7beln", "him\u00b7mel\u00b7ho\u00b7her", "A\u00b7et\u00b7nas\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Bergen Rauchdampf, den der W\u00fcrger sich", "tokens": ["Und", "Ber\u00b7gen", "Rauch\u00b7dampf", ",", "den", "der", "W\u00fcr\u00b7ger", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "$,", "PRELS", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Siegskleid umwand, und die Schwellen", "tokens": ["Zum", "Siegs\u00b7kleid", "um\u00b7wand", ",", "und", "die", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Erde kr\u00fcmmten sich.", "tokens": ["Der", "Er\u00b7de", "kr\u00fcmm\u00b7ten", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da zischten nieder G\u00fcsse Funkenregen,", "tokens": ["Da", "zischten", "nie\u00b7der", "G\u00fcs\u00b7se", "Fun\u00b7ken\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis hier und dort ein Pharos wie ein Heer", "tokens": ["Bis", "hier", "und", "dort", "ein", "Pha\u00b7ros", "wie", "ein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "KON", "ADV", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Riesen flammt', und allerwegen", "tokens": ["Von", "Rie\u00b7sen", "flammt'", ",", "und", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flo\u00df siebenfarbig Meer,", "tokens": ["Flo\u00df", "sie\u00b7ben\u00b7far\u00b7big", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auf dem der Sturmwind ritt und Schrecken sauste.", "tokens": ["Auf", "dem", "der", "Sturm\u00b7wind", "ritt", "und", "Schre\u00b7cken", "saus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O weint und mischet m\u00e4cht'gen Jammerton", "tokens": ["O", "weint", "und", "mi\u00b7schet", "m\u00e4cht'\u00b7gen", "Jam\u00b7mer\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Ach, das die Verzweiflung brauste!", "tokens": ["Zum", "Ach", ",", "das", "die", "Ver\u00b7zwei\u00b7flung", "braus\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Ach, das hier ein Sohn,", "tokens": ["Zum", "Ach", ",", "das", "hier", "ein", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Greis, ein Sterbender, ein Volk von Kranken", "tokens": ["Ein", "Greis", ",", "ein", "Ster\u00b7ben\u00b7der", ",", "ein", "Volk", "von", "Kran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Flammen wimmert, wie aus Moloch's Arm!", "tokens": ["Aus", "Flam\u00b7men", "wim\u00b7mert", ",", "wie", "aus", "Mo\u00b7loch's", "Arm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWAV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbh\u00f6r's, Cherub! la\u00df Dir Thr\u00e4nen danken!\u00ab", "tokens": ["\u00bb", "h\u00f6r's", ",", "Che\u00b7rub", "!", "la\u00df", "Dir", "Thr\u00e4\u00b7nen", "dan\u00b7ken", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$.", "VVIMP", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da w\u00e4gt er \u2013 und sein Arm", "tokens": ["Da", "w\u00e4gt", "er", "\u2013", "und", "sein", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "St\u00fcrzt weiter. Es schl\u00e4gt Mitternacht und Morgen;", "tokens": ["St\u00fcrzt", "wei\u00b7ter", ".", "Es", "schl\u00e4gt", "Mit\u00b7ter\u00b7nacht", "und", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wage t\u00f6nt noch fort, die D\u00e4mmerung,", "tokens": ["Die", "Wa\u00b7ge", "t\u00f6nt", "noch", "fort", ",", "die", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die Nacht, den Mittag durch zum Morgen;", "tokens": ["Die", "Nacht", ",", "den", "Mit\u00b7tag", "durch", "zum", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun steht sie! \u2013 Endlich g'nung!", "tokens": ["Nun", "steht", "sie", "!", "\u2013", "End\u00b7lich", "g'\u00b7nung", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Kniet! \u2013 Mit der letzten Loh' fuhr er, umkreiset", "tokens": ["Kniet", "!", "\u2013", "Mit", "der", "letz\u00b7ten", "Loh'", "fuhr", "er", ",", "um\u00b7krei\u00b7set"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von drei\u00dfig M\u00e4rtrern, auf! \u2013 Gott sah, da war", "tokens": ["Von", "drei\u00b7\u00dfig", "M\u00e4r\u00b7trern", ",", "auf", "!", "\u2013", "Gott", "sah", ",", "da", "war"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "CARD", "NN", "$,", "PTKVZ", "$.", "$(", "NN", "VVFIN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Stadt ein Babel, nackt, verwaiset,", "tokens": ["Die", "Stadt", "ein", "Ba\u00b7bel", ",", "nackt", ",", "ver\u00b7wai\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein weiter Rauchaltar,", "tokens": ["Ein", "wei\u00b7ter", "Rauch\u00b7al\u00b7tar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Auf dem Noth, Hunger, Frost, Harpyenheere!", "tokens": ["Auf", "dem", "Noth", ",", "Hun\u00b7ger", ",", "Frost", ",", "Har\u00b7py\u00b7en\u00b7hee\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielklauicht ruheten; sie \u00e4chzt, sie heult!", "tokens": ["Viel\u00b7klau\u00b7icht", "ru\u00b7he\u00b7ten", ";", "sie", "\u00e4chzt", ",", "sie", "heult", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Da sprach er: \u00bbGnade! was die Schwere", "tokens": ["Da", "sprach", "er", ":", "\u00bb", "Gna\u00b7de", "!", "was", "die", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "PWS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Zorns schlug, sei geheilt!\u00ab", "tokens": ["Des", "Zorns", "schlug", ",", "sei", "ge\u00b7heilt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er sprach's! Macht ging von ihm; die Aschenst\u00e4tte", "tokens": ["Er", "sprach's", "!", "Macht", "ging", "von", "ihm", ";", "die", "A\u00b7schen\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "APPR", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewegte sich, des Schutts Gebein erhob", "tokens": ["Be\u00b7weg\u00b7te", "sich", ",", "des", "Schutts", "Ge\u00b