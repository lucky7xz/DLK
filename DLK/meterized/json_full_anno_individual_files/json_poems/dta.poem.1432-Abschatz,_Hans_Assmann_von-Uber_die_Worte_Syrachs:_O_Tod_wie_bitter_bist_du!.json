{"dta.poem.1432": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Worte Syrachs:  \n O Tod/ wie bitter bist du!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie bitter bistu herber Tod/", "tokens": ["Wie", "bit\u00b7ter", "bis\u00b7tu", "her\u00b7ber", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du uns das entziehst/", "tokens": ["Wenn", "du", "uns", "das", "ent\u00b7ziehst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PDS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was uns auff dieser Welt n\u00e4chst GOtt", "tokens": ["Was", "uns", "auff", "die\u00b7ser", "Welt", "n\u00e4chst", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am allerliebsten ist/", "tokens": ["Am", "al\u00b7ler\u00b7liebs\u00b7ten", "ist", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn mit betr\u00fcbtem Hertzzerschneiden", "tokens": ["Wenn", "mit", "be\u00b7tr\u00fcb\u00b7tem", "Hertz\u00b7zer\u00b7schnei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die treusten Freunde von uns scheiden!", "tokens": ["Die", "treus\u00b7ten", "Freun\u00b7de", "von", "uns", "sch