{"textgrid.poem.51015": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verschlossen und dunkel ist um und um", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschlossen und dunkel ist um und um", "tokens": ["Ver\u00b7schlos\u00b7sen", "und", "dun\u00b7kel", "ist", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "APPR", "KON", "KOUI"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein winterlich Herze zu schauen,", "tokens": ["Mein", "win\u00b7ter\u00b7lich", "Her\u00b7ze", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch innen, da ist es leuchtend und hell", "tokens": ["Doch", "in\u00b7nen", ",", "da", "ist", "es", "leuch\u00b7tend", "und", "hell"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "KON", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dehnen sich gr\u00fcnende Auen.", "tokens": ["Und", "deh\u00b7nen", "sich", "gr\u00fc\u00b7nen\u00b7de", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Da stell ich den Fr\u00fchling im kleinen auf", "tokens": ["Da", "stell", "ich", "den", "Fr\u00fch\u00b7ling", "im", "klei\u00b7nen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Roseng\u00e4rten und Bronnen", "tokens": ["Mit", "Ro\u00b7sen\u00b7g\u00e4r\u00b7ten", "und", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und spann ich ein zierliches Himmelsgezelt", "tokens": ["Und", "spann", "ich", "ein", "zier\u00b7li\u00b7ches", "Him\u00b7mels\u00b7ge\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit Regenb\u00f6gen und Sonnen.", "tokens": ["Mit", "Re\u00b7gen\u00b7b\u00f6\u00b7gen", "und", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da entz\u00fcnd ich Morgen- und Abendrot", "tokens": ["Da", "ent\u00b7z\u00fcnd", "ich", "Mor\u00b7gen", "und", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "TRUNC", "KON", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lasse die Nachtigall schlagen,", "tokens": ["Und", "las\u00b7se", "die", "Nach\u00b7ti\u00b7gall", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schlank gehende, bl\u00fchende Jungfr\u00e4ulein", "tokens": ["Schlank", "ge\u00b7hen\u00b7de", ",", "bl\u00fc\u00b7hen\u00b7de", "Jung\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Meergr\u00fcne Gew\u00e4nder tragen.", "tokens": ["Meer\u00b7gr\u00fc\u00b7ne", "Ge\u00b7w\u00e4n\u00b7der", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann \u00e4ndr' ich die Szene, dann la\u00df ich mit Macht", "tokens": ["Dann", "\u00e4ndr'", "ich", "die", "Sze\u00b7ne", ",", "dann", "la\u00df", "ich", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVIMP", "PPER", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den gewaltigen Sommer ergl\u00fchen,", "tokens": ["Den", "ge\u00b7wal\u00b7ti\u00b7gen", "Som\u00b7mer", "er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Schnitter auf goldenen Garben ruhn,", "tokens": ["Die", "Schnit\u00b7ter", "auf", "gol\u00b7de\u00b7nen", "Gar\u00b7ben", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Blutrot das Mohnfeld bl\u00fchen.", "tokens": ["Blut\u00b7rot", "das", "Mohn\u00b7feld", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann pl\u00f6tzlich erhell ich mit Wetterschein", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "er\u00b7hell", "ich", "mit", "Wet\u00b7ter\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein Herz und f\u00fcll es mit St\u00fcrmen,", "tokens": ["Mein", "Herz", "und", "f\u00fcll", "es", "mit", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df Schiffe und M\u00e4nner zu Grunde gehn,", "tokens": ["La\u00df", "Schif\u00b7fe", "und", "M\u00e4n\u00b7ner", "zu", "Grun\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dann \u00bbFeuer\u00ab auf Bergen und T\u00fcrmen!", "tokens": ["Dann", "\u00bb", "Feu\u00b7er", "\u00ab", "auf", "Ber\u00b7gen", "und", "T\u00fcr\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Hei! Revolution und Mordgeschrei", "tokens": ["Hei", "!", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "und", "Mord\u00b7ge\u00b7schrei"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Galgen und Guillotinen!", "tokens": ["Mit", "Gal\u00b7gen", "und", "Guil\u00b7lo\u00b7ti\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gek\u00f6pfte K\u00f6nige, wahnsinnig Volk,", "tokens": ["Ge\u00b7k\u00f6pf\u00b7te", "K\u00f6\u00b7ni\u00b7ge", ",", "wahn\u00b7sin\u00b7nig", "Volk", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Konvente und H\u00f6llenmaschinen!", "tokens": ["Kon\u00b7ven\u00b7te", "und", "H\u00f6l\u00b7len\u00b7ma\u00b7schi\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Nun ist mein Busen der Gr\u00e8veplatz", "tokens": ["Nun", "ist", "mein", "Bu\u00b7sen", "der", "Gr\u00e8\u00b7ve\u00b7platz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Voll P\u00f6bels und blutiger Leichen;", "tokens": ["Voll", "P\u00f6\u00b7bels", "und", "blu\u00b7ti\u00b7ger", "Lei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich sehe mich selber im dicksten Gew\u00fchl", "tokens": ["Ich", "se\u00b7he", "mich", "sel\u00b7ber", "im", "dicks\u00b7ten", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Entsetzt und todbla\u00df schleichen.", "tokens": ["Ent\u00b7setzt", "und", "tod\u00b7bla\u00df", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es wird mir so bang, kaum find ich die Kraft,", "tokens": ["Es", "wird", "mir", "so", "bang", ",", "kaum", "find", "ich", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Greuel noch wegzuhauchen:", "tokens": ["Den", "Greu\u00b7el", "noch", "weg\u00b7zu\u00b7hau\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Braun d\u00e4mmert ein Moor, ich liege tot,", "tokens": ["Braun", "d\u00e4m\u00b7mert", "ein", "Moor", ",", "ich", "lie\u00b7ge", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo verlassene Tr\u00fcmmer rauchen.", "tokens": ["Wo", "ver\u00b7las\u00b7se\u00b7ne", "Tr\u00fcm\u00b7mer", "rau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wie alles so stumm und erstorben ist,", "tokens": ["Wie", "al\u00b7les", "so", "stumm", "und", "ers\u00b7tor\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So trag ich mich schweigend zu Grabe", "tokens": ["So", "trag", "ich", "mich", "schwei\u00b7gend", "zu", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und pflanz ein schwarzes Kreuz darauf,", "tokens": ["Und", "pflanz", "ein", "schwar\u00b7zes", "Kreuz", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich selber gezimmert habe.", "tokens": ["Das", "ich", "sel\u00b7ber", "ge\u00b7zim\u00b7mert", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Ich schreibe darauf: Hier ist ins Gras", "tokens": ["Ich", "schrei\u00b7be", "da\u00b7rauf", ":", "Hier", "ist", "ins", "Gras"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$.", "ADV", "VAFIN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein spielender Tr\u00e4umer gekrochen.", "tokens": ["Ein", "spie\u00b7len\u00b7der", "Tr\u00e4u\u00b7mer", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wohl ihm und uns: w\u00e4r die Welt von Glas,", "tokens": ["Wohl", "ihm", "und", "uns", ":", "w\u00e4r", "die", "Welt", "von", "Glas", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "PPER", "$.", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Er h\u00e4tte sie lange zerbrochen!", "tokens": ["Er", "h\u00e4t\u00b7te", "sie", "lan\u00b7ge", "zer\u00b7bro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Verschlossen und dunkel ist u