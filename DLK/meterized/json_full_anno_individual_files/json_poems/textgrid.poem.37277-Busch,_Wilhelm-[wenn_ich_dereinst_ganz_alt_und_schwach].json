{"textgrid.poem.37277": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ich dereinst ganz alt und schwach]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich dereinst ganz alt und schwach,", "tokens": ["Wenn", "ich", "de\u00b7reinst", "ganz", "alt", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und 's ist mal ein milder Sommertag,", "tokens": ["Und", "'s", "ist", "mal", "ein", "mil\u00b7der", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So hink ich wohl aus dem kleinen Haus", "tokens": ["So", "hink", "ich", "wohl", "aus", "dem", "klei\u00b7nen", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis unter den Lindenbaum hinaus.", "tokens": ["Bis", "un\u00b7ter", "den", "Lin\u00b7den\u00b7baum", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da setz ich mich denn im Sonnenschein", "tokens": ["Da", "setz", "ich", "mich", "denn", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.