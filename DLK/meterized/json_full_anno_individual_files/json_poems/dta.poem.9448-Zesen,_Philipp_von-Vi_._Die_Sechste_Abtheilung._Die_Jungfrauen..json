{"dta.poem.9448": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n Die Sechste Abtheilung.  \n Die Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kehre doch wider/ kom\u0303 wider zu r\u00fccke/", "tokens": ["Keh\u00b7re", "doch", "wi\u00b7der", "/", "kom\u0303", "wi\u00b7der", "zu", "r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$(", "VVIMP", "PTKVZ", "PTKZU", "VVFIN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Kehre doch wieder/ was steuchstu f\u00fcr mier?", "tokens": ["Keh\u00b7re", "doch", "wie\u00b7der", "/", "was", "steuchs\u00b7tu", "f\u00fcr", "mier", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$(", "PWS", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "G\u00f6nne mir deine verliebete bl\u00fccke/", "tokens": ["G\u00f6n\u00b7ne", "mir", "dei\u00b7ne", "ver\u00b7lie\u00b7be\u00b7te", "bl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Eyle mein Leben/ mein\u2019 einige zier:", "tokens": ["Ey\u00b7le", "mein", "Le\u00b7ben", "/", "mein'", "ei\u00b7ni\u00b7ge", "zier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "PPOSAT", "PIAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "La\u00df sehen und schauen", "tokens": ["La\u00df", "se\u00b7hen", "und", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die lustigen auen/", "tokens": ["Die", "lus\u00b7ti\u00b7gen", "au\u00b7en", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die sch\u00f6ne gestalt;", "tokens": ["Die", "sch\u00f6\u00b7ne", "ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Seht! wie mich mein Leben", "tokens": ["Seht", "!", "wie", "mich", "mein", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Den banden begeben;", "tokens": ["Den", "ban\u00b7den", "be\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Gleichet den Heeren/ der Krieges-gewalt.", "tokens": ["Glei\u00b7chet", "den", "Hee\u00b7ren", "/", "der", "Krie\u00b7ge\u00b7sge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}