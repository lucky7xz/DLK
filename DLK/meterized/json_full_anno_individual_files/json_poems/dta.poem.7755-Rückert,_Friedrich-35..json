{"dta.poem.7755": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast auf stein'ger H\u00f6h mit M\u00fch gepflanzt den Garten,", "tokens": ["Du", "hast", "auf", "stein'\u00b7ger", "H\u00f6h", "mit", "M\u00fch", "ge\u00b7pflanzt", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch m\u00fchsamer ist der W\u00e4sserung zu warten.", "tokens": ["Und", "noch", "m\u00fch\u00b7sa\u00b7mer", "ist", "der", "W\u00e4s\u00b7se\u00b7rung", "zu", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich w\u00fcnschte dir dazu solch einen Quell verliehn,", "tokens": ["Ich", "w\u00fcnschte", "dir", "da\u00b7zu", "solch", "ei\u00b7nen", "Quell", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie der von dem ich las, selbst sah ich niemals ihn;", "tokens": ["Wie", "der", "von", "dem", "ich", "las", ",", "selbst", "sah", "ich", "nie\u00b7mals", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "PRELS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der so willf\u00e4hrig ist, wie alle Elemente", "tokens": ["Der", "so", "will\u00b7f\u00e4h\u00b7rig", "ist", ",", "wie", "al\u00b7le", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dem Menschen w\u00e4ren, wenn er erst den Zauber kennte.", "tokens": ["Dem", "Men\u00b7schen", "w\u00e4\u00b7ren", ",", "wenn", "er", "erst", "den", "Zau\u00b7ber", "kenn\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der flie\u00dft aus einer Schlucht, doch flie\u00dft nur wenn man will;", "tokens": ["Der", "flie\u00dft", "aus", "ei\u00b7ner", "Schlucht", ",", "doch", "flie\u00dft", "nur", "wenn", "man", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ADV", "KOUS", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn man es befiehlt, so steht er wieder still.", "tokens": ["Und", "wenn", "man", "es", "be\u00b7fiehlt", ",", "so", "steht", "er", "wie\u00b7der", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer von den Nachbarn nun will seinen Garten w\u00e4ssern,", "tokens": ["Wer", "von", "den", "Nach\u00b7barn", "nun", "will", "sei\u00b7nen", "Gar\u00b7ten", "w\u00e4s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der geht zum Quell hin nicht mit N\u00e4pfen oder F\u00e4ssern.", "tokens": ["Der", "geht", "zum", "Quell", "hin", "nicht", "mit", "N\u00e4p\u00b7fen", "o\u00b7der", "F\u00e4s\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er geht nur hin und ruft laut in die Felsenschlucht:", "tokens": ["Er", "geht", "nur", "hin", "und", "ruft", "laut", "in", "die", "Fel\u00b7sen\u00b7schlucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich brauche Wasser, Quell! und nimmt sogleich die Flucht.", "tokens": ["Ich", "brau\u00b7che", "Was\u00b7ser", ",", "Quell", "!", "und", "nimmt", "sog\u00b7leich", "die", "Flucht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$.", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Alsbald kommt auf den Fu\u00df die Flut ihm nachgeflossen,", "tokens": ["Als\u00b7bald", "kommt", "auf", "den", "Fu\u00df", "die", "Flut", "ihm", "nach\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hat aufs Gartenland befruchtend sich ergossen.", "tokens": ["Und", "hat", "aufs", "Gar\u00b7ten\u00b7land", "be\u00b7fruch\u00b7tend", "sich", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und wenn hinreichend nun scheint die Bew\u00e4sserung,", "tokens": ["Und", "wenn", "hin\u00b7rei\u00b7chend", "nun", "scheint", "die", "Be\u00b7w\u00e4s\u00b7se\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So gehst du hin zur Schlucht und rufst: Es ist genung!", "tokens": ["So", "gehst", "du", "hin", "zur", "Schlucht", "und", "rufst", ":", "Es", "ist", "ge\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "KON", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und stampfest mit dem Fu\u00df dreimal. Auf dieses Zeichen", "tokens": ["Und", "stamp\u00b7fest", "mit", "dem", "Fu\u00df", "drei\u00b7mal", ".", "Auf", "die\u00b7ses", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Alsbald siehst du die Flut zur\u00fcck zum Quelle weichen.", "tokens": ["Als\u00b7bald", "siehst", "du", "die", "Flut", "zu\u00b7r\u00fcck", "zum", "Quel\u00b7le", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Er sammelt wieder die entsandten Wasserschlangen,", "tokens": ["Er", "sam\u00b7melt", "wie\u00b7der", "die", "ent\u00b7sand\u00b7ten", "Was\u00b7ser\u00b7schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00e4lt im Schoo\u00df sie bis auf weiteres gefangen.", "tokens": ["Und", "h\u00e4lt", "im", "Schoo\u00df", "sie", "bis", "auf", "wei\u00b7te\u00b7res", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ADV", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}