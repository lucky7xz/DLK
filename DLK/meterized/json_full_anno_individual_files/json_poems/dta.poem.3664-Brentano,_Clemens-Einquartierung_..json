{"dta.poem.3664": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Einquartierung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Enten sprechen: Soldaten kommen! Solda-               ", "tokens": ["Die", "En\u00b7ten", "spre\u00b7chen", ":", "Sol\u00b7da\u00b7ten", "kom\u00b7men", "!", "Solda"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVINF", "$.", "VMFIN", "VVINF", "$.", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ten kommen!", "tokens": ["ten", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Enterich spricht: Sackerlot, sackerlot!", "tokens": ["Der", "En\u00b7te\u00b7rich", "spricht", ":", "Sa\u00b7cker\u00b7lot", ",", "sa\u00b7cker\u00b7lot", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Haushund spricht: Wo? wo? wo? wo?", "tokens": ["Der", "Haus\u00b7hund", "spricht", ":", "Wo", "?", "wo", "?", "wo", "?", "wo", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "$.", "PWAV", "$.", "PWAV", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Katze spricht: Von Bernau, von Bernau!", "tokens": ["Die", "Kat\u00b7ze", "spricht", ":", "Von", "Bern\u00b7au", ",", "von", "Bern\u00b7au", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NE", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+++", "measure": "zehnsilber"}, "line.6": {"text": "Der Hahn auf der Muuer: Sie sind schon da.", "tokens": ["Der", "Hahn", "auf", "der", "Muu\u00b7er", ":", "Sie", "sind", "schon", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}