{"textgrid.poem.66148": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Liebe im Kerker", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H \u2013 ist der Ort, wo ich gefangen bin.", "tokens": ["H", "\u2013", "ist", "der", "Ort", ",", "wo", "ich", "ge\u00b7fan\u00b7gen", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "$(", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Banden wein' ich hier mein Trauerleben hin,", "tokens": ["In", "Ban\u00b7den", "wein'", "ich", "hier", "mein", "Trau\u00b7er\u00b7le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und immer dennoch bleibt dies ungl\u00fcckvolle Leben", "tokens": ["Und", "im\u00b7mer", "den\u00b7noch", "bleibt", "dies", "un\u00b7gl\u00fc\u00b7ck\u00b7vol\u00b7le", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PDS", "ADJA", "NN"], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Liebe Tyrannei zum Opfer hingegeben.", "tokens": ["Der", "Lie\u00b7be", "Ty\u00b7ran\u00b7nei", "zum", "Op\u00b7fer", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gezwungen tugendhaft, weil du nicht bei mir bist,", "tokens": ["Ge\u00b7zwun\u00b7gen", "tu\u00b7gend\u00b7haft", ",", "weil", "du", "nicht", "bei", "mir", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fluch' ich der Unschuld oft, die mir beschwerlich ist.", "tokens": ["Fluch'", "ich", "der", "Un\u00b7schuld", "oft", ",", "die", "mir", "be\u00b7schwer\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch bis zur Wuth verliebt soll ich die Liebe zwingen!", "tokens": ["Noch", "bis", "zur", "Wuth", "ver\u00b7liebt", "soll", "ich", "die", "Lie\u00b7be", "zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "VVPP", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie schwer, wie grausam ist's, bei meiner Pein zu ringen", "tokens": ["Wie", "schwer", ",", "wie", "grau\u00b7sam", "ist's", ",", "bei", "mei\u00b7ner", "Pein", "zu", "rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach, eh' einmal die Ruh' dies arme Herz erquickt,", "tokens": ["Ach", ",", "eh'", "ein\u00b7mal", "die", "Ruh'", "dies", "ar\u00b7me", "Herz", "er\u00b7quickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ART", "NN", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Eh' die Vernunft einmal die Glut in mir erstickt:", "tokens": ["Eh'", "die", "Ver\u00b7nunft", "ein\u00b7mal", "die", "Glut", "in", "mir", "er\u00b7stickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie oft, wie oft werd' ich noch lieben, noch bereuen,", "tokens": ["Wie", "oft", ",", "wie", "oft", "werd'", "ich", "noch", "lie\u00b7ben", ",", "noch", "be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verlangen, hassen, flehn, verzweifeln, suchen, scheuen!", "tokens": ["Ver\u00b7lan\u00b7gen", ",", "has\u00b7sen", ",", "flehn", ",", "ver\u00b7zwei\u00b7feln", ",", "su\u00b7chen", ",", "scheu\u00b7en", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mich ", "tokens": ["Mich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Und alles will ich thun, nur dich vergessen nicht.", "tokens": ["Und", "al\u00b7les", "will", "ich", "thun", ",", "nur", "dich", "ver\u00b7ges\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "wor