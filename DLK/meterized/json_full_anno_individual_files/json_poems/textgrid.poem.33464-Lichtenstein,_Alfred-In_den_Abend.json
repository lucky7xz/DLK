{"textgrid.poem.33464": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "In den Abend", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus krummen Nebeln wachsen K\u00f6stlichkeiten.", "tokens": ["Aus", "krum\u00b7men", "Ne\u00b7beln", "wach\u00b7sen", "K\u00f6st\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz winzge Dinge wurden pl\u00f6tzlich wichtig.", "tokens": ["Ganz", "winz\u00b7ge", "Din\u00b7ge", "wur\u00b7den", "pl\u00f6tz\u00b7lich", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel ist schon gr\u00fcn und undurchsichtig", "tokens": ["Der", "Him\u00b7mel", "ist", "schon", "gr\u00fcn", "und", "un\u00b7durch\u00b7sich\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort hinten, wo die blinden H\u00fcgel gleiten.", "tokens": ["Dort", "hin\u00b7ten", ",", "wo", "die", "blin\u00b7den", "H\u00fc\u00b7gel", "glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zerlumpte B\u00e4ume strolchen in die Ferne.", "tokens": ["Zer\u00b7lump\u00b7te", "B\u00e4u\u00b7me", "strol\u00b7chen", "in", "die", "Fer\u00b7ne", "."], "token_info": ["word", "word", "