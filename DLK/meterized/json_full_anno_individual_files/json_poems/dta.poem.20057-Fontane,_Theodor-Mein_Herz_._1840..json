{"dta.poem.20057": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Mein Herz .  \n 1840.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Der stolzen Sonne, hei\u00df und gl\u00fchend,", "tokens": ["Der", "stol\u00b7zen", "Son\u00b7ne", ",", "hei\u00df", "und", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem stillen Monde tr\u00fcb und bleich,", "tokens": ["Dem", "stil\u00b7len", "Mon\u00b7de", "tr\u00fcb", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2014 Sehns\u00fcchtig tausend Sterne spr\u00fchend \u2014", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "tau\u00b7send", "Ster\u00b7ne", "spr\u00fc\u00b7hend"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "CARD", "NN", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mein Herz, mein Herz ist beiden gleich.", "tokens": ["Mein", "Herz", ",", "mein", "Herz", "ist", "bei\u00b7den", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PIAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Himmel, klar und rein und blauend,", "tokens": ["Dem", "Him\u00b7mel", ",", "klar", "und", "rein", "und", "blau\u00b7end", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wolke, \u2014 jetzt gewitterreich", "tokens": ["Der", "Wol\u00b7ke", ",", "jetzt", "ge\u00b7wit\u00b7ter\u00b7reich"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "$(", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und jetzt in Thr\u00e4nen niederthauend, \u2014", "tokens": ["Und", "jetzt", "in", "Thr\u00e4\u00b7nen", "nie\u00b7dert\u00b7hau\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz, mein Herz ist beiden gleich.", "tokens": ["Mein", "Herz", ",", "mein", "Herz", "ist", "bei\u00b7den", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PIAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Nachtigall voll frischer Lieder,", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", "voll", "fri\u00b7scher", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rose \u2014 bl\u00fcthen- \u2014 dornenreich,", "tokens": ["Der", "Ro\u00b7se", "bl\u00fcthen", "dor\u00b7nen\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "TRUNC", "$(", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Fr\u00fchling und dem Winter wieder,", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "und", "dem", "Win\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz es ist dem Allen gleich.", "tokens": ["Mein", "Herz", "es", "ist", "dem", "Al\u00b7len", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur Einem gleicht es nicht auf Erden:", "tokens": ["Nur", "Ei\u00b7nem", "gleicht", "es", "nicht", "auf", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie will in seinem kleinen Reich", "tokens": ["Nie", "will", "in", "sei\u00b7nem", "klei\u00b7nen", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der langersehnte Friede werden,", "tokens": ["Der", "lan\u00b7ger\u00b7sehn\u00b7te", "Frie\u00b7de", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum ist es nie sich selber gleich.", "tokens": ["Drum", "ist", "es", "nie", "sich", "sel\u00b7ber", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}