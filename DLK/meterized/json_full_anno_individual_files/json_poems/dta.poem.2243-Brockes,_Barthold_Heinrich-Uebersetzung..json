{"dta.poem.2243": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jedes Sittlichs Lehrgeb\u00e4ude,", "tokens": ["Ein", "je\u00b7des", "Sitt\u00b7lichs", "Lehr\u00b7ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Worinn man melancholisch lehrt:", "tokens": ["Wo\u00b7rinn", "man", "me\u00b7lan\u00b7cho\u00b7lisch", "lehrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da\u00df eine m\u00e4\u00dfige vern\u00fcnftge Freude", "tokens": ["Da\u00df", "ei\u00b7ne", "m\u00e4\u00b7\u00dfi\u00b7ge", "ver\u00b7n\u00fcnft\u00b7ge", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Kreaturen, S\u00fcnde sey;", "tokens": ["An", "Kre\u00b7a\u00b7tu\u00b7ren", ",", "S\u00fcn\u00b7de", "sey", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist eine Schw\u00e4rmerey,", "tokens": ["Ist", "ei\u00b7ne", "Schw\u00e4r\u00b7me\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die die Religion und die Natur entehrt.", "tokens": ["Die", "die", "Re\u00b7li\u00b7gi\u00b7on", "und", "die", "Na\u00b7tur", "ent\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}