{"textgrid.poem.48201": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Musikant", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung, in den hohen Spielmannsorden", "tokens": ["Jung", ",", "in", "den", "ho\u00b7hen", "Spiel\u00b7mann\u00b7sor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trat ein ich, weil es mir gefiel,", "tokens": ["Trat", "ein", "ich", ",", "weil", "es", "mir", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun \u00bbalter Musikant\u00ab geworden,", "tokens": ["Nun", "\u00bb", "al\u00b7ter", "Mu\u00b7si\u00b7kant", "\u00ab", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zieh' ich umher mit meinem Spiel.", "tokens": ["Zieh'", "ich", "um\u00b7her", "mit", "mei\u00b7nem", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um schweift mein Aug', um geht der Teller,", "tokens": ["Um", "schweift", "mein", "Aug'", ",", "um", "geht", "der", "Tel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVFIN", "PPOSAT", "NN", "$,", "KOUI", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Scherflein, z\u00f6gernd, f\u00e4llt hinein,", "tokens": ["Ein", "Scher\u00b7flein", ",", "z\u00f6\u00b7gernd", ",", "f\u00e4llt", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich nehme meinen Beifalls-Heller", "tokens": ["Ich", "neh\u00b7me", "mei\u00b7nen", "Bei\u00b7falls\u00b7Hel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df es noch zufrieden sein.", "tokens": ["Und", "mu\u00df", "es", "noch", "zu\u00b7frie\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, hingeschwundne junge Tage,", "tokens": ["Ach", ",", "hin\u00b7ge\u00b7schwund\u00b7ne", "jun\u00b7ge", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie wieder kehrt ihr mir zur\u00fcck, \u2013", "tokens": ["Nie", "wie\u00b7der", "kehrt", "ihr", "mir", "zu\u00b7r\u00fcck", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch an Frau Fortunas Schlage,", "tokens": ["Und", "doch", "an", "Frau", "For\u00b7tu\u00b7nas", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+