{"textgrid.poem.60158": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Glanze der Religion", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Glanze der Religion", "tokens": ["Vom", "Glan\u00b7ze", "der", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Umgebner Prinz! dir singen Engel Lieder;", "tokens": ["Um\u00b7ge\u00b7bner", "Prinz", "!", "dir", "sin\u00b7gen", "En\u00b7gel", "Lie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wirfst dich vor dem h\u00f6chsten Thron", "tokens": ["Du", "wirfst", "dich", "vor", "dem", "h\u00f6chs\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des K\u00f6niges der K\u00f6nige danieder!", "tokens": ["Des", "K\u00f6\u00b7ni\u00b7ges", "der", "K\u00f6\u00b7ni\u00b7ge", "da\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Freudigkeit bist du ein Christ", "tokens": ["Mit", "Freu\u00b7dig\u00b7keit", "bist", "du", "ein", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tiefgeb\u00fccket huldigst du dem grossen", "tokens": ["Und", "tief\u00b7ge\u00b7b\u00fc\u00b7cket", "hul\u00b7digst", "du", "dem", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Regierer, der allm\u00e4chtig ist,", "tokens": ["Re\u00b7gie\u00b7rer", ",", "der", "all\u00b7m\u00e4ch\u00b7tig", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00f6nige von ihrem Stuhl zu stossen!", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "von", "ih\u00b7rem", "Stuhl", "zu", "stos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der dessen Reich von dieser Welt", "tokens": ["Der", "des\u00b7sen", "Reich", "von", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRELAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht war, kam aus dem Himmel auf die Erde;", "tokens": ["Nicht", "war", ",", "kam", "aus", "dem", "Him\u00b7mel", "auf", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Menschen hat er sich gesellt", "tokens": ["Zu", "Men\u00b7schen", "hat", "er", "sich", "ge\u00b7sellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit der Mensch mit Gott vers\u00f6hnet werde!", "tokens": ["Da\u00b7mit", "der", "Mensch", "mit", "Gott", "ver\u00b7s\u00f6h\u00b7net", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er stieg nach seiner grossen That", "tokens": ["Er", "stieg", "nach", "sei\u00b7ner", "gros\u00b7sen", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seinen Engeln in den Himmel wieder;", "tokens": ["Zu", "sei\u00b7nen", "En\u00b7geln", "in", "den", "Him\u00b7mel", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er war ein Siegesheld; er trat", "tokens": ["Er", "war", "ein", "Sie\u00b7ge\u00b7sheld", ";", "er", "trat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zehntausend H\u00f6llen-Ungeheuer nieder!", "tokens": ["Zehn\u00b7tau\u00b7send", "H\u00f6l\u00b7len\u00b7Un\u00b7ge\u00b7heu\u00b7er", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er lie\u00df uns ein Ged\u00e4chtni\u00dfmahl,", "tokens": ["Er", "lie\u00df", "uns", "ein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Zeugni\u00df seiner Liebe, bis zum Ende", "tokens": ["Zum", "Zeug\u00b7ni\u00df", "sei\u00b7ner", "Lie\u00b7be", ",", "bis", "zum", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Zeitenwechsels, und befahl", "tokens": ["Des", "Zei\u00b7ten\u00b7wech\u00b7sels", ",", "und", "be\u00b7fahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geheimnisse in seiner Diener H\u00e4nde.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7se", "in", "sei\u00b7ner", "Die\u00b7ner", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Prinz! Seiner sch\u00e4mest du dich nicht,", "tokens": ["Prinz", "!", "Sei\u00b7ner", "sch\u00e4\u00b7mest", "du", "dich", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Weiser gab der Erde besser Lehren:", "tokens": ["Kein", "Wei\u00b7ser", "gab", "der", "Er\u00b7de", "bes\u00b7ser", "Leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gott lieben ist die erste Pflicht,", "tokens": ["Gott", "lie\u00b7ben", "ist", "die", "ers\u00b7te", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bald nach Gott die K\u00f6nige verehren.", "tokens": ["Und", "bald", "nach", "Gott", "die", "K\u00f6\u00b7ni\u00b7ge", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Den N\u00e4chsten lieben so wie sich", "tokens": ["Den", "N\u00e4chs\u00b7ten", "lie\u00b7ben", "so", "wie", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOKOM", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle Tugenden der Engel \u00fcben;", "tokens": ["Und", "al\u00b7le", "Tu\u00b7gen\u00b7den", "der", "En\u00b7gel", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht seyn, was gegen Friederich", "tokens": ["Nicht", "seyn", ",", "was", "ge\u00b7gen", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VAINF", "$,", "PRELS", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Feinde sind, die nicht den Frieden lieben;", "tokens": ["Die", "Fein\u00b7de", "sind", ",", "die", "nicht", "den", "Frie\u00b7den", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dis lehret die Religion", "tokens": ["Dis", "leh\u00b7ret", "die", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Jesum Christum ihren Stifter nennet.", "tokens": ["Die", "Je\u00b7sum", "Chris\u00b7tum", "ih\u00b7ren", "Stif\u00b7ter", "nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heyl sey dem Herrscher auf dem Thron", "tokens": ["Heyl", "sey", "dem", "Herr\u00b7scher", "auf", "dem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich mit Herz und Mund dazu bekennet.", "tokens": ["Der", "sich", "mit", "Herz", "und", "Mund", "da\u00b7zu", "be\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Du gehst o Prinz! dem Volke vor;", "tokens": ["Du", "gehst", "o", "Prinz", "!", "dem", "Vol\u00b7ke", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "NN", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ermuntert hebt es seine matten Blicke", "tokens": ["Er\u00b7mun\u00b7tert", "hebt", "es", "sei\u00b7ne", "mat\u00b7ten", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Krieges Angst, zu dir empor", "tokens": ["Aus", "Krie\u00b7ges", "Angst", ",", "zu", "dir", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht dir Heyl, sich selber w\u00fcnscht es Gl\u00fccke.", "tokens": ["Und", "w\u00fcnscht", "dir", "Heyl", ",", "sich", "sel\u00b7ber", "w\u00fcnscht", "es", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PRF", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Du wirst mit rechtem Christenmuth", "tokens": ["Du", "wirst", "mit", "rech\u00b7tem", "Chris\u00b7ten\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Friedrichs Hand empfangen Kriegeswaffen;", "tokens": ["Aus", "Fried\u00b7richs", "Hand", "emp\u00b7fan\u00b7gen", "Krie\u00b7ges\u00b7waf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht zu vergiessen Menschenblut:", "tokens": ["Nicht", "zu", "ver\u00b7gies\u00b7sen", "Men\u00b7schen\u00b7blut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, deinem Vaterlande Recht zu schaffen.", "tokens": ["Nein", ",", "dei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", "Recht", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Schon r\u00fcstest du dic