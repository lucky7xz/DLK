{"textgrid.poem.43111": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es liegt voll seichter Logik", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt voll seichter Logik", "tokens": ["Es", "liegt", "voll", "seich\u00b7ter", "Lo\u00b7gik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Brief in meinen H\u00e4nden;", "tokens": ["Dein", "Brief", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du meinst, was einen Anfang gehabt,", "tokens": ["Du", "meinst", ",", "was", "ei\u00b7nen", "An\u00b7fang", "ge\u00b7habt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das m\u00fcss' auch wieder enden.", "tokens": ["Das", "m\u00fcss'", "auch", "wie\u00b7der", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kann mit solcher Weisheit", "tokens": ["Ich", "kann", "mit", "sol\u00b7cher", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich heute nimmer raufen;", "tokens": ["Mich", "heu\u00b7te", "nim\u00b7mer", "rau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PP