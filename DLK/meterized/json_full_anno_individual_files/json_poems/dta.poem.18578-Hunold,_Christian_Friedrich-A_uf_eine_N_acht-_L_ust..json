{"dta.poem.18578": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A uf eine  N acht- L ust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Sonne la\u00df den heissen Strahl", "tokens": ["Son\u00b7ne", "la\u00df", "den", "heis\u00b7sen", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich doch mit der See verm\u00e4hlen/", "tokens": ["Sich", "doch", "mit", "der", "See", "ver\u00b7m\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn so kan ich abermahl", "tokens": ["Denn", "so", "kan", "ich", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend s\u00fcsse Stunden zehlen/", "tokens": ["Tau\u00b7send", "s\u00fcs\u00b7se", "Stun\u00b7den", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil allein in schwartzer Nacht/", "tokens": ["Weil", "al\u00b7lein", "in", "schwart\u00b7zer", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Meine Freuden Sonne lacht.", "tokens": ["Mei\u00b7ne", "Freu\u00b7den", "Son\u00b7ne", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pranget heunt ihr Sternen nicht/", "tokens": ["Pran\u00b7get", "heunt", "ihr", "Ster\u00b7nen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcllet euch in dunckle Schatten/", "tokens": ["H\u00fcl\u00b7let", "euch", "in", "dunck\u00b7le", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es soll ein sch\u00f6ner Licht/", "tokens": ["Denn", "es", "soll", "ein", "sch\u00f6\u00b7ner", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mit meiner Liebe Gatten.", "tokens": ["Sich", "mit", "mei\u00b7ner", "Lie\u00b7be", "Gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur allein dich seh' ich gern/", "tokens": ["Nur", "al\u00b7lein", "dich", "seh'", "ich", "gern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du gew\u00fcnschter Venus-Stern.", "tokens": ["Du", "ge\u00b7w\u00fcnschter", "Ve\u00b7nus\u00b7S\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alles was mich sonst erg\u00f6tzt/", "tokens": ["Al\u00b7les", "was", "mich", "sonst", "er\u00b7g\u00f6tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df numehro von mir weichen/", "tokens": ["Mu\u00df", "nu\u00b7meh\u00b7ro", "von", "mir", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und was sterblich wird gesch\u00e4tzt/", "tokens": ["Und", "was", "sterb\u00b7lich", "wird", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll anitzt die Seegel streichen;", "tokens": ["Soll", "a\u00b7nitzt", "die", "See\u00b7gel", "strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ein Engel gleicher Geist/", "tokens": ["Weil", "ein", "En\u00b7gel", "glei\u00b7cher", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heunte mein Vergn\u00fcgen heist.", "tokens": ["Heun\u00b7te", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}