nct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sollt es mich einmahl erfreun,", "tokens": ["Wie", "sollt", "es", "mich", "ein\u00b7mahl", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit dir viel Grillen zu vertreiben", "tokens": ["Mit", "dir", "viel", "Gril\u00b7len", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und in dem Alter froh zu seyn,", "tokens": ["Und", "in", "dem", "Al\u00b7ter", "froh", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Mit dir, mein R(asper), dem ich mich", "tokens": ["Mit", "dir", ",", "mein", "R", "(", "as\u00b7per", ")", ",", "dem", "ich", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NE", "$(", "NE", "$(", "$,", "PRELS", "PPER", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bey dieser Trennung selber schencke.", "tokens": ["Bey", "die\u00b7ser", "Tren\u00b7nung", "sel\u00b7ber", "schen\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist was versehn, verzeih und dencke,", "tokens": ["Ist", "was", "ver\u00b7sehn", ",", "ver\u00b7zeih", "und", "den\u00b7cke", ","], "token_inf