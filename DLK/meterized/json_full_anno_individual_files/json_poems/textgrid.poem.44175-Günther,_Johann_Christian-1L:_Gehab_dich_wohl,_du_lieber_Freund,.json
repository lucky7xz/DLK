{"textgrid.poem.44175": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gehab dich wohl, du lieber Freund,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehab dich wohl, du lieber Freund,", "tokens": ["Ge\u00b7hab", "dich", "wohl", ",", "du", "lie\u00b7ber", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt geh ich halb verwaist von hinnen", "tokens": ["Jezt", "geh", "ich", "halb", "ver\u00b7waist", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00f6ste die betr\u00fcbten Sinnen", "tokens": ["Und", "tr\u00f6s\u00b7te", "die", "be\u00b7tr\u00fcb\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allem, was nur m\u00f6glich scheint.", "tokens": ["Mit", "al\u00b7lem", ",", "was", "nur", "m\u00f6g\u00b7lich", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich komme wohl so bald nicht wieder", "tokens": ["Ich", "kom\u00b7me", "wohl", "so", "bald", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dencke weiter in die Welt,", "tokens": ["Und", "den\u00b7cke", "wei\u00b7ter", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nachdem der Lohn vor meine Lieder", "tokens": ["Nach\u00b7dem", "der", "Lohn", "vor", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Vaterlande mager f\u00e4llt.", "tokens": ["Im", "Va\u00b7ter\u00b7lan\u00b7de", "ma\u00b7ger", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich habe warlich hohe Zeit,", "tokens": ["Ich", "ha\u00b7be", "war\u00b7lich", "ho\u00b7he", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein zeitlich Wohlseyn fest zu gr\u00fcnden.", "tokens": ["Mein", "zeit\u00b7lich", "Wohl\u00b7seyn", "fest", "zu", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo werd ich wohl den Ruhplaz finden,", "tokens": ["Wo", "werd", "ich", "wohl", "den", "Ruh\u00b7plaz", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ist der Herd vor mich bereit,", "tokens": ["Wo", "ist", "der", "Herd", "vor", "mich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit mein Flei\u00df nach so viel Sorgen", "tokens": ["Da\u00b7mit", "mein", "Flei\u00df", "nach", "so", "viel", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht stets von leerer Hofnung bl\u00fch", "tokens": ["Nicht", "stets", "von", "lee\u00b7rer", "Hof\u00b7nung", "bl\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und endlich auch ein heitrer Morgen", "tokens": ["Und", "end\u00b7lich", "auch", "ein", "hei\u00b7trer", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Gl\u00fcck aus Nacht und Schlummer zieh?", "tokens": ["Mein", "Gl\u00fcck", "aus", "Nacht", "und", "Schlum\u00b7mer", "zieh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es sey auch welcher Ort es will", "tokens": ["Es", "sey", "auch", "wel\u00b7cher", "Ort", "es", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PWAT", "NN", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In S\u00fcden, Norden oder Westen,", "tokens": ["In", "S\u00fc\u00b7den", ",", "Nor\u00b7den", "o\u00b7der", "Wes\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich halt ihn dennoch vor den besten", "tokens": ["Ich", "halt", "ihn", "den\u00b7noch", "vor", "den", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seze mich vergn\u00fcgt und still.", "tokens": ["Und", "se\u00b7ze", "mich", "ver\u00b7gn\u00fcgt", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wird der \u00fcberstandne Jammer", "tokens": ["Da", "wird", "der", "\u00fc\u00b7bers\u00b7tand\u00b7ne", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein treues Herz noch oft erfreun,", "tokens": ["Mein", "treu\u00b7es", "Herz", "noch", "oft", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da soll mir die geringste Kammer", "tokens": ["Da", "soll", "mir", "die", "ge\u00b7rings\u00b7te", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Epicurus Garthen seyn.", "tokens": ["Des", "E\u00b7pi\u00b7cu\u00b7rus", "Gar\u00b7then", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Macht Phillis Wuntsch und Armen voll", "tokens": ["Macht", "Phil\u00b7lis", "Wunt\u00b7sch", "und", "Ar\u00b7men", "voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NN", "KON", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und l\u00e4st mich Gott der Welt noch n\u00fczen,", "tokens": ["Und", "l\u00e4st", "mich", "Gott", "der", "Welt", "noch", "n\u00fc\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schw\u00f6r ich, da\u00df kein Sturm noch Blizen", "tokens": ["So", "schw\u00f6r", "ich", ",", "da\u00df", "kein", "Sturm", "noch", "Bli\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich innerlich mehr r\u00fchren soll.", "tokens": ["Mich", "in\u00b7ner\u00b7lich", "mehr", "r\u00fch\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da will ich ihr und jedem leben,", "tokens": ["Da", "will", "ich", "ihr", "und", "je\u00b7dem", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem mein Verm\u00f6gen dienen kan,", "tokens": ["Dem", "mein", "Ver\u00b7m\u00f6\u00b7gen", "die\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da will ich Red und Antwort geben,", "tokens": ["Da", "will", "ich", "Red", "und", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Warum ich oft nicht recht gethan.", "tokens": ["Wa\u00b7rum", "ich", "oft", "nicht", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Himmel sey mir nur noch hold!", "tokens": ["Der", "Him\u00b7mel", "sey", "mir", "nur", "noch", "hold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach so viel schlecht gerathnen Spr\u00fcngen", "tokens": ["Nach", "so", "viel", "schlecht", "ge\u00b7rath\u00b7nen", "Spr\u00fcn\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wird ja wohl einer gut gelingen.", "tokens": ["Wird", "ja", "wohl", "ei\u00b7ner", "gut", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz verliebt sich nicht in Gold,", "tokens": ["Mein", "Herz", "ver\u00b7liebt", "sich", "nicht", "in", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Ohr verachtet gro\u00dfe Tittel;", "tokens": ["Mein", "Ohr", "ver\u00b7ach\u00b7tet", "gro\u00b7\u00dfe", "Tit\u00b7tel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vermeid ich Schulden und Betrug,", "tokens": ["Ver\u00b7meid", "ich", "Schul\u00b7den", "und", "Be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So hab ich warlich Ehr und Mittel", "tokens": ["So", "hab", "ich", "war\u00b7lich", "Ehr", "und", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und auf der Welt Verdienst genug.", "tokens": ["Und", "auf", "der", "Welt", "Ver\u00b7dienst", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch etwas kommt der Sehnsucht an,", "tokens": ["Noch", "et\u00b7was", "kommt", "der", "Sehn\u00b7sucht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch einmahl in der N\u00e4h zu haben,", "tokens": ["Euch", "ein\u00b7mahl", "in", "der", "N\u00e4h", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Euch Freunde, derer Herz und Gaben", "tokens": ["Euch", "Freun\u00b7de", ",", "de\u00b7rer", "Herz", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PDS", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Musen so viel Guts gethan.", "tokens": ["Den", "Mu\u00b7sen", "so", "viel", "Guts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was wollt ich vor ein Dancklied schreiben,", "tokens": ["Was", "wollt", "ich", "vor", "ein", "Danck\u00b7lied", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sollt es mich einmahl erfreun,", "tokens": ["Wie", "sollt", "es", "mich", "ein\u00b7mahl", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit dir viel Grillen zu vertreiben", "tokens": ["Mit", "dir", "viel", "Gril\u00b7len", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und in dem Alter froh zu seyn,", "tokens": ["Und", "in", "dem", "Al\u00b7ter", "froh", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit dir, mein R(asper), dem ich mich", "tokens": ["Mit", "dir", ",", "mein", "R", "(", "as\u00b7per", ")", ",", "dem", "ich", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NE", "$(", "NE", "$(", "$,", "PRELS", "PPER", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bey dieser Trennung selber schencke.", "tokens": ["Bey", "die\u00b7ser", "Tren\u00b7nung", "sel\u00b7ber", "schen\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist was versehn, verzeih und dencke,", "tokens": ["Ist", "was", "ver\u00b7sehn", ",", "ver\u00b7zeih", "und", "den\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$,", "VVIMP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder Mensch verirret sich,", "tokens": ["Ein", "je\u00b7der", "Mensch", "ver\u00b7ir\u00b7ret", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zumahl wenn Noth und Sp\u00f6tter w\u00fcten,", "tokens": ["Zu\u00b7mahl", "wenn", "Noth", "und", "Sp\u00f6t\u00b7ter", "w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Armuth und Verfolgung pre\u00dft", "tokens": ["Wenn", "Ar\u00b7muth", "und", "Ver\u00b7fol\u00b7gung", "pre\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unsern Flei\u00df schon in den Bl\u00fcthen", "tokens": ["Und", "un\u00b7sern", "Flei\u00df", "schon", "in", "den", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch L\u00e4stergift verwelcken l\u00e4st.", "tokens": ["Durch", "L\u00e4s\u00b7ter\u00b7gift", "ver\u00b7wel\u00b7cken", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich g\u00f6nne dir nebst Gl\u00fcck und Heil", "tokens": ["Ich", "g\u00f6n\u00b7ne", "dir", "nebst", "Gl\u00fcck", "und", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kind von Sch\u00f6nheit, Wiz und Tugend;", "tokens": ["Ein", "Kind", "von", "Sch\u00f6n\u00b7heit", ",", "Wiz", "und", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn vor die saure M\u00fch der Jugend", "tokens": ["Denn", "vor", "die", "sau\u00b7re", "M\u00fch", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist dies doch wohl der beste Theil.", "tokens": ["Ist", "dies", "doch", "wohl", "der", "bes\u00b7te", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erf\u00fcllt sie dir nun Arm und Herze,", "tokens": ["Er\u00b7f\u00fcllt", "sie", "dir", "nun", "Arm", "und", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So las mir auch ein Pl\u00e4zchen zu,", "tokens": ["So", "las", "mir", "auch", "ein", "Pl\u00e4z\u00b7chen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und w\u00fcntsche mir bey deinem Scherze", "tokens": ["Und", "w\u00fcnt\u00b7sche", "mir", "bey", "dei\u00b7nem", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bisweilen auch ein St\u00fcndchen Ruh!", "tokens": ["Bis\u00b7wei\u00b7len", "auch", "ein", "St\u00fcnd\u00b7chen", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Gehab dich wohl, du lieber Freund,", "tokens": ["Ge\u00b7hab", "dich", "wohl", ",", "du", "lie\u00b7ber", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt geh ich halb verwaist von hinnen", "tokens": ["Jezt", "geh", "ich", "halb", "ver\u00b7waist", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00f6ste die betr\u00fcbten Sinnen", "tokens": ["Und", "tr\u00f6s\u00b7te", "die", "be\u00b7tr\u00fcb\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allem, was nur m\u00f6glich scheint.", "tokens": ["Mit", "al\u00b7lem", ",", "was", "nur", "m\u00f6g\u00b7lich", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich komme wohl so bald nicht wieder", "tokens": ["Ich", "kom\u00b7me", "wohl", "so", "bald", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dencke weiter in die Welt,", "tokens": ["Und", "den\u00b7cke", "wei\u00b7ter", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nachdem der Lohn vor meine Lieder", "tokens": ["Nach\u00b7dem", "der", "Lohn", "vor", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Vaterlande mager f\u00e4llt.", "tokens": ["Im", "Va\u00b7ter\u00b7lan\u00b7de", "ma\u00b7ger", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich habe warlich hohe Zeit,", "tokens": ["Ich", "ha\u00b7be", "war\u00b7lich", "ho\u00b7he", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein zeitlich Wohlseyn fest zu gr\u00fcnden.", "tokens": ["Mein", "zeit\u00b7lich", "Wohl\u00b7seyn", "fest", "zu", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo werd ich wohl den Ruhplaz finden,", "tokens": ["Wo", "werd", "ich", "wohl", "den", "Ruh\u00b7plaz", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ist der Herd vor mich bereit,", "tokens": ["Wo", "ist", "der", "Herd", "vor", "mich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit mein Flei\u00df nach so viel Sorgen", "tokens": ["Da\u00b7mit", "mein", "Flei\u00df", "nach", "so", "viel", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht stets von leerer Hofnung bl\u00fch", "tokens": ["Nicht", "stets", "von", "lee\u00b7rer", "Hof\u00b7nung", "bl\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und endlich auch ein heitrer Morgen", "tokens": ["Und", "end\u00b7lich", "auch", "ein", "hei\u00b7trer", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Gl\u00fcck aus Nacht und Schlummer zieh?", "tokens": ["Mein", "Gl\u00fcck", "aus", "Nacht", "und", "Schlum\u00b7mer", "zieh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es sey auch welcher Ort es will", "tokens": ["Es", "sey", "auch", "wel\u00b7cher", "Ort", "es", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PWAT", "NN", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In S\u00fcden, Norden oder Westen,", "tokens": ["In", "S\u00fc\u00b7den", ",", "Nor\u00b7den", "o\u00b7der", "Wes\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich halt ihn dennoch vor den besten", "tokens": ["Ich", "halt", "ihn", "den\u00b7noch", "vor", "den", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seze mich vergn\u00fcgt und still.", "tokens": ["Und", "se\u00b7ze", "mich", "ver\u00b7gn\u00fcgt", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wird der \u00fcberstandne Jammer", "tokens": ["Da", "wird", "der", "\u00fc\u00b7bers\u00b7tand\u00b7ne", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein treues Herz noch oft erfreun,", "tokens": ["Mein", "treu\u00b7es", "Herz", "noch", "oft", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da soll mir die geringste Kammer", "tokens": ["Da", "soll", "mir", "die", "ge\u00b7rings\u00b7te", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Epicurus Garthen seyn.", "tokens": ["Des", "E\u00b7pi\u00b7cu\u00b7rus", "Gar\u00b7then", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Macht Phillis Wuntsch und Armen voll", "tokens": ["Macht", "Phil\u00b7lis", "Wunt\u00b7sch", "und", "Ar\u00b7men", "voll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NN", "KON", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und l\u00e4st mich Gott der Welt noch n\u00fczen,", "tokens": ["Und", "l\u00e4st", "mich", "Gott", "der", "Welt", "noch", "n\u00fc\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schw\u00f6r ich, da\u00df kein Sturm noch Blizen", "tokens": ["So", "schw\u00f6r", "ich", ",", "da\u00df", "kein", "Sturm", "noch", "Bli\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich innerlich mehr r\u00fchren soll.", "tokens": ["Mich", "in\u00b7ner\u00b7lich", "mehr", "r\u00fch\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da will ich ihr und jedem leben,", "tokens": ["Da", "will", "ich", "ihr", "und", "je\u00b7dem", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem mein Verm\u00f6gen dienen kan,", "tokens": ["Dem", "mein", "Ver\u00b7m\u00f6\u00b7gen", "die\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da will ich Red und Antwort geben,", "tokens": ["Da", "will", "ich", "Red", "und", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Warum ich oft nicht recht gethan.", "tokens": ["Wa\u00b7rum", "ich", "oft", "nicht", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der Himmel sey mir nur noch hold!", "tokens": ["Der", "Him\u00b7mel", "sey", "mir", "nur", "noch", "hold", "!"], "t