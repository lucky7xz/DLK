{"dta.poem.7196": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Adam selbst im Paradeise,", "tokens": ["A\u00b7dam", "selbst", "im", "Pa\u00b7ra\u00b7dei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat, auf eben diese Weise,", "tokens": ["Hat", ",", "auf", "e\u00b7ben", "die\u00b7se", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich zur Lust und GOTT zum Preise,", "tokens": ["Sich", "zur", "Lust", "und", "GoTT", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "KON", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhn als Sch\u00f6pffer zu erh\u00f6hn,", "tokens": ["Jhn", "als", "Sch\u00f6pf\u00b7fer", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein Gesch\u00f6pff offt angesehn:", "tokens": ["Sein", "Ge\u00b7sch\u00f6pff", "offt", "an\u00b7ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja er w\u00fcrde, GOTT zur Ehre,", "tokens": ["Ja", "er", "w\u00fcr\u00b7de", ",", "GoTT", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "$,", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn er nicht gefallen w\u00e4re;", "tokens": ["Wenn", "er", "nicht", "ge\u00b7fal\u00b7len", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An des Sch\u00f6pffers Wunder-Gaben", "tokens": ["An", "des", "Sch\u00f6pf\u00b7fers", "Wun\u00b7der\u00b7Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ewig sich gelabet haben.", "tokens": ["E\u00b7wig", "sich", "ge\u00b7la\u00b7bet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Darum wende jedermann", "tokens": ["Da\u00b7rum", "wen\u00b7de", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Seiner Seelen Sinn und Kr\u00e4ffte,", "tokens": ["Sei\u00b7ner", "See\u00b7len", "Sinn", "und", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zu so lieblichem Gesch\u00e4ffte,", "tokens": ["Zu", "so", "lieb\u00b7li\u00b7chem", "Ge\u00b7sch\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wiederum mit Freuden an:", "tokens": ["Wie\u00b7de\u00b7rum", "mit", "Freu\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Sich von neuen zu bereiten,", "tokens": ["Sich", "von", "neu\u00b7en", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Zum Genu\u00df der Herrlichkeiten,", "tokens": ["Zum", "Ge\u00b7nu\u00df", "der", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die in jenem Himmels-Garten", "tokens": ["Die", "in", "je\u00b7nem", "Him\u00b7mels\u00b7Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Unsrer warten.", "tokens": ["Uns\u00b7rer", "war\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}