{"dta.poem.12568": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auch  \n  die\u00df geh\u00f6rt dem K\u00f6nig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wu\u00dft', ein K\u00f6nig ist ein irrer Stern,", "tokens": ["Ich", "wu\u00dft'", ",", "ein", "K\u00f6\u00b7nig", "ist", "ein", "ir\u00b7rer", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nur der Zufall regelt ihm die Bahnen \u2014", "tokens": ["Und", "nur", "der", "Zu\u00b7fall", "re\u00b7gelt", "ihm", "die", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch warnt' ich vor dem Schweif, nicht vor dem Kern,", "tokens": ["Doch", "warnt'", "ich", "vor", "dem", "Schweif", ",", "nicht", "vor", "dem", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Schweif von Sklaven und von Charlatanen.", "tokens": ["Dem", "Schweif", "von", "Skla\u00b7ven", "und", "von", "Char\u00b7la\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich dachte mir: Dein eigen F\u00fcrstenherz", "tokens": ["Ich", "dach\u00b7te", "mir", ":", "Dein", "ei\u00b7gen", "F\u00fcrs\u00b7ten\u00b7herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sei mehr als ein Register seiner Ahnen,", "tokens": ["Sei", "mehr", "als", "ein", "Re\u00b7gis\u00b7ter", "sei\u00b7ner", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und ich verga\u00df, da\u00df stets ein dreifach Erz", "tokens": ["Und", "ich", "ver\u00b7ga\u00df", ",", "da\u00df", "stets", "ein", "drei\u00b7fach", "Erz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Euch, selbst im Tod, von Eurem Volk noch trenne \u2014", "tokens": ["Euch", ",", "selbst", "im", "Tod", ",", "von", "Eu\u00b7rem", "Volk", "noch", "tren\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir Thoren war's als ob ich Dich schon kenne,", "tokens": ["Mir", "Tho\u00b7ren", "wa\u00b7r's", "als", "ob", "ich", "Dich", "schon", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "KOKOM", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ob ges\u00e4ugt uns Einer Mutter Br\u00fcste,", "tokens": ["Als", "ob", "ge\u00b7s\u00e4ugt", "uns", "Ei\u00b7ner", "Mut\u00b7ter", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "VVPP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Mutter, die ich mein Jahrhundert nenne;", "tokens": ["Der", "Mut\u00b7ter", ",", "die", "ich", "mein", "Jahr\u00b7hun\u00b7dert", "nen\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Mir war's, als ob ich in der deutschen W\u00fcste", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "ob", "ich", "in", "der", "deut\u00b7schen", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von einem fernen Quell das Rieseln h\u00f6re,", "tokens": ["Von", "ei\u00b7nem", "fer\u00b7nen", "Quell", "das", "Rie\u00b7seln", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und tr\u00e4umend lag ich an Atlantis' K\u00fcste,", "tokens": ["Und", "tr\u00e4u\u00b7mend", "lag", "ich", "an", "At\u00b7lan\u00b7tis'", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und ich vernahm so feierlich: \u201eIch schw\u00f6re!\u201c", "tokens": ["Und", "ich", "ver\u00b7nahm", "so", "fei\u00b7er\u00b7lich", ":", "\u201e", "Ich", "schw\u00f6\u00b7re", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$.", "$(", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Her\u00fcber klangen von der Ostsee Borden", "tokens": ["Her\u00b7\u00fc\u00b7ber", "klan\u00b7gen", "von", "der", "Ost\u00b7see", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meine Republik die Jubelch\u00f6re.", "tokens": ["In", "mei\u00b7ne", "Re\u00b7pub\u00b7lik", "die", "Ju\u00b7belc\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Begeistert rief ich: \u201eHoher F\u00fcrst im Norden!", "tokens": ["Be\u00b7geis\u00b7tert", "rief", "ich", ":", "\u201e", "Ho\u00b7her", "F\u00fcrst", "im", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das M\u00e4dchen, drum die V\u00e4ter einst gefreit,", "tokens": ["Das", "M\u00e4d\u00b7chen", ",", "drum", "die", "V\u00e4\u00b7ter", "einst", "ge\u00b7freit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist f\u00fcr die S\u00f6hne schier zu alt geworden:", "tokens": ["Ist", "f\u00fcr", "die", "S\u00f6h\u00b7ne", "schier", "zu", "alt", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "PTKA", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "La\u00df unbesorgt den welken Reiz vermodern", "tokens": ["La\u00df", "un\u00b7be\u00b7sorgt", "den", "wel\u00b7ken", "Reiz", "ver\u00b7mo\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und um den Tod der Knechtschaft trag' kein Leid,", "tokens": ["Und", "um", "den", "Tod", "der", "Knecht\u00b7schaft", "trag'", "kein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u201eden Geistern gieb die S\u00fchne, die sie fodern.", "tokens": ["\u201e", "den", "Geis\u00b7tern", "gieb", "die", "S\u00fch\u00b7ne", ",", "die", "sie", "fo\u00b7dern", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVIMP", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df endlich das gelobte Land uns erben!", "tokens": ["La\u00df", "end\u00b7lich", "das", "ge\u00b7lob\u00b7te", "Land", "uns", "er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Freiheit Oriflamme, la\u00df sie lodern!", "tokens": ["Der", "Frei\u00b7heit", "O\u00b7ri\u00b7flam\u00b7me", ",", "la\u00df", "sie", "lo\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "\u201ela\u00df all den Spuck beim Hahnenruf ersterben,", "tokens": ["\u201e", "la\u00df", "all", "den", "Spuck", "beim", "Hah\u00b7nen\u00b7ruf", "ers\u00b7ter\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PIAT", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Getrosten Muths: Gev\u00f6gel nur der Nacht", "tokens": ["Ge\u00b7tros\u00b7ten", "Muths", ":", "Ge\u00b7v\u00f6\u00b7gel", "nur", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird elend an dem neuen Licht verderben,", "tokens": ["Wird", "e\u00b7lend", "an", "dem", "neu\u00b7en", "Licht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "\u201edem Lichte, das den V\u00f6lkern Heil gebracht!", "tokens": ["\u201e", "dem", "Lich\u00b7te", ",", "das", "den", "V\u00f6l\u00b7kern", "Heil", "ge\u00b7bracht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O sprich ein Wort, das ihre Angst vermindert!", "tokens": ["O", "sprich", "ein", "Wort", ",", "das", "ih\u00b7re", "Angst", "ver\u00b7min\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O sprich ein Wort, vor dem der Schlaf erwacht!", "tokens": ["O", "sprich", "ein", "Wort", ",", "vor", "dem", "der", "Schlaf", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "\u201egieb ein Gesetz, das heilet, nicht nur lindert:", "tokens": ["\u201e", "gieb", "ein", "Ge\u00b7setz", ",", "das", "hei\u00b7let", ",", "nicht", "nur", "lin\u00b7dert", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja gieb ein wahrhaft k\u00f6niglich Gesetz,", "tokens": ["Ja", "gieb", "ein", "wahr\u00b7haft", "k\u00f6\u00b7nig\u00b7lich", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVIMP", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das uns am Fallen, nicht am Gehn verhindert!", "tokens": ["Das", "uns", "am", "Fal\u00b7len", ",", "nicht", "am", "Gehn", "ver\u00b7hin\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "\u201eso sei ein F\u00fcrst! so wag' es und verletz'", "tokens": ["\u201e", "so", "sei", "ein", "F\u00fcrst", "!", "so", "wag'", "es", "und", "ver\u00b7letz'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den alten heil'gen hergebrachten Plunder:", "tokens": ["Den", "al\u00b7ten", "heil'\u00b7gen", "her\u00b7ge\u00b7brach\u00b7ten", "Plun\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zertritt das Pfaffen- und das Adelsnetz!", "tokens": ["Zer\u00b7tritt", "das", "Pfaf\u00b7fen", "und", "das", "A\u00b7dels\u00b7netz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "TRUNC", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "\u201ewirf in die harrende Welt hinaus den Zunder,", "tokens": ["\u201e", "wirf", "in", "die", "har\u00b7ren\u00b7de", "Welt", "hin\u00b7aus", "den", "Zun\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "APPR", "ART", "ADJA", "NN", "APZR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und spreng' den morschen Bau hoch in die Luft!", "tokens": ["Und", "spreng'", "den", "mor\u00b7schen", "Bau", "hoch", "in", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bist Du von Gott, wohlan, so thue Wunder!", "tokens": ["Bist", "Du", "von", "Gott", ",", "wo\u00b7hlan", ",", "so", "thue", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,", "PWAV", "$,", "ADV", "VVFIN", "NN", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "\u201edie Todten nur la\u00df in der Todtengruft:", "tokens": ["\u201e", "die", "Tod\u00b7ten", "nur", "la\u00df", "in", "der", "Tod\u00b7ten\u00b7gruft", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VVIMP", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es ist zu fr\u00fch, wenn man am j\u00fcngsten Tage", "tokens": ["Es", "ist", "zu", "fr\u00fch", ",", "wenn", "man", "am", "j\u00fcng\u00b7sten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "KOUS", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All diesem Volk zur Auferstehung ruft.\u201c", "tokens": ["All", "die\u00b7sem", "Volk", "zur", "Auf\u00b7er\u00b7ste\u00b7hung", "ruft", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Nicht ganz so lautet' es, wie jetzt ich sage,", "tokens": ["Nicht", "ganz", "so", "lau\u00b7tet'", "es", ",", "wie", "jetzt", "ich", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV