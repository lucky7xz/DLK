{"textgrid.poem.60998": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Im April", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du feuchter Fr\u00fchlingsabend,", "tokens": ["Du", "feuch\u00b7ter", "Fr\u00fch\u00b7lings\u00b7a\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie hab' ich dich so gern!", "tokens": ["Wie", "hab'", "ich", "dich", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel wolkenverhangen,", "tokens": ["Der", "Him\u00b7mel", "wol\u00b7ken\u00b7ver\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur hie und da ein Stern.", "tokens": ["Nur", "hie", "und", "da", "ein", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie leiser Liebesodem", "tokens": ["Wie", "lei\u00b7ser", "Lie\u00b7be\u00b7so\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hauchet so lau die Luft,", "tokens": ["Hau\u00b7chet", "so", "lau", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es steiget aus allen Talen", "tokens": ["Es", "stei\u00b7get",