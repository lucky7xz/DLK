{"dta.poem.2857": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Uebersetzung Kunst, die h\u00f6chste, dahin geht,", "tokens": ["Der", "Ue\u00b7ber\u00b7set\u00b7zung", "Kunst", ",", "die", "h\u00f6chs\u00b7te", ",", "da\u00b7hin", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu \u00fcbersetzen recht, was man nicht recht versteht.", "tokens": ["Zu", "\u00fc\u00b7bers\u00b7et\u00b7zen", "recht", ",", "was", "man", "nicht", "recht", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,", "PRELS", "PIS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit allem Lernen ist es ebenso bestellt;", "tokens": ["Mit", "al\u00b7lem", "Ler\u00b7nen", "ist", "es", "e\u00b7ben\u00b7so", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn was man ganz versteht, ist wenig auf der Welt.", "tokens": ["Denn", "was", "man", "ganz", "ver\u00b7steht", ",", "ist", "we\u00b7nig", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum lerne zeitig nur zu lernen, wo du gehst,", "tokens": ["Drum", "ler\u00b7ne", "zei\u00b7tig", "nur", "zu", "ler\u00b7nen", ",", "wo", "du", "gehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch manches was du halb und auch nicht halb verstehst.", "tokens": ["Auch", "man\u00b7ches", "was", "du", "halb", "und", "auch", "nicht", "halb", "ver\u00b7stehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PWS", "PPER", "ADJD", "KON", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}