{"dta.poem.6060": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem der Sonnen-Strahl die Erde nun erhellt,", "tokens": ["Nach\u00b7dem", "der", "Son\u00b7nen\u00b7Strahl", "die", "Er\u00b7de", "nun", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mi\u00df\u2019t der Astronomus die Zeit mit grossem Rechte.", "tokens": ["Mi\u00df't", "der", "Ast\u00b7ro\u00b7no\u00b7mus", "die", "Zeit", "mit", "gros\u00b7sem", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Es zeigt dies grosse Licht im Schoo\u00df der grossen Welt", "tokens": ["Es", "zeigt", "dies", "gros\u00b7se", "Licht", "im", "Schoo\u00df", "der", "gros\u00b7sen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Jahre, Monden, Tag und N\u00e4chte.", "tokens": ["Die", "Jah\u00b7re", ",", "Mon\u00b7den", ",", "Tag", "und", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}