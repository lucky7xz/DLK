{"dta.poem.19555": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Blutstropfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Zeit der Lese war's im Winzerhaus", "tokens": ["Zur", "Zeit", "der", "Le\u00b7se", "wa\u00b7r's", "im", "Win\u00b7zer\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Herdes goldne Flamme prasselte,", "tokens": ["Des", "Her\u00b7des", "gold\u00b7ne", "Flam\u00b7me", "pras\u00b7sel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die Fensterscheiben \u00fcberhauchten sich", "tokens": ["Die", "Fens\u00b7ter\u00b7schei\u00b7ben", "\u00fc\u00b7ber\u00b7hauch\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und drau\u00dfen scholl das Evo\u00eb geisterhaft", "tokens": ["Und", "drau\u00b7\u00dfen", "scholl", "das", "E\u00b7vo\u00eb", "geis\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Nebeld\u00e4mmer. Becher klangen. Jung", "tokens": ["Aus", "Ne\u00b7bel\u00b7d\u00e4m\u00b7mer", ".", "Be\u00b7cher", "klan\u00b7gen", ".", "Jung"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$.", "NN", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Alt empfand die bacchische Gewalt.", "tokens": ["Und", "Alt", "emp\u00b7fand", "die", "bac\u00b7chi\u00b7sche", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit einem zarten Schimmer r\u00f6teten", "tokens": ["Mit", "ei\u00b7nem", "zar\u00b7ten", "Schim\u00b7mer", "r\u00f6\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Selbst ihr die Wangen sich, die unser Gast", "tokens": ["Selbst", "ihr", "die", "Wan\u00b7gen", "sich", ",", "die", "un\u00b7ser", "Gast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ART", "NN", "PRF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und dieser Erde Gast nicht lange war,", "tokens": ["Und", "die\u00b7ser", "Er\u00b7de", "Gast", "nicht", "lan\u00b7ge", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein stilles, scheues, ungez\u00e4hmtes Kind.", "tokens": ["Ein", "stil\u00b7les", ",", "scheu\u00b7es", ",", "un\u00b7ge\u00b7z\u00e4hm\u00b7tes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zum Reigen rief Lyaeus. Jene schlich", "tokens": ["Zum", "Rei\u00b7gen", "rief", "Ly\u00b7ae\u00b7us", ".", "Je\u00b7ne", "schlich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$.", "PDAT", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sich weg. Ins Freie blickte sie hinaus", "tokens": ["Sich", "weg", ".", "Ins", "Frei\u00b7e", "blick\u00b7te", "sie", "hin\u00b7aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Durchs Fenster. Dann beschrieb sie tr\u00e4umerisch,", "tokens": ["Durchs", "Fens\u00b7ter", ".", "Dann", "be\u00b7schrieb", "sie", "tr\u00e4u\u00b7me\u00b7risch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die ganz sich unbeachtet W\u00e4hnende,", "tokens": ["Die", "ganz", "sich", "un\u00b7be\u00b7ach\u00b7tet", "W\u00e4h\u00b7nen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Die Scheibe mit dem Finger. Weh! umstellt,", "tokens": ["Die", "Schei\u00b7be", "mit", "dem", "Fin\u00b7ger", ".", "Weh", "!", "um\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Belauert wurde sie von einem Schwarm", "tokens": ["Be\u00b7lau\u00b7ert", "wur\u00b7de", "sie", "von", "ei\u00b7nem", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und \u00fcberfallen. Rasch in Tr\u00fcmmer schlug,", "tokens": ["Und", "\u00fc\u00b7berf\u00b7al\u00b7len", ".", "Rasch", "in", "Tr\u00fcm\u00b7mer", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Das Antlitz glutbedeckt, die Scheibe sie,", "tokens": ["Das", "Ant\u00b7litz", "glut\u00b7be\u00b7deckt", ",", "die", "Schei\u00b7be", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sich selbst verwundend. Dieses T\u00fcchlein hier,", "tokens": ["Sich", "selbst", "ver\u00b7wun\u00b7dend", ".", "Die\u00b7ses", "T\u00fcch\u00b7lein", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$.", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Das als Reliquie mir im Schreine liegt,", "tokens": ["Das", "als", "Re\u00b7li\u00b7qui\u00b7e", "mir", "im", "Schrei\u00b7ne", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Fing, \u00fcber die verletzte Hand gelegt,", "tokens": ["Fing", ",", "\u00fc\u00b7ber", "die", "ver\u00b7letz\u00b7te", "Hand", "ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das Quellen eines Tropfen Blutes auf,", "tokens": ["Das", "Quel\u00b7len", "ei\u00b7nes", "Trop\u00b7fen", "Blu\u00b7tes", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der warm ihr eben erst im Herzen rann.", "tokens": ["Der", "warm", "ihr", "e\u00b7ben", "erst", "im", "Her\u00b7zen", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jung schwand sie hin, und kein Lebend'ger wei\u00df,", "tokens": ["Jung", "schwand", "sie", "hin", ",", "und", "kein", "Le\u00b7ben\u00b7d'\u00b7ger", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was dort geschrieben auf der Scheibe stand \u2014", "tokens": ["Was", "dort", "ge\u00b7schrie\u00b7ben", "auf", "der", "Schei\u00b7be", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als dieser bleiche Tropfen Bluts vielleicht.", "tokens": ["Als", "die\u00b7ser", "blei\u00b7che", "Trop\u00b7fen", "Bluts", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}