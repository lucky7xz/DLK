{"dta.poem.10718": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Beschlu\u00df Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Das blinde liebes werck/ die f\u00fcsse Gifft der Sinnen/", "tokens": ["Das", "blin\u00b7de", "lie\u00b7bes", "werck", "/", "die", "f\u00fcs\u00b7se", "Gifft", "der", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd rechte Zauberey hat letzlich hier ein end/", "tokens": ["Vnd", "rech\u00b7te", "Zau\u00b7be\u00b7rey", "hat", "letz\u00b7lich", "hier", "ein", "end", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Kindt/ das lose Kind/ so mich verf\u00fchren k\u00fcnnen/", "tokens": ["Das", "Kindt", "/", "das", "lo\u00b7se", "Kind", "/", "so", "mich", "ver\u00b7f\u00fch\u00b7ren", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ADV", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird entlich gantz vnd gar von mir hinweg gewendt/", "tokens": ["Wird", "ent\u00b7lich", "gantz", "vnd", "gar", "von", "mir", "hin\u00b7weg", "ge\u00b7wendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "KON", "ADV", "APPR", "PPER", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun suche wo du wilt dir andere P\u00f6eten/", "tokens": ["Nun", "su\u00b7che", "wo", "du", "wilt", "dir", "an\u00b7de\u00b7re", "P\u00f6e\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWAV", "PPER", "VMFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hier Venus hab ich mir gesteckt mein eigen Ziel/", "tokens": ["Hier", "Ve\u00b7nus", "hab", "ich", "mir", "ge\u00b7steckt", "mein", "ei\u00b7gen", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PPER", "VVPP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist auch deine gunst jetzt weiter nit von n\u00f6ten/", "tokens": ["Es", "ist", "auch", "dei\u00b7ne", "gunst", "jetzt", "wei\u00b7ter", "nit", "von", "n\u00f6\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "PTKNEG", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "All eitelkeit ich ha\u00df/ es liebe wer da will.", "tokens": ["All", "ei\u00b7tel\u00b7keit", "ich", "ha\u00df", "/", "es", "lie\u00b7be", "wer", "da", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was meine schwache Hand von dieser Zeit geschrieben/", "tokens": ["Was", "mei\u00b7ne", "schwa\u00b7che", "Hand", "von", "die\u00b7ser", "Zeit", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch deinen Geist gef\u00fcrt/ das ist der Jugent schult/", "tokens": ["Durch", "dei\u00b7nen", "Geist", "ge\u00b7f\u00fcrt", "/", "das", "ist", "der", "Ju\u00b7gent", "schult", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$(", "PDS", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich werde weiter nit von solcher lust getrieben/", "tokens": ["Ich", "wer\u00b7de", "wei\u00b7ter", "nit", "von", "sol\u00b7cher", "lust", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was dir geh\u00e4ssig ist/ demselben trag ich huld/", "tokens": ["Was", "dir", "ge\u00b7h\u00e4s\u00b7sig", "ist", "/", "dem\u00b7sel\u00b7ben", "trag", "ich", "huld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$(", "PDS", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wann vrthel vnd verstand bei mir zu rahte sitzen/", "tokens": ["Wann", "vr\u00b7thel", "vnd", "ver\u00b7stand", "bei", "mir", "zu", "rah\u00b7te", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hast du zuvor regirt in meinem jungen sinn/", "tokens": ["Hast", "du", "zu\u00b7vor", "re\u00b7girt", "in", "mei\u00b7nem", "jun\u00b7gen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jetzt seh\u2019 ich da\u00df dein Sohn sey ohne wohn vnd witzen/", "tokens": ["Jetzt", "seh'", "ich", "da\u00df", "dein", "Sohn", "sey", "oh\u00b7ne", "wohn", "vnd", "wit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du aber selber bist ein Edle Kuplerin.", "tokens": ["Du", "a\u00b7ber", "sel\u00b7ber", "bist", "ein", "Ed\u00b7le", "Kup\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die gantze G\u00f6tterzunfft/ was jemahls hat gefallen", "tokens": ["Die", "gant\u00b7ze", "G\u00f6t\u00b7ter\u00b7zunfft", "/", "was", "je\u00b7mahls", "hat", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PWS", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der blinden Heidenschafft/ halt ich vor hohn vnd schmach/", "tokens": ["Der", "blin\u00b7den", "Hei\u00b7den\u00b7schafft", "/", "halt", "ich", "vor", "hohn", "vnd", "schmach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich schende dich vnd Sie: der Oberst vnder allen", "tokens": ["Ich", "schen\u00b7de", "dich", "vnd", "Sie", ":", "der", "O\u00b7berst", "vn\u00b7der", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "$.", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Lest Plitz vnd Hagel stehn/ vnd kreucht den Huren nach.", "tokens": ["Lest", "Plitz", "vnd", "Ha\u00b7gel", "stehn", "/", "vnd", "kreucht", "den", "Hu\u00b7ren", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "VVINF", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Bacchuswird nit ehe/ al\u00df truncken/ vbertretten/", "tokens": ["Der", "Bac\u00b7chus\u00b7wird", "nit", "e\u00b7he", "/", "al\u00df", "trun\u00b7cken", "/", "vbert\u00b7ret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "KOUS", "$(", "KOUS", "ADJD", "$(", "VVFIN", "$("], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Dich hat der starcke Mars/ Aleides Hylam lieb/", "tokens": ["Dich", "hat", "der", "star\u00b7cke", "Mars", "/", "A\u00b7lei\u00b7des", "Hy\u00b7lam", "lieb", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "NE", "NE", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Priapus komt gar fein vnd z\u00fcchtig vffgetretten/", "tokens": ["Pria\u00b7pus", "komt", "gar", "fein", "vnd", "z\u00fcch\u00b7tig", "vff\u00b7ge\u00b7tret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Neptunus ist ein Schelm/ Mercurius ein Dieb.", "tokens": ["Nep\u00b7tu\u00b7nus", "ist", "ein", "Schelm", "/", "Mer\u00b7cu\u00b7rius", "ein", "Dieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$(", "NE", "ART", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Dein wesen ist ein Marck/ da leyd wird feil getragen/", "tokens": ["Dein", "we\u00b7sen", "ist", "ein", "Marck", "/", "da", "leyd", "wird", "feil", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "ADV", "ADJD", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein Winckel da verdru\u00df vnd wehmuth innen steht/", "tokens": ["Ein", "Win\u00b7ckel", "da", "ver\u00b7dru\u00df", "vnd", "weh\u00b7muth", "in\u00b7nen", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "KON", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein Herberg aller noth/ ein Hau\u00df voll schmertz vnd klagen.", "tokens": ["Ein", "Her\u00b7berg", "al\u00b7ler", "noth", "/", "ein", "Hau\u00df", "voll", "schmertz", "vnd", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "ART", "NN", "ADJD", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein Schiff der pein/ ein meer/ da Tugent vndergeht.", "tokens": ["Ein", "Schiff", "der", "pein", "/", "ein", "meer", "/", "da", "Tu\u00b7gent", "vn\u00b7der\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "$(", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wo soll die sch\u00f6nheit sein/ wan\u0303 alles wird vergehen/", "tokens": ["Wo", "soll", "die", "sch\u00f6n\u00b7heit", "sein", "/", "wa\u00f1", "al\u00b7les", "wird", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJD", "VAINF", "$(", "PWAV", "PIS", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Lippen von Corall/ di\u00df Alabaster Bilt/", "tokens": ["Die", "Lip\u00b7pen", "von", "Co\u00b7rall", "/", "di\u00df", "A\u00b7la\u00b7bas\u00b7ter", "Bilt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$(", "PDS", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Augen/ so jhr secht/ gleich al\u00df zwo Sonnen/ stehen/", "tokens": ["Die", "Au\u00b7gen", "/", "so", "jhr", "secht", "/", "gleich", "al\u00df", "zwo", "Son\u00b7nen", "/", "ste\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPOSAT", "NN", "$(", "ADV", "KOKOM", "CARD", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der rote Rosenmundt/ der weissen Br\u00fcste schilt?", "tokens": ["Der", "ro\u00b7te", "Ro\u00b7sen\u00b7mundt", "/", "der", "weis\u00b7sen", "Br\u00fcs\u00b7te", "schilt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie sollen/ wie ich h\u00f6r/ al\u00df Asch vnd Staub entfliehen/", "tokens": ["Sie", "sol\u00b7len", "/", "wie", "ich", "h\u00f6r", "/", "al\u00df", "Asch", "vnd", "Staub", "ent\u00b7flie\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "KOUS", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vnd gehen allzugleich den weg der Eitelkeit/", "tokens": ["Vnd", "ge\u00b7hen", "all\u00b7zu\u00b7gleich", "den", "weg", "der", "Ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Pracht/ Hoffart/ Gut vnd Gelt/ warumb wir vns bem\u00fchen/", "tokens": ["Pracht", "/", "Hof\u00b7fart", "/", "Gut", "vnd", "Gelt", "/", "wa\u00b7rumb", "wir", "vns", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ADJD", "KON", "NN", "$(", "PWAV", "PPER", "PRF", "VVINF", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.36": {"text": "Wird Wind vnd fl\u00fcgel noch bekommen mit der Zeit.", "tokens": ["Wird", "Wind", "vnd", "fl\u00fc\u00b7gel", "noch", "be\u00b7kom\u00b7men", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich la\u00df es alles stehen: das ende meiner Jugent/", "tokens": ["Ich", "la\u00df", "es", "al\u00b7les", "ste\u00b7hen", ":", "das", "en\u00b7de", "mei\u00b7ner", "Ju\u00b7gent", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$.", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Die frucht der liebes lust beschlie\u00df ich gantz hierein/", "tokens": ["Die", "frucht", "der", "lie\u00b7bes", "lust", "be\u00b7schlie\u00df", "ich", "gantz", "hier\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein ander h\u00f6her Werck/ der anfang meiner Tugent.", "tokens": ["Ein", "an\u00b7der", "h\u00f6\u00b7her", "Werck", "/", "der", "an\u00b7fang", "mei\u00b7ner", "Tu\u00b7gent", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wo dieses vndergeht/ soll nimmer sterblich sein.", "tokens": ["Wo", "die\u00b7ses", "vn\u00b7der\u00b7geht", "/", "soll", "nim\u00b7mer", "sterb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$(", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}