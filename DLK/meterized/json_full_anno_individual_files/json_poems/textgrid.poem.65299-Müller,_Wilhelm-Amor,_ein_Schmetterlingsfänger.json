{"textgrid.poem.65299": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Schmetterlingsf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich fange Schmetterlinge", "tokens": ["Ich", "fan\u00b7ge", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu meinem Zeitvertreibe.", "tokens": ["Zu", "mei\u00b7nem", "Zeit\u00b7ver\u00b7trei\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo aber soll ich alle", "tokens": ["Wo", "a\u00b7ber", "soll", "ich", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bunten Thierchen lassen?", "tokens": ["Die", "bun\u00b7ten", "Thier\u00b7chen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich werfe gleich die Pfeile", "tokens": ["Ich", "wer\u00b7fe", "gleich", "die", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heraus aus meinem K\u00f6cher,", "tokens": ["He\u00b7raus", "aus", "mei\u00b7nem", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und lasse sie indessen", "tokens": ["Und", "las\u00b7se", "sie", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im hohen Grase liegen.", "tokens": ["Im", "ho\u00b7hen", "Gra\u00b7se", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und wenn die Schnitterinnen", "tokens": ["Und", "wenn", "die", "Schnit\u00b7te\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mit blo\u00dfen F\u00fc\u00dfen kommen", "tokens": ["Mit", "blo\u00b7\u00dfen", "F\u00fc\u00b7\u00dfen", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Heut' Abend von der Wiese,", "tokens": ["Heut'", "A\u00b7bend", "von", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So sollen sie sich ritzen;", "tokens": ["So", "sol\u00b7len", "sie", "sich", "rit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Denn meine Pfeile d\u00fcrfen", "tokens": ["Denn", "mei\u00b7ne", "Pfei\u00b7le", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Mir nimmer m\u00fc\u00dfig liegen.", "tokens": ["Mir", "nim\u00b7mer", "m\u00fc\u00b7\u00dfig", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-