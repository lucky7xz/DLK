{"textgrid.poem.63723": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pinsel, Griffel und Mei\u00dfel und was irgend", "tokens": ["Pin\u00b7sel", ",", "Grif\u00b7fel", "und", "Mei\u00b7\u00dfel", "und", "was", "ir\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "PWS", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Macht hat, schwankende Formen festzubannen,", "tokens": ["Macht", "hat", ",", "schwan\u00b7ken\u00b7de", "For\u00b7men", "fest\u00b7zu\u00b7ban\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Euch beneidet der Kiel des armen Dichters.", "tokens": ["Euch", "be\u00b7nei\u00b7det", "der", "Kiel", "des", "ar\u00b7men", "Dich\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Denn er m\u00fcht sich vergebens, nachzukritzeln,", "tokens": ["Denn", "er", "m\u00fcht", "sich", "ver\u00b7ge\u00b7bens", ",", "nach\u00b7zu\u00b7krit\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADV", "$,", "VVIZU", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Was soeben geschaut die sel'gen Augen.", "tokens": ["Was", "soe\u00b7ben", "ge\u00b7schaut", "die", "sel'\u00b7gen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wei\u00df denn einer, wie reizend keck das Dirnchen", "tokens": ["Wei\u00df", "denn", "ei\u00b7ner", ",", "wie", "rei\u00b7zend", "keck", "das", "Dirn\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "$,", "PWAV", "ADJD", "ADJD", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Auf dem Eselchen thronte, wenn ich melde,", "tokens": ["Auf", "dem", "E\u00b7sel\u00b7chen", "thron\u00b7te", ",", "wenn", "ich", "mel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie zwischen den K\u00f6rben sa\u00df, das eine", "tokens": ["Da\u00df", "sie", "zwi\u00b7schen", "den", "K\u00f6r\u00b7ben", "sa\u00df", ",", "das", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Veinchen \u00fcber des Tiers geduld'gen R\u00fccken,", "tokens": ["Vein\u00b7chen", "\u00fc\u00b7ber", "des", "Tiers", "ge\u00b7duld'\u00b7gen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Frei das andere baumelnd, da\u00df ihr rotes", "tokens": ["Frei", "das", "an\u00b7de\u00b7re", "bau\u00b7melnd", ",", "da\u00df", "ihr", "ro\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "VVPP", "$,", "KOUS", "PPOSAT", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "R\u00f6cklein \u00fcber die Wade sich hinaufzog?", "tokens": ["R\u00f6ck\u00b7lein", "\u00fc\u00b7ber", "die", "Wa\u00b7de", "sich", "hin\u00b7auf\u00b7zog", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und so sa\u00df sie mit vorgeneigten Schultern,", "tokens": ["Und", "so", "sa\u00df", "sie", "mit", "vor\u00b7ge\u00b7neig\u00b7ten", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "In die Rechte geschmiegt das Kinn, am kleinen", "tokens": ["In", "die", "Rech\u00b7te", "ge\u00b7schmiegt", "das", "Kinn", ",", "am", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "ART", "NN", "$,", "APPRART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Finger saugend, vertr\u00e4umt und aus der Wimpern", "tokens": ["Fin\u00b7ger", "sau\u00b7gend", ",", "ver\u00b7tr\u00e4umt", "und", "aus", "der", "Wim\u00b7pern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "VVPP", "KON", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Schwarzer Seidengardine Blitze spr\u00fchend;", "tokens": ["Schwar\u00b7zer", "Sei\u00b7den\u00b7gar\u00b7di\u00b7ne", "Blit\u00b7ze", "spr\u00fc\u00b7hend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$."], "met