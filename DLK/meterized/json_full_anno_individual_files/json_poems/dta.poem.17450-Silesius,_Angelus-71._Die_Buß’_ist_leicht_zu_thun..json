{"dta.poem.17450": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "71.  Die Bu\u00df\u2019 ist leicht zu thun.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Bu\u00df\u2019 ist hald gethan/ da\u00df dich GOtt lo\u00df mu\u00df sagen/", "tokens": ["Die", "Bu\u00df'", "ist", "hald", "ge\u00b7than", "/", "da\u00df", "dich", "Gott", "lo\u00df", "mu\u00df", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "KOUS", "PPER", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du darffst nur an die Brust wie jener S\u00fcnder schlagen.", "tokens": ["Du", "darffst", "nur", "an", "die", "Brust", "wie", "je\u00b7ner", "S\u00fcn\u00b7der", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "KOKOM", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}