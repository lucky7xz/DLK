{"textgrid.poem.37421": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "So war's", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Teetopf war so wundersch\u00f6n,", "tokens": ["Der", "Tee\u00b7topf", "war", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liebt ihn, wie ihr Leben.", "tokens": ["Sie", "liebt", "ihn", ",", "wie", "ihr", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat ihm leider aus Versehn", "tokens": ["Sie", "hat", "ihm", "lei\u00b7der", "aus", "Ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Todessto\u00df gegeben.", "tokens": ["Den", "To\u00b7des\u00b7sto\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was sie f\u00fcr Kummer da empfand,", "tokens": ["Was", "sie", "f\u00fcr", "Kum\u00b7mer", "da", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie wird sie es vergessen.", "tokens": ["Nie", "wird", "sie", "es", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hielt die Scherben aneinand", "tokens": ["Sie", "hielt", "die", "Scher\u00b7ben", "an\u00b7ei\u00b7nand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: So hat's gesessen!", "tokens": ["Und", "sprach", ":", "So", "hat's", "ge\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Teetopf war so wundersch\u00f6n,", "tokens": ["Der", "Tee\u00b7topf", "war", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liebt ihn, wie ihr Leben.", "tokens": ["Sie", "liebt", "ihn", ",", "wie", "ihr", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat ihm leider aus Versehn", "tokens": ["Sie", "hat", "ihm", "lei\u00b7der", "aus", "Ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Todessto\u00df gegeben.", "tokens": ["Den", "To\u00b7des\u00b7sto\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was sie f\u00fcr Kummer da empfand,", "tokens": ["Was", "sie", "f\u00fcr", "Ku