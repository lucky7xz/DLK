{"dta.poem.11697": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Pastor an seine Zuh\u00f6rer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Gefall' ich euch nicht, ei so bleibt doch zu Haus,", "tokens": ["Ge\u00b7fall'", "ich", "euch", "nicht", ",", "ei", "so", "bleibt", "doch", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "$,", "ITJ", "ADV", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Oder geht zu einem Andern!", "tokens": ["O\u00b7der", "geht", "zu", "ei\u00b7nem", "An\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der zieht euch die Z\u00e4hn' mit dem Stiefelknecht aus;", "tokens": ["Der", "zieht", "euch", "die", "Z\u00e4hn'", "mit", "dem", "Stie\u00b7fel\u00b7knecht", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir sind noch von den Galantern.", "tokens": ["Wir", "sind", "noch", "von", "den", "Ga\u00b7lan\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}