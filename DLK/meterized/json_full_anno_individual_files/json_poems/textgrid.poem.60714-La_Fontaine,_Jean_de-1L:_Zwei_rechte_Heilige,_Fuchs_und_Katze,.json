{"textgrid.poem.60714": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei rechte Heilige, Fuchs und Katze,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei rechte Heilige, Fuchs und Katze,", "tokens": ["Zwei", "rech\u00b7te", "Hei\u00b7li\u00b7ge", ",", "Fuchs", "und", "Kat\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "ADJA", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Begaben sich auf Pilgerschaft:", "tokens": ["Be\u00b7ga\u00b7ben", "sich", "auf", "Pil\u00b7ger\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwei wahre Schelme, die trotz ihrer Sammettatze", "tokens": ["Zwei", "wah\u00b7re", "Schel\u00b7me", ",", "die", "trotz", "ih\u00b7rer", "Sam\u00b7met\u00b7tat\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Reisezehrung massenhaft", "tokens": ["Als", "Rei\u00b7se\u00b7zeh\u00b7rung", "mas\u00b7sen\u00b7haft"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das k\u00f6stlichste Gefl\u00fcgel knackten", "tokens": ["Das", "k\u00f6st\u00b7lichs\u00b7te", "Ge\u00b7fl\u00fc\u00b7gel", "knack\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und manchen fremden K\u00e4se packten.", "tokens": ["Und", "man\u00b7chen", "frem\u00b7den", "K\u00e4\u00b7se", "pack\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man schlug sich durch, so gut es ging.", "tokens": ["Man", "schlug", "sich", "durch", ",", "so", "gut", "es", "ging", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Langweilig war der Weg; man fing", "tokens": ["Lang\u00b7wei\u00b7lig", "war", "der", "Weg", ";", "man", "fing"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zur Kurzweil an zu disputieren;", "tokens": ["Zur", "Kurz\u00b7weil", "an", "zu", "dis\u00b7pu\u00b7tie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Denn ohne solches ist Spazieren", "tokens": ["Denn", "oh\u00b7ne", "sol\u00b7ches", "ist", "Spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bisweilen ein erm\u00fcdend Ding.", "tokens": ["Bis\u00b7wei\u00b7len", "ein", "er\u00b7m\u00fc\u00b7dend", "Ding", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ihr Eifer schob sie gut vom Platze.", "tokens": ["Ihr", "Ei\u00b7fer", "schob", "sie", "gut", "vom", "Plat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kaum war die eine Sache t\u00fcchtig durchgestritten,", "tokens": ["Kaum", "war", "die", "ei\u00b7ne", "Sa\u00b7che", "t\u00fcch\u00b7tig", "durch\u00b7ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da wurde schon ein neues Thema angeschnitten.", "tokens": ["Da", "wur\u00b7de", "schon", "ein", "neu\u00b7es", "The\u00b7ma", "an\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und schlie\u00dflich sprach der Fuchs zur Katze:", "tokens": ["Und", "schlie\u00df\u00b7lich", "sprach", "der", "Fuchs", "zur", "Kat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bbf\u00fcr sehr bewandert h\u00e4ltst du dich \u2013", "tokens": ["\u00bb", "f\u00fcr", "sehr", "be\u00b7wan\u00b7dert", "h\u00e4ltst", "du", "dich", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADJD", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch bist du so geschickt wie ich?", "tokens": ["Doch", "bist", "du", "so", "ge\u00b7schickt", "wie", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich trage hundert Listen hier in meinem Sack.\u00ab", "tokens": ["Ich", "tra\u00b7ge", "hun\u00b7dert", "Lis\u00b7ten", "hier", "in", "mei\u00b7nem", "Sack", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "\u00bbnein,\u00ab sprach die Katze drauf, \u00bbin meinem Beutel ist", "tokens": ["\u00bb", "nein", ",", "\u00ab", "sprach", "die", "Kat\u00b7ze", "drauf", ",", "\u00bb", "in", "mei\u00b7nem", "Beu\u00b7tel", "ist"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$,", "$(", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur eine einzige List;", "tokens": ["Nur", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", "List", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Doch steh ich daf\u00fcr ein, sie wiegt ein ganzes Pack", "tokens": ["Doch", "steh", "ich", "da\u00b7f\u00fcr", "ein", ",", "sie", "wiegt", "ein", "gan\u00b7zes", "Pack"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von tausend Listen auf.\u00ab", "tokens": ["Von", "tau\u00b7send", "Lis\u00b7ten", "auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schon ist der neuste Streit in Lauf:", "tokens": ["Schon", "ist", "der", "neus\u00b7te", "Streit", "in", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob ja, ob nein, wird hin und her erwogen.", "tokens": ["Ob", "ja", ",", "ob", "nein", ",", "wird", "hin", "und", "her", "er\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PTKANT", "$,", "VAFIN", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da pl\u00f6tzlich st\u00f6rt sie eine Meute.", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "st\u00f6rt", "sie", "ei\u00b7ne", "Meu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Katze spricht: \u00bbMein Freund, sofern du nicht gelogen,", "tokens": ["Die", "Kat\u00b7ze", "spricht", ":", "\u00bb", "Mein", "Freund", ",", "so\u00b7fern", "du", "nicht", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Durchsuche nun den Sack, damit du nicht zur Beute", "tokens": ["Durch\u00b7su\u00b7che", "nun", "den", "Sack", ",", "da\u00b7mit", "du", "nicht", "zur", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hunde wirst, nach deiner besten List.", "tokens": ["Der", "Hun\u00b7de", "wirst", ",", "nach", "dei\u00b7ner", "bes\u00b7ten", "List", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was mich betrifft, schau her, wie meine eine ist!\u00ab", "tokens": ["Was", "mich", "be\u00b7tr\u00b7ifft", ",", "schau", "her", ",", "wie", "mei\u00b7ne", "ei\u00b7ne", "ist", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "PTKVZ", "$,", "PWAV", "VVFIN", "PIS", "VAFIN", "$.", "$("], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Sie sprach's und klomm den n\u00e4chsten Baum empor.", "tokens": ["Sie", "sprach's", "und", "klomm", "den", "n\u00e4chs\u00b7ten", "Baum", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Fuchs erprobte hundert Finten,", "tokens": ["Der", "Fuchs", "er\u00b7prob\u00b7te", "hun\u00b7dert", "Fin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch hundert L\u00f6cher t\u00e4uschte er der Hunde Chor,", "tokens": ["Durch", "hun\u00b7dert", "L\u00f6\u00b7cher", "t\u00e4uschte", "er", "der", "Hun\u00b7de", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Er glaubt sich manchmal schon in Sicherheit, doch nein:", "tokens": ["Er", "glaubt", "sich", "manch\u00b7mal", "schon", "in", "Si\u00b7cher\u00b7heit", ",", "doch", "nein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPR", "NN", "$,", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Schon wieder sind die Hunde hinten.", "tokens": ["Schon", "wie\u00b7der", "sind", "die", "Hun\u00b7de", "hin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier treibt man ihn mit Rauch hervor,", "tokens": ["Hier", "treibt", "man", "ihn", "mit", "Rauch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort dringen D\u00e4chsel zu ihm ein;", "tokens": ["Dort", "drin\u00b7gen", "D\u00e4ch\u00b7sel", "zu", "ihm", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie er wieder flieht aus seiner H\u00f6hle Tor,", "tokens": ["Und", "wie", "er", "wie\u00b7der", "flieht", "aus", "sei\u00b7ner", "H\u00f6h\u00b7le", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "