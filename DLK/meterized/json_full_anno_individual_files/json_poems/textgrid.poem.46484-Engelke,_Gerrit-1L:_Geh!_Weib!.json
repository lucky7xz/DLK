 der Ferne:", "tokens": ["Kommt", "nur", ",", "ihr", "an\u00b7dern", "aus", "der", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du, wie zittern deine Augensterne;", "tokens": ["Du", ",", "wie", "zit\u00b7tern", "dei\u00b7ne", "Au\u00b7gens\u00b7ter\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du mit dem Mundrubin \u2013 hah! ich kenne euch nicht!", "tokens": ["Du", "mit", "dem", "Mun\u00b7dru\u00b7bin", "\u2013", "hah", "!", "ich", "ken\u00b7ne", "euch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$(", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Doch, Weiber ihr, sch\u00f6n und verflucht,", "tokens": ["Doch", ",", "Wei\u00b7ber", "ihr", ",", "sch\u00f6n", "und", "ver\u00b7flucht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wo ist die, die ich meines Lebens Ewigkeit gesucht?", "tokens": ["Wo", "ist", "die", ",", "die", "ich", "mei\u00b7nes", "Le\u00b7bens", "E\u00b7wig\u00b7keit", "ge\u00b7sucht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "$,", "PRELS", "PPER", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Wankt doch die Eine schon im Licht? \u2013", "tokens": ["Wankt", "doch", "die", "Ei\u00b7ne", "schon", "im", "Licht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "ART", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich hebe wieder mein verw\u00fcstet Herz", "tokens": ["Ich", "he\u00b7be", "wie\u00b7der", "mein", "ver\u00b7w\u00fcs\u00b7tet", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu neuer Sehnsucht, neuem Schmerz:", "tokens": ["Zu", "neu\u00b7er", "Sehn\u00b7sucht", ",", "neu\u00b7em", "Schmerz", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe selig den verkl\u00e4rten Leib", "tokens": ["Ich", "se\u00b7he", "se\u00b7lig", "den", "ver\u00b7kl\u00e4r\u00b7ten", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Einen, der sich meine Adern weiten,", "tokens": ["Der", "Ei\u00b7nen", ",", "der", "sich", "mei\u00b7ne", "A\u00b7dern", "wei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Strahlenweg hernieder gleiten \u2013", "tokens": ["Den", "Strah\u00b7len\u00b7weg", "her\u00b7nie\u00b7der", "glei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Komm! Du! \u2013 Komm, Weib!", "tokens": ["Komm", "!", "Du", "!", "\u2013", "Komm", ",", "Weib", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "$.", "$(", "VVFIN", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}