{"textgrid.poem.24275": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ein Traum", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war wundervoll: Ich tr\u00e4umte:", "tokens": ["Das", "war", "wun\u00b7der\u00b7voll", ":", "Ich", "tr\u00e4um\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es stand ein Haus dicht an der gr\u00fcnen See.", "tokens": ["Es", "stand", "ein", "Haus", "dicht", "an", "der", "gr\u00fc\u00b7nen", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihre Wellen, smaragden, rasten gegen seine Mauern:", "tokens": ["Ih\u00b7re", "Wel\u00b7len", ",", "sma\u00b7rag\u00b7den", ",", "ras\u00b7ten", "ge\u00b7gen", "sei\u00b7ne", "Mau\u00b7ern", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Tausende, tausende vorgereckte H\u00e4lse,", "tokens": ["Tau\u00b7sen\u00b7de", ",", "tau\u00b7sen\u00b7de", "vor\u00b7ge\u00b7reck\u00b7te", "H\u00e4l\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Gebogene Nacken, gepeitschte Schweife,", "tokens": ["Ge\u00b7bo\u00b7ge\u00b7ne", "Na\u00b7cken", ",", "ge\u00b7peitschte", "Schwei\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Eine w\u00fctende Meute heiseren Gebelles.", "tokens": ["Ei\u00b7ne", "w\u00fc\u00b7ten\u00b7de", "Meu\u00b7te", "hei\u00b7se\u00b7ren", "Ge\u00b7bel\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ich sa\u00df hinterm Fenster, das wie in einer Kirche hoch war,", "tokens": ["Ich", "sa\u00df", "hin\u00b7term", "Fens\u00b7ter", ",", "das", "wie", "in", "ei\u00b7ner", "Kir\u00b7che", "hoch", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "KOKOM", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "In einem Porphyrthrone, belegt mit gelben Kissen", "tokens": ["In", "ei\u00b7nem", "Por\u00b7phyr\u00b7th\u00b7ro\u00b7ne", ",", "be\u00b7legt", "mit", "gel\u00b7ben", "Kis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Aus rauhem chinesischen Brokat, vom Kaiser Kanghi", "tokens": ["Aus", "rau\u00b7hem", "chi\u00b7ne\u00b7si\u00b7schen", "Bro\u00b7kat", ",", "vom", "Kai\u00b7ser", "Kang\u00b7hi"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "APPRART", "NN", "NE"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Im Jahre sechzehnhundertundf\u00fcnfundneunzig mir verliehen", "tokens": ["Im", "Jah\u00b7re", "sech\u00b7zehn\u00b7hun\u00b7der\u00b7tund\u00b7f\u00fcn\u00b7fund\u00b7neun\u00b7zig", "mir", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "CARD", "PPER", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "F\u00fcr einen Hymnus auf den Sohn des Himmels im Stile", "tokens": ["F\u00fcr", "ei\u00b7nen", "Hym\u00b7nus", "auf", "den", "Sohn", "des", "Him\u00b7mels", "im", "Sti\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Der purpurblauen P\u00e4onie. Ich sah hinaus,", "tokens": ["Der", "pur\u00b7pur\u00b7blau\u00b7en", "P\u00e4o\u00b7nie", ".", "Ich", "sah", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Gekleidet wie ein Amsterdamer Ratsherr zur Zeit Rembrandts.", "tokens": ["Ge\u00b7klei\u00b7det", "wie", "ein", "A\u00b7mster\u00b7da\u00b7mer", "Rats\u00b7herr", "zur", "Zeit", "Rem\u00b7brandts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Dr\u00fcben, hinter dem bellenden gr\u00fcnen Meere,", "tokens": ["Dr\u00fc\u00b7ben", ",", "hin\u00b7ter", "dem", "bel\u00b7len\u00b7den", "gr\u00fc\u00b7nen", "Mee\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Aus lauter Lapislazuli, aber Silber in den Kl\u00fcften,", "tokens": ["Aus", "lau\u00b7ter", "La\u00b7pis\u00b7la\u00b7zu\u00b7li", ",", "a\u00b7ber", "Sil\u00b7ber", "in", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Hob sich Gebirg. Der Himmel war aus Gold,", "tokens": ["Hob", "sich", "Ge\u00b7birg", ".", "Der", "Him\u00b7mel", "war", "aus", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "$.", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.17": {"text": "Geh\u00e4mmertem, ein Hintergrund f\u00fcr Heilige.", "tokens": ["Ge\u00b7h\u00e4m\u00b7mer\u00b7tem", ",", "ein", "Hin\u00b7ter\u00b7grund", "f\u00fcr", "Hei\u00b7li\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und eine Insel lag im gr\u00fcnen Meer", "tokens": ["Und", "ei\u00b7ne", "In\u00b7sel", "lag", "im", "gr\u00fc\u00b7nen", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mit einem Tempel, \u2013 nein: mit einem Schlosse, \u2013 nein:", "tokens": ["Mit", "ei\u00b7nem", "Tem\u00b7pel", ",", "\u2013", "nein", ":", "mit", "ei\u00b7nem", "Schlos\u00b7se", ",", "\u2013", "nein", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "$(", "PTKANT", "$.", "APPR", "ART", "NN", "$,", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit einem Haus der Venus. Silbergrau,", "tokens": ["Mit", "ei\u00b7nem", "Haus", "der", "Ve\u00b7nus", ".", "Sil\u00b7ber\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Von Feuchte \u00fcberronnen, war das Haus.", "tokens": ["Von", "Feuch\u00b7te", "\u00fc\u00b7ber\u00b7ron\u00b7nen", ",", "war", "das", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die Ecke, die es mir entgegenkehrte,", "tokens": ["Die", "E\u00b7cke", ",", "die", "es", "mir", "ent\u00b7ge\u00b7gen\u00b7kehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "War sch\u00f6n behauen. Eine nackte Frau", "tokens": ["War", "sch\u00f6n", "be\u00b7hau\u00b7en", ".", "Ei\u00b7ne", "nack\u00b7te", "Frau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Bog sich, aus gelbem rosa\u00e4drigen Gestein,", "tokens": ["Bog", "sich", ",", "aus", "gel\u00b7bem", "ro\u00b7sa\u00e4d\u00b7ri\u00b7gen", "Ge\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Wie Marmor, aber rauher, poriger,", "tokens": ["Wie", "Mar\u00b7mor", ",", "a\u00b7ber", "rau\u00b7her", ",", "po\u00b7ri\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADV", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Hervor und wog in ihren schmalen H\u00e4nden", "tokens": ["Her\u00b7vor", "und", "wog", "in", "ih\u00b7ren", "schma\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die vollen Br\u00fcste. Und sie l\u00e4chelte.", "tokens": ["Die", "vol\u00b7len", "Br\u00fcs\u00b7te", ".", "Und", "sie", "l\u00e4\u00b7chel\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bboh Theodora, Hure, Kaiserin!\u00ab rief da mein Ratsherrnmund,", "tokens": ["\u00bb", "oh", "Theo\u00b7do\u00b7ra", ",", "Hu\u00b7re", ",", "Kai\u00b7se\u00b7rin", "!", "\u00ab", "rief", "da", "mein", "Rats\u00b7herrn\u00b7mund", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "NN", "$,", "NN", "$.", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbich komme gleich!\u00ab", "tokens": ["\u00bb", "ich", "kom\u00b7me", "gleich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Es war ein Traum. Drum ging ich \u00fcbers Meer", "tokens": ["Es", "war", "ein", "Traum", ".", "Drum", "ging", "ich", "\u00fc\u00b7bers", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PAV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie \u00fcber eine weiche gr\u00fcne Wiese.", "tokens": ["Wie", "\u00fc\u00b7ber", "ei\u00b7ne", "wei\u00b7che", "gr\u00fc\u00b7ne", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war ein Traum. Drum sah ich ihren Scho\u00df", "tokens": ["Es", "war", "ein", "Traum", ".", "Drum", "sah", "ich", "ih\u00b7ren", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als einen Lotoskelch. Es war ein Traum,", "tokens": ["Als", "ei\u00b7nen", "Lo\u00b7tos\u00b7kelch", ".", "Es", "war", "ein", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum schlug man mich ans Kreuz.", "tokens": ["Drum", "schlug", "man", "mich", "ans", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die sch\u00f6ne Dame mit dem Lotoskelch,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Da\u00b7me", "mit", "dem", "Lo\u00b7tos\u00b7kelch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es war ein Traum, sah mit Vergn\u00fcgen zu,", "tokens": ["Es", "war", "ein", "Traum", ",", "sah", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie man die N\u00e4gel mir durch Hand und Fu\u00df", "tokens": ["Wie", "man", "die", "N\u00e4\u00b7gel", "mir", "durch", "Hand", "und", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit h\u00f6lzernen H\u00e4mmern trieb. \u00bbTuts gut, mi fili?\u00ab", "tokens": ["Mit", "h\u00f6l\u00b7zer\u00b7nen", "H\u00e4m\u00b7mern", "trieb", ".", "\u00bb", "Tuts", "gut", ",", "mi", "fi\u00b7li", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "$(", "NN", "ADJD", "$,", "FM", "FM", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Rief Ihre Majest\u00e4t und nahm", "tokens": ["Rief", "Ih\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", "und", "nahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Pralinee aus ihrer Bonbonniere.", "tokens": ["Ein", "Pra\u00b7li\u00b7nee", "aus", "ih\u00b7rer", "Bon\u00b7bon\u00b7nie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es war ein Traum. Drum war mein Schmerz Genu\u00df.", "tokens": ["Es", "war", "ein", "Traum", ".", "Drum", "war", "mein", "Schmerz", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PAV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war ein Traum. Drum scho\u00df der Kreuzesast,", "tokens": ["Es", "war", "ein", "Traum", ".", "Drum", "scho\u00df", "der", "Kreu\u00b7ze\u00b7sast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von einem unsichtbaren Riesen wie ein Bogen", "tokens": ["Von", "ei\u00b7nem", "un\u00b7sicht\u00b7ba\u00b7ren", "Rie\u00b7sen", "wie", "ein", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erdw\u00e4rts gezerrt, mich einem Pfeile gleich", "tokens": ["Erd\u00b7w\u00e4rts", "ge\u00b7zerrt", ",", "mich", "ei\u00b7nem", "Pfei\u00b7le", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "PRF", "ART", "NN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Hinauf zum goldnen Himmel: wo ich nun,", "tokens": ["Hin\u00b7auf", "zum", "gold\u00b7nen", "Him\u00b7mel", ":", "wo", "ich", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$.", "PWAV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+