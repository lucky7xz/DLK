{"textgrid.poem.60504": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Traum", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich hatt ich einen Traum,", "tokens": ["N\u00e4cht\u00b7lich", "hatt", "ich", "ei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe Mutter, einen guten,", "tokens": ["Lie\u00b7be", "Mut\u00b7ter", ",", "ei\u00b7nen", "gu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob wir unter einem Baum,", "tokens": ["Ob", "wir", "un\u00b7ter", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wanderungsm\u00fcde, beide ruhten.", "tokens": ["Wan\u00b7de\u00b7rungs\u00b7m\u00fc\u00b7de", ",", "bei\u00b7de", "ruh\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "In den Scho\u00df zu s\u00fc\u00dfer Ruh", "tokens": ["In", "den", "Scho\u00df", "zu", "s\u00fc\u00b7\u00dfer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Legt ich dir mein Haupt, das schw\u00fcle,", "tokens": ["Legt", "ich", "dir", "mein", "Haupt", ",", "das", "schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "wo