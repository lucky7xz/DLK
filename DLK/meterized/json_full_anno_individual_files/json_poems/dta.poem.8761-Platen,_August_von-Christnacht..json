{"dta.poem.8761": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Christnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Was seh' ich? Umgaukelt mich Schwindel und Traum?", "tokens": ["Was", "seh'", "ich", "?", "Um\u00b7gau\u00b7kelt", "mich", "Schwin\u00b7del", "und", "Traum", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein leuchtender Saum", "tokens": ["Ein", "leuch\u00b7ten\u00b7der", "Saum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durchwebt den azurenen, ewigen Raum,", "tokens": ["Durch\u00b7webt", "den", "a\u00b7zu\u00b7re\u00b7nen", ",", "e\u00b7wi\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Es schreitet die Sterne des Himmels entlang,", "tokens": ["Es", "schrei\u00b7tet", "die", "Ster\u00b7ne", "des", "Him\u00b7mels", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Mit leisem Gesang,", "tokens": ["Mit", "lei\u00b7sem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der seligen Schaaren musikischer Gang.", "tokens": ["Der", "se\u00b7li\u00b7gen", "Schaa\u00b7ren", "mu\u00b7si\u00b7ki\u00b7scher", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}}}}}