{"dta.poem.14836": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Parole.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und Abends die W\u00e4lder rauschen,", "tokens": ["Und", "A\u00b7bends", "die", "W\u00e4l\u00b7der", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von fern nur f\u00e4llt noch ein Schu\u00df,", "tokens": ["Von", "fern", "nur", "f\u00e4llt", "noch", "ein", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da steht sie stille, zu lauschen:", "tokens": ["Da", "steht", "sie", "stil\u00b7le", ",", "zu", "lau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201edas war meines Liebsten Gru\u00df!\u201c", "tokens": ["\u201e", "das", "war", "mei\u00b7nes", "Liebs\u00b7ten", "Gru\u00df", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}