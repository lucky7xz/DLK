{"dta.poem.14283": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So wunderbare Weise", "tokens": ["So", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Singt nun Dein bleicher Mund,", "tokens": ["Singt", "nun", "Dein", "blei\u00b7cher", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist, als \u00f6ffnet' leise", "tokens": ["Es", "ist", ",", "als", "\u00f6ff\u00b7net'", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich unter mir der Grund.", "tokens": ["Sich", "un\u00b7ter", "mir", "der", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}