-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seiner Lehren Wunderdingen ...", "tokens": ["Mit", "sei\u00b7ner", "Leh\u00b7ren", "Wun\u00b7der\u00b7din\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich f\u00fcrchte nur, da\u00df Blut \u2013 viel Blut", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nur", ",", "da\u00df", "Blut", "\u2013", "viel", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "NN", "$(", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch flie\u00dfen mu\u00df, sie zu erringen ...", "tokens": ["Noch", "flie\u00b7\u00dfen", "mu\u00df", ",", "sie", "zu", "er\u00b7rin\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "'s ist Samstag. M\u00fcder Glockenton", "tokens": ["'s", "ist", "Sams\u00b7tag", ".", "M\u00fc\u00b7der", "Glo\u00b7cken\u00b7ton"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "NN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Klingt mir \u00fcber die Felder her\u00fcber ...", "tokens": ["Klingt", "mir", "\u00fc\u00b7ber", "die", "Fel\u00b7der", "her\u00b7\u00fc\u00b7ber", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Da denk' ich an den \u00bbGottessohn\u00ab \u2013", "tokens": ["Da", "denk'", "ich", "an", "den", "\u00bb", "Got\u00b7tes\u00b7sohn", "\u00ab", "\u2013"], "token_info": ["word",