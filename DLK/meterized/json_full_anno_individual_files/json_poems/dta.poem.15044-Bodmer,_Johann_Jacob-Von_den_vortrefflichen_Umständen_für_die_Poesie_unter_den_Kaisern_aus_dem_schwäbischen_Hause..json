{"dta.poem.15044": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.85", "tr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "von koufe der vil nuzzes birt", "tokens": ["von", "kou\u00b7fe", "der", "vil", "nuz\u00b7zes", "birt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die knehte solten einen wirt", "tokens": ["die", "kneh\u00b7te", "sol\u00b7ten", "ei\u00b7nen", "wirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in kiesen der in gebe sa", "tokens": ["in", "kie\u00b7sen", "der", "in", "ge\u00b7be", "sa"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des si durftic weren da", "tokens": ["des", "si", "durf\u00b7tic", "we\u00b7ren", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ze ritterliche koste", "tokens": ["ze", "rit\u00b7ter\u00b7li\u00b7che", "kos\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "si wolten uf die joste", "tokens": ["si", "wol\u00b7ten", "uf", "die", "jos\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "nach hoher minne lone", "tokens": ["nach", "ho\u00b7her", "min\u00b7ne", "lo\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "bereiten sich vil schone", "tokens": ["be\u00b7rei\u00b7ten", "sich", "vil", "scho\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nu daz die knechte v&#250;r geriten", "tokens": ["Nu", "daz", "die", "knech\u00b7te", "v", "&#250;", "r", "ge\u00b7ri\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ADV", "KOUS", "ART", "VVFIN", "XY", "XY", "XY", "XY"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "die ritter da niht langer biten", "tokens": ["die", "rit\u00b7ter", "da", "niht", "lan\u00b7ger", "bi\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Gaudin unde Partinopier", "tokens": ["Gau\u00b7din", "un\u00b7de", "Par\u00b7ti\u00b7no\u00b7pier"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "zuo der Stat schone unde fier", "tokens": ["zuo", "der", "Stat", "scho\u00b7ne", "un\u00b7de", "fier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "diu Schiefdere waz genant", "tokens": ["diu", "Schief\u00b7de\u00b7re", "waz", "ge\u00b7nant"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "kamen si da sa zehant", "tokens": ["ka\u00b7men", "si", "da", "sa", "ze\u00b7hant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "FM.it", "FM.it", "FM.it", "FM.it"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "da si vil geste sahen", "tokens": ["da", "si", "vil", "ges\u00b7te", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "zuo riten unde gahen", "tokens": ["zuo", "ri\u00b7ten", "un\u00b7de", "ga\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "uz manegem kunecriche", "tokens": ["uz", "ma\u00b7ne\u00b7gem", "ku\u00b7ne\u00b7cri\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "si beide vil geliche", "tokens": ["si", "bei\u00b7de", "vil", "ge\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "ir ougenblicke niht verlurn", "tokens": ["ir", "ou\u00b7gen\u00b7bli\u00b7cke", "niht", "ver\u00b7lurn"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "wan si da spurten unde kurn", "tokens": ["wan", "si", "da", "spur\u00b7ten", "un\u00b7de", "kurn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "werder kunege ein wunder", "tokens": ["wer\u00b7der", "ku\u00b7ne\u00b7ge", "ein", "wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "ouch sahen si dar under", "tokens": ["ouch", "sa\u00b7hen", "si", "dar", "un\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "manegen herzogen hoch", "tokens": ["ma\u00b7ne\u00b7gen", "her\u00b7zo\u00b7gen", "hoch"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "vil grafen unde frigen zock", "tokens": ["vil", "gra\u00b7fen", "un\u00b7de", "fri\u00b7gen", "zock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "hin zuo dem turneie", "tokens": ["hin", "zuo", "dem", "tur\u00b7nei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.26": {"text": "rich unde manger leie", "tokens": ["rich", "un\u00b7de", "man\u00b7ger", "lei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "schein diu gezierde ir werden hers", "tokens": ["schein", "diu", "ge\u00b7zier\u00b7de", "ir", "wer\u00b7den", "hers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "hie dissit unde ienhalp mers", "tokens": ["hie", "dis\u00b7sit", "un\u00b7de", "ien\u00b7halp", "mers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "was ir volc gewahsen", "tokens": ["was", "ir", "volc", "ge\u00b7wah\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VAPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Westfalen unde sahsen", "tokens": ["West\u00b7fa\u00b7len", "un\u00b7de", "sah\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.31": {"text": "vranken peier swabe", "tokens": ["vran\u00b7ken", "pei\u00b7er", "swa\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "durch werder minne gabe", "tokens": ["durch", "wer\u00b7der", "min\u00b7ne", "ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "kamen dar mit hoher craft", "tokens": ["ka\u00b7men", "dar", "mit", "ho\u00b7her", "craft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}