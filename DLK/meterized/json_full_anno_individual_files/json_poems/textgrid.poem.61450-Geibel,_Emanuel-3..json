{"textgrid.poem.61450": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Rebeng\u00e4rten an den Kl\u00fcften,", "tokens": ["Ihr", "Re\u00b7ben\u00b7g\u00e4r\u00b7ten", "an", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Nelken, die vom Fels ihr lauscht,", "tokens": ["Ihr", "Nel\u00b7ken", ",", "die", "vom", "Fels", "ihr", "lauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie habt ihr heut mit euren D\u00fcften", "tokens": ["Wie", "habt", "ihr", "heut", "mit", "eu\u00b7ren", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir r\u00e4tselhaft den Sinn berauscht!", "tokens": ["Mir", "r\u00e4t\u00b7sel\u00b7haft", "den", "Sinn", "be\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch all mein Wesen flutet wieder", "tokens": ["Durch", "all", "mein", "We\u00b7sen", "flu\u00b7tet", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verge\u00dfne Lust, erinnernd Lei