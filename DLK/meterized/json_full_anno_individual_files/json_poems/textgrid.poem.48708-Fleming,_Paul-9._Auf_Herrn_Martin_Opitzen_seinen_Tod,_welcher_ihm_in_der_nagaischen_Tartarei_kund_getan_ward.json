{"textgrid.poem.48708": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Auf Herrn Martin Opitzen seinen Tod, welcher ihm in der nagaischen Tartarei kund getan ward", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat mein Verh\u00e4ngn\u00fc\u00df denn so ganz zur Unzeit mich", "tokens": ["Hat", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "denn", "so", "ganz", "zur", "Un\u00b7zeit", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "ADV", "ADV", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von Gl\u00fccke, Vaterland und Allem ausgetrieben,", "tokens": ["von", "Gl\u00fc\u00b7cke", ",", "Va\u00b7ter\u00b7land", "und", "Al\u00b7lem", "aus\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und war mir denn also annoch nicht Alles blieben,", "tokens": ["und", "war", "mir", "denn", "al\u00b7so", "an\u00b7noch", "nicht", "Al\u00b7les", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "das einen Geist belebt und mutig macht auf sich,", "tokens": ["das", "ei\u00b7nen", "Geist", "be\u00b7lebt", "und", "mu\u00b7tig", "macht", "auf", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "KON", "ADJD", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "du Herzog meiner Lust? Mir hat auch endlich dich", "tokens": ["du", "Her\u00b7zog", "mei\u00b7ner", "Lust", "?", "Mir", "hat", "auch", "end\u00b7lich", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der allgemeine Rat auf Deutschland aufgerieben,", "tokens": ["der", "all\u00b7ge\u00b7mei\u00b7ne", "Rat", "auf", "Deutschland", "auf\u00b7ge\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "dich, du der Menschen Lust und einziges Belieben,", "tokens": ["dich", ",", "du", "der", "Men\u00b7schen", "Lust", "und", "ein\u00b7zi\u00b7ges", "Be\u00b7lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ART", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die wahrer Liebe lieb und f\u00e4hig sind, wie ich.", "tokens": ["die", "wah\u00b7rer", "Lie\u00b7be", "lieb", "und", "f\u00e4\u00b7hig", "sind", ",", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich hier der Scythen Raub, wie Naso dort der Geten,", "tokens": ["Ich", "hier", "der", "Scyt\u00b7hen", "Raub", ",", "wie", "Na\u00b7so", "dort", "der", "Ge\u00b7ten", ","], "toke