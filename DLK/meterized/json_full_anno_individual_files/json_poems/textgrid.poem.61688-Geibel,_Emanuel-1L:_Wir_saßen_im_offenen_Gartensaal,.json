{"textgrid.poem.61688": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sa\u00dfen im offenen Gartensaal,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sa\u00dfen im offenen Gartensaal,", "tokens": ["Wir", "sa\u00b7\u00dfen", "im", "of\u00b7fe\u00b7nen", "Gar\u00b7ten\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Versunken war die Sonne;", "tokens": ["Ver\u00b7sun\u00b7ken", "war", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In wilden Zweifeln ging mein Herz,", "tokens": ["In", "wil\u00b7den", "Zwei\u00b7feln", "ging", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sturm von Weh und Wonne.", "tokens": ["Im", "Sturm", "von", "Weh", "und", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da schlug im Busch die Nachtigall,", "tokens": ["Da", "schlug", "im", "Busch", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pl\u00f6tzlich unter Tr\u00e4nen", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "un\u00b7ter", "Tr\u00e4\u