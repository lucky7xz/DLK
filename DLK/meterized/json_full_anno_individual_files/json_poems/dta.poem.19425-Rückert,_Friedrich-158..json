{"dta.poem.19425": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "158.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tod, der die Geburt ist in ein h\u00f6hres Leben,", "tokens": ["Der", "Tod", ",", "der", "die", "Ge\u00b7burt", "ist", "in", "ein", "h\u00f6h\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist auch wie jegliche Geburt mit Weh umgeben.", "tokens": ["Ist", "auch", "wie", "jeg\u00b7li\u00b7che", "Ge\u00b7burt", "mit", "Weh", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "PIAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alswie ein Kindlein tritt in diese Welt mit Klagen,", "tokens": ["Als\u00b7wie", "ein", "Kin\u00b7dlein", "tritt", "in", "die\u00b7se", "Welt", "mit", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus dieser so die Seel' in jene mit Verzagen.", "tokens": ["Aus", "die\u00b7ser", "so", "die", "Seel'", "in", "je\u00b7ne", "mit", "Ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "ART", "NN", "APPR", "PDAT", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie schwer das Kindlein sich entwand dem Mutterschoo\u00df,", "tokens": ["Wie", "schwer", "das", "Kin\u00b7dlein", "sich", "ent\u00b7wand", "dem", "Mut\u00b7ter\u00b7schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ringt die Seele sich aus diesem Leibe los.", "tokens": ["So", "ringt", "die", "See\u00b7le", "sich", "aus", "die\u00b7sem", "Lei\u00b7be", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wie das Kindlein nun, gew\u00f6hnt der neuen Lust,", "tokens": ["Doch", "wie", "das", "Kin\u00b7dlein", "nun", ",", "ge\u00b7w\u00f6hnt", "der", "neu\u00b7en", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$,", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichtmehr zur\u00fcck zum Schoo\u00df sich sehnet von der Brust;", "tokens": ["Nicht\u00b7mehr", "zu\u00b7r\u00fcck", "zum", "Schoo\u00df", "sich", "seh\u00b7net", "von", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "APPRART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So wird die Seele bald, von h\u00f6herm Licht umfangen,", "tokens": ["So", "wird", "die", "See\u00b7le", "bald", ",", "von", "h\u00f6\u00b7herm", "Licht", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum dunkeln Aufenthalt nicht mehr zur\u00fcck verlangen.", "tokens": ["Zum", "dun\u00b7keln", "Auf\u00b7ent\u00b7halt", "nicht", "mehr", "zu\u00b7r\u00fcck", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKNEG", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}