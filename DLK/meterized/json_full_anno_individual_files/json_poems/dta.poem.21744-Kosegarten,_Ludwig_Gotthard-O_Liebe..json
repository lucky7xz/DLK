"], "pos": ["NE", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich untergehn, gar untergehn,", "tokens": ["Mich", "un\u00b7ter\u00b7gehn", ",", "gar", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um nimmer, nimmer zu erstehn . . .", "tokens": ["Um", "nim\u00b7mer", ",", "nim\u00b7mer", "zu", "er\u00b7stehn", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUI", "ADV", "$,", "ADV", "PTKZU", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Liebe,", "tokens": ["O", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Tilg' alle niedre Triebe,", "tokens": ["Til\u00b7g'", "al\u00b7le", "nied\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dass ich nur dich! dich! liebe!", "tokens": ["Dass", "ich", "nur", "dich", "!", "dich", "!", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "$.", "PPER", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}