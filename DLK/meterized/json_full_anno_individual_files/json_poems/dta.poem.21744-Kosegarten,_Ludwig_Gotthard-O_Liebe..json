{"dta.poem.21744": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "O Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Liebe!", "tokens": ["O", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die mich bis in den Tod geliebt,", "tokens": ["Die", "mich", "bis", "in", "den", "Tod", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schmerzlich sich um mich betr\u00fcbt,", "tokens": ["Die", "schmerz\u00b7lich", "sich", "um", "mich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die f\u00fcr mich lebt' und litt und starb,", "tokens": ["Die", "f\u00fcr", "mich", "lebt'", "und", "litt", "und", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sterbend mir das Heil erwarb . . ,", "tokens": ["Und", "ster\u00b7bend", "mir", "das", "Heil", "er\u00b7warb", ".", ".", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "NN", "VVFIN", "$.", "$.", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Liebe,", "tokens": ["O", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sieh, wie ich mich betr\u00fcbe,", "tokens": ["Sieh", ",", "wie", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dass ich nicht gnug dich liebe.", "tokens": ["Dass", "ich", "nicht", "gnug", "dich", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Liebe,", "tokens": ["O", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die du f\u00fcr mich der Thr\u00e4nen Fluth,", "tokens": ["Die", "du", "f\u00fcr", "mich", "der", "Thr\u00e4\u00b7nen", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr mich verstr\u00f6mt dein rothes Blut,", "tokens": ["F\u00fcr", "mich", "ver\u00b7str\u00f6mt", "dein", "ro\u00b7thes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr mich dein Leben ausgest\u00f6hnt,", "tokens": ["F\u00fcr", "mich", "dein", "Le\u00b7ben", "aus\u00b7ge\u00b7st\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Vater sterbend mir vers\u00f6hnt . . .", "tokens": ["Den", "Va\u00b7ter", "ster\u00b7bend", "mir", "ver\u00b7s\u00f6hnt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "PPER", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Liebe,", "tokens": ["O", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sieh, wie ich mich betr\u00fcbe,", "tokens": ["Sieh", ",", "wie", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dass ich nicht gnug dich liebe.", "tokens": ["Dass", "ich", "nicht", "gnug", "dich", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Liebe,", "tokens": ["O", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "O du in deinem Dornenkranz,", "tokens": ["O", "du", "in", "dei\u00b7nem", "Dor\u00b7nen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deines Blutes Purpurglanz,", "tokens": ["In", "dei\u00b7nes", "Blu\u00b7tes", "Pur\u00b7pur\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O s\u00fcndeb\u00fcssend Opferlamm,", "tokens": ["O", "s\u00fcn\u00b7de\u00b7b\u00fcs\u00b7send", "Op\u00b7fer\u00b7lamm