{"textgrid.poem.63033": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem biedern Schulzen Ringewald", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem biedern Schulzen Ringewald", "tokens": ["Dem", "bie\u00b7dern", "Schul\u00b7zen", "Rin\u00b7ge\u00b7wald"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Starb seine liebe sch\u00f6ne Hanne,", "tokens": ["Starb", "sei\u00b7ne", "lie\u00b7be", "sch\u00f6\u00b7ne", "Han\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie brach sein Herz, doch fand er bald,", "tokens": ["Sie", "brach", "sein", "Herz", ",", "doch", "fand", "er", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df einem reichen Ackersmanne,", "tokens": ["Da\u00df", "ei\u00b7nem", "rei\u00b7chen", "A\u00b7ckers\u00b7man\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Knecht und Magd und Sch\u00f6sser qu\u00e4lt,", "tokens": ["Den", "Knecht", "und", "Magd", "und", "Sch\u00f6s\u00b7ser", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seiner Gattin alles fehlt.", "t