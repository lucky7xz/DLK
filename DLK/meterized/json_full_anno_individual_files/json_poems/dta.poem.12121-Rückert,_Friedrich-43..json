{"dta.poem.12121": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier steht das Schl\u00f6\u00dflein noch, von dessen Hochaltan", "tokens": ["Hier", "steht", "das", "Schl\u00f6\u00df\u00b7lein", "noch", ",", "von", "des\u00b7sen", "Hoc\u00b7hal\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufs Innthal niedersah Held Maximilian.", "tokens": ["Aufs", "I\u00b7nnthal", "nie\u00b7der\u00b7sah", "Held", "Ma\u00b7xi\u00b7mi\u00b7li\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier steht der Steintisch noch, wo er hielt in der Hand", "tokens": ["Hier", "steht", "der", "Stein\u00b7tisch", "noch", ",", "wo", "er", "hielt", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Humpen, eh er sich verstieg zur Martinswand.", "tokens": ["Den", "Hum\u00b7pen", ",", "eh", "er", "sich", "ver\u00b7stieg", "zur", "Mar\u00b7tins\u00b7wand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier ist noch farbenhell zu sehn der Baldachin,", "tokens": ["Hier", "ist", "noch", "far\u00b7ben\u00b7hell", "zu", "sehn", "der", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo zu Gericht er sa\u00df; wo ist er selber hin?", "tokens": ["Wo", "zu", "Ge\u00b7richt", "er", "sa\u00df", ";", "wo", "ist", "er", "sel\u00b7ber", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}