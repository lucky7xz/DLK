{"textgrid.poem.38868": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon f\u00e4ngt die alte Nacht sich an zu hellen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon f\u00e4ngt die alte Nacht sich an zu hellen,", "tokens": ["Schon", "f\u00e4ngt", "die", "al\u00b7te", "Nacht", "sich", "an", "zu", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wieder scheinen licht aus klarer Ferne", "tokens": ["Und", "wie\u00b7der", "schei\u00b7nen", "licht", "aus", "kla\u00b7rer", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die hohen Bilder, freundlich liebe Sterne,", "tokens": ["Die", "ho\u00b7hen", "Bil\u00b7der", ",", "freund\u00b7lich", "lie\u00b7be", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Piloten auf der weiten Bahn der Wellen.", "tokens": ["Pi\u00b7lo\u00b7ten", "auf", "der", "wei\u00b7ten", "Bahn", "der", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wen k\u00fcmmert's, da\u00df die Hund' am Ufer bellen?", "tokens": ["Wen", "k\u00fcm\u00b7mert's", ",", "da\u00df", "die", "Hund'", "am", "U\u00