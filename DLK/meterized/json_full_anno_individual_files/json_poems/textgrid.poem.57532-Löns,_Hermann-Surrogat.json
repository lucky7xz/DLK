{"textgrid.poem.57532": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Surrogat", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00fcsse mich, dein K\u00fcssen ist", "tokens": ["O", "k\u00fcs\u00b7se", "mich", ",", "dein", "K\u00fcs\u00b7sen", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So s\u00fc\u00df fast wie des Todes Ku\u00df,", "tokens": ["So", "s\u00fc\u00df", "fast", "wie", "des", "To\u00b7des", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei deinem leisen Ku\u00df vergi\u00dft", "tokens": ["Bei", "dei\u00b7nem", "lei\u00b7sen", "Ku\u00df", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz, da\u00df es noch schlagen mu\u00df.", "tokens": ["Mein", "Herz", ",", "da\u00df", "es", "noch", "schla\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O k\u00fc\u00df und k\u00fc\u00df mich immerzu,", "tokens": ["O", "k\u00fc\u00df", "und", "k\u00fc\u00df", "mich", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "i