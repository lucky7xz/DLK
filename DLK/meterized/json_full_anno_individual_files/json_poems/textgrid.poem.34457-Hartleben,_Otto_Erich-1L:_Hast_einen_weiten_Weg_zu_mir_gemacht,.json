{"textgrid.poem.34457": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast einen weiten Weg zu mir gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast einen weiten Weg zu mir gemacht,", "tokens": ["Hast", "ei\u00b7nen", "wei\u00b7ten", "Weg", "zu", "mir", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dein Knie ist heiss und rosig angehaucht.", "tokens": ["dein", "Knie", "ist", "heiss", "und", "ro\u00b7sig", "an\u00b7ge\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es war der Wind, der durch die Strassen ging", "tokens": ["Es", "war", "der", "Wind", ",", "der", "durch", "die", "Stras\u00b7sen", "ging"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und deinen Schritt zu mir befl\u00fcgelte,", "tokens": ["und", "dei\u00b7nen", "Schritt", "zu", "mir", "be\u00b7fl\u00fc\u00b7gel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "es war die stille Gluth in deinem Herzen,", "tokens": ["es", "war", "die", "stil\u00b7le", "Gluth", "in", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die dich so schnell in meinen Arm gef\u00fchrt.", "tokens": ["die", "dich", "so", "schnell", "in", "mei\u00b7nen", "Arm", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nun ruhe aus. Ich lege meine B\u00fccher", "tokens": ["Nun", "ru\u00b7he", "aus", ".", "Ich", "le\u00b7ge", "mei\u00b7ne", "B\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "bei Seite nun. Zu deinen F\u00fcssen kauernd \u2013", "tokens": ["bei", "Sei\u00b7te", "nun", ".", "Zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "kau\u00b7ernd", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "auch ich will ruhn. Des wohligen Gef\u00fchls", "tokens": ["auch", "ich", "will", "ruhn", ".", "Des", "woh\u00b7li\u00b7gen", "Ge\u00b7f\u00fchls"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "mich freuend, das des Schiffers Brust durchstr\u00f6mt,", "tokens": ["mich", "freu\u00b7end", ",", "das", "des", "Schif\u00b7fers", "Brust", "durch\u00b7str\u00f6mt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "liegt er im Hafen seiner Heimath wieder,", "tokens": ["liegt", "er", "im", "Ha\u00b7fen", "sei\u00b7ner", "Hei\u00b7math", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.12": {"text": "schmieg ich mein Haupt an diese weichen Kniee,", "tokens": ["schmieg", "ich", "mein", "Haupt", "an", "die\u00b7se", "wei\u00b7chen", "Kni\u00b7ee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "die vor dem Weg zu mir err\u00f6thet sind,", "tokens": ["die", "vor", "dem", "Weg", "zu", "mir", "er\u00b7r\u00f6t\u00b7het", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "die auf dem Weg zu mir so heiss geworden.", "tokens": ["die", "auf", "dem", "Weg", "zu", "mir", "so", "heiss", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hast einen weiten Weg zu mir gemacht,", "tokens": ["Hast", "ei\u00b7nen", "wei\u00b7ten", "Weg", "zu", "mir", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "meas