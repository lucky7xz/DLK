{"dta.poem.2653": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Als Hr. M. Z. den 5. Novembr. 1673. zur  \n Erden bestattet wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje handelt nicht mit uns das G\u00f6ttliche Geschicke?", "tokens": ["Wie", "han\u00b7delt", "nicht", "mit", "uns", "das", "G\u00f6tt\u00b7li\u00b7che", "Ge\u00b7schi\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer forscht den innern Rath der h\u00f6chsten Allmacht", "tokens": ["Wer", "forscht", "den", "in\u00b7nern", "Rath", "der", "h\u00f6chs\u00b7ten", "All\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Jetzt kr\u00f6nt uns Freud und Lust/ in einem Augenblicke/", "tokens": ["Jetzt", "kr\u00f6nt", "uns", "Freud", "und", "Lust", "/", "in", "ei\u00b7nem", "Au\u00b7gen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Treibt uns ein rauher Sturm ins Weh\u2019 und Klage-", "tokens": ["Treibt", "uns", "ein", "rau\u00b7her", "Sturm", "ins", "Weh'", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wolte/ wolte Gott/ Betr\u00fcbtste von den Frauen/", "tokens": ["Und", "wol\u00b7te", "/", "wol\u00b7te", "Gott", "/", "Be\u00b7tr\u00b7\u00fcbts\u00b7te", "von", "den", "Frau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "VMFIN", "NN", "$(", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So je das g\u00fcldne Licht der Sonnen hat bestrahlt/", "tokens": ["So", "je", "das", "g\u00fcld\u00b7ne", "Licht", "der", "Son\u00b7nen", "hat", "be\u00b7strahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir d\u00fcrfften nicht an ihr ein traurig Beyspiel schauen", "tokens": ["Wir", "d\u00fcrff\u00b7ten", "nicht", "an", "ihr", "ein", "trau\u00b7rig", "Bey\u00b7spiel", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein wahres Contrafeit das Noth und Elend mahlt.", "tokens": ["Ein", "wah\u00b7res", "Cont\u00b7ra\u00b7feit", "das", "Noth", "und", "E\u00b7lend", "mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer ohn\u2019 Erbarmn\u00fc\u00df kan bey ihren Thr\u00e4nen sitzen/", "tokens": ["Wer", "ohn'", "Er\u00b7barm\u00b7n\u00fc\u00df", "kan", "bey", "ih\u00b7ren", "Thr\u00e4\u00b7nen", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem ist ein kalter Stahl umb seine Brust gelegt/", "tokens": ["Dem", "ist", "ein", "kal\u00b7ter", "Stahl", "umb", "sei\u00b7ne", "Brust", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Augen/ so jetzund mehr Blut/ als Z\u00e4hren schwitzen/", "tokens": ["Die", "Au\u00b7gen", "/", "so", "je\u00b7tzund", "mehr", "Blut", "/", "als", "Z\u00e4h\u00b7ren", "schwit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "PIAT", "NN", "$(", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat/ weil sie Athem sch\u00f6pfft/ kein Zufall so bewegt.", "tokens": ["Hat", "/", "weil", "sie", "A\u00b7them", "sch\u00f6pfft", "/", "kein", "Zu\u00b7fall", "so", "be\u00b7wegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "NN", "VVFIN", "$(", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie als Penelope sitzt in des Hauses Gr\u00e4ntzen/", "tokens": ["Sie", "als", "Pe\u00b7ne\u00b7lo\u00b7pe", "sitzt", "in", "des", "Hau\u00b7ses", "Gr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Hofft mit dem Untergang noch ihren Schatz zu sehn/", "tokens": ["Hofft", "mit", "dem", "Un\u00b7ter\u00b7gang", "noch", "ih\u00b7ren", "Schatz", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sein Ankunfft soll den Harm der Einsamkeit bekr\u00e4ntzen/", "tokens": ["Sein", "An\u00b7kunfft", "soll", "den", "Harm", "der", "Ein\u00b7sam\u00b7keit", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sie will der Liebe West ihm durch den Ku\u00df zuwehn.", "tokens": ["Sie", "will", "der", "Lie\u00b7be", "West", "ihm", "durch", "den", "Ku\u00df", "zu\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie denckt jetzt kommt er an/ jetzt tritt er auf die Schwelle/", "tokens": ["Sie", "denckt", "jetzt", "kommt", "er", "an", "/", "jetzt", "tritt", "er", "auf", "die", "Schwel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie bringt den Kindern auch dergleichen Freuden bey/", "tokens": ["Sie", "bringt", "den", "Kin\u00b7dern", "auch", "derg\u00b7lei\u00b7chen", "Freu\u00b7den", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIS", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und h\u00f6rt man da\u00df ein Hund starck in der Gegend belle/", "tokens": ["Und", "h\u00f6rt", "man", "da\u00df", "ein", "Hund", "starck", "in", "der", "Ge\u00b7gend", "bel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So glaubt mans vor gewi\u00df/ da\u00df er verhanden sey.", "tokens": ["So", "glaubt", "mans", "vor", "ge\u00b7wi\u00df", "/", "da\u00df", "er", "ver\u00b7han\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADV", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein die tieffe Nacht geziert mit tausend Sternen", "tokens": ["Al\u00b7lein", "die", "tief\u00b7fe", "Nacht", "ge\u00b7ziert", "mit", "tau\u00b7send", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hat nun das g\u00fcldne Heer/ schon auff ihr Mittel bracht/", "tokens": ["Hat", "nun", "das", "g\u00fcld\u00b7ne", "Heer", "/", "schon", "auff", "ihr", "Mit\u00b7tel", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$(", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als sich ihr Liebster will nur l\u00e4nger noch entfernen/", "tokens": ["Als", "sich", "ihr", "Liebs\u00b7ter", "will", "nur", "l\u00e4n\u00b7ger", "noch", "ent\u00b7fer\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VMFIN", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ihr getreues Hertz in m\u00fcden Sorgen wacht.", "tokens": ["Und", "ihr", "ge\u00b7treu\u00b7es", "Hertz", "in", "m\u00fc\u00b7den", "Sor\u00b7gen", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie hofft/ wird nur den Tag Aurorens Purpur f\u00e4rben/", "tokens": ["Sie", "hofft", "/", "wird", "nur", "den", "Tag", "Au\u00b7ro\u00b7rens", "Pur\u00b7pur", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VAFIN", "ADV", "ART", "NN", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den Treusten von der Welt zu schliessen in den Arm/", "tokens": ["Den", "Treus\u00b7ten", "von", "der", "Welt", "zu", "schlies\u00b7sen", "in", "den", "Arm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "In dem kommt eine Post/ und sagt von Tod und Sterben/", "tokens": ["In", "dem", "kommt", "ei\u00b7ne", "Post", "/", "und", "sagt", "von", "Tod", "und", "Ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Ja wie ihr Schatz nicht mehr von Blut und Geistern warm.", "tokens": ["Ja", "wie", "ihr", "Schatz", "nicht", "mehr", "von", "Blut", "und", "Geis\u00b7tern", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ists m\u00f6glich da\u00df sie nicht wie Niobe zu Steinen/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "sie", "nicht", "wie", "Ni\u00b7o\u00b7be", "zu", "Stei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PPER", "PTKNEG", "KOKOM", "NE", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ja wie die Byblis sich in einen Flu\u00df verkehrt/", "tokens": ["Ja", "wie", "die", "Byb\u00b7lis", "sich", "in", "ei\u00b7nen", "Flu\u00df", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "ART", "NE", "PRF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df ein unendlich Ach! und unaufh\u00f6rlich weinen/", "tokens": ["Da\u00df", "ein", "un\u00b7end\u00b7lich", "Ach", "!", "und", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ITJ", "$.", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Di\u00df was noch \u00fcbrig ist/ vom Leben/ gantz verzehrt.", "tokens": ["Di\u00df", "was", "noch", "\u00fcb\u00b7rig", "ist", "/", "vom", "Le\u00b7ben", "/", "gantz", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ADV", "ADJD", "VAFIN", "$(", "APPRART", "NN", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie h\u00f6rt/ zwar halb entseelt/ da\u00df Seyfersdorff die B\u00fchne", "tokens": ["Sie", "h\u00f6rt", "/", "zwar", "halb", "ent\u00b7seelt", "/", "da\u00df", "Sey\u00b7fers\u00b7dorff", "die", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADJD", "VVPP", "$(", "KOUS", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der h\u00f6chsten Traurigkeit und Hertzens Trennung ist/", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Trau\u00b7rig\u00b7keit", "und", "Hert\u00b7zens", "Tren\u00b7nung", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df ein gewaltig Flu\u00df/ in dem er reist durchs gr\u00fcne", "tokens": ["Da\u00df", "ein", "ge\u00b7wal\u00b7tig", "Flu\u00df", "/", "in", "dem", "er", "reist", "durchs", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "De\u00df Lebens letztes Ziel da ihrem Schatz erkiest.", "tokens": ["De\u00df", "Le\u00b7bens", "letz\u00b7tes", "Ziel", "da", "ih\u00b7rem", "Schatz", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da\u00df ihn ein eintzig Nun der Erden hat entrissen/", "tokens": ["Da\u00df", "ihn", "ein", "eint\u00b7zig", "Nun", "der", "Er\u00b7den", "hat", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein unverhoffter Tod den Bleichen zugesellt/", "tokens": ["Ein", "un\u00b7ver\u00b7hoff\u00b7ter", "Tod", "den", "Blei\u00b7chen", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und da\u00df durch solchen Fall sie alles mu\u00df vermissen/", "tokens": ["Und", "da\u00df", "durch", "sol\u00b7chen", "Fall", "sie", "al\u00b7les", "mu\u00df", "ver\u00b7mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PIAT", "NN", "PPER", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Was sie noch eintzig hat vergn\u00fcgt auf dieser Welt.", "tokens": ["Was", "sie", "noch", "eint\u00b7zig", "hat", "ver\u00b7gn\u00fcgt", "auf", "die\u