{"textgrid.poem.66398": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Prolog zu Lessings Emilia Galotti gesprochen von Demoiselle Reichard 1776 in Ulm", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erlauchte G\u00f6nner unsrer Spiele,", "tokens": ["Er\u00b7lauch\u00b7te", "G\u00f6n\u00b7ner", "uns\u00b7rer", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier steht Emilia", "tokens": ["Hier", "steht", "E\u00b7mi\u00b7lia"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Zum erstenmal voll sch\u00fcchterner Gef\u00fchle", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "voll", "sch\u00fcch\u00b7ter\u00b7ner", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihrer Unschuld vor Euch da! \u2013", "tokens": ["In", "ih\u00b7rer", "Un\u00b7schuld", "vor", "Euch", "da", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie werden ihre H\u00e4nde wanken,", "tokens": ["Wie", "wer\u00b7den", "ih\u00b7re", "H\u00e4n\u00b7de", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie die Rosenbl\u00e4tter pfl\u00fcckt,", "tokens": ["Wenn", "sie", "die", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn vertieft in schreckliche Gedanken", "tokens": ["Und", "wenn", "ver\u00b7tieft", "in", "schreck\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Vater \u2013 ha! den Dolch nach ihrem Busen z\u00fcckt.", "tokens": ["Der", "Va\u00b7ter", "\u2013", "ha", "!", "den", "Dolch", "nach", "ih\u00b7rem", "Bu\u00b7sen", "z\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ITJ", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O st\u00e4rke mich, Natur, und \u00f6ffne du die\u00df Herze,", "tokens": ["O", "st\u00e4r\u00b7ke", "mich", ",", "Na\u00b7tur", ",", "und", "\u00f6ff\u00b7ne", "du", "die\u00df", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df sein Gef\u00fchl sich hei\u00df und wahr ergie\u00dft,", "tokens": ["Da\u00df", "sein", "Ge\u00b7f\u00fchl", "sich", "hei\u00df", "und", "wahr", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bei des Geliebten Tod und bei des Vaters Schmerze", "tokens": ["Bei", "des", "Ge\u00b7lieb\u00b7ten", "Tod", "und", "bei", "des", "Va\u00b7ters", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auch meine Thr\u00e4ne \u2013 wirklich flie\u00dft.", "tokens": ["Auch", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "\u2013", "wirk\u00b7lich", "flie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wenn aus weiter aufgerissner Wunde", "tokens": ["Und", "wenn", "aus", "wei\u00b7ter", "auf\u00b7ge\u00b7riss\u00b7ner", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das Blut in Purpurtropfen quillt,", "tokens": ["Das", "Blut", "in", "Pur\u00b7pur\u00b7trop\u00b7fen", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ach, wenn verbleicht die R\u00f6th' auf meinem Munde", "tokens": ["Ach", ",", "wenn", "ver\u00b7bleicht", "die", "R\u00f6\u00b7th'", "auf", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Und Nebel meine Blicke h\u00fcllt \u2013", "tokens": ["Und", "Ne\u00b7bel", "mei\u00b7ne", "Bli\u00b7cke", "h\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So bebt mit uns vor dieser Scene,", "tokens": ["So", "bebt", "mit", "uns", "vor", "die\u00b7ser", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der furchtbarsten, der schrecklichsten!", "tokens": ["Der", "furcht\u00b7bars\u00b7ten", ",", "der", "schreck\u00b7lichs\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.19": {"text": "Schenkt meinem Vater eine Thr\u00e4ne", "tokens": ["Schenkt", "mei\u00b7nem", "Va\u00b7ter", "ei\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und mir \u2013 der Hingeopferten!", "tokens": ["Und", "mir", "\u2013", "der", "Hin\u00b7ge\u00b7o\u00b7pfer\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Wenn dann ein Edler spr\u00e4che:", "tokens": ["Wenn", "dann", "ein", "Ed\u00b7ler", "spr\u00e4\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "O Himmel, r\u00e4che, r\u00e4che", "tokens": ["O", "Him\u00b7mel", ",", "r\u00e4\u00b7che", ",", "r\u00e4\u00b7che"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die Unschuld! S\u00e4ume nicht,", "tokens": ["Die", "Un\u00b7schuld", "!", "S\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Du furchtbares Gericht!", "tokens": ["Du", "furcht\u00b7ba\u00b7res", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Dein Wetter soll die Marinellis treffen,", "tokens": ["Dein", "Wet\u00b7ter", "soll", "die", "Ma\u00b7ri\u00b7nel\u00b7lis", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die ihre bessre F\u00fcrsten \u00e4ffen!", "tokens": ["Die", "ih\u00b7re", "bess\u00b7re", "F\u00fcrs\u00b7ten", "\u00e4f\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Sie hat die H\u00f6ll' heraufgesandt! \u2013", "tokens": ["Sie", "hat", "die", "H\u00f6ll'", "her\u00b7auf\u00b7ge\u00b7sandt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und dann der Beifall jeder Hand", "tokens": ["Und", "dann", "der", "Bei\u00b7fall", "je\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Uns zuklatscht; wie belohnt ist da", "tokens": ["Uns", "zu\u00b7klatscht", ";", "wie", "be\u00b7lohnt", "ist", "da"], "