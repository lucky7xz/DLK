{"dta.poem.725": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  Menschliches Elende.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was sind wir Menschen doch? ein Wohnhau\u00df grimmer ", "tokens": ["Was", "sind", "wir", "Men\u00b7schen", "doch", "?", "ein", "Wohn\u00b7hau\u00df", "grim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "NN", "ADV", "$.", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schmertzen.", "tokens": ["schmert\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ein bald verschmeltzter ", "tokens": ["Ein", "bald", "ver\u00b7schmeltz\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Di\u00df Leben fleucht davon wie ein geschw\u00e4tz vnd schertzen.", "tokens": ["Di\u00df", "Le\u00b7ben", "fleucht", "da\u00b7von", "wie", "ein", "ge\u00b7schw\u00e4tz", "vnd", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PAV", "KOKOM", "ART", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die vor vns abgelegt de\u00df schwachen Leibes kleidt", "tokens": ["Die", "vor", "vns", "ab\u00b7ge\u00b7legt", "de\u00df", "schwa\u00b7chen", "Lei\u00b7bes", "kleidt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVPP", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd in das todten Buch der grossen sterbligkeit", "tokens": ["Vnd", "in", "das", "tod\u00b7ten", "Buch", "der", "gros\u00b7sen", "ster\u00b7blig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "L\u00e4ngst eingeschrieben sind/ sind vns au\u00df sinn vnd hertzen.", "tokens": ["L\u00e4ngst", "ein\u00b7ge\u00b7schrie\u00b7ben", "sind", "/", "sind", "vns", "au\u00df", "sinn", "vnd", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$(", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gleich wie ein eitel traum leicht au\u00df der acht hinf\u00e4lt", "tokens": ["Gleich", "wie", "ein", "ei\u00b7tel", "traum", "leicht", "au\u00df", "der", "acht", "hin\u00b7f\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "NN", "ADJD", "APPR", "ART", "CARD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd wie ein strom verscheust/ den keine macht auffh\u00e4lt/", "tokens": ["Vnd", "wie", "ein", "strom", "ver\u00b7scheust", "/", "den", "kei\u00b7ne", "macht", "auff\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$(", "ART", "PIAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df auch vnser Nahm/ Lob Ehr vnd ruhm verschwind&#275;. ", "tokens": ["So", "mu\u00df", "auch", "vn\u00b7ser", "Nahm", "/", "Lob", "Ehr", "vnd", "ruhm", "ver\u00b7schwind", "&#275;", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$(", "NN", "NN", "KON", "NN", "ADJD", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was jtzund athem holt/ mu\u00df mit der lust entflihn/", "tokens": ["Was", "jt\u00b7zund", "at\u00b7hem", "holt", "/", "mu\u00df", "mit", "der", "lust", "ent\u00b7flihn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "VVFIN", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was nach vns kom\u0303en wird/ wird vns ins grab nach zihn.", "tokens": ["Was", "nach", "vns", "kom\u0303en", "wird", "/", "wird", "vns", "ins", "grab", "nach", "zihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVINF", "VAFIN", "$(", "VAFIN", "PPER", "APPRART", "NN", "APPR", "VVINF", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Was sag ich? wir vergehn wie Rauch von starcken winden. ", "tokens": ["Was", "sag", "ich", "?", "wir", "ver\u00b7gehn", "wie", "Rauch", "von", "star\u00b7cken", "win\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVINF", "KOKOM", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}