{"textgrid.poem.37592": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der unversch\u00e4mte Igel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Baumstamm, unten hohl,", "tokens": ["In", "ei\u00b7nem", "Baum\u00b7stamm", ",", "un\u00b7ten", "hohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sitzt der Has und f\u00fchlt sich wohl.", "tokens": ["Da", "sitzt", "der", "Has", "und", "f\u00fchlt", "sich", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Igel, dem der Ort gefiel,", "tokens": ["Ein", "I\u00b7gel", ",", "dem", "der", "Ort", "ge\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht auch hinein, weil's drau\u00dfen k\u00fchl.", "tokens": ["M\u00f6cht", "auch", "hin\u00b7ein", ",", "weil's", "drau\u00b7\u00dfen", "k\u00fchl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKVZ", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Hase, voller G\u00fctigkeit,", "tokens": ["Der", "Ha\u00b7se", ",", "vol\u00b7ler", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht h\u00f6flich Platz und r\u00fcckt beiseit.", "tokens": ["Macht", "h\u00f6f\u00b7lich", "Platz", "und", "r\u00fcckt", "bei\u00b7seit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch kaum ist er beiseit ger\u00fcckt,", "tokens": ["Doch", "kaum", "ist", "er", "bei\u00b7seit", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "F\u00fchlt er bereits, da\u00df ihn was prickt.", "tokens": ["F\u00fchlt", "er", "be\u00b7reits", ",", "da\u00df", "ihn", "was", "prickt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und pl\u00f6tzlich so geprickelt hat's,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "so", "ge\u00b7pri\u00b7ckelt", "hat's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er mu\u00df heraus mit einem Satz.", "tokens": ["Er", "mu\u00df", "he\u00b7raus", "mit", "ei\u00b7nem", "Satz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Oh, unversch\u00e4mtes Stacheltier!", "tokens": ["Oh", ",", "un\u00b7ver\u00b7sch\u00e4m\u00b7tes", "Sta\u00b7chel\u00b7tier", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aha, der Fuchs ist auch schon hier.", "tokens": ["A\u00b7ha", ",", "der", "Fuchs", "ist", "auch", "schon", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NE", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Zwar gleich macht sich der Igel dick", "tokens": ["Zwar", "gleich", "macht", "sich", "der", "I\u00b7gel", "dick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zieht sich in sich selbst zur\u00fcck.", "tokens": ["Und", "zieht", "sich", "in", "sich", "selbst", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Fuchs, der gern den Igel fri\u00dft,", "tokens": ["Der", "Fuchs", ",", "der", "gern", "den", "I\u00b7gel", "fri\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df aber, was zu machen ist.", "tokens": ["Wei\u00df", "a\u00b7ber", ",", "was", "zu", "ma\u00b7chen", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wei\u00df, wie man ihn fassen kann,", "tokens": ["Und", "wei\u00df", ",", "wie", "man", "ihn", "fas\u00b7sen", "kann", ","], "token_inf