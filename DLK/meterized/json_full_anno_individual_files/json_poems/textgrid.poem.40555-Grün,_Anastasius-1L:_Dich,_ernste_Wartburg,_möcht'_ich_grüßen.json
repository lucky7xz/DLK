{"textgrid.poem.40555": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich, ernste Wartburg, m\u00f6cht' ich gr\u00fc\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, ernste Wartburg, m\u00f6cht' ich gr\u00fc\u00dfen", "tokens": ["Dich", ",", "erns\u00b7te", "Wart\u00b7burg", ",", "m\u00f6cht'", "ich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Fr\u00fchlings Burg zu aller Frist,", "tokens": ["Als", "Fr\u00fch\u00b7lings", "Burg", "zu", "al\u00b7ler", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da deutschen Lenz treu zu umschlie\u00dfen", "tokens": ["Da", "deut\u00b7schen", "Lenz", "treu", "zu", "um\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freist\u00e4tt' und Liebeshort du bist!", "tokens": ["Frei\u00b7st\u00e4tt'", "und", "Lie\u00b7bes\u00b7hort", "du", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In dichter W\u00e4lder dunklem Rahmen", "tokens": ["In", "dich\u00b7ter", "W\u00e4l\u00b7der", "dunk\u00b7lem", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wahrst du ein lichtes Fr\u00fchlingsbild,", "tokens": ["Wahrst", "du", "ein", "lich\u00b7tes", "Fr\u00fch\u00b7lings\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Allen, die zu dir je kamen,", "tokens": ["Da\u00df", "Al\u00b7len", ",", "die", "zu", "dir", "je", "ka\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lenzahnung s\u00fc\u00df im Herzen quillt.", "tokens": ["Len\u00b7zah\u00b7nung", "s\u00fc\u00df", "im", "Her\u00b7zen", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "War's nicht in deinen luft'gen Hallen,", "tokens": ["Wa\u00b7r's", "nicht", "in", "dei\u00b7nen", "luft'\u00b7gen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo einst in alter Zeit erwacht,", "tokens": ["Wo", "einst", "in", "al\u00b7ter", "Zeit", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Leu-gewordne Nachtigallen,", "tokens": ["Wie", "Leu\u00b7ge\u00b7word\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Rauschen einer Liederschlacht?", "tokens": ["Das", "Rau\u00b7schen", "ei\u00b7ner", "Lie\u00b7der\u00b7schlacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein sch\u00f6nes K\u00e4mpfen, wo der Sieger", "tokens": ["Ein", "sch\u00f6\u00b7nes", "K\u00e4mp\u00b7fen", ",", "wo", "der", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wohllaut s\u00fc\u00df den Gegner l\u00e4hmt", "tokens": ["Mit", "Wohl\u00b7laut", "s\u00fc\u00df", "den", "Geg\u00b7ner", "l\u00e4hmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den besiegten schw\u00e4chern Krieger", "tokens": ["Und", "den", "be\u00b7sieg\u00b7ten", "schw\u00e4\u00b7chern", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wonne g\u00f6ttlich \u00fcberstr\u00f6mt!", "tokens": ["Mit", "Won\u00b7ne", "g\u00f6tt\u00b7lich", "\u00fc\u00b7ber\u00b7str\u00f6mt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Fels, dran los die Donnerwolke,", "tokens": ["Du", "Fels", ",", "dran", "los", "die", "Don\u00b7ner\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lenzgewitter, Luther, brach,", "tokens": ["Das", "Lenz\u00b7ge\u00b7wit\u00b7ter", ",", "Lu\u00b7ther", ",", "brach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da der Prophet zu seinem Volke", "tokens": ["Da", "der", "Pro\u00b7phet", "zu", "sei\u00b7nem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verh\u00fcllt aus Wolkenschleiern sprach!", "tokens": ["Ver\u00b7h\u00fcllt", "aus", "Wol\u00b7ken\u00b7schlei\u00b7ern", "sprach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Wetter hat gereint, durchsch\u00fcttert", "tokens": ["Das", "Wet\u00b7ter", "hat", "ge\u00b7reint", ",", "durch\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmel, da\u00df er heller blaut,", "tokens": ["Den", "Him\u00b7mel", ",", "da\u00df", "er", "hel\u00b7ler", "blaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch morsches Haus in Grund gesplittert", "tokens": ["Manch", "mor\u00b7sches", "Haus", "in", "Grund", "ge\u00b7split\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df fester, sch\u00f6ner man's erbaut!", "tokens": ["Da\u00df", "fes\u00b7ter", ",", "sch\u00f6\u00b7ner", "man's", "er\u00b7baut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du Steinwand, dran in sp\u00e4tern Tagen", "tokens": ["Du", "Stein\u00b7wand", ",", "dran", "in", "sp\u00e4\u00b7tern", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jugend \u00fcpp'ger Rebenspro\u00df", "tokens": ["Der", "Ju\u00b7gend", "\u00fcpp'\u00b7ger", "Re\u00b7bens\u00b7pro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lenzungeduldig ausgeschlagen,", "tokens": ["Len\u00b7zun\u00b7ge\u00b7dul\u00b7dig", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Lenz\u00fcberm\u00fcthig frei aufscho\u00df!", "tokens": ["Lenz\u00b7\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", "frei", "auf\u00b7scho\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Die Rebe wollt' im Keime spr\u00fchen", "tokens": ["Die", "Re\u00b7be", "wollt'", "im", "Kei\u00b7me", "spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Fr\u00fcchten, die dem Herbst gespart!", "tokens": ["Von", "Fr\u00fcch\u00b7ten", ",", "die", "dem", "Herbst", "ge\u00b7spart", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Edelreis, das nicht im Bl\u00fchen", "tokens": ["Kein", "E\u00b7del\u00b7reis", ",", "das", "nicht", "im", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon k\u00fcnft'ger Frucht Bewu\u00dftsein wahrt!", "tokens": ["Schon", "k\u00fcnft'\u00b7ger", "Frucht", "Be\u00b7wu\u00df\u00b7tsein", "wahrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch jetzt kein Fr\u00fchlingslied mehr fl\u00f6tet,", "tokens": ["Doch", "jetzt", "kein", "Fr\u00fch\u00b7lings\u00b7lied", "mehr", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Bl\u00fchn wagt sich zur Marmorflur;", "tokens": ["Kein", "Bl\u00fchn", "wagt", "sich", "zur", "Mar\u00b7mor\u00b7flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Lenz hat selbst den Lenz get\u00f6dtet,", "tokens": ["Der", "Lenz", "hat", "selbst", "den", "Lenz", "ge\u00b7t\u00f6d\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gras s\u00e4end auf der Edlen Spur.", "tokens": ["Gras", "s\u00e4\u00b7end", "auf", "der", "Ed\u00b7len", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie Polens Reichstag, als zerstoben", "tokens": ["Wie", "Po\u00b7lens", "Reichs\u00b7tag", ",", "als", "zer\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "NN", "$,", "KOUS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Heer, im fremden Lande doch", "tokens": ["Sein", "Heer", ",", "im", "frem\u00b7den", "Lan\u00b7de", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Treu hielt zusammen, gotterhoben:", "tokens": ["Treu", "hielt", "zu\u00b7sam\u00b7men", ",", "got\u00b7ter\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da Polen nicht verloren noch!", "tokens": ["Da", "Po\u00b7len", "nicht", "ver\u00b7lo\u00b7ren", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So schaarten Fr\u00fchlings Auserkorne", "tokens": ["So", "schaar\u00b7ten", "Fr\u00fch\u00b7lings", "Au\u00b7ser\u00b7kor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen hier sich bald aufs neu',", "tokens": ["Die", "Blu\u00b7men", "hier", "sich", "bald", "aufs", "neu'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Lenz, der noch nicht ganz verlorne,", "tokens": ["Da\u00df", "Lenz", ",", "der", "noch", "nicht", "ganz", "ver\u00b7lor\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich guter Stellvertreter freu'.", "tokens": ["Sich", "gu\u00b7ter", "Stell\u00b7ver\u00b7tre\u00b7ter", "freu'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Da stehn sie, h\u00fctend seine Krone,", "tokens": ["Da", "stehn", "sie", ",", "h\u00fc\u00b7tend", "sei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Feuerw\u00e4chters Gartenplan:", "tokens": ["In", "Feu\u00b7er\u00b7w\u00e4ch\u00b7ters", "Gar\u00b7ten\u00b7plan", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch hat der Mann die L\u00e4rmkanone", "tokens": ["Doch", "hat", "der", "Mann", "die", "L\u00e4rm\u00b7ka\u00b7no\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hart aufgefahren nebendran;", "tokens": ["Hart", "auf\u00b7ge\u00b7fah\u00b7ren", "ne\u00b7ben\u00b7dran", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da\u00df nimmer Feuersnoth emp\u00f6re", "tokens": ["Da\u00df", "nim\u00b7mer", "Feu\u00b7ers\u00b7noth", "em\u00b7p\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das liebe St\u00e4dtchen Eisenach,", "tokens": ["Das", "lie\u00b7be", "St\u00e4dt\u00b7chen", "Ei\u00b7se\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den tiefen Waldesfrieden st\u00f6re,", "tokens": ["Den", "tie\u00b7fen", "Wal\u00b7des\u00b7frie\u00b7den", "st\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der es umw\u00f6lbt mit gr\u00fcnem Dach!", "tokens": ["Der", "es", "um\u00b7w\u00f6lbt", "mit", "gr\u00fc\u00b7nem", "Dach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der eh'rne Nachbar d\u00fcnkt erschreckend", "tokens": ["Der", "eh'r\u00b7ne", "Nach\u00b7bar", "d\u00fcnkt", "er\u00b7schre\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl eben nicht den Blumenbund;", "tokens": ["Wohl", "e\u00b7ben", "nicht", "den", "Blu\u00b7men\u00b7bund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mohnk\u00f6pfe sp\u00e4hn, empor sich streckend,", "tokens": ["Mohn\u00b7k\u00f6p\u00b7fe