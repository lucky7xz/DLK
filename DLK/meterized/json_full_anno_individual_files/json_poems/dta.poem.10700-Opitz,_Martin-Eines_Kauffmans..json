{"dta.poem.10700": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Eines Kauffmans.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ich machte rechnung wohl/ es k\u00f6nte mirnicht fehlen/", "tokens": ["Ich", "mach\u00b7te", "rech\u00b7nung", "wohl", "/", "es", "k\u00f6n\u00b7te", "mir\u00b7nicht", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PPER", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wolte richtig Gelt f\u00fcr mein Credit au\u00dfzehlen/", "tokens": ["Ich", "wol\u00b7te", "rich\u00b7tig", "Gelt", "f\u00fcr", "mein", "Cre\u00b7dit", "au\u00df\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df Todes ich verga\u00df/ der dan\u0303 ohn all gedult", "tokens": ["De\u00df", "To\u00b7des", "ich", "ver\u00b7ga\u00df", "/", "der", "da\u00f1", "ohn", "all", "ge\u00b7dult"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$(", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich balt verarrestirt allem vmb seine Schult.", "tokens": ["Mich", "balt", "ver\u00b7ar\u00b7res\u00b7tirt", "al\u00b7lem", "vmb", "sei\u00b7ne", "Schult", "."], "token_info": ["word", "word", "word", "word", "word", "wor