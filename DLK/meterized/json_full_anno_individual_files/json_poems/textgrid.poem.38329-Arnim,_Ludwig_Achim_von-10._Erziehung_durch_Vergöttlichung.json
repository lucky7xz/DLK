": "iambic.penta"}, "line.5": {"text": "Das macht, er kennt nicht Gottes Wunderhand", "tokens": ["Das", "macht", ",", "er", "kennt", "nicht", "Got\u00b7tes", "Wun\u00b7der\u00b7hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In diesem Band.", "tokens": ["In", "die\u00b7sem", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Darum versteckt", "tokens": ["Da\u00b7rum", "ver\u00b7steckt"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Herr, was er erweckt,", "tokens": ["Der", "Herr", ",", "was", "er", "er\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder gehn nur immer im Verborgen,", "tokens": ["Die", "Kin\u00b7der", "gehn", "nur", "im\u00b7mer", "im", "Ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die doch noch kein Gericht besorgen;", "tokens": ["Die", "doch", "noch", "kein", "Ge\u00b7richt", "be\u00b7sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis endlich Gott die Herrlichkeit entdeckt,", "tokens": ["Bis", "end\u00b7lich", "Gott", "die", "Herr\u00b7lich\u00b7keit", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die war versteckt.", "tokens": ["Die", "war", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "So wandelt er", "tokens": ["So", "wan\u00b7delt", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im Heiligthum umher,", "tokens": ["Im", "Hei\u00b7lig\u00b7thum", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit leisem Schritt, der kann ihn nicht vernehmen,", "tokens": ["Mit", "lei\u00b7sem", "Schritt", ",", "der", "kann", "ihn", "nicht", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer sich zur Einfalt nicht will ganz bequemen,", "tokens": ["Wer", "sich", "zur", "Ein\u00b7falt", "nicht", "will", "ganz", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "PTKNEG", "VMFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie er sonst nichts zu thun pflegt ohngef\u00e4hr,", "tokens": ["Wie", "er", "sonst", "nichts", "zu", "thun", "pflegt", "ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wandelt er.", "tokens": ["So", "wan\u00b7delt", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}