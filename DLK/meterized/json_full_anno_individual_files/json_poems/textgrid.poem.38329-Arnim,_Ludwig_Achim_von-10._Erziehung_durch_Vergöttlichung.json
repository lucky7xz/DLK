{"textgrid.poem.38329": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "10. Erziehung durch Verg\u00f6ttlichung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verborgenheit!", "tokens": ["Ver\u00b7bor\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie ist dein Meer so breit", "tokens": ["Wie", "ist", "dein", "Meer", "so", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wundertief, ich kann es nicht ergr\u00fcnden,", "tokens": ["Und", "wun\u00b7der\u00b7tief", ",", "ich", "kann", "es", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Man wei\u00df kein Maa\u00df, noch Ziel, noch End zu finden,", "tokens": ["Man", "wei\u00df", "kein", "Maa\u00df", ",", "noch", "Ziel", ",", "noch", "End", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So lang man ist in der Verg\u00e4nglichkeit,", "tokens": ["So", "lang", "man", "ist", "in", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verborgenheit.", "tokens": ["Ver\u00b7bor\u00b7gen\u00b7heit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Herrlichkeit,", "tokens": ["Die", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die du hast allbereit,", "tokens": ["Die", "du", "hast", "all\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Kindern deiner Lieb hier beygeleget,", "tokens": ["Den", "Kin\u00b7dern", "dei\u00b7ner", "Lieb", "hier", "bey\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist sonderlich. Wer dies Geheimni\u00df heget,", "tokens": ["Ist", "son\u00b7der\u00b7lich", ".", "Wer", "dies", "Ge\u00b7heim\u00b7ni\u00df", "he\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PWS", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der tr\u00e4gt in sich auch zur elendsten Zeit", "tokens": ["Der", "tr\u00e4gt", "in", "sich", "auch", "zur", "e\u00b7lends\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PRF", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Herrlichkeit.", "tokens": ["Die", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du selber bist", "tokens": ["Du", "sel\u00b7ber", "bist"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Brunn, der ihnen ist", "tokens": ["Der", "Brunn", ",", "der", "ih\u00b7nen", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ihrem Geist zum steten Heil entsprungen,", "tokens": ["In", "ih\u00b7rem", "Geist", "zum", "ste\u00b7ten", "Heil", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch dich ist ihnen manches Werk gelungen;", "tokens": ["Durch", "dich", "ist", "ih\u00b7nen", "man\u00b7ches", "Werk", "ge\u00b7lun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch leidets nicht so mancher falsche Christ,", "tokens": ["Doch", "lei\u00b7dets", "nicht", "so", "man\u00b7cher", "fal\u00b7sche", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df selbst du's bist.", "tokens": ["Da\u00df", "selbst", "du's", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Liebe Band", "tokens": ["Der", "Lie\u00b7be", "Band"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist vielen unbekannt;", "tokens": ["Ist", "vie\u00b7len", "un\u00b7be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie segnet sich der Geitzige im Herzen,", "tokens": ["Wie", "seg\u00b7net", "sich", "der", "Geit\u00b7zi\u00b7ge", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er mit Geld die Christen siehet scherzen;", "tokens": ["Wenn", "er", "mit", "Geld", "die", "Chris\u00b7ten", "sie\u00b7het", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das macht, er kennt nicht Gottes Wunderhand", "tokens": ["Das", "macht", ",", "er", "kennt", "nicht", "Got\u00b7tes", "Wun\u00b7der\u00b7hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In diesem Band.", "tokens": ["In", "die\u00b7sem", "Band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Darum versteckt", "tokens": ["Da\u00b7rum", "ver\u00b7steckt"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Herr, was er erweckt,", "tokens": ["Der", "Herr", ",", "was", "er", "er\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder gehn nur immer im Verborgen,", "tokens": ["Die", "Kin\u00b7der", "gehn", "nur", "im\u00b7mer", "im", "Ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die doch noch kein Gericht besorgen;", "tokens": ["Die", "doch", "noch", "kein", "Ge\u00b7richt", "be\u00b7sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis endlich Gott die Herrlichkeit entdeckt,", "tokens": ["Bis", "end\u00b7lich", "Gott", "die", "Herr\u00b7lich\u00b7keit", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die war versteckt.", "tokens": ["Die", "war", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "So wandelt er", "tokens": ["So", "wan\u00b7delt", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im Heiligthum umher,", "tokens": ["Im", "Hei\u00b7lig\u00b7thum", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit leisem Schritt, der kann ihn nicht vernehmen,", "tokens": ["Mit", "lei\u00b7sem", "Schritt", ",", "der", "kann", "ihn", "nicht", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer sich zur Einfalt nicht will ganz bequemen,", "tokens": ["Wer", "sich", "zur", "Ein\u00b7falt", "nicht", "will", "ganz", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "PTKNEG", "VMFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie er sonst nichts zu thun pflegt ohngef\u00e4hr,", "tokens": ["Wie", "er", "sonst", "nichts", "zu", "thun", "pflegt", "ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wandelt er.", "tokens": ["So", "wan\u00b7delt", "er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Verborgenheit!", "tokens": ["Ver\u00b7bor\u00b7gen\u00b7heit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie ist dein Meer so breit", "tokens": ["Wie", "ist", "dein", "Meer", "so", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wundertief, ich kann es nicht ergr\u00fcnden,", "tokens": ["Und", "wun\u00b7der\u00b7tief", ",", "ich", "kann", "es", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "wo