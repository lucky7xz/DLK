{"dta.poem.3351": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh! Opitz steht voran: Sein Geist kennt keine", "tokens": ["Sieh", "!", "O\u00b7pitz", "steht", "vo\u00b7ran", ":", "Sein", "Geist", "kennt", "kei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schranken:", "tokens": ["Schran\u00b7ken", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Natur ist, was er denkt, und was er schreibt, Gedan-", "tokens": ["Na\u00b7tur", "ist", ",", "was", "er", "denkt", ",", "und", "was", "er", "schreibt", ",", "Ge\u00b7dan"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ken:", "tokens": ["ken", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Er sang, unsterblicher Gesang!", "tokens": ["Er", "sang", ",", "uns\u00b7terb\u00b7li\u00b7cher", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beseelt von einem sanften Feuer,", "tokens": ["Be\u00b7seelt", "von", "ei\u00b7nem", "sanf\u00b7ten", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch rauh, doch m\u00e4nnlich sch\u00f6n, in seine neue Leyer:", "tokens": ["Noch", "rauh", ",", "doch", "m\u00e4nn\u00b7lich", "sch\u00f6n", ",", "in", "sei\u00b7ne", "neu\u00b7e", "Le\u00b7yer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Da dessen fl\u00fcchtig Lied, der bis zum Tigris drang,", "tokens": ["Da", "des\u00b7sen", "fl\u00fcch\u00b7tig", "Lied", ",", "der", "bis", "zum", "Tig\u00b7ris", "drang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJD", "NN", "$,", "PRELS", "APPR", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Oft k\u00fchner, \u00f6fter schwach erklang.", "tokens": ["Oft", "k\u00fch\u00b7ner", ",", "\u00f6f\u00b7ter", "schwach", "er\u00b7klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie richtig sprach, wie edel dachte", "tokens": ["Wie", "rich\u00b7tig", "sprach", ",", "wie", "e\u00b7del", "dach\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der weise Hofmann an der Spree,", "tokens": ["Der", "wei\u00b7se", "Hof\u00b7mann", "an", "der", "Spree", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Um den, in Blumbergs weichem Klee,", "tokens": ["Um", "den", ",", "in", "Blum\u00b7bergs", "wei\u00b7chem", "Klee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein wohlgezogner Satyr lachte!", "tokens": ["Ein", "wohl\u00b7ge\u00b7zog\u00b7ner", "Sa\u00b7tyr", "lach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sieh einen Menschenfreund, um reicher Elbe Strand,", "tokens": ["Sieh", "ei\u00b7nen", "Men\u00b7schen\u00b7freund", ",", "um", "rei\u00b7cher", "El\u00b7be", "Strand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUI", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Von reger Phantasie entbrannt,", "tokens": ["Von", "re\u00b7ger", "Phan\u00b7ta\u00b7sie", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.16": {"text": "Sein irdisches Vergn\u00fcgen mahlen,", "tokens": ["Sein", "ir\u00b7di\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wo doch der \u00fcbereilten Hand", "tokens": ["Wo", "doch", "der", "\u00fc\u00b7be\u00b7reil\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Manch schwacher Zug entwischt, oft falsche Farben", "tokens": ["Manch", "schwa\u00b7cher", "Zug", "ent\u00b7wischt", ",", "oft", "fal\u00b7sche", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "prahlen.", "tokens": ["prah\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Bey Popen steht ein gro\u00dfer Mann,", "tokens": ["Bey", "Po\u00b7pen", "steht", "ein", "gro\u00b7\u00dfer", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der auf der Alpen Lob im Schnee der Alpen sann:", "tokens": ["Der", "auf", "der", "Al\u00b7pen", "Lob", "im", "Schnee", "der", "Al\u00b7pen", "sann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Des neuen Ausdrucks Glanz umleuchtet weise Lehren;", "tokens": ["Des", "neu\u00b7en", "Aus\u00b7drucks", "Glanz", "um\u00b7leuch\u00b7tet", "wei\u00b7se", "Leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und stimmt sein Saitenspiel ein feurig Straflied an,", "tokens": ["Und", "stimmt", "sein", "Sai\u00b7ten\u00b7spiel", "ein", "feu\u00b7rig", "Straf\u00b7lied", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wer wird nicht seinen Schwung, den edlen Schwung", "tokens": ["Wer", "wird", "nicht", "sei\u00b7nen", "Schwung", ",", "den", "ed\u00b7len", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "verehren,", "tokens": ["ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Und harte T\u00f6ne gern verh\u00f6ren?", "tokens": ["Und", "har\u00b7te", "T\u00f6\u00b7ne", "gern", "ver\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mit ihm schwingt am entfernten Belt", "tokens": ["Mit", "ihm", "schwingt", "am", "ent\u00b7fern\u00b7ten", "Belt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Ein angenehmer Geist sein gl\u00e4nzendes Gefieder:", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Geist", "sein", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7fie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nie fliegt er bis zum P\u00f6bel nieder:", "tokens": ["Nie", "fliegt", "er", "bis", "zum", "P\u00f6\u00b7bel", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Er unterrichtet, er gef\u00e4llt", "tokens": ["Er", "un\u00b7ter\u00b7rich\u00b7tet", ",", "er", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Dem Weisen, wie der gro\u00dfen Welt", "tokens": ["Dem", "Wei\u00b7sen", ",", "wie", "der", "gro\u00b7\u00dfen", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Jm feinen Scherz der sch\u00f6nsten Lieder", "tokens": ["Jm", "fei\u00b7nen", "Scherz", "der", "sch\u00f6ns\u00b7ten", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und im Johann, dem Seifensieder.", "tokens": ["Und", "im", "Jo\u00b7hann", ",", "dem", "Sei\u00b7fen\u00b7sie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Auch dieser junge Greis, der aller Freude Feind,", "tokens": ["Auch", "die\u00b7ser", "jun\u00b7ge", "Greis", ",", "der", "al\u00b7ler", "Freu\u00b7de", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Umw\u00f6lkt mit kranker Schwermuth, scheint,", "tokens": ["Um\u00b7w\u00f6lkt", "mit", "kran\u00b7ker", "Schwer\u00b7muth", ",", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Hat mit so heitrem Witz erzehlet,", "tokens": ["Hat", "mit", "so", "heit\u00b7rem", "Witz", "er\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Da\u00df, wenn die Fabel spricht, sie seine Sprache w\u00e4h-", "tokens": ["Da\u00df", ",", "wenn", "die", "Fa\u00b7bel", "spricht", ",", "sie", "sei\u00b7ne", "Spra\u00b7che", "w\u00e4h"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "let.", "tokens": ["let", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.39": {"text": "Doch, ach! Melpomene beweint", "tokens": ["Doch", ",", "ach", "!", "Mel\u00b7po\u00b7me\u00b7ne", "be\u00b7weint"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "NE", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.40": {"text": "Dich, welcher im Canut ihr Thr\u00e4nen einst entrissen:", "tokens": ["Dich", ",", "wel\u00b7cher", "im", "Ca\u00b7nut", "ihr", "Thr\u00e4\u00b7nen", "einst", "ent\u00b7ris\u00b7sen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie selbst hat ihren jungen Freund", "tokens": ["Sie", "selbst", "hat", "ih\u00b7ren", "jun\u00b7gen", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "In Marmor aufgestellt, bethr\u00e4nt mit ihren K\u00fcssen.", "tokens": ["In", "Mar\u00b7mor", "auf\u00b7ge\u00b7stellt", ",", "be\u00b7thr\u00e4nt", "mit", "ih\u00b7ren", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Dem, dessen sanfter Sch\u00e4fer-Ton", "tokens": ["Dem", ",", "des\u00b7sen", "sanf\u00b7ter", "Sch\u00e4\u00b7fer\u00b7Ton"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Die feinste Schalkheit deckt, da seine leichten Saiten", "tokens": ["Die", "feins\u00b7te", "Schalk\u00b7heit", "deckt", ",", "da", "sei\u00b7ne", "leich\u00b7ten", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Selbst mit Fontainens Leyer streiten;", "tokens": ["Selbst", "mit", "Fon\u00b7tai\u00b7nens", "Le\u00b7yer", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.46": {"text": "Und deinem alten Freund, Berlins Anakreon,", "tokens": ["Und", "dei\u00b7nem", "al\u00b7ten", "Freund", ",", "Ber\u00b7lins", "A\u00b7nak\u00b7re\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Den alle Grazien begleiten,", "tokens": ["Den", "al\u00b7le", "Gra\u00b7zi\u00b7en", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "L\u00e4sst Amor ihren Ort beym Tejer zubereiten.", "tokens": ["L\u00e4sst", "A\u00b7mor", "ih\u00b7ren", "Ort", "beym", "Te\u00b7jer", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "An seiner Seite wird noch einem seiner Art,", "tokens": ["An", "sei\u00b7ner", "Sei\u00b7te", "wird", "noch", "ei\u00b7nem", "sei\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Dem Vater holder Kleinigkeiten,", "tokens": ["Dem", "Va\u00b7ter", "hol\u00b7der", "Klei\u00b7nig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Ein ehrenvoller Platz bewahrt.", "tokens": ["Ein", "eh\u00b7ren\u00b7vol\u00b7ler", "Platz", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}