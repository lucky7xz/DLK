{"dta.poem.22414": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "218. Die Wei\u00dfheit wird gemeistert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": " die Wei\u00dfheit tadelt nichts: sie aber mu\u00df allein/", "tokens": ["die", "Wei\u00df\u00b7heit", "ta\u00b7delt", "nichts", ":", "sie", "a\u00b7ber", "mu\u00df", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$.", "PPER", "ADV", "VMFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von jhrer Creatur so offt getadelt sein.", "tokens": ["Von", "jhrer", "Crea\u00b7tur", "so", "offt", "ge\u00b7ta\u00b7delt", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}