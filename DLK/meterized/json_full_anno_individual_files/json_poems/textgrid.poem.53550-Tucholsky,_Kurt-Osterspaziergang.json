a"}}, "stanza.8": {"line.1": {"text": "ist ehrenvoll und ist Gewinn;", "tokens": ["ist", "eh\u00b7ren\u00b7voll", "und", "ist", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": " doch w\u00fcrd ich nicht allein mich her verlieren,", "tokens": ["doch", "w\u00fcrd", "ich", "nicht", "al\u00b7lein", "mich", "her", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "PRF", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": " weil ich ein Feind von allem Rohen bin.", "tokens": ["weil", "ich", "ein", "Feind", "von", "al\u00b7lem", "Ro\u00b7hen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": " das Schreien und Sozialisieren", "tokens": ["das", "Schrei\u00b7en", "und", "So\u00b7zi\u00b7a\u00b7li\u00b7sie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": " ist mir ein gar verha\u00dfter Klang;", "tokens": ["ist", "mir", "ein", "gar", "ver\u00b7ha\u00df\u00b7ter", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.6": {"text": " das will ja nur das Volk verf\u00fchren \u2013", "tokens": ["das", "will", "ja", "nur", "das", "Volk", "ver\u00b7f\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": " uns Reichen wird ganz angst und bang.", "tokens": ["uns", "Rei\u00b7chen", "wird", "ganz", "angst", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": " wir wollen wieder die alten Zeiten,", "tokens": ["wir", "wol\u00b7len", "wie\u00b7der", "die", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": " wir wollen wieder die Menge leiten \u2013", "tokens": ["wir", "wol\u00b7len", "wie\u00b7der", "die", "Men\u00b7ge", "lei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": " zufrieden jauchzt dann Gro\u00df und Klein:", "tokens": ["zu\u00b7frie\u00b7den", "jauchzt", "dann", "Gro\u00df", "und", "Klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": " ich bin kein Mensch! Ich darfs nicht sein!", "tokens": ["ich", "bin", "kein", "Mensch", "!", "Ich", "darfs", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}