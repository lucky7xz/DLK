{"textgrid.poem.53550": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Osterspaziergang", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "FaUST: Vom Eise befreit sind Strom und B\u00e4che", "tokens": ["FaUST", ":", "Vom", "Ei\u00b7se", "be\u00b7freit", "sind", "Strom", "und", "B\u00e4\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPRART", "NN", "VVPP", "VAFIN", "NN", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "durch des Fr\u00fchlings holden, belebenden Blick;", "tokens": ["durch", "des", "Fr\u00fch\u00b7lings", "hol\u00b7den", ",", "be\u00b7le\u00b7ben\u00b7den", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": " das deutsche Volk zahlt des Krieges Zeche,", "tokens": ["das", "deut\u00b7sche", "Volk", "zahlt", "des", "Krie\u00b7ges", "Ze\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": " und keiner bringt das Verlorene zur\u00fcck.", "tokens": ["und", "kei\u00b7ner", "bringt", "das", "Ver\u00b7lo\u00b7re\u00b7ne", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": " die alten Monarchen, in ihrer Schw\u00e4che,", "tokens": ["die", "al\u00b7ten", "Mon\u00b7ar\u00b7chen", ",", "in", "ih\u00b7rer", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": " zogen sich in die Versenkung zur\u00fcck.", "tokens": ["zo\u00b7gen", "sich", "in", "die", "Ver\u00b7sen\u00b7kung", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": " von dorther senden sie, fliehend nur,", "tokens": ["von", "dor\u00b7ther", "sen\u00b7den", "sie", ",", "flie\u00b7hend", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": " ohnm\u00e4chtige Schauer k\u00f6rniger Reden.", "tokens": ["ohn\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Schau\u00b7er", "k\u00f6r\u00b7ni\u00b7ger", "Re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": " und sie beschuldigen jeder jeden,", "tokens": ["und", "sie", "be\u00b7schul\u00b7di\u00b7gen", "je\u00b7der", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "PIS", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": " und sch\u00fctten Memoiren auf die Flur.", "tokens": ["und", "sch\u00fct\u00b7ten", "Me\u00b7moi\u00b7ren", "auf", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": " \u00fcberall regt sich G\u00e4rung und Streben.", "tokens": ["\u00fc\u00b7be\u00b7rall", "regt", "sich", "G\u00e4\u00b7rung", "und", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": " alles will sich mit Rot beleben.", "tokens": ["al\u00b7les", "will", "sich", "mit", "Rot", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": " doch an Blumen fehlts im Revier.", "tokens": ["doch", "an", "Blu\u00b7men", "fehlts", "im", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": " nehmt kompromittierte F\u00fchrer daf\u00fcr!", "tokens": ["nehmt", "kom\u00b7pro\u00b7mit\u00b7tier\u00b7te", "F\u00fch\u00b7rer", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PAV", "$."], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": " kehre dich um, von diesen H\u00f6hen", "tokens": ["keh\u00b7re", "dich", "um", ",", "von", "die\u00b7sen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PDAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": " auf das Land zur\u00fcck zu sehen.", "tokens": ["auf", "das", "Land", "zu\u00b7r\u00fcck", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": " aus dem hohlen, finstern Tor", "tokens": ["aus", "dem", "hoh\u00b7len", ",", "fins\u00b7tern", "Tor"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": " dringt ein buntes Gewimmel hervor.", "tokens": ["dringt", "ein", "bun\u00b7tes", "Ge\u00b7wim\u00b7mel", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": " die Kriegsgesellschaft, der Stahlkonzern,", "tokens": ["die", "Kriegs\u00b7ge\u00b7sell\u00b7schaft", ",", "der", "Stahl\u00b7kon\u00b7zern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": " denn sie sind wieder auferstanden", "tokens": ["denn", "sie", "sind", "wie\u00b7der", "auf\u00b7er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": " aus Reklamierungs- und andern Banden,", "tokens": ["aus", "Re\u00b7kla\u00b7mie\u00b7rungs", "und", "an\u00b7dern", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": " aus niedriger H\u00e4user dumpfen Gem\u00e4chern,", "tokens": ["aus", "nied\u00b7ri\u00b7ger", "H\u00e4u\u00b7ser", "dum\u00b7pfen", "Ge\u00b7m\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": " aus dem Druck von mitunter beschossenen D\u00e4chern,", "tokens": ["aus", "dem", "Druck", "von", "mi\u00b7tun\u00b7ter", "be\u00b7schos\u00b7se\u00b7nen", "D\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.23": {"text": " aus der Stra\u00dfen quietschender Enge,", "tokens": ["aus", "der", "Stra\u00b7\u00dfen", "quiet\u00b7schen\u00b7der", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": " aus der Kirchen ehrw\u00fcrdiger Nacht", "tokens": ["aus", "der", "Kir\u00b7chen", "ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": " sind sie wieder ans Licht gebracht.", "tokens": ["sind", "sie", "wie\u00b7der", "ans", "Licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": " sieh nur, sieh! wie behend sich die Menge", "tokens": ["sieh", "nur", ",", "sieh", "!", "wie", "be\u00b7hend", "sich", "die", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "VVIMP", "$.", "PWAV", "ADJD", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": " durch die D\u00f6rfer zum Hamstern schl\u00e4gt.", "tokens": ["durch", "die", "D\u00f6r\u00b7fer", "zum", "Hams\u00b7tern", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": " mancher bezieht manchmal etwas Senge,", "tokens": ["man\u00b7cher", "be\u00b7zieht", "manch\u00b7mal", "et\u00b7was", "Sen\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": " weil er zu wenig Geld hinlegt.", "tokens": ["weil", "er", "zu", "we\u00b7nig", "Geld", "hin\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": " hier f\u00fchl ich wahrhaft mich erhoben:", "tokens": ["hier", "f\u00fchl", "ich", "wahr\u00b7haft", "mich", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": " was k\u00fcmmert uns ein verlorener Krieg!", "tokens": ["was", "k\u00fcm\u00b7mert", "uns", "ein", "ver\u00b7lo\u00b7re\u00b7ner", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": " amerikanisches Mehl wird verschoben \u2013", "tokens": ["a\u00b7me\u00b7ri\u00b7ka\u00b7ni\u00b7sches", "Mehl", "wird", "ver\u00b7scho\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.33": {"text": " nur der Schieber reitet den Sieg!", "tokens": ["nur", "der", "Schie\u00b7ber", "rei\u00b7tet", "den", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.34": {"text": " h\u00e4tten wir nur genug zu essen,", "tokens": ["h\u00e4t\u00b7ten", "wir", "nur", "ge\u00b7nug", "zu", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.35": {"text": " w\u00e4r das Alte mit Gunst vergessen;", "tokens": ["w\u00e4r", "das", "Al\u00b7te", "mit", "Gunst", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": " ludendorffen entbieten wir Huld . . .", "tokens": ["lu\u00b7den\u00b7dorf\u00b7fen", "ent\u00b7bie\u00b7ten", "wir", "Huld", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "NN", "$.", "$.", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": " keiner ist schuld! Keiner ist schuld!", "tokens": ["kei\u00b7ner", "ist", "schuld", "!", "Kei\u00b7ner", "ist", "schuld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$.", "PIS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.38": {"text": " ich h\u00f6re schon des Dorfs Get\u00fcmmel,", "tokens": ["ich", "h\u00f6\u00b7re", "schon", "des", "Dorfs", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": " hier ist des Volkes wahrer Himmel.", "tokens": ["hier", "ist", "des", "Vol\u00b7kes", "wah\u00b7rer", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": " zufrieden jauchzt die Reaktion:", "tokens": ["zu\u00b7frie\u00b7den", "jauchzt", "die", "Re\u00b7ak\u00b7ti\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": " keine Angst! sie vergessen schon!", "tokens": ["kei\u00b7ne", "Angst", "!", "sie", "ver\u00b7ges\u00b7sen", "schon", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "WaGNER: Mit euch, Herr Doktor, zu spazieren", "tokens": ["WaG\u00b7NER", ":", "Mit", "euch", ",", "Herr", "Dok\u00b7tor", ",", "zu", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "APPR", "PPER", "$,", "NN", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "ist ehrenvoll und ist Gewinn;", "tokens": ["ist", "eh\u00b7ren\u00b7voll", "und", "ist", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": " doch w\u00fcrd ich nicht allein mich her verlieren,", "tokens": ["doch", "w\u00fcrd", "ich", "nicht", "al\u00b7lein", "mich", "her", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "PRF", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": " weil ich ein Feind von allem Rohen bin.", "tokens": ["weil", "ich", "ein", "Feind", "von", "al\u00b7lem", "Ro\u00b7hen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": " das Schreien und Sozialisieren", "tokens": ["das", "Schrei\u00b7en", "und", "So\u00b7zi\u00b7a\u00b7li\u00b7sie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": " ist mir ein gar verha\u00dfter Klang;", "tokens": ["ist", "mir", "ein", "gar", "ver\u00b7ha\u00df\u00b7ter", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.6": {"text": " das will ja nur das Volk verf\u00fchren \u2013", "tokens": ["das", "will", "ja", "nur", "das", "Volk", "ver\u00b7f\u00fch\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": " uns Reichen wird ganz angst und bang.", "tokens": ["uns", "Rei\u00b7chen", "wird", "ganz", "angst", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": " wir wollen wieder die alten Zeiten,", "tokens": ["wir", "wol\u00b7len", "wie\u00b7der", "die", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": " wir w