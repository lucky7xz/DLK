{"textgrid.poem.53174": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer der Jugend Kertzen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer der Jugend Kertzen", "tokens": ["Wer", "der", "Ju\u00b7gend", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tr\u00e4gt im frischen Hertzen,", "tokens": ["Tr\u00e4gt", "im", "fri\u00b7schen", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat zu tantzen Lust,", "tokens": ["Hat", "zu", "tant\u00b7zen", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Amor regt ohn Ende", "tokens": ["A\u00b7mor", "regt", "ohn", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihm die F\u00fc\u00df und H\u00e4nde", "tokens": ["Ihm", "die", "F\u00fc\u00df", "und", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd die junge Brust,", "tokens": ["Vnd", "die", "jun\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df er nimmer still kan stehn", "tokens": ["Da\u00df", "er", "nim\u00b7mer", "still", "kan", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd mu\u00df wie im Sprunge gehn,", "tokens": ["Vnd", "mu\u00df", "wie", "im", "Sprun\u00b7ge", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOKOM", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mu\u00df den Leib den Meeres-Wellen", "tokens": ["Mu\u00df", "den", "Leib", "den", "Mee\u00b7res\u00b7Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aehnlich stellen.", "tokens": ["A\u00b7ehn\u00b7lich", "stel\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie in strengen Kriegen", "tokens": ["Wie", "in", "stren\u00b7gen", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sieht ein Ro\u00df schon fliegen", "tokens": ["Sieht", "ein", "Ro\u00df", "schon", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die wild-k\u00fchne Fahn,", "tokens": ["Die", "wild\u00b7k\u00fch\u00b7ne", "Fahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "H\u00f6rt die Trommten klingen", "tokens": ["H\u00f6rt", "die", "Tromm\u00b7ten", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd die Kugeln singen,", "tokens": ["Vnd", "die", "Ku\u00b7geln", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "O wie geht es an!", "tokens": ["O", "wie", "geht", "es", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Es reckt seine M\u00e4hn empor,", "tokens": ["Es", "reckt", "sei\u00b7ne", "M\u00e4hn", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wiegert, strampffet, spitzt das Ohr,", "tokens": ["Wie\u00b7gert", ",", "strampf\u00b7fet", ",", "spitzt", "das", "Ohr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Vnd man k\u00f6nte Halt den winden,", "tokens": ["Vnd", "man", "k\u00f6n\u00b7te", "Halt", "den", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihm nicht, finden.", "tokens": ["Ihm", "nicht", ",", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Also junge Leute,", "tokens": ["Al\u00b7so", "jun\u00b7ge", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Amors Sieg und Beute,", "tokens": ["A\u00b7mors", "Sieg", "und", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "R\u00fchrt man ohn gefehr", "tokens": ["R\u00fchrt", "man", "ohn", "ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wo die s\u00fcssen Geigen,", "tokens": ["Wo", "die", "s\u00fcs\u00b7sen", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die die Sinne neigen,", "tokens": ["Die", "die", "Sin\u00b7ne", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gehen nicht einher,", "tokens": ["Ge\u00b7hen", "nicht", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sondern wallen immerzu,", "tokens": ["Son\u00b7dern", "wal\u00b7len", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein Baum, wenn ohne Rhu", "tokens": ["Wie", "ein", "Baum", ",", "wenn", "oh\u00b7ne", "Rhu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ein S\u00fcd-Ost, imfall er k\u00fchlet,", "tokens": ["Ein", "S\u00fcd\u00b7Ost", ",", "im\u00b7fall", "er", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Mit ihm spielet.", "tokens": ["Mit", "ihm", "spie\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Amor wil indessen", "tokens": ["A\u00b7mor", "wil", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seiner nicht vergessen,", "tokens": ["Sei\u00b7ner", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nimmt der Zeiten war,", "tokens": ["Nimmt", "der", "Zei\u00b7ten", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Er, das Vngehewer,", "tokens": ["Er", ",", "das", "Vn\u00b7ge\u00b7he\u00b7wer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Legt zu seinem Fewer", "tokens": ["Legt", "zu", "sei\u00b7nem", "Fe\u00b7wer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fleissig immerdar,", "tokens": ["Fleis\u00b7sig", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Dann wird alles liechte loh", "tokens": ["Dann", "wird", "al\u00b7les", "liech\u00b7te", "loh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie die Flamm' im d\u00fcrren Stroh,", "tokens": ["Wie", "die", "Flamm'", "im", "d\u00fcr\u00b7ren", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dann entstehn erst Heyraht-Schl\u00fcsse", "tokens": ["Dann", "ent\u00b7stehn", "erst", "Hey\u00b7raht\u00b7Sch\u00b7l\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.10": {"text": "Vnd viel K\u00fcsse.", "tokens": ["Vnd", "viel", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "F\u00fchrt dann wer im Reyen", "tokens": ["F\u00fchrt", "dann", "wer", "im", "Re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PWS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die er meint mit Trewen,", "tokens": ["Die", "er", "meint", "mit", "Tre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die durch Gegen-Gunst", "tokens": ["Die", "durch", "Ge\u00b7gen\u00b7Gunst"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mercklich ihn l\u00e4sst hoffen,", "tokens": ["Merck\u00b7lich", "ihn", "l\u00e4sst", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie sey auch getroffen,", "tokens": ["Sie", "sey", "auch", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dann erhebt sich Brunst,", "tokens": ["Dann", "er\u00b7hebt", "sich", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Dann wil er der erst allein", "tokens": ["Dann", "wil", "er", "der", "erst", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nur in jedem Tantze seyn,", "tokens": ["Nur", "in", "je\u00b7dem", "Tant\u00b7ze", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hat den Himmel, seinen Sinnen", "tokens": ["Hat", "den", "Him\u00b7mel", ",", "sei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nach, gar innen.", "tokens": ["Nach", ",", "gar", "in\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Kinder, strebt nach Frewden,", "tokens": ["Kin\u00b7der", ",", "strebt", "nach", "Frew\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Niemand wird euch neiden,", "tokens": ["Nie\u00b7mand", "wird", "euch", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nur der Erbarkeit", "tokens": ["Nur", "der", "Er\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vnd der Zucht indessen", "tokens": ["Vnd", "der", "Zucht", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Werde nicht vergessen,", "tokens": ["Wer\u00b7de", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lebt und liebt allzeit,", "tokens": ["Lebt", "und", "liebt", "all\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Flieht dabey auch M\u00fcssiggang,", "tokens": ["Flieht", "da\u00b7bey", "auch", "M\u00fcs\u00b7sig\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seyd der Wollust ernster Zwang,", "tokens": ["Seyd", "der", "Wol\u00b7lust", "erns\u00b7ter", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So wird ewer Werck f\u00fcr allen", "tokens": ["So", "wird", "e\u00b7wer", "Werck", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Gott gefallen.", "tokens": ["Gott", "ge\u00b7fal\u0