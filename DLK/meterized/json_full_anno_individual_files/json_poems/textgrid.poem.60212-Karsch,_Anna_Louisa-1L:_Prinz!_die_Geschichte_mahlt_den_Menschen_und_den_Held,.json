{"textgrid.poem.60212": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Prinz! die Geschichte mahlt den Menschen und den Held,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinz! die Geschichte mahlt den Menschen und den Held,", "tokens": ["Prinz", "!", "die", "Ge\u00b7schich\u00b7te", "mahlt", "den", "Men\u00b7schen", "und", "den", "Held", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den K\u00f6nig und die Unterthanen;", "tokens": ["Den", "K\u00f6\u00b7nig", "und", "die", "Un\u00b7ter\u00b7tha\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lehret dich von Rom, wie unter seine Fahnen", "tokens": ["Sie", "leh\u00b7ret", "dich", "von", "Rom", ",", "wie", "un\u00b7ter", "sei\u00b7ne", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es niederwarf die ganze Welt;", "tokens": ["Es", "nie\u00b7der\u00b7warf", "die", "gan\u00b7ze", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie zeigt dir Griechenland die Siegerhand erheben", "tokens": ["Sie", "zeigt", "dir", "Grie\u00b7chen\u00b7land", "die", "Sie\u00b7ger\u00b7hand", "er\u00b7