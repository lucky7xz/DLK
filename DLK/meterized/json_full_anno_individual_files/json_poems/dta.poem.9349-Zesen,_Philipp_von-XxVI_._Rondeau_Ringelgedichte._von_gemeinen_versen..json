{"dta.poem.9349": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XxVI .  \n Rondeau/ Ringelgedichte.  \n von gemeinen versen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist nicht blind/ du Gott der s\u00fc\u00dfen schmertze\u0303/", "tokens": ["Du", "bist", "nicht", "blind", "/", "du", "Gott", "der", "s\u00fc\u00b7\u00dfen", "schmertz\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "PPER", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich gleub es nicht weil du so nah zum hertzen", "tokens": ["Ich", "gleub", "es", "nicht", "weil", "du", "so", "nah", "zum", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die pfeile schie\u00dft/ und weil du so geschwind", "tokens": ["die", "pfei\u00b7le", "schie\u00dft", "/", "und", "weil", "du", "so", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die stra\u00dfe triffst; f\u00fcrwar ", "tokens": ["die", "stra\u00b7\u00dfe", "triffst", ";", "f\u00fcr\u00b7war"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Mahier treugt; kanstu so grade zielen/", "tokens": ["Der", "Ma\u00b7hier", "treugt", ";", "kans\u00b7tu", "so", "gra\u00b7de", "zie\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VMFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und auf der See mit deinen pfeilen spielen?", "tokens": ["und", "auf", "der", "See", "mit", "dei\u00b7nen", "pfei\u00b7len", "spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hastu den Mars mit Liebes gluth entz\u00fcndt?", "tokens": ["Has\u00b7tu", "den", "Mars", "mit", "Lie\u00b7bes", "gluth", "ent\u00b7z\u00fcndt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So sag\u2019 ich noch/ f\u00fcrwar ", "tokens": ["So", "sag'", "ich", "noch", "/", "f\u00fcr\u00b7war"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}