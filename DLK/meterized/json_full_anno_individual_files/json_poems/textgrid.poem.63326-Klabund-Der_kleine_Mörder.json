{"textgrid.poem.63326": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine M\u00f6rder", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er wu\u00dfte nicht, warum er so elend war", "tokens": ["Er", "wu\u00df\u00b7te", "nicht", ",", "wa\u00b7rum", "er", "so", "e\u00b7lend", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und warum der Himmel an jenem Abend so schwelend war.", "tokens": ["Und", "wa\u00b7rum", "der", "Him\u00b7mel", "an", "je\u00b7nem", "A\u00b7bend", "so", "schwe\u00b7lend", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PDAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sein Sch\u00e4deldeckel war aufgeklappt und Fliegen setzten sich auf sein rosiges Hirn", "tokens": ["Sein", "Sch\u00e4\u00b7del\u00b7de\u00b7ckel", "war", "auf\u00b7ge\u00b7klappt", "und", "Flie\u00b7gen", "setz\u00b7ten", "sich", "auf", "sein", "ro\u00b7si\u00b7ges", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "KON", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und leckten daran. G\u00f6ttliche Gedanken schienen ihn zu durchirr'n.", "tokens": ["Und", "leck\u00b7ten", "da\u00b7ran", ".", "G\u00f6tt\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", "schie\u00b7nen", "ihn", "zu", "durch\u00b7irr'", "n."], "tok