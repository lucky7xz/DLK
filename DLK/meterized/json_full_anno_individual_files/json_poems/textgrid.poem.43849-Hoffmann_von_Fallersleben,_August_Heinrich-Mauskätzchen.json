ne.3": {"text": "Ein T\u00e4nzchen mit Allen,", "tokens": ["Ein", "T\u00e4nz\u00b7chen", "mit", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das soll dir gefallen.", "tokens": ["Das", "soll", "dir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Erst sitz' ich, dann schleich' ich,", "tokens": ["Erst", "sitz'", "ich", ",", "dann", "schleich'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Dann nah' ich, dann weich' ich,", "tokens": ["Dann", "nah'", "ich", ",", "dann", "weich'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Dann leg' ich mich nieder,", "tokens": ["Dann", "leg'", "ich", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Dann heb' ich mich wieder.", "tokens": ["Dann", "heb'", "ich", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Kaum schwing' ich mein Schw\u00e4nzchen", "tokens": ["Kaum", "schwing'", "ich", "mein", "Schw\u00e4nz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PP