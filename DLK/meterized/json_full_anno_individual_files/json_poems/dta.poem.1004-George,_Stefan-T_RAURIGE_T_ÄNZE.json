{"dta.poem.1004": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht ist weise bis zur lezten frist               ", "tokens": ["Nicht", "ist", "wei\u00b7se", "bis", "zur", "lez\u00b7ten", "frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ADV", "APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu geniessen wo verg&#228;ngnis ist               ", "tokens": ["Zu", "ge\u00b7nies\u00b7sen", "wo", "verg", "&#228;", "ng\u00b7nis", "ist"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PTKZU", "VVINF", "PWAV", "NE", "$(", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "V\u00f6gel flogen s&#252;dw&#228;rts an die see               ", "tokens": ["V\u00f6\u00b7gel", "flo\u00b7gen", "s", "&#252;", "dw", "&#228;", "rts", "an", "die", "see"], "token_info": ["word", "word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "FM", "FM", "FM", "$(", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Blumen welkend warten auf den schnee             ", "tokens": ["Blu\u00b7men", "wel\u00b7kend", "war\u00b7ten", "auf", "den", "schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie dein finger scheu die m&#252;den flicht               ", "tokens": ["Wie", "dein", "fin\u00b7ger", "scheu", "die", "m", "&#252;", "den", "flicht"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "ADJD", "ART", "NN", "$(", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Andre blumen schenkt dies jahr uns nicht               ", "tokens": ["And\u00b7re", "blu\u00b7men", "schenkt", "dies", "jahr", "uns", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PDS", "NN", "PPER", "PTKNEG"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keine bitte riefe sie herbei               ", "tokens": ["Kei\u00b7ne", "bit\u00b7te", "rie\u00b7fe", "sie", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Andre bringt vielleicht uns einst ein mai             ", "tokens": ["And\u00b7re", "bringt", "viel\u00b7leicht", "uns", "einst", "ein", "mai"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "L\u00f6se meinen arm und bleibe stark               ", "tokens": ["L\u00f6\u00b7se", "mei\u00b7nen", "arm", "und", "blei\u00b7be", "stark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lass mit mir vorm scheidestrahl den park               ", "tokens": ["Lass", "mit", "mir", "vorm", "schei\u00b7des\u00b7trahl", "den", "park"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "APPRART", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eh vom berg der nebel dr&#252;ber fleucht               ", "tokens": ["Eh", "vom", "berg", "der", "ne\u00b7bel", "dr", "&#252;", "ber", "fleucht"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "NE", "$(", "ADV", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Schwinden wir eh winter uns verscheucht.             ", "tokens": ["Schwin\u00b7den", "wir", "eh", "win\u00b7ter", "uns", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "NE", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}