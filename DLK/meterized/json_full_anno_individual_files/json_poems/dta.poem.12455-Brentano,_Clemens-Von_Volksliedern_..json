{"dta.poem.12455": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Volksliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer damit nicht zufrieden, noch mehr sehen will,", "tokens": ["Wer", "da\u00b7mit", "nicht", "zu\u00b7frie\u00b7den", ",", "noch", "mehr", "se\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geh grade von da aus zum Ringlspil,", "tokens": ["Geh", "gra\u00b7de", "von", "da", "aus", "zum", "Ringl\u00b7spil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da drehen sich zwey und zwey", "tokens": ["Da", "dre\u00b7hen", "sich", "zwey", "und", "zwey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "CARD", "KON", "CARD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Rund herum in der Reih,", "tokens": ["Rund", "he\u00b7rum", "in", "der", "Reih", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Oft schreien die Medeln, nicht gar so geschwind,", "tokens": ["Oft", "schrei\u00b7en", "die", "Me\u00b7deln", ",", "nicht", "gar", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es ist nicht wegen meiner, es ist wegens Kind.", "tokens": ["Es", "ist", "nicht", "we\u00b7gen", "mei\u00b7ner", ",", "es", "ist", "we\u00b7gens", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "$,", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}