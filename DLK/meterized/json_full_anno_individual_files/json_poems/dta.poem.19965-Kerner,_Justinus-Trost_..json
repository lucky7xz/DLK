{"dta.poem.19965": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.85", "tl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lang noch Berg und Thale bl\u00fch'n,", "tokens": ["So", "lang", "noch", "Berg", "und", "Tha\u00b7le", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch sie melodisch Fl\u00fcsse zieh'n,", "tokens": ["Durch", "sie", "me\u00b7lo\u00b7disch", "Fl\u00fcs\u00b7se", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Vogel hoch im Blauen schwebt,", "tokens": ["Ein", "Vo\u00b7gel", "hoch", "im", "Blau\u00b7en", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gold\u00e4hren licht im Westhauch wallen,", "tokens": ["Gol\u00b7d\u00e4h\u00b7ren", "licht", "im", "West\u00b7hauch", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gebirge steh'n, Alph\u00f6rner schallen,", "tokens": ["Ge\u00b7bir\u00b7ge", "steh'n", ",", "Al\u00b7ph\u00f6r\u00b7ner", "schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat diese Welt nicht ausgelebt.", "tokens": ["Hat", "die\u00b7se", "Welt", "nicht", "aus\u00b7ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was die Menschen thun und treiben", "tokens": ["Und", "was", "die", "Men\u00b7schen", "thun", "und", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ob frey sie oder Knechte bleiben,", "tokens": ["Ob", "frey", "sie", "o\u00b7der", "Knech\u00b7te", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dem Fr\u00fchling gr\u00e4bt es sich nicht ein.", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "gr\u00e4bt", "es", "sich", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kein Treiber bringt mich je in Zweifel,", "tokens": ["Kein", "Trei\u00b7ber", "bringt", "mich", "je", "in", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ist er ein Teufel aller Teufel \u2014", "tokens": ["Ist", "er", "ein", "Teu\u00b7fel", "al\u00b7ler", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er \u00e4ndert nicht der Sonne Schein.", "tokens": ["Er", "\u00e4n\u00b7dert", "nicht", "der", "Son\u00b7ne", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}