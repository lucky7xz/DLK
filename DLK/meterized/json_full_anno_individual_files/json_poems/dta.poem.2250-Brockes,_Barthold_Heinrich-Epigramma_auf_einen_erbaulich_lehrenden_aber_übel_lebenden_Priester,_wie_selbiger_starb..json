{"dta.poem.2250": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n auf einen erbaulich lehrenden  \n  aber \u00fcbel lebenden Priester,  \n  wie selbiger starb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erw\u00e4gte man doch diese Lehre: da\u00df unsere Rechthabe-", "tokens": ["Er\u00b7w\u00e4g\u00b7te", "man", "doch", "die\u00b7se", "Leh\u00b7re", ":", "da\u00df", "un\u00b7se\u00b7re", "Recht\u00b7ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "PDAT", "NN", "$.", "KOUS", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "rey,", "tokens": ["rey", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Anstatt uns Ehr und Ruhm zu bringen, ohn Ausnahm,", "tokens": ["An\u00b7statt", "uns", "Ehr", "und", "Ruhm", "zu", "brin\u00b7gen", ",", "ohn", "Aus\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "immer sch\u00e4dlich sey!", "tokens": ["im\u00b7mer", "sch\u00e4d\u00b7lich", "sey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der, den auch deine Klugheit selber im Zanken zwingt,", "tokens": ["Der", ",", "den", "auch", "dei\u00b7ne", "Klug\u00b7heit", "sel\u00b7ber", "im", "Zan\u00b7ken", "zwingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "dir Recht zu lassen,", "tokens": ["dir", "Recht", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wird, zur Belohnung deines Sieges, betr\u00fcbter Lohn!", "tokens": ["Wird", ",", "zur", "Be\u00b7loh\u00b7nung", "dei\u00b7nes", "Sie\u00b7ges", ",", "be\u00b7tr\u00fcb\u00b7ter", "Lohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "gewi\u00df dich hassen.", "tokens": ["ge\u00b7wi\u00df", "dich", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}