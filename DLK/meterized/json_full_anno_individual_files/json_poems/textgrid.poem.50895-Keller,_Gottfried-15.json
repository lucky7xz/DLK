{"textgrid.poem.50895": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "15", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4dchen! gestern qu\u00e4lte mich ein eitler Christ, ein Esel!", "tokens": ["O", "M\u00e4d\u00b7chen", "!", "ge\u00b7stern", "qu\u00e4l\u00b7te", "mich", "ein", "eit\u00b7ler", "Christ", ",", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Heute schreckte mich par excellence ein Atheist, ein Esel!", "tokens": ["Heu\u00b7te", "schreck\u00b7te", "mich", "par", "ex\u00b7cel\u00b7len\u00b7ce", "ein", "A\u00b7theist", ",", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "FM", "FM", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Dort naht mit worteschwangerm Nichts, mit ungeheurem Unsinn,", "tokens": ["Dort", "naht", "mit", "wor\u00b7te\u00b7schwan\u00b7germ", "Nichts", ",", "mit", "un\u00b7ge\u00b7heu\u00b7rem", "Un\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PWAV", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Mit t\u00f6nender Salbaderei ein Pantheist, ein Esel \u2013", "tokens": ["Mit", "t\u00f6\u00b7nen\u00b7der", "Sal\u00b7ba\u00b7de\u00b7rei", "ein", "Pan\u00b7theist", ",", "ein", "E\u00b7sel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Birg mich an deiner jungen Brust und rette meine Menschheit,", "tokens": ["Birg", "mich", "an", "dei\u00b7ner", "jun\u00b7gen", "Brust", "und", "ret\u00b7te", "mei\u00b7ne", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Mein Kind! sonst werd ich selber noch zu dieser Frist ein Esel!", "tokens": ["Mein", "Kind", "!", "sonst", "werd", "ich", "sel\u00b7ber", "noch", "zu", "die\u00b7ser", "Frist", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.2": {"line.1": {"text": "O M\u00e4dchen! gestern qu\u00e4lte mich ein eitler Christ, ein Esel!", "tokens": ["O", "M\u00e4d\u00b7chen", "!", "ge\u00b7stern", "qu\u00e4l\u00b7te", "mich", "ein", "eit\u00b7ler", "Christ", ",", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Heute schreckte mich par excellence ein Atheist, ein Esel!", "tokens": ["Heu\u00b7te", "schreck\u00b7te", "mich", "par", "ex\u00b7cel\u00b7len\u00b7ce", "ein", "A\u00b7theist", ",", "ein", "E\u00b7sel", "!"], "token_info": ["word", "word", "word", "wo