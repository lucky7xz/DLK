{"dta.poem.23395": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied  \n  eines lahmen Invaliden vom  J. 1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viel man auch verspricht,", "tokens": ["Wie", "viel", "man", "auch", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O traut den Worten nicht!", "tokens": ["O", "traut", "den", "Wor\u00b7ten", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wort ist Schall und Wind \u2014", "tokens": ["Ein", "Wort", "ist", "Schall", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seid doch nicht taub und blind!", "tokens": ["Seid", "doch", "nicht", "taub", "und", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O seht euch vor und um,", "tokens": ["O", "seht", "euch", "vor", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Seid doch nicht gar zu dumm!", "tokens": ["Seid", "doch", "nicht", "gar", "zu", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "PTKNEG", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist's immer noch nicht Zeit,", "tokens": ["Ist's", "im\u00b7mer", "noch", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zu werden mal gescheit?", "tokens": ["Zu", "wer\u00b7den", "mal", "ge\u00b7scheit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Deutschland! o Deutschland!", "tokens": ["O", "Deutschland", "!", "o", "Deutschland", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "FM", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wann kommt denn wohl die Zeit?", "tokens": ["Wann", "kommt", "denn", "wohl", "die", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wann wird die Welt gescheit?", "tokens": ["Wann", "wird", "die", "Welt", "ge\u00b7scheit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Gutes wird gedacht,", "tokens": ["Viel", "Gu\u00b7tes", "wird", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mehr Schlechtes wird gemacht.", "tokens": ["Mehr", "Schlech\u00b7tes", "wird", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch f\u00e4llt mir gar nicht ein,", "tokens": ["Doch", "f\u00e4llt", "mir", "gar", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein Schuft und Lump zu sein.", "tokens": ["Ein", "Schuft", "und", "Lump", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie oft sie auch erliegt,", "tokens": ["Wie", "oft", "sie", "auch", "er\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die gute Sache siegt \u2014", "tokens": ["Die", "gu\u00b7te", "Sa\u00b7che", "siegt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Hoch Deutschland! hoch Deutschland!", "tokens": ["Hoch", "Deutschland", "!", "hoch", "Deutschland", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJD", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}