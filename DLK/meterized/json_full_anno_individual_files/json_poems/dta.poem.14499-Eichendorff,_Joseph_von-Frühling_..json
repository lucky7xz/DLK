{"dta.poem.14499": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die dunkeln Gr\u00fcnde s\u00e4useln kaum,", "tokens": ["Die", "dun\u00b7keln", "Gr\u00fcn\u00b7de", "s\u00e4u\u00b7seln", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schau'n so fremd herauf.", "tokens": ["Sie", "schau'n", "so", "fremd", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tiefschauernd f\u00fchlt' er, 's war ein Traum \u2014", "tokens": ["Tief\u00b7schau\u00b7ernd", "f\u00fchlt'", "er", ",", "'s", "war", "ein", "Traum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wacht im Himmel auf.", "tokens": ["Und", "wacht", "im", "Him\u00b7mel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}