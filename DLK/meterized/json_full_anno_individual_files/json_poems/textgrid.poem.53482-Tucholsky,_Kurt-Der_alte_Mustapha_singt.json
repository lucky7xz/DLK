{"textgrid.poem.53482": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Mustapha singt", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin Eunuch und will es ewig bleiben!", "tokens": ["Ich", "bin", "Eu\u00b7nuch", "und", "will", "es", "e\u00b7wig", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch meine Enkel sollens grad so treiben.", "tokens": ["Auch", "mei\u00b7ne", "En\u00b7kel", "sol\u00b7lens", "grad", "so", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin ein Ha- Ha- Haremskind,", "tokens": ["Ich", "bin", "ein", "Ha", "Ha", "Ha\u00b7rems\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "TRUNC", "TRUNC", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie so Eunuchen sind,", "tokens": ["Wie", "so", "Eu\u00b7nu\u00b7chen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie so Eunuchen sind.", "tokens": ["Wie", "so", "Eu\u00b7nu\u00b7chen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Jung-T\u00fcrkei wird hier nicht re\u00fcssieren:", "tokens": ["Die", "Jung\u00b7T\u00fcr\u00b7kei", "wird", "hier", "nicht", "re\u00b7\u00fcs\u00b7sie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich bin bestimmt nicht mehr zu reformieren.", "tokens": ["Ich", "bin", "be\u00b7stimmt", "nicht", "mehr", "zu", "re\u00b7for\u00b7mie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich sage vornehm, da\u00df als Oberbey", "tokens": ["Ich", "sa\u00b7ge", "vor\u00b7nehm", ",", "da\u00df", "als", "O\u00b7ber\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich bei der Harems-Wach- und Schlie\u00dfgesellschaft sei.", "tokens": ["Ich", "bei", "der", "Ha\u00b7rems\u00b7Wach", "und", "Schlie\u00df\u00b7ge\u00b7sell\u00b7schaft", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "TRUNC", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bum bum!", "tokens": ["Bum", "bum", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Ich bin ein Eunuch", "tokens": ["Ich", "bin", "ein", "Eu\u00b7nuch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Und hier zu Besuch.", "tokens": ["Und", "hier", "zu", "Be\u00b7such", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Zu Hause, da sang ich Sopran.", "tokens": ["Zu", "Hau\u00b7se", ",", "da", "sang", "ich", "Sop\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Hier klingts wie Tenor \u2013", "tokens": ["Hier", "klingts", "wie", "Te\u00b7nor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NE", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wie kommt mir das vor!", "tokens": ["Wie", "kommt", "mir", "das", "vor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Was ist das? Wer hat das getan?", "tokens": ["Was", "ist", "das", "?", "Wer", "hat", "das", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$.", "PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.17": {"text": "Ich f\u00fchl mich verwandelt!", "tokens": ["Ich", "f\u00fchl", "mich", "ver\u00b7wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Ich f\u00fchl mich verschandelt!", "tokens": ["Ich", "f\u00fchl", "mich", "ver\u00b7schan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Was ist das? Wer hat das getan?", "tokens": ["Was", "ist", "das", "?", "Wer", "hat", "das", "ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$.", "PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Vor kurzer Zeit kam ich auf den Gedanken:", "tokens": ["Vor", "kur\u00b7zer", "Zeit", "kam", "ich", "auf", "den", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sieh dich mal um im Vaterland der Franken.", "tokens": ["Sieh", "dich", "mal", "um", "im", "Va\u00b7ter\u00b7land", "der", "Fran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich las im Zei- Zei- Zeitungsblatt,", "tokens": ["Ich", "las", "im", "Zei", "Zei", "Zei\u00b7tungs\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "TRUNC", "TRUNC", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was man in Preu\u00dfen hat,", "tokens": ["Was", "man", "in", "Preu\u00b7\u00dfen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was man in Preu\u00dfen hat.", "tokens": ["Was", "man", "in", "Preu\u00b7\u00dfen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zum Abschied sang ich meinen h\u00f6chsten Triller", "tokens": ["Zum", "Ab\u00b7schied", "sang", "ich", "mei\u00b7nen", "h\u00f6chs\u00b7ten", "Tril\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und lie\u00df im Stich die gro\u00dfe Sultansvilla,", "tokens": ["Und", "lie\u00df", "im", "Stich", "die", "gro\u00b7\u00dfe", "Sul\u00b7tans\u00b7vil\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Wo es die sch\u00f6nen, dicken Frauen gibt:", "tokens": ["Wo", "es", "die", "sch\u00f6\u00b7nen", ",", "di\u00b7cken", "Frau\u00b7en", "gibt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich war zwar ungef\u00e4hrlich, aber doch beliebt.", "tokens": ["Ich", "war", "zwar", "un\u00b7ge\u00b7f\u00e4hr\u00b7lich", ",", "a\u00b7ber", "doch", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bum bum!", "tokens": ["Bum", "bum", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Ich bin ein Eunuch", "tokens": ["Ich", "bin", "ein", "Eu\u00b7nuch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Und hier zu Besuch.", "tokens": ["Und", "hier", "zu", "Be\u00b7such", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos"