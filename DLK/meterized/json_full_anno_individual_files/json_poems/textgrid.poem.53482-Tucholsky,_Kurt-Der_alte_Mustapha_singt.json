n", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$.", "PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Ich habs entdeckt: im Land der Klassenw\u00e4hler", "tokens": ["Ich", "habs", "ent\u00b7deckt", ":", "im", "Land", "der", "Klas\u00b7sen\u00b7w\u00e4h\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "VVPP", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fall ich nicht auf durch meinen kleinen Fehler.", "tokens": ["Fall", "ich", "nicht", "auf", "durch", "mei\u00b7nen", "klei\u00b7nen", "Feh\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Mann der Po- Po- Politik", "tokens": ["Der", "Mann", "der", "Po", "Po", "Po\u00b7li\u00b7tik"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "TRUNC", "TRUNC", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht hier Sopranmusik,", "tokens": ["Macht", "hier", "Sop\u00b7ran\u00b7mu\u00b7sik", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Macht hier Sopranmusik.", "tokens": ["Macht", "hier", "Sop\u00b7ran\u00b7mu\u00b7sik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Jedweden B\u00fcrger k\u00f6nnt der Harem hegen:", "tokens": ["Jed\u00b7we\u00b7den", "B\u00fcr\u00b7ger", "k\u00f6nnt", "der", "Ha\u00b7rem", "he\u00b7gen", ":"], "token_inf