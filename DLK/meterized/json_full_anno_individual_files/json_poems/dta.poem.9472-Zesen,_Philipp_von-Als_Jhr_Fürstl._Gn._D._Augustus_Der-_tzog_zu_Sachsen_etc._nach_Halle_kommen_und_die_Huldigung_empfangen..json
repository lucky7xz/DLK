{"dta.poem.9472": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als Jhr F\u00fcrstl. Gn. D. Augustus Der-  \n tzog zu Sachsen etc. nach Halle kommen und die  \n Huldigung empfangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Edler Printz/ von Sachsen-Stamme/", "tokens": ["Ed\u00b7ler", "Printz", "/", "von", "Sach\u00b7sen\u00b7Stam\u00b7me", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweig der Rauten/ so jtzund", "tokens": ["Zweig", "der", "Rau\u00b7ten", "/", "so", "jt\u00b7zund"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "ADV", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Zeigt den angenehmen Mund", "tokens": ["Zeigt", "den", "an\u00b7ge\u00b7neh\u00b7men", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und die heisse Liebes-flamme", "tokens": ["und", "die", "heis\u00b7se", "Lie\u00b7bes\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dier ", "tokens": ["Dier"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Gantz/ verla\u00dfen war zuvor", "tokens": ["Gantz", "/", "ver\u00b7la\u00b7\u00dfen", "war", "zu\u00b7vor"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "VVPP", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "und sehr traurig sich befunden?", "tokens": ["und", "sehr", "trau\u00b7rig", "sich", "be\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sey gegr\u00fc\u00dft zu diesen stunden", "tokens": ["Sey", "ge\u00b7gr\u00fc\u00dft", "zu", "die\u00b7sen", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "und ihr stunden auch zugleich/", "tokens": ["und", "ihr", "stun\u00b7den", "auch", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Stunden die Jhr Freuden-reich.", "tokens": ["Stun\u00b7den", "die", "Ihr", "Freu\u00b7den\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Gl\u00fcck zu dem Rauten Zweig! h\u00f6rt wie die Pleisse", "tokens": ["Gl\u00fcck", "zu", "dem", "Rau\u00b7ten", "Zweig", "!", "h\u00f6rt", "wie", "die", "Pleis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$.", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "schreyet/", "tokens": ["schre\u00b7yet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "H\u00f6rt wie die Saale rufft und sich ", "tokens": ["H\u00f6rt", "wie", "die", "Saa\u00b7le", "rufft", "und", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "VVFIN", "KON", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Auf eure gegenwart/ die St\u00e4nde ruffen zu/", "tokens": ["Auf", "eu\u00b7re", "ge\u00b7gen\u00b7wart", "/", "die", "St\u00e4n\u00b7de", "ruf\u00b7fen", "zu", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "NN", "VVINF", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gott geb Euch gl\u00fcck und heil/ Gott geb Euch fried", "tokens": ["Gott", "geb", "Euch", "gl\u00fcck", "und", "heil", "/", "Gott", "geb", "Euch", "fried"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$(", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und ruh!", "tokens": ["und", "ruh", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Gantz Sachsen w\u00fcndschet noch gl\u00fcck auf gl\u00fcck auf", "tokens": ["Gantz", "Sach\u00b7sen", "w\u00fcnd\u00b7schet", "noch", "gl\u00fcck", "auf", "gl\u00fcck", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADV", "ADJD", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "die reise/", "tokens": ["die", "rei\u00b7se", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "O Edles F\u00fcrsten-blut/ und singt die freuden weise/", "tokens": ["O", "Ed\u00b7les", "F\u00fcrs\u00b7ten\u00b7blut", "/", "und", "singt", "die", "freu\u00b7den", "wei\u00b7se", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Sonne schaut herf\u00fcr aus jhrem blauen Zelt", "tokens": ["Die", "Son\u00b7ne", "schaut", "her\u00b7f\u00fcr", "aus", "jhrem", "blau\u00b7en", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "und schickt dier/ Halle zu den langgew\u00fcndschten", "tokens": ["und", "schickt", "dier", "/", "Hal\u00b7le", "zu", "den", "lang\u00b7ge\u00b7w\u00fcnd\u00b7schten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "NE", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Held.", "tokens": ["Held", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Halle/ schicke dich zu gr\u00fc\u00dfen", "tokens": ["Hal\u00b7le", "/", "schi\u00b7cke", "dich", "zu", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen neuen Breutigam/", "tokens": ["Dei\u00b7nen", "neu\u00b7en", "Breu\u00b7ti\u00b7gam", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den dier schickt der Sachsenstam\u0303", "tokens": ["Den", "dier", "schickt", "der", "Sach\u00b7sen\u00b7stam\u0303"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er wil in den Arm Dich schlie\u00dfen/", "tokens": ["Er", "wil", "in", "den", "Arm", "Dich", "schlie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Neige deinen zahrten Mund", "tokens": ["Nei\u00b7ge", "dei\u00b7nen", "zahr\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhm entgegen auch jtzund:", "tokens": ["Jhm", "ent\u00b7ge\u00b7gen", "auch", "jt\u00b7zund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sihstu deinen F\u00fcrsten reiten?", "tokens": ["Sih\u00b7stu", "dei\u00b7nen", "F\u00fcrs\u00b7ten", "rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sihstu kommen Jhn von weiten?", "tokens": ["Sih\u00b7stu", "kom\u00b7men", "Jhn", "von", "wei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Steig auf deine T\u00fcrne Du/", "tokens": ["Steig", "auf", "dei\u00b7ne", "T\u00fcr\u00b7ne", "Du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schaue wie Er naht herzu?", "tokens": ["Schau\u00b7e", "wie", "Er", "naht", "her\u00b7zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach seht wie blinckt das Feld mit Rossen fast bedecket/", "tokens": ["Ach", "seht", "wie", "blinckt", "das", "Feld", "mit", "Ros\u00b7sen", "fast", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "KOKOM", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Seht wie die traurigkeit sich allgemach verstecket", "tokens": ["Seht", "wie", "die", "trau\u00b7rig\u00b7keit", "sich", "all\u00b7ge\u00b7mach", "ver\u00b7ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jtzt k\u00f6m\u0303t der Printz herran/ jtzt neiget sich die Stadt/", "tokens": ["Jtzt", "k\u00f6m\u0303t", "der", "Printz", "her\u00b7ran", "/", "jtzt", "nei\u00b7get", "sich", "die", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$(", "ADV", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Er zu sch\u00fctzen Jhm jtzund erwehlet hatt.", "tokens": ["Die", "Er", "zu", "sch\u00fct\u00b7zen", "Jhm", "jt\u00b7zund", "er\u00b7weh\u00b7let", "hatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie lieblich lacht er doch/ wie redt er doch so freu\u0303dlich/", "tokens": ["Wie", "lieb\u00b7lich", "lacht", "er", "doch", "/", "wie", "redt", "er", "doch", "so", "fre\u0169d\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$(", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er meinet alles guth und stellet sich nicht feindlich", "tokens": ["Er", "mei\u00b7net", "al\u00b7les", "guth", "und", "stel\u00b7let", "sich", "nicht", "feind\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "KON", "VVFIN", "PRF", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie jener Boreas: der s\u00fc\u00dfe Zephyr weht", "tokens": ["Wie", "je\u00b7ner", "Bo\u00b7re\u00b7as", ":", "der", "s\u00fc\u00b7\u00dfe", "Ze\u00b7phyr", "weht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NE", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den Frieden mildiglich wo dieser F\u00fcrste steht.", "tokens": ["Den", "Frie\u00b7den", "mil\u00b7dig\u00b7lich", "wo", "die\u00b7ser", "F\u00fcrs\u00b7te", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PWAV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wei\u00dfheit/ Kunst/ Verstand und Ehre", "tokens": ["Wei\u00df\u00b7heit", "/", "Kunst", "/", "Ver\u00b7stand", "und", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhn begleiten jederzeit/", "tokens": ["Jhn", "be\u00b7glei\u00b7ten", "je\u00b7der\u00b7zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER