{"dta.poem.10366": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lache ob den abgeschmackten Laffen,", "tokens": ["Ich", "la\u00b7che", "ob", "den", "ab\u00b7ge\u00b7schmack\u00b7ten", "Laf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich anglotzen mit den Bocksgesichtern;", "tokens": ["Die", "mich", "an\u00b7glot\u00b7zen", "mit", "den", "Bocks\u00b7ge\u00b7sich\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich lache ob den F\u00fcchsen, die so n\u00fcchtern", "tokens": ["Ich", "la\u00b7che", "ob", "den", "F\u00fcch\u00b7sen", ",", "die", "so", "n\u00fcch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00e4misch mich beschn\u00fcffeln und begaffen.", "tokens": ["Und", "h\u00e4\u00b7misch", "mich", "be\u00b7schn\u00fcf\u00b7feln", "und", "be\u00b7ga\u00b7ffen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ich lache ob den hochgelahrten Affen,", "tokens": ["Ich", "la\u00b7che", "ob", "den", "hoch\u00b7ge\u00b7lahr\u00b7ten", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sich aufbl\u00e4hn zu stolzen Geistesrichtern;", "tokens": ["Die", "sich", "auf\u00b7bl\u00e4hn", "zu", "stol\u00b7zen", "Geis\u00b7tes\u00b7rich\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich lache ob den feigen B\u00f6sewichtern,", "tokens": ["Ich", "la\u00b7che", "ob", "den", "fei\u00b7gen", "B\u00f6\u00b7se\u00b7wich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die mich umdrohn mit giftgetr\u00e4nkten Waffen.", "tokens": ["Die", "mich", "um\u00b7drohn", "mit", "gift\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn wenn des Gl\u00fcckes h\u00fcbsche sieben Sachen", "tokens": ["Denn", "wenn", "des", "Gl\u00fc\u00b7ckes", "h\u00fcb\u00b7sche", "sie\u00b7ben", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Uns von des Schicksals H\u00e4nden sind zerbrochen,", "tokens": ["Uns", "von", "des", "Schick\u00b7sals", "H\u00e4n\u00b7den", "sind", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und so zu unsern F\u00fc\u00dfen hingeschmissen;", "tokens": ["Und", "so", "zu", "un\u00b7sern", "F\u00fc\u00b7\u00dfen", "hin\u00b7ge\u00b7schmis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und wenn das Herz im Leibe ist zerrissen,", "tokens": ["Und", "wenn", "das", "Herz", "im", "Lei\u00b7be", "ist", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zerrissen, und zerschnitten, und zerstochen, \u2014", "tokens": ["Zer\u00b7ris\u00b7sen", ",", "und", "zer\u00b7schnit\u00b7ten", ",", "und", "zer\u00b7sto\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "KON", "VVFIN", "$,", "KON", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dann bleibt uns doch das sch\u00f6ne gelle Lachen.", "tokens": ["Dann", "bleibt", "uns", "doch", "das", "sch\u00f6\u00b7ne", "gel\u00b7le", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}