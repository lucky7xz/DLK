{"dta.poem.13025": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees braucht des Redens nicht; hieraus ist Heil entsprossen,", "tokens": ["\u201e", "es", "braucht", "des", "Re\u00b7dens", "nicht", ";", "hier\u00b7aus", "ist", "Heil", "ent\u00b7spros\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "PAV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ehierinnen ist das Gl\u00fcck des Vaterlands verschlossen.", "tokens": ["\u201e", "hie\u00b7rin\u00b7nen", "ist", "das", "Gl\u00fcck", "des", "Va\u00b7ter\u00b7lands", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201enimm! lies! (sie bot es an) Freundinnen! sch\u00e4zet es!", "tokens": ["\u201e", "nimm", "!", "lies", "!", "(", "sie", "bot", "es", "an", ")", "Freun\u00b7din\u00b7nen", "!", "sch\u00e4\u00b7zet", "es", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$.", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$(", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201esprecht aus, ob dieses Buch nicht ein vollst\u00e4ndiges", "tokens": ["\u201e", "sprecht", "aus", ",", "ob", "die\u00b7ses", "Buch", "nicht", "ein", "voll\u00b7st\u00e4n\u00b7di\u00b7ges"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "KOUS", "PDAT", "NN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "155\u201dStaats-Kriegs- und Friedens-Werck, ein wahrer Grund-Stein seye,", "tokens": ["\"", "Staats\u00b7Kriegs", "und", "Frie\u00b7dens\u00b7\u00b7Werck", ",", "ein", "wah\u00b7rer", "Grun\u00b7dStein", "se\u00b7ye", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "TRUNC", "KON", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "\u201evermittels dessen ich den Bau des Throns erneue!", "tokens": ["\u201e", "ver\u00b7mit\u00b7tels", "des\u00b7sen", "ich", "den", "Bau", "des", "Throns", "er\u00b7neu\u00b7e", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}