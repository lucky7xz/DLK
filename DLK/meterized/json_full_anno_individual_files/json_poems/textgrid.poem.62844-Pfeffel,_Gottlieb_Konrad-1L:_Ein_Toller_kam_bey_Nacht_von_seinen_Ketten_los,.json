{"textgrid.poem.62844": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Toller kam bey Nacht von seinen Ketten los,", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Toller kam bey Nacht von seinen Ketten los,", "tokens": ["Ein", "Tol\u00b7ler", "kam", "bey", "Nacht", "von", "sei\u00b7nen", "Ket\u00b7ten", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lief aufs freye Feld, in dessen gr\u00fcnem Schoo\u00df", "tokens": ["Und", "lief", "aufs", "frey\u00b7e", "Feld", ",", "in", "des\u00b7sen", "gr\u00fc\u00b7nem", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein stiller Bach, hell wie ein Spiegel flo\u00df.", "tokens": ["Ein", "stil\u00b7ler", "Bach", ",", "hell", "wie", "ein", "Spie\u00b7gel", "flo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Sterne gl\u00e4nzendes Gewimmel,", "tokens": ["Der", "Ster\u00b7ne", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mond in voller Pracht erleuchteten den Himmel", "toke