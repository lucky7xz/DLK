{"textgrid.poem.38232": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ich stand an einem Morgen (2)", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand an einem Morgen", "tokens": ["Ich", "stand", "an", "ei\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heimlich an einem Ort,", "tokens": ["Heim\u00b7lich", "an", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da hielt ich mich verborgen,", "tokens": ["Da", "hielt", "ich", "mich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich h\u00f6rt kl\u00e4gliche Wort,", "tokens": ["Ich", "h\u00f6rt", "kl\u00e4g\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Von einem frommen Christen fein,", "tokens": ["Von", "ei\u00b7nem", "from\u00b7men", "Chris\u00b7ten", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sprach zu Gott seinem Herrn:", "tokens": ["Er", "sprach", "zu", "Gott", "sei\u00b7nem", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mu\u00df denn gelitten seyn?", "tokens": ["Mu\u00df", "denn", "ge\u00b7lit\u00b7ten", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Gott ich hab vernommen,", "tokens": ["Herr", "Gott", "ich", "hab", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du willt mich lassen schier,", "tokens": ["Du", "willt", "mich", "las\u00b7sen", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In viel Anfechtung kommen,", "tokens": ["In", "viel", "An\u00b7fech\u00b7tung", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thut nicht gefallen mir.", "tokens": ["Thut", "nicht", "ge\u00b7fal\u00b7len", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVPP", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Merk m\u00e4nnlich auf, was ich dir sag,", "tokens": ["Merk", "m\u00e4nn\u00b7lich", "auf", ",", "was", "ich", "dir", "sag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Thu dich nicht hart beklagen,", "tokens": ["Thu", "dich", "nicht", "hart", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein Christ mu\u00df haben Plag.", "tokens": ["Ein", "Christ", "mu\u00df", "ha\u00b7ben", "Plag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der fromm Christ weinet sehre,", "tokens": ["Der", "fromm", "Christ", "wei\u00b7net", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Herz war unmuthsvoll.", "tokens": ["Sein", "Herz", "war", "un\u00b7muths\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gieb mir Weis und Lehre,", "tokens": ["So", "gieb", "mir", "Weis", "und", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ich mich halten soll,", "tokens": ["Wie", "ich", "mich", "hal\u00b7ten", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Glaub ist schwach und kalt in mir,", "tokens": ["Der", "Glaub", "ist", "schwach", "und", "kalt", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Fleisch will mich verf\u00fchren,", "tokens": ["Mein", "Fleisch", "will", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich soll weichen von dir.", "tokens": ["Da\u00df", "ich", "soll", "wei\u00b7chen", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Gott sprach, lachend zu muthe,", "tokens": ["Gott", "sprach", ",", "la\u00b7chend", "zu", "mu\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dein Willen ich wohl sp\u00fcr,", "tokens": ["Dein", "Wil\u00b7len", "ich", "wohl", "sp\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wollst wohl han das gute,", "tokens": ["Du", "wollst", "wohl", "han", "das", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenns dir nicht w\u00fcrde saur,", "tokens": ["Wenns", "dir", "nicht", "w\u00fcr\u00b7de", "saur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer aber will mit mir han Theil,", "tokens": ["Wer", "a\u00b7ber", "will", "mit", "mir", "han", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df alles fahren lassen,", "tokens": ["Mu\u00df", "al\u00b7les", "fah\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Viel Gl\u00fcck ist ihm nicht feil.", "tokens": ["Viel", "Gl\u00fcck", "ist", "ihm", "nicht", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der fromme Christ schrie Morde,", "tokens": ["Der", "from\u00b7me", "Christ", "schrie", "Mor\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mord \u00fcber alles Leid,", "tokens": ["Mord", "\u00fc\u00b7ber", "al\u00b7les", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich schrecken deine Worte,", "tokens": ["Mich", "schre\u00b7cken", "dei\u00b7ne", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr Gott mach mich bereit,", "tokens": ["Herr", "Gott", "mach", "mich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wollt doch alles tragen gern,", "tokens": ["Ich", "wollt", "doch", "al\u00b7les", "tra\u00b7gen", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Weltlust gerne hassen,", "tokens": ["Die", "Welt\u00b7lust", "ger\u00b7ne", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie lassen von mir fern.", "tokens": ["Sie", "las\u00b7sen", "von", "mir", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gott sprach: ich thu dich z\u00fcchten,", "tokens": ["Gott", "sprach", ":", "ich", "thu", "dich", "z\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab nur ein guten Muth,", "tokens": ["Hab", "nur", "ein", "gu\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und thu mich allzeit f\u00fcrchten,", "tokens": ["Und", "thu", "mich", "all\u00b7zeit", "f\u00fcrch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erkauft bist mit mein'm Blut;", "tokens": ["Er\u00b7kauft", "bist", "mit", "mein'm", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daran gedenk mit ganzem Flei\u00df,", "tokens": ["Da\u00b7ran", "ge\u00b7denk", "mit", "gan\u00b7zem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All die ich fast thu lieben,", "tokens": ["All", "die", "ich", "fast", "thu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PRELS", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Straf ich, das ist mein Weis.", "tokens": ["Straf", "ich", ",", "das", "ist", "mein", "Weis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da kehrt Gott ihm den R\u00fccken,", "tokens": ["Da", "kehrt", "Gott", "ihm", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er redt zu ihm nicht mehr,", "tokens": ["Er", "redt", "zu", "ihm", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der arm Christ th\u00e4t sich schm\u00fccken", "tokens": ["Der", "arm", "Christ", "th\u00e4t", "sich", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem Winkel leer;", "tokens": ["In", "ei\u00b7nem", "Win\u00b7kel", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er weint aus der massen viel:", "tokens": ["Er", "weint", "aus", "der", "mas\u00b7sen", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u00bbdem Herrn im Creutz aushalten,", "tokens": ["\u00bb", "dem", "Herrn", "im", "Creutz", "aus\u00b7hal\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist kein Kinderspiel.\u00ab", "tokens": ["Das", "ist", "kein", "Kin\u00b7der\u00b7spiel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich stand an einem Morgen", "tokens": ["Ich", "stand", "an", "ei\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heimlich an einem Ort,", "tokens": ["Heim\u00b7lich", "an", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da hielt ich mich verborgen,", "tokens": ["Da", "hielt", "ich", "mich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich h\u00f6rt kl\u00e4gliche Wort,", "tokens": ["Ich", "h\u00f6rt", "kl\u00e4g\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Von einem frommen Christen fein,", "tokens": ["Von", "ei\u00b7nem", "from\u00b7men", "Chris\u00b7ten", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sprach zu Gott seinem Herrn:", "tokens": ["Er", "sprach", "zu", "Gott", "sei\u00b7nem", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mu\u00df denn gelitten seyn?", "tokens": ["Mu\u00df", "denn", "ge\u00b7lit\u00b7ten", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Herr Gott ich hab vernommen,", "tokens": ["Herr", "Gott", "ich", "hab", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du willt mich lassen schier,", "tokens": ["Du", "willt", "mich", "las\u00b7sen", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In viel Anfechtung kommen,", "tokens": ["In", "viel", "An\u00b7fech\u00b7tung", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thut nicht gefallen mir.", "tokens": ["Thut", "nicht", "ge\u00b7fal\u00b7len", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVPP", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Merk m\u00e4nnlich auf, was ich dir sag,", "tokens": ["Merk", "m\u00e4nn\u00b7lich", "auf", ",", "was", "ich", "dir", "sag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Thu dich nicht hart beklagen,", "tokens": ["Thu", "dich", "nicht", "hart", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein Christ mu\u00df haben Plag.", "tokens": ["Ein", "Christ", "mu\u00df", "ha\u00b7ben", "Plag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der fromm Christ weinet sehre,", "tokens": ["Der", "fromm", "Christ", "wei\u00b7net", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Herz war unmuthsvoll.", "tokens": ["Sein", "Herz", "war", "un\u00b7muths\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gieb mir Weis und Lehre,", "tokens": ["So", "gieb", "mir", "Weis", "und", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ich mich halten soll,", "tokens": ["Wie", "ich", "mich", "hal\u00b7ten", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Glaub ist schwach und kalt in mir,", "tokens": ["Der", "Glaub", "ist", "schwach", "und", "kalt", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Fleisch will mich verf\u00fchren,", "tokens": ["Mein", "Fleisch", "will", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich soll weichen von dir.", "tokens": ["Da\u00df", "ich", "soll", "wei\u00b7chen", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Gott sprach, lachend zu muthe,", "tokens": ["Gott", "sprach", ",", "la\u00b7chend", "zu", "mu\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dein Willen ich wohl sp\u00fcr,", "tokens": ["Dein", "Wil\u00b7len", "ich", "wohl", "sp\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wollst wohl han das gute,", "tokens": ["Du", "wollst", "wohl", "han", "das", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenns dir nicht w\u00fcrde saur,", "tokens": ["Wenns", "dir", "nicht", "w\u00fcr\u00b7de", "saur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer aber will mit mir han Theil,", "tokens": ["Wer", "a\u00b7ber", "will", "mit", "mir", "han", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df alles fahren lassen,", "tokens": ["Mu\u00df", "al\u00b7les", "fah\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Viel Gl\u00fcck ist ihm nicht feil.", "tokens": ["Viel", "Gl\u00fcck", "ist", "ihm", "nicht", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der fromme Christ schrie Morde,", "tokens": ["Der", "from\u00b7me", "Christ", "schrie", "Mor\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mord \u00fcber alles Leid,", "tokens": ["Mord", "\u00fc\u00b7ber", "al\u00b7les", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich schrecken deine Worte,", "tokens": ["Mich", "schre\u00b7cken", "dei\u00b7ne", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr Gott mach mich bereit,", "tokens": ["Herr", "Gott", "mach", "mich", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wollt doch alles tragen gern,", "tokens": ["Ich", "wollt", "doch", "al\u00b7les", "tra\u00b7gen", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Weltlust gerne hassen,", "tokens": ["Die", "Welt\u00b7lust", "ger\u00b7ne", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie lassen von mir fern.", "tokens": ["Sie", "las\u00b7sen", "von", "mir", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Gott sprach: ich thu dich z\u00fcchten,", "tokens": ["Gott", "sprach", ":", "ich", "thu", "dich", "z\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab nur ein guten Muth,", "tokens": ["Hab", "nur", "ein", "gu\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und thu mich allzeit f\u00fcrchten,", "tokens": ["Und", "thu", "mich", "all\u00b7zeit", "f\u00fcrch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erkauft bist mit mein'm Blut;", "tokens": ["Er\u00b7kauft", "bist", "mit", "mein'm", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Daran gedenk mit ganzem Flei\u00df,", "tokens": ["Da\u00b7ran", "ge\u00b7denk", "mit", "gan\u00b7zem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All die ich fast thu lieben,", "tokens": ["All", "die", "ich", "fast", "thu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PRELS", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Straf ich, das ist mein Weis.", "tokens": ["Straf", "ich", ",", "das", "ist", "mein", "Weis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da kehrt Gott ihm den R\u00fccken,", "tokens": ["Da", "kehrt", "Gott", "ihm", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er redt zu ihm nicht mehr,", "tokens": ["Er", "redt", "zu", "ihm", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der arm Christ th\u00e4t sich schm\u00fccken", "tokens": ["Der", "arm", "Christ", "th\u00e4t", "sich", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem Winkel leer;", "tokens": ["In", "ei\u00b7nem", "Win\u00b7kel", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er weint aus der massen viel:", "tokens": ["Er", "weint", "aus", "der", "mas\u00b7sen", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u00bbdem Herrn im Creutz aushalten,", "tokens": ["\u00bb", "dem", "Herrn", "im", "Creutz", "aus\u00b7hal\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist kein Kinderspiel.\u00ab", "tokens": ["Das", "ist", "kein", "Kin\u00b7der\u00b7spiel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}