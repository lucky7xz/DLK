{"textgrid.poem.39942": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo vom Berg die Felsen rollen,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo vom Berg die Felsen rollen,", "tokens": ["Wo", "vom", "Berg", "die", "Fel\u00b7sen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo kein Wasser friedlich flie\u00dft,", "tokens": ["Wo", "kein", "Was\u00b7ser", "fried\u00b7lich", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Sturze sich ergie\u00dft,", "tokens": ["Nur", "im", "Stur\u00b7ze", "sich", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die langen Donner grollen;", "tokens": ["Wo", "die", "lan\u00b7gen", "Don\u00b7ner", "grol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort in Rh\u00e4tiens finstern Gr\u00fcnden,", "tokens": ["Dort", "in", "Rh\u00e4\u00b7ti\u00b7ens", "fins\u00b7tern", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wo sich die Natur bekriegt,", "tokens": ["Wo", "sich", "die", "Na\u00b7tur", "be\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wald und Sturm im Kampfe liegt,", "tokens": ["Wald", "und", "Sturm", "im", "Kamp\u00b7fe", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann der Mensch nicht Ruhe finden.", "tokens": ["Kann", "der", "Mensch", "nicht", "Ru\u00b7he", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Feindschaft zeugt in seiner Seele", "tokens": ["Feind\u00b7schaft", "zeugt", "in", "sei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dort der Elemente Streit,", "tokens": ["Dort", "der", "E\u00b7le\u00b7men\u00b7te", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Und die F\u00fcrsten sind entzweit", "tokens": ["Und", "die", "F\u00fcrs\u00b7ten", "sind", "ent\u00b7zweit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.12": {"text": "Um den Sitz in Hain und H\u00f6hle.", "tokens": ["Um", "den", "Sitz", "in", "Hain", "und", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Seine finstern Th\u00e4ler neidet", "tokens": ["Sei\u00b7ne", "fins\u00b7tern", "Th\u00e4\u00b7ler", "nei\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Rudpert drum dem Adalbert;", "tokens": ["Rud\u00b7pert", "drum", "dem", "A\u00b7dal\u00b7bert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Jeder greifet nach dem Schwert,", "tokens": ["Je\u00b7der", "grei\u00b7fet", "nach", "dem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Ob sie schon der Waldstrom scheidet.", "tokens": ["Ob", "sie", "schon", "der", "Wald\u00b7strom", "schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "An der \u00fcberspritzten Klippe", "tokens": ["An", "der", "\u00fc\u00b7bers\u00b7pritz\u00b7ten", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "K\u00e4mpfen sie im Wasserschaum,", "tokens": ["K\u00e4mp\u00b7fen", "sie", "im", "Was\u00b7ser\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "K\u00e4mpfen, wo f\u00fcr zween ist Raum", "tokens": ["K\u00e4mp\u00b7fen", ",", "wo", "f\u00fcr", "zween", "ist", "Raum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "VVINF", "VAFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Auf der wald'gen Felsenrippe.", "tokens": ["Auf", "der", "wald'\u00b7gen", "Fel\u00b7sen\u00b7rip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dann im Anger und im Thale", "tokens": ["Dann", "im", "An\u00b7ger", "und", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Jeder mit ergrimmtem Tro\u00df,", "tokens": ["Je\u00b7der", "mit", "er\u00b7grimm\u00b7tem", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Jeder auf dem wild'sten Ro\u00df;", "tokens": ["Je\u00b7der", "auf", "dem", "wild'\u00b7sten", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Lang erhitzt sich Stahl am Stahle.", "tokens": ["Lang", "er\u00b7hitzt", "sich", "Stahl", "am", "Stah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Beider Mut ist stets der gleiche,", "tokens": ["Bei\u00b7der", "Mut", "ist", "stets", "der", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Beider Sehne gleich gest\u00e4hlt,", "tokens": ["Bei\u00b7der", "Seh\u00b7ne", "gleich", "ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Beide gleicher Ha\u00df beseelt,", "tokens": ["Bei\u00b7de", "glei\u00b7cher", "Ha\u00df", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Keiner weicht dem letzten Streiche.", "tokens": ["Kei\u00b7ner", "weicht", "dem", "letz\u00b7ten", "Strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Bis die Herrscherin der Gegend", "tokens": ["Bis", "die", "Herr\u00b7sche\u00b7rin", "der", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "In den Streit sich mischt, Natur,", "tokens": ["In", "den", "Streit", "sich", "mischt", ",", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Irre macht der Rosse Spur,", "tokens": ["Ir\u00b7re", "macht", "der", "Ros\u00b7se", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Felsen in die Wege legend.", "tokens": ["Fel\u00b7sen", "in", "die", "We\u00b7ge", "le\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Rudpert schwanket auf dem Pferde,", "tokens": ["Rud\u00b7pert", "schwan\u00b7ket", "auf", "dem", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und es b\u00e4umet sich das Thier,", "tokens": ["Und", "es", "b\u00e4u\u00b7met", "sich", "das", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Und mit zornigem Gewieh'r", "tokens": ["Und", "mit", "zor\u00b7ni\u00b7gem", "Ge\u00b7wieh'r"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Schleudert es den Herrn zur Erde.", "tokens": ["Schleu\u00b7dert", "es", "den", "Herrn", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und man h\u00f6rt die Wasser toben,", "tokens": ["Und", "man", "h\u00f6rt", "die", "Was\u00b7ser", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Weil es stille ward vom Kampf,", "tokens": ["Weil", "es", "stil\u00b7le", "ward", "vom", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Nur im grauen Nebeldampf", "tokens": ["Nur", "im", "grau\u00b7en", "Ne\u00b7bel\u00b7dampf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "K\u00e4mpft der Wind im Walde droben.", "tokens": ["K\u00e4mpft", "der", "Wind", "im", "Wal\u00b7de", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Auf des Feindes Angesichte", "tokens": ["Auf", "des", "Fein\u00b7des", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Kehrt mit Frieden ein der Tod,", "tokens": ["Kehrt", "mit", "Frie\u00b7den", "ein", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Wischet ab des Zornes Rot,", "tokens": ["Wi\u00b7schet", "ab", "des", "Zor\u00b7nes", "Rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Ueberzieht's mit blassem Lichte.", "tokens": ["Ue\u00b7ber\u00b7zieht's", "mit", "blas\u00b7sem", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Und es nahet sich der Leiche", "tokens": ["Und", "es", "na\u00b7het", "sich", "der", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Abgestiegen von dem Pferd", "tokens": ["Ab\u00b7ge\u00b7stie\u00b7gen", "von", "dem", "Pferd"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Auch der K\u00e4mpfer Adalbert,", "tokens": ["Auch", "der", "K\u00e4mp\u00b7fer", "A\u00b7dal\u00b7bert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Schaut in's Antlitz ihm, in's bleiche.", "tokens": ["Schaut", "in's", "Ant\u00b7litz", "ihm", ",", "in's", "blei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "$,", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Lauter bei des Grabes Stille", "tokens": ["Lau\u00b7ter", "bei", "des", "Gra\u00b7bes", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Schl\u00e4gt lebend'ges Menschenherz,", "tokens": ["Schl\u00e4gt", "le\u00b7ben\u00b7d'\u00b7ges", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Groll und Zorn flieht niederw\u00e4rts,", "tokens": ["Groll", "und", "Zorn", "flieht", "nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Und die Brust bewegt der Wille.", "tokens": ["Und", "die", "Brust", "be\u00b7wegt", "der", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Jetzt erbarmt ihn erst der Sch\u00f6ne,", "tokens": ["Jetzt", "er\u00b7barmt", "ihn", "erst", "der", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die das Schicksal f\u00fcr ihn schlug,", "tokens": ["Die", "das", "Schick\u00b7sal", "f\u00fcr", "ihn", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "O wie holde Z\u00fcge trug", "tokens": ["O", "wie", "hol\u00b7de", "Z\u00fc\u00b7ge", "trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Dieser J\u00fcngling, wert der Thr\u00e4ne!", "tokens": ["Die\u00b7ser", "J\u00fcng\u00b7ling", ",", "wert", "der", "Thr\u00e4\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Und er hat den Feind umfangen,", "tokens": ["Und", "er", "hat", "den", "Feind", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Wie den Bruder seiner Wahl: \u2013", "tokens": ["Wie", "den", "Bru\u00b7der", "sei\u00b7ner", "Wahl", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Da zuerst durchs wilde Thal", "tokens": ["Da", "zu\u00b7erst", "durchs", "wil\u00b7de", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Ist des Friedens Geist gegangen.", "tokens": ["Ist", "des", "Frie\u00b7dens", "Geist", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Und die Sonne dringet nieder", "tokens": ["Und", "die", "Son\u00b7ne", "drin\u00b7get", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Durch der Nebel alte Nacht,", "tokens": ["Durch", "der", "Ne\u00b7bel", "al\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Da\u00df der gr\u00fcnen Wildni\u00df Pracht,", "tokens": ["Da\u00df", "der", "gr\u00fc\u00b7nen", "Wild\u00b7ni\u00df", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Fels und Strom, von Licht gl\u00e4nzt wieder.", "tokens": ["Fels", "und", "Strom", ",", "von", "Licht", "gl\u00e4nzt", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "APPR", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Wie den Sieger, auf der Bahre,", "tokens": ["Wie", "den", "Sie\u00b7ger", ",", "auf", "der", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "F\u00fchret den gefallnen Feind", "