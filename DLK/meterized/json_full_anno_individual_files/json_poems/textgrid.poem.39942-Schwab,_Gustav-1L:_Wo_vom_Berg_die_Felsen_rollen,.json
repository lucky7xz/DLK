, "line.73": {"text": "Sei Natur im Kampf geschieden,", "tokens": ["Sei", "Na\u00b7tur", "im", "Kampf", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Krieg der blinden Kr\u00e4fte Ruhm:", "tokens": ["Krieg", "der", "blin\u00b7den", "Kr\u00e4f\u00b7te", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.75": {"text": "Als sein heilig Eigentum", "tokens": ["Als", "sein", "hei\u00b7lig", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.76": {"text": "Pflege doch der Mensch den Frieden!", "tokens": ["Pfle\u00b7ge", "doch", "der", "Mensch", "den", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo vom Berg die Felsen rollen,", "tokens": ["Wo", "vom", "Berg", "die", "Fel\u00b7sen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo kein Wasser friedlich flie\u00dft,", "tokens": ["Wo", "kein", "Was\u00b7ser", "fried\u00b7lich", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Sturze sich ergie\u00dft,", "tokens": ["Nur", "im", "Stur\u00b7ze", "sich", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "