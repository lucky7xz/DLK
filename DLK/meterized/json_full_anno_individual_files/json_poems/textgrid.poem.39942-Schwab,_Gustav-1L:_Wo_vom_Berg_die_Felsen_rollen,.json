", "measure": "trochaic.tetra"}, "line.17": {"text": "An der \u00fcberspritzten Klippe", "tokens": ["An", "der", "\u00fc\u00b7bers\u00b7pritz\u00b7ten", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "K\u00e4mpfen sie im Wasserschaum,", "tokens": ["K\u00e4mp\u00b7fen", "sie", "im", "Was\u00b7ser\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "K\u00e4mpfen, wo f\u00fcr zween ist Raum", "tokens": ["K\u00e4mp\u00b7fen", ",", "wo", "f\u00fcr", "zween", "ist", "Raum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "VVINF", "VAFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Auf der wald'gen Felsenrippe.", "tokens": ["Auf", "der", "wald'\u00b7gen", "Fel\u00b7sen\u00b7rip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dann im Anger und im Thale", "tokens": ["Dann", "im", "An\u00b7ger", "und", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Jeder mit ergrimmtem Tro\u00df,", "tokens": ["Je\u00b7der", "mit", "er\u00b7grimm\u00b7tem", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Jeder auf dem wild'sten Ro\u00df;", "tokens": ["Je\u00b7der", "auf", "dem", "wild'\u00b7sten", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Lang erhitzt sich Stahl am Stahle.", "tokens": ["Lang", "er\u00b7hitzt", "sich", "Stahl", "am", "Stah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Beider Mut ist stets der gleiche,", "tokens": ["Bei\u00b7der", "Mut", "ist", "stets", "der", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Beider Sehne gleich gest\u00e4hlt,", "tokens": ["Bei\u00b7der", "Seh\u00b7ne", "gleich", "ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Beide gleicher Ha\u00df beseelt,", "tokens": ["Bei\u00b7de", "glei\u00b7cher", "Ha\u00df", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Keiner weicht dem letzten Streiche.", "tokens": ["Kei\u00b7ner", "weicht", "dem", "letz\u00b7ten", "Strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Bis die Herrscherin der Gegend", "tokens": ["Bis", "die", "Herr\u00b7sche\u00b7rin", "der", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "In den Streit sich mischt, Natur,", "tokens": ["In", "den", "Streit", "sich