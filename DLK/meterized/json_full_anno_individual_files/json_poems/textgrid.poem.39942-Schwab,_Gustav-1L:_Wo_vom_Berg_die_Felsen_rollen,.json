etra"}, "line.37": {"text": "Und man h\u00f6rt die Wasser toben,", "tokens": ["Und", "man", "h\u00f6rt", "die", "Was\u00b7ser", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Weil es stille ward vom Kampf,", "tokens": ["Weil", "es", "stil\u00b7le", "ward", "vom", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Nur im grauen Nebeldampf", "tokens": ["Nur", "im", "grau\u00b7en", "Ne\u00b7bel\u00b7dampf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "K\u00e4mpft der Wind im Walde droben.", "tokens": ["K\u00e4mpft", "der", "Wind", "im", "Wal\u00b7de", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Auf des Feindes Angesichte", "tokens": ["Auf", "des", "Fein\u00b7des", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Kehrt mit Frieden ein der Tod,", "tokens": ["Kehrt", "mit", "Frie\u00b7den", "ein", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Wischet ab des Zornes Rot,", "tokens": ["Wi\u00b7schet", "ab", "des", "Zor\u00b7nes", "Rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "