{"dta.poem.15038": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und meiner Adern Brunn, f\u00fcr dem Cristall nicht rein,", "tokens": ["Und", "mei\u00b7ner", "A\u00b7dern", "Brunn", ",", "f\u00fcr", "dem", "Cris\u00b7tall", "nicht", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Schwanen flekigt sind, soll ein Gef\u00e4sse seyn,", "tokens": ["Und", "Schwa\u00b7nen", "fle\u00b7kigt", "sind", ",", "soll", "ein", "Ge\u00b7f\u00e4s\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "VAFIN", "$,", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darinn der geile Hengst den Schaum der Unzucht spr\u00fcze?", "tokens": ["Da\u00b7rinn", "der", "gei\u00b7le", "Hengst", "den", "Schaum", "der", "Un\u00b7zucht", "spr\u00fc\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}