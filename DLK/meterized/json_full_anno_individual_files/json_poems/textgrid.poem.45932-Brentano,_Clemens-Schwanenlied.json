{"textgrid.poem.45932": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schwanenlied", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Augen brechen,", "tokens": ["Wenn", "die", "Au\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn die Lippen nicht mehr sprechen,", "tokens": ["Wenn", "die", "Lip\u00b7pen", "nicht", "mehr", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn das pochende Herz sich stillet", "tokens": ["Wenn", "das", "po\u00b7chen\u00b7de", "Herz", "sich", "stil\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und der warme Blutstrom nicht mehr quillet:", "tokens": ["Und", "der", "war\u00b7me", "Bluts\u00b7trom", "nicht", "mehr", "quil\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O dann sinkt der Traum zum Spiegel nieder,", "tokens": ["O", "dann", "sinkt", "der", "Traum", "zum", "Spie\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und ich h\u00f6r' der Engel Lieder wieder,", "tokens": ["Und", "ich", "h\u00f6r'", "der", "En\u00b7gel", "Lie\u00b7der", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Die das Leben mir vor\u00fcber trugen,", "tokens": ["Die", "das", "Le\u00b7ben", "mir", "vor\u00b7\u00fc\u00b7ber", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die so selig mit den Fl\u00fcgeln schlugen", "tokens": ["Die", "so", "se\u00b7lig", "mit", "den", "Fl\u00fc\u00b7geln", "schlu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ans Gel\u00e4ut der keuschen Maiesglocken,", "tokens": ["Ans", "Ge\u00b7l\u00e4ut", "der", "keu\u00b7schen", "Mai\u00b7es\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Da\u00df sie all die V\u00f6glein in den Tempel locken,", "tokens": ["Da\u00df", "sie", "all", "die", "V\u00f6\u00b7glein", "in", "den", "Tem\u00b7pel", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Die so s\u00fc\u00dfe wildentbrannte Psalmen sangen:", "tokens": ["Die", "so", "s\u00fc\u00b7\u00dfe", "wil\u00b7dent\u00b7brann\u00b7te", "Psal\u00b7men", "san\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Da\u00df die Liebe und die Lust so br\u00fcnstig rangen,", "tokens": ["Da\u00df", "die", "Lie\u00b7be", "und", "die", "Lust", "so", "br\u00fcns\u00b7tig", "ran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Bis das Leben war gefangen und empfangen;", "tokens": ["Bis", "das", "Le\u00b7ben", "war", "ge\u00b7fan\u00b7gen", "und", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Bis die Blumen bl\u00fchten;", "tokens": ["Bis", "die", "Blu\u00b7men", "bl\u00fch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Bis die Fr\u00fcchte gl\u00fchten,", "tokens": ["Bis", "die", "Fr\u00fcch\u00b7te", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Und gereift zum Scho\u00df der Erde fielen,", "tokens": ["Und", "ge\u00b7reift", "zum", "Scho\u00df", "der", "Er\u00b7de", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Rund und bunt zum Spielen;", "tokens": ["Rund", "und", "bunt", "zum", "Spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Bis die goldnen Bl\u00e4tter an der Erde rauschten,", "tokens": ["Bis", "die", "gold\u00b7nen", "Bl\u00e4t\u00b7ter", "an", "der", "Er\u00b7de", "rauschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Und die Wintersterne sinnend lauschten,", "tokens": ["Und", "die", "Win\u00b7ters\u00b7ter\u00b7ne", "sin\u00b7nend", "lauschten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Wo der st\u00fcrmende S\u00e4mann hin sie s\u00e4et,", "tokens": ["Wo", "der", "st\u00fcr\u00b7men\u00b7de", "S\u00e4\u00b7mann", "hin", "sie", "s\u00e4et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Da\u00df ein neuer Fr\u00fchling sch\u00f6n erstehet.", "tokens": ["Da\u00df", "ein", "neu\u00b7er", "Fr\u00fch\u00b7ling", "sch\u00f6n", "er\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Stille wird's, es gl\u00e4nzt der Schnee am H\u00fcgel", "tokens": ["Stil\u00b7le", "wird's", ",", "es", "gl\u00e4nzt", "der", "Schnee", "am", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Und ich k\u00fchl' im Silberreif den schw\u00fclen Fl\u00fcgel,", "tokens": ["Und", "ich", "k\u00fchl'", "im", "Sil\u00b7ber\u00b7reif", "den", "schw\u00fc\u00b7len", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "M\u00f6cht' ihn hin nach neuem Fr\u00fchling z\u00fccken,", "tokens": ["M\u00f6cht'", "ihn", "hin", "nach", "neu\u00b7em", "Fr\u00fch\u00b7ling", "z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Da erstarret mich ein kalt Entz\u00fccken \u2013", "tokens": ["Da", "er\u00b7star\u00b7ret", "mich", "ein", "kalt", "Ent\u00b7z\u00fc\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Es erfriert mein Herz, ein See voll Wonne", "tokens": ["Es", "er\u00b7friert", "mein", "Herz", ",", "ein", "See", "voll", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Auf ihm gleitet still der Mond und sanft die Sonne", "tokens": ["Auf", "ihm", "glei\u00b7tet", "still", "der", "Mond", "und", "sanft", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "ART", "NN", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "Unter den sinnenden, denkenden, klugen Sternen", "tokens": ["Un\u00b7ter", "den", "sin\u00b7nen\u00b7den", ",", "den\u00b7ken\u00b7den", ",", "klu\u00b7gen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "PRELS", "$,", "ADJA", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.29": {"text": "Schau' ich mein Sternbild an in Himmelsfernen;", "tokens": ["Schau'", "ich", "mein", "Stern\u00b7bild", "an", "in", "Him\u00b7mels\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Alle Leiden sind Freuden, alle Schmerzen scherzen", "tokens": ["Al\u00b7le", "Lei\u00b7den", "sind", "Freu\u00b7den", ",", "al\u00b7le", "Schmer\u00b7zen", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$,", "PIAT", "NN", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Und das ganze Leben singt aus meinem Herzen:", "tokens": ["Und", "das", "gan\u00b7ze", "Le\u00b7ben", "singt", "aus", "mei\u00b7nem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.32": {"text": "S\u00fc\u00dfer Tod, s\u00fc\u00dfer Tod", "tokens": ["S\u00fc\u00b7\u00dfer", "Tod", ",", "s\u00fc\u00b7\u00dfer", "Tod"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.33": {"text": "Zwischen dem Morgen- und Abendrot.", "tokens": ["Zwi\u00b7schen", "dem", "Mor\u00b7gen", "und", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wenn die Augen brechen,", "tokens": ["Wenn", "die", "Au\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn die Lippen nicht mehr sprechen,", "tokens": ["Wenn", "die", "Lip\u00b7pen", "nicht", "mehr", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn das pochende Herz sich stillet", "tokens": ["Wenn", "das", "po\u00b7chen\u00b7de", "Herz", "sich", "stil\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und der warme Blutstrom nicht mehr quillet:", "tokens": ["Und", "der", "war\u00b7me", "Bluts\u00b7trom", "nicht", "mehr", "quil\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O dann sinkt der Traum zum Spiegel nieder,", "tokens": ["O", "dann", "sinkt", "der", "Traum", "zum", "Spie\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und ich h\u00f6r' der Engel Lieder wieder,", "tokens": ["Und", "ich", "