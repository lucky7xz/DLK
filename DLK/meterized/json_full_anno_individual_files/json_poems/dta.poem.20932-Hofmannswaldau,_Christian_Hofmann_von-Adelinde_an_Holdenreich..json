{"dta.poem.20932": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Adelinde an Holdenreich.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Du kennst ja das Spital/ in dem ich itzt mu\u00df blei-", "tokens": ["Du", "kennst", "ja", "das", "Spi\u00b7tal", "/", "in", "dem", "ich", "itzt", "mu\u00df", "blei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "ADV", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Du nennest meine Noth aus Schertz den kal-", "tokens": ["Du", "nen\u00b7nest", "mei\u00b7ne", "Noth", "aus", "Schertz", "den", "ka\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "ten Brandt/", "tokens": ["ten", "Brandt", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Gewi\u00dflich kalt genug! was will ich viel beschreiben?", "tokens": ["Ge\u00b7wi\u00df\u00b7lich", "kalt", "ge\u00b7nug", "!", "was", "will", "ich", "viel", "be\u00b7schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein\u2019 Angst ist dir so wohl als meine Faust bekandt.", "tokens": ["Mein'", "Angst", "ist", "dir", "so", "wohl", "als", "mei\u00b7ne", "Faust", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich solte Furcht und Scham ietzt wohl zur\u00fccke hal-", "tokens": ["Mich", "sol\u00b7te", "Furcht", "und", "Scham", "ietzt", "wohl", "zu\u00b7r\u00fc\u00b7cke", "ha\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "ADV", "ADV", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "ten/", "tokens": ["ten", "/"], "token_info": ["word", "punct"], "pos": ["FM", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Doch Lieb und Ungedult vertr\u00e4gt den Z\u00fcgel nicht/", "tokens": ["Doch", "Lieb", "und", "Un\u00b7ge\u00b7dult", "ver\u00b7tr\u00e4gt", "den", "Z\u00fc\u00b7gel", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Eh\u2019 als der Jugend Gluth soll unterm Ei\u00df erkalten/", "tokens": ["Eh'", "als", "der", "Ju\u00b7gend", "Gluth", "soll", "un\u00b7term", "Ei\u00df", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So will ich lieber seyn durch Hencker hingericht.", "tokens": ["So", "will", "ich", "lie\u00b7ber", "seyn", "durch", "Hen\u00b7cker", "hin\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VAINF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df ist ein hartes Wort und in der Frauen Munde", "tokens": ["Di\u00df", "ist", "ein", "har\u00b7tes", "Wort", "und", "in", "der", "Frau\u00b7en", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein ungemeiner Spruch; doch wer mein Hertze kenne/", "tokens": ["Ein", "un\u00b7ge\u00b7mei\u00b7ner", "Spruch", ";", "doch", "wer", "mein", "Hert\u00b7ze", "ken\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PWS", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der glaube was itzt folgt. Es steht auf festem Grunde:", "tokens": ["Der", "glau\u00b7be", "was", "itzt", "folgt", ".", "Es", "steht", "auf", "fes\u00b7tem", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PWS", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Alte/ so mich plagt/ mu\u00df seyn von mir getrennt.", "tokens": ["Der", "Al\u00b7te", "/", "so", "mich", "plagt", "/", "mu\u00df", "seyn", "von", "mir", "ge\u00b7trennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$(", "VMFIN", "PPOSAT", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was soll mein warmer Leib in seinen kalten Armen?", "tokens": ["Was", "soll", "mein", "war\u00b7mer", "Leib", "in", "sei\u00b7nen", "kal\u00b7ten", "Ar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was soll mir denn ein Ku\u00df der nach dem Grabe", "tokens": ["Was", "soll", "mir", "denn", "ein", "Ku\u00df", "der", "nach", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "schmeckt?", "tokens": ["schmeckt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Was soll mir den\u0303 ein Mann/ der niemahls kan erwar-", "tokens": ["Was", "soll", "mir", "de\u00f1", "ein", "Mann", "/", "der", "nie\u00b7mahls", "kan", "er\u00b7wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "$(", "ART", "ADV", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "men?", "tokens": ["men", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Und seine beste Krafft aus einer Krause leckt?", "tokens": ["Und", "sei\u00b7ne", "bes\u00b7te", "Krafft", "aus", "ei\u00b7ner", "Krau\u00b7se", "leckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der ohne Brillen mich nicht eigen kan betrachten/", "tokens": ["Der", "oh\u00b7ne", "Bril\u00b7len", "mich", "nicht", "ei\u00b7gen", "kan", "be\u00b7trach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und mehr das Spiel der Jagt als meine Jugend sucht;", "tokens": ["Und", "mehr", "das", "Spiel", "der", "Jagt", "als", "mei\u00b7ne", "Ju\u00b7gend", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "VVFIN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Grafe/ l\u00e4st du mich in dieser Noht verschmachte\u0303/", "tokens": ["Mein", "Gra\u00b7fe", "/", "l\u00e4st", "du", "mich", "in", "die\u00b7ser", "Noht", "ver\u00b7schmacht\u1ebd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So scheint es ja/ ich sey von der Natur verflucht.", "tokens": ["So", "scheint", "es", "ja", "/", "ich", "sey", "von", "der", "Na\u00b7tur", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Finger schmecken mir noch nach dem starcken", "tokens": ["Die", "Fin\u00b7ger", "schme\u00b7cken", "mir", "noch", "nach", "dem", "star\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Oehlen/", "tokens": ["O\u00b7eh\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Damit ich seinen Leib fast t\u00e4glich schmieren mu\u00df/", "tokens": ["Da\u00b7mit", "ich", "sei\u00b7nen", "Leib", "fast", "t\u00e4g\u00b7lich", "schmie\u00b7ren", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es will zu keiner Zeit mir hier an Marter fehlen/", "tokens": ["Es", "will", "zu", "kei\u00b7ner", "Zeit", "mir", "hier", "an", "Mar\u00b7ter", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch stinckt mir nichts so sehr/ als sein verfaulter Ku\u00df.", "tokens": ["Doch", "stinckt", "mir", "nichts", "so", "sehr", "/", "als", "sein", "ver\u00b7faul\u00b7ter", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "ADV", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bald liegt er an der Gicht/ bald liegt er an dem Steine/", "tokens": ["Bald", "liegt", "er", "an", "der", "Gicht", "/", "bald", "liegt", "er", "an", "dem", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dann werd\u2019 als W\u00e4rterin/ ich Tag und Nacht ge-", "tokens": ["Dann", "werd'", "als", "W\u00e4r\u00b7te\u00b7rin", "/", "ich", "Tag", "und", "Nacht", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOKOM", "NN", "$(", "PPER", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "plagt/", "tokens": ["plagt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Dann salb\u2019 ich seinen Leib und seine d\u00fcrre Beine/", "tokens": ["Dann", "salb'", "ich", "sei\u00b7nen", "Leib", "und", "sei\u00b7ne", "d\u00fcr\u00b7re", "Bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und wenn es besser wird/ so eilt er auff die Jagt.", "tokens": ["Und", "wenn", "es", "bes\u00b7ser", "wird", "/", "so", "eilt", "er", "auff", "die", "Jagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dann ist mein gantzer Leib in Einsamkeit verschlossen/", "tokens": ["Dann", "ist", "mein", "gant\u00b7zer", "Leib", "in", "Ein\u00b7sam\u00b7keit", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und wallet wie ein Schiff auff seinen Kummer Meer/", "tokens": ["Und", "wal\u00b7let", "wie", "ein", "Schiff", "auff", "sei\u00b7nen", "Kum\u00b7mer", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich dencke wie ich nichts von rechter Lust genossen/", "tokens": ["Ich", "den\u00b7cke", "wie", "ich", "nichts", "von", "rech\u00b7ter", "Lust", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "PIS", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch macht mir dieses nicht so viel verdru\u00df/ als Er.", "tokens": ["Doch", "macht", "mir", "die\u00b7ses", "nicht", "so", "viel", "ver\u00b7dru\u00df", "/", "als", "Er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "PTKNEG", "ADV", "PIAT", "NN", "$(", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Verl\u00e4st er seine Jagt und kommet nun zur\u00fccke/", "tokens": ["Ver\u00b7l\u00e4st", "er", "sei\u00b7ne", "Jagt", "und", "kom\u00b7met", "nun", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So ist die Mattigkeit/ was er erjaget hat/", "tokens": ["So", "ist", "die", "Mat\u00b7tig\u00b7keit", "/", "was", "er", "er\u00b7ja\u00b7get", "hat", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PWS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Bald will er einen Stuel/ bald fodert er die Kr\u00fccke/", "tokens": ["Bald", "will", "er", "ei\u00b7nen", "Stu\u00b7el", "/", "bald", "fo\u00b7dert", "er", "die", "Kr\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Bald sucht er zitternde beym Apothecker Rath.", "tokens": ["Bald", "sucht", "er", "zit\u00b7tern\u00b7de", "beym", "A\u00b7pot\u00b7hec\u00b7ker", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da ist kein Feuer mehr ihn satsam zu erhitzen/", "tokens": ["Da", "ist", "kein", "Feu\u00b7er", "mehr", "ihn", "sat\u00b7sam", "zu", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dann weht die Ungedult ihm