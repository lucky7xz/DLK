{"dta.poem.18146": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  aufgelebte Welt  \n im Fr\u00fchlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst lag noch die erstorbne Welt,", "tokens": ["J\u00fcngst", "lag", "noch", "die", "ers\u00b7torb\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit Schnee und Eis, als todt, be-", "tokens": ["Mit", "Schnee", "und", "Eis", ",", "als", "todt", ",", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "decket:", "tokens": ["de\u00b7cket", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Nun da die Fr\u00fchlings Sonn das Feld,", "tokens": ["Nun", "da", "die", "Fr\u00fch\u00b7lings", "Sonn", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch ihre Strahlen abgelecket,", "tokens": ["Durch", "ih\u00b7re", "Strah\u00b7len", "ab\u00b7ge\u00b7le\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Lebt die Natur von neuen auf,", "tokens": ["Lebt", "die", "Na\u00b7tur", "von", "neu\u00b7en", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr Todtenkleid hat sich verlohren,", "tokens": ["Ihr", "Tod\u00b7ten\u00b7kleid", "hat", "sich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wird beym n\u00e4hern Sonnenlauf,", "tokens": ["Und", "wird", "beym", "n\u00e4\u00b7hern", "Son\u00b7nen\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gleichsam mit neuer Krafft gebohren.", "tokens": ["Gleich\u00b7sam", "mit", "neu\u00b7er", "Krafft", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Die Erde keimt die Frucht hervor,", "tokens": ["Die", "Er\u00b7de", "keimt", "die", "Frucht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die sie im Winter eingesogen,", "tokens": ["Die", "sie", "im", "Win\u00b7ter", "ein\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und zeigt sich im verj\u00fcngten Flor,", "tokens": ["Und", "zeigt", "sich", "im", "ver\u00b7j\u00fcng\u00b7ten", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den sie nun wieder angezogen;", "tokens": ["Den", "sie", "nun", "wie\u00b7der", "an\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Es gr\u00fcnen, Auen, Feld und Wald", "tokens": ["Es", "gr\u00fc\u00b7nen", ",", "Au\u00b7en", ",", "Feld", "und", "Wald"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und wirken eine Sonnenhaube,", "tokens": ["Und", "wir\u00b7ken", "ei\u00b7ne", "Son\u00b7nen\u00b7hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "F\u00fcr dem, was lebt, zum Aufenthalt", "tokens": ["F\u00fcr", "dem", ",", "was", "lebt", ",", "zum", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PWS", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Von Blumen, Saaten, frischen Laube.", "tokens": ["Von", "Blu\u00b7men", ",", "Saa\u00b7ten", ",", "fri\u00b7schen", "Lau\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dies merkt der V\u00f6gel muntres Heer", "tokens": ["Dies", "merkt", "der", "V\u00f6\u00b7gel", "mun\u00b7tres", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und k\u00f6mt mit freudigen Gefieder,", "tokens": ["Und", "k\u00f6mt", "mit", "freu\u00b7di\u00b7gen", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und f\u00fcllt die Lufft, zu GOttes Ehr,", "tokens": ["Und", "f\u00fcllt", "die", "Lufft", ",", "zu", "Got\u00b7tes", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mit neuen Lustges\u00e4ngen wieder:", "tokens": ["Mit", "neu\u00b7en", "Lust\u00b7ge\u00b7s\u00e4n\u00b7gen", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ein Theil schwingt sich in warmer Lufft;", "tokens": ["Ein", "Theil", "schwingt", "sich", "in", "war\u00b7mer", "Lufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Ein anders klammert sich an Zweigen,", "tokens": ["Ein", "an\u00b7ders", "klam\u00b7mert", "sich", "an", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da eins zum andern schwitschernd rufft,", "tokens": ["Da", "eins", "zum", "an\u00b7dern", "schwit\u00b7schernd", "rufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "ADJA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die Fr\u00fchlings Zeit, sich anzuzeigen.", "tokens": ["Die", "Fr\u00fch\u00b7lings", "Zeit", ",", "sich", "an\u00b7zu\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Das Fusvolk, das im Thierreich lebt,", "tokens": ["Das", "Fus\u00b7volk", ",", "das", "im", "Thier\u00b7reich", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Kreucht auch aus den verborgnen H\u00f6len,", "tokens": ["Kreucht", "auch", "aus", "den", "ver\u00b7borg\u00b7nen", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und l\u00e4uft, da sich die Lufft bewegt,", "tokens": ["Und", "l\u00e4uft", ",", "da", "sich", "die", "Lufft", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gleichfals das Erdreich zu beseelen.", "tokens": ["Gleich\u00b7fals", "das", "Er\u00b7dreich", "zu", "be\u00b7see\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Der laue Frosch erwacht im Teich,", "tokens": ["Der", "lau\u00b7e", "Frosch", "er\u00b7wacht", "im", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und quakst, da andre im Geb\u00fcschen,", "tokens": ["Und", "quakst", ",", "da", "and\u00b7re", "im", "Ge\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Ob der empfundnen Lust zugleich,", "tokens": ["Ob", "der", "emp\u00b7fund\u00b7nen", "Lust", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Mit hei\u00dfrer Kehle murmelnd zischen.", "tokens": ["Mit", "hei\u00df\u00b7rer", "Keh\u00b7le", "mur\u00b7melnd", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die Auen sind belebt vom Vieh,", "tokens": ["Die", "Au\u00b7en", "sind", "be\u00b7lebt", "vom", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die aus dem St\u00e4llen losgelassen,", "tokens": ["Die", "aus", "dem", "St\u00e4l\u00b7len", "los\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Um da mit einer s\u00fcssen M\u00fch,", "tokens": ["Um", "da", "mit", "ei\u00b7ner", "s\u00fcs\u00b7sen", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Den jungen Grasklee aufzufassen:", "tokens": ["Den", "jun\u00b7gen", "Gras\u00b7klee", "auf\u00b7zu\u00b7fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Sie finden ihren Tisch gedekt,", "tokens": ["Sie", "fin\u00b7den", "ih\u00b7ren", "Tisch", "ge\u00b7dekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Mit aufgekeimter Nahrungsbl\u00fcthe;", "tokens": ["Mit", "auf\u00b7ge\u00b7keim\u00b7ter", "Nah\u00b7rungs\u00b7bl\u00fct\u00b7he", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und werden auch dadurch erwekt,", "tokens": ["Und", "wer\u00b7den", "auch", "da\u00b7durch", "er\u00b7wekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Zu preisen ihres Sch\u00f6pfers G\u00fcte.", "tokens": ["Zu", "prei\u00b7sen", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Der Schafe bl\u00f6kendes Geschrey,", "tokens": ["Der", "Scha\u00b7fe", "bl\u00f6\u00b7ken\u00b7des", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Erth\u00f6nt in aufgegr\u00fcnten Trifften,", "tokens": ["Er\u00b7th\u00f6nt", "in", "auf\u00b7ge\u00b7gr\u00fcn\u00b7ten", "Triff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Dem der da alles machet neu,", "tokens": ["Dem", "der", "da", "al\u00b7les", "ma\u00b7chet", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "KOUS", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Gleichsam ein Danklied auch zu stifften.", "tokens": ["Gleich\u00b7sam", "ein", "Dank\u00b7lied", "auch", "zu", "stiff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Das zarte Lam springt hin und her,", "tokens": ["Das", "zar\u00b7te", "Lam", "springt", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und rupft die Erd, als wen es w\u00fcste,", "tokens": ["Und", "rupft", "die", "Erd", ",", "als", "wen", "es", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Da\u00df wen der Mutter Eyter leer,", "tokens": ["Da\u00df", "wen", "der", "Mut\u00b7ter", "Ey\u00b7ter", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Es diese Mutter n\u00e4hren m\u00fcste.", "tokens": ["Es", "die\u00b7se", "Mut\u00b7ter", "n\u00e4h\u00b7ren", "m\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Es frist mit unversuchten Trieb,", "tokens": ["Es", "frist", "mit", "un\u00b7ver\u00b7such\u00b7ten", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und schmekt die safftig fetten Keimen,", "tokens": ["Und", "schmekt", "die", "saff\u00b7tig", "fet\u00b7ten", "Kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und scheint darob die Mutter Lieb,", "tokens": ["Und", "scheint", "da\u00b7rob", "die", "Mut\u00b7ter", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Bey dieser Kost fast zu vers\u00e4umen:", "tokens": ["Bey", "die\u00b7ser", "Kost", "fast", "zu", "ver\u00b7s\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und wen es sich den satt gegras\u2019t,", "tokens": ["Und", "wen", "es", "sich", "den", "satt", "ge\u00b7gras't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PRF", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "So legt es seine sanfften Glieder,", "tokens": ["So", "legt", "es", "sei\u00b7ne", "sanff\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Bey stiller und vergn\u00fcgter Rast,", "tokens": ["Bey", "stil\u00b7ler", "und", "ver\u00b7gn\u00fcg\u00b7ter", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Ins weiche Blumen Bette nieder.", "tokens": ["Ins", "wei\u00b7che", "Blu\u00b7men", "Bet\u00b7te", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Der Rinder Trifften sind da auch,", "tokens": ["Der", "Rin\u00b7der", "Triff\u00b7ten", "sind", "da", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "So bald die Morgenr\u00f6th erwachet,", "tokens": ["So", "bald", "die", "Mor\u00b7gen\u00b7r\u00f6th", "er\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Und durch der N\u00e4chte Nebelrauch,", "tokens": ["Und", "durch", "der", "N\u00e4ch\u00b7te", "Ne\u00b7bel\u00b7rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Die schimmernd graue Demmrung machet;", "tokens": ["Die", "schim\u00b7mernd", "grau\u00b7e", "Demm\u00b7rung", "ma\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Sie kosten ihren frischen Klee,", "tokens": ["Sie", "kos\u00b7ten", "ih\u00b7ren", "fri\u00b7schen", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Vom Thau beperlt, vom Licht verg\u00fcldet,", "tokens": ["Vom", "Thau", "be\u00b7perlt", ",", "vom", "Licht", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Und sehen schmatzend in die H\u00f6h,", "tokens": ["Und", "se\u00b7hen", "schmat\u00b7zend", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Zu dem, der darin abgebildet", "tokens": ["Zu", "dem", ",", "der", "da\u00b7rin", "ab\u00b7ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PAV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Dem folgt, so bald es heller Tag,", "tokens": ["Dem", "folgt", ",", "so", "bald", "es", "hel\u00b7ler", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADV", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Ein schnatternd kreischendes Gewimmel", "tokens": ["Ein", "schnat\u00b7ternd", "krei\u00b7schen\u00b7des", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Von G\u00e4nsen, auf dem Anger nach,", "tokens": ["Von", "G\u00e4n\u00b7sen", ",", "auf", "dem", "An\u00b7ger", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.69": {"text": "Und lagert da auch sein Get\u00fcmmel,", "tokens": ["Und", "la\u00b7gert", "da", "auch", "sein", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.70": {"text": "Jhr Schnab lsucht den frischen Keim,", "tokens": ["Ihr", "Schnab", "lsucht", "den", "fri\u00b7schen", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.71": {"text": "So bald sie nur denselben schmecken,", "tokens": ["So", "bald", "sie", "nur", "den\u00b7sel\u00b7ben", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.72": {"text": "Pflegt sich nach diesen Honigseim,", "tokens": ["Pflegt", "sich", "nach", "die\u00b7sen", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Nochmehr Begierd und Hals zu strecken.", "tokens": ["Noch\u00b7mehr", "Be\u00b7gierd", "und", "Hals", "zu", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.74": {"text": "Sie werden satt bey dieser Lust,", "tokens": ["Sie", "wer\u00b7den", "satt", "bey", "die\u00b7ser", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.75": {"text": "Nach langen emsigen Gew\u00fchle,", "tokens": ["Nach", "lan\u00b7gen", "em\u00b7si\u00b7gen", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.76": {"text": "Es reget sich in ihrer Brust", "tokens": ["Es", "re\u00b7get", "sich", "in", "ih\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.77": {"text": "Ein treibend freudiges Gef\u00fchle:", "tokens": ["Ein", "trei\u00b7bend", "freu\u00b7di\u00b7ges", "Ge\u00b7f\u00fch\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.78": {"text": "Sie dehnen ihrer Fl\u00fcgel Krafft,", "tokens": ["Sie", "deh\u00b7nen", "ih\u00b7rer", "Fl\u00fc\u00b7gel", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.79": {"text": "Ausbreitent sich empor zu schwingen,", "tokens": ["Aus\u00b7brei\u00b7tent", "sich", "em\u00b7por", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.80": {"text": "Als wen sie, dem der alles schafft,", "tokens": ["Als", "wen", "sie", ",", "dem", "der", "al\u00b7les", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "$,", "PRELS", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.81": {"text": "Drauf wolten ein Dankopfer bringen.", "tokens": ["Drauf", "wol\u00b7ten", "ein", "Dan\u00b7kop\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.82": {"text": "Dies sieh O! Mensch, mit Andacht an,", "tokens": ["Dies", "sieh", "O", "!", "Mensch", ",", "mit", "An\u00b7dacht", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$.", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.83": {"text": "Bey diesen frohen Fr\u00fchlings Zeiten;", "tokens": ["Bey", "die\u00b7sen", "fro\u00b7hen", "Fr\u00fch\u00b7lings", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.84": {"text": "Und la\u00df dein Hertz, das denken kan,", "tokens": ["Und", "la\u00df", "dein", "Hertz", ",", "das", "den\u00b7ken", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "$,", "PDS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.85": {"text": "Dadurch zum grossen Sch\u00f6pfer leiten.", "tokens": ["Da\u00b7durch", "zum", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.86": {"text": "Du siehst hiebey ein sch\u00f6nes Bild", "tokens": ["Du", "sie