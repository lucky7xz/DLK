{"dta.poem.18405": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ludovicus  XIV.  K\u00f6nig in Franckreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es h\u00e4lt die schmeicheley dein thun vor meisterst\u00fccke/", "tokens": ["Es", "h\u00e4lt", "die", "schmei\u00b7che\u00b7ley", "dein", "thun", "vor", "meis\u00b7ter\u00b7st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "VVINF", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nennet dich dadurch den grossen deiner zeit.", "tokens": ["Und", "nen\u00b7net", "dich", "da\u00b7durch", "den", "gros\u00b7sen", "dei\u00b7ner", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und freylich bistu gro\u00df an muth und an gel\u00fccke/", "tokens": ["Und", "frey\u00b7lich", "bis\u00b7tu", "gro\u00df", "an", "muth", "und", "an", "ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was am meisten gilt/ an ungerechtigkeit.", "tokens": ["Und", "was", "am", "meis\u00b7ten", "gilt", "/", "an", "un\u00b7ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "PIS", "VVFIN", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}