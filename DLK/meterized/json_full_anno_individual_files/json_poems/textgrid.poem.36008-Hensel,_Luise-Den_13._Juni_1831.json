{"textgrid.poem.36008": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Den 13. Juni 1831", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei uns gegr\u00fc\u00dft, o sch\u00f6ne Morgenr\u00f6te!", "tokens": ["Sei", "uns", "ge\u00b7gr\u00fc\u00dft", ",", "o", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schl\u00e4gt dir unser Herz voll Fr\u00f6hlichkeit. \u2013", "tokens": ["Es", "schl\u00e4gt", "dir", "un\u00b7ser", "Herz", "voll", "Fr\u00f6h\u00b7lich\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erh\u00f6r', o Gott! die heiligen Gebete,", "tokens": ["Er\u00b7h\u00f6r'", ",", "o", "Gott", "!", "die", "hei\u00b7li\u00b7gen", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unsre Seele dir voll Inbrunst weiht.", "tokens": ["Die", "uns\u00b7re", "See\u00b7le", "dir", "voll", "In\u00b7brunst", "weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir flehen heut f\u00fcr unsrer Mutter Gl\u00fcck;", "tokens": ["Wir", "fle\u00b7hen", "heut", "f\u00fcr", "uns\u00b7rer", "Mut\u00b7ter", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O sto\u00df die frommen W\u00fcnsche nicht zur\u00fcck!", "tokens": ["O", "sto\u00df", "die", "from\u00b7men", "W\u00fcn\u00b7sche", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir danken dir; du gabst ihr neues Leben", "tokens": ["Wir", "dan\u00b7ken", "dir", ";", "du", "gabst", "ihr", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wandtest ab der herben Krankheit Schmerz.", "tokens": ["Und", "wand\u00b7test", "ab", "der", "her\u00b7ben", "Krank\u00b7heit", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun soll dich freudig unser Lied erheben,", "tokens": ["Nun", "soll", "dich", "freu\u00b7dig", "un\u00b7ser", "Lied", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir dankend schlagen unser frohes Herz.", "tokens": ["Dir", "dan\u00b7kend", "schla\u00b7gen", "un\u00b7ser", "fro\u00b7hes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir, guter Vater! schalle unser Sang,", "tokens": ["Dir", ",", "gu\u00b7ter", "Va\u00b7ter", "!", "schal\u00b7le", "un\u00b7ser", "Sang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dir jauchze unsre ganze Seele Dank. \u2013", "tokens": ["Dir", "jauch\u00b7ze", "uns\u00b7re", "gan\u00b7ze", "See\u00b7le", "Dank", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Willkommen uns, o du, der M\u00fctter beste!", "tokens": ["Will\u00b7kom\u00b7men", "uns", ",", "o", "du", ",", "der", "M\u00fct\u00b7ter", "bes\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An diesem frohen, freudevollen Tag!", "tokens": ["An", "die\u00b7sem", "fro\u00b7hen", ",", "freu\u00b7de\u00b7vol\u00b7len", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sei uns gegr\u00fc\u00dft an deinem sch\u00f6nen Feste!", "tokens": ["Sei", "uns", "ge\u00b7gr\u00fc\u00dft", "an", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Fes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir schl\u00e4gt heut jubelnd unser Herzensschlag.", "tokens": ["Dir", "schl\u00e4gt", "heut", "ju\u00b7belnd", "un\u00b7ser", "Her\u00b7zens\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Mutter! lies in deiner T\u00f6chter Blick", "tokens": ["O", "Mut\u00b7ter", "!", "lies", "in", "dei\u00b7ner", "T\u00f6ch\u00b7ter", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die w\u00e4rmsten W\u00fcnsche f\u00fcr dein Lebensgl\u00fcck.", "tokens": ["Die", "w\u00e4rms\u00b7ten", "W\u00fcn\u00b7sche", "f\u00fcr", "dein", "Le\u00b7bens\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es l\u00e4chle dir das Leben sch\u00f6n und helle,", "tokens": ["Es", "l\u00e4ch\u00b7le", "dir", "das", "Le\u00b7ben", "sch\u00f6n", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Zukunft sei dir wonnevoll und licht.", "tokens": ["Die", "Zu\u00b7kunft", "sei", "dir", "won\u00b7ne\u00b7voll", "und", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie flie\u00dfe dir des Kummers Tr\u00e4nenquelle", "tokens": ["Nie", "flie\u00b7\u00dfe", "dir", "des", "Kum\u00b7mers", "Tr\u00e4\u00b7nen\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von deinem frommen Mutterangesicht.", "tokens": ["Von", "dei\u00b7nem", "from\u00b7men", "Mut\u00b7ter\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gott gebe dir der reinsten Freuden viel", "tokens": ["Gott", "ge\u00b7be", "dir", "der", "reins\u00b7ten", "Freu\u00b7den", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und f\u00fchre sanft dich zu des Lebens Ziel!", "tokens": ["Und", "f\u00fch\u00b7re", "sanft", "dich", "zu", "des", "Le\u00b7bens", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir k\u00f6nnen dir nicht reiche Gaben spenden,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "dir", "nicht", "rei\u00b7che", "Ga\u00b7ben", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur diesen Blumenteppich, diesen Kranz,", "tokens": ["Nur", "die\u00b7sen", "Blu\u00b7men\u00b7tep\u00b7pich", ",", "die\u00b7sen", "Kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gewirket von der Liebe treuen H\u00e4nden", "tokens": ["Ge\u00b7wir\u00b7ket", "von", "der", "Lie\u00b7be", "treu\u00b7en", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schimmernd in der Farben buntem Glanz.", "tokens": ["Und", "schim\u00b7mernd", "in", "der", "Far\u00b7ben", "bun\u00b7tem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nimm hin die kleine Gabe, dir geweiht,", "tokens": ["Nimm", "hin", "die", "klei\u00b7ne", "Ga\u00b7be", ",", "dir", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sieh auf deiner T\u00f6chter Dankbarkeit.", "tokens": ["Und", "sieh", "auf", "dei\u00b7ner", "T\u00f6ch\u00b7ter", "Dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O! bleib uns auch in fernen Lebenstagen", "tokens": ["O", "!", "bleib", "uns", "auch", "in", "fer\u00b7nen", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch Mutter, wie du's uns gewesen bist \u2013", "tokens": ["Noch", "Mut\u00b7ter", ",", "wie", "du's", "uns", "ge\u00b7we\u00b7sen", "bist", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PWAV", "PIS", "PPER", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stets soll dir unsre Brust voll Liebe schlagen,", "tokens": ["Stets", "soll", "dir", "uns\u00b7re", "Brust", "voll", "Lie\u00b7be", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis einst das Grab auch unser Herz umschlie\u00dft.", "tokens": ["Bis", "einst", "das", "Grab", "auch", "un\u00b7ser", "Herz", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O dann, dann ruf' uns Gott zu ew'gem Gl\u00fcck,", "tokens": ["O", "dann", ",", "dann", "ruf'", "uns", "Gott", "zu", "ew'\u00b7gem", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und keine suche dann umsonst dein Blick.", "tokens": ["Und", "kei\u00b7ne", "su\u00b7che", "dann", "um\u00b7sonst", "dein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sei uns gegr\u00fc\u00dft, o sch\u00f6ne Morgenr\u00f6te!", "tokens": ["Sei", "uns", "ge\u00b7gr\u00fc\u00dft", ",", "o", "sch\u00f6\u00b7ne",