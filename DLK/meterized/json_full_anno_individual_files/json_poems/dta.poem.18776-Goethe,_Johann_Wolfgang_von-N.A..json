{"dta.poem.18776": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Kenne wohl der M\u00e4nner Blicke,", "tokens": ["Ken\u00b7ne", "wohl", "der", "M\u00e4n\u00b7ner", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer sagt: ich liebe, leide!", "tokens": ["Ei\u00b7ner", "sagt", ":", "ich", "lie\u00b7be", ",", "lei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich begehre, ja verzweifle!", "tokens": ["Ich", "be\u00b7geh\u00b7re", ",", "ja", "ver\u00b7zweif\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und was sonst ist kennt ein M\u00e4dchen.", "tokens": ["Und", "was", "sonst", "ist", "kennt", "ein", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VAFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles das kann mir nicht helfen,", "tokens": ["Al\u00b7les", "das", "kann", "mir", "nicht", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Alles das kann mich nicht r\u00fchren;", "tokens": ["Al\u00b7les", "das", "kann", "mich", "nicht", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Aber Hatem! deine Blicke", "tokens": ["A\u00b7ber", "Ha\u00b7tem", "!", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geben erst dem Tage Glanz.", "tokens": ["Ge\u00b7ben", "erst", "dem", "Ta\u00b7ge", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn sie sagen: ", "tokens": ["Denn", "sie", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Wie mir sonst nicht\u2019s mag gefallen.", "tokens": ["Wie", "mir", "sonst", "nicht's", "mag", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIS", "VMFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seh ich Rosen, seh ich Lilien,", "tokens": ["Seh", "ich", "Ro\u00b7sen", ",", "seh", "ich", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Aller G\u00e4rten Zier und Ehre,", "tokens": ["Al\u00b7ler", "G\u00e4r\u00b7ten", "Zier", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "So Cypressen, Myrten, Veilchen,", "tokens": ["So", "Cyp\u00b7res\u00b7sen", ",", "Myr\u00b7ten", ",", "Veil\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Aufgeregt zum Schmuck der Erde.", "tokens": ["Auf\u00b7ge\u00b7regt", "zum", "Schmuck", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und geschm\u00fcckt ist sie ein Wunder,", "tokens": ["Und", "ge\u00b7schm\u00fcckt", "ist", "sie", "ein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Mit Erstaunen uns umfangend,", "tokens": ["Mit", "Er\u00b7stau\u00b7nen", "uns", "um\u00b7fan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Uns erquickend, heilend, segnend,", "tokens": ["Uns", "er\u00b7qui\u00b7ckend", ",", "hei\u00b7lend", ",", "seg\u00b7nend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dass wir uns gesundet f\u00fchlen.", "tokens": ["Dass", "wir", "uns", "ge\u00b7sun\u00b7det", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wieder gern erkranken m\u00f6chten.", "tokens": ["Wie\u00b7der", "gern", "er\u00b7kran\u00b7ken", "m\u00f6ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Da erblicktest du Suleika", "tokens": ["Da", "er\u00b7blick\u00b7test", "du", "Sul\u00b7ei\u00b7ka"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Und gesundetest erkrankend,", "tokens": ["Und", "ge\u00b7sun\u00b7de\u00b7test", "er\u00b7kran\u00b7kend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und erkranketest gesundend,", "tokens": ["Und", "er\u00b7kran\u00b7ke\u00b7test", "ge\u00b7sun\u00b7dend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "L\u00e4cheltest und sahst her\u00fcber", "tokens": ["L\u00e4\u00b7chel\u00b7test", "und", "sahst", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wie du nie der Welt gel\u00e4chlet.", "tokens": ["Wie", "du", "nie", "der", "Welt", "ge\u00b7l\u00e4ch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und Suleika f\u00fchlt des Blickes", "tokens": ["Und", "Sul\u00b7ei\u00b7ka", "f\u00fchlt", "des", "Bli\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Ewge Rede: ", "tokens": ["Ew\u00b7ge", "Re\u00b7de", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Wie mir sonst nichts mag gefallen.", "tokens": ["Wie", "mir", "sonst", "nichts", "mag", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIS", "VMFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}