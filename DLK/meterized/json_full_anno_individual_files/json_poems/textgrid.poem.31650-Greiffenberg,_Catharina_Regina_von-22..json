{"textgrid.poem.31650": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer kan die Wunder-Bahn in Gottes Wegen finden?", "tokens": ["Wer", "kan", "die", "Wun\u00b7der\u00b7Bahn", "in", "Got\u00b7tes", "We\u00b7gen", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wer solt sein Wei\u00dfheit sich zu gr\u00fcnden unterwinden?", "tokens": ["wer", "solt", "sein", "Wei\u00df\u00b7heit", "sich", "zu", "gr\u00fcn\u00b7den", "un\u00b7ter\u00b7win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "denn / Meer-Abgr\u00fcndlich-tieff' und \u00fcber- Sternen hoch", "tokens": ["denn", "/", "Meer\u00b7Ab\u00b7gr\u00fcnd\u00b7lich\u00b7tief\u00b7f'", "und", "\u00fc\u00b7ber", "Ster\u00b7nen", "hoch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "NN", "KON", "TRUNC", "NN", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "ist Gottes W\u00fcrkungs-Zweck nie ausgesonnen noch.", "tokens": ["ist", "Got\u00b7tes", "W\u00fcr\u00b7kungs\u00b7Zweck", "nie", "aus\u00b7ge\u00b7son\u00b7nen", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Gott! es gn\u00fcget mir / da\u00df ich mich dich la\u00df lenken.", "tokens": ["Mein", "Gott", "!", "es", "gn\u00fc\u00b7get", "mir", "/", "da\u00df", "ich", "mich", "dich", "la\u00df", "len\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "will auf die Folge nur / nicht auf die Ziel-Frucht / denken:", "tokens": ["will", "auf", "die", "Fol\u00b7ge", "nur", "/", "nicht", "auf", "die", "Ziel\u00b7Frucht", "/", "den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "$(", "PTKNEG", "APPR", "ART", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.7": {"text": "geht alls in seine Quell / wie aller Wei\u00dfen Schlu\u00df /", "tokens": ["geht", "alls", "in", "sei\u00b7ne", "Quell", "/", "wie", "al\u00b7ler", "Wei\u00b7\u00dfen", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$(", "KOKOM", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so folget / da\u00df dein Spiel mit mir zu dir gehn mu\u00df.", "tokens": ["so", "fol\u00b7get", "/", "da\u00df", "dein", "Spiel", "mit", "mir", "zu", "dir", "gehn", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2