{"dta.poem.1022": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.71", "sv:0.28"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Willst du noch l&#228;nger auf den kahlen b\u00f6den               ", "tokens": ["Willst", "du", "noch", "l", "&#228;", "nger", "auf", "den", "kah\u00b7len", "b\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "NE", "$(", "ADJD", "APPR", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach fr&#252;hern vollen farben sp&#228;hn               ", "tokens": ["Nach", "fr", "&#252;", "hern", "vol\u00b7len", "far\u00b7ben", "sp", "&#228;", "hn"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Auf fr&#252;chte warten in den fahlen \u00f6den               ", "tokens": ["Auf", "fr", "&#252;", "ch\u00b7te", "war\u00b7ten", "in", "den", "fah\u00b7len", "\u00f6\u00b7den"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und &#228;hren von verdr&#228;ngten sommern m&#228;hn?             ", "tokens": ["Und", "&#228;", "hren", "von", "ver\u00b7dr", "&#228;", "ng\u00b7ten", "som\u00b7mern", "m", "&#228;", "hn", "?"], "token_info": ["word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Bescheide dich wenn nur im schattenschleier               ", "tokens": ["Be\u00b7schei\u00b7de", "dich", "wenn", "nur", "im", "schat\u00b7ten\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mild schimmernd du genossene f&#252;lle schaust               ", "tokens": ["Mild", "schim\u00b7mernd", "du", "ge\u00b7nos\u00b7se\u00b7ne", "f", "&#252;", "lle", "schaust"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und durch die m&#252;den l&#252;fte ein befreier               ", "tokens": ["Und", "durch", "die", "m", "&#252;", "den", "l", "&#252;", "fte", "ein", "be\u00b7frei\u00b7er"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der wind der weiten z&#228;rtlich um uns braust             ", "tokens": ["Der", "wind", "der", "wei\u00b7ten", "z", "&#228;", "rt\u00b7lich", "um", "uns", "braust"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und sieh die tage die wie wunden brannten               ", "tokens": ["Und", "sieh", "die", "ta\u00b7ge", "die", "wie", "wun\u00b7den", "brann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "ART", "KOKOM", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In unsrer vorgeschichte schwinden schnell               ", "tokens": ["In", "uns\u00b7rer", "vor\u00b7ge\u00b7schich\u00b7te", "schwin\u00b7den", "schnell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch alle dinge die wir blumen nannten               ", "tokens": ["Doch", "al\u00b7le", "din\u00b7ge", "die", "wir", "blu\u00b7men", "nann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versammeln sich am toten quell.             ", "tokens": ["Ver\u00b7sam\u00b7meln", "sich", "am", "to\u00b7ten", "quell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}