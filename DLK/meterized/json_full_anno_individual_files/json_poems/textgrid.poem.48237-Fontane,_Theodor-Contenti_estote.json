{"textgrid.poem.48237": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Contenti estote", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tieck, jung noch, kam zum alten Reil.", "tokens": ["Tieck", ",", "jung", "noch", ",", "kam", "zum", "al\u00b7ten", "Reil", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbherr Geheimrat, ich leide schon eine Weil',", "tokens": ["\u00bb", "herr", "Ge\u00b7heim\u00b7rat", ",", "ich", "lei\u00b7de", "schon", "ei\u00b7ne", "Weil'", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eigentlich hab' ich immer gelitten-", "tokens": ["Ei\u00b7gent\u00b7lich", "hab'", "ich", "im\u00b7mer", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich m\u00f6chte mir Ihren Rat erbitten.\u00ab", "tokens": ["Ich", "m\u00f6ch\u00b7te", "mir", "Ih\u00b7ren", "Rat", "er\u00b7bit\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bb\u203aNun, lassen Sie h\u00f6ren, lieber Tieck,", "tokens": ["\u00bb", "\u203a", "Nun", ",", "las\u00b7sen", "Sie", "h\u00f6\u00b7ren", ",", "lie\u00b7ber", "Tieck", ","], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "$,", "VVFIN", "PPER", "VVINF", "$,", "ADV", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vielleicht Migr\u00e4ne, vielleicht Kolik?", "tokens": ["Viel\u00b7leicht", "Mig\u00b7r\u00e4\u00b7ne", ",", "viel\u00b7leicht", "Ko\u00b7lik", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie sch\u00fctteln den Kopf. Vielleicht was am Herzen", "tokens": ["Sie", "sch\u00fct\u00b7teln", "den", "Kopf", ".", "Viel\u00b7leicht", "was", "am", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ADV", "PRELS", "APPRART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oder an der Leber? Haben Sie Schmerzen?\u2039\u00ab", "tokens": ["O\u00b7der", "an", "der", "Le\u00b7ber", "?", "Ha\u00b7ben", "Sie", "Schmer\u00b7zen", "?", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$.", "VAFIN", "PPER", "NN", "$.", "$(", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbnicht eigentlich das. Wohl mal, da\u00df es sticht,", "tokens": ["\u00bb", "nicht", "ei\u00b7gent\u00b7lich", "das", ".", "Wohl", "mal", ",", "da\u00df", "es", "sticht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "PDS", "$.", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aber wirkliche Schmerzen hab' ich nicht.\u00ab", "tokens": ["A\u00b7ber", "wirk\u00b7li\u00b7che", "Schmer\u00b7zen", "hab'", "ich", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bb\u203aSehr erfreulich. Und wenn ich's damit nicht traf,", "tokens": ["\u00bb", "\u203a", "Sehr", "er\u00b7freu\u00b7lich", ".", "Und", "wenn", "ich's", "da\u00b7mit", "nicht", "traf", ","], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "ADJD", "$.", "KON", "KOUS", "PIS", "PAV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie steht's mit der Hauptsach'? Wie steht's mit dem Schlaf?\u2039\u00ab", "tokens": ["Wie", "steht's", "mit", "der", "Haupt\u00b7sach'", "?", "Wie", "steht's", "mit", "dem", "Schlaf", "?", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "$.", "PWAV", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbin ", "tokens": ["\u00bb", "in"], "token_info": ["punct", "word"], "pos": ["$(", "APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ich schlafe doch mindestens meine neun Stunden.\u00ab", "tokens": ["Ich", "schla\u00b7fe", "doch", "min\u00b7des\u00b7tens", "mei\u00b7ne", "neun", "Stun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "CARD", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bb\u203aVortrefflich. So bleibt uns als letztes Gebiet", "tokens": ["\u00bb", "\u203a", "Vor\u00b7treff\u00b7lich", ".", "So", "bleibt", "uns", "als", "letz\u00b7tes", "Ge\u00b7biet"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "ADJD", "$.", "ADV", "VVFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nur noch die Verdauung; wie ist der Apptit?\u2039\u00ab", "tokens": ["Nur", "noch", "die", "Ver\u00b7dau\u00b7ung", ";", "wie", "ist", "der", "App\u00b7tit", "?", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbauch damit geht es; ich kann nicht klagen,", "tokens": ["\u00bb", "auch", "da\u00b7mit", "geht", "es", ";", "ich", "kann", "nicht", "kla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PAV", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ja, ich glaube, mein Bestes ist der Magen;", "tokens": ["Ja", ",", "ich", "glau\u00b7be", ",", "mein", "Bes\u00b7tes", "ist", "der", "Ma\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oft wenn ich ersch\u00f6pft bin \u2013 mit Freunden bei Tische,", "tokens": ["Oft", "wenn", "ich", "er\u00b7sch\u00f6pft", "bin", "\u2013", "mit", "Freun\u00b7den", "bei", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "VAFIN", "$(", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gleich hab' ich wieder die volle Frische.\u00ab", "tokens": ["Gleich", "hab'", "ich", "wie\u00b7der", "die", "vol\u00b7le", "Fri\u00b7sche", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Da lachte boshaft der alte Reil.", "tokens": ["Da", "lach\u00b7te", "bos\u00b7haft", "der", "al\u00b7te", "Reil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bb\u203aLieber Tieck, mit Ihnen hat es nicht Eil',", "tokens": ["\u00bb", "\u203a", "Lie\u00b7ber", "Tieck", ",", "mit", "Ih\u00b7nen", "hat", "es", "nicht", "Eil'", ","], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADJD", "NE", "$,", "APPR", "PPER", "VAFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Appetit und Schlaf und keine Schmerzen,", "tokens": ["Ap\u00b7pe\u00b7tit", "und", "Schlaf", "und", "kei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da danken andere Gott im Herzen,", "tokens": ["Da", "dan\u00b7ken", "an\u00b7de\u00b7re", "Gott", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihre Krankheit ist nichts als ein krankhaft Verlangen,", "tokens": ["Ih\u00b7re", "Krank\u00b7heit", "ist", "nichts", "als", "ein", "krank\u00b7haft", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "+-+--+--++-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Es ist Ihnen immer zu gut gegangen,", "tokens": ["Es", "ist", "Ih\u00b7nen", "im\u00b7mer", "zu", "gut", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ein bi\u00dfchen mehr Sorge bei schmalerem Brote,", "tokens": ["Ein", "bi\u00df\u00b7chen", "mehr", "Sor\u00b7ge", "bei", "schma\u00b7le\u00b7rem", "Bro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Tieck, jung noch, kam zum alten Reil.", "tokens": ["Tieck", ",", "jung", "noch", ",", "kam", "zum", "al\u00b7ten", "Reil", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbherr Geheimrat, ich leide schon eine Weil',", "tokens": ["\u00bb", "herr", "Ge\u00b7heim\u00b7rat", ",", "ich", "lei\u00b7de", "schon", "ei\u00b7ne", "Weil'", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punc