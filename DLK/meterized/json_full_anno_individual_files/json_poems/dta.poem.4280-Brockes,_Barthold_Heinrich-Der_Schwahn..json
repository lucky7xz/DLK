{"dta.poem.4280": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwahn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An eines breiten Wasser-Grabens dem Spiegel gleichen", "tokens": ["An", "ei\u00b7nes", "brei\u00b7ten", "Was\u00b7ser\u00b7Gra\u00b7bens", "dem", "Spie\u00b7gel", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "glatten Fluht,", "tokens": ["glat\u00b7ten", "Fluht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die, zwischen B\u00e4ume, Schilf und Stauden, in ungest\u00f6rter", "tokens": ["Die", ",", "zwi\u00b7schen", "B\u00e4u\u00b7me", ",", "Schilf", "und", "Stau\u00b7den", ",", "in", "un\u00b7ge\u00b7st\u00f6r\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stille, ruht,", "tokens": ["Stil\u00b7le", ",", "ruht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Besch\u00e4ftigt\u2019 ich mich j\u00fcngst, um von den sch\u00f6nen Bildern,", "tokens": ["Be\u00b7sch\u00e4f\u00b7tigt'", "ich", "mich", "j\u00fcngst", ",", "um", "von", "den", "sch\u00f6\u00b7nen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KOUI", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die, durch den holden Wiederschein,", "tokens": ["Die", ",", "durch", "den", "hol\u00b7den", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Versch\u00f6nert und verdoppelt seyn,", "tokens": ["Ver\u00b7sch\u00f6\u00b7nert", "und", "ver\u00b7dop\u00b7pelt", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Schmuck, zu Gottes Ruhm, in Versen abzuschildern;", "tokens": ["Den", "Schmuck", ",", "zu", "Got\u00b7tes", "Ruhm", ",", "in", "Ver\u00b7sen", "ab\u00b7zu\u00b7schil\u00b7dern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "NN", "$,", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da denn mein fr\u00f6hlicher Gesang,", "tokens": ["Da", "denn", "mein", "fr\u00f6h\u00b7li\u00b7cher", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bey diesem Gegenwurf, wie folgt, erklang:", "tokens": ["Bey", "die\u00b7sem", "Ge\u00b7gen\u00b7wurf", ",", "wie", "folgt", ",", "er\u00b7klang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In dem beschatteten Geb\u00fcsche", "tokens": ["In", "dem", "be\u00b7schat\u00b7te\u00b7ten", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r ich vom scharfen Schilf ein lispelndes Gezische,", "tokens": ["H\u00f6r", "ich", "vom", "schar\u00b7fen", "Schilf", "ein", "lis\u00b7peln\u00b7des", "Ge\u00b7zi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und recht als wie ein fl\u00fcsternd Schw\u00e4tzen.", "tokens": ["Und", "recht", "als", "wie", "ein", "fl\u00fcs\u00b7ternd", "Schw\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh die theils gerad- und theils gebogne Spitzen", "tokens": ["Ich", "seh", "die", "theils", "ge\u00b7ra\u00b7d", "und", "theils", "ge\u00b7bog\u00b7ne", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "TRUNC", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Jm Strahl der Sonnen schimmernd blitzen,", "tokens": ["Jm", "Strahl", "der", "Son\u00b7nen", "schim\u00b7mernd", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihren Fu\u00df das klare Wasser netzen,", "tokens": ["Und", "ih\u00b7ren", "Fu\u00df", "das", "kla\u00b7re", "Was\u00b7ser", "net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf dessen Fl\u00e4che die Figur", "tokens": ["Auf", "des\u00b7sen", "Fl\u00e4\u00b7che", "die", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der zierlichen Structur", "tokens": ["Der", "zier\u00b7li\u00b7chen", "Struc\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vom schwanken Rohr so deutlich schwimmt,", "tokens": ["Vom", "schwan\u00b7ken", "Rohr", "so", "deut\u00b7lich", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jm holden Wiederschein, da\u00df, in der glatten Klahrheit,", "tokens": ["Jm", "hol\u00b7den", "Wie\u00b7der\u00b7schein", ",", "da\u00df", ",", "in", "der", "glat\u00b7ten", "Klahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Schein und die Copie fast von der Wahrheit,", "tokens": ["Der", "Schein", "und", "die", "Co\u00b7pie", "fast", "von", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-++-++-+-", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Und dem Original, das Wesen an sich nimmt.", "tokens": ["Und", "dem", "O\u00b7rig\u00b7i\u00b7nal", ",", "das", "We\u00b7sen", "an", "sich", "nimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Es ist f\u00fcrwahr nicht auszudr\u00fccken,", "tokens": ["Es", "ist", "f\u00fcr\u00b7wahr", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie alles das so wundersch\u00f6n,", "tokens": ["Wie", "al\u00b7les", "das", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "(wenn wir mit Menschen-Augen sehn)", "tokens": ["(", "wenn", "wir", "mit", "Men\u00b7schen\u00b7Au\u00b7gen", "sehn", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Was wir an solchem Ort erblicken,", "tokens": ["Was", "wir", "an", "sol\u00b7chem", "Ort", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den selbst der Himmel scheint zu schm\u00fccken,", "tokens": ["Den", "selbst", "der", "Him\u00b7mel", "scheint", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jm hellen Glanz, mit seinem eignen Bilde.", "tokens": ["Jm", "hel\u00b7len", "Glanz", ",", "mit", "sei\u00b7nem", "eig\u00b7nen", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man sieht hier, wie so gar der Wolken Zier,", "tokens": ["Man", "sieht", "hier", ",", "wie", "so", "gar", "der", "Wol\u00b7ken", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch auf dem Wasser, sich verg\u00fclde.", "tokens": ["Auch", "auf", "dem", "Was\u00b7ser", ",", "sich", "ver\u00b7g\u00fcl\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wobey ich das sapphirne Blau,", "tokens": ["Wo\u00b7bey", "ich", "das", "sap\u00b7phir\u00b7ne", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Auf dieser flach- und klaren Fluht,", "tokens": ["Auf", "die\u00b7ser", "flach", "und", "kla\u00b7ren", "Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fast recht als eine blaue Gluht,", "tokens": ["Fast", "recht", "als", "ei\u00b7ne", "blau\u00b7e", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich ebenfalls verdoppeln schau.", "tokens": ["Sich", "e\u00b7ben\u00b7falls", "ver\u00b7dop\u00b7peln", "schau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald sah ich br\u00e4unlich-gelb, bald r\u00f6htlich-braun sich gatten.", "tokens": ["Bald", "sah", "ich", "br\u00e4un\u00b7lich\u00b7gelb", ",", "bald", "r\u00f6ht\u00b7lich\u00b7braun", "sich", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf Wasser-Linsen schwammen Schatten,", "tokens": ["Auf", "Was\u00b7ser\u00b7Lin\u00b7sen", "schwam\u00b7men", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und da, woselbst das Wasser rein,", "tokens": ["Und", "da", ",", "wo\u00b7selbst", "das", "Was\u00b7ser", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durchsichtig, glatt, und als ein Spiegel war,", "tokens": ["Durch\u00b7sich\u00b7tig", ",", "glatt", ",", "und", "als", "ein", "Spie\u00b7gel", "war", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KON", "KOUS", "ART", "NN", "VAFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.12": {"text": "Schwamm wundersch\u00f6n ein bunter Wiederschein", "tokens": ["Schwamm", "wun\u00b7der\u00b7sch\u00f6n", "ein", "bun\u00b7ter", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von Bluhmen, Kr\u00e4utern, Klee und Gras.", "tokens": ["Von", "Bluh\u00b7men", ",", "Kr\u00e4u\u00b7tern", ",", "Klee", "und", "Gras", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das allerreinste Spiegel-Glas", "tokens": ["Das", "al\u00b7ler\u00b7reins\u00b7te", "Spie\u00b7gel\u00b7Glas"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Kann den so holden Schmuck von Kr\u00e4utern und von", "tokens": ["Kann", "den", "so", "hol\u00b7den", "Schmuck", "von", "Kr\u00e4u\u00b7tern", "und", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADV", "ADJA", "NN", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Zweigen", "tokens": ["Zwei\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "So zierlich, deutlich, rein, so sch\u00f6n gef\u00e4rbt nicht zeigen.", "tokens": ["So", "zier\u00b7lich", ",", "deut\u00b7lich", ",", "rein", ",", "so", "sch\u00f6n", "ge\u00b7f\u00e4rbt", "nicht", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "VVPP", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein gr\u00fcner Glanz, der recht Smaragden-gr\u00fcn,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Glanz", ",", "der", "recht", "Sma\u00b7rag\u00b7den\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ja recht wie eine gr\u00fcne Gluht,", "tokens": ["Ja", "recht", "wie", "ei\u00b7ne", "gr\u00fc\u00b7ne", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Voll Gl\u00e4tte, Licht und Schimmer schien,", "tokens": ["Voll", "Gl\u00e4t\u00b7te", ",", "Licht", "und", "Schim\u00b7mer", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bedeckte mehrentheils die unbewegte Fluht.", "tokens": ["Be\u00b7deck\u00b7te", "meh\u00b7ren\u00b7theils", "die", "un\u00b7be\u00b7weg\u00b7te", "Fluht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man sahe recht in licht- und dunkel-gr\u00fcnen Bildern", "tokens": ["Man", "sa\u00b7he", "recht", "in", "licht", "und", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Des gr\u00fcnen Ufers Schmuck, wie Gr\u00fcn in Gr\u00fcn, sich schil-", "tokens": ["Des", "gr\u00fc\u00b7nen", "U\u00b7fers", "Schmuck", ",", "wie", "Gr\u00fcn", "in", "Gr\u00fcn", ",", "sich", "schil"], "t