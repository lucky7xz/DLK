{"dta.poem.8806": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Sie tadeln Keinen, und Alles ist gut;", "tokens": ["Sie", "ta\u00b7deln", "Kei\u00b7nen", ",", "und", "Al\u00b7les", "ist", "gut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMINF", "$,", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "D\u2019rum keck, o Schenke, kredenze den Wein,", "tokens": ["D'\u00b7rum", "keck", ",", "o", "Schen\u00b7ke", ",", "kre\u00b7den\u00b7ze", "den", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Den s\u00fc\u00dfen, reinen, und Alles ist gut;", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", ",", "rei\u00b7nen", ",", "und", "Al\u00b7les", "ist", "gut", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sonnenaugen entflammen den Stern,", "tokens": ["Die", "Son\u00b7nen\u00b7au\u00b7gen", "ent\u00b7flam\u00b7men", "den", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und mich die Deinen, und Alles ist gut;", "tokens": ["Und", "mich", "die", "Dei\u00b7nen", ",", "und", "Al\u00b7les", "ist", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$,", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein Schmeicheln, Z\u00fcrnen und Trotzen und Flehn,", "tokens": ["Dein", "Schmei\u00b7cheln", ",", "Z\u00fcr\u00b7nen", "und", "Trot\u00b7zen", "und", "Flehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "KON", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dein Lachen, Weinen und Alles ist gut;", "tokens": ["Dein", "La\u00b7chen", ",", "Wei\u00b7nen", "und", "Al\u00b7les", "ist", "gut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Welt im Gro\u00dfen, und du mir in ihr", "tokens": ["Die", "Welt", "im", "Gro\u00b7\u00dfen", ",", "und", "du", "mir", "in", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KON", "PPER", "PRF", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Welt im Kleinen und Alles ist gut;", "tokens": ["Die", "Welt", "im", "Klei\u00b7nen", "und", "Al\u00b7les", "ist", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Du r\u00fchmst die meinen, und Alles ist gut.", "tokens": ["Du", "r\u00fchmst", "die", "mei\u00b7nen", ",", "und", "Al\u00b7les", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "$,", "KON", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}