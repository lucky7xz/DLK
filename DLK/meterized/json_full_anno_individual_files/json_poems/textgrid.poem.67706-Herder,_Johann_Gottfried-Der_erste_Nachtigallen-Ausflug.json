{"textgrid.poem.67706": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der erste Nachtigallen-Ausflug", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag kaum durch die Wolken drang,", "tokens": ["Der", "Tag", "kaum", "durch", "die", "Wol\u00b7ken", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als schon die junge Nachtigall", "tokens": ["Als", "schon", "die", "jun\u00b7ge", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Neste zarten Fl\u00fcgel schwang", "tokens": ["Im", "Nes\u00b7te", "zar\u00b7ten", "Fl\u00fc\u00b7gel", "schwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sang mit Freudeschall:", "tokens": ["Und", "sang", "mit", "Freu\u00b7de\u00b7schall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbheran, willkommen, sch\u00f6ner Tag,", "tokens": ["\u00bb", "he\u00b7ran", ",", "will\u00b7kom\u00b7men", ",", "sch\u00f6\u00b7ner", "Tag", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der endlich mich ins Freie ruft,", "tokens": ["Der", "end\u00b7lich", "mich", "ins", "Frei\u00b7e", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir endlich, die so lang' hier lag,", "tokens": ["Mir", "end\u00b7lich", ",", "die", "so", "lang'", "hier", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuerst verleihet Luft!", "tokens": ["Zu\u00b7erst", "ver\u00b7lei\u00b7het", "Luft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Werd' heut zuerst die Welt durchwehn", "tokens": ["Werd'", "heut", "zu\u00b7erst", "die", "Welt", "durch\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen hoch auf freiem Baum,", "tokens": ["Und", "sin\u00b7gen", "hoch", "auf", "frei\u00b7em", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel neuer Art Gespielen sehn", "tokens": ["Viel", "neu\u00b7er", "Art", "Ge\u00b7spie\u00b7len", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und neuen Wunderraum.\u00ab", "tokens": ["Und", "neu\u00b7en", "Wun\u00b7der\u00b7raum", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbtrau nicht,\u00ab sprach Mutter Nachtigall,", "tokens": ["\u00bb", "trau", "nicht", ",", "\u00ab", "sprach", "Mut\u00b7ter", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$,", "$(", "VVFIN", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00bbtrau nicht, o Kind, dem Wunderraum!", "tokens": ["\u00bb", "trau", "nicht", ",", "o", "Kind", ",", "dem", "Wun\u00b7der\u00b7raum", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$,", "FM", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es giebt auch treulos s\u00fc\u00dfen Schall", "tokens": ["Es", "giebt", "auch", "treu\u00b7los", "s\u00fc\u00b7\u00dfen", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und K\u00f6rnchen unterm Baum,", "tokens": ["Und", "K\u00f6rn\u00b7chen", "un\u00b7term", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die uns ein Volk hinstreuet klug", "tokens": ["Die", "uns", "ein", "Volk", "hins\u00b7treu\u00b7et", "klug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00fcglich singt als Nachtigall,", "tokens": ["Und", "tr\u00fcg\u00b7lich", "singt", "als", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Streut K\u00f6rner aus voll List und Trug", "tokens": ["Streut", "K\u00f6r\u00b7ner", "aus", "voll", "List", "und", "Trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lockt mit s\u00fc\u00dfem Schall", "tokens": ["Und", "lockt", "mit", "s\u00fc\u00b7\u00dfem", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und macht uns Fu\u00df und Fl\u00fcgel fest", "tokens": ["Und", "macht", "uns", "Fu\u00df", "und", "Fl\u00fc\u00b7gel", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dann uns ein in Kerker schlie\u00dft,", "tokens": ["Und", "dann", "uns", "ein", "in", "Ker\u00b7ker", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Kerker, mehr als Kluft und Nest,", "tokens": ["In", "Ker\u00b7ker", ",", "mehr", "als", "Kluft", "und", "Nest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Winter \u00e4rger ist.", "tokens": ["Als", "Win\u00b7ter", "\u00e4r\u00b7ger", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bist da in W\u00fcsten, Fels und Stein,", "tokens": ["Bist", "da", "in", "W\u00fcs\u00b7ten", ",", "Fels", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist schwester-, gatte-, mutterlos,", "tokens": ["Bist", "schwes\u00b7ter", ",", "gat\u00b7te", ",", "mut\u00b7ter\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "TRUNC", "$,", "TRUNC", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Siehst keinen Baum, siehst keinen Hain \u2013", "tokens": ["Siehst", "kei\u00b7nen", "Baum", ",", "siehst", "kei\u00b7nen", "Hain", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schmuck- und federlos;", "tokens": ["Und", "schmuck", "und", "fe\u00b7der\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Stimme stirbt Dir, Lied und Schall,", "tokens": ["Die", "Stim\u00b7me", "stirbt", "Dir", ",", "Lied", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwingst nie die freien Fl\u00fcgel mehr!\u00ab", "tokens": ["Schwingst", "nie", "die", "frei\u00b7en", "Fl\u00fc\u00b7gel", "mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbach Mutter!\u00ab sprach die Nachtigall,", "tokens": ["\u00bb", "ach", "Mut\u00b7ter", "!", "\u00ab", "sprach", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdu z\u00f6gerst auch zu sehr!", "tokens": ["\u00bb", "du", "z\u00f6\u00b7gerst", "auch", "zu", "sehr", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Bin ja kein Kind mehr, bin so klug", "tokens": ["Bin", "ja", "kein", "Kind", "mehr", ",", "bin", "so", "klug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "$,", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als jede, jede Nachtigall.\u00ab", "tokens": ["Als", "je\u00b7de", ",", "je\u00b7de", "Nach\u00b7ti\u00b7gall", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIAT", "$,", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbbeginn nur, Liebe, Deinen Flug!\u00ab", "tokens": ["\u00bb", "be\u00b7ginn", "nur", ",", "Lie\u00b7be", ",", "Dei\u00b7nen", "Flug", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlug mit Freudenschall", "tokens": ["Und", "schlug", "mit", "Freu\u00b7den\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Fl\u00fcgel! \u00bbNur entferne nie,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "!", "\u00bb", "Nur", "ent\u00b7fer\u00b7ne", "nie", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entfern, o Kind, Dich nie von mir!\u00ab", "tokens": ["Ent\u00b7fern", ",", "o", "Kind", ",", "Dich", "nie", "von", "mir", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie flog: die junge Neugier, sie", "tokens": ["Sie", "flog", ":", "die", "jun\u00b7ge", "Neu\u00b7gier", ",", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flog kaum noch hinter ihr,", "tokens": ["Flog", "kaum", "noch", "hin\u00b7ter", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Als schnell schon Wunder an sie zog;", "tokens": ["Als", "schnell", "schon", "Wun\u00b7der", "an", "sie", "zog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sah so bunt und war ein Netz", "tokens": ["Es", "sah", "so", "bunt", "und", "war", "ein", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lag voll Kornes. Schnell hinflog", "tokens": ["Und", "lag", "voll", "Kor\u00b7nes", ".", "Schnell", "hin\u00b7flog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ab, seitab, ins Netz.", "tokens": ["Sie", "ab", ",", "seit\u00b7ab", ",", "ins", "Netz", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Mutter kommt. Um Fu\u00df und Haupt", "tokens": ["Die", "Mut\u00b7ter", "kommt", ".", "Um", "Fu\u00df", "und", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt t\u00f6dtlich, ach! ihr Kind verstrickt;", "tokens": ["Liegt", "t\u00f6dt\u00b7lich", ",", "ach", "!", "ihr", "Kind", "ver\u00b7strickt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ITJ", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schwirrt umher, kann, kindberaubt,", "tokens": ["Sie", "schwirrt", "um\u00b7her", ",", "kann", ",", "kind\u00b7be\u00b7raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur jammern, ach! und pfl\u00fcckt,", "tokens": ["Nur", "jam\u00b7mern", ",", "ach", "!", "und", "pfl\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ITJ", "$.", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Pfl\u00fcckt angstbet\u00e4ubt am Netze \u2013 zieht", "tokens": ["Pfl\u00fcckt", "angst\u00b7be\u00b7t\u00e4ubt", "am", "Net\u00b7ze", "\u2013", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Todes Netz nur fester zu.", "tokens": ["Des", "To\u00b7des", "Netz", "nur", "fes\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Todt sinkt ihr Fr\u00fchlingskind! Sie flieht", "tokens": ["Todt", "sinkt", "ihr", "Fr\u00fch\u00b7lings\u00b7kind", "!", "Sie", "flieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fl\u00fcchtet neu herzu", "tokens": ["Und", "fl\u00fcch\u00b7tet", "neu", "her\u00b7zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und weinet. \u2013 Kinder, kennten sie", "tokens": ["Und", "wei\u00b7net", ".", "\u2013", "Kin\u00b7der", ",", "kenn\u00b7ten", "sie"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eltern liebevolles Herz", "tokens": ["Der", "El\u00b7tern", "lie\u00b7be\u00b7vol\u00b7les", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+