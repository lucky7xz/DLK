{"textgrid.poem.26188": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die gr\u00fcnen Roggenfelder liegen still umh\u00fcllt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00fchlingsnacht hat grau das Tal gef\u00fcllt,", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7nacht", "hat", "grau", "das", "Tal", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die gr\u00fcnen Roggenfelder liegen still umh\u00fcllt,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Rog\u00b7gen\u00b7fel\u00b7der", "lie\u00b7gen", "still", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und es verfliegen dunkel Weg und W\u00e4lder.", "tokens": ["Und", "es", "ver\u00b7flie\u00b7gen", "dun\u00b7kel", "Weg", "und", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Arbeit lie\u00df die Menschen endlich los.", "tokens": ["Die", "Ar\u00b7beit", "lie\u00df", "die", "Men\u00b7schen", "end\u00b7lich", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie sitzen in dem Dorf an allen T\u00fcren", "tokens": ["Sie", "sit\u00b7zen", "in", "dem", "Dorf", "an", "al\u00b7len"