{"dta.poem.10045": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Kaiser-Crone.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "B. Wer setzte deinen hohen Throu,", "tokens": ["B.", "Wer", "setz\u00b7te", "dei\u00b7nen", "ho\u00b7hen", "Throu", ","], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O pr\u00e4chtig-sch\u00f6ne Kaiser-Cron\u2019,", "tokens": ["O", "pr\u00e4ch\u00b7tig\u00b7sch\u00f6\u00b7ne", "Kai\u00b7ser\u00b7Cron'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allhier so nah am Rosen-Strauch?", "tokens": ["All\u00b7hier", "so", "nah", "am", "Ro\u00b7sen\u00b7Strauch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ja der Dornen starre Spitzen,", "tokens": ["Da", "ja", "der", "Dor\u00b7nen", "star\u00b7re", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An stat denselbigen zu st\u00fctzen,", "tokens": ["An", "stat", "den\u00b7sel\u00b7bi\u00b7gen", "zu", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An stat die Bl\u00e4tter zu besch\u00fctzen,", "tokens": ["An", "stat", "die", "Bl\u00e4t\u00b7ter", "zu", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dir deine Bl\u00e4tter gantz zerritzen?", "tokens": ["Dir", "dei\u00b7ne", "Bl\u00e4t\u00b7ter", "gantz", "zer\u00b7rit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "K. So wie du denckest, denck ich auch.", "tokens": ["K.", "So", "wie", "du", "den\u00b7ckest", ",", "denck", "ich", "auch", "."], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOKOM", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich meint\u2019, in stetigem Vergn\u00fcgen.", "tokens": ["Ich", "meint'", ",", "in", "ste\u00b7ti\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf frischen Rosen sanft zu liegen:", "tokens": ["Auf", "fri\u00b7schen", "Ro\u00b7sen", "sanft", "zu", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So aber reibet Eurus Hauch", "tokens": ["So", "a\u00b7ber", "rei\u00b7bet", "Eu\u00b7rus", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich an die Dornen, ehe noch", "tokens": ["Mich", "an", "die", "Dor\u00b7nen", ",", "e\u00b7he", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Rosen aus den Knospen brechen;", "tokens": ["Die", "Ro\u00b7sen", "aus", "den", "Knos\u00b7pen", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wodurch sie denn so manches Loch", "tokens": ["Wo\u00b7durch", "sie", "denn", "so", "man\u00b7ches", "Loch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In mich und meinen Purpur stechen,", "tokens": ["In", "mich", "und", "mei\u00b7nen", "Pur\u00b7pur", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr Herrscher, die ihr Cronen tragt,", "tokens": ["Ihr", "Herr\u00b7scher", ",", "die", "ihr", "Cro\u00b7nen", "tragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lasst, was der Bluhmen Crone sagt,", "tokens": ["Lasst", ",", "was", "der", "Bluh\u00b7men", "Cro\u00b7ne", "sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch euch zur Lehr\u2019 ans Hertze gehen,", "tokens": ["Auch", "euch", "zur", "Lehr'", "ans", "Hert\u00b7ze", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indem der allermeisten Cronen", "tokens": ["In\u00b7dem", "der", "al\u00b7ler\u00b7meis\u00b7ten", "Cro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch noch so fest gegr\u00fcndte Thronen", "tokens": ["Auch", "noch", "so", "fest", "ge\u00b7gr\u00fcnd\u00b7te", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beym Rosen-Strauch gepflantzet stehen.", "tokens": ["Beym", "Ro\u00b7sen\u00b7Strauch", "ge\u00b7pflant\u00b7zet", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}