{"textgrid.poem.29327": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcmmt vom freuen freyen her? Wie da\u00df manchem armen Tropffe", "tokens": ["K\u00fcmmt", "vom", "freu\u00b7en", "frey\u00b7en", "her", "?", "Wie", "da\u00df", "man\u00b7chem", "ar\u00b7men", "Tropf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$.", "KOKOM", "KOUS", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nicht das freuen k\u00fcmmt vom freyen, sondern krimmen in dem Kopffe?", "tokens": ["Nicht", "das", "freu\u00b7en", "k\u00fcmmt", "vom", "frey\u00b7en", ",", "son\u00b7dern", "krim\u00b7men", "in", "dem", "Kopf\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "VVFIN", "APPRART", "ADJA", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "K\u00fcmmt vom freyhen freyen her? Wie da\u00df manchem armen Freyer", "tokens": ["K\u00fcmmt", "vom", "frey\u00b7hen", "frey\u00b7en", "her", "?", "Wie", "da\u00df", "man\u00b7chem", "ar\u00b7men", "Frey\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$.", "KOKOM", "KOUS", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Frey zu walten, frey zu schalten, Freyheit bleibt f\u00fcr keinen Dreyer?", "tokens": ["Frey", "zu", "wal\u00b7ten", ",", "frey", "zu"