{"dta.poem.19387": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "120.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was knistert neben mir und st\u00f6rt mein einsam Denken,", "tokens": ["Was", "knis\u00b7tert", "ne\u00b7ben", "mir", "und", "st\u00f6rt", "mein", "ein\u00b7sam", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Sinnen ab den Sinn aufs Sinnliche zu lenken?", "tokens": ["Vom", "Sin\u00b7nen", "ab", "den", "Sinn", "aufs", "Sinn\u00b7li\u00b7che", "zu", "len\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Ist es die Schlange wol, die sich im Grase r\u00fchrt,", "tokens": ["Ist", "es", "die", "Schlan\u00b7ge", "wol", ",", "die", "sich", "im", "Gra\u00b7se", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schlange die zuerst den Menschen hat verf\u00fchrt?", "tokens": ["Die", "Schlan\u00b7ge", "die", "zu\u00b7erst", "den", "Men\u00b7schen", "hat", "ver\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch als ich um mich sah, war es ein grasend Lamm,", "tokens": ["Doch", "als", "ich", "um", "mich", "sah", ",", "war", "es", "ein", "gra\u00b7send", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ruhig dacht' ich fort, gelehnt an meinen Stamm.", "tokens": ["Und", "ru\u00b7hig", "dacht'", "ich", "fort", ",", "ge\u00b7lehnt", "an", "mei\u00b7nen", "Stamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}