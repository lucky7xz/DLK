{"textgrid.poem.45747": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Scheiden aus dem deutschen Bunde", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man warf einen zur T\u00fcre hinaus,", "tokens": ["Man", "warf", "ei\u00b7nen", "zur", "T\u00fc\u00b7re", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "APPRART", "NN", "APZR", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Er st\u00e4ubt sich ab und geht ruhig nach Haus.", "tokens": ["Er", "st\u00e4ubt", "sich", "ab", "und", "geht", "ru\u00b7hig", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und als man meint, er h\u00e4tts \u00fcbel nehmen sollen,", "tokens": ["Und", "als", "man", "meint", ",", "er", "h\u00e4tts", "\u00fc\u00b7bel", "neh\u00b7men", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Versichert er, er hab just selber weggehen wollen.", "tokens": ["Ver\u00b7si\u00b7chert", "er", ",", "er", "hab", "just", "sel\u00b7ber", "weg\u00b7ge\u00b7hen", "wol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text"