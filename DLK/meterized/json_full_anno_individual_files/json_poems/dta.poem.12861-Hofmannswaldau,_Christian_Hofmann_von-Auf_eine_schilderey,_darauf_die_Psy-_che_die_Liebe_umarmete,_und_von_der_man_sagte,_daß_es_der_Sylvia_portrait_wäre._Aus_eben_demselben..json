{"dta.poem.12861": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine schilderey, darauf die Psy-  \n che die Liebe umarmete, und von der man  \n sagte, da\u00df es der Sylvia portrait w\u00e4re.  \n Aus eben demselben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du magst dis sch\u00f6ne bild ein meisterst\u00fccke nennen,", "tokens": ["Du", "magst", "dis", "sch\u00f6\u00b7ne", "bild", "ein", "meis\u00b7ter\u00b7st\u00fc\u00b7cke", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich falle dir darinnen bey.", "tokens": ["Ich", "fal\u00b7le", "dir", "da\u00b7rin\u00b7nen", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aber Sylvia hier abgeschildert sey,", "tokens": ["Da\u00df", "a\u00b7ber", "Syl\u00b7via", "hier", "ab\u00b7ge\u00b7schil\u00b7dert", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Das wirst du nimmermehr mich \u00fcberreden k\u00f6nnen.", "tokens": ["Das", "wirst", "du", "nim\u00b7mer\u00b7mehr", "mich", "\u00fc\u00b7berr\u00b7e\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geficht und farbe trifft zwar richtig \u00fcberein;", "tokens": ["Ge\u00b7ficht", "und", "far\u00b7be", "trifft", "zwar", "rich\u00b7tig", "\u00fc\u00b7be\u00b7re\u00b7in", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Weil aber Psyche sich hier mit der Liebe tr\u00e4get,", "tokens": ["Weil", "a\u00b7ber", "Psy\u00b7che", "sich", "hier", "mit", "der", "Lie\u00b7be", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dieses zarte kind an ihre lippen leget,", "tokens": ["Und", "die\u00b7ses", "zar\u00b7te", "kind", "an", "ih\u00b7re", "lip\u00b7pen", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kan es Sylvia nicht seyn.", "tokens": ["So", "kan", "es", "Syl\u00b7via", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NE", "PTKNEG", "VAINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}