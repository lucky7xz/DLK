{"textgrid.poem.65975": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zieh mir zum Frommen ich die Summe aller Tage,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh mir zum Frommen ich die Summe aller Tage,", "tokens": ["Zieh", "mir", "zum", "From\u00b7men", "ich", "die", "Sum\u00b7me", "al\u00b7ler", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie vieler ward ich froh, wie viele brachten Plage?", "tokens": ["Wie", "vie\u00b7ler", "ward", "ich", "froh", ",", "wie", "vie\u00b7le", "brach\u00b7ten", "Pla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PIS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie oft im W\u00fcrfelspiel warf ich des wilden Lebens", "tokens": ["Wie", "oft", "im", "W\u00fcr\u00b7fel\u00b7spiel", "warf", "ich", "des", "wil\u00b7den", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Augen grad' genug, wie oft warf ich vergebens.", "tokens": ["Der", "Au\u00b7gen", "grad'", "ge\u00b7nug", ",", "wie", "oft", "warf", "ich", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sto\u00df' ich den Becher fort und scheide aus dem Spiele?", "tokens": ["Sto\u00df'", "ich", "den", "Be\u00b7cher", "fort", "und", "schei\u00b7de", "aus", "dem", "Spie\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was soll der Kn\u00f6cheltanz auf harter Lebensdiele?", "tokens": ["Was", "soll", "der", "Kn\u00f6\u00b7chel\u00b7tanz", "auf", "har\u00b7ter", "Le\u00b7bens\u00b7die\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der M\u00fche wert ist nicht, was uns die W\u00fcrfel bringen.", "tokens": ["Der", "M\u00fc\u00b7he", "wert", "ist", "nicht", ",", "was", "uns", "die", "W\u00fcr\u00b7fel", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Am Ende l\u00e4sst der Tod kein K\u00f6rnchen ab sich dingen.", "tokens": ["Am", "En\u00b7de", "l\u00e4sst", "der", "Tod", "kein", "K\u00f6rn\u00b7chen", "ab", "sich", "din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "APPR", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er winkt, und du musst fort, gewinnend, wie 