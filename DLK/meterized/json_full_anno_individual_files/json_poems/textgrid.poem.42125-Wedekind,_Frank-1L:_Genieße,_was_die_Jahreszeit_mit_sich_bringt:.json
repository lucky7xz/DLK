glicher Stauung", "tokens": ["Als", "Mit\u00b7tel", "zum", "Zweck", "bei", "un\u00b7be\u00b7hag\u00b7li\u00b7cher", "Stau\u00b7ung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Unterleib statt Karlsbader Brunnenkur.", "tokens": ["Im", "Un\u00b7ter\u00b7leib", "statt", "Karls\u00b7ba\u00b7der", "Brun\u00b7nen\u00b7kur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es gr\u00f6len die D\u00e4rme im Chor den Gesang der Verdauung. \u2013", "tokens": ["Es", "gr\u00f6\u00b7len", "die", "D\u00e4r\u00b7me", "im", "Chor", "den", "Ge\u00b7sang", "der", "Ver\u00b7dau\u00b7ung", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.12": {"line.1": {"text": "Noch manches w\u00e4re notwendig hier beigedruckt,", "tokens": ["Noch", "man\u00b7ches", "w\u00e4\u00b7re", "not\u00b7wen\u00b7dig", "hier", "bei\u00b7ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Mammut-Tr\u00fcffeln, die aus Thessalien stammen;", "tokens": ["Wie", "Mam\u00b7mut\u00b7Tr\u00fcf\u00b7feln", ",", "die", "aus", "Thes\u00b7sa\u00b7li\u00b7en", "stam\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hab ich den ganzen Hymnus schon vollgespuckt,", "tokens": ["Doch", "hab", "ich", "den", "gan\u00b7zen", "Hym\u00b7nus", "schon", "voll\u00b7ge\u00b7spuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So l\u00e4uft mir dabei das Wasser im Munde zusammen.", "tokens": ["So", "l\u00e4uft", "mir", "da\u00b7bei", "das", "Was\u00b7ser", "im", "Mun\u00b7de", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}}}}