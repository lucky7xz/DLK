{"textgrid.poem.42125": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "1L: Genie\u00dfe, was die Jahreszeit mit sich bringt:", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genie\u00dfe, was die Jahreszeit mit sich bringt:", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", ",", "was", "die", "Jah\u00b7res\u00b7zeit", "mit", "sich", "bringt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Radieschen, Erdbeeren, gr\u00fcne Erbsen und Pflaumen!", "tokens": ["Ra\u00b7die\u00b7schen", ",", "Erd\u00b7bee\u00b7ren", ",", "gr\u00fc\u00b7ne", "Erb\u00b7sen", "und", "Pflau\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Was der Ver\u00e4ndrung in Sonne und Luft entspringt,", "tokens": ["Was", "der", "Ver\u00b7\u00e4n\u00b7drung", "in", "Son\u00b7ne", "und", "Luft", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Ist stets das Beste f\u00fcr deinen gebildeten Gaumen.", "tokens": ["Ist", "stets", "das", "Bes\u00b7te", "f\u00fcr", "dei\u00b7nen", "ge\u00b7bil\u00b7de\u00b7ten", "Gau\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Radieschen knackt man, wenn man noch jung und keusch", "tokens": ["Ra\u00b7die\u00b7schen", "knackt", "man", ",", "wenn", "man", "noch", "jung", "und", "keusch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "$,", "KOUS", "PIS", "ADV", "ADJD", "KON", "ADJD"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und sich noch die ersten Z\u00e4hne nicht ausgebissen;", "tokens": ["Und", "sich", "noch", "die", "ers\u00b7ten", "Z\u00e4h\u00b7ne", "nicht", "aus\u00b7ge\u00b7bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die prallen B\u00e4ckchen zerbersten mit lautem Gekreisch,", "tokens": ["Die", "pral\u00b7len", "B\u00e4ck\u00b7chen", "zer\u00b7bers\u00b7ten", "mit", "lau\u00b7tem", "Ge\u00b7kreisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Zunge schwelgt in uns\u00e4glichen Bitternissen.", "tokens": ["Die", "Zun\u00b7ge", "schwelgt", "in", "un\u00b7s\u00e4g\u00b7li\u00b7chen", "Bit\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Erdbeeren aus Wald und Garten, wie duften sie fein,", "tokens": ["Erd\u00b7bee\u00b7ren", "aus", "Wald", "und", "Gar\u00b7ten", ",", "wie", "duf\u00b7ten", "sie", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+---+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die gro\u00dfen voll Saft, die kleinen sind mir noch lieber.", "tokens": ["Die", "gro\u00b7\u00dfen", "voll", "Saft", ",", "die", "klei\u00b7nen", "sind", "mir", "noch", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "NN", "$,", "ART", "ADJA", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich mache sie trunken zuvor mit gez\u00fcckertem Wein,", "tokens": ["Ich", "ma\u00b7che", "sie", "trun\u00b7ken", "zu\u00b7vor", "mit", "ge\u00b7z\u00fc\u00b7cker\u00b7tem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Pechv\u00f6gel nur erkranken am Nesselfieber.", "tokens": ["Pech\u00b7v\u00f6\u00b7gel", "nur", "er\u00b7kran\u00b7ken", "am", "Nes\u00b7sel\u00b7fie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die gr\u00fcnen Erbsen brauch ich schon gar gekocht;", "tokens": ["Die", "gr\u00fc\u00b7nen", "Erb\u00b7sen", "brauch", "ich", "schon", "gar", "ge\u00b7kocht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die tolle Jugend allein fri\u00dft sie aus den Schoten.", "tokens": ["Die", "tol\u00b7le", "Ju\u00b7gend", "al\u00b7lein", "fri\u00dft", "sie", "aus", "den", "Scho\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe sie stets nur gepfeffert zu kosten vermocht,", "tokens": ["Ich", "ha\u00b7be", "sie", "stets", "nur", "ge\u00b7pfef\u00b7fert", "zu", "kos\u00b7ten", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Und neuerdings auch hat sie der Arzt mir verboten.", "tokens": ["Und", "neu\u00b7er\u00b7dings", "auch", "hat", "sie", "der", "Arzt", "mir", "ver\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die \u00fcppigen Pflaumen des Herbstes genie\u00df ich fast nur", "tokens": ["Die", "\u00fcp\u00b7pi\u00b7gen", "Pflau\u00b7men", "des", "Herbs\u00b7tes", "ge\u00b7nie\u00df", "ich", "fast", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Als Mittel zum Zweck bei unbehaglicher Stauung", "tokens": ["Als", "Mit\u00b7tel", "zum", "Zweck", "bei", "un\u00b7be\u00b7hag\u00b7li\u00b7cher", "Stau\u00b7ung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+