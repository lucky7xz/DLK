{"dta.poem.2271": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  Messias.  \n Neunter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1756", "urn": "urn:nbn:de:kobv:b4-200905198237", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzo kam Eloa zur\u00fcck vom Throne des Richters.", "tokens": ["Jt\u00b7zo", "kam", "E\u00b7loa", "zu\u00b7r\u00fcck", "vom", "Thro\u00b7ne", "des", "Rich\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Voll von tief\u1ebd Gedank\u1ebd, u. langsamer schwebt\u2019 er des Tempels", "tokens": ["Voll", "von", "tie\u00b7f\u1ebd", "Ge\u00b7dank\u1ebd", ",", "u.", "lang\u00b7sa\u00b7mer", "schwebt'", "er", "des", "Tem\u00b7pels"], "token_info": ["word", "word", "word", "word", "punct", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Zinne vor\u00fcber, und trat in der V\u00e4ter Versam\u0303lung und sagte:", "tokens": ["Zin\u00b7ne", "vor\u00b7\u00fc\u00b7ber", ",", "und", "trat", "in", "der", "V\u00e4\u00b7ter", "Ver\u00b7sam\u0303\u00b7lung", "und", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Eh ich rede, betet mit mir! Ich will anbeten,", "tokens": ["Eh", "ich", "re\u00b7de", ",", "be\u00b7tet", "mit", "mir", "!", "Ich", "will", "an\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eh ich rede! Da fielen sie all\u2019 aufs Angesicht nieder;", "tokens": ["Eh", "ich", "re\u00b7de", "!", "Da", "fie\u00b7len", "sie", "all'", "aufs", "An\u00b7ge\u00b7sicht", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PIS", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Beteten still den Unendlichen an. Mit eben der Stille", "tokens": ["Be\u00b7te\u00b7ten", "still", "den", "Un\u00b7end\u00b7li\u00b7chen", "an", ".", "Mit", "e\u00b7ben", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$.", "APPR", "ADV", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Standen sie auf. Eloa verstummte noch. Endlich redt\u2019 er.", "tokens": ["Stan\u00b7den", "sie", "auf", ".", "E\u00b7loa", "ver\u00b7stumm\u00b7te", "noch", ".", "End\u00b7lich", "redt'", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$.", "NE", "VVFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+---+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O du, welchen Namen nicht nennen, Gedanken nicht denken,", "tokens": ["O", "du", ",", "wel\u00b7chen", "Na\u00b7men", "nicht", "nen\u00b7nen", ",", "Ge\u00b7dan\u00b7ken", "nicht", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAT", "NN", "PTKNEG", "VVINF", "$,", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Erster! \u2026 Zu ihm erhub ich mich, wollte, von Antliz zu Antliz,", "tokens": ["Ers\u00b7ter", "!", "\u2026", "Zu", "ihm", "er\u00b7hub", "ich", "mich", ",", "woll\u00b7te", ",", "von", "Ant\u00b7liz", "zu", "Ant\u00b7liz", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "APPR", "PPER", "VVFIN", "PPER", "PRF", "$,", "VMFIN", "$,", "APPR", "NE", "APPR", "NE", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Sehn, der Weltgericht h\u00e4lt, den Unausges\u00f6nten im Dunkeln!", "tokens": ["Sehn", ",", "der", "Welt\u00b7ge\u00b7richt", "h\u00e4lt", ",", "den", "Un\u00b7aus\u00b7ge\u00b7s\u00f6n\u00b7ten", "im", "Dun\u00b7keln", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "In der furchtbaren Herrlichkeit, Gott! Ich kam an die Sonnen;", "tokens": ["In", "der", "furcht\u00b7ba\u00b7ren", "Herr\u00b7lich\u00b7keit", ",", "Gott", "!", "Ich", "kam", "an", "die", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-++--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und die d\u00e4mmerten! Kam zu des Himmels Pole; da rangen", "tokens": ["Und", "die", "d\u00e4m\u00b7mer\u00b7ten", "!", "Kam", "zu", "des", "Him\u00b7mels", "Po\u00b7le", ";", "da", "ran\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$.", "NE", "APPR", "ART", "NN", "NN", "$.", "ADV", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Tr\u00fcbe Schimmer mit N\u00e4chten! Ich ging zum Throne, da wurd es", "tokens": ["Tr\u00fc\u00b7be", "Schim\u00b7mer", "mit", "N\u00e4ch\u00b7ten", "!", "Ich", "ging", "zum", "Thro\u00b7ne", ",", "da", "wurd", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Dunkler um mich, und nun noch dunkler, und nun. \u2026 Doch ich suche", "tokens": ["Dunk\u00b7ler", "um", "mich", ",", "und", "nun", "noch", "dunk\u00b7ler", ",", "und", "nun", ".", "\u2026", "Doch", "ich", "su\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "KON", "ADV", "ADV", "ADJA", "$,", "KON", "ADV", "$.", "$(", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Namen, und finde sie nicht, wie es um den Unendlichen Nacht war!", "tokens": ["Na\u00b7men", ",", "und", "fin\u00b7de", "sie", "nicht", ",", "wie", "es", "um", "den", "Un\u00b7end\u00b7li\u00b7chen", "Nacht", "war", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Keine Namen dem Schauer, der von dem Unendlichen ausging.", "tokens": ["Kei\u00b7ne", "Na\u00b7men", "dem", "Schau\u00b7er", ",", "der", "von", "dem", "Un\u00b7end\u00b7li\u00b7chen", "aus\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und ich stand, und h\u00f6rte von fern die Str\u00f6me der H\u00f6lle,", "tokens": ["Und", "ich", "stand", ",", "und", "h\u00f6r\u00b7te", "von", "fern", "die", "Str\u00f6\u00b7me", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Unter der tiefen schweigenden Sch\u00f6pfung, rauschen. Ich schwebte", "tokens": ["Un\u00b7ter", "der", "tie\u00b7fen", "schwei\u00b7gen\u00b7den", "Sch\u00f6p\u00b7fung", ",", "rau\u00b7schen", ".", "Ich", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Langsam weiter. Da rief der erste der Todesengel", "tokens": ["Lang\u00b7sam", "wei\u00b7ter", ".", "Da", "rief", "der", "ers\u00b7te", "der", "To\u00b7de\u00b7sen\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "ADJA", "ART", "NN"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Gegen mich her: We\u00df Schweben ist dieses Endlichen Schweben?", "tokens": ["Ge\u00b7gen", "mich", "her", ":", "We\u00df", "Schwe\u00b7ben", "ist", "die\u00b7ses", "End\u00b7li\u00b7chen", "Schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$.", "NE", "NN", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und ich bebte zur\u00fcck, sank auf mein Angesicht nieder,", "tokens": ["Und", "ich", "beb\u00b7te", "zu\u00b7r\u00fcck", ",", "sank", "auf", "mein", "An\u00b7ge\u00b7sicht", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Betet\u2019 ihn an, und verstummt\u2019 und betet\u2019 ihn an, der Gericht hielt.", "tokens": ["Be\u00b7tet'", "ihn", "an", ",", "und", "ver\u00b7stummt'", "und", "be\u00b7tet'", "ihn", "an", ",", "der", "Ge\u00b7richt", "hielt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Also sagt\u2019 er, und wandte sich weg, und verh\u00fcllte sein Antliz.", "tokens": ["Al\u00b7so", "sagt'", "er", ",", "und", "wand\u00b7te", "sich", "weg", ",", "und", "ver\u00b7h\u00fcll\u00b7te", "sein", "Ant\u00b7liz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Jesu war sein Haupt zum Herzen herunter gesunken,", "tokens": ["Je\u00b7su", "war", "sein", "Haupt", "zum", "Her\u00b7zen", "her\u00b7un\u00b7ter", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "APZR", "VVPP", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und es schien, als schlummert\u2019 er. Selbst der l\u00e4sternden Menge", "tokens": ["Und", "es", "schien", ",", "als", "schlum\u00b7mert'", "er", ".", "Selbst", "der", "l\u00e4s\u00b7tern\u00b7den", "Men\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ungest\u00fcm legte sich, wie am unbest\u00fcrmten Gestade", "tokens": ["Un\u00b7ge\u00b7st\u00fcm", "leg\u00b7te", "sich", ",", "wie", "am", "un\u00b7be\u00b7st\u00fcrm\u00b7ten", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sich der Ocean legt. Die den G\u00f6ttlichen liebten, umirrten", "tokens": ["Sich", "der", "O\u00b7cean", "legt", ".", "Die", "den", "G\u00f6tt\u00b7li\u00b7chen", "lieb\u00b7ten", ",", "um\u00b7irr\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "ART", "NN", "VVFIN", "$.", "ART", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Golgatha, oder die \u00e4ussersten Fernen, woraus sie den Mittler", "tokens": ["Gol\u00b7ga\u00b7tha", ",", "o\u00b7der", "die", "\u00e4us\u00b7sers\u00b7ten", "Fer\u00b7nen", ",", "wo\u00b7raus", "sie", "den", "Mitt\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Noch mit weinendem Blicke zu sehn vermochten. Doch jeder", "tokens": ["Noch", "mit", "wei\u00b7nen\u00b7dem", "Bli\u00b7cke", "zu", "sehn", "ver\u00b7moch\u00b7ten", ".", "Doch", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$.", "KON", "PIS"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Mied den andern, damit sie sich nicht die tiefe Wunde", "tokens": ["Mied", "den", "an\u00b7dern", ",", "da\u00b7mit", "sie", "sich", "nicht", "die", "tie\u00b7fe", "Wun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Tiefer gr\u00fcben; spr\u00e4chen sie sich. Nur der J\u00fcnger der Liebe,", "tokens": ["Tie\u00b7fer", "gr\u00fc\u00b7ben", ";", "spr\u00e4\u00b7chen", "sie", "sich", ".", "Nur", "der", "J\u00fcn\u00b7ger", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "VVFIN", "PPER", "PRF", "$.", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Und des Leidenden Mutter, verliessen sich nicht. Sie standen", "tokens": ["Und", "des", "Lei\u00b7den\u00b7den", "Mut\u00b7ter", ",", "ver\u00b7lies\u00b7sen", "sich", "nicht", ".", "Sie", "stan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "VVFIN", "PRF", "PTKNEG", "$.", "PPER", "VVFIN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Unten am Kreuz. Der J\u00fcnger, der schwur, da\u00df er Jesum nicht kenne,", "tokens": ["Un\u00b7ten", "am", "Kreuz", ".", "Der", "J\u00fcn\u00b7ger", ",", "der", "schwur", ",", "da\u00df", "er", "Je\u00b7sum", "nicht", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+--+---+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "War die schlaflose Nacht und den Morgen umher gezittert,", "tokens": ["War", "die", "schlaf\u00b7lo\u00b7se", "Nacht", "und", "den", "Mor\u00b7gen", "um\u00b7her", "ge\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "KON", "ART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Hatte Ruhe gesucht, und keine Ruhe gefunden.", "tokens": ["Hat\u00b7te", "Ru\u00b7he", "ge\u00b7sucht", ",", "und", "kei\u00b7ne", "Ru\u00b7he", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Also irrt ein Sohn an des Meers betr\u00fcmmertem Ufer,", "tokens": ["Al\u00b7so", "irrt", "ein", "Sohn", "an", "des", "Meers", "be\u00b7tr\u00fcm\u00b7mer\u00b7tem", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Dem sein Vater nicht ferne von ihm an einem der Felsen", "tokens": ["Dem", "sein", "Va\u00b7ter", "nicht", "fer\u00b7ne", "von", "ihm", "an", "ei\u00b7nem", "der", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "PPER", "APPR", "ART", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Umkam. Sprachlos irrt er umher, und sieht unverwendet", "tokens": ["Um\u00b7kam", ".", "Sprach\u00b7los", "irrt", "er", "um\u00b7her", ",", "und", "sieht", "un\u00b7ver\u00b7wen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+--+--+-++-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Nach dem Felsen, auf dem sein Vater geschmettert und todt liegt.", "tokens": ["Nach", "dem", "Fel\u00b7sen", ",", "auf", "dem", "sein", "Va\u00b7ter", "ge\u00b7schmet\u00b7tert", "und", "todt", "liegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Endlich ruft er jammernd gen Himmel: Er habe den Vater,", "tokens": ["End\u00b7lich", "ruft", "er", "jam\u00b7mernd", "gen", "Him\u00b7mel", ":", "Er", "ha\u00b7be", "den", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Ach er hab ihn verlassen, im tiefen Meere, verlassen!", "tokens": ["Ach", "er", "hab", "ihn", "ver\u00b7las\u00b7sen", ",", "im", "tie\u00b7fen", "Mee\u00b7re", ",", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "PPER", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.19": {"text": "Petrus ermattet izt ganz, und bleibt auf einer der Anh\u00f6hn", "tokens": ["Pet\u00b7rus", "er\u00b7mat\u00b7tet", "izt", "ganz", ",", "und", "bleibt", "auf", "ei\u00b7ner", "der", "An\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "VVFIN", "ADV", "ADV", "$,", "KON", "VVFIN", "APPR", "ART", "ART", "NN"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Nah an Golgatha stehen; und l\u00e4\u00dft die bleicheren H\u00e4nde,", "tokens": ["Nah", "an", "Gol\u00b7ga\u00b7tha", "ste\u00b7hen", ";", "und", "l\u00e4\u00dft", "die", "blei\u00b7che\u00b7ren", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "$.", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+----+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Die er nicht mehr zu ringen vermag, hinsinken. Sein Schuzgeist,", "tokens": ["Die", "er", "nicht", "mehr", "zu", "rin\u00b7gen", "ver\u00b7mag", ",", "hin\u00b7sin\u00b7ken", ".", "Sein", "Schuz\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$,", "VVIZU", "$.", "PPOSAT", "NN", "$,"], "meter": "---+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Seraph Jthuriel, sieht ihn, und gie\u00dft ihm einige Tropfen", "tokens": ["Se\u00b7raph", "Jthu\u00b7riel", ",", "sieht", "ihn", ",", "und", "gie\u00dft", "ihm", "ei\u00b7ni\u00b7ge", "Trop\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.23": {"text": "Ruh in sein Herz. Nur dieses vermag er itzo zu geben,", "tokens": ["Ruh", "in", "sein", "Herz", ".", "Nur", "die\u00b7ses", "ver\u00b7mag", "er", "it\u00b7zo", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "ADV", "PDS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Ob er gleich ein Unsterblicher ist. Der traurende J\u00fcnger", "tokens": ["Ob", "er", "gleich", "ein", "U\u00b7nsterb\u00b7li\u00b7cher", "ist", ".", "Der", "trau\u00b7ren\u00b7de", "J\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "F\u00fchlt die Lindrung, und k\u00f6mmt so weit zu sich selbst, da\u00df er aufsieht,", "tokens": ["F\u00fchlt", "die", "Lind\u00b7rung", ",", "und", "k\u00f6mmt", "so", "weit", "zu", "sich", "selbst", ",", "da\u00df", "er", "auf\u00b7sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD", "APPR", "PRF", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Und mit w\u00fcnschendem Auge nach seinen Freunden umhersucht,", "tokens": ["Und", "mit", "w\u00fcn\u00b7schen\u00b7dem", "Au\u00b7ge", "nach", "sei\u00b7nen", "Freun\u00b7den", "um\u00b7her\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.27": {"text": "Da\u00df er zu ihnen hingeh, und sie ihn strafen, und tr\u00f6sten.", "tokens": ["Da\u00df", "er", "zu", "ih\u00b7nen", "hin\u00b7geh", ",", "und", "sie", "ihn", "stra\u00b7fen", ",", "und", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "KON", "PPER", "PPER", "VVFIN", "$,", "KON", "VVINF", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Aber er stand noch immer, und sah nach Jerusalem nieder.", "tokens": ["A\u00b7ber", "er", "stand", "noch", "im\u00b7mer", ",", "und", "sah", "nach", "Je\u00b7ru\u00b7sa\u00b7lem", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,", "KON", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.29": {"text": "Denn zum H\u00fcgel hinauf, zum Todesh\u00fcgel, zu sehen,", "tokens": ["Denn", "zum", "H\u00fc\u00b7gel", "hin\u00b7auf", ",", "zum", "To\u00b7des\u00b7h\u00fc\u00b7gel", ",", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Die\u00df vermocht er izt nicht. Sein Aug arbeitet mit scharfem", "tokens": ["Die\u00df", "ver\u00b7mocht", "er", "izt", "nicht", ".", "Sein", "Aug", "ar\u00b7bei\u00b7tet", "mit", "schar\u00b7fem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Untersuchendem Blicke, die stolze Stadt zu erkennen.", "tokens": ["Un\u00b7ter\u00b7su\u00b7chen\u00b7dem", "Bli\u00b7cke", ",", "die", "stol\u00b7ze", "Stadt", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Aber sie lag, so weit sie Gefilde dekte, so hoch sie", "tokens": ["A\u00b7ber", "sie", "lag", ",", "so", "weit", "sie", "Ge\u00b7fil\u00b7de", "dek\u00b7te", ",", "so", "hoch", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "NN", "VVFIN", "$,", "ADV", "ADJD", "PPER"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.33": {"text": "Th\u00fcrmte, geh\u00fcllt in traurende, schwerbelastende D\u00e4mmrung,", "tokens": ["Th\u00fcrm\u00b7te", ",", "ge\u00b7h\u00fcllt", "in", "trau\u00b7ren\u00b7de", ",", "schwer\u00b7be\u00b7las\u00b7ten\u00b7de", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "F\u00fcrchterlich da. Kaum da\u00df noch von seinen Zinnen der Tempel", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "da", ".", "Kaum", "da\u00df", "noch", "von", "sei\u00b7nen", "Zin\u00b7nen", "der", "Tem\u00b7pel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "ADV", "KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Und von seinen Th\u00fcrmen der Sion, sterbenden Schimmer", "tokens": ["Und", "von", "sei\u00b7nen", "Th\u00fcr\u00b7men", "der", "Si\u00b7on", ",", "ster\u00b7ben\u00b7den", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Sinken liessen. So lag Jerusalem. Petrus wandte", "tokens": ["Sin\u00b7ken", "lies\u00b7sen", ".", "So", "lag", "Je\u00b7ru\u00b7sa\u00b7lem", ".", "Pet\u00b7rus", "wand\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ADV", "VVFIN", "NE", "$.", "FM", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.37": {"text": "Nach der Seite sein Auge, von der ein dumpfes Gemurmel", "tokens": ["Nach", "der", "Sei\u00b7te", "sein", "Au\u00b7ge", ",", "von", "der", "ein", "dum\u00b7pfes", "Ge\u00b7mur\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Ausging. Es waren Fremdlinge, die zum Feste gekommen,", "tokens": ["Aus\u00b7ging", ".", "Es", "wa\u00b7ren", "Fremd\u00b