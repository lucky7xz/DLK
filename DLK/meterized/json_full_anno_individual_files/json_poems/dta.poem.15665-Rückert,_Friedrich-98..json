{"dta.poem.15665": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehalt an! das war ein Sprung; wie reimt sich das zusammen?", "tokens": ["\u201e", "halt", "an", "!", "das", "war", "ein", "Sprung", ";", "wie", "reimt", "sich", "das", "zu\u00b7sam\u00b7men", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "PRF", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gr\u00fcnde seh' ich nicht, daraus die Folgen stammen.", "tokens": ["Die", "Gr\u00fcn\u00b7de", "seh'", "ich", "nicht", ",", "da\u00b7raus", "die", "Fol\u00b7gen", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn ich dir folgen soll, so mu\u00dft du Schritt vor Schritt", "tokens": ["Wenn", "ich", "dir", "fol\u00b7gen", "soll", ",", "so", "mu\u00dft", "du", "Schritt", "vor", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fein schreiten, und auch mein Verst\u00e4ndnis nehmen mit.\u201c", "tokens": ["Fein", "schrei\u00b7ten", ",", "und", "auch", "mein", "Ver\u00b7st\u00e4nd\u00b7nis", "neh\u00b7men", "mit", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "ADV", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun, wenn geschritten nicht, so war es denn gesprungen;", "tokens": ["Nun", ",", "wenn", "ge\u00b7schrit\u00b7ten", "nicht", ",", "so", "war", "es", "denn", "ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADJA", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Sprung, was schadet er, wenn er uns ist gelungen?", "tokens": ["Ein", "Sprung", ",", "was", "scha\u00b7det", "er", ",", "wenn", "er", "uns", "ist", "ge\u00b7lun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ohn' einen Sprung von dort wirds nicht her\u00fcber gehn;", "tokens": ["Ohn'", "ei\u00b7nen", "Sprung", "von", "dort", "wirds", "nicht", "her\u00b7\u00fc\u00b7ber", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADV", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du nicht springen willst, so bleib nur dr\u00fcben stehn.", "tokens": ["Wenn", "du", "nicht", "sprin\u00b7gen", "willst", ",", "so", "bleib", "nur", "dr\u00fc\u00b7ben", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}