{"dta.poem.19983": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n  Kepler .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arm, preisgegeben jeglicher Beschwerde,", "tokens": ["Arm", ",", "preis\u00b7ge\u00b7ge\u00b7ben", "jeg\u00b7li\u00b7cher", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Vom undankbaren Heimatland vertrieben,", "tokens": ["Vom", "un\u00b7dank\u00b7ba\u00b7ren", "Hei\u00b7mat\u00b7land", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah er empor von dieser kalten Erde,", "tokens": ["Sah", "er", "em\u00b7por", "von", "die\u00b7ser", "kal\u00b7ten", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lernte recht die warmen Sonnen lieben.", "tokens": ["Und", "lern\u00b7te", "recht", "die", "war\u00b7men", "Son\u00b7nen", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Erd' entlehntes Licht er gern entbehrte,", "tokens": ["Der", "Erd'", "ent\u00b7lehn\u00b7tes", "Licht", "er", "gern", "ent\u00b7behr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "War ihm die hell're Heimat doch geblieben,", "tokens": ["War", "ihm", "die", "hell'\u00b7re", "Hei\u00b7mat", "doch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von Sonnengold sein hehres Haupt umflossen,", "tokens": ["Von", "Son\u00b7nen\u00b7gold", "sein", "heh\u00b7res", "Haupt", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Standen die Himmel all' ihm aufgeschlossen.", "tokens": ["Stan\u00b7den", "die", "Him\u00b7mel", "all'", "ihm", "auf\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}