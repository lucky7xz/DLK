{"textgrid.poem.26084": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Als gingen die Schatten einer Welt in St\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wolkenschatten kommen \u00fcber Morgenwiesen geflogen,", "tokens": ["Wol\u00b7ken\u00b7schat\u00b7ten", "kom\u00b7men", "\u00fc\u00b7ber", "Mor\u00b7gen\u00b7wie\u00b7sen", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Als wollen sie das Rasengr\u00fcn wie Teppiche rollen;", "tokens": ["Als", "wol\u00b7len", "sie", "das", "Ra\u00b7sen\u00b7gr\u00fcn", "wie", "Tep\u00b7pi\u00b7che", "rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie haben den Morgen in den Arm genommen", "tokens": ["Sie", "ha\u00b7ben", "den", "Mor\u00b7gen", "in", "den", "Arm", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sind dir begeistert entgegengezogen.", "tokens": ["Und", "sind", "dir", "be\u00b7geis\u00b7tert", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Wolkenschatten schwimmen hin wie dunkle Schollen,", "tokens": ["Die", "Wol\u00