{"dta.poem.21053": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Sulvina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erste der M&#228;dchen, du Edelste Beste,               ", "tokens": ["Ers\u00b7te", "der", "M", "&#228;", "dchen", ",", "du", "E\u00b7dels\u00b7te", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "ART", "NE", "$(", "VVINF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Traulich, wie flisternde Schatten am Bach,", "tokens": ["Trau\u00b7lich", ",", "wie", "flis\u00b7tern\u00b7de", "Schat\u00b7ten", "am", "Bach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Treu, wie des Himmels saffirene Feste,", "tokens": ["Treu", ",", "wie", "des", "Him\u00b7mels", "saf\u00b7fi\u00b7re\u00b7ne", "Fes\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Z\u00e4rtlich, wie fl\u00f6tender Nachtigallschlag,", "tokens": ["Z\u00e4rt\u00b7lich", ",", "wie", "fl\u00f6\u00b7ten\u00b7der", "Nach\u00b7ti\u00b7gall\u00b7schlag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Siehe, wie feyern, wie fr\u00f6hlich, o Beste,", "tokens": ["Sie\u00b7he", ",", "wie", "fey\u00b7ern", ",", "wie", "fr\u00f6h\u00b7lich", ",", "o", "Bes\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Feyern wir deinen gesegneten Tag!", "tokens": ["Fey\u00b7ern", "wir", "dei\u00b7nen", "ge\u00b7se\u00b7gne\u00b7ten", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Dass wir dich haben, und dass du uns liebest,", "tokens": ["Dass", "wir", "dich", "ha\u00b7ben", ",", "und", "dass", "du", "uns", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dass uns umg\u00fcrtet dein freundlicher Arm,", "tokens": ["Dass", "uns", "um\u00b7g\u00fcr\u00b7tet", "dein", "freund\u00b7li\u00b7cher", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Dass du mit uns dich erfreust und betr\u00fcbest,", "tokens": ["Dass", "du", "mit", "uns", "dich", "er\u00b7freust", "und", "be\u00b7tr\u00fc\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dass du uns l\u00e4chelst in Lust und in Harm,", "tokens": ["Dass", "du", "uns", "l\u00e4\u00b7chelst", "in", "Lust", "und", "in", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dass du der Tugenden Beyspiel uns giebest \u2014", "tokens": ["Dass", "du", "der", "Tu\u00b7gen\u00b7den", "Bey\u00b7spiel", "uns", "gie\u00b7best"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fchlen wir heute so innig, so warm!", "tokens": ["F\u00fch\u00b7len", "wir", "heu\u00b7te", "so", "in\u00b7nig", ",", "so", "warm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Heiter, Geliebte, verwalle dein Leben,", "tokens": ["Hei\u00b7ter", ",", "Ge\u00b7lieb\u00b7te", ",", "ver\u00b7wal\u00b7le", "dein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Freundlich, wie Quellengeriesel im Hayn!", "tokens": ["Freund\u00b7lich", ",", "wie", "Quel\u00b7len\u00b7ge\u00b7rie\u00b7sel", "im", "Hayn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Jedem erblassenden Tag' entschweben", "tokens": ["Je\u00b7dem", "er\u00b7blas\u00b7sen\u00b7den", "Tag'", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seelen, wie Engel, so l\u00e4chelnd, so rein!", "tokens": ["See\u00b7len", ",", "wie", "En\u00b7gel", ",", "so", "l\u00e4\u00b7chelnd", ",", "so", "rein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Heiss ist dein Ringen, und sch\u00f6n ist dein Streben,", "tokens": ["Heiss", "ist", "dein", "Rin\u00b7gen", ",", "und", "sch\u00f6n", "ist", "dein", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "N\u00fctzlich und theuer den Deinen zu seyn!", "tokens": ["N\u00fctz\u00b7lich", "und", "theu\u00b7er", "den", "Dei\u00b7nen", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "N\u00fctzlich, du Edle, und theuer den Deinen,", "tokens": ["N\u00fctz\u00b7lich", ",", "du", "Ed\u00b7le", ",", "und", "theu\u00b7er", "den", "Dei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wandle die Wallfahrt der Erde hinab!", "tokens": ["Wand\u00b7le", "die", "Wall\u00b7fahrt", "der", "Er\u00b7de", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ruhe umarm' dich in k\u00fchlenden Haynen,", "tokens": ["Ru\u00b7he", "um\u00b7arm'", "dich", "in", "k\u00fch\u00b7len\u00b7den", "Hay\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Fr\u00f6mmigkeit reiche dir Anker und Stab?", "tokens": ["Fr\u00f6m\u00b7mig\u00b7keit", "rei\u00b7che", "dir", "An\u00b7ker", "und", "Stab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Redliche werden den Rasen umweinen,", "tokens": ["Red\u00b7li\u00b7che", "wer\u00b7den", "den", "Ra\u00b7sen", "um\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Welcher einst h\u00fcllet dein h\u00fcglichtes Grab.", "tokens": ["Wel\u00b7cher", "einst", "h\u00fcl\u00b7let", "dein", "h\u00fcg\u00b7lich\u00b7tes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber der himmlischen Heimath der Tugend", "tokens": ["A\u00b7ber", "der", "himm\u00b7li\u00b7schen", "Hei\u00b7math", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwinget die rechte Sulvina sich zu,", "tokens": ["Schwin\u00b7get", "die", "rech\u00b7te", "Sul\u00b7vi\u00b7na", "sich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "PRF", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wandelt dort oben in ewiger Jugend,", "tokens": ["Wan\u00b7delt", "dort", "o\u00b7ben", "in", "e\u00b7wi\u00b7ger", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ruhe folgt Thaten, und Thaten die Ruh.", "tokens": ["Ru\u00b7he", "folgt", "Tha\u00b7ten", ",", "und", "Tha\u00b7ten", "die", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "KON", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Siehe, sie klimmet von Tugend zu Tugend,", "tokens": ["Sie\u00b7he", ",", "sie", "klim\u00b7met", "von", "Tu\u00b7gend", "zu", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kr\u00e4ftiger, seliger jegliches Nu.", "tokens": ["Kr\u00e4f\u00b7ti\u00b7ger", ",", "se\u00b7li\u00b7ger", "jeg\u00b7li\u00b7ches", "Nu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebe, Geliebte, die Himmlische immer.", "tokens": ["Lie\u00b7be", ",", "Ge\u00b7lieb\u00b7te", ",", "die", "Himm\u00b7li\u00b7sche", "im\u00b7mer", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Lilien welken und Rosen verbl\u00fchn.", "tokens": ["Li\u00b7li\u00b7en", "wel\u00b7ken", "und", "Ro\u00b7sen", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Welten zerfallen in grausige Tr\u00fcmmer;", "tokens": ["Wel\u00b7ten", "zer\u00b7fal\u00b7len", "in", "grau\u00b7si\u00b7ge", "Tr\u00fcm\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sonnen erl\u00f6schen und Sterne zerspr\u00fchn.", "tokens": ["Son\u00b7nen", "er\u00b7l\u00f6\u00b7schen", "und", "Ster\u00b7ne", "zer\u00b7spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Th\u00e4tige Tugend gl\u00e4nzt ewigen Schimmer;", "tokens": ["Th\u00e4\u00b7ti\u00b7ge", "Tu\u00b7gend", "gl\u00e4nzt", "e\u00b7wi\u00b7gen", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Nimmer verwelkt ihr unsterbliches Gr\u00fcn.", "tokens": ["Nim\u00b7mer", "ver\u00b7welkt", "ihr", "uns\u00b7terb\u00b7li\u00b7ches", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}