{"dta.poem.10749": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n  Von angetragener Lieb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Da Mopsus newlich erst die Nisa mir trug' an/", "tokens": ["Da", "Mop\u00b7sus", "new\u00b7lich", "erst", "die", "Ni\u00b7sa", "mir", "trug'", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "ADV", "ART", "NN", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd ", "tokens": ["Vnd"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ihr jeder lobte sie/ vermeint auch jederman/", "tokens": ["Ihr", "je\u00b7der", "lob\u00b7te", "sie", "/", "ver\u00b7meint", "auch", "je\u00b7der\u00b7man", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "$(", "VVFIN", "ADV", "PIS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hette mich f\u00fcrwar mit nichten jhr zuschemen/", "tokens": ["Ich", "het\u00b7te", "mich", "f\u00fcr\u00b7war", "mit", "nich\u00b7ten", "jhr", "zu\u00b7sche\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich antwort jhn behendt: so nempt sie selber hin/", "tokens": ["Ich", "ant\u00b7wort", "jhn", "be\u00b7hendt", ":", "so", "nempt", "sie", "sel\u00b7ber", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil sie euch so sehr gefellt vor andern allen/", "tokens": ["Die\u00b7weil", "sie", "euch", "so", "sehr", "ge\u00b7fellt", "vor", "an\u00b7dern", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVPP", "APPR", "PIS", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich nem mir eine/ die gefellet meinem Sinn:", "tokens": ["Ich", "nem", "mir", "ei\u00b7ne", "/", "die", "ge\u00b7fel\u00b7let", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "$(", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann was ich lieben soll/ das mu\u00df mir selbst gefallen.", "tokens": ["Dann", "was", "ich", "lie\u00b7ben", "soll", "/", "das", "mu\u00df", "mir", "selbst", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVINF", "VMFIN", "$(", "PDS", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}