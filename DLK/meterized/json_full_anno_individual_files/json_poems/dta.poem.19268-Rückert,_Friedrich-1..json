{"dta.poem.19268": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube nicht, da\u00df ich viel eignes neues lehre,", "tokens": ["Ich", "glau\u00b7be", "nicht", ",", "da\u00df", "ich", "viel", "eig\u00b7nes", "neu\u00b7es", "leh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch durch mein Scherflein Witz den Schatz der Weisheit mehre.", "tokens": ["Noch", "durch", "mein", "Scher\u00b7flein", "Witz", "den", "Schatz", "der", "Weis\u00b7heit", "meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch denk' ich von der M\u00fch mir zweierlei Gewinn;", "tokens": ["Doch", "denk'", "ich", "von", "der", "M\u00fch", "mir", "zwei\u00b7er\u00b7lei", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einmal, da\u00df ich nun selbst an Einsicht weiter bin;", "tokens": ["Ein\u00b7mal", ",", "da\u00df", "ich", "nun", "selbst", "an", "Ein\u00b7sicht", "wei\u00b7ter", "bin", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sodann, da\u00df doch dadurch an manchen Mann wird kommen", "tokens": ["So\u00b7dann", ",", "da\u00df", "doch", "da\u00b7durch", "an", "man\u00b7chen", "Mann", "wird", "kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "PAV", "APPR", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manches, wovon er sonst gar h\u00e4tte nichts vernommen.", "tokens": ["Man\u00b7ches", ",", "wo\u00b7von", "er", "sonst", "gar", "h\u00e4t\u00b7te", "nichts", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "ADV", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und auch der dritte Grund scheint werth nicht des Gel\u00e4chters:", "tokens": ["Und", "auch", "der", "drit\u00b7te", "Grund", "scheint", "werth", "nicht", "des", "Ge\u00b7l\u00e4ch\u00b7ters", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, wer dis B\u00fcchlein list, derweil doch list kein schlechters.", "tokens": ["Da\u00df", ",", "wer", "dis", "B\u00fcch\u00b7lein", "list", ",", "der\u00b7weil", "doch", "list", "kein", "schlech\u00b7ters", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PDS", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}