, "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr schwarzes Haar \u00fcbers wei\u00dfe Gewand,", "tokens": ["Ihr", "schwar\u00b7zes", "Haar", "\u00fc\u00b7bers", "wei\u00b7\u00dfe", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie fl\u00fcstert: \u00bbIch lieb' ihn mit Seel' und Leib \u2013", "tokens": ["Sie", "fl\u00fcs\u00b7tert", ":", "\u00bb", "Ich", "lieb'", "ihn", "mit", "Seel'", "und", "Leib", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was soll mich k\u00fcmmern sein grambla\u00df Weib.", "tokens": ["Was", "soll", "mich", "k\u00fcm\u00b7mern", "sein", "gram\u00b7bla\u00df", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Und ob ihr br\u00e4che das Herz in der Brust,", "tokens": ["Und", "ob", "ihr", "br\u00e4\u00b7che", "das", "Herz", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Je blasser die Tote, je r\u00f6ter die Lust,", "tokens": ["Je", "blas\u00b7ser", "die", "To\u00b7te", ",", "je", "r\u00f6\u00b7ter", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Feigherzig Gewissen, fahr hin, fahr hin,", "tokens": ["Feig\u00b7her\u00b7zig", "Ge\u00b7wis\u00b7sen", ",", "fahr", "hin", ",", "fahr", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es brennt mein Blut, und es schwindelt mein Sinn.\u00ab", "tokens": ["Es", "brennt", "mein", "Blut", ",", "und", "es", "schwin\u00b7delt", "mein", "Sinn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Sie spricht es. Da sieh, hellblendender Schein", "tokens": ["Sie", "spricht", "es", ".", "Da", "sieh", ",", "hell\u00b7blen\u00b7den\u00b7der", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00e4llt von der T\u00fcr in den Spiegel hinein;", "tokens": ["F\u00e4llt", "von", "der", "T\u00fcr", "in", "den", "Spie\u00b7gel", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie wendet sich um, auf schreit sie j\u00e4h \u2013", "tokens": ["Sie", "wen\u00b7det", "sich", "um", ",", "auf", "schreit", "sie", "j\u00e4h", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "APPR", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein trat Kurf\u00fcrstin ", "tokens": ["Ein", "trat", "Kur\u00b7f\u00fcrs\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["ART", "VVFIN", "NN"], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Mit bebt die Kerze, halb niedergebrannt ...", "tokens": ["Mit", "bebt", "die", "Ker\u00b7ze", ",", "halb", "nie\u00b7der\u00b7ge\u00b7brannt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}