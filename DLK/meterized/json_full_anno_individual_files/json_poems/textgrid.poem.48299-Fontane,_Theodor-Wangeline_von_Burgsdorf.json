{"textgrid.poem.48299": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wangeline von Burgsdorf", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die Sage: Und will Gefahr", "tokens": ["Das", "ist", "die", "Sa\u00b7ge", ":", "Und", "will", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "KON", "VMFIN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Hohenzollern umgarnen,", "tokens": ["Die", "Ho\u00b7hen\u00b7zol\u00b7lern", "um\u00b7gar\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da wird lebendig ein alter Fluch,", "tokens": ["Da", "wird", "le\u00b7ben\u00b7dig", "ein", "al\u00b7ter", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Zeigt sich, um zu warnen.", "tokens": ["Zeigt", "sich", ",", "um", "zu", "war\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sie kommt dreimal, geht um dreimal,", "tokens": ["Sie", "kommt", "drei\u00b7mal", ",", "geht", "um", "drei\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00f6gernder immer und tr\u00fcber,", "tokens": ["Z\u00f6\u00b7gern\u00b7der", "im\u00b7mer", "und", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Wache ruft ihr Halt-Werda nicht mehr,", "tokens": ["Die", "Wa\u00b7che", "ruft", "ihr", "Hal\u00b7tWer\u00b7da", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie wei\u00df, ", "tokens": ["Sie", "wei\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Der Schatten schreitet vor\u00fcber.", "tokens": ["Der", "Schat\u00b7ten", "schrei\u00b7tet", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Lichter vergl\u00fchn, im Schlo\u00df wird's still,", "tokens": ["Die", "Lich\u00b7ter", "ver\u00b7gl\u00fchn", ",", "im", "Schlo\u00df", "wird's", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPRART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur Eine, die sich noch schm\u00fccken will,", "tokens": ["Nur", "Ei\u00b7ne", ",", "die", "sich", "noch", "schm\u00fc\u00b7cken", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie tritt an den Spiegel und l\u00f6st ihr Haar,", "tokens": ["Sie", "tritt", "an", "den", "Spie\u00b7gel", "und", "l\u00f6st", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wangeline die sch\u00f6ne, wie sch\u00f6n sie war.", "tokens": ["Wan\u00b7ge\u00b7li\u00b7ne", "die", "sch\u00f6\u00b7ne", ",", "wie", "sch\u00f6n", "sie", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie schm\u00fcckt sich; f\u00fcr wen? F\u00fcr ihren Galan:", "tokens": ["Sie", "schm\u00fcckt", "sich", ";", "f\u00fcr", "wen", "?", "F\u00fcr", "ih\u00b7ren", "Ga\u00b7lan", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "APPR", "PWS", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kurt Jagow hat es ihr angetan;", "tokens": ["Kurt", "Ja\u00b7gow", "hat", "es", "ihr", "an\u00b7ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie sahen sich viel, sie k\u00fc\u00dften sich oft,", "tokens": ["Sie", "sa\u00b7hen", "sich", "viel", ",", "sie", "k\u00fc\u00df\u00b7ten", "sich", "oft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird heut er kommen? Sie harrt, sie hofft.", "tokens": ["Wird", "heut", "er", "kom\u00b7men", "?", "Sie", "harrt", ",", "sie", "hofft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVINF", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie hofft und wirft mit schimmernder Hand", "tokens": ["Sie", "hofft", "und", "wirft", "mit", "schim\u00b7mern\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr schwarzes Haar \u00fcbers wei\u00dfe Gewand,", "tokens": ["Ihr", "schwar\u00b7zes", "Haar", "\u00fc\u00b7bers", "wei\u00b7\u00dfe", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie fl\u00fcstert: \u00bbIch lieb' ihn mit Seel' und Leib \u2013", "tokens": ["Sie", "fl\u00fcs\u00b7tert", ":", "\u00bb", "Ich", "lieb'", "ihn", "mit", "Seel'", "und", "Leib", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was soll mich k\u00fcmmern sein grambla\u00df Weib.", "tokens": ["Was", "soll", "mich", "k\u00fcm\u00b7mern", "sein", "gram\u00b7bla\u00df", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und ob ihr br\u00e4che das Herz in der Brust,", "tokens": ["Und", "ob", "ihr", "br\u00e4\u00b7che", "das", "Herz", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Je blasser die Tote, je r\u00f6ter die Lust,", "tokens": ["Je", "blas\u00b7ser", "die", "To\u00b7te", ",", "je", "r\u00f6\u00b7ter", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Feigherzig Gewissen, fahr hin, fahr hin,", "tokens": ["Feig\u00b7her\u00b7zig", "Ge\u00b7wis\u00b7sen", ",", "fahr", "hin", ",", "fahr", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es brennt mein Blut, und es schwindelt mein Sinn.\u00ab", "tokens": ["Es", "brennt", "mein", "Blut", ",", "und", "es", "schwin\u00b7delt", "mein", "Sinn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie spricht es. Da sieh, hellblendender Schein", "tokens": ["Sie", "spricht", "es", ".", "Da", "sieh", ",", "hell\u00b7blen\u00b7den\u00b7der", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00e4llt von der T\u00fcr in den Spiegel hinein;", "tokens": ["F\u00e4llt", "von", "der", "T\u00fcr", "in", "den", "Spie\u00b7gel", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie wendet sich um, auf schreit sie j\u00e4h \u2013", "tokens": ["Sie", "wen\u00b7det", "sich", "um", ",", "auf", "schreit", "sie", "j\u00e4h", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "APPR", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein trat Kurf\u00fcrstin ", "tokens": ["Ein", "trat", "Kur\u00b7f\u00fcrs\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["ART", "VVFIN", "NN"], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Mit bebt die Kerze, halb niedergebrannt ...", "tokens": ["Mit", "bebt", "die", "Ker\u00b7ze", ",", "halb", "nie\u00b7der\u00b7ge\u00b7brannt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Das ist die Sage: Und will Gefahr", "tokens": ["Das", "ist", "die", "Sa\u00b7ge", ":", "Und", "will", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "KON", "VMFIN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Hohenzollern umgarnen,", "tokens": ["Die", 