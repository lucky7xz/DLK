{"dta.poem.20527": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer auff schw\u00fcre bauet/", "tokens": ["Wer", "auff", "schw\u00fc\u00b7re", "bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und auff worte trauet/", "tokens": ["Und", "auff", "wor\u00b7te", "trau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So die zeit zerbricht/", "tokens": ["So", "die", "zeit", "zer\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dem gel\u00fcckt es nicht;", "tokens": ["Dem", "ge\u00b7l\u00fcckt", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ja wird leichtlich nein/", "tokens": ["Ja", "wird", "leicht\u00b7lich", "nein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ADJD", "PTKANT", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Und ein falscher schein", "tokens": ["Und", "ein", "fal\u00b7scher", "schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Rei\u00dft offt allen f\u00fcrsatz ein.", "tokens": ["Rei\u00dft", "offt", "al\u00b7len", "f\u00fcr\u00b7satz", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zirckel der gedancken", "tokens": ["Zir\u00b7ckel", "der", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "K\u00f6nnen leichtlich wancken/", "tokens": ["K\u00f6n\u00b7nen", "leicht\u00b7lich", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der liebe-schlu\u00df", "tokens": ["Und", "der", "lie\u00b7be\u00b7schlu\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00e4lt nicht lange fu\u00df.", "tokens": ["H\u00e4lt", "nicht", "lan\u00b7ge", "fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Was die welt erfreut/", "tokens": ["Was", "die", "welt", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nennt die leichte zeit", "tokens": ["Nennt", "die", "leich\u00b7te", "zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wechsel der ergetzligkeit.", "tokens": ["Wech\u00b7sel", "der", "er\u00b7getz\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Allzu lange sonne", "tokens": ["All\u00b7zu", "lan\u00b7ge", "son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PTKA", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Machet schlechte wonne/", "tokens": ["Ma\u00b7chet", "schlech\u00b7te", "won\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Muscateller must/", "tokens": ["Mus\u00b7ca\u00b7tel\u00b7ler", "must", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ja die h\u00f6chste lust", "tokens": ["Ja", "die", "h\u00f6chs\u00b7te", "lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bringet dem verdru\u00df/", "tokens": ["Brin\u00b7get", "dem", "ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der mit \u00fcberflu\u00df", "tokens": ["Der", "mit", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Solches stets geniessen mu\u00df.", "tokens": ["Sol\u00b7ches", "stets", "ge\u00b7nies\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Honig wird zu gallen/", "tokens": ["Ho\u00b7nig", "wird", "zu", "gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Greul will dem gefallen", "tokens": ["Greul", "will", "dem", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "An der seite stehn/", "tokens": ["An", "der", "sei\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und zu nechste gehn.", "tokens": ["Und", "zu", "nechs\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mancher blume pracht/", "tokens": ["Man\u00b7cher", "blu\u00b7me", "pracht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Heut/ als gold geacht/", "tokens": ["Heut", "/", "als", "gold", "ge\u00b7acht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "NN", "VVPP", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wird in kurtzer zeit verlacht.", "tokens": ["Wird", "in", "kurt\u00b7zer", "zeit", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer nicht menschen kennet", "tokens": ["Wer", "nicht", "men\u00b7schen", "ken\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "VVINF", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Meynt/ da\u00df alles brennet/", "tokens": ["Meynt", "/", "da\u00df", "al\u00b7les", "bren\u00b7net", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PIS", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was da feuer rufft.", "tokens": ["Was", "da", "feu\u00b7er", "rufft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ach/ der hertzen grufft!", "tokens": ["Ach", "/", "der", "hert\u00b7zen", "grufft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den die treu entweicht/", "tokens": ["Den", "die", "treu", "ent\u00b7weicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Die viel list durchschleicht/", "tokens": ["Die", "viel", "list", "durch\u00b7schleicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Hat kein maa\u00df noch bley erreicht.", "tokens": ["Hat", "kein", "maa\u00df", "noch", "bley", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}