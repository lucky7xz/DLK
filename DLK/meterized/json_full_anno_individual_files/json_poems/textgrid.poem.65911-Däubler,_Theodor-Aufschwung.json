{"textgrid.poem.65911": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Aufschwung", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tript\u00f3lemos h\u00e4lt Rast auf Kores Fluren.", "tokens": ["Trip\u00b7t\u00f3le\u00b7mos", "h\u00e4lt", "Rast", "auf", "Ko\u00b7res", "Flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Besinnt sich ihr Gem\u00fct des Hades schon?", "tokens": ["Be\u00b7sinnt", "sich", "ihr", "Ge\u00b7m\u00fct", "des", "Ha\u00b7des", "schon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erwacht, dem Schlafe zugedacht, der Mohn?", "tokens": ["Er\u00b7wacht", ",", "dem", "Schla\u00b7fe", "zu\u00b7ge\u00b7dacht", ",", "der", "Mohn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei Herbstzeitlosen mahnen Plutos Spuren.", "tokens": ["Bei", "Herbst\u00b7zeit\u00b7lo\u00b7sen", "mah\u00b7nen", "Plu\u00b7tos", "Spu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob auch die Menschen seinen Ruf erfuhren?", "tokens": ["Ob", "auch", "die", "Men\u00b7schen", "sei\u00b7nen", "Ruf", "er\u00b7fuh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punc