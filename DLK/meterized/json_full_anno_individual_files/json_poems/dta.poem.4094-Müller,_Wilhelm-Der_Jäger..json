{"dta.poem.4094": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bleib, trotziger J\u00e4ger, in deinem Revier!", "tokens": ["Bleib", ",", "trot\u00b7zi\u00b7ger", "J\u00e4\u00b7ger", ",", "in", "dei\u00b7nem", "Re\u00b7vier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hier giebt es kein Wild zu jagen f\u00fcr dich,", "tokens": ["Hier", "giebt", "es", "kein", "Wild", "zu", "ja\u00b7gen", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "APPR", "PPER", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hier wohnt nur ein Rehlein, ein zahmes, f\u00fcr mich.", "tokens": ["Hier", "wohnt", "nur", "ein", "Reh\u00b7lein", ",", "ein", "zah\u00b7mes", ",", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und willst du das z\u00e4rtliche Rehlein sehn,", "tokens": ["Und", "willst", "du", "das", "z\u00e4rt\u00b7li\u00b7che", "Reh\u00b7lein", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "So la\u00df deine B\u00fcchsen im Walde stehn,", "tokens": ["So", "la\u00df", "dei\u00b7ne", "B\u00fcch\u00b7sen", "im", "Wal\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und la\u00df deine klaffenden Hunde zu Haus,", "tokens": ["Und", "la\u00df", "dei\u00b7ne", "klaf\u00b7fen\u00b7den", "Hun\u00b7de", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und la\u00df auf dem Horne den Saus und Braus,", "tokens": ["Und", "la\u00df", "auf", "dem", "Hor\u00b7ne", "den", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und scheere vom Kinne das struppige Haar,", "tokens": ["Und", "schee\u00b7re", "vom", "Kin\u00b7ne", "das", "strup\u00b7pi\u00b7ge", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Sonst scheut sich im Garten das Rehlein, f\u00fcrwahr.", "tokens": ["Sonst", "scheut", "sich", "im", "Gar\u00b7ten", "das", "Reh\u00b7lein", ",", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Doch besser, du bliebest im Walde dazu,", "tokens": ["Doch", "bes\u00b7ser", ",", "du", "blie\u00b7best", "im", "Wal\u00b7de", "da\u00b7zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VVFIN", "APPRART", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und lie\u00dfest die M\u00fchlen und M\u00fcller in Ruh'.", "tokens": ["Und", "lie\u00b7\u00dfest", "die", "M\u00fch\u00b7len", "und", "M\u00fcl\u00b7ler", "in", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NE", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was taugen die Fischlein im gr\u00fcnen Gezweig?", "tokens": ["Was", "tau\u00b7gen", "die", "Fisc\u00b7hlein", "im", "gr\u00fc\u00b7nen", "Ge\u00b7zweig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was will denn das Eichhorn im bl\u00e4ulichen Teich?", "tokens": ["Was", "will", "denn", "das", "Eich\u00b7horn", "im", "bl\u00e4u\u00b7li\u00b7chen", "Teich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Drum bleibe, du trotziger J\u00e4ger, im Hain,", "tokens": ["Drum", "blei\u00b7be", ",", "du", "trot\u00b7zi\u00b7ger", "J\u00e4\u00b7ger", ",", "im", "Hain", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und la\u00df mich mit meinen drei R\u00e4dern allein;", "tokens": ["Und", "la\u00df", "mich", "mit", "mei\u00b7nen", "drei", "R\u00e4\u00b7dern", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPOSAT", "CARD", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und willst meinem Sch\u00e4tzchen dich machen beliebt,", "tokens": ["Und", "willst", "mei\u00b7nem", "Sch\u00e4tz\u00b7chen", "dich", "ma\u00b7chen", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "So wisse, mein Freund, was ihr Herzchen betr\u00fcbt:", "tokens": ["So", "wis\u00b7se", ",", "mein", "Freund", ",", "was", "ihr", "Herz\u00b7chen", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die Eber, die kommen zu Nacht aus dem Hain", "tokens": ["Die", "E\u00b7ber", ",", "die", "kom\u00b7men", "zu", "Nacht", "aus", "dem", "Hain"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und brechen in ihren Kohlgarten ein,", "tokens": ["Und", "bre\u00b7chen", "in", "ih\u00b7ren", "Kohl\u00b7gar\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und treten und w\u00fchlen herum in dem Feld:", "tokens": ["Und", "tre\u00b7ten", "und", "w\u00fch\u00b7len", "he\u00b7rum", "in", "dem", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Die Eber, die schie\u00dfe, du J\u00e4gerheld!", "tokens": ["Die", "E\u00b7ber", ",", "die", "schie\u00b7\u00dfe", ",", "du", "J\u00e4\u00b7ger\u00b7held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PPER", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}