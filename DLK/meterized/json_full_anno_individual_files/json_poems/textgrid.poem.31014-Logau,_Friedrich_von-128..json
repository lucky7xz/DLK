{"textgrid.poem.31014": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "128.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die ihr die Venus h\u00f6net, da\u00df sie ihr zum Mann erlesen,", "tokens": ["Ihr", ",", "die", "ihr", "die", "Ve\u00b7nus", "h\u00f6\u00b7net", ",", "da\u00df", "sie", "ihr", "zum", "Mann", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der da lahm, grob, starck und t\u00f6lpisch, der ein Hammerschmid gewesen,", "tokens": ["Der", "da", "lahm", ",", "grob", ",", "starck", "und", "t\u00f6l\u00b7pisch", ",", "der", "ein", "Ham\u00b7mer\u00b7schmid", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PRELS", "ART", "NN", "VAPP", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wist ihr nicht, da\u00df G\u00f6tter-Augen tieff auch ins verborgne dringen?", "tokens": ["Wist", "ihr", "nicht", ",", "da\u00df", "G\u00f6t\u00b7ter\u00b7Au\u00b7gen", "tieff", "auch", "ins", "ver\u00b7borg\u00b7ne", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "NN", "ADJD", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Venus wuste, was ihr diente, sehnte sich nach andren Dingen,", "tokens": ["Ve\u00b7nus", "wus\u00b7te", ",", "was", "ihr", "dien\u00b7te", ",", "sehn\u00b7te", "sich", "nach", "an\u00b7dren", "Din\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Als ein albres Mensch gemeinet. Ihr Vulcanus war gefasset", "tokens": ["Als", "ein", "alb\u00b7res", "Mensch", "ge\u00b7mei\u00b7net", ".", "Ihr", "Vul\u00b7ca\u00b7nus", "war", "ge\u00b7fas\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Mit Gezeug und Hau\u00dfger\u00e4the, so ihr auch lieb. Sonsten lasset", "tokens": ["Mit", "Ge\u00b7zeug", "und", "Hau\u00df\u00b7ge\u00b7r\u00e4\u00b7the", ",", "so", "ihr", "auch", "lieb", ".", "Sons\u00b7ten", "las\u00b7set"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADV", "PPER", "ADV", "ADJD", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Ihr in gutem hin nur gehen, wann sie auff die derben Speisen", "tokens": ["Ihr", "in", "gu\u00b7tem", "hin", "nur", "ge\u00b7hen", ",", "wann", "sie", "auff", "die", "der\u00b7ben", "Spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "ADV", "ADV", "VVINF", "$,", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Ihrem Magen mit Confecten etwas nietlichs wolln erweisen.", "tokens": ["Ih\u00b7rem", "Ma\u00b7gen", "mit", "Con\u00b7fec\u00b7ten", "et\u00b7was", "niet\u00b7lichs", "wolln", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADV", "PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ihr, die ihr die Venus h\u00f6net, da\u00df sie ihr zum Mann erlesen,", "tokens": ["Ihr", ",", "die", "ihr", "die", "Ve\u00b7nus", "h\u00f6\u00b7net", ",", "da\u00df", "sie", "ihr", "zum", "Mann", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der da lahm, grob, starck und t\u00f6lpisch, der ein Hammerschmid gewesen,", "tokens": ["Der", "da", "lahm", ",", "grob", ",", "starck", "und", "t\u00f6l\u00b7pisch", ",", "der", "ein", "Ham\u00b7mer\u00b7schmid", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PRELS", "ART", "NN", "VAPP", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wist ihr nicht, da\u00df G\u00f6tter-Augen tieff auch ins verborgne dringen?", "tokens": ["Wist", "ihr", "nicht", ",", "da\u00df", "G\u00f6t\u00b7ter\u00b7Au\u00b7gen", "tieff", "auch", "ins", "ver\u00b7borg\u00b7ne", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "