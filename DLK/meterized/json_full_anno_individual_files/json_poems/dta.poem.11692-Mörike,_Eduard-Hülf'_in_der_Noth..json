{"dta.poem.11692": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "H\u00fclf' in der Noth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ein rechter Freund erscheint uns in der Noth", "tokens": ["Ein", "rech\u00b7ter", "Freund", "er\u00b7scheint", "uns", "in", "der", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu rechter Zeit und sicher wie der Tod.", "tokens": ["Zu", "rech\u00b7ter", "Zeit", "und", "si\u00b7cher", "wie", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch offen, Bester, sag' ich dir,", "tokens": ["Doch", "of\u00b7fen", ",", "Bes\u00b7ter", ",", "sag'", "ich", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast eine ganz verw\u00fcnschte Manier!", "tokens": ["Du", "hast", "ei\u00b7ne", "ganz", "ver\u00b7w\u00fcnschte", "Ma\u00b7nier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Du trocknest mir den Jammerschwei\u00df,", "tokens": ["Du", "trock\u00b7nest", "mir", "den", "Jam\u00b7mer\u00b7schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und machst mir doch die H\u00f6lle hei\u00df,", "tokens": ["Und", "machst", "mir", "doch", "die", "H\u00f6l\u00b7le", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bringst das ganze j\u00fcngste Gericht", "tokens": ["Du", "bringst", "das", "gan\u00b7ze", "j\u00fcngs\u00b7te", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Mit dir; \u2014 bei Gott, so meint' ich's nicht!", "tokens": ["Mit", "dir", ";", "bei", "Gott", ",", "so", "meint'", "ich's", "nicht", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "$(", "APPR", "NN", "$,", "ADV", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}