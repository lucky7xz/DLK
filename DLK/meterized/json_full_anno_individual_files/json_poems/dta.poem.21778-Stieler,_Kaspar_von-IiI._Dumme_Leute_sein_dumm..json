{"dta.poem.21778": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Dumme Leute sein dumm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' an Fozis k\u00fchlen Fl\u00fcssen", "tokens": ["Ich", "hab'", "an", "Fo\u00b7zis", "k\u00fch\u00b7len", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dir Delia/ manch Lied zu Ehren auffgespielt.", "tokens": ["dir", "De\u00b7lia", "/", "manch", "Lied", "zu", "Eh\u00b7ren", "auff\u00b7ge\u00b7spielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$(", "PIAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Die Musen und Apollo wissen", "tokens": ["Die", "Mu\u00b7sen", "und", "A\u00b7pol\u00b7lo", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NE", "VVINF"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "wie offt der Sonnen-Licht mich brant\u2019 und F\u00f6be", "tokens": ["wie", "offt", "der", "Son\u00b7nen\u00b7Licht", "mich", "brant'", "und", "F\u00f6\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und wie ich manche Nacht gewacht", "tokens": ["und", "wie", "ich", "man\u00b7che", "Nacht", "ge\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und einen Vers auff dich erdacht.", "tokens": ["und", "ei\u00b7nen", "Vers", "auff", "dich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Pallas hat mich offt geneidet/", "tokens": ["Die", "Pal\u00b7las", "hat", "mich", "offt", "ge\u00b7nei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df ich nicht ihr zu Ruhmgebraucht der Poesie/", "tokens": ["da\u00df", "ich", "nicht", "ihr", "zu", "Ruhm\u00b7ge\u00b7braucht", "der", "Poe\u00b7sie", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPER", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "weil sie die Venus nimmer leidet", "tokens": ["weil", "sie", "die", "Ve\u00b7nus", "nim\u00b7mer", "lei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und sonder Liebe lebt in Keuschheit ie und ie.", "tokens": ["und", "son\u00b7der", "Lie\u00b7be", "lebt", "in", "Keuschheit", "ie", "und", "ie."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "NN", "ADV", "KON", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch hab\u2019 ich stets die Nacht gewacht/", "tokens": ["Doch", "hab'", "ich", "stets", "die", "Nacht", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und einen Vers auff dich erdacht.", "tokens": ["und", "ei\u00b7nen", "Vers", "auff", "dich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Amor machte mir von Myrten", "tokens": ["Der", "A\u00b7mor", "mach\u00b7te", "mir", "von", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "vor mein verliebt Gedicht so manchen Siegeskranz", "tokens": ["vor", "mein", "ver\u00b7liebt", "Ge\u00b7dicht", "so", "man\u00b7chen", "Sie\u00b7ges\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "VVFIN", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Musen sah\u2019 ich mich umg\u00fcrten", "tokens": ["die", "Mu\u00b7sen", "sah'", "ich", "mich", "um\u00b7g\u00fcr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit dunkelm Efen-laub und g\u00f6ldnem Lorbeer-glanz/", "tokens": ["mit", "dun\u00b7kelm", "E\u00b7fen\u00b7laub", "und", "g\u00f6ld\u00b7nem", "Lor\u00b7beer\u00b7glanz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "indehm ich manche Nacht gewacht", "tokens": ["in\u00b7dehm", "ich", "man\u00b7che", "Nacht", "ge\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und einen Vers auff dich erdacht.", "tokens": ["und", "ei\u00b7nen", "Vers", "auff", "dich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die dummen geht zu beyden Ohren", "tokens": ["Die", "dum\u00b7men", "geht", "zu", "bey\u00b7den", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der s\u00fcssen Reime Schall bald au\u00df/ bald wieder ein", "tokens": ["der", "s\u00fcs\u00b7sen", "Rei\u00b7me", "Schall", "bald", "au\u00df", "/", "bald", "wie\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "PTKVZ", "$(", "ADV", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Kunst hat ganz an dir verlohren/", "tokens": ["die", "Kunst", "hat", "ganz", "an", "dir", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ich mu\u00df bey dir umsonst des F\u00f6bus Lehrling sein/", "tokens": ["ich", "mu\u00df", "bey", "dir", "um\u00b7sonst", "des", "F\u00f6\u00b7bus", "Lehr\u00b7ling", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADV", "ART", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wiewol ich manche Nacht gewacht", "tokens": ["wie\u00b7wol", "ich", "man\u00b7che", "Nacht", "ge\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und einen Vers auff dich erdacht.", "tokens": ["und", "ei\u00b7nen", "Vers", "auff", "dich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du/ Orffeus k\u00f6nnst die H\u00f6lle zwingen/", "tokens": ["Du", "/", "Orf\u00b7feus", "k\u00f6nnst", "die", "H\u00f6l\u00b7le", "zwin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NE", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich kan sie nicht zu rechte bringen", "tokens": ["ich", "kan", "sie", "nicht", "zu", "rech\u00b7te", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "was hilfft es da\u00df ich nu gewacht", "tokens": ["was", "hilfft", "es", "da\u00df", "ich", "nu", "ge\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und manchen Vers auff sie erdacht?", "tokens": ["und", "man\u00b7chen", "Vers", "auff", "sie", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Soll ich mein Dicht-werk nu verschweren", "tokens": ["Soll", "ich", "mein", "Dicht\u00b7werk", "nu", "ver\u00b7schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was du/ du Kunst-spott/ hast bi\u00dfher an mir veracht.", "tokens": ["was", "du", "/", "du", "Kunst\u00b7spott", "/", "hast", "bi\u00df\u00b7her", "an", "mir", "ver\u00b7acht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$(", "PPER", "NN", "$(", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Pfui! da\u00df ich manche Nacht gewacht/", "tokens": ["Pfui", "!", "da\u00df", "ich", "man\u00b7che", "Nacht", "ge\u00b7wacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und keinen Schimpff auff dich erdacht.", "tokens": ["und", "kei\u00b7nen", "Schimpff", "auff", "dich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}