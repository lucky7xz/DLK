, "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und komme mit den besten Seegel-Winden", "tokens": ["Und", "kom\u00b7me", "mit", "den", "bes\u00b7ten", "See\u00b7gel\u00b7Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fcck, auf Lasten seines Reichthums stolz.", "tokens": ["Zu\u00b7r\u00fcck", ",", "auf", "Las\u00b7ten", "sei\u00b7nes", "Reicht\u00b7hums", "stolz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "APPR", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ich fodre nichts, als Brod f\u00fcr meine Tage,", "tokens": ["Ich", "fod\u00b7re", "nichts", ",", "als", "Brod", "f\u00fcr", "mei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ruhig Herz, und Kleider, die ich trage,", "tokens": ["Ein", "ru\u00b7hig", "Herz", ",", "und", "Klei\u00b7der", ",", "die", "ich", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um, mehr bedeckt, als stolz geziert, zu seyn.", "tokens": ["Um", ",", "mehr", "be\u00b7deckt", ",", "als", "stolz", "ge\u00b7ziert", ",", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$,", "ADV", "VVPP", "$,", "KOUS", "ADJD", "VVPP", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Menschen-Arm erh\u00e4lt das Gl\u00fccke b\u00e4ndig;", "tokens": ["Kein", "Men\u00b7schen\u00b7Arm", "er\u00b7h\u00e4lt", "das", "Gl\u00fc\u00b7cke", "b\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn es will, sey es mir unbest\u00e4ndig;", "tokens": ["Und", "wenn", "es", "will", ",", "sey", "es", "mir", "un\u00b7be\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur bleibt alsdann, ihr wahren Freunde, mein!", "tokens": ["Nur", "bleibt", "als\u00b7dann", ",", "ihr", "wah\u00b7ren", "Freun\u00b7de", ",", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wird mir, wenn einst auch b\u00f6se Tage kommen,", "tokens": ["Wird", "mir", ",", "wenn", "einst", "auch", "b\u00f6\u00b7se", "Ta\u00b7ge", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur dieser Schatz, die Freundschaft nicht genommen;", "tokens": ["Nur", "die\u00b7ser", "Schatz", ",", "die", "Freund\u00b7schaft", "nicht", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Behalt ich alt, der Musen Saitenspiel;", "tokens": ["Be\u00b7halt", "ich", "alt", ",", "der", "Mu\u00b7sen", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hab ich gnug Gl\u00fcckseligkeit und Freuden,", "tokens": ["So", "hab", "ich", "gnug", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und heisse mich die Pr\u00e4chtigen beneiden.", "tokens": ["Und", "heis\u00b7se", "mich", "die", "Pr\u00e4ch\u00b7ti\u00b7gen", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie haben oft kein Herze zum Gef\u00fchl!", "tokens": ["Sie", "ha\u00b7ben", "oft", "kein", "Her\u00b7ze", "zum", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}