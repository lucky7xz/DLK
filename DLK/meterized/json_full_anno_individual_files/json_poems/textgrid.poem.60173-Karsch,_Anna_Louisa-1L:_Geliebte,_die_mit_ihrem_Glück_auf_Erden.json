t!", "tokens": ["Und", "gl\u00fcck\u00b7lich", ",", "da\u00df", "du", "mei\u00b7ne", "Freun\u00b7din", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es fahre die, der es das Gl\u00fcck gew\u00e4hret", "tokens": ["Es", "fah\u00b7re", "die", ",", "der", "es", "das", "Gl\u00fcck", "ge\u00b7w\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Wagen, sey an Kopf und Brust beschweret", "tokens": ["Im", "Wa\u00b7gen", ",", "sey", "an", "Kopf", "und", "Brust", "be\u00b7schwe\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Steinen, die ein Bergdurchw\u00fchler fand.", "tokens": ["Mit", "Stei\u00b7nen", ",", "die", "ein", "Berg\u00b7durch\u00b7w\u00fch\u00b7ler", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00fcnsche mir kein Haus mit Marmor W\u00e4nden", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "kein", "Haus", "mit", "Mar\u00b7mor", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und keinen Prunksaal; keine zum Verschwenden", "tokens": ["Und", "kei\u00b7nen", "Prun