{"textgrid.poem.60173": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebte, die mit ihrem Gl\u00fcck auf Erden", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte, die mit ihrem Gl\u00fcck auf Erden", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "die", "mit", "ih\u00b7rem", "Gl\u00fcck", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zufrieden bleibt, wenn gl\u00fccklicher zu werden", "tokens": ["Zu\u00b7frie\u00b7den", "bleibt", ",", "wenn", "gl\u00fcck\u00b7li\u00b7cher", "zu", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Thor erfindungsreich an W\u00fcnschen ist.", "tokens": ["Der", "Thor", "er\u00b7fin\u00b7dungs\u00b7reich", "an", "W\u00fcn\u00b7schen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchl in mir nicht eitle Triebe brennen,", "tokens": ["Ich", "f\u00fchl", "in", "mir", "nicht", "eit\u00b7le", "Trie\u00b7be", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin vergn\u00fcgt dein sch\u00f6nes Herz zu kennen,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "dein", "sch\u00f6\u00b7nes", "Herz", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gl\u00fccklich, da\u00df du meine Freundin bist!", "tokens": ["Und", "gl\u00fcck\u00b7lich", ",", "da\u00df", "du", "mei\u00b7ne", "Freun\u00b7din", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es fahre die, der es das Gl\u00fcck gew\u00e4hret", "tokens": ["Es", "fah\u00b7re", "die", ",", "der", "es", "das", "Gl\u00fcck", "ge\u00b7w\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Wagen, sey an Kopf und Brust beschweret", "tokens": ["Im", "Wa\u00b7gen", ",", "sey", "an", "Kopf", "und", "Brust", "be\u00b7schwe\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Steinen, die ein Bergdurchw\u00fchler fand.", "tokens": ["Mit", "Stei\u00b7nen", ",", "die", "ein", "Berg\u00b7durch\u00b7w\u00fch\u00b7ler", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00fcnsche mir kein Haus mit Marmor W\u00e4nden", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "kein", "Haus", "mit", "Mar\u00b7mor", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und keinen Prunksaal; keine zum Verschwenden", "tokens": ["Und", "kei\u00b7nen", "Prunk\u00b7saal", ";", "kei\u00b7ne", "zum", "Ver\u00b7schwen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$.", "PIAT", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gedeckte Tafel, von des Schicksahls Hand!", "tokens": ["Ge\u00b7deck\u00b7te", "Ta\u00b7fel", ",", "von", "des", "Schick\u00b7sahls", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Gl\u00fccklicher mag von dem Traubenstocke", "tokens": ["Ein", "Gl\u00fcck\u00b7li\u00b7cher", "mag", "von", "dem", "Trau\u00b7ben\u00b7sto\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Kelter f\u00fcllen, und den Kaufmann locke", "tokens": ["Die", "Kel\u00b7ter", "f\u00fcl\u00b7len", ",", "und", "den", "Kauf\u00b7mann", "lo\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oft der Gewinn, auf ein zerbrechlich Holz.", "tokens": ["Oft", "der", "Ge\u00b7winn", ",", "auf", "ein", "zer\u00b7brech\u00b7lich", "Holz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er schiffe schwer von Hoffnung hin nach Inden,", "tokens": ["Er", "schif\u00b7fe", "schwer", "von", "Hoff\u00b7nung", "hin", "nach", "In\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und komme mit den besten Seegel-Winden", "tokens": ["Und", "kom\u00b7me", "mit", "den", "bes\u00b7ten", "See\u00b7gel\u00b7Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fcck, auf Lasten seines Reichthums stolz.", "tokens": ["Zu\u00b7r\u00fcck", ",", "auf", "Las\u00b7ten", "sei\u00b7nes", "Reicht\u00b7hums", "stolz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "APPR", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich fodre nichts, als Brod f\u00fcr meine Tage,", "tokens": ["Ich", "fod\u00b7re", "nichts", ",", "als", "Brod", "f\u00fcr", "mei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ruhig Herz, und Kleider, die ich trage,", "tokens": ["Ein", "ru\u00b7hig", "Herz", ",", "und", "Klei\u00b7der", ",", "die", "ich", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um, mehr bedeckt, als stolz geziert, zu seyn.", "tokens": ["Um", ",", "mehr", "be\u00b7deckt", ",", "als", "stolz", "ge\u00b7ziert", ",", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$,", "ADV", "VVPP", "$,", "KOUS", "ADJD", "VVPP", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Menschen-Arm erh\u00e4lt das Gl\u00fccke b\u00e4ndig;", "tokens": ["Kein", "Men\u00b7schen\u00b7Arm", "er\u00b7h\u00e4lt", "das", "Gl\u00fc\u00b7cke", "b\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn es will, sey es mir unbest\u00e4ndig;", "tokens": ["Und", "wenn", "es", "will", ",", "sey", "es", "mir", "un\u00b7be\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur bleibt alsdann, ihr wahren Freunde, mein!", "tokens": ["Nur", "bleibt", "als\u00b7dann", ",", "ihr", "wah\u00b7ren", "Freun\u00b7de", ",", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wird mir, wenn einst auch b\u00f6se Tage kommen,", "tokens": ["Wird", "mir", ",", "wenn", "einst", "auch", "b\u00f6\u00b7se", "Ta\u00b7ge", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur dieser Schatz, die Freundschaft nicht genommen;", "tokens": ["Nur", "die\u00b7ser", "Schatz", ",", "die", "Freund\u00b7schaft", "nicht", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Behalt ich alt, der Musen Saitenspiel;", "tokens": ["Be\u00b7halt", "ich", "alt", ",", "der", "Mu\u00b7sen", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hab ich gnug Gl\u00fcckseligkeit und Freuden,", "tokens": ["So", "hab", "ich", "gnug", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und heisse mich die Pr\u00e4chtigen beneiden.", "tokens": ["Und", "heis\u00b7se", "mich", "die", "Pr\u00e4ch\u00b7ti\u00b7gen", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie haben oft kein Herze zum Gef\u00fchl!", "tokens": ["Sie", "ha\u00b7ben", "oft", "kein", "Her\u00b7ze", "zum", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line