{"dta.poem.19631": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die F\u00fc\u00dfe im Feuer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wild zuckt der Blitz. In fahlem Lichte steht ein Thurm.", "tokens": ["Wild", "zuckt", "der", "Blitz", ".", "In", "fah\u00b7lem", "Lich\u00b7te", "steht", "ein", "Thurm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Donner rollt. Ein Reiter k\u00e4mpft mit seinem Ro\u00df,", "tokens": ["Der", "Don\u00b7ner", "rollt", ".", "Ein", "Rei\u00b7ter", "k\u00e4mpft", "mit", "sei\u00b7nem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Springt ab und pocht ans Thor und l\u00e4rmt. Sein Mantel saust", "tokens": ["Springt", "ab", "und", "pocht", "ans", "Thor", "und", "l\u00e4rmt", ".", "Sein", "Man\u00b7tel", "saust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Wind. Er h\u00e4lt den scheuen Fuchs am Z\u00fcgel fest.", "tokens": ["Im", "Wind", ".", "Er", "h\u00e4lt", "den", "scheu\u00b7en", "Fuchs", "am", "Z\u00fc\u00b7gel", "fest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein schmales Gitterfenster schimmert golden hell", "tokens": ["Ein", "schma\u00b7les", "Git\u00b7ter\u00b7fens\u00b7ter", "schim\u00b7mert", "gol\u00b7den", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und knarrend \u00f6ffnet jetzt das Thor ein Edelmann ...", "tokens": ["Und", "knar\u00b7rend", "\u00f6ff\u00b7net", "jetzt", "das", "Thor", "ein", "E\u00b7del\u00b7mann", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "\u2014 \u201eIch bin ein Knecht des K\u00f6nigs, als Courier geschickt", "tokens": ["\u201e", "Ich", "bin", "ein", "Knecht", "des", "K\u00f6\u00b7nigs", ",", "als", "Cou\u00b7rier", "ge\u00b7schickt"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "NN", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nach Nimes. Herbergt mich! Ihr kennt des K\u00f6nigs Rock!\u201c", "tokens": ["Nach", "Ni\u00b7mes", ".", "Her\u00b7bergt", "mich", "!", "Ihr", "kennt", "des", "K\u00f6\u00b7nigs", "Rock", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$.", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u2014 \u201eEs st\u00fcrmt. Mein Gast bist Dein Kleid, was k\u00fcmmert's mich?", "tokens": ["\u201e", "Es", "st\u00fcrmt", ".", "Mein", "Gast", "bist", "Dein", "Kleid", ",", "was", "k\u00fcm\u00b7mert's", "mich", "?"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "--+--+-++-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Tritt ein und w\u00e4rme dich! Ich sorge f\u00fcr dein Thier!\u201c", "tokens": ["Tritt", "ein", "und", "w\u00e4r\u00b7me", "dich", "!", "Ich", "sor\u00b7ge", "f\u00fcr", "dein", "Thier", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Reiter tritt in einen dunklen Ahnensaal,", "tokens": ["Der", "Rei\u00b7ter", "tritt", "in", "ei\u00b7nen", "dunk\u00b7len", "Ah\u00b7nen\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von eines weiten Herdes Feuer schwach erhellt,", "tokens": ["Von", "ei\u00b7nes", "wei\u00b7ten", "Her\u00b7des", "Feu\u00b7er", "schwach", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und je nach seines Flackerns launenhaftem Licht", "tokens": ["Und", "je", "nach", "sei\u00b7nes", "Fla\u00b7ckerns", "lau\u00b7nen\u00b7haf\u00b7tem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Droht hier ein Hugenott im Harnisch, dort ein Weib,", "tokens": ["Droht", "hier", "ein", "Hu\u00b7ge\u00b7nott", "im", "Har\u00b7nisch", ",", "dort", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPRART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein stolzes Edelweib aus braunem Ahnenbild ...", "tokens": ["Ein", "stol\u00b7zes", "E\u00b7del\u00b7weib", "aus", "brau\u00b7nem", "Ah\u00b7nen\u00b7bild", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Reiter wirft sich in den Sessel vor dem Herd", "tokens": ["Der", "Rei\u00b7ter", "wirft", "sich", "in", "den", "Ses\u00b7sel", "vor", "dem", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und starrt in den lebend'gen Brand. Er br\u00fctet, gafft ...", "tokens": ["Und", "starrt", "in", "den", "le\u00b7ben\u00b7d'\u00b7gen", "Brand", ".", "Er", "br\u00fc\u00b7tet", ",", "gafft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Leis str\u00e4ubt sich ihm das Haar. Er kennt den Herd, den Saal ...", "tokens": ["Leis", "str\u00e4ubt", "sich", "ihm", "das", "Haar", ".", "Er", "kennt", "den", "Herd", ",", "den", "Saal", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Flamme zischt. Zwei F\u00fc\u00dfe zucken in der Glut.", "tokens": ["Die", "Flam\u00b7me", "zischt", ".", "Zwei", "F\u00fc\u00b7\u00dfe", "zu\u00b7cken", "in", "der", "Glut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Abendtisch bestellt die greise Schaffnerin", "tokens": ["Den", "A\u00b7bend\u00b7tisch", "be\u00b7stellt", "die", "grei\u00b7se", "Schaff\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Linnen blendend wei\u00df. Das Edelm\u00e4gdlein hilft.", "tokens": ["Mit", "Lin\u00b7nen", "blen\u00b7dend", "wei\u00df", ".", "Das", "E\u00b7del\u00b7m\u00e4gd\u00b7lein", "hilft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Knabe trug den Krug mit Wein. Der Kinder Blick", "tokens": ["Ein", "Kna\u00b7be", "trug", "den", "Krug", "mit", "Wein", ".", "Der", "Kin\u00b7der", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hangt schreckensstarr am Gast und hangt am Herd entsetzt ...", "tokens": ["Hangt", "schre\u00b7ckens\u00b7starr", "am", "Gast", "und", "hangt", "am", "Herd", "ent\u00b7setzt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Flamme zischt. Zwei F\u00fc\u00dfe zucken in der Glut.", "tokens": ["Die", "Flam\u00b7me", "zischt", ".", "Zwei", "F\u00fc\u00b7\u00dfe", "zu\u00b7cken", "in", "der", "Glut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "\u2014 \u201eVerdammt! Dasselbe Wappen! Dieser selbe Saal!", "tokens": ["\u201e", "Ver\u00b7dammt", "!", "Das\u00b7sel\u00b7be", "Wap\u00b7pen", "!", "Die\u00b7ser", "sel\u00b7be", "Saal", "!"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVPP", "$.", "PDAT", "NN", "$.", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drei Jahre sind's ... Auf einer Hugenottenjagd ...", "tokens": ["Drei", "Jah\u00b7re", "sin\u00b7d's", "...", "Auf", "ei\u00b7ner", "Hu\u00b7ge\u00b7not\u00b7ten\u00b7jagd", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NE", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein fein, halsstarrig Weib ... \u201eWo steckt der Junker? Sprich!\u201c", "tokens": ["Ein", "fein", ",", "hals\u00b7star\u00b7rig", "Weib", "...", "\u201e", "Wo", "steckt", "der", "Jun\u00b7ker", "?", "Sprich", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "NN", "$(", "$(", "PWAV", "VVFIN", "ART", "NN", "$.", "VVIMP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie schweigt. \u201eBekenn!\u201c Sie schweigt. \u201eGieb ihn heraus!\u201c", "tokens": ["Sie", "schweigt", ".", "\u201e", "Be\u00b7kenn", "!", "\u201c", "Sie", "schweigt", ".", "\u201e", "Gieb", "ihn", "he\u00b7raus", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$.", "$(", "PPER", "VVFIN", "$.", "$(", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich werde wild. ", "tokens": ["Ich", "wer\u00b7de", "wild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die F\u00fc\u00dfe pack' ich ihr und bl\u00f6\u00dfe sie und strecke sie", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "pack'", "ich", "ihr", "und", "bl\u00f6\u00b7\u00dfe", "sie", "und", "stre\u00b7cke", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Tief mitten in die Glut ... \u201eGieb ihn heraus!\u201c ... Sie", "tokens": ["Tief", "mit\u00b7ten", "in", "die", "Glut", "...", "\u201e", "Gieb", "ihn", "he\u00b7raus", "!", "\u201c", "...", "Sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$(", "$(", "VVIMP", "PPER", "PTKVZ", "$.", "$(", "$(", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Sie windet sich ... Sahst du das Wappen nicht am Thor?", "tokens": ["Sie", "win\u00b7det", "sich", "...", "Sahst", "du", "das", "Wap\u00b7pen", "nicht", "am", "Thor", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wer hie\u00df dich hier zu Gaste gehen, dummer Narr?", "tokens": ["Wer", "hie\u00df", "dich", "hier", "zu", "Gas\u00b7te", "ge\u00b7hen", ",", "dum\u00b7mer", "Narr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hat er nur einen Tropfen Bluts, erw\u00fcrgt er dich.\u201c", "tokens": ["Hat", "er", "nur", "ei\u00b7nen", "Trop\u00b7fen", "Bluts", ",", "er\u00b7w\u00fcrgt", "er", "dich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein tritt der Edelmann. \u201eDu tr\u00e4umst! Zu Tische, Gast ...\u201c", "tokens": ["Ein", "tritt", "der", "E\u00b7del\u00b7mann", ".", "\u201e", "Du", "tr\u00e4umst", "!", "Zu", "Ti\u00b7sche", ",", "Gast", "...", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "$.", "APPR", "NN", "$,", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da sitzen sie. Die Drei in ihrer schwarzen Tracht", "tokens": ["Da", "sit\u00b7zen", "sie", ".", "Die", "Drei", "in", "ih\u00b7rer", "schwar\u00b7zen", "Tracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "CARD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und er. Doch keins der Kinder spricht das Tischgebet.", "tokens": ["Und", "er", ".", "Doch", "keins", "der", "Kin\u00b7der", "spricht", "das", "Tischge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "KON", "PIAT", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn starren sie mit aufgerissnen Augen an \u2014", "tokens": ["Ihn", "star\u00b7ren", "sie", "mit", "auf\u00b7ge\u00b7riss\u00b7nen", "Au\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Becher f\u00fcllt und \u00fcbergie\u00dft er, st\u00fcrzt den Trunk,", "tokens": ["Den", "Be\u00b7cher", "f\u00fcllt", "und", "\u00fc\u00b7berg\u00b7ie\u00dft", "er", ",", "st\u00fcrzt", "den", "Trunk", ","], "token_info": ["word", "word", "word", "word", "word", 