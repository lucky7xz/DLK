{"textgrid.poem.49433": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tanzt mir den Reigen!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tanzt mir den Reigen!", "tokens": ["Tanzt", "mir", "den", "Rei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich liebt' ihr holdes Augenpaar,", "tokens": ["Ich", "liebt'", "ihr", "hol\u00b7des", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das heller als ein Stern mir war,", "tokens": ["Das", "hel\u00b7ler", "als", "ein", "Stern", "mir", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich liebt' die Augen sp\u00f6ttisch-klar,", "tokens": ["Ich", "liebt'", "die", "Au\u00b7gen", "sp\u00f6t\u00b7tischklar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Tanzt mir den Reigen!", "tokens": ["Tanzt", "mir", "den", "Rei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie plagte ihren Freund so lieb,", "tokens": ["Sie", "plag\u00b7te", "ih\u00b7ren", "Freund", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "po