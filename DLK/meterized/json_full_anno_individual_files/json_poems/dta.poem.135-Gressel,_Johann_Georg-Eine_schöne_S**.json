{"dta.poem.135": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eine sch\u00f6ne S**", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Zinnober ziehrt den Moo\u00df/ den Circul kr\u00f6hnen Myrthen/", "tokens": ["Zin\u00b7no\u00b7ber", "ziehrt", "den", "Moo\u00df", "/", "den", "Cir\u00b7cul", "kr\u00f6h\u00b7nen", "Myr\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein schwartz Wald deckt den Ort wo ", "tokens": ["Ein", "schwartz", "Wald", "deckt", "den", "Ort", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "PWAV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und h\u00fclt die Muschel bey/ so ", "tokens": ["Und", "h\u00fclt", "die", "Mu\u00b7schel", "bey", "/", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "$(", "ADV"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Sie m\u00fcste sonst zu viel den L\u00fcstern Gast bewirthen.", "tokens": ["Sie", "m\u00fcs\u00b7te", "sonst", "zu", "viel", "den", "L\u00fcs\u00b7tern", "Gast", "be\u00b7wirt\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKA", "PIS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Insul und ihr Port sind frey von allen Syrten/", "tokens": ["Die", "In\u00b7sul", "und", "ihr", "Port", "sind", "frey", "von", "al\u00b7len", "Syr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie sch\u00fctzt die ", "tokens": ["Sie", "sch\u00fctzt", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Da liegt ein ", "tokens": ["Da", "liegt", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Das angenehme Feld ist nur vor einen Hirten.", "tokens": ["Das", "an\u00b7ge\u00b7neh\u00b7me", "Feld", "ist", "nur", "vor", "ei\u00b7nen", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die eng-gew\u00f6lbte Schacht zeigt Rosen und Rubinen.", "tokens": ["Die", "eng\u00b7ge\u00b7w\u00f6lb\u00b7te", "Schacht", "zeigt", "Ro\u00b7sen", "und", "Ru\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Lust und Kitzelung der s\u00fcssen Anmuth dienen.", "tokens": ["Die", "Lust", "und", "Kit\u00b7ze\u00b7lung", "der", "s\u00fcs\u00b7sen", "An\u00b7muth", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Esse hegt ein Feur/ das es soweit gebracht", "tokens": ["Die", "Es\u00b7se", "hegt", "ein", "Feur", "/", "das", "es", "so\u00b7weit", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df es was St\u00e4hlern schien zu weichen Wachse macht.", "tokens": ["Da\u00df", "es", "was", "St\u00e4h\u00b7lern", "schien", "zu", "wei\u00b7chen", "Wach\u00b7se", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die sch\u00f6ne Gegend l\u00e4st in seichten Rosen - Gr\u00fcnden", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ge\u00b7gend", "l\u00e4st", "in", "seich\u00b7ten", "Ro\u00b7sen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den allerbesten Schatz der Lieblichkeiten finden.", "tokens": ["Den", "al\u00b7ler\u00b7bes\u00b7ten", "Schatz", "der", "Lieb\u00b7lich\u00b7kei\u00b7ten", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}