": "alexandrine.iambic.hexa"}, "line.86": {"text": "In Arzeneyen, uns so sehr zu freuen haben;", "tokens": ["In", "Ar\u00b7ze\u00b7ne\u00b7yen", ",", "uns", "so", "sehr", "zu", "freu\u00b7en", "ha\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "So ist es billig, unserm Gott das Opfer unsrer Lust zu bringen;", "tokens": ["So", "ist", "es", "bil\u00b7lig", ",", "un\u00b7serm", "Gott", "das", "Op\u00b7fer", "uns\u00b7rer", "Lust", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.88": {"text": "Und ihn, als einen weisen Sch\u00f6pfer, auch bey dem Flieder, zu", "tokens": ["Und", "ihn", ",", "als", "ei\u00b7nen", "wei\u00b7sen", "Sch\u00f6p\u00b7fer", ",", "auch", "bey", "dem", "Flie\u00b7der", ",", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}