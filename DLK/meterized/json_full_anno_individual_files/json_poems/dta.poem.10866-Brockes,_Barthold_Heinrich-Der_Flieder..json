{"dta.poem.10866": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Flieder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst ging ich auf das Feld, wie ich zuweilen pflag,", "tokens": ["J\u00fcngst", "ging", "ich", "auf", "das", "Feld", ",", "wie", "ich", "zu\u00b7wei\u00b7len", "pflag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An einem angenehm-jedoch bedeckten Tag.", "tokens": ["An", "ei\u00b7nem", "an\u00b7ge\u00b7nehm\u00b7je\u00b7doch", "be\u00b7deck\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es stralete das helle Sonnenlicht", "tokens": ["Es", "stra\u00b7le\u00b7te", "das", "hel\u00b7le", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In der gewohnten Klarheit nicht,", "tokens": ["In", "der", "ge\u00b7wohn\u00b7ten", "Klar\u00b7heit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch war es zum spatzieren sehr bequem.", "tokens": ["Doch", "war", "es", "zum", "spat\u00b7zie\u00b7ren", "sehr", "be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die sanfte Luft war k\u00fchl und angenehm;", "tokens": ["Die", "sanf\u00b7te", "Luft", "war", "k\u00fchl", "und", "an\u00b7ge\u00b7nehm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es schien, als ob sich Licht und Schatten,", "tokens": ["Es", "schien", ",", "als", "ob", "sich", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sonst so sehr getrennt, vermischet hatten.", "tokens": ["Die", "sonst", "so", "sehr", "ge\u00b7trennt", ",", "ver\u00b7mi\u00b7schet", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVPP", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Man kunnte fast von ihnen beyden", "tokens": ["Man", "kunn\u00b7te", "fast", "von", "ih\u00b7nen", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "PPER", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht eines eigentlich vom andern unterscheiden.", "tokens": ["Nicht", "ei\u00b7nes", "ei\u00b7gent\u00b7lich", "vom", "an\u00b7dern", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man sahe keinen Sonnenschein,", "tokens": ["Man", "sa\u00b7he", "kei\u00b7nen", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Man kunnt auch keinen Schatten sehen;", "tokens": ["Man", "kunnt", "auch", "kei\u00b7nen", "Schat\u00b7ten", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein gr\u00fcnlich D\u00e4mmrungslicht war allgemein,", "tokens": ["Ein", "gr\u00fcn\u00b7lich", "D\u00e4mm\u00b7rungs\u00b7licht", "war", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Man sah es \u00fcberall entstehen.", "tokens": ["Man", "sah", "es", "\u00fc\u00b7be\u00b7rall", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Es stund, bey dem bedeckten Wetter,", "tokens": ["Es", "stund", ",", "bey", "dem", "be\u00b7deck\u00b7ten", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Das sch\u00f6ne Gr\u00fcn der Pflanzen und der Bl\u00e4tter", "tokens": ["Das", "sch\u00f6\u00b7ne", "Gr\u00fcn", "der", "Pflan\u00b7zen", "und", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "In so harmonischem Zusammenhang,", "tokens": ["In", "so", "har\u00b7mo\u00b7ni\u00b7schem", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Da\u00df ihre lichte Dunkelheit,", "tokens": ["Da\u00df", "ih\u00b7re", "lich\u00b7te", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mit ungemeiner Lieblichkeit,", "tokens": ["Mit", "un\u00b7ge\u00b7mei\u00b7ner", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Durch mein Gesicht, in mein Gem\u00fcthe drang,", "tokens": ["Durch", "mein", "Ge\u00b7sicht", ",", "in", "mein", "Ge\u00b7m\u00fc\u00b7the", "drang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Den gar zu schnellen Lauf des Bluts allm\u00e4hlig stillte,", "tokens": ["Den", "gar", "zu", "schnel\u00b7len", "Lauf", "des", "Bluts", "all\u00b7m\u00e4h\u00b7lig", "still\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In meinem Geist ein Gleichgewicht erregte,", "tokens": ["In", "mei\u00b7nem", "Geist", "ein", "Gleich\u00b7ge\u00b7wicht", "er\u00b7reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mit einem sittsamen und sanften Trieb ihn f\u00fcllte,", "tokens": ["Mit", "ei\u00b7nem", "sitt\u00b7sa\u00b7men", "und", "sanf\u00b7ten", "Trieb", "ihn", "f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und mich zu einer Art Gela\u00dfenheit bewegte.", "tokens": ["Und", "mich", "zu", "ei\u00b7ner", "Art", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "be\u00b7weg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich setzte mich, in dick verwachsnen B\u00fcschen,", "tokens": ["Ich", "setz\u00b7te", "mich", ",", "in", "dick", "ver\u00b7wachs\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wo vielerley Gew\u00e4chs ihr vielfach Prangen mischen,", "tokens": ["Wo", "vie\u00b7ler\u00b7ley", "Ge\u00b7w\u00e4chs", "ihr", "viel\u00b7fach", "Pran\u00b7gen", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und sahe, zwischen Schwarz- und Wei\u00dfdorn, Asp und Schlehen,", "tokens": ["Und", "sa\u00b7he", ",", "zwi\u00b7schen", "Schwa\u00b7rz", "und", "Wei\u00df\u00b7dorn", ",", "Asp", "und", "Schle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "TRUNC", "KON", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Auch Brombeer, Stachelkraut und Schilf, als wie ein Licht,", "tokens": ["Auch", "Brom\u00b7beer", ",", "Sta\u00b7chel\u00b7kraut", "und", "Schilf", ",", "als", "wie", "ein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den weissen Fliederbaum in voller Bl\u00fchte stehen.", "tokens": ["Den", "weis\u00b7sen", "Flie\u00b7der\u00b7baum", "in", "vol\u00b7ler", "Bl\u00fch\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sein weisser Schimmer fiel so stark mir ins Gesicht,", "tokens": ["Sein", "weis\u00b7ser", "Schim\u00b7mer", "fiel", "so", "stark", "mir", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df ich sein sonderbar Gew\u00e4chse zu besehen,", "tokens": ["Da\u00df", "ich", "sein", "son\u00b7der\u00b7bar", "Ge\u00b7w\u00e4ch\u00b7se", "zu", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und auch, in seiner Zier, den Sch\u00f6pfer zu erh\u00f6hen,", "tokens": ["Und", "auch", ",", "in", "sei\u00b7ner", "Zier", ",", "den", "Sch\u00f6p\u00b7fer", "zu", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mich nicht enthalten kunnt. Ich brach ein Bl\u00fcmchen ab,", "tokens": ["Mich", "nicht", "ent\u00b7hal\u00b7ten", "kunnt", ".", "Ich", "brach", "ein", "Bl\u00fcm\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das mir zu folgender Betrachtung, Anla\u00df gab.", "tokens": ["Das", "mir", "zu", "fol\u00b7gen\u00b7der", "Be\u00b7trach\u00b7tung", ",", "An\u00b7la\u00df", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Billig bist du, holder Flieder,", "tokens": ["Bil\u00b7lig", "bist", "du", ",", "hol\u00b7der", "Flie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Auch ein Vorwurf meiner Lieder,", "tokens": ["Auch", "ein", "Vor\u00b7wurf", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Da du ja so n\u00fctz als sch\u00f6n.", "tokens": ["Da", "du", "ja", "so", "n\u00fctz", "als", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Deine Frucht, Laub, Zweig und Bl\u00fchte", "tokens": ["Dei\u00b7ne", "Frucht", ",", "Laub", ",", "Zweig", "und", "Bl\u00fch\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Lassen Weisheit, Macht und G\u00fcte", "tokens": ["Las\u00b7sen", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Eines Sch\u00f6pfers, kl\u00e4rlich sehn.", "tokens": ["Ei\u00b7nes", "Sch\u00f6p\u00b7fers", ",", "kl\u00e4r\u00b7lich", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Ein Auge, das, wenns siehet, wirklich siehet,", "tokens": ["Ein", "Au\u00b7ge", ",", "das", ",", "wenns", "sie\u00b7het", ",", "wirk\u00b7lich", "sie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "KOUS", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Erblickt am Fliederbaum, der bl\u00fchet,", "tokens": ["Er\u00b7blickt", "am", "Flie\u00b7der\u00b7baum", ",", "der", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Verschiednes, das ihn r\u00fchrt und ihn vergn\u00fcget.", "tokens": ["Ver\u00b7schied\u00b7nes", ",", "das", "ihn", "r\u00fchrt", "und", "ihn", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Es l\u00e4\u00dft das dunkle Gr\u00fcn der Bl\u00e4tter, da es sich", "tokens": ["Es", "l\u00e4\u00dft", "das", "dunk\u00b7le", "Gr\u00fcn", "der", "Bl\u00e4t\u00b7ter", ",", "da", "es", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So lieblich, als verwunderlich,", "tokens": ["So", "lieb\u00b7lich", ",", "als", "ver\u00b7wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Zur weissen Bl\u00fchte lieblich f\u00fcget,", "tokens": ["Zur", "weis\u00b7sen", "Bl\u00fch\u00b7te", "lieb\u00b7lich", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Mit sch\u00f6nen weissen Rosenstr\u00e4uchen", "tokens": ["Mit", "sch\u00f6\u00b7nen", "weis\u00b7sen", "Ro\u00b7sen\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "li