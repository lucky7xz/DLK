{"textgrid.poem.52206": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen angenehmen Hund", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Galantel, welch ein Gl\u00fcck! hat seine Frau erfreuet,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "welch", "ein", "Gl\u00fcck", "!", "hat", "sei\u00b7ne", "Frau", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "$.", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Galantel, welchem sich kein Sirius vergleicht,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "wel\u00b7chem", "sich", "kein", "Si\u00b7rius", "ver\u00b7gleicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Galantel, der den Prei\u00df der Trefligkeit erreicht,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "der", "den", "Prei\u00df", "der", "Tre\u00b7flig\u00b7keit", "er\u00b7reicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Galantel, dem die Gunst der Sternen viel verleihet,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "dem", "die", "Gunst", "der", "Ster\u00b7nen", "viel", "ver\u00b7lei\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Galantel, den man izt mit Majoran bestreuet,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "den", "man", "izt", "mit", "Ma\u00b7jo\u00b7ran", "be\u00b7streu\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Galantel, der den Schwantz oft an den Teppicht streicht,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "der", "den", "Schwantz", "oft", "an", "den", "Tep\u00b7picht", "streicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Galantel, welcher nicht dem gr\u00f6sten Mopsus weicht,", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "wel\u00b7cher", "nicht", "dem", "gr\u00f6s\u00b7ten", "Mop\u00b7sus", "weicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Galantel, dessen Zahn ein geiler Buhler scheuet.", "tokens": ["Ga\u00b7lan\u00b7tel", ",", "des\u00b7sen", "Zahn", "ein", "gei\u00b7ler", "Buh\u00b7ler", "scheu\u00b7et", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo, Ph\u00f6be, werther Hund, die Augen auf dir hat,", "tokens": ["Wo", ",", "Ph\u00f6\u00b7be", ",", "wert\u00b7her", "Hund", ",", "die", "Au\u00b7gen", "auf", "dir", "hat", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kr\u00e4ncket deinen Fu\u00df kein schnelles Wagen-Rad,", "tokens": ["So", "kr\u00e4n\u00b7cket", "dei\u00b7nen", "Fu\u00df", "kein", "schnel\u00b7les", "Wa\u00b7gen\u00b7Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch h\u00fctte dich vor dem, den wir den Pluto heissen:", "tokens": ["Doch", "h\u00fct\u00b7te", "dich", "vor", "dem", ",", "den", "wir", "den", "Plu\u00b7to", "heis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du kommst, so sch\u00f6n du bist, den Katzen ziemlich bey,", "tokens": ["Du", "kommst", ",", "so", "sch\u00f6n", "du", "bist", ",", "den", "Kat\u00b7zen", "ziem\u00b7lich", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum m\u00f6chte dermaleinst dich in der Raserey", "tokens": ["Drum", "m\u00f6ch\u00b7te", "der\u00b7ma\u00b7leinst", "dich", "in", "der", "Ra\u00b7se\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "