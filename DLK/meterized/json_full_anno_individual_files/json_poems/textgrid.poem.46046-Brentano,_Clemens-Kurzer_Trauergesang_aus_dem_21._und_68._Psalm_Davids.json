{"textgrid.poem.46046": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kurzer Trauergesang aus dem 21. und 68. Psalm Davids", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Not, Angst, und Schmerzen ungest\u00fcm,", "tokens": ["Not", ",", "Angst", ",", "und", "Schmer\u00b7zen", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir stark einherdringen,", "tokens": ["Zu", "mir", "stark", "ein\u00b7her\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Umgeben mich rings um und um,", "tokens": ["Um\u00b7ge\u00b7ben", "mich", "rings", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit ihnen mu\u00df ich ringen.", "tokens": ["Mit", "ih\u00b7nen", "mu\u00df", "ich", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein treuer Herr, mein frommer Gott,", "tokens": ["Mein", "treu\u00b7er", "Herr", ",", "mein", "from\u00b7mer", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht wollest mich verlassen,", "tokens": ["Nicht", "wol\u00b7lest", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schau her auf mich, in meiner Not,", "tokens": ["Schau", "her", "auf", "mich", ",", "in", "mei\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mein Leid ist ohne Ma\u00dfen.", "tokens": ["Mein", "Leid", "ist", "oh\u00b7ne", "Ma\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich heule fast in schwerer Pein,", "tokens": ["Ich", "heu\u00b7le", "fast", "in", "schwe\u00b7rer", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Heil ist noch zu finden,", "tokens": ["Kein", "Heil", "ist", "noch", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Gott! la\u00df bald es anders sein.", "tokens": ["O", "Gott", "!", "la\u00df", "bald", "es", "an\u00b7ders", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "ADV", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00df nicht Deiner Kinder;", "tokens": ["Ver\u00b7gi\u00df", "nicht", "Dei\u00b7ner", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weich nicht von mir zu dieser Frist,", "tokens": ["Weich", "nicht", "von", "mir", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil Elend noch vorhanden,", "tokens": ["Weil", "E\u00b7lend", "noch", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn sonst ich keinen Helfer w\u00fc\u00dft',", "tokens": ["Denn", "sonst", "ich", "kei\u00b7nen", "Hel\u00b7fer", "w\u00fc\u00dft'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich w\u00fcrde ja zu Schanden.", "tokens": ["Ich", "w\u00fcr\u00b7de", "ja", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nimm mich mein Gott in Deine Hut,", "tokens": ["Nimm", "mich", "mein", "Gott", "in", "Dei\u00b7ne", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df H\u00fclf' vom Himmel kommen,", "tokens": ["La\u00df", "H\u00fcl\u00b7f'", "vom", "Him\u00b7mel", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich fa\u00dft die gro\u00dfe Wasserflut,", "tokens": ["Mich", "fa\u00dft", "die", "gro\u00b7\u00dfe", "Was\u00b7ser\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u00fcberhand genommen;", "tokens": ["Die", "\u00fc\u00b7ber\u00b7hand", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dr\u00e4ng', Herr, zu Dir nicht mein Geschrei,", "tokens": ["Dr\u00e4ng'", ",", "Herr", ",", "zu", "Dir", "nicht", "mein", "Ge\u00b7schrei", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im tiefen Schlamm versunken,", "tokens": ["Im", "tie\u00b7fen", "Schlamm", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "St\u00fcndst Du mir, treuer Gott, nicht bei,", "tokens": ["St\u00fcndst", "Du", "mir", ",", "treu\u00b7er", "Gott", ",", "nicht", "bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich w\u00e4re l\u00e4ngst ertrunken.", "tokens": ["Ich", "w\u00e4\u00b7re", "l\u00e4ngst", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von Rufen bin ich heiser sehr,", "tokens": ["Von", "Ru\u00b7fen", "bin", "ich", "hei\u00b7ser", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Gott, komm doch zur Stunde,", "tokens": ["O", "Gott", ",", "komm", "doch", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald, bald, ach bald! Kann ja nicht mehr;", "tokens": ["Bald", ",", "bald", ",", "ach", "bald", "!", "Kann", "ja", "nicht", "mehr", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ITJ", "ADV", "$.", "VMFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald, bald! ich geh' zu Grunde!", "tokens": ["Bald", ",", "bald", "!", "ich", "geh'", "zu", "Grun\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hilf, hilf, Du frommer Gott, der Tod", "tokens": ["Hilf", ",", "hilf", ",", "Du", "from\u00b7mer", "Gott", ",", "der", "Tod"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVIMP", "$,", "PPER", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schwebt schon mir auf der Zunge,", "tokens": ["Schwebt", "schon", "mir", "auf", "der", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Heil! o Helfer in der Not,", "tokens": ["O", "Heil", "!", "o", "Hel\u00b7fer", "in", "der", "Not", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du nahst in schnellem Sprunge.", "tokens": ["Du", "nahst", "in", "schnel\u00b7lem", "Sprun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und rettest mich mit Deiner Hand,", "tokens": ["Und", "ret\u00b7test", "mich", "mit", "Dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erh\u00e4ltst mich in dem Leben,", "tokens": ["Er\u00b7h\u00e4ltst", "mich", "in", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von oben her ich H\u00fclfe fand,", "tokens": ["Von", "o\u00b7ben", "her", "ich", "H\u00fcl\u00b7fe", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann nun in Hoffnung schweben.", "tokens": ["Kann", "nun", "in", "Hoff\u00b7nung", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Gott, wer sich auf Dich verl\u00e4\u00dft,", "tokens": ["O", "Gott", ",", "wer", "sich", "auf", "Dich", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Sturm naht so geschwinde,", "tokens": ["Kein", "Sturm", "naht", "so", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df, wer bei Dir verharret fest,", "tokens": ["Da\u00df", ",", "wer", "bei", "Dir", "ver\u00b7har\u00b7ret", "fest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "APPR", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht endlich \u00fcberwinde!", "tokens": ["Nicht", "end\u00b7lich", "\u00fc\u00b7berw\u00b7in\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Not, Angst, und Schmerzen ungest\u00fcm,", "tokens": ["Not", ",", "Angst", ",", "und", "Schmer\u00b7zen", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu mir stark einherdringen,", "tokens": ["Zu", "mir", "stark", "ein\u00b7her\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Umgeben mich rings um und um,", "tokens": ["Um\u00b7ge\u00b7ben", "mich", "rings", "um", "und", "um", ","], "token_info":