{"dta.poem.2561": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Ring  \n  Bey Hn. T. S. R.   J. U. C.   u. Jfr. S. M. v.  \n S. Hochzeit abgebildet 23. Jan. 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcnsche mir ietzt Glut- und Flammen-reiche Zeilen/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "ietzt", "Glut", "und", "Flam\u00b7men\u00b7rei\u00b7che", "Zei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "TRUNC", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wer von Liebe schreibt und nicht die Feder", "tokens": ["Denn", "wer", "von", "Lie\u00b7be", "schreibt", "und", "nicht", "die", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird die ", "tokens": ["Wird", "die"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und hat den rechten Zweck der Anmuth nicht erkennt.", "tokens": ["Und", "hat", "den", "rech\u00b7ten", "Zweck", "der", "An\u00b7muth", "nicht", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Reden m\u00fcssen hier den sch\u00f6nsten Rosen gleichen/", "tokens": ["Die", "Re\u00b7den", "m\u00fcs\u00b7sen", "hier", "den", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In die der liebe West den s\u00fcssen Athem haucht/", "tokens": ["In", "die", "der", "lie\u00b7be", "West", "den", "s\u00fcs\u00b7sen", "A\u00b7them", "haucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es mu\u00df jedwedes Wort die Venus \u00fcberstreichen", "tokens": ["Es", "mu\u00df", "jed\u00b7we\u00b7des", "Wort", "die", "Ve\u00b7nus", "\u00fc\u00b7bers\u00b7trei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit Balsam/ den sie sonst zu ihrer Zierrath braucht.", "tokens": ["Mit", "Bal\u00b7sam", "/", "den", "sie", "sonst", "zu", "ih\u00b7rer", "Zier\u00b7rath", "braucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "ART", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So hat Ovidius voll Seel und Geist geschrieben/", "tokens": ["So", "hat", "O\u00b7vi\u00b7dius", "voll", "Seel", "und", "Geist", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADJD", "NN", "KON", "NN", "VVPP", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Wenn er dem klugen Rom die Kunst zu lieben wies/", "tokens": ["Wenn", "er", "dem", "klu\u00b7gen", "Rom", "die", "Kunst", "zu", "lie\u00b7ben", "wies", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NE", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der liebliche Tibull ist auf der Spur geblieben/", "tokens": ["Der", "lieb\u00b7li\u00b7che", "Ti\u00b7bull", "ist", "auf", "der", "Spur", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn er die Cynthia sein ander Leben hie\u00df.", "tokens": ["Wenn", "er", "die", "Cyn\u00b7thia", "sein", "an\u00b7der", "Le\u00b7ben", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Es kan ihr sch\u00f6nes Buch die Hertzen noch entz\u00fcnden/", "tokens": ["Es", "kan", "ihr", "sch\u00f6\u00b7nes", "Buch", "die", "Hert\u00b7zen", "noch", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr reitzender Gesang bezaubert Sinn und Muth;", "tokens": ["Ihr", "reit\u00b7zen\u00b7der", "Ge\u00b7sang", "be\u00b7zau\u00b7bert", "Sinn", "und", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So da\u00df der Kern der Lust bey ihnen ist zu finden", "tokens": ["So", "da\u00df", "der", "Kern", "der", "Lust", "bey", "ih\u00b7nen", "ist", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "NN", "APPR", "PPER", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ihre gantze Schrifft ein Aetna voller Glut.", "tokens": ["Und", "ih\u00b7re", "gant\u00b7ze", "Schrifft", "ein", "A\u00b7et\u00b7na", "vol\u00b7ler", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "Mir fehlt dergleichen Art und Zunder solcher Dinge/", "tokens": ["Mir", "fehlt", "derg\u00b7lei\u00b7chen", "Art", "und", "Zun\u00b7der", "sol\u00b7cher", "Din\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "KON", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es kleidet meinen Reim kein angenehme Zier;", "tokens": ["Es", "klei\u00b7det", "mei\u00b7nen", "Reim", "kein", "an\u00b7ge\u00b7neh\u00b7me", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch treu Verliebte Zwey ich stell in einem Ringe", "tokens": ["Doch", "treu", "Ver\u00b7lieb\u00b7te", "Zwey", "ich", "stell", "in", "ei\u00b7nem", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "CARD", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Liebe theurem Pfand/ euch eure Flammen f\u00fcr.", "tokens": ["Der", "Lie\u00b7be", "theu\u00b7rem", "Pfand", "/", "euch", "eu\u00b7re", "Flam\u00b7men", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "PPER", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist ein g\u00fcldner Ring verdienter W\u00fcrde Zeichen.", "tokens": ["Es", "ist", "ein", "g\u00fcld\u00b7ner", "Ring", "ver\u00b7dien\u00b7ter", "W\u00fcr\u00b7de", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es hat die alte Welt Gesandten mit geehrt/", "tokens": ["Es", "hat", "die", "al\u00b7te", "Welt", "Ge\u00b7sand\u00b7ten", "mit", "ge\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und R\u00e4then/ welche nie von ihrem F\u00fcrsten weichen/", "tokens": ["Und", "R\u00e4\u00b7then", "/", "wel\u00b7che", "nie", "von", "ih\u00b7rem", "F\u00fcrs\u00b7ten", "wei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hat vor erwie\u00dfne Treu ein g\u00fcldner Ring geh\u00f6rt.", "tokens": ["Hat", "vor", "er\u00b7wie\u00df\u00b7ne", "Treu", "ein", "g\u00fcld\u00b7ner", "Ring", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der zieret eure Lieb\u2019 und eurer ", "tokens": ["Der", "zie\u00b7ret", "eu\u00b7re", "Lieb'", "und", "eu\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wer kennet sch\u00f6nste Braut den werthen ", "tokens": ["Wer", "ken\u00b7net", "sch\u00f6ns\u00b7te", "Braut", "den", "wert\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "ART", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Es r\u00fchmet seine Treu/ F\u00fcrst/ F\u00fcrstenthum und St\u00e4nde/", "tokens": ["Es", "r\u00fch\u00b7met", "sei\u00b7ne", "Treu", "/", "F\u00fcrst", "/", "F\u00fcrs\u00b7ten\u00b7thum", "und", "St\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und nennet ihn mit recht/ der Rechte Trost und Licht.", "tokens": ["Und", "nen\u00b7net", "ihn", "mit", "recht", "/", "der", "Rech\u00b7te", "Trost", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJD", "$(", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sol was verb\u00fcndlich seyn in Menschlichen Gesch\u00e4fften/", "tokens": ["Sol", "was", "ver\u00b7b\u00fcnd\u00b7lich", "seyn", "in", "Menschli\u00b7chen", "Ge\u00b7sch\u00e4ff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "So mu\u00df der Siegel-Ring der beste Zeuge seyn.", "tokens": ["So", "mu\u00df", "der", "Sie\u00b7gel\u00b7Ring", "der", "bes\u00b7te", "Zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da eure Hertzen sich jetzt fest zusammen hefften/", "tokens": ["Da", "eu\u00b7re", "Hert\u00b7zen", "sich", "jetzt", "fest", "zu\u00b7sam\u00b7men", "heff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So r\u00e4umt ihr ja dem Ring das erste Vortheil ein.", "tokens": ["So", "r\u00e4umt", "ihr", "ja", "dem", "Ring", "das", "ers\u00b7te", "Vor\u00b7theil", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ja es vollzieht ein Ring den erst und letzten Willen/", "tokens": ["Ja", "es", "voll\u00b7zieht", "ein", "Ring", "den", "erst", "und", "letz\u00b7ten", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "NN", "ART", "ADV", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wir brauchen ihn zur Eh\u2019 und wieder in dem Todt/", "tokens": ["Wir", "brau\u00b7chen", "ihn", "zur", "Eh'", "und", "wie\u00b7der", "in", "dem", "Todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KON", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn als wir noch das Grab mit unserm Leibe f\u00fcllen/", "tokens": ["Denn", "als", "wir", "noch", "das", "Grab", "mit", "un\u00b7serm", "Lei\u00b7be", "f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bezeugt ein Testament des Sterbenden Geboth.", "tokens": ["Be\u00b7zeugt", "ein", "Tes\u00b7ta\u00b7ment", "des", "Ster\u00b7ben\u00b7den", "Ge\u00b7both", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "De\u00df Ringes Ubergab erkl\u00e4rt den wahren Erben:", "tokens": ["De\u00df", "Rin\u00b7ges", "U\u00b7berg\u00b7ab", "er\u00b7kl\u00e4rt", "den", "wah\u00b7ren", "Er\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Lie\u00df Alexander nicht sein weites Reich so stehn/", "tokens": ["Lie\u00df", "A\u00b7lex\u00b7an\u00b7der", "nicht", "sein", "wei\u00b7tes", "Reich", "so", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKNEG", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wer vom grossen Carl kont\u2019 ein Abtey erwerben/", "tokens": ["Und", "wer", "vom", "gros\u00b7sen", "Carl", "kont'", "ein", "Ab\u00b7tey", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NE", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Der mochte von ihm weg im Ring und Stabe gehn.", "tokens": ["Der", "moch\u00b7te", "von", "ihm", "weg", "im", "Ring", "und", "Sta\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja was mehr Wunderns werth/ des Meeres wilde Wellen/", "tokens": ["Ja", "was", "mehr", "Wun\u00b7derns", "werth", "/", "des", "Mee\u00b7res", "wil\u00b7de", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "PIAT", "NN", "ADJD", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die hat Venedig sich durch einen Ring verm\u00e4hlt.", "tokens": ["Die", "hat", "Ve\u00b7ne\u00b7dig", "sich", "durch", "ei\u00b7nen", "Ring", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.43":