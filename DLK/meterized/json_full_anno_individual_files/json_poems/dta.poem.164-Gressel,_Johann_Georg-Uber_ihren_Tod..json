{"dta.poem.164": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihren Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Augen flie\u00dft! la\u00dft h\u00e4uffig Thr\u00e4nen quillen/", "tokens": ["Ihr", "Au\u00b7gen", "flie\u00dft", "!", "la\u00dft", "h\u00e4uf\u00b7fig", "Thr\u00e4\u00b7nen", "quil\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Brunnen zin\u00dft! die T\u00f6chter herber Pein/", "tokens": ["Als", "Brun\u00b7nen", "zin\u00dft", "!", "die", "T\u00f6ch\u00b7ter", "her\u00b7ber", "Pein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$.", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Z\u00e4hren-Saltz! la\u00df heisse Perlen seyn/", "tokens": ["Der", "Z\u00e4h\u00b7ren\u00b7Saltz", "!", "la\u00df", "heis\u00b7se", "Per\u00b7len", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVIMP", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Flie\u00dft Augen flie\u00dft! nichts mu\u00df die Thr\u00e4nen stillen/", "tokens": ["Flie\u00dft", "Au\u00b7gen", "flie\u00dft", "!", "nichts", "mu\u00df", "die", "Thr\u00e4\u00b7nen", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$.", "PIS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Beweint den Tod der lieblichen ", "tokens": ["Be\u00b7weint", "den", "Tod", "der", "lieb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stellt alle Lust/ und alle Freude ein/", "tokens": ["Stellt", "al\u00b7le", "Lust", "/", "und", "al\u00b7le", "Freu\u00b7de", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "KON", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Himmel selbst verdeckt den hellen Schein/", "tokens": ["Der", "Him\u00b7mel", "selbst", "ver\u00b7deckt", "den", "hel\u00b7len", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und will in Flor sein sch\u00f6nes Haupt verh\u00fcllen.", "tokens": ["Und", "will", "in", "Flor", "sein", "sch\u00f6\u00b7nes", "Haupt", "ver\u00b7h\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich bin halb todt/ vor Schmertzen sterbe ich/", "tokens": ["Ich", "bin", "halb", "todt", "/", "vor", "Schmert\u00b7zen", "ster\u00b7be", "ich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$(", "APPR", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mein Hertze ist mit ihren Leib begraben/", "tokens": ["Mein", "Hert\u00b7ze", "ist", "mit", "ih\u00b7ren", "Leib", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nichts als nur Leid und Qu\u00e4hlen k\u00fcsset mich", "tokens": ["Nichts", "als", "nur", "Leid", "und", "Qu\u00e4h\u00b7len", "k\u00fcs\u00b7set", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ADV", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mit Jammer mu\u00df ich meine Geister laben:", "tokens": ["Mit", "Jam\u00b7mer", "mu\u00df", "ich", "mei\u00b7ne", "Geis\u00b7ter", "la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Denn alle Lust ist aus der Welt entwichen", "tokens": ["Denn", "al\u00b7le", "Lust", "ist", "aus", "der", "Welt", "ent\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Nachdem mein Licht ", "tokens": ["Nach\u00b7dem", "mein", "Licht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}}}}}