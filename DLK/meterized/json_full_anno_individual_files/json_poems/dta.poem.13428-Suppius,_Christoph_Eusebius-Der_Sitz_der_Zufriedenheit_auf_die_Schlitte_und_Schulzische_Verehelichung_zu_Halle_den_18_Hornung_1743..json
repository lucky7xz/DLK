{"dta.poem.13428": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Sitz der Zufriedenheit  \n auf die  \n  Schlitte  und  Schulzische  \n Verehelichung  \n zu Halle den 18 Hornung 1743.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Von hochbejahrten rauhen H\u00fcgeln", "tokens": ["Von", "hoch\u00b7be\u00b7jahr\u00b7ten", "rau\u00b7hen", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt itzt ein wohlgeneigter West,", "tokens": ["Bringt", "itzt", "ein", "wohl\u00b7ge\u00b7neig\u00b7ter", "West", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit annoch halbbethauten Fl\u00fcgeln,", "tokens": ["Mit", "an\u00b7noch", "halb\u00b7be\u00b7thau\u00b7ten", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df Blatt auf euer Hochzeitfest;", "tokens": ["Die\u00df", "Blatt", "auf", "eu\u00b7er", "Hoch\u00b7zeit\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dafern in seinem Elemente", "tokens": ["Da\u00b7fern", "in", "sei\u00b7nem", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein schwerer K\u00f6rper schwimmen k\u00f6nnte,", "tokens": ["Ein", "schwe\u00b7rer", "K\u00f6r\u00b7per", "schwim\u00b7men", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Lopez gl\u00fccklicher gedacht,", "tokens": ["Und", "Lo\u00b7pez", "gl\u00fcck\u00b7li\u00b7cher", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bey der erfundnen Monden-Reise,", "tokens": ["Bey", "der", "er\u00b7fund\u00b7nen", "Mon\u00b7den\u00b7Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So glaubt, da\u00df ich auf gleiche Weise", "tokens": ["So", "glaubt", ",", "da\u00df", "ich", "auf", "glei\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mich selbsten auf den Weg gemacht.", "tokens": ["Mich", "selbs\u00b7ten", "auf", "den", "Weg", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So bleib ich, wo ein m\u00e4ssig Gl\u00fccke", "tokens": ["So", "bleib", "ich", ",", "wo", "ein", "m\u00e4s\u00b7sig", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir noch den eignen Wirbel dreht,", "tokens": ["Mir", "noch", "den", "eig\u00b7nen", "Wir\u00b7bel", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4chle zu dem frohen Blicke,", "tokens": ["Und", "l\u00e4ch\u00b7le", "zu", "dem", "fro\u00b7hen", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ostw\u00e4rts qverfeld\u00fcber geht;", "tokens": ["Der", "ost\u00b7w\u00e4rts", "qver\u00b7fel\u00b7d\u00fc\u00b7ber", "geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er trifft euch, nun verbundne Beyde!", "tokens": ["Er", "trifft", "euch", ",", "nun", "ver\u00b7bund\u00b7ne", "Bey\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In eurer reifgewordnen Freude,", "tokens": ["In", "eu\u00b7rer", "reif\u00b7ge\u00b7word\u00b7nen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit W\u00fcnschen ganz umringet an,", "tokens": ["Mit", "W\u00fcn\u00b7schen", "ganz", "um\u00b7rin\u00b7get", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis da\u00df er endlich im Gedr\u00e4nge", "tokens": ["Bis", "da\u00df", "er", "end\u00b7lich", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bey solcher angenehmen Menge", "tokens": ["Bey", "sol\u00b7cher", "an\u00b7ge\u00b7neh\u00b7men", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich ruhig niederlassen kann.", "tokens": ["Sich", "ru\u00b7hig", "nie\u00b7der\u00b7las\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Schlitte! weil an diesem Heute", "tokens": ["O", "Schlit\u00b7te", "!", "weil", "an", "die\u00b7sem", "Heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dein Wunsch erlangt, was noch gefehlt,", "tokens": ["Dein", "Wunsch", "er\u00b7langt", ",", "was", "noch", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das macht so viel vergn\u00fcgte Leute,", "tokens": ["Das", "macht", "so", "viel", "ver\u00b7gn\u00fcg\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als solcher wahre Freunde z\u00e4hlt;", "tokens": ["Als", "sol\u00b7cher", "wah\u00b7re", "Freun\u00b7de", "z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des H\u00f6chsten wunderbares Leiten", "tokens": ["Des", "H\u00f6chs\u00b7ten", "wun\u00b7der\u00b7ba\u00b7res", "Lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bestellte Dich vor wenig Zeiten", "tokens": ["Be\u00b7stell\u00b7te", "Dich", "vor", "we\u00b7nig", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zum W\u00e4chter bey des Vaters Huth,", "tokens": ["Zum", "W\u00e4ch\u00b7ter", "bey", "des", "Va\u00b7ters", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun l\u00e4sset es Dich noch erfahren,", "tokens": ["Nun", "l\u00e4s\u00b7set", "es", "Dich", "noch", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Was bey beschwerten Lebensjahren", "tokens": ["Was", "bey", "be\u00b7schwer\u00b7ten", "Le\u00b7bens\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein recht zufriednes Herze thut.", "tokens": ["Ein", "recht", "zu\u00b7fried\u00b7nes", "Her\u00b7ze", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In deiner Liebsten Angesichte", "tokens": ["In", "dei\u00b7ner", "Liebs\u00b7ten", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da l\u00e4chelt Dir Zufriedenheit,", "tokens": ["Da", "l\u00e4\u00b7chelt", "Dir", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allwo bey jugendlichem Lichte", "tokens": ["All\u00b7wo", "bey", "ju\u00b7gend\u00b7li\u00b7chem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bescheiden spielt die Sittsamkeit;", "tokens": ["Be\u00b7schei\u00b7den", "spielt", "die", "Sitt\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In diesem Spiegel frommer Jugend", "tokens": ["In", "die\u00b7sem", "Spie\u00b7gel", "from\u00b7mer", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erblickest Du nur reine Tugend,", "tokens": ["Er\u00b7bli\u00b7ckest", "Du", "nur", "rei\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein redlich Wesen ohne Schein,", "tokens": ["Ein", "red\u00b7lich", "We\u00b7sen", "oh\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Willst Du von ihren Tagewerken", "tokens": ["Willst", "Du", "von", "ih\u00b7ren", "Ta\u00b7ge\u00b7wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Noch ein poetisch Gleichni\u00df merken,", "tokens": ["Noch", "ein", "po\u00b7e\u00b7tisch", "Gleich\u00b7ni\u00df", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So bilde Dir die Bienen ein.", "tokens": ["So", "bil\u00b7de", "Dir", "die", "Bie\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist Sie werth, und ihre Gaben", "tokens": ["Du", "bist", "Sie", "werth", ",", "und", "ih\u00b7re", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind ebenfalls druch Dich belohnt,", "tokens": ["Sind", "e\u00b7ben\u00b7falls", "druch", "Dich", "be\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort soll sie Dich zur Seiten haben,", "tokens": ["Dort", "soll", "sie", "Dich", "zur", "Sei\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo eine Welt voll Freundschaft wohnt;", "tokens": ["Wo", "ei\u00b7ne", "Welt", "voll", "Freund\u00b7schaft", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch kann in dem gepriesnen Peussen", "tokens": ["Euch", "kann", "in", "dem", "ge\u00b7pri\u00b7es\u00b7nen", "Peus\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Erdenkreis ganz eigen heissen,", "tokens": ["Der", "Er\u00b7den\u00b7kreis", "ganz", "ei\u00b7gen", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da seyd Jhr reich bey frohem Muth,", "tokens": ["Da", "seyd", "Ihr", "reich", "bey", "fro\u00b7hem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da k\u00f6nnet ihr von bunten H\u00f6hen", "tokens": ["Da", "k\u00f6n\u00b7net", "ihr", "von", "bun\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So manche sch\u00f6ne Gegend sehen,", "tokens": ["So", "man\u00b7che", "sch\u00f6\u00b7ne", "Ge\u00b7gend", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Frey von des w\u00fcsten W\u00fcrgers Wuth.", "tokens": ["Frey", "von", "des", "w\u00fcs\u00b7ten", "W\u00fcr\u00b7gers", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier lacht man hinter Thurm und Mauren", "tokens": ["Hier", "lacht", "man", "hin\u00b7ter", "Thurm", "und", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey manchem Sturm emp\u00f6rter Welt;", "tokens": ["Bey", "man\u00b7chem", "Sturm", "em\u00b7p\u00f6r\u00b7ter", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier l\u00e4sset sich im Trocknen lauren,", "tokens": ["Hier", "l\u00e4s\u00b7set", "sich", "im", "Trock\u00b7nen", "lau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann Donner, Blitz und Regen f\u00e4llt.", "tokens": ["Wann", "Don\u00b7ner", ",", "Blitz", "und", "Re\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sterblichen verwirrtes Rennen", "tokens": ["Der", "Sterb\u00b7li\u00b7chen", "ver\u00b7wirr\u00b7tes", "Ren\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist hier durchs Gitter zu erkennen,", "tokens": ["Ist", "hier", "durchs", "Git\u00b7ter", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"],