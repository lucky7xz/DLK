{"dta.poem.22433": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "237. GOtt wil vollkommne haben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Entwachse dir mein Kind: wiltu zu GOtt hinein:", "tokens": ["Ent\u00b7wach\u00b7se", "dir", "mein", "Kind", ":", "wil\u00b7tu", "zu", "Gott", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$.", "VMFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mustu vor ein Mann vollkomnes Alters sein.", "tokens": ["So", "mus\u00b7tu", "vor", "ein", "Mann", "voll\u00b7kom\u00b7nes", "Al\u00b7ters", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}