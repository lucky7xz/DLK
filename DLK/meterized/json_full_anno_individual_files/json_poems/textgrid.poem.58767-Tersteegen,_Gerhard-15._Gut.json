{"textgrid.poem.58767": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "15. Gut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, Gott, bist gut, machst alles gut,", "tokens": ["Du", ",", "Gott", ",", "bist", "gut", ",", "machst", "al\u00b7les", "gut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du tust uns Gut's. Wer's hat, wer's tut,", "tokens": ["Du", "tust", "uns", "Gut'", "s.", "Wer's", "hat", ",", "wer's", "tut", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "NE", "NE", "VAFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hat's aus dir, dem Gutheitsbrunnen.", "tokens": ["Der", "hat's", "aus", "dir", ",", "dem", "Gut\u00b7heits\u00b7brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Brunn quillt stets f\u00fcr jedermann;", "tokens": ["Dein", "Brunn", "quillt", "stets", "f\u00fcr", "je\u00b7der\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer glaubt's, wie nah man's haben kann?", "tokens": ["Wer", "glaubt's", ",", "wie", "nah", "man's", "ha\u00b7ben", "kann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mensch, kehr dich doch zur Gutheitssonnen!", "tokens": ["Mensch", ",", "kehr", "dich", "doch", "zur", "Gut\u00b7heits\u00b7son\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es taugt nicht in noch au\u00dfer mir;", "tokens": ["Es", "taugt", "nicht", "in", "noch", "au\u00b7\u00dfer", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hab's nur gut, ", "tokens": ["Ich", "hab's", "nur", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Du, Gott, bist gut, machst alles gut,", "tokens": ["Du", ",", "Gott", ",", "bist", "gut", ",", "machst", "al\u00b7les", "gut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du tust uns Gut's. Wer's hat, wer's tut,", "tokens": ["Du", "tust", "uns", "Gut'", "s.", "Wer's", "hat", ",", "wer's", "tut", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "