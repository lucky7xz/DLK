{"textgrid.poem.36424": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den F\u00fcrsten von Metternich", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df ein heitres Licht den Geist umschweben,", "tokens": ["La\u00df", "ein", "heit\u00b7res", "Licht", "den", "Geist", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anmut schm\u00fcckend Dir zur Seite gehn;", "tokens": ["An\u00b7mut", "schm\u00fc\u00b7ckend", "Dir", "zur", "Sei\u00b7te", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zarte Sitte in dem neuen Leben", "tokens": ["Zar\u00b7te", "Sit\u00b7te", "in", "dem", "neu\u00b7en", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frohe Stunden durch die Tage weben;", "tokens": ["Fro\u00b7he", "Stun\u00b7den", "durch", "die", "Ta\u00b7ge", "we\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bl\u00fchend soll Dein Gl\u00fcck im Glanze stehn.", "tokens": ["Bl\u00fc\u00b7hend", "soll", "Dein", "Gl\u00fcck", "im", "Glan\u00b7ze", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aus der Ruhe gehen starke Werke,", "tokens": ["Aus", "der", "Ru\u00b7he", "ge\u00b7hen", "star\u00b7ke", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Gl\u00fcck die volle Kraft hervor.", "tokens": ["Aus", "dem", "Gl\u00fcck", "die", "vol\u00b7le", "Kraft", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Es bedarf die Zeit der sichern St\u00e4rke,", "tokens": ["Es", "be\u00b7darf", "die", "Zeit", "der", "si\u00b7chern", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ergriffen es die Welt bemerke,", "tokens": ["Da\u00df", "er\u00b7grif\u00b7fen", "es", "die", "Welt", "be\u00b7mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wie ein neuer Schwung sie hebt empor.", "tokens": ["Wie", "ein", "neu\u00b7er", "Schwung", "sie", "hebt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie der Sinn um jedes Neue schwirre", "tokens": ["Wie", "der", "Sinn", "um", "je\u00b7des", "Neu\u00b7e", "schwir\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der m\u00fc\u00df'gen Rede eitlen Lust;", "tokens": ["In", "der", "m\u00fc\u00df'\u00b7gen", "Re\u00b7de", "eit\u00b7len", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nichtig bleibt der Geist in dem Gewirre,", "tokens": ["Nich\u00b7tig", "bleibt", "der", "Geist", "in", "dem", "Ge\u00b7wir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hundertf\u00e4ltig geht er in die Irre;", "tokens": ["Hun\u00b7dert\u00b7f\u00e4l\u00b7tig", "geht", "er", "in", "die", "Ir\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Innrer Friede macht erst fest die Brust.", "tokens": ["Inn\u00b7rer", "Frie\u00b7de", "macht", "erst", "fest", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dem Insektenschwarm zum Angebinde", "tokens": ["Dem", "In\u00b7sek\u00b7ten\u00b7schwarm", "zum", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lassen wir den kleinen Geisterstreit.", "tokens": ["Las\u00b7sen", "wir", "den", "klei\u00b7nen", "Geis\u00b7ter\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schlangen aber bilden ein Gewinde", "tokens": ["Schlan\u00b7gen", "a\u00b7ber", "bil\u00b7den", "ein", "Ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Giftig um die Welt, da\u00df sie erblinde,", "tokens": ["Gif\u00b7tig", "um", "die", "Welt", ",", "da\u00df", "sie", "er\u00b7blin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "All durchschlingend diese kranke Zeit.", "tokens": ["All", "durch\u00b7schlin\u00b7gend", "die\u00b7se", "kran\u00b7ke", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Die Zerst\u00f6rung droht aus finstern Gr\u00fcften,", "tokens": ["Die", "Zer\u00b7st\u00f6\u00b7rung", "droht", "aus", "fins\u00b7tern", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach dem Raube blickt sie gierig aus.", "tokens": ["Nach", "dem", "Rau\u00b7be", "blickt", "sie", "gie\u00b7rig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Blitze zucken flammend aus den L\u00fcften,", "tokens": ["Blit\u00b7ze", "zu\u00b7cken", "flam\u00b7mend", "aus", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Donnernd bebt die Erd' in Felsenkl\u00fcften,", "tokens": ["Don\u00b7nernd", "bebt", "die", "Erd'", "in", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und es schwankt das stille Friedenshaus.", "tokens": ["Und", "es", "schwankt", "das", "stil\u00b7le", "Frie\u00b7dens\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Neugef\u00fcgt von ewigem Gesteine", "tokens": ["Neu\u00b7ge\u00b7f\u00fcgt", "von", "e\u00b7wi\u00b7gem", "Ge\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sei also der feste Staatengrund.", "tokens": ["Sei", "al\u00b7so", "der", "fes\u00b7te", "Staa\u00b7ten\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf dem Gott geschlossenem Vereine", "tokens": ["Auf", "dem", "Gott", "ge\u00b7schlos\u00b7se\u00b7nem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruht die gro\u00dfe Friedensburg und keine", "tokens": ["Ruht", "die", "gro\u00b7\u00dfe", "Frie\u00b7dens\u00b7burg", "und", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00f6llenmacht ersch\u00fcttert diesen Bund.", "tokens": ["H\u00f6l\u00b7len\u00b7macht", "er\u00b7sch\u00fct\u00b7tert", "die\u00b7sen", "Bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wo nicht Gott die Mauern schirmend baute,", "tokens": ["Wo", "nicht", "Gott", "die", "Mau\u00b7ern", "schir\u00b7mend", "bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dient das Haus dem Zeitenwind als Spreu.", "tokens": ["Dient", "das", "Haus", "dem", "Zei\u00b7ten\u00b7wind", "als", "Spreu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn die Welt dem L\u00fcgner wieder traute,", "tokens": ["Wenn", "die", "Welt", "dem", "L\u00fcg\u00b7ner", "wie\u00b7der", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "R\u00fcckw\u00e4rts nach dem alten Babel schaute;", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "nach", "dem", "al\u00b7ten", "Ba\u00b7bel", "schau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kommt Ein Hauch und wendet alles neu.", "tokens": ["Kommt", "Ein", "Hauch", "und", "wen\u00b7det", "al\u00b7les", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "In dem Denken, Meinen, Wollen, Streiten", "tokens": ["In", "dem", "Den\u00b7ken", ",", "Mei\u00b7nen", ",", "Wol\u00b7len", ",", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "VVFIN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist des Unheils Quelle das Zuviel.", "tokens": ["Ist", "des", "Un\u00b7heils", "Quel\u00b7le", "das", "Zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieses mu\u00df die feste Hand bestreiten,", "tokens": ["Die\u00b7ses", "mu\u00df", "die", "fes\u00b7te", "Hand", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df nicht alle Kr\u00e4fte \u00fcberschreiten;", "tokens": ["Da\u00df", "nicht", "al\u00b7le", "Kr\u00e4f\u00b7te", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nur der gro\u00dfe Blick trifft ganz das Ziel.", "tokens": ["Nur", "der", "gro\u00b7\u00dfe", "Blick", "trifft", "ganz", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Meister an dem Steuer in der Wendung", "tokens": ["Meis\u00b7ter", "an", "dem", "Steu\u00b7er", "in", "der", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fa\u00dftest Du sogleich den Augenblick.", "tokens": ["Fa\u00df\u00b7test", "Du", "sog\u00b7leich", "den", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In dem Wechsel liegt der Kunst Vollendung,", "tokens": ["In", "dem", "Wech\u00b7sel", "liegt", "der", "Kunst", "Vol\u00b7len\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem Punkt beruht das Ziel der Sendung;", "tokens": ["Auf", "dem", "Punkt", "be\u00b7ruht", "das", "Ziel", "der", "Sen\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schnell kehrt solch ein Wendepunkt zur\u00fcck.", "tokens": ["Schnell", "kehrt", "solch", "ein", "Wen\u00b7de\u00b7punkt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Also durch die labyrinth'schen G\u00e4nge", "tokens": ["Al\u00b7so", "durch", "die", "la\u00b7by\u00b7rinth'\u00b7schen", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dienst Du sorgend dem gesalbten Haupt,", "tokens": ["Dienst", "Du", "sor\u00b7gend", "dem", "ge\u00b7salb\u00b7ten", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Den als Vater ehrt die treue Menge;", "tokens": ["Den", "als", "Va\u00b7ter", "ehrt", "die", "treu\u00b7e", "Men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gott verlie\u00df ihn nie in dem Gedr\u00e4nge,", "tokens": ["Gott", "ver\u00b7lie\u00df", "ihn", "nie", "in", "dem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und Europa hat an ihn geglaubt.", "tokens": ["Und", "Eu\u00b7ro\u00b7pa", "hat", "an", "ihn", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Was das ew'ge Licht und Recht begr\u00fcndet,", "tokens": ["Was", "das", "ew'\u00b7ge", "Licht", "und", "Recht", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "KON", "NN", "VVP