{"dta.poem.19932": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Ludwig Uhland .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Treibt auch f\u00fcr jezt der Menschen Treiben", "tokens": ["Treibt", "auch", "f\u00fcr", "jezt", "der", "Men\u00b7schen", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich dahin und dich dort hinaus,", "tokens": ["Mich", "da\u00b7hin", "und", "dich", "dort", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "KON", "PPER", "ADV", "APZR", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mu\u00df ich doch immer bey dir bleiben,", "tokens": ["Mu\u00df", "ich", "doch", "im\u00b7mer", "bey", "dir", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ja dein Herz schon lang mein Haus.", "tokens": ["Ist", "ja", "dein", "Herz", "schon", "lang", "mein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So kommt es, da\u00df in jeden N\u00e4chten", "tokens": ["So", "kommt", "es", ",", "da\u00df", "in", "je\u00b7den", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich jezt in Tr\u00e4umen bin bey dir,", "tokens": ["Ich", "jezt", "in", "Tr\u00e4u\u00b7men", "bin", "bey", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht \u00fcber Rechte wir da rechten,", "tokens": ["Nicht", "\u00fc\u00b7ber", "Rech\u00b7te", "wir", "da", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lenz und Liedern sprechen wir.", "tokens": ["Von", "Lenz", "und", "Lie\u00b7dern", "spre\u00b7chen", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da liegt kein Rechtsbuch aufgeschlagen,", "tokens": ["Da", "liegt", "kein", "Rechts\u00b7buch", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Zeitungsblatt auf deinem Tisch,", "tokens": ["Kein", "Zei\u00b7tungs\u00b7blatt", "auf", "dei\u00b7nem", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Heldenspiele, bunte Sagen,", "tokens": ["Doch", "Hel\u00b7den\u00b7spie\u00b7le", ",", "bun\u00b7te", "Sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und deine Lieder hold und frisch.", "tokens": ["Und", "dei\u00b7ne", "Lie\u00b7der", "hold", "und", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hell dein Buch von Freundestreue", "tokens": ["Und", "hell", "dein", "Buch", "von", "Freun\u00b7de\u00b7streu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Ernst, den keine Zeit verweht,", "tokens": ["Dein", "Ernst", ",", "den", "kei\u00b7ne", "Zeit", "ver\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wird mir alles wieder neue,", "tokens": ["Da", "wird", "mir", "al\u00b7les", "wie\u00b7der", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df der sch\u00f6ne Traum vergeht.", "tokens": ["Bis", "da\u00df", "der", "sch\u00f6\u00b7ne", "Traum", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Treibt dann der Menschen Treiben wieder", "tokens": ["Treibt", "dann", "der", "Men\u00b7schen", "Trei\u00b7ben", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich dahin und dich dort hinaus,", "tokens": ["Mich", "da\u00b7hin", "und", "dich", "dort", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "KON", "PPER", "ADV", "APZR", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So rufen fern mir deine Lieder:", "tokens": ["So", "ru\u00b7fen", "fern", "mir", "dei\u00b7ne", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Und wie so oft in Sommertagen", "tokens": ["Und", "wie", "so", "oft", "in", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rebe wieder Bl\u00fcthen tr\u00e4gt,", "tokens": ["Die", "Re\u00b7be", "wie\u00b7der", "Bl\u00fc\u00b7then", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Derselbe Wein, den sie getragen,", "tokens": ["Der\u00b7sel\u00b7be", "Wein", ",", "den", "sie", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehns\u00fcchtig sich im Fasse regt.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "sich", "im", "Fas\u00b7se", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "PRF", "APPRART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So regt, so oft als deinem Herzen", "tokens": ["So", "regt", ",", "so", "oft", "als", "dei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Neu des Gesanges Blum' erbl\u00fcht,", "tokens": ["Neu", "des", "Ge\u00b7san\u00b7ges", "Blum'", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es sich in mir mit Lust und Schmerzen:", "tokens": ["Es", "sich", "in", "mir", "mit", "Lust", "und", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hat dein ", "tokens": ["So", "hat", "dein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}}}}