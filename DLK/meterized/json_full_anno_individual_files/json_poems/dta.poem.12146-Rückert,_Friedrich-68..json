{"dta.poem.12146": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An heil'ger Berge Fu\u00df zu wohnen mag erheben,", "tokens": ["An", "heil'\u00b7ger", "Ber\u00b7ge", "Fu\u00df", "zu", "woh\u00b7nen", "mag", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Andachtfl\u00fcgeln wird der Geist sie \u00fcberschweben.", "tokens": ["Auf", "An\u00b7dacht\u00b7fl\u00fc\u00b7geln", "wird", "der", "Geist", "sie", "\u00fc\u00b7bersc\u00b7hwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ungeheiligte vom Glauben dr\u00fccken nur,", "tokens": ["Doch", "un\u00b7ge\u00b7hei\u00b7lig\u00b7te", "vom", "Glau\u00b7ben", "dr\u00fc\u00b7cken", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPRART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lieber wohn' ich fern davon auf offner Flur.", "tokens": ["Und", "lie\u00b7ber", "wohn'", "ich", "fern", "da\u00b7von", "auf", "off\u00b7ner", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}