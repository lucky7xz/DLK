{"dta.poem.12359": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Prager Schlacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als die Preussen marschirten vor Prag,               ", "tokens": ["Als", "die", "Preus\u00b7sen", "mar\u00b7schir\u00b7ten", "vor", "Prag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Vor Prag, die sch\u00f6ne Stadt.", "tokens": ["Vor", "Prag", ",", "die", "sch\u00f6\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben ein Lager geschlagen,", "tokens": ["Sie", "ha\u00b7ben", "ein", "La\u00b7ger", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit Pulver und mit Bley ward's betragen,", "tokens": ["Mit", "Pul\u00b7ver", "und", "mit", "Bley", "ward's", "be\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kanonen wurden drauf gef\u00fchrt,", "tokens": ["Ka\u00b7no\u00b7nen", "wur\u00b7den", "drauf", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schwerin hat sie da kommandirt.", "tokens": ["Schwe\u00b7rin", "hat", "sie", "da", "kom\u00b7man\u00b7dirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Darauf r\u00fcckte Prinz Heinrich heran,", "tokens": ["Da\u00b7rauf", "r\u00fcck\u00b7te", "Prinz", "Hein\u00b7rich", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "NE", "PTKVZ", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wohl mit achzig tausend Mann:", "tokens": ["Wohl", "mit", "ach\u00b7zig", "tau\u00b7send", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "CARD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201emeine ganze Armee wollt ich drum geben,", "tokens": ["\u201e", "mei\u00b7ne", "gan\u00b7ze", "Ar\u00b7mee", "wollt", "ich", "drum", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "\u201ewenn mein Schwerin noch w\u00e4r am Leben!\u201c", "tokens": ["\u201e", "wenn", "mein", "Schwe\u00b7rin", "noch", "w\u00e4r", "am", "Le\u00b7ben", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, ist das nicht eine gro\u00dfe Noth,", "tokens": ["O", ",", "ist", "das", "nicht", "ei\u00b7ne", "gro\u00b7\u00dfe", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PDS", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Schwerin ist geschossen todt!", "tokens": ["Schwe\u00b7rin", "ist", "ge\u00b7schos\u00b7sen", "todt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf schickten sie einen Trompeter hinein:", "tokens": ["Drauf", "schick\u00b7ten", "sie", "ei\u00b7nen", "Trom\u00b7pe\u00b7ter", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob sie Prag wollten geben ein?", "tokens": ["Ob", "sie", "Prag", "woll\u00b7ten", "ge\u00b7ben", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oder, ob sie's sollten einschie\u00dfen?", "tokens": ["O\u00b7der", ",", "ob", "sie's", "soll\u00b7ten", "ein\u00b7schie\u00b7\u00dfen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die B\u00fcrger lie\u00dfen sichs nicht verdrie\u00dfen,", "tokens": ["Die", "B\u00fcr\u00b7ger", "lie\u00b7\u00dfen", "sichs", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie wollten die Stadt nicht geben ein,", "tokens": ["Sie", "woll\u00b7ten", "die", "Stadt", "nicht", "ge\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es sollte und m\u00fc\u00dfte geschossen seyn.", "tokens": ["Es", "soll\u00b7te", "und", "m\u00fc\u00df\u00b7te", "ge\u00b7schos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wer hat dies Liedlein denn erdacht?", "tokens": ["Wer", "hat", "dies", "Lied\u00b7lein", "denn", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es habens drey Husaren gemacht,", "tokens": ["Es", "ha\u00b7bens", "drey", "Hu\u00b7sa\u00b7ren", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Unter Seydlitz sind sie gewesen,", "tokens": ["Un\u00b7ter", "Seyd\u00b7litz", "sind", "sie", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VAPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sind auch bey Prag selbst mitgewesen:", "tokens": ["Sind", "auch", "bey", "Prag", "selbst", "mit\u00b7ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Victoria, Victoria, Victoria,", "tokens": ["Vic\u00b7to\u00b7ria", ",", "Vic\u00b7to\u00b7ria", ",", "Vic\u00b7to\u00b7ria", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00f6nig von Preussen ist schon da!", "tokens": ["K\u00f6\u00b7nig", "von", "Preus\u00b7sen", "ist", "schon", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}