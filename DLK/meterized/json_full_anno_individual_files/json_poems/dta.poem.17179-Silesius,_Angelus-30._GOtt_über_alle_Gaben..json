{"dta.poem.17179": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "30.  GOtt \u00fcber alle Gaben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bitte dich mein GOtt zwar offt umb deine Gaben/", "tokens": ["Ich", "bit\u00b7te", "dich", "mein", "Gott", "zwar", "offt", "umb", "dei\u00b7ne", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wisse da\u00df ich dich viel lieber selbst wil haben.", "tokens": ["Doch", "wis\u00b7se", "da\u00df", "ich", "dich", "viel", "lie\u00b7ber", "selbst", "wil", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum\u0304 gieb mir was du wilt/ es sey auch ewges Leben:", "tokens": ["Drum\u0304", "gieb", "mir", "was", "du", "wilt", "/", "es", "sey", "auch", "ew\u00b7ges", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "PWS", "PPER", "VMFIN", "$(", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Giebstu mir dich nicht selbst/ so hastu nichts gegeben.", "tokens": ["Giebs\u00b7tu", "mir", "dich", "nicht", "selbst", "/", "so", "has\u00b7tu", "nichts", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$(", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}