{"dta.poem.11056": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Gegenliebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lasset uns ihn wieder lieben, da er zum ersten uns ge-\nliebet!", "tokens": ["So", "las\u00b7set", "uns", "ihn", "wie\u00b7der", "lie\u00b7ben", ",", "da", "er", "zum", "ers\u00b7ten", "uns", "ge", "lie\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "APPRART", "ADJA", "PPER", "TRUNC", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der angenehmste Gottesdienst, wird darin blo\u00df nur aus-", "tokens": ["Der", "an\u00b7ge\u00b7nehms\u00b7te", "Got\u00b7tes\u00b7dienst", ",", "wird", "da\u00b7rin", "blo\u00df", "nur", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PAV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Da\u00df wir ein kindliches Vertrauen zu ihm, in unsern Seelen,", "tokens": ["Da\u00df", "wir", "ein", "kind\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7en", "zu", "ihm", ",", "in", "un\u00b7sern", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Uns, voller br\u00fcnstgen Zuversicht, in seine Vater Arme legen,", "tokens": ["Uns", ",", "vol\u00b7ler", "br\u00fcnst\u00b7gen", "Zu\u00b7ver\u00b7sicht", ",", "in", "sei\u00b7ne", "Va\u00b7ter", "Ar\u00b7me", "le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "Und die verbothne knechtsche Furcht, f\u00fcr einen ewigen Ty-", "tokens": ["Und", "die", "ver\u00b7both\u00b7ne", "knecht\u00b7sche", "Furcht", ",", "f\u00fcr", "ei\u00b7nen", "e\u00b7wi\u00b7gen", "Ty"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Als die sein wahres Wesen h\u00f6hnt, sein ewigs Lieben schimpft,", "tokens": ["Als", "die", "sein", "wah\u00b7res", "We\u00b7sen", "h\u00f6hnt", ",", "sein", "e\u00b7wigs", "Lie\u00b7ben", "schimpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Aus dieser Wurzel wird die Frucht, und zwar die seligste", "tokens": ["Aus", "die\u00b7ser", "Wur\u00b7zel", "wird", "die", "Frucht", ",", "und", "zwar", "die", "se\u00b7ligs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Zu seines Namens Ehr, entspriessen, da\u00df wir, damit wir ihm", "tokens": ["Zu", "sei\u00b7nes", "Na\u00b7mens", "Ehr", ",", "ent\u00b7spries\u00b7sen", ",", "da\u00df", "wir", ",", "da\u00b7mit", "wir", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVPP", "$,", "KOUS", "PPER", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "In kindlicher, nicht knechtscher Furcht, aus allen Kr\u00e4ften, uns", "tokens": ["In", "kind\u00b7li\u00b7cher", ",", "nicht", "knecht\u00b7scher", "Furcht", ",", "aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", ",", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "$,", "PTKNEG", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Nach seiner offenbarten Vorschrift, und Regeln der Natur", "tokens": ["Nach", "sei\u00b7ner", "of\u00b7fen\u00b7bar\u00b7ten", "Vor\u00b7schrift", ",", "und", "Re\u00b7geln", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Zu meiden, was wir meiden sollen, zu thun, was Gott zu thun", "tokens": ["Zu", "mei\u00b7den", ",", "was", "wir", "mei\u00b7den", "sol\u00b7len", ",", "zu", "thun", ",", "was", "Gott", "zu", "thun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "PTKZU", "VVINF", "$,", "PRELS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Und uns bem\u00fchen, stets zu zeigen, da\u00df man ihn herzlich liebt", "tokens": ["Und", "uns", "be\u00b7m\u00fc\u00b7hen", ",", "stets", "zu", "zei\u00b7gen", ",", "da\u00df", "man", "ihn", "herz\u00b7lich", "liebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Was k\u00f6nnten doch des ewgen Vaters, der ewgen Liebe, Liebes-", "tokens": ["Was", "k\u00f6nn\u00b7ten", "doch", "des", "ew\u00b7gen", "Va\u00b7ters", ",", "der", "ew\u00b7gen", "Lie\u00b7be", ",", "Lie\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Doch anders wohl von uns verlangen, als kindlich br\u00fcnstge", "tokens": ["Doch", "an\u00b7ders", "wohl", "von", "uns", "ver\u00b7lan\u00b7gen", ",", "als", "kind\u00b7lich", "br\u00fcnst\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,", "KOUS", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Vor allen andern Leidenschaften, die alleredelst\u2019 ist allein,", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "Lei\u00b7den\u00b7schaf\u00b7ten", ",", "die", "al\u00b7le\u00b7re\u00b7delst'", "ist", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.16": {"text": "Sich herzlich sehnen, als ein Kind, solch einem Vater werth zu seyn,", "tokens": ["Sich", "herz\u00b7lich", "seh\u00b7nen", ",", "als", "ein", "Kind", ",", "solch", "ei\u00b7nem", "Va\u00b7ter", "werth", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$,", "KOUS", "ART", "NN", "$,", "PIAT", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.17": {"text": "Der, wie er uns hervor gebracht, auch unaufh\u00f6rlich Proben", "tokens": ["Der", ",", "wie", "er", "uns", "her\u00b7vor", "ge\u00b7bracht", ",", "auch", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "Pro\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PPER", "PPER", "PTKVZ", "VVPP", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Da\u00df er uns, blo\u00df zu unserm Besten, geliebt, hier liebt und", "tokens": ["Da\u00df", "er", "uns", ",", "blo\u00df", "zu", "un\u00b7serm", "Bes\u00b7ten", ",", "ge\u00b7liebt", ",", "hier", "liebt", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$,", "ADV", "VVFIN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}