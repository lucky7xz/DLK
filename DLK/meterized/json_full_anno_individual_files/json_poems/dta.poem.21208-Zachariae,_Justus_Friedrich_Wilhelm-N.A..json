{"dta.poem.21208": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und dennoch that er es!", "tokens": ["Und", "den\u00b7noch", "that", "er", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich d\u00fcnkt, ich sehe hier aufs neu", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "se\u00b7he", "hier", "aufs", "neu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die gro\u00dfen heilig furchtbarn Scenen wieder \u2014", "tokens": ["Die", "gro\u00b7\u00dfen", "hei\u00b7lig", "furcht\u00b7barn", "Sce\u00b7nen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Cherub f\u00e4llt erschrocken nieder,", "tokens": ["Der", "Che\u00b7rub", "f\u00e4llt", "er\u00b7schro\u00b7cken", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00fcllt sein Angesicht", "tokens": ["Und", "h\u00fcllt", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tief in sein gl\u00e4nzendes Gefieder;", "tokens": ["Tief", "in", "sein", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Seraphinen Lieder", "tokens": ["Der", "Se\u00b7ra\u00b7phi\u00b7nen", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verstummen vor des H\u00f6chsten Thron \u2014", "tokens": ["Ver\u00b7stum\u00b7men", "vor", "des", "H\u00f6chs\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Man h\u00f6ret, um der Allmacht Sohn,", "tokens": ["Man", "h\u00f6\u00b7ret", ",", "um", "der", "All\u00b7macht", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUI", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein banges Klagen in den Sternen,", "tokens": ["Ein", "ban\u00b7ges", "Kla\u00b7gen", "in", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein banges Klagen in den Himmeln,", "tokens": ["Ein", "ban\u00b7ges", "Kla\u00b7gen", "in", "den", "Him\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein banges Klagen auf der Erde. \u2014", "tokens": ["Ein", "ban\u00b7ges", "Kla\u00b7gen", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Abgrund thut sich auf \u2014", "tokens": ["Der", "Ab\u00b7grund", "thut", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Die H\u00f6lle br\u00fcllt Triumph herauf;", "tokens": ["Die", "H\u00f6l\u00b7le", "br\u00fcllt", "Tri\u00b7umph", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Sonne starrt zur\u00fcck in ihrem Lauf,", "tokens": ["Die", "Son\u00b7ne", "starrt", "zu\u00b7r\u00fcck", "in", "ih\u00b7rem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und schwarze Mitternacht verh\u00fcllt die Welt.", "tokens": ["Und", "schwar\u00b7ze", "Mit\u00b7ter\u00b7nacht", "ver\u00b7h\u00fcllt", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ersch\u00fcttert f\u00fchlt, der Todten weites Feld,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "f\u00fchlt", ",", "der", "Tod\u00b7ten", "wei\u00b7tes", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der neuen Auferstehung Macht;", "tokens": ["Der", "neu\u00b7en", "Auf\u00b7er\u00b7ste\u00b7hung", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie gehn hervor aus Grab und Nacht.", "tokens": ["Sie", "gehn", "her\u00b7vor", "aus", "Grab", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}