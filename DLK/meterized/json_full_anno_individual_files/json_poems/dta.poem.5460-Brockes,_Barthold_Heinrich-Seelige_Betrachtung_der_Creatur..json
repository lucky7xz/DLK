{"dta.poem.5460": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seelige Betrachtung der Creatur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sind auf dem Bau der Erden doch vor Wunder!", "tokens": ["Was", "sind", "auf", "dem", "Bau", "der", "Er\u00b7den", "doch", "vor", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "welche Menge", "tokens": ["wel\u00b7che", "Men\u00b7ge"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Lust-erregender Gesch\u00f6pfe! es ist gleichsam ein Gedr\u00e4nge", "tokens": ["Lust\u00b7er\u00b7re\u00b7gen\u00b7der", "Ge\u00b7sch\u00f6p\u00b7fe", "!", "es", "ist", "gleich\u00b7sam", "ein", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jrdisch-G\u00f6ttlichen Vergn\u00fcgens um uns Menschen rings-", "tokens": ["Jr\u00b7dischG\u00f6tt\u00b7li\u00b7chen", "Ver\u00b7gn\u00fc\u00b7gens", "um", "uns", "Men\u00b7schen", "rings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "umher,", "tokens": ["um\u00b7her", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Wenn man sie nur mit Verstand anzusehn besch\u00e4ftigt w\u00e4r.", "tokens": ["Wenn", "man", "sie", "nur", "mit", "Ver\u00b7stand", "an\u00b7zu\u00b7sehn", "be\u00b7sch\u00e4f\u00b7tigt", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "NN", "VVIZU", "VVPP", "VAFIN", "$."], "meter": "-+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Es hat (wenn mit den Gesch\u00f6pfen wir, im Brauch, den", "tokens": ["Es", "hat", "(", "wenn", "mit", "den", "Ge\u00b7sch\u00f6p\u00b7fen", "wir", ",", "im", "Brauch", ",", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "APPR", "ART", "NN", "PPER", "$,", "APPRART", "NN", "$,", "ART"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Sch\u00f6pfer f\u00fcgen,", "tokens": ["Sch\u00f6p\u00b7fer", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und sie als sein Werck betrachten) jeder Vorwurf sein", "tokens": ["Und", "sie", "als", "sein", "Werck", "be\u00b7trach\u00b7ten", ")", "je\u00b7der", "Vor\u00b7wurf", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "KOUS", "PPOSAT", "NN", "VVINF", "$(", "PIAT", "NN", "PPOSAT"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Jeder Sinn sein Paradie\u00df.", "tokens": ["Je\u00b7der", "Sinn", "sein", "Pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Es ist sonder GOtt kein Himmel: doch, da GOtt auch", "tokens": ["Es", "ist", "son\u00b7der", "Gott", "kein", "Him\u00b7mel", ":", "doch", ",", "da", "Gott", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PIAT", "NN", "$.", "ADV", "$,", "KOUS", "NN", "ADV"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "in der Welt;", "tokens": ["in", "der", "Welt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Ist auch hier ein ird\u2019scher Himmel denen Seelen vorgestellt,", "tokens": ["Ist", "auch", "hier", "ein", "ird'\u00b7scher", "Him\u00b7mel", "de\u00b7nen", "See\u00b7len", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Welche, da sie ihren Sch\u00f6pfer mit der Creatur verbinden,", "tokens": ["Wel\u00b7che", ",", "da", "sie", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", "mit", "der", "Crea\u00b7tur", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Jhm zu Ehren, Lust und Anmuth in den Creaturen finden.", "tokens": ["Jhm", "zu", "Eh\u00b7ren", ",", "Lust", "und", "An\u00b7muth", "in", "den", "Crea\u00b7tu\u00b7ren", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "NN", "KON", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}}}}}