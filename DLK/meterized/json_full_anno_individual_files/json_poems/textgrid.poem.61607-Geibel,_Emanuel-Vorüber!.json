{"textgrid.poem.61607": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Vor\u00fcber!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Dampfro\u00df schnaubt entlang der Halde,", "tokens": ["Das", "Dam\u00b7pfro\u00df", "schnaubt", "ent\u00b7lang", "der", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da, pl\u00f6tzlich, \u00f6ffnet sich das Tal,", "tokens": ["Da", ",", "pl\u00f6tz\u00b7lich", ",", "\u00f6ff\u00b7net", "sich", "das", "Tal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ferne d\u00e4mmert \u00fcberm Walde", "tokens": ["Und", "fer\u00b7ne", "d\u00e4m\u00b7mert", "\u00fc\u00b7berm", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schlo\u00df empor im Abendstrahl.", "tokens": ["Ein", "Schlo\u00df", "em\u00b7por", "im", "A\u00b7bend\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Turm und Erkern seh' ich's ragen,", "tokens": ["Mit", "Turm", "und", "Er\u00b7kern", "seh'", "ich's", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es naht, es gr\u00fc\u00dft, es flieht vorbei; \u2013", "tokens": ["Es", "naht", ",", "es", "gr\u00fc\u00dft", ",", "es", "flieht", "vor\u00b7bei", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir aber tr\u00e4umt von alten Tagen,", "tokens": ["Mir", "a\u00b7ber", "tr\u00e4umt", "von", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem sch\u00f6nen Monat Mai.", "tokens": ["Von", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie flog zu jenen gr\u00fcnen Schatten", "tokens": ["Wie", "flog", "zu", "je\u00b7nen", "gr\u00fc\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Fr\u00fchrot einst mein leichter Schritt!", "tokens": ["Beim", "Fr\u00fch\u00b7rot", "einst", "mein", "leich\u00b7ter", "Schritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Blumen standen Forst und Matten,", "tokens": ["In", "Blu\u00b7men", "stan\u00b7den", "Forst", "und", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Seele bl\u00fchte mit.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "bl\u00fch\u00b7te", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Liedes tiefen Drang im Busen,", "tokens": ["Des", "Lie\u00b7des", "tie\u00b7fen", "Drang", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschw\u00e4rmt' ich jung und sorgenfrei", "tokens": ["Ver\u00b7schw\u00e4rmt'", "ich", "jung", "und", "sor\u00b7gen\u00b7frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den goldnen Tag dort mit den Musen \u2013", "tokens": ["Den", "gold\u00b7nen", "Tag", "dort", "mit", "den", "Mu\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es war im sch\u00f6nen Monat Mai.", "tokens": ["Es", "war", "im", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wenn der Mond um Busch und Gipfel", "tokens": ["Doch", "wenn", "der", "Mond", "um", "Busch", "und", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein traumhaft Silberlicht ergo\u00df,", "tokens": ["Sein", "traum\u00b7haft", "Sil\u00b7ber\u00b7licht", "er\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Berauschend durch die Nacht der Wipfel", "tokens": ["Be\u00b7rau\u00b7schend", "durch", "die", "Nacht", "der", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nachtigallen Stimme flo\u00df,", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "Stim\u00b7me", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann harrt' ich, da\u00df sie mir erschiene,", "tokens": ["Dann", "harrt'", "ich", ",", "da\u00df", "sie", "mir", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie, meines Waldes schlanke Fei,", "tokens": ["Sie", ",", "mei\u00b7nes", "Wal\u00b7des", "schlan\u00b7ke", "Fei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die lockendunkle Melusine \u2013", "tokens": ["Die", "lo\u00b7cken\u00b7dunk\u00b7le", "Me\u00b7lu\u00b7si\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es war im sch\u00f6nen Monat Mai.", "tokens": ["Es", "war", "im", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und jetzt, entgegen meinem Gru\u00dfe,", "tokens": ["Und", "jetzt", ",", "ent\u00b7ge\u00b7gen", "mei\u00b7nem", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als ging' der Mond noch einmal auf,", "tokens": ["Als", "ging'", "der", "Mond", "noch", "ein\u00b7mal", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unh\u00f6rbar, mit beschwingtem Fu\u00dfe", "tokens": ["Un\u00b7h\u00f6r\u00b7bar", ",", "mit", "be\u00b7schwing\u00b7tem", "Fu\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Baumgang schwebte sie herauf.", "tokens": ["Den", "Baum\u00b7gang", "schweb\u00b7te", "sie", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir scho\u00df das Blut in Stirn und Wangen,", "tokens": ["Mir", "scho\u00df", "das", "Blut", "in", "Stirn", "und", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Lipp' entfuhr ein Freudenschrei;", "tokens": ["Der", "Lipp'", "ent\u00b7fuhr", "ein", "Freu\u00b7den\u00b7schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Armen durft' ich sie umfangen \u2013", "tokens": ["Mit", "Ar\u00b7men", "durft'", "ich", "sie", "um\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es war im sch\u00f6nen Monat Mai.", "tokens": ["Es", "war", "im", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Sterne, die mit klarem Funkeln", "tokens": ["Ihr", "Ster\u00b7ne", ",", "die", "mit", "kla\u00b7rem", "Fun\u00b7keln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr in dies Tal herniederscheint,", "tokens": ["Ihr", "in", "dies", "Tal", "her\u00b7nie\u00b7der\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wi\u00dft allein, wie wir im Dunkeln", "tokens": ["Ihr", "wi\u00dft", "al\u00b7lein", ",", "wie", "wir", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gek\u00fc\u00dft, gejubelt und geweint!", "tokens": ["Ge\u00b7k\u00fc\u00dft", ",", "ge\u00b7ju\u00b7belt", "und", "ge\u00b7weint", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr wi\u00dft's, wie wir so selig waren,", "tokens": ["Ihr", "wi\u00dft's", ",", "wie", "wir", "so", "se\u00b7lig", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So selig und so rein dabei,", "tokens": ["So", "se\u00b7lig", "und", "so", "rein", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Rein, wie man's ist mit achtzehn Jahren \u2013", "tokens": ["Rein", ",", "wie", "man's", "ist", "mit", "acht\u00b7zehn", "Jah\u00b7ren", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "VAFIN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es war im sch\u00f6nen Monat Mai.", "tokens": ["Es", "war", "im", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O, denk' ich dran, so fliegt der Schauer", "tokens": ["O", ",", "denk'", "ich", "dran", ",", "so", "fliegt", "der", "Schau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PAV", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch heut mir durch die m\u00fcde Brust;", "tokens": ["Noch", "heut", "mir", "durch", "die", "m\u00fc\u00b7de", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erquickend flie\u00dft in meine Trauer", "tokens": ["Er\u00b7qui\u00b7ckend", "flie\u00dft", "in", "mei\u00b7ne", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sonnenblick verge\u00dfner Lust.", "tokens": ["Ein", "Son\u00b7nen\u00b7blick", "ver\u00b7ge\u00df\u00b7ner", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mag nimmermehr dies Herz genesen,", "tokens": ["Mag", "nim\u00b7mer\u00b7mehr", "dies", "Herz", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind Glanz und Fr\u00fchling l\u00e4ngst vorbei:", "tokens": ["Sind", "Glanz", "und", "Fr\u00fch\u00b7ling", "l\u00e4ngst", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gl\u00fcckselig bin auch ich gewesen;", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "bin", "auch", "ich", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es war im sch\u00f6nen Monat Mai.", "tokens": ["Es", "war", "im", "sch\u00f6\u00b7nen", "Mo\u00b7nat", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Dampfro\u00df schnaubt entlang der Halde,", "tokens": ["Das", "Dam\u00b7pfro\u00df", "schnaubt", "ent\u00b7lang", "der", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da, pl\u00f6tzlich, \u00f6ffnet sich das Tal,", "tokens": ["Da", ",", "pl\u00f6tz\u00b7lich", ",", "\u00f6ff\u00b7net", "sich", "das", "Tal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ferne d\u00e4mmert \u00fcberm Walde", "tokens": ["Und", "fer\u00b7ne", "d\u00e4m\u00b7mert", "\u00fc\u00b7berm", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schlo\u00df empor im Abendstrahl.", "tokens": ["Ein", "Schlo\u00df", "em\u00b7por", "im", "A\u00b7bend\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Turm und Erkern seh' ich's ragen,", "tokens": ["Mit", "Turm", "und", "Er\u00b7kern", "seh'", "ich's", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es naht, es gr\u00fc\u00dft,