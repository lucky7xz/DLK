{"dta.poem.12651": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihren Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von hertzen ist allhier ein grabmahl aufgericht,", "tokens": ["Von", "hert\u00b7zen", "ist", "all\u00b7hier", "ein", "grab\u00b7mahl", "auf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn hier vergruben wir die meisterin der hertzen;", "tokens": ["Denn", "hier", "ver\u00b7gru\u00b7ben", "wir", "die", "meis\u00b7te\u00b7rin", "der", "hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr daseyn bracht uns lust, ihr abzug bringt uns schmertzen,", "tokens": ["Ihr", "da\u00b7seyn", "bracht", "uns", "lust", ",", "ihr", "ab\u00b7zug", "bringt", "uns", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir aber sind ihr doch auch in der grufft verpflicht,", "tokens": ["Wir", "a\u00b7ber", "sind", "ihr", "doch", "auch", "in", "der", "grufft", "ver\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr angedencken kan in uns die lieb erregen,", "tokens": ["Ihr", "an\u00b7ge\u00b7den\u00b7cken", "kan", "in", "uns", "die", "lieb", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "PPER", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und aller unser brust zu vieler lust bewegen.", "tokens": ["Und", "al\u00b7ler", "un\u00b7ser", "brust", "zu", "vie\u00b7ler", "lust", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach k\u00f6nte man (jedoch ich w\u00fcnsch etwas zu viel)", "tokens": ["Ach", "k\u00f6n\u00b7te", "man", "(", "je\u00b7doch", "ich", "w\u00fcnsch", "et\u00b7was", "zu", "viel", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "$(", "ADV", "PPER", "ADJD", "ADV", "PTKA", "PIS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur durch ein l\u00f6se-geld ihr leben wieder kauffen,", "tokens": ["Nur", "durch", "ein", "l\u00f6\u00b7se\u00b7geld", "ihr", "le\u00b7ben", "wie\u00b7der", "kauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es solt uns nicht zu hoch am werthe sich belauffen;", "tokens": ["Es", "solt", "uns", "nicht", "zu", "hoch", "am", "wert\u00b7he", "sich", "be\u00b7lauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "APPRART", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allein wo finden wir zu dieser axt den stiehl?", "tokens": ["Al\u00b7lein", "wo", "fin\u00b7den", "wir", "zu", "die\u00b7ser", "axt", "den", "stiehl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der tod der l\u00e4st sich nicht durch geld und gold vergn\u00fcgen,", "tokens": ["Der", "tod", "der", "l\u00e4st", "sich", "nicht", "durch", "geld", "und", "gold", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es mu\u00df, was er verlangt, ihm zu den f\u00fc\u00dfen liegen.", "tokens": ["Es", "mu\u00df", ",", "was", "er", "ver\u00b7langt", ",", "ihm", "zu", "den", "f\u00fc\u00b7\u00dfen", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wei\u00df ihr schatten irrt noch um die gegend rum,", "tokens": ["Ich", "wei\u00df", "ihr", "schat\u00b7ten", "irrt", "noch", "um", "die", "ge\u00b7gend", "rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.14": {"text": "Und will der wiesen pracht nun zum gefehrten haben;", "tokens": ["Und", "will", "der", "wie\u00b7sen", "pracht", "nun", "zum", "ge\u00b7fehr\u00b7ten", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ADV", "APPRART", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nichts konte den so wohl als die gemeinschafft laben,", "tokens": ["Nichts", "kon\u00b7te", "den", "so", "wohl", "als", "die", "ge\u00b7mein\u00b7schafft", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADV", "ADV", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drum thut er sich nach ihr auch in dem tode um.", "tokens": ["Drum", "thut", "er", "sich", "nach", "ihr", "auch", "in", "dem", "to\u00b7de", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.17": {"text": "Ach solt er bey uns seyn, so m\u00fcste alles weichen,", "tokens": ["Ach", "solt", "er", "bey", "uns", "seyn", ",", "so", "m\u00fcs\u00b7te", "al\u00b7les", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "VAINF", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was uns verhinderte den entzweck zu erreichen.", "tokens": ["Was", "uns", "ver\u00b7hin\u00b7der\u00b7te", "den", "ent\u00b7zweck", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Jhr thr\u00e4nen! samlet euch zu einer trauer-bach,", "tokens": ["Ihr", "thr\u00e4\u00b7nen", "!", "sam\u00b7let", "euch", "zu", "ei\u00b7ner", "trau\u00b7e\u00b7rbach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich will den letzten dienst durch euch allhier bezahlen,", "tokens": ["Ich", "will", "den", "letz\u00b7ten", "dienst", "durch", "euch", "all\u00b7hier", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich will mit euch allhier ein angedencken mahlen,", "tokens": ["Ich", "will", "mit", "euch", "all\u00b7hier", "ein", "an\u00b7ge\u00b7den\u00b7cken", "mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das unvergleichlich ist. Flie\u00dft thr\u00e4nen, flie\u00dft nur nach!", "tokens": ["Das", "un\u00b7ver\u00b7gleich\u00b7lich", "ist", ".", "Flie\u00dft", "thr\u00e4\u00b7nen", ",", "flie\u00dft", "nur", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$.", "VVFIN", "NN", "$,", "VVFIN", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der thr\u00e4nen kostbarkeit ist \u00fcber alle dinge,", "tokens": ["Der", "thr\u00e4\u00b7nen", "kost\u00b7bar\u00b7keit", "ist", "\u00fc\u00b7ber", "al\u00b7le", "din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der beste diamant ist gegen ihr geringe.", "tokens": ["Der", "bes\u00b7te", "di\u00b7a\u00b7mant", "ist", "ge\u00b7gen", "ihr", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Schlaf, Sch\u00f6nst\u2019! inzwischen wohl in deiner k\u00fchlen grufft,", "tokens": ["Schlaf", ",", "Sch\u00f6nst'", "!", "in\u00b7zwi\u00b7schen", "wohl", "in", "dei\u00b7ner", "k\u00fch\u00b7len", "grufft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die liebe wird dich itzt nicht mehr, wie neulich, plagen:", "tokens": ["Die", "lie\u00b7be", "wird", "dich", "itzt", "nicht", "mehr", ",", "wie", "neu\u00b7lich", ",", "pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jtzt kanst du gute nacht den eitelkeiten sagen,", "tokens": ["Jtzt", "kanst", "du", "gu\u00b7te", "nacht", "den", "ei\u00b7tel\u00b7kei\u00b7ten", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil dich die einsamkeit aus deren circkeln rufft,", "tokens": ["Weil", "dich", "die", "ein\u00b7sam\u00b7keit", "aus", "de\u00b7ren", "cir\u00b7ckeln", "rufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRELAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den heissen liebes-brand k\u00fchlt itzt die frische erde,", "tokens": ["Den", "heis\u00b7sen", "lie\u00b7bes\u00b7brand", "k\u00fchlt", "itzt", "die", "fri\u00b7sche", "er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und w\u00fcnscht, da\u00df ihr dein leib in kurtzem \u00e4hnlich werde.", "tokens": ["Und", "w\u00fcnscht", ",", "da\u00df", "ihr", "dein", "leib", "in", "kurt\u00b7zem", "\u00e4hn\u00b7lich", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich mache dekne grufft auch itzt zu meiner grufft,", "tokens": ["Ich", "ma\u00b7che", "de\u00b7kne", "grufft", "auch", "itzt", "zu", "mei\u00b7ner", "grufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo ich die eitelkeit nun g\u00e4ntzlich will verscharren,", "tokens": ["Wo", "ich", "die", "ei\u00b7tel\u00b7keit", "nun", "g\u00e4ntz\u00b7lich", "will", "ver\u00b7schar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und nicht, wie vor geschehn, wie die verliebten narren,", "tokens": ["Und", "nicht", ",", "wie", "vor", "ge\u00b7schehn", ",", "wie", "die", "ver\u00b7lieb\u00b7ten", "nar\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "APPR", "VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil mich dein zufall itzt von diesem wege rufft.", "tokens": ["Weil", "mich", "dein", "zu\u00b7fall", "itzt", "von", "die\u00b7sem", "we\u00b7ge", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein grabmahl aber soll zu seinem ruhme haben,", "tokens": ["Dein", "grab\u00b7mahl", "a\u00b7ber", "soll", "zu", "sei\u00b7nem", "ruh\u00b7me", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df allda Citron hat sein liebes-werck vergraben.", "tokens": ["Da\u00df", "all\u00b7da", "Cit\u00b7ron", "hat", "sein", "lie\u00b7bes\u00b7\u00b7werck", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jhr todten! nehmet sie in eure blasse zahl,", "tokens": ["Ihr", "tod\u00b7ten", "!", "neh\u00b7met", "sie", "in", "eu\u00b7re", "blas\u00b7se", "zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die hier bey uns ein licht des frauen-volcks gewesen,", "tokens": ["Die", "hier", "bey", "uns", "ein", "licht", "des", "frau\u00b7en\u00b7volcks", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die tugend hatte sie zum leit-compa\u00df erlesen,", "tokens": ["Die", "tu\u00b7gend", "hat\u00b7te", "sie", "zum", "leit\u00b7com\u00b7pa\u00df", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Drum ist man so bem\u00fcht um ihr ged\u00e4chtni\u00df-mahl,", "tokens": ["Drum", "ist", "man", "so", "be\u00b7m\u00fcht", "um", "ihr", "ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Denn diese kan doch nicht auch in der grufft ersterben,", "tokens": ["Denn", "die\u00b7se", "kan", "doch", "nicht", "auch", "in", "der", "grufft", "ers\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Sie kan eh durch den tod ein ander leben erben.", "tokens": ["Sie", "kan", "eh", "durch", "den", "tod", "ein", "an\u00b7der", "le\u00b7ben", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "APPR", "ART", "NN", "ART", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}