{"dta.poem.19458": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Fei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Mondnacht und Flut. Sie hangt am Kiel,", "tokens": ["Mond\u00b7nacht", "und", "Flut", ".", "Sie", "hangt", "am", "Kiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PPER", "VVFIN", "APPRART", "NE", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umklammert mit den Armen ihn,", "tokens": ["Um\u00b7klam\u00b7mert", "mit", "den", "Ar\u00b7men", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie treibt ein grausam l\u00fcstern Spiel,", "tokens": ["Sie", "treibt", "ein", "grau\u00b7sam", "l\u00fcs\u00b7tern", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Nachen in den Grund zu ziehn.", "tokens": ["Den", "Na\u00b7chen", "in", "den", "Grund", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Ferge st\u00f6hnt: \u201eIn Seegestr\u00e4uch", "tokens": ["Der", "Fer\u00b7ge", "st\u00f6hnt", ":", "\u201e", "In", "See\u00b7ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rei\u00dft nieder uns der blanke Leib!", "tokens": ["Rei\u00dft", "nie\u00b7der", "uns", "der", "blan\u00b7ke", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rasch, Herr! Von S\u00fcnde reinigt Euch,", "tokens": ["Rasch", ",", "Herr", "!", "Von", "S\u00fcn\u00b7de", "rei\u00b7nigt", "Euch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$.", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begehrt Ihr heim zu Kind und Weib!\u201c", "tokens": ["Be\u00b7gehrt", "Ihr", "heim", "zu", "Kind", "und", "Weib", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Ritter h\u00e4lt den Schwertesgriff", "tokens": ["Der", "Rit\u00b7ter", "h\u00e4lt", "den", "Schwer\u00b7tes\u00b7griff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich als das heil'ge Zeichen vor \u2014", "tokens": ["Sich", "als", "das", "heil'\u00b7ge", "Zei\u00b7chen", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus dunkeln Haaren lauscht am Schiff", "tokens": ["Aus", "dun\u00b7keln", "Haa\u00b7ren", "lauscht", "am", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schmerzlich bleiches Haupt empor.", "tokens": ["Ein", "schmerz\u00b7lich", "blei\u00b7ches", "Haupt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eherr Christ! Ich beichte Ritterthat,", "tokens": ["\u201e", "herr", "Christ", "!", "Ich", "beich\u00b7te", "Rit\u00b7ter\u00b7that", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.xy", "FM.xy", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Streit, Flammenschein und str\u00f6mend Blut,", "tokens": ["Streit", ",", "Flam\u00b7men\u00b7schein", "und", "str\u00f6\u00b7mend", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nichts von Frevel noch Verrat,", "tokens": ["Doch", "nichts", "von", "Fre\u00b7vel", "noch", "Ver\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Treu und Glauben hielt ich gut.\u201c", "tokens": ["Denn", "Treu", "und", "Glau\u00b7ben", "hielt", "ich", "gut", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er k\u00fc\u00dft das Kreuz. Gell schreit die Fee!", "tokens": ["Er", "k\u00fc\u00dft", "das", "Kreuz", ".", "Gell", "schreit", "die", "Fee", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auflangen sieht er eine Hand", "tokens": ["Auf\u00b7lan\u00b7gen", "sieht", "er", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Steuer, blendend wei\u00df wie Schnee,", "tokens": ["Am", "Steu\u00b7er", ",", "blen\u00b7dend", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und starrt darauf, von Graun gebannt.", "tokens": ["Und", "starrt", "da\u00b7rauf", ",", "von", "Graun", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eherr Christ! Ich beichte Missethat!", "tokens": ["\u201e", "herr", "Christ", "!", "Ich", "beich\u00b7te", "Mis\u00b7se\u00b7that", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.xy", "FM.xy", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich brach den Glauben und die Treu,", "tokens": ["Ich", "brach", "den", "Glau\u00b7ben", "und", "die", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich \u00fcbt' an einem Lieb Verrat.", "tokens": ["Ich", "\u00fcbt'", "an", "ei\u00b7nem", "Lieb", "Ver\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es starb. Ich thue Leid und Reu!\u201c", "tokens": ["Es", "starb", ".", "Ich", "thue", "Leid", "und", "Reu", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NN", "KON", "NE", "$.", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Sie l\u00f6st die Arme. Sie versinkt.", "tokens": ["Sie", "l\u00f6st", "die", "Ar\u00b7me", ".", "Sie", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ruder schl\u00e4gt. Der Nachen fliegt.", "tokens": ["Das", "Ru\u00b7der", "schl\u00e4gt", ".", "Der", "Na\u00b7chen", "fliegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Strand das Licht des Erkers winkt,", "tokens": ["Vom", "Strand", "das", "Licht", "des", "Er\u00b7kers", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Weib und Kind ihm schlummernd liegt.", "tokens": ["Wo", "Weib", "und", "Kind", "ihm", "schlum\u00b7mernd", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}