{"textgrid.poem.40383": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kann Eisenbahn-Zugf\u00fchrer werden;", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann Eisenbahn-Zugf\u00fchrer werden;", "tokens": ["Ich", "kann", "Ei\u00b7sen\u00b7bahn\u00b7Zug\u00b7f\u00fch\u00b7rer", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "nein, Lokomotivf\u00fchrer lieber!", "tokens": ["nein", ",", "Lo\u00b7ko\u00b7mo\u00b7tiv\u00b7f\u00fch\u00b7rer", "lie\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bin ich kleiner Menschenknirps", "tokens": ["Dann", "bin", "ich", "klei\u00b7ner", "Men\u00b7schen\u00b7knirps"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der gr\u00f6\u00dften Maschine \u00fcber,", "tokens": ["der", "gr\u00f6\u00df\u00b7ten", "Ma\u00b7schi\u00b7ne", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "die tausend Pferdekraft stark ist.", "tokens": ["die", "tau\u00b7send", "Pfer\u00b7de\u00b7kraft", "stark", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und tausend andre Menschen", "tokens": ["Und", "tau\u00b7send", "and\u00b7re", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "regiert Ein Griff meiner Hand,", "tokens": ["re\u00b7giert", "Ein", "Griff", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "tagein tagaus, bei Nacht, bei Nebel,", "tokens": ["ta\u00b7gein", "ta\u00b7gaus", ",", "bei", "Nacht", ",", "bei", "Ne\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "im Sturm von Land zu Land;", "tokens": ["im", "Sturm", "von", "Land", "zu", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bahn frei! schreit meine Maschine.", "tokens": ["Bahn", "frei", "!", "schreit", "mei\u00b7ne", "Ma\u00b7schi\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Bahn frei \u2013 was schreit da wider?", "tokens": ["Bahn", "frei", "\u2013", "was", "schreit", "da", "wi\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$(", "PWS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "im Dunkeln welch Gestampf?", "tokens": ["im", "Dun\u00b7keln", "welch", "Ge\u00b7stampf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PWAT", "NN", "$."]