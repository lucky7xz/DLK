{"dta.poem.764": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "L.  Vber seines H. Brudern  P. GRYPHII  \n Grab.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ruht/ dem keine ruh' auff dieser welt bescheret: ", "tokens": ["Hjer", "ruht", "/", "dem", "kei\u00b7ne", "ruh'", "auff", "die\u00b7ser", "welt", "be\u00b7sche\u00b7ret", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ART", "PIAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier ligt der keinmal fiel/ hier schl\u00e4fft das hohe Haupt/", "tokens": ["Hier", "ligt", "der", "kein\u00b7mal", "fiel", "/", "hier", "schl\u00e4fft", "das", "ho\u00b7he", "Haupt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das f\u00fcr die Kirche wacht/ hier ist/ den GOTt geraubt/", "tokens": ["Das", "f\u00fcr", "die", "Kir\u00b7che", "wacht", "/", "hier", "ist", "/", "den", "GoTt", "ge\u00b7raubt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "VAFIN", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der voll von GOTT/ doch nichts denn GOtt allein begeh- ", "tokens": ["Der", "voll", "von", "GoTT", "/", "doch", "nichts", "denn", "Gott", "al\u00b7lein", "be\u00b7geh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NE", "$(", "ADV", "PIS", "KON", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ret.", "tokens": ["ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "wehret", "tokens": ["weh\u00b7ret"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Durch Elendt/ Schwerdt/ vnd Pest/ der vnverzagt ge-", "tokens": ["Durch", "E\u00b7lendt", "/", "Schwerdt", "/", "vnd", "Pest", "/", "der", "vn\u00b7ver\u00b7zagt", "ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NE", "$(", "KON", "NN", "$(", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "glaubt:", "tokens": ["glaubt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Dem Gott nach st\u00e4tter angst/ hat st\u00e4tte Lust erlaubt", "tokens": ["Dem", "Gott", "nach", "st\u00e4t\u00b7ter", "angst", "/", "hat", "st\u00e4t\u00b7te", "Lust", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$(", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem jhn Seuch/ vnd Angst/ vnd Todt vmbsonst be-", "tokens": ["Nach", "dem", "jhn", "Seuch", "/", "vnd", "Angst", "/", "vnd", "Todt", "vmbsonst", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NN", "$(", "KON", "NN", "$(", "KON", "NN", "ADV", "TRUNC"], "meter": "+--+-+-++-", "measure": "iambic.penta.invert"}, "line.3": {"text": "schweret.", "tokens": ["schwe\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Dein Bischoff Crossen/ ach! den GOttes Geist entz\u00fcndt. ", "tokens": ["Dein", "Bi\u00b7schoff", "Cros\u00b7sen", "/", "ach", "!", "den", "Got\u00b7tes", "Geist", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$(", "XY", "$.", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem an verstandt vnd Kunst man wenig gleiche findt. ", "tokens": ["Dem", "an", "ver\u00b7standt", "vnd", "Kunst", "man", "we\u00b7nig", "glei\u00b7che", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "VVPP", "KON", "NN", "PIS", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd de\u00df beredtsamkeit kaum einer wirdt erreichen.", "tokens": ["Vnd", "de\u00df", "be\u00b7redt\u00b7sam\u00b7keit", "kaum", "ei\u00b7ner", "wirdt", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In dem die Tugend lebt/ durch den die Tugendt lehrt/", "tokens": ["In", "dem", "die", "Tu\u00b7gend", "lebt", "/", "durch", "den", "die", "Tu\u00b7gendt", "lehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$(", "APPR", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dem die ", "tokens": ["Mit", "dem", "die"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was sich mit keinem Schatz der Erden l\u00e4st vergleichen.", "tokens": ["Was", "sich", "mit", "kei\u00b7nem", "Schatz", "der", "Er\u00b7den", "l\u00e4st", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}