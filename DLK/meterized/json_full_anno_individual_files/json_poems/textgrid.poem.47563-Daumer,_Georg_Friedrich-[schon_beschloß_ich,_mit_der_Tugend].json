{"textgrid.poem.47563": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[schon beschlo\u00df ich, mit der Tugend]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon beschlo\u00df ich, mit der Tugend", "tokens": ["Schon", "be\u00b7schlo\u00df", "ich", ",", "mit", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen festen Bund zu machen \u2013", "tokens": ["Ei\u00b7nen", "fes\u00b7ten", "Bund", "zu", "ma\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwelch ein Einfall\u00ab, sprach der Fr\u00fchling,", "tokens": ["\u00bb", "welch", "ein", "Ein\u00b7fall", "\u00ab", ",", "sprach", "der", "Fr\u00fch\u00b7ling", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "ART", "NN", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbeinen armen Hund zu machen!\u00ab", "tokens": ["\u00bb", "ei\u00b7nen", "ar\u00b7men", "Hund", "zu", "ma\u00b7chen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sollte mir an einem kranken", "tokens": ["Soll\u00b7te", "mir", "an", "ei\u00b7nem", "kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+