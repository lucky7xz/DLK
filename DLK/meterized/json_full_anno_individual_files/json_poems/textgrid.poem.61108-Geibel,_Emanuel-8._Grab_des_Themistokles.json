{"textgrid.poem.61108": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8. Grab des Themistokles", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo am zackigen Fels das Gewog' sich brandend emporb\u00e4umt,", "tokens": ["Wo", "am", "za\u00b7cki\u00b7gen", "Fels", "das", "Ge\u00b7wo\u00b7g'", "sich", "bran\u00b7dend", "em\u00b7por\u00b7b\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "--+--+---+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Senkten die Freunde bei Nacht heimlich Themistokles' Leib", "tokens": ["Senk\u00b7ten", "die", "Freun\u00b7de", "bei", "Nacht", "heim\u00b7lich", "The\u00b7mi\u00b7sto\u00b7kles'", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "NE", "NN"], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "In heimatlichen Grund. Festgaben und Totengeschenke", "tokens": ["In", "hei\u00b7mat\u00b7li\u00b7chen", "Grund", ".", "Fest\u00b7ga\u00b7ben", "und", "To\u00b7ten\u00b7ge\u00b7schen\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "KON", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Brachten sie dar, und es flo\u00df reichlich die Spende des Weins.", "tokens": ["Brach\u00b7ten", "sie", "dar", ",", "und", "es", "flo\u00df", "reich\u00b7lich", "die", "Spen\u00b7de", "des", "Weins", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--++-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Aber den Zorn des verblendeten Volks kleinm\u00fctig bef\u00fcrchtend,", "tokens": ["A\u00b7ber", "den", "Zorn", "des", "ver\u00b7blen\u00b7de\u00b7ten", "Volks", "klein\u00b7m\u00fc\u00b7tig", "be\u00b7f\u00fcrch\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Stahlen sie leise sich heim, ehe die D\u00e4mmrung erschien.", "tokens": ["Stah\u00b7len", "sie", "lei\u00b7se", "sich", "heim", ",", "e\u00b7he", "die", "D\u00e4mm\u00b7rung", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "PTKVZ", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Denksteinlos nun schlummert der Held. Doch dr\u00fcben im Sp\u00e4trot", "tokens": ["Denk\u00b7stein\u00b7los", "nun", "schlum\u00b7mert", "der", "Held", ".", "Doch", "dr\u00fc\u00b7ben", "im", "Sp\u00e4t\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "$.", "KON", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+--++", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ragt ihm, ein ewiges Mal, Salamis' Felsengestad'.", "tokens": ["Ragt", "ihm", ",", "ein", "e\u00b7wi\u00b7ges", "Mal", ",", "Sa\u00b7la\u00b7mis'", "Fel\u00b7sen\u00b7ge\u00b7stad'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "NE", "NE", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": 