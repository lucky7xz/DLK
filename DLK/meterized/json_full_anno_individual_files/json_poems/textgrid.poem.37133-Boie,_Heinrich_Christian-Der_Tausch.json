, "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VMFIN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du wirst dich doch nicht scheuen,", "tokens": ["Du", "wirst", "dich", "doch", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit mir allein zu gehen?\u00ab \u2013", "tokens": ["Mit", "mir", "al\u00b7lein", "zu", "ge\u00b7hen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wir gingen. Endlich m\u00fcde", "tokens": ["Wir", "gin\u00b7gen", ".", "End\u00b7lich", "m\u00fc\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sank sie am Wasserfall.", "tokens": ["Sank", "sie", "am", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir horcheten dem Liede", "tokens": ["Wir", "hor\u00b7che\u00b7ten", "dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Der lauten Nachtigall", "tokens": ["Der", "lau\u00b7ten", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sangen auch und lauschten", "tokens": ["Und", "san\u00b7gen", "auch", "und", "lauschten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bei s\u00fc\u00dfem Spiel und Scherz,", "tokens": ["Bei", "s\u00fc\u00b7\u00dfem", "Spiel", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und k\u00fcssten und vertauschten", "tokens": ["Und", "k\u00fcss\u00b7ten", "und", "ver\u00b7tauschten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Unwi\u00dfend unser Herz.", "tokens": ["Un\u00b7wi\u00b7\u00dfend", "un\u00b7ser", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich malt ihr mein Entz\u00fccken,", "tokens": ["Ich", "malt", "ihr", "mein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als schnell an Aedons Hand,", "tokens": ["Als", "schnell", "an", "A\u00b7e\u00b7dons", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vergn\u00fcgen in den Blicken,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "in", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein M\u00e4dchen vor mir stand.", "tokens": ["Mein", "M\u00e4d\u00b7chen", "vor", "mir", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbfolgt, sagte sie, die Rache", "tokens": ["\u00bb", "folgt", ",", "sag\u00b7te", "sie", ",", "die", "Ra\u00b7che"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So pl\u00f6tzlich dem Vergehn?", "tokens": ["So", "pl\u00f6tz\u00b7lich", "dem", "Ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kaum da\u00df ich diesem lache,", "tokens": ["Kaum", "da\u00df", "ich", "die\u00b7sem", "la\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bestrafet mich Tiren!\u00ab", "tokens": ["Be\u00b7stra\u00b7fet", "mich", "Ti\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}