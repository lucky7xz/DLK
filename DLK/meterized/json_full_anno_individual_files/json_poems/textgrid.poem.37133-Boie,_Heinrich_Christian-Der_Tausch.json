{"textgrid.poem.37133": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Tausch", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbhier sollt ich sie erwarten!", "tokens": ["\u00bb", "hier", "sollt", "ich", "sie", "er\u00b7war\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verga\u00df sie Schwur und Pflicht?", "tokens": ["Ver\u00b7ga\u00df", "sie", "Schwur", "und", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Find ich im ganzen Garten", "tokens": ["Find", "ich", "im", "gan\u00b7zen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eleonoren nicht?", "tokens": ["E\u00b7leo\u00b7no\u00b7ren", "nicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "L\u00e4\u00dft dieser Schatten H\u00fclle", "tokens": ["L\u00e4\u00dft", "die\u00b7ser", "Schat\u00b7ten", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich keinen Fu\u00dftritt sehn?", "tokens": ["Mich", "kei\u00b7nen", "Fu\u00df\u00b7tritt", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dringt durch die tiefe Stille", "tokens": ["Dringt", "durch", "die", "tie\u00b7fe", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein einziges: Tiren?\u00ab", "tokens": ["Kein", "ein\u00b7zi\u00b7ges", ":", "Ti\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "$.", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sprachs und immer weiter", "tokens": ["Ich", "sprachs", "und", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sucht ich der Freundin Spur.", "tokens": ["Sucht", "ich", "der", "Freun\u00b7din", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der d\u00fcstre Mond ward heiter,", "tokens": ["Der", "d\u00fcst\u00b7re", "Mond", "ward", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch B\u00e4ume sah ich nur.", "tokens": ["Doch", "B\u00e4u\u00b7me", "sah", "ich", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jezt im Begriff zu weichen,", "tokens": ["Jezt", "im", "Be\u00b7griff", "zu", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trift ein Ger\u00e4usch mein Ohr", "tokens": ["Trift", "ein", "Ge\u00b7r\u00e4usch", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und aus den dichten Str\u00e4uchen", "tokens": ["Und", "aus", "den", "dich\u00b7ten", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Springt lachend was hervor.", "tokens": ["Springt", "la\u00b7chend", "was", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbagathe? wie?\u00ab \u2013 \u00bbVerloren", "tokens": ["\u00bb", "a\u00b7gat\u00b7he", "?", "wie", "?", "\u00ab", "\u2013", "\u00bb", "Ver\u00b7lo\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "punct", "word"], "pos": ["$(", "VVFIN", "$.", "PWAV", "$.", "$(", "$(", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab ich den Aedon hier.\u00ab \u2013", "tokens": ["Hab", "ich", "den", "A\u00b7e\u00b7don", "hier", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbund ich Eleonoren", "tokens": ["\u00bb", "und", "ich", "E\u00b7leo\u00b7no\u00b7ren"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu sehn geglaubt in dir.\u00ab \u2013", "tokens": ["Zu", "sehn", "ge\u00b7glaubt", "in", "dir", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "APPR", "PPER", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbkomm unsern ungetreuen,", "tokens": ["\u00bb", "komm", "un\u00b7sern", "un\u00b7ge\u00b7treu\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprach sie, soll Recht geschehn.", "tokens": ["Sprach", "sie", ",", "soll", "Recht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VMFIN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du wirst dich doch nicht scheuen,", "tokens": ["Du", "wirst", "dich", "doch", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit mir allein zu gehen?\u00ab \u2013", "tokens": ["Mit", "mir", "al\u00b7lein", "zu", "ge\u00b7hen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir gingen. Endlich m\u00fcde", "tokens": ["Wir", "gin\u00b7gen", ".", "End\u00b7lich", "m\u00fc\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sank sie am Wasserfall.", "tokens": ["Sank", "sie", "am", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir horcheten dem Liede", "tokens": ["Wir", "hor\u00b7che\u00b7ten", "dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Der lauten Nachtigall", "tokens": ["Der", "lau\u00b7ten", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sangen auch und lauschten", "tokens": ["Und", "san\u00b7gen", "auch", "und", "lauschten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bei s\u00fc\u00dfem Spiel und Scherz,", "tokens": ["Bei", "s\u00fc\u00b7\u00dfem", "Spiel", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und k\u00fcssten und vertauschten", "tokens": ["Und", "k\u00fcss\u00b7ten", "und", "ver\u00b7tauschten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Unwi\u00dfend unser Herz.", "tokens": ["Un\u00b7wi\u00b7\u00dfend", "un\u00b7ser", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich malt ihr mein Entz\u00fccken,", "tokens": ["Ich", "malt", "ihr", "mein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als schnell an Aedons Hand,", "tokens": ["Als", "schnell", "an", "A\u00b7e\u00b7dons", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+",