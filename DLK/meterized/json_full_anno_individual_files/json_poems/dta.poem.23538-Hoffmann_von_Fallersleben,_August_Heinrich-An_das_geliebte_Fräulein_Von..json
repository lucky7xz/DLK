{"dta.poem.23538": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An das geliebte Fr\u00e4ulein Von.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Berg' und Meere trennen mich,", "tokens": ["Nicht", "Ber\u00b7g'", "und", "Mee\u00b7re", "tren\u00b7nen", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich trennt ein Wort von dir:", "tokens": ["Mich", "trennt", "ein", "Wort", "von", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist von Staub und Staub bin ich \u2014", "tokens": ["Du", "bist", "von", "Staub", "und", "Staub", "bin", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VAFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das trennet dich von mir.", "tokens": ["Das", "tren\u00b7net", "dich", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sitzen magst du neben mir,", "tokens": ["Und", "sit\u00b7zen", "magst", "du", "ne\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nahe sein um mich:", "tokens": ["Und", "na\u00b7he", "sein", "um", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wort, es trennet mich von dir,", "tokens": ["Ein", "Wort", ",", "es", "tren\u00b7net", "mich", "von", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ewig fern bin ich.", "tokens": ["Und", "e\u00b7wig", "fern", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Erlisch nun Sonn' und Sternenzelt", "tokens": ["Er\u00b7lisch", "nun", "Sonn'", "und", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Nacht und Nebelgraus!", "tokens": ["In", "Nacht", "und", "Ne\u00b7bel\u00b7graus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn alle Liebe dieser Welt,", "tokens": ["Denn", "al\u00b7le", "Lie\u00b7be", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie l\u00f6scht das Wort nicht aus.", "tokens": ["Sie", "l\u00f6scht", "das", "Wort", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}