{"textgrid.poem.49706": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Heimarbeit", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter n\u00e4ht, die Tochter n\u00e4ht,", "tokens": ["Die", "Mut\u00b7ter", "n\u00e4ht", ",", "die", "Toch\u00b7ter", "n\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wurde fr\u00fch und es wurde sp\u00e4t,", "tokens": ["Es", "wur\u00b7de", "fr\u00fch", "und", "es", "wur\u00b7de", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Mondlicht schien zum Fenster herein,", "tokens": ["Das", "Mond\u00b7licht", "schien", "zum", "Fens\u00b7ter", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dann kam der d\u00e4mmernde Morgenschein.", "tokens": ["Dann", "kam", "der", "d\u00e4m\u00b7mern\u00b7de", "Mor\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Mutter seufzt, die Tochter g\u00e4hnt,", "tokens": ["Die", "Mut\u00b7ter", "seufzt", ",", "die", "Toch\u00b7ter", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat sich m\u00fcde zur\u00fcckgelehnt;", "tokens": ["Sie", "hat", "sich", "m\u00fc\u00b7de", "zu\u00b7r\u00fcck\u00b7ge\u00b7lehnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Rad geht um, die Nadel tickt,", "tokens": ["Das", "Rad", "geht", "um", ",", "die", "Na\u00b7del", "tickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie w\u00e4re beinahe eingenickt.", "tokens": ["Sie", "w\u00e4\u00b7re", "bei\u00b7na\u00b7he", "ein\u00b7ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie redet m\u00fcde vor sich hin:", "tokens": ["Sie", "re\u00b7det", "m\u00fc\u00b7de", "vor", "sich", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach ja, die edle Kaiserin,", "tokens": ["\u00bb", "ach", "ja", ",", "die", "ed\u00b7le", "Kai\u00b7se\u00b7rin", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wei\u00df es jetzt, wir sind schlimm dran,", "tokens": ["Sie", "wei\u00df", "es", "jetzt", ",", "wir", "sind", "schlimm", "dran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagt es heute noch ihrem Mann.", "tokens": ["Und", "sagt", "es", "heu\u00b7te", "noch", "ih\u00b7rem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und, Mutter, wenn sie nun mit ihm spricht,", "tokens": ["Und", ",", "Mut\u00b7ter", ",", "wenn", "sie", "nun", "mit", "ihm", "spricht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er soll uns helfen, so glaubst du nicht,", "tokens": ["Er", "soll", "uns", "hel\u00b7fen", ",", "so", "glaubst", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er wird uns helfen? Wenn er nur h\u00f6rt,", "tokens": ["Er", "wird", "uns", "hel\u00b7fen", "?", "Wenn", "er", "nur", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Was seine Frau im Herzen emp\u00f6rt?\u00ab", "tokens": ["Was", "sei\u00b7ne", "Frau", "im", "Her\u00b7zen", "em\u00b7p\u00f6rt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPRART", "NN"