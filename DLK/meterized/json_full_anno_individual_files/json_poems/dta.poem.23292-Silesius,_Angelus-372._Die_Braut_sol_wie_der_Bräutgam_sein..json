{"dta.poem.23292": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "372. Die Braut sol wie der Br\u00e4utgam  \n sein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df verwundet sein. Warumb? weil voller", "tokens": ["Ich", "mu\u00df", "ver\u00b7wun\u00b7det", "sein", ".", "Wa\u00b7rumb", "?", "weil", "vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$.", "PWAV", "$.", "KOUS", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wunden", "tokens": ["Wun\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mein ewger Br\u00e4utigam der Heyland wird gefunden:", "tokens": ["Mein", "ew\u00b7ger", "Br\u00e4u\u00b7ti\u00b7gam", "der", "Hey\u00b7land", "wird", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Nutzen bringt es dir? Es stehet gar nicht fein/", "tokens": ["Was", "Nut\u00b7zen", "bringt", "es", "dir", "?", "Es", "ste\u00b7het", "gar", "nicht", "fein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn Braut und Br\u00e4utigam einander ungleich sein.", "tokens": ["Wenn", "Braut", "und", "Br\u00e4u\u00b7ti\u00b7gam", "ein\u00b7an\u00b7der", "un\u