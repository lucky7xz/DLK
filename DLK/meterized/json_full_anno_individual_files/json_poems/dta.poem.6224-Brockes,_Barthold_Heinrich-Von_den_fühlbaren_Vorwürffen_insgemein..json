{"dta.poem.6224": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bishero haben wir besehen", "tokens": ["Bis\u00b7he\u00b7ro", "ha\u00b7ben", "wir", "be\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die allgemeinen Ding\u2019, die Ordnung, das Bewegen,", "tokens": ["Die", "all\u00b7ge\u00b7mei\u00b7nen", "Ding'", ",", "die", "Ord\u00b7nung", ",", "das", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So uns des Firmamentes H\u00f6hen", "tokens": ["So", "uns", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unverr\u00fccktem Lauff f\u00fcr Augen legen,", "tokens": ["In", "un\u00b7ver\u00b7r\u00fcck\u00b7tem", "Lauff", "f\u00fcr", "Au\u00b7gen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die richtig\u2019 Aenderung der Tag und Zeiten,", "tokens": ["Die", "rich\u00b7tig'", "A\u00b7en\u00b7de\u00b7rung", "der", "Tag", "und", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Durch was f\u00fcr Handlungen die C\u00f6rper sich bereiten", "tokens": ["Durch", "was", "f\u00fcr", "Hand\u00b7lun\u00b7gen", "die", "C\u00f6r\u00b7per", "sich", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "ART", "NN", "PRF", "VVINF"], "meter": "+--++--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Und Wechsels-Weise bald sich \u00e4ndern, bald vergehn;", "tokens": ["Und", "Wech\u00b7sels\u00b7Wei\u00b7se", "bald", "sich", "\u00e4n\u00b7dern", ",", "bald", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PRF", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}