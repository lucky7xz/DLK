{"textgrid.poem.51295": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lobe diese Zeit. Dann solt ich sie wol kennen,", "tokens": ["Ich", "lo\u00b7be", "die\u00b7se", "Zeit", ".", "Dann", "solt", "ich", "sie", "wol", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$.", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und b\u00f6\u00df' und ungerecht ohn Gottesl\u00e4stern nennen?", "tokens": ["Und", "b\u00f6\u00df'", "und", "un\u00b7ge\u00b7recht", "ohn", "Got\u00b7tes\u00b7l\u00e4s\u00b7tern", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er, de\u00dfen Macht und St\u00e4rck' ein ieder Wurm erh\u00f6ht,", "tokens": ["Er", ",", "de\u00b7\u00dfen", "Macht", "und", "St\u00e4rck", "ein", "ie\u00b7der", "Wurm", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "KON", "NN", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ein iedes Kraut beweist, ein ieder Stern versteht.", "tokens": ["Ein", "ie\u00b7des", "Kraut", "be\u00b7weist", ",", "ein", "ie\u00b7der", "Stern", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er, ohne de\u00dfen Wort kein Sperling f\u00e4llt zur Erden;", "tokens": ["Er", ",", "oh\u00b7ne", "de\u00b7\u00dfen", "Wort", "kein", "Sper\u00b7ling", "f\u00e4llt", "zur", "Er\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "ART", "NN", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er, ohne de\u00dfen Rath kein Haar versehrt kan werden.", "tokens": ["Er", ",", "oh\u00b7ne", "de\u00b7\u00dfen", "Rath", "kein", "Haar", "ver\u00b7sehrt", "kan", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "ART", "NN", "PIAT", "NN", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er, solt' er diesen Lauff, der ohn ihn steht und f\u00e4llt,", "tokens": ["Er", ",", "solt'", "er", "die\u00b7sen", "Lauff", ",", "der", "ohn", "ihn", "steht", "und", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VMFIN", "PPER", "PDAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht sehen, ordnen, f\u00fchrn der Zeit Geschicht und Welt,", "tokens": ["Nicht", "se\u00b7hen", ",", "ord\u00b7nen", ",", "f\u00fchrn", "der", "Zeit", "Ge\u00b7schicht", "und", "Welt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "VVINF", "$,", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann es von Gott dann k\u00f6mmt: so ist es gut vor allen:", "tokens": ["Wann", "es", "von", "Gott", "dann", "k\u00f6mmt", ":", "so", "ist", "es", "gut", "vor", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und der was be\u00dfers hofft ist schon von Gott gefallen.", "tokens": ["Und", "der", "was", "be\u00b7\u00dfers", "hofft", "ist", "schon", "von", "Gott", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PWS", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Das H\u00f6chst' ist Gottes Rath, das best' ist, den erkannt:", "tokens": ["Das", "H\u00f6chst'", "ist", "Got\u00b7tes", "Rath", ",", "das", "best'", "ist", ",", "den", "er\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Seeligst' allzeit seyn auff seinen Schlu\u00df gewandt.", "tokens": ["Das", "See\u00b7ligst'", "all\u00b7zeit", "seyn", "auff", "sei\u00b7nen", "Schlu\u00df", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du Hand voll Asch und Staub. Sol Gott nach deinem Dencken,", "tokens": ["Du", "Hand", "voll", "Asch", "und", "Staub", ".", "Sol", "Gott", "nach", "dei\u00b7nem", "Den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "KON", "NN", "$.", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gott, der dich seelig macht, sein ew'ge Ordnung lencken?", "tokens": ["Gott", ",", "der", "dich", "see\u00b7lig", "macht", ",", "sein", "ew'\u00b7ge", "Ord\u00b7nung", "len\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Geh' in dich, schau wie dir dein eigne Hoffnung fehlt,", "tokens": ["Geh'", "in", "dich", ",", "schau", "wie", "dir", "dein", "eig\u00b7ne", "Hoff\u00b7nung", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "ADJD", "KOKOM", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Dein eigner Sinn dich ta\u00fcscht; dein eigner Trost dich qu\u00e4lt.", "tokens": ["Dein", "eig\u00b7ner", "Sinn", "dich", "ta\u00fcscht", ";", "dein", "eig\u00b7ner", "Trost", "dich", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie recht ists seinem Gott im Creutze stille stehen,", "tokens": ["Wie", "recht", "ists", "sei\u00b7nem", "Gott", "im", "Creut\u00b7ze", "stil\u00b7le", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dann, was dich trifft, das mu\u00df vor durch sein Hertze gehen:", "tokens": ["Dann", ",", "was", "dich", "trifft", ",", "das", "mu\u00df", "vor", "durch", "sein", "Hert\u00b7ze", "ge\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie frey ists, seinem Gott auffladen alle Last,", "tokens": ["Wie", "frey", "ists", ",", "sei\u00b7nem", "Gott", "auf\u00b7fla\u00b7den", "al\u00b7le", "Last", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie wei\u00df ists'. So du Gott sein Thun gebillicht hast.", "tokens": ["Wie", "wei\u00df", "ist\u00b7s'", ".", "So", "du", "Gott", "sein", "Thun", "ge\u00b7bil\u00b7licht", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKVZ", "$.", "ADV", "PPER", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Wie gut ists, sich getrost in Gottes Willen sencken,", "tokens": ["Wie", "gut", "ists", ",", "sich", "ge\u00b7trost", "in", "Got\u00b7tes", "Wil\u00b7len", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PRF", "ADJD", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ohn den du nicht einmal kanst an den Himmel dencken.", "tokens": ["Ohn", "den", "du", "nicht", "ein\u00b7mal", "kanst", "an", "den", "Him\u00b7mel", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PTKNEG", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Mensch! Wilstu ja gerecht, frey, gut und weise seyn,", "tokens": ["Mensch", "!", "Wils\u00b7tu", "ja", "ge\u00b7recht", ",", "frey", ",", "gut", "und", "wei\u00b7se", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So lebe diese Zeit mit mir und Gott allein.", "tokens": ["So", "le\u00b7be", "die\u00b7se", "Zeit", "mit", "mir", "und", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPR", "PPER", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Folg' ihm, er ist gerecht, frey; sey ihm gantz ergeben,", "tokens": ["Fol\u00b7g'", "ihm", ",", "er", "ist", "ge\u00b7recht", ",", "frey", ";", "sey", "ihm", "gantz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "$.", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "Weis' eher sein Gebot: Gut: stell ihm heim dein Leben.", "tokens": ["Weis'", "e\u00b7her", "sein", "Ge\u00b7bot", ":", "Gut", ":", "stell", "ihm", "heim", "dein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$.", "ADJD", "$.", "ADJD", "PPER", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich lobe diese Zeit. Dann solt ich sie wol kennen,", "tokens": ["Ich", "lo\u00b7be", "die\u00b7se", "Zeit", ".", "Dann", "solt", "ich", "sie", "wol", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$.", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und b\u00f6\u00df' und ungerecht ohn Gottesl\u00e4stern nennen?", "tokens": ["Und", "b\u00f6\u00df'", "und", "un\u00b7ge\u00b7recht", "ohn", "Got\u00b7tes\u00b7l\u00e4s\u00b7tern", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er, de\u00dfen Macht und St\u00e4rck' ein ieder Wurm erh\u00f6ht,", "tokens": ["Er", ",", "de\u00b7\u00dfen", "Macht", "und", "St\u00e4rck", "ein", "ie\u00b7der", "Wurm", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "KON", "NN", "ART", "PIAT", "NN", "VVPP", "$,"], "met