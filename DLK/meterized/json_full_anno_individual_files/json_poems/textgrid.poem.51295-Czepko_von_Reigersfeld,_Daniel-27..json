: "alexandrine.iambic.hexa"}, "line.25": {"text": "Folg' ihm, er ist gerecht, frey; sey ihm gantz ergeben,", "tokens": ["Fol\u00b7g'", "ihm", ",", "er", "ist", "ge\u00b7recht", ",", "frey", ";", "sey", "ihm", "gantz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "$.", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "Weis' eher sein Gebot: Gut: stell ihm heim dein Leben.", "tokens": ["Weis'", "e\u00b7her", "sein", "Ge\u00b7bot", ":", "Gut", ":", "stell", "ihm", "heim", "dein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$.", "ADJD", "$.", "ADJD", "PPER", "PTKVZ", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}