{"dta.poem.20071": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcrde mich in M\u00e4hrchentr\u00e4umen wiegen,", "tokens": ["Ich", "w\u00fcr\u00b7de", "mich", "in", "M\u00e4hr\u00b7chen\u00b7tr\u00e4u\u00b7men", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lerchenfroh begr\u00fc\u00dfen jeden Morgen,", "tokens": ["Und", "ler\u00b7chen\u00b7froh", "be\u00b7gr\u00fc\u00b7\u00dfen", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6nnt\u2019 ich den irdisch\u2019sten der Erdensorgen", "tokens": ["K\u00f6nnt'", "ich", "den", "ir\u00b7disch'\u00b7sten", "der", "Er\u00b7den\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebieten, sich zu F\u00fc\u00dfen mir zu schmiegen.", "tokens": ["Ge\u00b7bie\u00b7ten", ",", "sich", "zu", "F\u00fc\u00b7\u00dfen", "mir", "zu", "schmie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir ist als m\u00fc\u00dft\u2019 ich durch die L\u00fcfte fliegen,", "tokens": ["Mir", "ist", "als", "m\u00fc\u00dft'", "ich", "durch", "die", "L\u00fcf\u00b7te", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als w\u00fcrde mir die Freude Fl\u00fcgel borgen,", "tokens": ["Als", "w\u00fcr\u00b7de", "mir", "die", "Freu\u00b7de", "Fl\u00fc\u00b7gel", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verm\u00f6cht ich je, gleich jenem Sankt Georgen,", "tokens": ["Ver\u00b7m\u00f6cht", "ich", "je", ",", "gleich", "je\u00b7nem", "Sankt", "Ge\u00b7or\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "PDAT", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Noth \u2014 den ew\u2019gen Drachen zu besiegen.", "tokens": ["Die", "Noth", "den", "ew'\u00b7gen", "Dra\u00b7chen", "zu", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch ob das Gl\u00fcck mir auch ein d\u00fcrrer Bronnen,", "tokens": ["Doch", "ob", "das", "Gl\u00fcck", "mir", "auch", "ein", "d\u00fcr\u00b7rer", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ob ich auch entbehren mag und leiden,", "tokens": ["Und", "ob", "ich", "auch", "ent\u00b7beh\u00b7ren", "mag", "und", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe doch das beste Theil gewonnen.", "tokens": ["Ich", "ha\u00b7be", "doch", "das", "bes\u00b7te", "Theil", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sollt\u2019 ich, diese Stunde noch, entscheiden", "tokens": ["Und", "sollt'", "ich", ",", "die\u00b7se", "Stun\u00b7de", "noch", ",", "ent\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "PDAT", "NN", "ADV", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich zwischen Dir und einer Welt voll Wonnen,", "tokens": ["Mich", "zwi\u00b7schen", "Dir", "und", "ei\u00b7ner", "Welt", "voll", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es bliebe doch beim Alten mit uns Beiden.", "tokens": ["Es", "blie\u00b7be", "doch", "beim", "Al\u00b7ten", "mit", "uns", "Bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}