t"], "pos": ["KON", "PWAV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rausche, Gesang, mir in andern T\u00f6nen!", "tokens": ["Rau\u00b7sche", ",", "Ge\u00b7sang", ",", "mir", "in", "an\u00b7dern", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Mit Hohngelache seh' ihn der G\u00e4nsehirt!", "tokens": ["Mit", "Hohn\u00b7ge\u00b7la\u00b7che", "seh'", "ihn", "der", "G\u00e4n\u00b7se\u00b7hirt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Buchstabierer, welcher die Wichter bl\u00e4ut,", "tokens": ["Der", "Buch\u00b7sta\u00b7bie\u00b7rer", ",", "wel\u00b7cher", "die", "Wich\u00b7ter", "bl\u00e4ut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Hohngelach! doch kaltverachtend,", "tokens": ["Mit", "Hohn\u00b7ge\u00b7lach", "!", "doch", "kalt\u00b7ver\u00b7ach\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer vor der \u00c4hre die Sense wetzet.", "tokens": ["Wer", "vor", "der", "\u00c4h\u00b7re", "die", "Sen\u00b7se", "wet\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Vom