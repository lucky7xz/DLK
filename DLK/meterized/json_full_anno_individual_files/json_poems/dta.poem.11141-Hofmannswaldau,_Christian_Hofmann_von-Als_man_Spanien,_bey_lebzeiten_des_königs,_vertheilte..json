{"dta.poem.11141": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als man Spanien, bey lebzeiten des  \n k\u00f6nigs, vertheilte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der adler sucht ein nest, die lilgen einen garten,", "tokens": ["Der", "ad\u00b7ler", "sucht", "ein", "nest", ",", "die", "lil\u00b7gen", "ei\u00b7nen", "gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "VVFIN", "$,", "PRELS", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die leuen eineu raub, Sanct Peter noch ein schwerdt.", "tokens": ["Die", "leu\u00b7en", "ei\u00b7neu", "raub", ",", "Sanct", "Pe\u00b7ter", "noch", "ein", "schwerdt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "VVFIN", "NE", "ADV", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spielet man um dich, und mischet schon die karten,", "tokens": ["So", "spie\u00b7let", "man", "um", "dich", ",", "und", "mi\u00b7schet", "schon", "die", "kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "$,", "KON", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geht dir der k\u00f6nig ab, so ist martsch ungewehrt.", "tokens": ["Geht", "dir", "der", "k\u00f6\u00b7nig", "ab", ",", "so", "ist", "martsch", "un\u00b7ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "PTKVZ", "$,", "ADV", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}