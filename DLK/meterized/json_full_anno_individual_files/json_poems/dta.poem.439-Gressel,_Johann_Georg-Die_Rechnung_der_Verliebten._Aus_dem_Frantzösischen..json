{"dta.poem.439": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Rechnung der Verliebten.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So wie das neue Tuch vom Wasser lauffet ein/", "tokens": ["So", "wie", "das", "neu\u00b7e", "Tuch", "vom", "Was\u00b7ser", "lauf\u00b7fet", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So pflegt die Rechnung auch/ die sich Verliebte machen/", "tokens": ["So", "pflegt", "die", "Rech\u00b7nung", "auch", "/", "die", "sich", "Ver\u00b7lieb\u00b7te", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$(", "PRELS", "PRF", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn mans beym Licht besieht gar offte falsch zu seyn/", "tokens": ["Wenn", "mans", "beym", "Licht", "be\u00b7sieht", "gar", "off\u00b7te", "falsch", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Kluger mu\u00df f\u00fcrwahr der albern Thorheit lachen.", "tokens": ["Ein", "Klu\u00b7ger", "mu\u00df", "f\u00fcr\u00b7wahr", "der", "al\u00b7bern", "Thor\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn die Gedancken gehn auf Himmel hohen Spitzen/", "tokens": ["Denn", "die", "Ge\u00b7dan\u00b7cken", "gehn", "auf", "Him\u00b7mel", "ho\u00b7hen", "Spit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da sie doch in der That in tieffen Th\u00e4lern sitzen.", "tokens": ["Da", "sie", "doch", "in", "der", "That", "in", "tief\u00b7fen", "Th\u00e4\u00b7lern", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}