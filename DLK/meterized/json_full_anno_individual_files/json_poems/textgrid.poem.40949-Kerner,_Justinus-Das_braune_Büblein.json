{"textgrid.poem.40949": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das braune B\u00fcblein", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich freute herzlich, herzlich, traun!", "tokens": ["Mich", "freu\u00b7te", "herz\u00b7lich", ",", "herz\u00b7lich", ",", "traun", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft ich es gesehen,", "tokens": ["So", "oft", "ich", "es", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stets springen, niemals gehen,", "tokens": ["Stets", "sprin\u00b7gen", ",", "nie\u00b7mals", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein B\u00fcblein, ganz kastanienbraun.", "tokens": ["Ein", "B\u00fcb\u00b7lein", ",", "ganz", "kas\u00b7ta\u00b7ni\u00b7en\u00b7braun", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es war so frisch, so munter, ach!", "tokens": ["Es", "war", "so", "frisch", ",", "so", "mun\u00b7ter", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam wo ein Pferd gesprungen,", "tokens": ["Kam", "wo", "ein", "Pferd", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sah man den braunen Jungen", "tokens": ["Sah", "man", "den", "brau\u00b7nen", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm rennen wie ein Windspiel nach.", "tokens": ["Ihm", "ren\u00b7nen", "wie", "ein", "Wind\u00b7spiel", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo ist dein B\u00fcblein, Mutter? \u2013 weh!", "tokens": ["Wo", "ist", "dein", "B\u00fcb\u00b7lein", ",", "Mut\u00b7ter", "?", "\u2013", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du f\u00fchrst mich in die Kammer,", "tokens": ["Du", "f\u00fchrst", "mich", "in", "die", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da liegt \u2013 o herber Jammer! \u2013", "tokens": ["Da", "liegt", "\u2013", "o", "her\u00b7ber", "Jam\u00b7mer", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein braunes B\u00fcblein wei\u00df wie Schnee.", "tokens": ["Dein", "brau\u00b7nes", "B\u00fcb\u00b7lein", "wei\u00df", "wie", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Wind weht durch sein Sarggewand,", "tokens": ["Der", "Wind", "weht", "durch", "sein", "Sarg\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fenster stehen offen,", "tokens": ["Die", "Fens\u00b7ter", "ste\u00b7hen", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Uhr ist abgeloffen,", "tokens": ["Die", "Uhr", "ist", "ab\u00b7ge\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tautropfen rinnen ab die Wand.", "tokens": ["Tau\u00b7trop\u00b7fen", "rin\u00b7nen", "ab", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Mich freute herzlich, herzlich, traun!", "tokens": ["Mich", "freu\u00b7te", "herz\u00b7lich", ",", "herz\u00b7lich", ",", "traun", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft ich es gesehen,", "tokens": ["So", "oft", "ich", "es", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER