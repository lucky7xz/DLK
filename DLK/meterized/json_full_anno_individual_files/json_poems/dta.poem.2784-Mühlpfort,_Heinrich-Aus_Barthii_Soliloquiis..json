{"dta.poem.2784": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus   Barthii Soliloquiis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich nur hoffen kan/ was ich nur kan begehren/", "tokens": ["Was", "ich", "nur", "hof\u00b7fen", "kan", "/", "was", "ich", "nur", "kan", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein JEsu/ Sieges-F\u00fcrst/ das \u00fcberbring ich dir.", "tokens": ["Mein", "Je\u00b7su", "/", "Sie\u00b7ges\u00b7F\u00fcrst", "/", "das", "\u00fc\u00b7berb\u00b7ring", "ich", "dir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$(", "NE", "$(", "PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wil die Seele selbst zum Unterpfand gew\u00e4hren/", "tokens": ["Ich", "wil", "die", "See\u00b7le", "selbst", "zum", "Un\u00b7ter\u00b7pfand", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Leben das ist dein/ und ich bin ausser mir.", "tokens": ["Mein", "Le\u00b7ben", "das", "ist", "dein", "/", "und", "ich", "bin", "aus\u00b7ser", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "PPOSAT", "$(", "KON", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach der du diesen Tag von Todten aufferstanden/", "tokens": ["Ach", "der", "du", "die\u00b7sen", "Tag", "von", "Tod\u00b7ten", "auf\u00b7fer\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "PPER", "PDAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zeuch mich mit deinem Arm aus meiner S\u00fcnden Nacht.", "tokens": ["Zeuch", "mich", "mit", "dei\u00b7nem", "Arm", "aus", "mei\u00b7ner", "S\u00fcn\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach w\u00fcrcke doch in mir/ ich bin/ mein GOtt/ vorhanden;", "tokens": ["Ach", "w\u00fcr\u00b7cke", "doch", "in", "mir", "/", "ich", "bin", "/", "mein", "Gott", "/", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPER", "$(", "PPER", "VAFIN", "$(", "PPOSAT", "NN", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und bitt/ es werd\u2019 aus mir ein ander Mensch gemacht.", "tokens": ["Und", "bitt", "/", "es", "werd'", "aus", "mir", "ein", "an\u00b7der", "Mensch", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ich mir vor gefiel in nichts/ als b\u00f6sen Wercken/", "tokens": ["Der", "ich", "mir", "vor", "ge\u00b7fiel", "in", "nichts", "/", "als", "b\u00f6\u00b7sen", "Wer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "VVFIN", "APPR", "PIS", "$(", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gib da\u00df ich dir allein nur wohlgefallen mag!", "tokens": ["Gib", "da\u00df", "ich", "dir", "al\u00b7lein", "nur", "wohl\u00b7ge\u00b7fal\u00b7len", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein Aufferstehungs-Fest/ das wolle mich so st\u00e4rcken/", "tokens": ["Dein", "Auf\u00b7fer\u00b7ste\u00b7hungs\u00b7Fest", "/", "das", "wol\u00b7le", "mich", "so", "st\u00e4r\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df ich zu keinem Ding mehr ein Verlangen trag.", "tokens": ["Da\u00df", "ich", "zu", "kei\u00b7nem", "Ding", "mehr", "ein", "Ver\u00b7lan\u00b7gen", "trag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und da\u00df mir alles stinckt/ und da\u00df ich alles hasse/", "tokens": ["Und", "da\u00df", "mir", "al\u00b7les", "stinckt", "/", "und", "da\u00df", "ich", "al\u00b7les", "has\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$(", "KON", "KOUS", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was dir zuwider ist/ und wider dein Gebot:", "tokens": ["Was", "dir", "zu\u00b7wi\u00b7der", "ist", "/", "und", "wi\u00b7der", "dein", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$(", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hergegen dich/ O HErr/ in solchem Glauben fasse/", "tokens": ["Her\u00b7ge\u00b7gen", "dich", "/", "O", "Herr", "/", "in", "sol\u00b7chem", "Glau\u00b7ben", "fas\u00b7se", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NE", "NN", "$(", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df ich stets sagen kan/ mein h\u00f6chster Schatz ist GOtt.", "tokens": ["Da\u00df", "ich", "stets", "sa\u00b7gen", "kan", "/", "mein", "h\u00f6chs\u00b7ter", "Schatz", "ist", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.17": {"text": "Ach k\u00f6nt ich mein Gem\u00fcth und Regungen der Sinnen", "tokens": ["Ach", "k\u00f6nt", "ich", "mein", "Ge\u00b7m\u00fcth", "und", "Re\u00b7gun\u00b7gen", "der", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Verlassen! weil sie nur sind St\u00f6rer meiner Ruh!", "tokens": ["Ver\u00b7las\u00b7sen", "!", "weil", "sie", "nur", "sind", "St\u00f6\u00b7rer", "mei\u00b7ner", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KOUS", "PPER", "ADV", "VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ja diesen Augenblick w\u00fcnsch ich zu ziehn von hinnen/", "tokens": ["Ja", "die\u00b7sen", "Au\u00b7gen\u00b7blick", "w\u00fcnsch", "ich", "zu", "ziehn", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So bald mein Heyland nur dein Wille k\u00e4m darzu/", "tokens": ["So", "bald", "mein", "Hey\u00b7land", "nur", "dein", "Wil\u00b7le", "k\u00e4m", "dar\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Damit ich mich aufs neu mit S\u00fcnden nicht befleckte/", "tokens": ["Da\u00b7mit", "ich", "mich", "aufs", "neu", "mit", "S\u00fcn\u00b7den", "nicht", "be\u00b7fleck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJD", "APPR", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vergesse den Beruff da ich dir dienen mu\u00df;", "tokens": ["Ver\u00b7ges\u00b7se", "den", "Be\u00b7ruff", "da", "ich", "dir", "die\u00b7nen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und wieder in die Welt mein eitles Auge steckte/", "tokens": ["Und", "wie\u00b7der", "in", "die", "Welt", "mein", "eit\u00b7les", "Au\u00b7ge", "steck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wo Laster/ S\u00fcnd und Schand im gr\u00f6stem Uberflu\u00df.", "tokens": ["Wo", "Las\u00b7ter", "/", "S\u00fcnd", "und", "Schand", "im", "gr\u00f6s\u00b7tem", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mein Heyland halte mich! Ach mein Erretter rette!", "tokens": ["Mein", "Hey\u00b7land", "hal\u00b7te", "mich", "!", "Ach", "mein", "Er\u00b7ret\u00b7ter", "ret\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "ITJ", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.26": {"text": "Schlag die Begierden todt; Gib da\u00df von dieser Zeit", "tokens": ["Schlag", "die", "Be\u00b7gier\u00b7den", "todt", ";", "Gib", "da\u00df", "von", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "$.", "VVIMP", "KOUS", "APPR", "PDAT", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Ich nimmermehr gedenck an mein als S\u00fcnden-Bette/", "tokens": ["Ich", "nim\u00b7mer\u00b7mehr", "ge\u00b7denck", "an", "mein", "als", "S\u00fcn\u00b7den\u00b7Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPOSAT", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nur einzig aber dir zu dienen bin bereit.", "tokens": ["Nur", "ein\u00b7zig", "a\u00b7ber", "dir", "zu", "die\u00b7nen", "bin", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "PPER", "PTKZU", "VVINF", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df ich auf ewig nicht mit jemand m\u00f6ge zancken/", "tokens": ["Da\u00df", "ich", "auf", "e\u00b7wig", "nicht", "mit", "je\u00b7mand", "m\u00f6\u00b7ge", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "PTKNEG", "APPR", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df ich nichts begehr was irdisch ist/ und heist.", "tokens": ["Und", "da\u00df", "ich", "nichts", "be\u00b7gehr", "was", "ir\u00b7disch", "ist", "/", "und", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "PWS", "ADJD", "VAFIN", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df mein Verbrechen ich stets f\u00fchr\u2019 in den Gedancken", "tokens": ["Da\u00df", "mein", "Ver\u00b7bre\u00b7chen", "ich", "stets", "f\u00fchr'", "in", "den", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und da\u00df mein reuig Hertz die Thr\u00e4nen dr\u00fcber geust.", "tokens": ["Und", "da\u00df", "mein", "reu\u00b7ig", "Hertz", "die", "Thr\u00e4\u00b7nen", "dr\u00fc\u00b7ber", "geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJD", "NN", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich thu dem Nechsten Guts/ und wil nun niemand schaden.", "tokens": ["Ich", "thu", "dem", "Nechs\u00b7ten", "Guts", "/", "und", "wil", "nun", "nie\u00b7mand", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KON", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich g\u00f6nn auch jederman/ was ich mir selbst erwehlt.", "tokens": ["Ich", "g\u00f6nn", "auch", "je\u00b7der\u00b7man", "/", "was", "ich", "mir", "selbst", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$(", "PWS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Nachdem ich theilhafft bin der Aufferstehungs-Gnaden/", "tokens": ["Nach\u00b7dem", "ich", "theil\u00b7hafft", "bin", "der", "Auf\u00b7fer\u00b7ste\u00b7hungs\u00b7Gna\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So bin ich aufferweckt/ und dir auch zugezehlt.", "tokens": ["So", "bin", "ich", "auf\u00b7fer\u00b7weckt", "/", "und", "dir", "auch", "zu\u00b7ge\u00b7zehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$(", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Di\u00df Wollen kanst du doch/ mein GOtt/ in mir vollbringen:", "tokens": ["Di\u00df", "Wol\u00b7len", "kanst", "du", "doch", "/", "mein", "Gott", "/", "in", "mir", "voll\u00b7brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich wei\u00df/ ich f\u00fchl\u2019 es auch; denn ausser deiner Gnad", "tokens": ["Ich", "wei\u00df", "/", "ich", "f\u00fchl'", "es", "auch", ";", "denn", "aus\u00b7ser", "dei\u00b7ner", "Gnad"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$.", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kan solche Lebens-Art unm\u00f6glich mir gelingen.", "tokens": ["Kan", "sol\u00b7che", "Le\u00b7bens\u00b7Art", "un\u00b7m\u00f6g\u00b7lich", "mir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und weil dein Wort ins Hertz mir di\u00df geleget hat;", "tokens": ["Und", "weil", "dein", "Wort", "ins", "Hertz", "mir", "di\u00df", "ge\u00b7le\u00b7get", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "PPER", "PDS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja diese Zeilen auch/ so ich jetzunder schreibe/", "tokens": ["Ja", "die\u00b7se", "Zei\u00b7len", "auch", "/", "so", "ich", "je\u00b7tzun\u00b7der", "schrei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "NN", "ADV", "$(", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Dein Geist in mir erweckt/ so w\u00fcrcke doch in mir/", "tokens": ["Dein", "Geist", "in", "mir", "er\u00b7weckt", "/", "so", "w\u00fcr\u00b7cke", "doch", "in", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP", "$(", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df ich ein brennend Tacht in meinem Leben bleibe/", "tokens": ["Da\u00df", "ich", "ein", "bren\u00b7nend", "Tacht", "in", "mei\u00b7nem", "Le\u00b7ben", "blei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und di\u00df ins k\u00fcnfftige mir kr\u00e4fftig setze f\u00fcr/", "tokens": ["Und", "di\u00df", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "mir", "kr\u00e4ff\u00b7tig", "set\u00b7ze", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPRART", "ADJA", "PPER", "ADJD", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da\u00df nicht ein ander Schall aus meinen Lippen gehe;", "tokens": ["Da\u00df", "nicht", "ein", "an\u00b7der", "Schall", "aus", "mei\u00b7nen", "Lip\u00b7pen", "ge\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df ich nichts reden mag/ als was dein Lob betrifft;", "tokens": ["Da\u00df", "ich", "nichts", "re\u00b7den", "mag", "/", "als", "was", "dein", "Lob", "be\u00b7tr\u00b7ifft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$(", "KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Da\u00df ich/ Erstandener/ mit dir so aufferstehe/", "tokens": ["Da\u00df", "ich", "/", "Er\u00b7stan\u00b7de\u00b7ner", "/", "mit", "dir", "so", "auf\u00b7fer\u00b7ste\u00b7he", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und in dem starcken Trost/ trotz auch der H\u00f6llen Gifft.", "tokens": ["Und", "in", "dem", "star\u00b7cken", "Trost", "/", "trotz", "auch", "der", "H\u00f6l\u00b7len", "Gifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$(", "APPR", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Mein Name der ist ja in deine Hand geschrieben/", "tokens": ["Mein", "Na\u00b7me", "der", "ist", "ja", "in", "dei\u00b7ne", "Hand", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ich bin ins Lebens-Buch von dir getragen ein.", "tokens": ["Ich", "bin", "ins", "Le\u00b7bens\u00b7Buch", "von", "dir", "ge\u00b7tra\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Du Sieger/ der du hast all Ubelthat vertrieben/", "tokens": ["Du", "Sie\u00b7ger", "/", "der", "du", "hast", "all", "U\u00b7belt\u00b7hat", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PRELS", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ich hoff\u2019/ ich werde noch von denen S\u00fcndern seyn/", "tokens": ["Ich", "hoff'", "/", "ich", "wer\u00b7de", "noch", "von", "de\u00b7nen", "S\u00fcn\u00b7dern", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "APPR", "PRELS", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "F\u00fcr welche du dein Blut so milde hast vergossen;", "tokens": ["F\u00fcr", "wel\u00b7che", "du", "dein", "Blut", "so", "mil\u00b7de", "hast", "ver\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Das Wasser und das Blut so aus der Seiten rann/", "tokens": ["Das", "Was\u00b7ser", "und", "das", "Blut", "so", "aus", "der", "Sei\u00b7ten", "rann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Dient auch zu meinem Heyl/ macht mich zum Mitgenossen", "tokens": ["Dient", "auch", "zu", "mei\u00b7nem", "Heyl", "/", "macht", "mich", "zum", "Mit\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Des Himmels/ da\u00df ich nun versichert leben kan.", "tokens": ["Des", "Him\u00b7mels", "/", "da\u00df", "ich", "nun", "ver\u00b7si\u00b7chert", "le\u00b7ben", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "ADV", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.57": {"text": "So nun der Thau darvon macht seelig meine Seele/", "tokens": ["So", "nun", "der", "Thau", "dar\u00b7von", "macht", "see\u00b7lig", "mei\u00b7ne", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PAV", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "So nun ein einzig Hauch/ wie auf die J\u00fcnger fiel.", "tokens": ["So", "nun", "ein", "ein\u00b7zig", "Hauch", "/", "wie", "auf", "die", "J\u00fcn\u00b7ger", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Auch meinen Geist erleucht in dieses Leibes H\u00f6le/", "tokens": ["Auch", "mei\u00b7nen", "Geist", "er\u00b7leucht", "in", "die\u00b7ses", "Lei\u00b7bes", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "So gib doch/ welches ich gehorsam bitten wil.", "tokens": ["So", "gib", "doch", "/", "wel\u00b7ches", "ich", "ge\u00b7hor\u00b7sam", "bit\u00b7ten", "wil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$(", "PWS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Gib/ sag ich/ mir die Krafft/ die S\u00fcnd hinfort zu meiden/", "tokens": ["Gib", "/", "sag", "ich", "/", "mir", "die", "Krafft", "/", "die", "S\u00fcnd", "hin\u00b7fort", "zu", "mei\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "VVIMP", "PPER", "$(", "PPER", "ART", "NN", "$(", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Der ich so hertzlich gram in meinem Hertzen bin.", "tokens": ["Der", "ich", "so", "hertz\u00b7lich", "gram", "in", "mei\u00b7nem", "Hert\u00b7zen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Du wirst mich/ als mein Haupt/ dein Glied nicht von dir scheiden/", "tokens": ["Du", "wirst", "mich", "/", "als", "mein", "Haupt", "/", "dein", "Glied", "nicht", "von", "dir", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "KOKOM", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Weil du erstanden bist/ du nimmst mich mit dir hin.", "tokens": ["Weil", "du", "er\u00b7stan\u00b7den", "bist", "/", "du", "nimmst", "mich", "mit", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$(", "PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Und wie du nicht mehr stirbst/ und lebest gantz verkl\u00e4ret/", "tokens": ["Und", "wie", "du", "nicht", "mehr", "stirbst", "/", "und", "le\u00b7best", "gantz", "ver\u00b7kl\u00e4\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PTKNEG", "ADV", "VVFIN", "$(", "KON", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "So hilff/ da\u00df mich die Zeit/ so ich noch leben mu\u00df", "tokens": ["So", "hilff", "/", "da\u00df", "mich", "die", "Zeit", "/", "so", "ich", "noch", "le\u00b7ben", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "KOUS", "PPER", "ART", "NN", "$(", "ADV", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "In meines Kerckers Hau\u00df/ mit Ach und Noth beschweret/", "tokens": ["In", "mei\u00b7nes", "Ker\u00b7ckers", "Hau\u00df", "/", "mit", "Ach", "und", "Noth", "be\u00b7schwe\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Dein guter Geist regir in meiner Wercke Schlu\u00df.", "tokens": ["Dein", "gu\u00b7ter", "Geist", "re\u00b7gir", "in", "mei\u00b7ner", "Wer\u00b7cke", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Und endlich wenn ich nun aus diesem Leben scheide/", "tokens": ["Und", "end\u00b7lich", "wenn", "ich", "nun", "aus", "die\u00b7sem", "Le\u00b7ben", "schei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "So nimm mich in dein Reich/ da\u00df ich dich preisen kan.", "tokens": ["So", "nimm", "mich", "in", "dein", "Reich", "/", "da\u00df", "ich", "dich", "prei\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Und deine Majest\u00e4t in auserlesner Freude", "tokens": ["Und", "dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "in", "au\u00b7ser\u00b7les\u00b7ner", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und h\u00f6chster Herrlichkeit unendlich beten an.", "tokens": ["Und", "h\u00f6chs\u00b7ter", "Herr\u00b7lich\u00b7keit", "un\u00b7end\u00b7lich", "be\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es kan das Marter-Hau\u00df die Welt uns nicht vergn\u00fcgen.", "tokens": ["Es", "kan", "das", "Mar\u00b7ter\u00b7Hau\u00df", "die", "Welt", "uns", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff Seele! la\u00df uns bald aus diesem Kercker fliegen.", "tokens": ["Auff", "See\u00b7le", "!", "la\u00df", "uns", "bald", "aus", "die\u00b7sem", "Ker\u00b7cker", "flie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}