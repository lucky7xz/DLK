{"dta.poem.2272": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  Messias .  \n Zehnter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1756", "urn": "urn:nbn:de:kobv:b4-200905198237", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jmmer weiter komm ich, auf meinem furchtbaren Wege,", "tokens": ["Jm\u00b7mer", "wei\u00b7ter", "komm", "ich", ",", "auf", "mei\u00b7nem", "furcht\u00b7ba\u00b7ren", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Jm\u0303er n\u00e4her zum Tode des Sohns. Ach, w\u00e4rs nicht der Liebe,", "tokens": ["Jm\u0303er", "n\u00e4\u00b7her", "zum", "To\u00b7de", "des", "Sohns", ".", "Ach", ",", "w\u00e4rs", "nicht", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$.", "ITJ", "$,", "VAFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht der Tod der ewigen Liebe; so w\u00fcrd ich erliegen,", "tokens": ["Nicht", "der", "Tod", "der", "e\u00b7wi\u00b7gen", "Lie\u00b7be", ";", "so", "w\u00fcrd", "ich", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Unter der Last der Betrachtung! Auf beyden Seiten ist Abgrund", "tokens": ["Un\u00b7ter", "der", "Last", "der", "Be\u00b7trach\u00b7tung", "!", "Auf", "bey\u00b7den", "Sei\u00b7ten", "ist", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "APPR", "PIAT", "NN", "VAFIN", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Da zur Linken: Ich soll nicht zu k\u00fchn von dem G\u00f6ttlichen singen!", "tokens": ["Da", "zur", "Lin\u00b7ken", ":", "Ich", "soll", "nicht", "zu", "k\u00fchn", "von", "dem", "G\u00f6tt\u00b7li\u00b7chen", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "PTKA", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Hier zur Rechten: Ich soll ihn mit feyrlicher W\u00fcrdigkeit singen!", "tokens": ["Hier", "zur", "Rech\u00b7ten", ":", "Ich", "soll", "ihn", "mit", "feyr\u00b7li\u00b7cher", "W\u00fcr\u00b7dig\u00b7keit", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Und ich bin Staub! \u2026 O du, de\u00df Blut auf Golgatha str\u00f6mte,", "tokens": ["Und", "ich", "bin", "Staub", "!", "\u2026", "O", "du", ",", "de\u00df", "Blut", "auf", "Gol\u00b7ga\u00b7tha", "str\u00f6m\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$.", "$(", "NE", "NE", "$,", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Dessen Allgegenwart mich, von allen Seiten, umringt hat,", "tokens": ["Des\u00b7sen", "All\u00b7ge\u00b7gen\u00b7wart", "mich", ",", "von", "al\u00b7len", "Sei\u00b7ten", ",", "um\u00b7ringt", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "$,", "APPR", "PIAT", "NN", "$,", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Du erforschest meine Gedanken! Du siehest es alles,", "tokens": ["Du", "er\u00b7for\u00b7schest", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "!", "Du", "sie\u00b7hest", "es", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Was ich denke, vorher, du Naher! Ja, selber kein Wort ist", "tokens": ["Was", "ich", "den\u00b7ke", ",", "vor\u00b7her", ",", "du", "Na\u00b7her", "!", "Ja", ",", "sel\u00b7ber", "kein", "Wort", "ist"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "$,", "PPER", "NN", "$.", "PTKANT", "$,", "ADV", "PIAT", "NN", "VAFIN"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Mir auf der Zunge, das du nicht wissest. Mein Gott! mein Vers\u00f6ner", "tokens": ["Mir", "auf", "der", "Zun\u00b7ge", ",", "das", "du", "nicht", "wis\u00b7sest", ".", "Mein", "Gott", "!", "mein", "Ver\u00b7s\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Leite mich, mein Vers\u00f6ner, und, wenn ich strauchle, vergieb mirs.", "tokens": ["Lei\u00b7te", "mich", ",", "mein", "Ver\u00b7s\u00f6\u00b7ner", ",", "und", ",", "wenn", "ich", "strauch\u00b7le", ",", "ver\u00b7gieb", "mirs", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVIMP", "NE", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Deines Lichts Ein Schimmer, von deiner Gnad Ein Tropfen,", "tokens": ["Dei\u00b7nes", "Lichts", "Ein", "Schim\u00b7mer", ",", "von", "dei\u00b7ner", "Gnad", "Ein", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Ist, dem Erkenntni\u00dfbegierigen, ist, dem Durstenden, F\u00fclle!", "tokens": ["Ist", ",", "dem", "Er\u00b7kenn\u00b7tni\u00df\u00b7be\u00b7gie\u00b7ri\u00b7gen", ",", "ist", ",", "dem", "Durs\u00b7ten\u00b7den", ",", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "$,", "VAFIN", "$,", "ART", "NN", "$,", "NN", "$."], "meter": "+---+-+--+-+--+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Von dem Throne, der sonst, die hellste sichtbare Sch\u00f6nheit,", "tokens": ["Von", "dem", "Thro\u00b7ne", ",", "der", "sonst", ",", "die", "hells\u00b7te", "sicht\u00b7ba\u00b7re", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Leuchtete, nun in schreckenerschaffende N\u00e4chte geh\u00fcllt stand,", "tokens": ["Leuch\u00b7te\u00b7te", ",", "nun", "in", "schre\u00b7cke\u00b7ner\u00b7schaf\u00b7fen\u00b7de", "N\u00e4ch\u00b7te", "ge\u00b7h\u00fcllt", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Einsam dastand; um den izt kein Unsterblicher feyrte;", "tokens": ["Ein\u00b7sam", "das\u00b7tand", ";", "um", "den", "izt", "kein", "U\u00b7nsterb\u00b7li\u00b7cher", "feyr\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "APPR", "ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+---+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ausser, da\u00df, von dem bebenden Hange der untersten Stufe,", "tokens": ["Aus\u00b7ser", ",", "da\u00df", ",", "von", "dem", "be\u00b7ben\u00b7den", "Han\u00b7ge", "der", "un\u00b7ters\u00b7ten", "Stu\u00b7fe", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "$,", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Kniend, mit betendem Auge, mit banggerungenen H\u00e4nden,", "tokens": ["Kniend", ",", "mit", "be\u00b7ten\u00b7dem", "Au\u00b7ge", ",", "mit", "bang\u00b7ge\u00b7run\u00b7ge\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Starr vor Erwartung, der erste der Todesengel emporsah:", "tokens": ["Starr", "vor", "Er\u00b7war\u00b7tung", ",", "der", "ers\u00b7te", "der", "To\u00b7de\u00b7sen\u00b7gel", "em\u00b7por\u00b7sah", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ART", "ADJA", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+--++", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Von dem Throne schaute, mit unverwendetem Antliz,", "tokens": ["Von", "dem", "Thro\u00b7ne", "schau\u00b7te", ",", "mit", "un\u00b7ver\u00b7wen\u00b7de\u00b7tem", "Ant\u00b7liz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Auf den g\u00f6ttlichen S\u00fcndevers\u00f6ner, Jehova herunter.", "tokens": ["Auf", "den", "g\u00f6tt\u00b7li\u00b7chen", "S\u00fcn\u00b7de\u00b7ver\u00b7s\u00f6\u00b7ner", ",", "Je\u00b7ho\u00b7va", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NE", "PTKVZ", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Durch die helleren St\u00e4ubchen der Sonnen, die dunklern der Erden,", "tokens": ["Durch", "die", "hel\u00b7le\u00b7ren", "St\u00e4ub\u00b7chen", "der", "Son\u00b7nen", ",", "die", "dunk\u00b7lern", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Durch die verstummte Natur; mit Blicken, von dem nur verstanden", "tokens": ["Durch", "die", "ver\u00b7stumm\u00b7te", "Na\u00b7tur", ";", "mit", "Bli\u00b7cken", ",", "von", "dem", "nur", "ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "APPR", "NN", "$,", "APPR", "PRELS", "ADV", "VVINF"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Dem nur gef\u00fchlt, auf den sie, vom Auge des Ewigen, str\u00f6mten,", "tokens": ["Dem", "nur", "ge\u00b7f\u00fchlt", ",", "auf", "den", "sie", ",", "vom", "Au\u00b7ge", "des", "E\u00b7wi\u00b7gen", ",", "str\u00f6m\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "$,", "APPR", "PRELS", "PPER", "$,", "APPRART", "NN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Schaut\u2019 er hinab. Es empfindet, den Blick des richtenden Vaters,", "tokens": ["Schaut'", "er", "hin\u00b7ab", ".", "Es", "emp\u00b7fin\u00b7det", ",", "den", "Blick", "des", "rich\u00b7ten\u00b7den", "Va\u00b7ters", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Jesus Christus; weis, da\u00df Jehova noch nicht vers\u00f6nt ist!", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", ";", "weis", ",", "da\u00df", "Je\u00b7ho\u00b7va", "noch", "nicht", "ver\u00b7s\u00f6nt", "ist", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PTKVZ", "$,", "KOUS", "NE", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Weis es, und f\u00fchlts unaussprechlich, durchstr\u00f6mt von des n\u00e4heren Todes", "tokens": ["Weis", "es", ",", "und", "f\u00fchlts", "un\u00b7aus\u00b7sprech\u00b7lich", ",", "durch\u00b7str\u00f6mt", "von", "des", "n\u00e4\u00b7he\u00b7ren", "To\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KON", "VVFIN", "ADJD", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Schauer. \u2026 Es zittern in ihrem verborgensten Leben die Welten!", "tokens": ["Schau\u00b7er", ".", "\u2026", "Es", "zit\u00b7tern", "in", "ih\u00b7rem", "ver\u00b7bor\u00b7gens\u00b7ten", "Le\u00b7ben", "die", "Wel\u00b7ten", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Banger, tr\u00fcber, verstummender stehn die Unsterblichen alle,", "tokens": ["Ban\u00b7ger", ",", "tr\u00fc\u00b7ber", ",", "ver\u00b7stum\u00b7men\u00b7der", "stehn", "die", "U\u00b7nsterb\u00b7li\u00b7chen", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "+-+--+--+-+---+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Bey der Empfindung des Sohns, die mit mehr Todesbl\u00e4sse", "tokens": ["Bey", "der", "Emp\u00b7fin\u00b7dung", "des", "Sohns", ",", "die", "mit", "mehr", "To\u00b7des\u00b7bl\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.18": {"text": "In des G\u00f6ttlichen Angesicht stieg. Dem m\u00fcden Auge,", "tokens": ["In", "des", "G\u00f6tt\u00b7li\u00b7chen", "An\u00b7ge\u00b7sicht", "stieg", ".", "Dem", "m\u00fc\u00b7den", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Das zu brechen begann, entsanken verl\u00f6schende Blicke,", "tokens": ["Das", "zu", "bre\u00b7chen", "be\u00b7gann", ",", "ent\u00b7san\u00b7ken", "ver\u00b7l\u00f6\u00b7schen\u00b7de", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "VVFIN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Fielen auf sein Grabmaal, das gegen Golgatha \u00fcber", "tokens": ["Fie\u00b7len", "auf", "sein", "Grab\u00b7maal", ",", "das", "ge\u00b7gen", "Gol\u00b7ga\u00b7tha", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NE", "APPR"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Einsam, unter alternden B\u00e4umen, in Felsen gehaun, lag.", "tokens": ["Ein\u00b7sam", ",", "un\u00b7ter", "al\u00b7tern\u00b7den", "B\u00e4u\u00b7men", ",", "in", "Fel\u00b7sen", "ge\u00b7haun", ",", "lag", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+--+--+--++", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Todesschlummer, bald wird dich mein Leib dort schlummern! So dachte", "tokens": ["To\u00b7des\u00b7schlum\u00b7mer", ",", "bald", "wird", "dich", "mein", "Leib", "dort", "schlum\u00b7mern", "!", "So", "dach\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Jesus Christus, indem sein Blick an dem Grabe verweilte.", "tokens": ["Je\u00b7sus", "Chris\u00b7tus", ",", "in\u00b7dem", "sein", "Blick", "an", "dem", "Gra\u00b7be", "ver\u00b7weil\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+----+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Darum nahm ich dich an, du Leib von Staube! Verwesen", "tokens": ["Da\u00b7rum", "nahm", "ich", "dich", "an", ",", "du", "Leib", "von", "Stau\u00b7be", "!", "Ver\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPER", "NN", "APPR", "NN", "$.", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sollst du nicht; doch sollst du entschlafen liegen. Mein Vater,", "tokens": ["Sollst", "du", "nicht", ";", "doch", "sollst", "du", "ent\u00b7schla\u00b7fen", "lie\u00b7gen", ".", "Mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$.", "ADV", "VMFIN", "PPER", "VVINF", "VVFIN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Trockne die Thr\u00e4nen von deren Gesicht, die dann um mich weinen!", "tokens": ["Trock\u00b7ne", "die", "Thr\u00e4\u00b7nen", "von", "de\u00b7ren", "Ge\u00b7sicht", ",", "die", "dann", "um", "mich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PRELAT", "NN", "$,", "PRELS", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Ausges\u00f6nter! erbarme dich ihrer, sie weinen um Jesum,", "tokens": ["Aus\u00b7ge\u00b7s\u00f6n\u00b7ter", "!", "er\u00b7bar\u00b7me", "dich", "ih\u00b7rer", ",", "sie", "wei\u00b7nen", "um", "Je\u00b7sum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPOSAT", "$,", "PPER", "VVINF", "APPR", "NE", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Deinen Eingebornen! Erbarme dich ihrer, wenn nun auch", "tokens": ["Dei\u00b7nen", "Ein\u00b7ge\u00b7bor\u00b7nen", "!", "Er\u00b7bar\u00b7me", "dich", "ih\u00b7rer", ",", "wenn", "nun", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "PPER", "PPOSAT", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Jhre lezte Stunde von dir zu ihnen gesandt wird!", "tokens": ["Ih\u00b7re", "lez\u00b7te", "Stun\u00b7de", "von", "dir", "zu", "ih\u00b7nen", "ge\u00b7sandt", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Heiliger Vater, erbarme dich aller, die an den Geliebten,", "tokens": ["Hei\u00b7li\u00b7ger", "Va\u00b7ter", ",", "er\u00b7bar\u00b7me", "dich", "al\u00b7ler", ",", "die", "an", "den", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PIAT", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Deinen ewigen Sohn, den Gottgeopferten, glauben;", "tokens": ["Dei\u00b7nen", "e\u00b7wi\u00b7gen", "Sohn", ",", "den", "Gott\u00b7ge\u00b7o\u00b7pfer\u00b7ten", ",", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Wenn sie, in diesem Glauben, nun auch mit dem Tode ringen", "tokens": ["Wenn", "sie", ",", "in", "die\u00b7sem", "Glau\u00b7ben", ",", "nun", "auch", "mit", "dem", "To\u00b7de", "rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PDAT", "NN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Ach, ich f\u00fchl ihn, ich f\u00fchl ihn, den Tod! Des Ewigen Schrecken", "tokens": ["Ach", ",", "ich", "f\u00fchl", "ihn", ",", "ich", "f\u00fchl", "ihn", ",", "den", "Tod", "!", "Des", "E\u00b7wi\u00b7gen", "Schre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Tr\u00e4gt er! Er ist ein Schwert in der Hand des Allm\u00e4chtigen! Furchtbar", "tokens": ["Tr\u00e4gt", "er", "!", "Er", "ist", "ein", "Schwert", "in", "der", "Hand", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "!", "Furcht\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$.", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Ist er! \u2026 Zwar sie werden es, was ich empfand, nicht empfinden;", "tokens": ["Ist", "er", "!", "\u2026", "Zwar", "sie", "wer\u00b7den", "es", ",", "was", "ich", "emp\u00b7fand", ",", "nicht", "emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "$(", "ADV", "PPER", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Sie sind endlich! Allein aus dem Meer, in welches ich sinke,", "tokens": ["Sie", "sind", "end\u00b7lich", "!", "Al\u00b7lein", "aus", "dem", "Meer", ",", "in", "wel\u00b7ches", "ich", "sin\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Kann ein Tropfen in ihnen des Todes Schrecken verbreiten!", "tokens": ["Kann", "ein", "Trop\u00b7fen", "in", "ih\u00b7nen", "des", "To\u00b7des", "Schre\u00b7cken", "ver\u00b7brei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Einige, g\u