ich anflehn.", "tokens": ["Die", ",", "im", "Kamp\u00b7fe", "des", "To\u00b7des", ",", "um", "Lab\u00b7sal", "!", "um", "Gna\u00b7de", "!", "dich", "an\u00b7flehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "ART", "NN", "$,", "KOUI", "NN", "$.", "APPR", "NN", "$.", "PPER", "VVINF", "$."], "meter": "--+--+--++-+-+-+", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Derer, die aus viel Tr\u00fcbsal ihr m\u00fcdes Leben dem Grabe", "tokens": ["De\u00b7rer", ",", "die", "aus", "viel", "Tr\u00fcb\u00b7sal", "ihr", "m\u00fc\u00b7des", "Le\u00b7ben", "dem", "Gra\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.23": {"text": "Brachten, in D\u00fcrftigkeit lebten, und dennoch dich nicht verkannten;", "tokens": ["Brach\u00b7ten", ",", "in", "D\u00fcrf\u00b7tig\u00b7keit", "leb\u00b7ten", ",", "und", "den\u00b7noch", "dich", "nicht", "ver\u00b7kann\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "VVFIN", "$,", "KON", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Die, wie schuldlos sie waren, mit Schmach der S\u00fcnder befleckte;", "tokens": ["Die", ",", "wie", "schuld\u00b7los", "sie", "wa\u00b7ren", ",", "mit", "Schmach", "der", "S\u00fcn\u00b7der", "be\u00b7fleck\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Die, dem Freunde getreu, die Feinde segneten; Demut,", "tokens": ["Die", ",", "dem", "Freun\u00b7de", "ge\u00b7treu", ",", "die", "Fein\u00b7de", "seg\u00b7ne\u00b7ten", ";", "De\u00b7mut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$.", "NN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Liebe der Br\u00fcder, und Liebe der Menschen, durch Handlungen, zeigten;", "tokens": ["Lie\u00b7be", "der", "Br\u00fc\u00b7der", ",", "und", "Lie\u00b7be", "der", "Men\u00b7schen", ",", "durch", "Hand\u00b7lun\u00b7gen", ",", "zeig\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "NN", "ART", "NN", "$,", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+--+--++-+-", "measure": "dactylic.tetra.plus"}, "line.27": {"text": "Derer, die, unverblendet von Ehre, Reichthum, und Hoheit,", "tokens": ["De\u00b7rer", ",", "die", ",", "un\u00b7ver\u00b7blen\u00b7det", "von", "Eh\u00b7re", ",", "Reicht\u00b7hum", ",", "und", "Ho\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "$,", "ADJD", "APPR", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Gutes zu thun sie gebrauchten, und, sie zu entbehren, vermochten;", "tokens": ["Gu\u00b7tes", "zu", "thun", "sie", "ge\u00b7brauch\u00b7ten", ",", "und", ",", "sie", "zu", "ent\u00b7beh\u00b7ren", ",", "ver\u00b7moch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PPER", "VVFIN", "$,", "KON", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.29": {"text": "Aller, die, nach den verschiednen, von dir gegebenen Gaben,", "tokens": ["Al\u00b7ler", ",", "die", ",", "nach", "den", "ver\u00b7schied\u00b7nen", ",", "von", "dir", "ge\u00b7ge\u00b7be\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PRELS", "$,", "APPR", "ART", "ADJA", "$,", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+---+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.30": {"text": "Nach dem kleinern und gr\u00f6sseren Anla\u00df, durch welchen die Vorsicht", "tokens": ["Nach", "dem", "klei\u00b7nern", "und", "gr\u00f6s\u00b7se\u00b7ren", "An\u00b7la\u00df", ",", "durch", "wel\u00b7chen", "die", "Vor\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$,", "APPR", "PWAT", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.31": {"text": "Sie anlockte: mit reiner, mit herzlicher Liebe, dir dienten:", "tokens": ["Sie", "an\u00b7lock\u00b7te", ":", "mit", "rei\u00b7ner", ",", "mit", "herz\u00b7li\u00b7cher", "Lie\u00b7be", ",", "dir", "dien\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ADJA", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Derer erbarme dich, Vater, in ihrer lezten Stunde!", "tokens": ["De\u00b7rer", "er\u00b7bar\u00b7me", "dich", ",", "Va\u00b7ter", ",", "in", "ih\u00b7rer", "lez\u00b7ten", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.33": {"text": "Wenn ihr Auge nun auch zu brechen beginnt, die Verwesung", "tokens": ["Wenn", "ihr", "Au\u00b7ge", "nun", "auch", "zu", "bre\u00b7chen", "be\u00b7ginnt", ",", "die", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,", "ART", "NN"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Jhren K\u00f6rper verlangt; der Sch\u00f6pfer die Seele: dann sende", "tokens": ["Ih\u00b7ren", "K\u00f6r\u00b7per", "ver\u00b7langt", ";", "der", "Sch\u00f6p\u00b7fer", "die", "See\u00b7le", ":", "dann", "sen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "ART", "NN", "ART", "NN", "$.", "ADV", "ADJA"], "meter": "+-+--+-+--+-+--", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Deine Tr\u00f6stung, den Geist, der unaussprechlich in ihnen", "tokens": ["Dei\u00b7ne", "Tr\u00f6s\u00b7tung", ",", "den", "Geist", ",", "der", "un\u00b7aus\u00b7sprech\u00b7lich", "in", "ih\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Bete, bis du sie \u00fcber das, so sie verstanden und baten,", "tokens": ["Be\u00b7te", ",", "bis", "du", "sie", "\u00fc\u00b7ber", "das", ",", "so", "sie", "ver\u00b7stan\u00b7den", "und", "ba\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "PDS", "$,", "ADV", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.37": {"text": "Ueberschwenglich erh\u00f6rst, und zu deiner Ruhe sie einf\u00fchrst.", "tokens": ["Ue\u00b7ber\u00b7schweng\u00b7lich", "er\u00b7h\u00f6rst", ",", "und", "zu", "dei\u00b7ner", "Ru\u00b7he", "sie", "ein\u00b7f\u00fchrst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Gott der Liebe, mein Vater, um dieser quellenden Wunden!", "tokens": ["Gott", "der", "Lie\u00b7be", ",", "mein", "Va\u00b7ter", ",", "um", "die\u00b7ser", "quel\u00b7len\u00b7den", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "KOUI", "PDAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.39": {"text": "Dieser blutigen Krone, die meiner Schl\u00e4fe sich eingrub!", "tokens": ["Die\u00b7ser", "blu\u00b7ti\u00b7gen", "Kro\u00b7ne", ",", "die", "mei\u00b7ner", "Schl\u00e4\u00b7fe", "sich", "ein\u00b7grub", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.40": {"text": "Um der Todesangst willen, die meine Gebeine durchsch\u00fcttert!", "tokens": ["Um", "der", "To\u00b7de\u00b7sangst", "wil\u00b7len", ",", "die", "mei\u00b7ne", "Ge\u00b7bei\u00b7ne", "durch\u00b7sch\u00fct\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.41": {"text": "Um de\u00df, was ich izt leide, noch leiden werde! der Liebe,", "tokens": ["Um", "de\u00df", ",", "was", "ich", "izt", "lei\u00b7de", ",", "noch", "lei\u00b7den", "wer\u00b7de", "!", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVINF", "VAFIN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Dieser Liebe willen, mit der ich, erniedrigt zum Tode,", "tokens": ["Die\u00b7ser", "Lie\u00b7be", "wil\u00b7len", ",", "mit", "der", "ich", ",", "er\u00b7nied\u00b7rigt", "zum", "To\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.43": {"text": "Bis zum Tod am Kreuze, das Heil der Menschen vollende:", "tokens": ["Bis", "zum", "Tod", "am", "Kreu\u00b7ze", ",", "das", "Heil", "der", "Men\u00b7schen", "voll\u00b7en\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "ADJA", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "H\u00f6r mich, und la\u00df, die ich liebe, getreu bis ans Ende mir bleiben!", "tokens": ["H\u00f6r", "mich", ",", "und", "la\u00df", ",", "die", "ich", "lie\u00b7be", ",", "ge\u00b7treu", "bis", "ans", "En\u00b7de", "mir", "blei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KON", "VVIMP", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "APPR", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.45": {"text": "Trostvoll sterben! den Lohn der Ueberwinder empfangen!", "tokens": ["Trost\u00b7voll", "ster\u00b7ben", "!", "den", "Lohn", "der", "Ue\u00b7ber\u00b7win\u00b7der", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}}}}}