{"dta.poem.1074": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Darumb beschlie\u00df ich meinen mund", "tokens": ["Da\u00b7rumb", "be\u00b7schlie\u00df", "ich", "mei\u00b7nen", "mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd will dir mein hertz \u00fcbergeben;", "tokens": ["Vnd", "will", "dir", "mein", "hertz", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sih/ du wirst es erfinden rund/", "tokens": ["Sih", "/", "du", "wirst", "es", "er\u00b7fin\u00b7den", "rund", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VAFIN", "PPER", "VVFIN", "ADJD", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Auch der Tugent allein nachstreben/", "tokens": ["Auch", "der", "Tu\u00b7gent", "al\u00b7lein", "nach\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Bi\u00df einmahl mit vil h\u00f6herm klang", "tokens": ["Bi\u00df", "ein\u00b7mahl", "mit", "vil", "h\u00f6\u00b7herm", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In deiner Lorb\u00f6rcr\u00e4ntzen schatten", "tokens": ["In", "dei\u00b7ner", "Lor\u00b7b\u00f6r\u00b7cr\u00b7\u00e4nt\u00b7zen", "schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die G\u00f6tter ein newes gesang", "tokens": ["Die", "G\u00f6t\u00b7ter", "ein", "ne\u00b7wes", "ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dir zusingen mir gestatten;", "tokens": ["Von", "dir", "zu\u00b7sin\u00b7gen", "mir", "ge\u00b7stat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wan mir namlich einmahl das Gl\u00fcck", "tokens": ["Wan", "mir", "nam\u00b7lich", "ein\u00b7mahl", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Gibt einen freindlichern anblick/", "tokens": ["Gibt", "ei\u00b7nen", "freind\u00b7li\u00b7chern", "an\u00b7blick", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd von mir all vnruh vnd plagen/", "tokens": ["Vnd", "von", "mir", "all", "vn\u00b7ruh", "vnd", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PIAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So die armut gibt/ wirdt verjagen.", "tokens": ["So", "die", "ar\u00b7mut", "gibt", "/", "wirdt", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "VAFIN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}