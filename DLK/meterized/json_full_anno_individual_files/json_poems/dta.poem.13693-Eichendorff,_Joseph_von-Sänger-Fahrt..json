{"dta.poem.13693": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4nger-Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "af:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie stehn wir hier so helle!", "tokens": ["Wie", "stehn", "wir", "hier", "so", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird mancher bald schlafen gehn,", "tokens": ["Wird", "man\u00b7cher", "bald", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Leben, wie bist du schnelle,", "tokens": ["O", "Le\u00b7ben", ",", "wie", "bist", "du", "schnel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O Leben, wie bist du sch\u00f6n!", "tokens": ["O", "Le\u00b7ben", ",", "wie", "bist", "du", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}