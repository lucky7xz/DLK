{"dta.poem.18259": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Chorus der Lyriker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Wer schw\u00e4rmte fr\u00fcher f\u00fcr Lassalle?", "tokens": ["Wer", "schw\u00e4rm\u00b7te", "fr\u00fc\u00b7her", "f\u00fcr", "Las\u00b7sal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heut gellt der Pfiff der Dampfmaschine", "tokens": ["Heut", "gellt", "der", "Pfiff", "der", "Dampf\u00b7ma\u00b7schi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Hohelied der Nachtigall!", "tokens": ["Ins", "Ho\u00b7he\u00b7lied", "der", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man schimpft uns \u201eewge Sekundaner\u201c,", "tokens": ["Man", "schimpft", "uns", "\u201e", "ew\u00b7ge", "Se\u00b7kun\u00b7da\u00b7ner", "\u201c", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch falsch ist ihre Strategie:", "tokens": ["Doch", "falsch", "ist", "ih\u00b7re", "Stra\u00b7te\u00b7gie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind die letzten Mohikaner", "tokens": ["Wir", "sind", "die", "letz\u00b7ten", "Mo\u00b7hi\u00b7ka\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutschen Stimmungspoesie.", "tokens": ["Der", "deut\u00b7schen", "Stim\u00b7mungs\u00b7po\u00b7e\u00b7sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir klopfen an die leere Tonne", "tokens": ["Wir", "klop\u00b7fen", "an", "die", "lee\u00b7re", "Ton\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rufen: Wein her, rothen Wein!", "tokens": ["Und", "ru\u00b7fen", ":", "Wein", "her", ",", "ro\u00b7then", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch uns erfreut das Licht der Sonne,", "tokens": ["Auch", "uns", "er\u00b7freut", "das", "Licht", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur darf es nicht elektrisch sein.", "tokens": ["Nur", "darf", "es", "nicht", "e\u00b7lekt\u00b7risch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00dft uns die Henkelkr\u00fcge schwingen:", "tokens": ["La\u00dft", "uns", "die", "Hen\u00b7kel\u00b7kr\u00fc\u00b7ge", "schwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was geht die Zeit uns an? Wir singen", "tokens": ["Was", "geht", "die", "Zeit", "uns", "an", "?", "Wir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Mammuth und vom Mastodon!", "tokens": ["Vom", "Mam\u00b7muth", "und", "vom", "Mas\u00b7to\u00b7don", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}