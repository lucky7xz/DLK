{"dta.poem.6360": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern ein Sauerteig den Gang verstopfft und schl\u00fcsset,", "tokens": ["Wo\u00b7fern", "ein", "Sau\u00b7er\u00b7teig", "den", "Gang", "ver\u00b7stopfft", "und", "schl\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So, da\u00df die Fluth nicht ferner fl\u00fcsset,", "tokens": ["So", ",", "da\u00df", "die", "Fluth", "nicht", "fer\u00b7ner", "fl\u00fcs\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihren Lauf nicht wohl vollziehen mag.", "tokens": ["Und", "ih\u00b7ren", "Lauf", "nicht", "wohl", "voll\u00b7zie\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn des verdickten Bluts erz\u00fcrnte Wellen,", "tokens": ["Wenn", "des", "ver\u00b7dick\u00b7ten", "Bluts", "er\u00b7z\u00fcrn\u00b7te", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch Hinderni\u00df noch mehr gereitzet, schwellen.", "tokens": ["Durch", "Hin\u00b7der\u00b7ni\u00df", "noch", "mehr", "ge\u00b7reit\u00b7zet", ",", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wovon sodann im Hirn und Hertzen, wie wir sehn,", "tokens": ["Wo\u00b7von", "so\u00b7dann", "im", "Hirn", "und", "Hert\u00b7zen", ",", "wie", "wir", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch solch ein hefftiges Bewegen,", "tokens": ["Durch", "solch", "ein", "heff\u00b7ti\u00b7ges", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ausserordentliches Regen,", "tokens": ["Und", "aus\u00b7ser\u00b7or\u00b7dent\u00b7li\u00b7ches", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Solch Kochen, Hitz und Feur, entstehn.", "tokens": ["Solch", "Ko\u00b7chen", ",", "Hitz", "und", "Feur", ",", "ent\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}