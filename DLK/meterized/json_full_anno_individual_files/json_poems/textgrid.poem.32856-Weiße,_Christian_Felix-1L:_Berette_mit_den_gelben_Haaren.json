"Will mich, allein ich mag sie nicht.", "tokens": ["Will", "mich", ",", "al\u00b7lein", "ich", "mag", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "PPER", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Climene, die die Macht nicht f\u00fchlet,", "tokens": ["Cli\u00b7me\u00b7ne", ",", "die", "die", "Macht", "nicht", "f\u00fch\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie gef\u00e4llig weichend ficht,", "tokens": ["Wenn", "sie", "ge\u00b7f\u00e4l\u00b7lig", "wei\u00b7chend", "ficht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn sie z\u00e4rtlich fleht, befiehlet,", "tokens": ["Und", "wenn", "sie", "z\u00e4rt\u00b7lich", "fleht", ",", "be\u00b7fieh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die will ich, doch sie will mich nicht.", "tokens": ["Die", "will", "ich", ",", "doch", "sie", "will", "mich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nerine, die in tiefen Schl\u00fcssen", "tokens": ["Ne\u00b7ri\u00b7ne", ",", "die", "in", "tie\u00b7fen", "Schl\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich grundgelehrt den Kopf zerbricht,", "tokens": ["Sich", "grund\u00b7ge\u00b7lehrt", "den", "Kopf", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Grundtrieb redt, wenn sie soll k\u00fcssen,", "tokens": ["Vom", "Grund\u00b7trieb", "redt", ",", "wenn", "sie", "soll", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will mich, jedoch ich mag sie nicht.", "tokens": ["Will", "mich", ",", "je\u00b7doch", "ich", "mag", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "PPER", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lucinden, die von Witz beseelet,", "tokens": ["Lu\u00b7cin\u00b7den", ",", "die", "von", "Witz", "be\u00b7see\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vern\u00fcnftig mehr, als witzig spricht,", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", "mehr", ",", "als", "wit\u00b7zig", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bescheiden urtheilt, niemals fehlet,", "tokens": ["Be\u00b7schei\u00b7den", "ur\u00b7theilt", ",", "nie\u00b7mals", "feh\u00b7let", "