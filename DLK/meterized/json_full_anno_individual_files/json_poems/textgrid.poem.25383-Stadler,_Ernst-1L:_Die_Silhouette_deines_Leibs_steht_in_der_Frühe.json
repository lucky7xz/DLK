{"textgrid.poem.25383": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Silhouette deines Leibs steht in der Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Silhouette deines Leibs steht in der Fr\u00fche", "tokens": ["Die", "Sil\u00b7hou\u00b7et\u00b7te", "dei\u00b7nes", "Leibs", "steht", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dunkel vor dem tr\u00fcben Licht", "tokens": ["dun\u00b7kel", "vor", "dem", "tr\u00fc\u00b7ben", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der zugehangnen Jalousien. Ich f\u00fchl, im Bette liegend,", "tokens": ["Der", "zu\u00b7ge\u00b7hang\u00b7nen", "Ja\u00b7lou\u00b7si\u00b7en", ".", "Ich", "f\u00fchl", ",", "im", "Bet\u00b7te", "lie\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "hostiengleich mir zugewendet dein Gesicht.", "tokens": ["hos\u00b7ti\u00b7en\u00b7gleich", "mir", "zu\u00b7ge\u00b7wen\u00b7det", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Da du aus meinen Armen dich gel\u00f6st, hat dein gefl\u00fcstert", "tokens": ["Da", "du", "aus", "mei\u00b7nen", "Ar\u00b7men", "dich", "ge\u00b7l\u00f6st", ",", "hat", "dein", "ge\u00b7fl\u00fcs\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,", "VAFIN", "PPOSAT", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "\u00bbich mu\u00df fort\u00ab nur an die fernsten Tore", "tokens": ["\u00bb", "ich", "mu\u00df", "fort", "\u00ab", "nur", "an", "die", "ferns\u00b7ten", "To\u00b7re"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PTKVZ", "$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "meines Traums gereicht \u2013", "tokens": ["mei\u00b7nes", "Traums", "ge\u00b7reicht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Nun seh ich, wie durch Schleier, deine Hand,", "tokens": ["Nun", "seh", "ich", ",", "wie", "durch", "Schlei\u00b7er", ",", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "wie sie mit leichtem Griff das wei\u00dfe Hemd", "tokens": ["wie", "sie", "mit", "leich\u00b7tem", "Griff", "das", "wei\u00b7\u00dfe", "Hemd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "die Br\u00fcste niederstreicht ...", "tokens": ["die", "Br\u00fcs\u00b7te", "nie\u00b7der\u00b7streicht", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die Str\u00fcmpfe ... nun den Rock ...", "tokens": ["Die", "Str\u00fcmp\u00b7fe", "...", "nun", "den", "Rock", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "das Haar gerafft ... schon bist du fremd,", "tokens": ["das", "Haar", "ge\u00b7rafft", "...", "schon", "bist", "du", "fremd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "f\u00fcr Tag und Welt geschm\u00fcckt ...", "tokens": ["f\u00fcr", "Tag", "und", "Welt", "ge\u00b7schm\u00fcckt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ich \u00f6ffne leis die T\u00fcre ... k\u00fcsse dich ... du nickst,", "tokens": ["Ich", "\u00f6ff\u00b7ne", "leis", "die", "T\u00fc\u00b7re", "...", "k\u00fcs\u00b7se", "dich", "...", "du", "nickst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$(", "VVFIN", "PPER", "$(", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "schon fern, ein Lebewohl ... und bist entr\u00fcckt.", "tokens": ["schon", "fern", ",", "ein", "Le\u00b7be\u00b7wohl", "...", "und", "bist", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "$(", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "lin