{"dta.poem.23462": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anhang.  \n Stimmen aus der Vergangenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Asche will nicht lassen ab,", "tokens": ["Die", "A\u00b7sche", "will", "nicht", "las\u00b7sen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie st\u00e4ubt in allen Landen.", "tokens": ["Sie", "st\u00e4ubt", "in", "al\u00b7len", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hie hilft kein Bach, Loch, Grub noch Grab,", "tokens": ["Hie", "hilft", "kein", "Bach", ",", "Loch", ",", "Grub", "noch", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie macht den Feind zu Schanden.", "tokens": ["Sie", "macht", "den", "Feind", "zu", "Schan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die er im Leben durch den Mord", "tokens": ["Die", "er", "im", "Le\u00b7ben", "durch", "den", "Mord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu schweigen hat gedrungen,", "tokens": ["Zu", "schwei\u00b7gen", "hat", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die mu\u00df er todt an allem Ort", "tokens": ["Die", "mu\u00df", "er", "todt", "an", "al\u00b7lem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit aller Stimm und Zungen", "tokens": ["Mit", "al\u00b7ler", "Stimm", "und", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gar fr\u00f6hlich lassen singen.", "tokens": ["Gar", "fr\u00f6h\u00b7lich", "las\u00b7sen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}