{"textgrid.poem.63929": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Hecht regierte lange Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hecht regierte lange Zeit", "tokens": ["Ein", "Hecht", "re\u00b7gier\u00b7te", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Wasser weit und breit,", "tokens": ["In", "ei\u00b7nem", "Was\u00b7ser", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubte voller Stolz, nun sey er", "tokens": ["Und", "glaub\u00b7te", "vol\u00b7ler", "Stolz", ",", "nun", "sey", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der F\u00fcrst und Herr im ganzen Weiher.", "tokens": ["Der", "F\u00fcrst", "und", "Herr", "im", "gan\u00b7zen", "Wei\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was hindert mich denn, (fing er an)", "tokens": ["Was", "hin\u00b7dert", "mich", "denn", ",", "(", "fing", "er", "an", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "$(", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich im weiten Ocean", "tokens": ["Da\u00df", "ich", "im", "wei\u00b7ten", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Nicht eben so gewaltsam w\u00fcthe,", "tokens": ["Nicht", "e\u00b7ben", "so", "ge\u00b7walt\u00b7sam", "w\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht eben so als Herr gebiete,", "tokens": ["Nicht", "e\u00b7ben", "so", "als", "Herr", "ge\u00b7bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie hier? Er sagt's und schwimmt sogleich", "tokens": ["Wie", "hier", "?", "Er", "sagt's", "und", "schwimmt", "sog\u00b7leich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hinab in's gro\u00dfe Wasserreich.", "tokens": ["Hin\u00b7ab", "in's", "gro\u00b7\u00dfe", "Was\u00b7ser\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch wie erschrak er, da er nah'", "tokens": ["Doch", "wie", "er\u00b7schrak", "er", ",", "da", "er", "nah'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des Meeres Ungeheuer sah!", "tokens": ["Des", "Mee\u00b7res", "Un\u00b7ge\u00b7heu\