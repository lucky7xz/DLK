{"textgrid.poem.59345": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Fl\u00fcgelro\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab nicht viel hienieden,", "tokens": ["Ich", "hab", "nicht", "viel", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab nicht Geld noch Gut;", "tokens": ["Ich", "hab", "nicht", "Geld", "noch", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was vielen nicht beschieden,", "tokens": ["Was", "vie\u00b7len", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mein: \u2013 der frische Mut.", "tokens": ["Ist", "mein", ":", "\u2013", "der", "fri\u00b7sche", "Mut", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was andre mag erg\u00f6tzen,", "tokens": ["Was", "and\u00b7re", "mag", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das k\u00fcmmert wenig mich,", "tokens": ["Das", "k\u00fcm\u00b7mert", "we\u00b7nig", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie leben in den Sch\u00e4tzen,", "tokens": ["Sie", "le\u00b7ben", "in", "den", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Freuden lebe ich.", "tokens": ["In", "Freu\u00b7den", "le\u00b7be", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich hab ein Ro\u00df mit Fl\u00fcgeln,", "tokens": ["Ich", "hab", "ein", "Ro\u00df", "mit", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Getreu in Lust und Not,", "tokens": ["Ge\u00b7treu", "in", "Lust", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wiehernd spannt die Fl\u00fcgel", "tokens": ["Das", "wie\u00b7hernd", "spannt", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei jedem Morgenrot.", "tokens": ["Bei", "je\u00b7dem", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Liebchen! wie so \u00f6de", "tokens": ["Mein", "Lieb\u00b7chen", "!", "wie", "so", "\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird's oft in Stadt und Schlo\u00df,", "tokens": ["Wird's", "oft", "in", "Stadt", "und", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frisch auf und sei nicht bl\u00f6de,", "tokens": ["Frisch", "auf", "und", "sei", "nicht", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VAFIN", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besteig mit mir mein Ro\u00df!", "tokens": ["Be\u00b7steig", "mit", "mir", "mein", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir segeln durch die R\u00e4ume", "tokens": ["Wir", "se\u00b7geln", "durch", "die", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich zeig dir Meer und Land,", "tokens": ["Ich", "zeig", "dir", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie wunderbare Tr\u00e4ume", "tokens": ["Wie", "wun\u00b7der\u00b7ba\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief unten ausgespannt.", "tokens": ["Tief", "un\u00b7ten", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hellblinkend zu den F\u00fc\u00dfen", "tokens": ["Hell\u00b7blin\u00b7kend", "zu", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unz\u00e4hl'ger Str\u00f6me Lauf \u2013", "tokens": ["Un\u00b7z\u00e4hl'\u00b7ger", "Str\u00f6\u00b7me", "Lauf", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es steigt ein Fr\u00fchlingsgr\u00fc\u00dfen", "tokens": ["Es", "steigt", "ein", "Fr\u00fch\u00b7lings\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verhallend zu uns auf.", "tokens": ["Ver\u00b7hal\u00b7lend", "zu", "uns", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und bunt und immer wilder", "tokens": ["Und", "bunt", "und", "im\u00b7mer", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Liebe, Ha\u00df und Lust", "tokens": ["In", "Lie\u00b7be", ",", "Ha\u00df", "und", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verwirren sich die Bilder \u2013", "tokens": ["Ver\u00b7wir\u00b7ren", "sich", "die", "Bil\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was schwindelt dir die Brust?", "tokens": ["Was", "schwin\u00b7delt", "dir", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So fr\u00f6hlich tief im Herzen,", "tokens": ["So", "fr\u00f6h\u00b7lich", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh ich all' himmelw\u00e4rts,", "tokens": ["Zieh", "ich", "all'", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "++-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es kommen selbst die Schmerzen", "tokens": ["Es", "kom\u00b7men", "selbst", "die", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Melodisch an das Herz.", "tokens": ["Me\u00b7lo\u00b7disch", "an", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der S\u00e4nger zwingt mit Kl\u00e4ngen", "tokens": ["Der", "S\u00e4n\u00b7ger", "zwingt", "mit", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was st\u00f6rrig, dumpf und wild,", "tokens": ["Was", "st\u00f6r\u00b7rig", ",", "dumpf", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es spiegelt in Ges\u00e4ngen", "tokens": ["Es", "spie\u00b7gelt", "in", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt sich g\u00f6ttlich mild.", "tokens": ["Die", "Welt", "sich", "g\u00f6tt\u00b7lich", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und unten nun verbrauset", "tokens": ["Und", "un\u00b7ten", "nun", "ver\u00b7brau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des breiten Lebens Strom,", "tokens": ["Des", "brei\u00b7ten", "Le\u00b7bens", "Strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Adler einsam hauset", "tokens": ["Der", "Ad\u00b7ler", "ein\u00b7sam", "hau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im stillen Himmelsdom. \u2013", "tokens": ["Im", "stil\u00b7len", "Him\u00b7mels\u00b7dom", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und sehn wir dann den Abend", "tokens": ["Und", "sehn", "wir", "dann", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verhallen und verbl\u00fchn,", "tokens": ["Ver\u00b7hal\u00b7len", "und", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Meere, k\u00fchle labend,", "tokens": ["Im", "Mee\u00b7re", ",", "k\u00fch\u00b7le", "la\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die heil'gen Sterne gl\u00fchn:", "tokens": ["Die", "heil'\u00b7gen", "Ster\u00b7ne", "gl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So lenken wir hernieder", "tokens": ["So", "len\u00b7ken", "wir", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Waldes gr\u00fcnem Haus,", "tokens": ["Zu", "Wal\u00b7des", "gr\u00fc\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruhn vom Schwung der Lieder", "tokens": ["Und", "ruhn", "vom", "Schwung", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf bl\u00fchndem Moose aus.", "tokens": ["Auf", "bl\u00fchn\u00b7dem", "Moo\u00b7se", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "O sterndurchwebtes D\u00fcstern,", "tokens": ["O", "stern\u00b7durch\u00b7web\u00b7tes", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O heimlich stiller Grund!", "tokens": ["O", "heim\u00b7lich", "stil\u00b7ler", "Grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O s\u00fc\u00dfes Liebesfl\u00fcstern", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Lie\u00b7bes\u00b7fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So innig Mund an Mund!", "tokens": ["So", "in\u00b7nig", "Mund", "an", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Nachtigallen locken,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Liebchen atmet lind,", "tokens": ["Mein", "Lieb\u00b7chen", "at\u00b7met", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schleier zart und Locken", "tokens": ["Mit", "Schlei\u00b7er", "zart", "und", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spielt buhlerisch der Wind.", "tokens": ["Spielt", "buh\u00b7le\u00b7risch", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und schlaf denn bis zum Morgen", "tokens": ["Und", "schlaf", "denn", "bis", "zum", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sanft gelehnt an mich!", "tokens": ["So", "sanft", "ge\u00b7lehnt", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "S\u00fc\u00df sind der Liebe Sorgen,", "tokens": ["S\u00fc\u00df", "sind", "der", "Lie\u00b7be", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Liebster wacht f\u00fcr dich.", "tokens": ["Dein", "Liebs\u00b7ter", "wacht", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich halt die bl\u00fchnden Glieder,", "tokens": ["Ich", "halt", "die", "bl\u00fchn\u00b7den", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor s\u00fc\u00dfen Schauern bang,", "tokens": ["Vor", "s\u00fc\u00b7\u00dfen", "Schau\u00b7ern", "bang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich la\u00df dich ja nicht wieder", "tokens": ["Ich", "la\u00df", "dich", "ja", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein ganzes Leben lang! \u2013", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "lang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Aurora will sich heben,", "tokens": ["Au\u00b7ro\u00b7ra", "will", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schl\u00e4gst die Augen auf,", "tokens": ["Du", "schl\u00e4gst", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wonniges Erbeben,", "tokens": ["O", "won\u00b7ni\u00b7ges", "Er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sch\u00f6ner Lebenslauf! \u2013", "tokens": ["O", "sch\u00f6\u00b7ner", "Le\u00b7bens\u00b7lauf", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Ich hab nicht viel hienieden,", "tokens": ["Ich", "hab", "nicht", "viel", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab nicht Geld noch Gut;", "tokens": ["Ich", "hab", "nicht", "Geld", "noch", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was vielen nicht beschieden,", "tokens": ["Was", "vie\u00b7len", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mein: \u2013 der frische Mut.", "tokens": ["Ist", "mein", ":", "\u2013", "der", "fri\u00b7sche", "Mut", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Was andre mag erg\u00f6tzen,", "tokens": ["Was", "and\u00b7re", "mag", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das k\u00fcmmert wenig mich,", "tokens": ["Das", "k\u00fcm\u00b7mert", "we\u00b7nig", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie leben in den Sch\u00e4tzen,", "tokens": ["Sie", "le\u00b7ben", "in", "den", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Freuden lebe ich.", "tokens": ["In", "Freu\u00b7den", "le\u00b7be", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Ich hab ein Ro\u00df mit Fl\u00fcgeln,", "tokens": ["Ich", "hab", "ein", "Ro\u00df", "mit", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Getreu in Lust und Not,", "tokens": ["Ge\u00b7treu", "in", "Lust", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wiehernd spannt die Fl\u00fcgel", "tokens": ["Das", "wie\u00b7hernd", "spannt", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei jedem Morgenrot.", "tokens": ["Bei", "je\u00b7dem", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Mein Liebchen! wie so \u00f6de", "tokens": ["Mein", "Lieb\u00b7chen", "!", "wie", "so", "\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird's oft in Stadt und Schlo\u00df,", "tokens": ["Wird's", "oft", "in", "Stadt", "und", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frisch auf und sei nicht bl\u00f6de,", "tokens": ["Frisch", "auf", "und", "sei", "nicht", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VAFIN", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besteig mit mir mein Ro\u00df!", "tokens": ["Be\u00b7steig", "mit", "mir", "mein", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wir segeln durch die R\u00e4ume", "tokens": ["Wir", "se\u00b7geln", "durch", "die", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich zeig dir Meer und Land,", "tokens": ["Ich", "zeig", "dir", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie wunderbare Tr\u00e4ume", "tokens": ["Wie", "wun\u00b7der\u00b7ba\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief unten ausgespannt.", "tokens": ["Tief", "un\u00b7ten", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Hellblinkend zu den F\u00fc\u00dfen", "tokens": ["Hell\u00b7blin\u00b7kend", "zu", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unz\u00e4hl'ger Str\u00f6me Lauf \u2013", "tokens": ["Un\u00b7z\u00e4hl'\u00b7ger", "Str\u00f6\u00b7me", "Lauf", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es steigt ein Fr\u00fchlingsgr\u00fc\u00dfen", "tokens": ["Es", "steigt", "ein", "Fr\u00fch\u00b7lings\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verhallend zu uns auf.", "tokens": ["Ver\u00b7hal\u00b7lend", "zu", "uns", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Und bunt und immer wilder", "tokens": ["Und", "bunt", "und", "im\u00b7mer", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Liebe, Ha\u00df und Lust", "tokens": ["In", "Lie\u00b7be", ",", "Ha\u00df", "und", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verwirren sich die Bilder \u2013", "tokens": ["Ver\u00b7wir\u00b7ren", "sich", "die", "Bil\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was schwindelt dir die Brust?", "tokens": ["Was", "schwin\u00b7delt", "dir", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "So fr\u00f6hlich tief im Herzen,", "tokens": ["So", "fr\u00f6h\u00b7lich", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh ich all' himmelw\u00e4rts,", "tokens": ["Zieh", "ich", "all'", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "++-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es kommen selbst die Schmerzen", "tokens": ["Es", "kom\u00b7men", "selbst", "die", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Melodisch an das Herz.", "tokens": ["Me\u00b7lo\u00b7disch", "an", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Der S\u00e4nger zwingt mit Kl\u00e4ngen", "tokens": ["Der", "S\u00e4n\u00b7ger", "zwingt", "mit", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was st\u00f6rrig, dumpf und wild,", "tokens": ["Was", "st\u00f6r\u00b7rig", ",", "dumpf", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es spiegelt in Ges\u00e4ngen", "tokens": ["Es", "spie\u00b7gelt", "in", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt sich g\u00f6ttlich mild.", "tokens": ["Die", "Welt", "sich", "g\u00f6tt\u00b7lich", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Und unten nun verbrauset", "tokens": ["Und", "un\u00b7ten", "nun", "ver\u00b7brau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des breiten Lebens Strom,", "tokens": ["Des", "brei\u00b7ten", "Le\u00b7bens", "Strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Adler einsam hauset", "tokens": ["Der", "Ad\u00b7ler", "ein\u00b7sam", "hau\u00b7set"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im stillen Himmelsdom. \u2013", "tokens": ["Im", "stil\u00b7len", "Him\u00b7mels\u00b7dom", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Und sehn wir dann den Abend", "tokens": ["Und", "sehn", "wir", "dann", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verhallen und verbl\u00fchn,", "tokens": ["Ver\u00b7hal\u00b7len", "und", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Meere, k\u00fchle labend,", "tokens": ["Im", "Mee\u00b7re", ",", "k\u00fch\u00b7le", "la\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die heil'gen Sterne gl\u00fchn:", "tokens": ["Die", "heil'\u00b7gen", "Ster\u00b7ne", "gl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "So lenken wir hernieder", "tokens": ["So", "len\u00b7ken", "wir", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Waldes gr\u00fcnem Haus,", "tokens": ["Zu", "Wal\u00b7des", "gr\u00fc\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruhn vom Schwung der Lieder", "tokens": ["Und", "ruhn", "vom", "Schwung", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf bl\u00fchndem Moose aus.", "tokens": ["Auf", "bl\u00fchn\u00b7dem", "Moo\u00b7se", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "O sterndurchwebtes D\u00fcstern,", "tokens": ["O", "stern\u00b7durch\u00b7web\u00b7tes", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O heimlich stiller Grund!", "tokens": ["O", "heim\u00b7lich", "stil\u00b7ler", "Grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O s\u00fc\u00dfes Liebesfl\u00fcstern", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Lie\u00b7bes\u00b7fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So innig Mund an Mund!", "tokens": ["So", "in\u00b7nig", "Mund", "an", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Die Nachtigallen locken,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Liebchen atmet lind,", "tokens": ["Mein", "Lieb\u00b7chen", "at\u00b7met", "lind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schleier zart und Locken", "tokens": ["Mit", "Schlei\u00b7er", "zart", "und", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spielt buhlerisch der Wind.", "tokens": ["Spielt", "buh\u00b7le\u00b7risch", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Und schlaf denn bis zum Morgen", "tokens": ["Und", "schlaf", "denn", "bis", "zum", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sanft gelehnt an mich!", "tokens": ["So", "sanft", "ge\u00b7lehnt", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "S\u00fc\u00df sind der Liebe Sorgen,", "tokens": ["S\u00fc\u00df", "sind", "der", "Lie\u00b7be", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Liebster wacht f\u00fcr dich.", "tokens": ["Dein", "Liebs\u00b7ter", "wacht", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Ich halt die bl\u00fchnden Glieder,", "tokens": ["Ich", "halt", "die", "bl\u00fchn\u00b7den", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor s\u00fc\u00dfen Schauern bang,", "tokens": ["Vor", "s\u00fc\u00b7\u00dfen", "Schau\u00b7ern", "bang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich la\u00df dich ja nicht wieder", "tokens": ["Ich", "la\u00df", "dich", "ja", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein ganzes Leben lang! \u2013", "tokens": ["Mein", "gan\u00b7zes", "Le\u00b7ben", "lang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Aurora will sich heben,", "tokens": ["Au\u00b7ro\u00b7ra", "will", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schl\u00e4gst die Augen auf,", "tokens": ["Du", "schl\u00e4gst", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wonniges Erbeben,", "tokens": ["O", "won\u00b7ni\u00b7ges", "Er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sch\u00f6ner Lebenslauf! \u2013", "tokens": ["O", "sch\u00f6\u00b7ner", "Le\u00b7bens\u00b7lauf", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}