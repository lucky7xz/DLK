{"textgrid.poem.46345": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Vater an den Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was qu\u00e4lst du dich mein Sohn, und wilst nur stets studiren?", "tokens": ["Was", "qu\u00e4lst", "du", "dich", "mein", "Sohn", ",", "und", "wilst", "nur", "stets", "stu\u00b7di\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$,", "KON", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Wissenschaft will ich dich bessre Wege f\u00fchren!", "tokens": ["Zur", "Wis\u00b7sen\u00b7schaft", "will", "ich", "dich", "bess\u00b7re", "We\u00b7ge", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm dir den Bayl' und lie\u00df und bl\u00e4ttr' ihn hin und her,", "tokens": ["Nimm", "dir", "den", "Bayl'", "und", "lie\u00df", "und", "bl\u00e4t\u00b7tr'", "ihn", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Lie\u00df weiter nichts als ihn, denn sprich die Creutz die Queer", "tokens": ["Lie\u00df", "wei\u00b7ter", "nichts", "als", "ihn", ",", "denn", "sprich", "die", "Creutz", "die", "Que\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIS", "KOKOM", "PPER", "$,", "KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vom Anaxagoras, Atlas, Hegesias,", "tokens": ["Vom", "A\u00b7na\u00b7xa\u00b7go\u00b7ras", ",", "At\u00b7las", ",", "He\u00b7ge\u00b7sias", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NE", "$,", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Anta ... Prota ... Pytha ... und Aristagoras,", "tokens": ["An\u00b7ta", "...", "Pro\u00b7ta", "...", "Py\u00b7tha", "...", "und", "A\u00b7ris\u00b7ta\u00b7go\u00b7ras", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NE", "$(", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vom Mydas, Critias, Hylas, Pausanias,", "tokens": ["Vom", "My\u00b7das", ",", "Cri\u00b7ti\u00b7as", ",", "Hy\u00b7las", ",", "Pau\u00b7sa\u00b7ni\u00b7as", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wirst du \u00fcberall als ein Orakel sprechen,", "tokens": ["So", "wirst", "du", "\u00fc\u00b7be\u00b7rall", "als", "ein", "O\u00b7ra\u00b7kel", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und es am Othem dir eh' als am Ruhm gebrechen.", "tokens": ["Und", "es", "am", "O\u00b7them", "dir", "eh'", "als", "am", "Ruhm", "ge\