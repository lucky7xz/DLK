{"dta.poem.20727": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Adler und die Pfeifv\u00f6gel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Adlererbe gab vor Zeiten               ", "tokens": ["Ein", "Ad\u00b7le\u00b7rer\u00b7be", "gab", "vor", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ersten Nachtigall den Preis,", "tokens": ["Der", "ers\u00b7ten", "Nach\u00b7ti\u00b7gall", "den", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hatte sie behorcht von weiten", "tokens": ["Er", "hat\u00b7te", "sie", "be\u00b7horcht", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon manchen Mai, da noch der Grei\u00df", "tokens": ["Schon", "man\u00b7chen", "Mai", ",", "da", "noch", "der", "Grei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Vorfahr k\u00f6niglich regierte,", "tokens": ["Sein", "Vor\u00b7fahr", "k\u00f6\u00b7nig\u00b7lich", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der auf ihr Lied nie Acht gehabt.", "tokens": ["Der", "auf", "ihr", "Lied", "nie", "Acht", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADV", "CARD", "VAPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jezt ward sie \u00f6ffentlich begabt", "tokens": ["Jezt", "ward", "sie", "\u00f6f\u00b7fent\u00b7lich", "be\u00b7gabt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Mit Ehre, die ihr l\u00e4ngst geb\u00fchrte.", "tokens": ["Mit", "Eh\u00b7re", ",", "die", "ihr", "l\u00e4ngst", "ge\u00b7b\u00fchr\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ha! nun erhub sich ein Geschrei:", "tokens": ["Ha", "!", "nun", "er\u00b7hub", "sich", "ein", "Ge\u00b7schrei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Der neue Felsbesitzer sey", "tokens": ["Der", "neu\u00b7e", "Fels\u00b7be\u00b7sit\u00b7zer", "sey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein gro\u00dfer Freund des Singechores \u2014", "tokens": ["Ein", "gro\u00b7\u00dfer", "Freund", "des", "Sin\u00b7ge\u00b7cho\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da pfiffen Amsel, Drossel, Staar,", "tokens": ["Da", "pfif\u00b7fen", "Am\u00b7sel", ",", "Dros\u00b7sel", ",", "Staar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und G\u00fcmpel, und die ganze Schaar", "tokens": ["Und", "G\u00fcm\u00b7pel", ",", "und", "die", "gan\u00b7ze", "Schaar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Versprach sich auch des Adlerohres", "tokens": ["Ver\u00b7sprach", "sich", "auch", "des", "Ad\u00b7le\u00b7roh\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Geh\u00f6r bei ihrem Schnabelschall;", "tokens": ["Ge\u00b7h\u00f6r", "bei", "ih\u00b7rem", "Schna\u00b7bel\u00b7schall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Er aber lie\u00df herab befehlen:", "tokens": ["Er", "a\u00b7ber", "lie\u00df", "her\u00b7ab", "be\u00b7feh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Schont Eure Schn\u00e4bel, eure Kehlen!", "tokens": ["Schont", "Eu\u00b7re", "Schn\u00e4\u00b7bel", ",", "eu\u00b7re", "Keh\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mein Ohr geh\u00f6rt der Nachtigall.", "tokens": ["Mein", "Ohr", "ge\u00b7h\u00f6rt", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}