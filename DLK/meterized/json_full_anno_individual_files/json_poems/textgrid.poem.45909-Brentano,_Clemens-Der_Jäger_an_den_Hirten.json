{"textgrid.poem.45909": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger an den Hirten", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den Wald mit raschen Schritten", "tokens": ["Durch", "den", "Wald", "mit", "ra\u00b7schen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trage ich die Laute hin,", "tokens": ["Tra\u00b7ge", "ich", "die", "Lau\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude singt, was Leid gelitten,", "tokens": ["Freu\u00b7de", "singt", ",", "was", "Leid", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweres Herz hat leichten Sinn.", "tokens": ["Schwe\u00b7res", "Herz", "hat", "leich\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch die B\u00fcsche mu\u00df ich dringen", "tokens": ["Durch", "die", "B\u00fc\u00b7sche", "mu\u00df", "ich", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder zu dem Felsenborn,", "tokens": ["Nie\u00b7der", "zu", "dem", "Fel\u00b7sen\u00b7born", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es schlingen sich mit Klingen", "tokens": ["Und", "es", "schlin\u00b7gen", "sich", "mit", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Saiten Ros' und Dorn.", "tokens": ["In", "die", "Sai\u00b7ten", "Ros'", "und", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Wildnis wild Gew\u00e4sser", "tokens": ["In", "der", "Wild\u00b7nis", "wild", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breche ich mir k\u00fchne Bahn,", "tokens": ["Bre\u00b7che", "ich", "mir", "k\u00fch\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klimm' ich aufw\u00e4rts in die Schl\u00f6sser,", "tokens": ["Klimm'", "ich", "auf\u00b7w\u00e4rts", "in", "die", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaun sie mich befreundet an.", "tokens": ["Schaun", "sie", "mich", "be\u00b7freun\u00b7det", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil ich alles Leben ehre,", "tokens": ["Weil", "ich", "al\u00b7les", "Le\u00b7ben", "eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheuen mich die Geister nicht,", "tokens": ["Scheu\u00b7en", "mich", "die", "Geis\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich spring' durch ihre Ch\u00f6re", "tokens": ["Und", "ich", "spring'", "durch", "ih\u00b7re", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein irrend Zauberlicht.", "tokens": ["Wie", "ein", "ir\u00b7rend", "Zau\u00b7ber\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Haus' ich n\u00e4chtlich in Kapellen", "tokens": ["Haus'", "ich", "n\u00e4cht\u00b7lich", "in", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6rt sich kein Gespenst an mir,", "tokens": ["St\u00f6rt", "sich", "kein", "Ge\u00b7spenst", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sich Wandrer gern gesellen,", "tokens": ["Weil", "sich", "Wand\u00b7rer", "gern", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn auch ich bin nicht von hier.", "tokens": ["Denn", "auch", "ich", "bin", "nicht", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PTKNEG", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Geister reichen mir den Becher,", "tokens": ["Geis\u00b7ter", "rei\u00b7chen", "mir", "den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichen mir die kalte Hand,", "tokens": ["Rei\u00b7chen", "mir", "die", "kal\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich bin ein frommer Zecher,", "tokens": ["Denn", "ich", "bin", "ein", "from\u00b7mer", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheue nicht den gl\u00fchen Rand.", "tokens": ["Scheu\u00b7e", "nicht", "den", "gl\u00fc\u00b7hen", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Sirene in den Wogen,", "tokens": ["Die", "Si\u00b7re\u00b7ne", "in", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' sie mich im Wasserschlo\u00df,", "tokens": ["H\u00e4tt'", "sie", "mich", "im", "Was\u00b7ser\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4be, den sie hingezogen,", "tokens": ["G\u00e4\u00b7be", ",", "den", "sie", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern den Fischer wieder los.", "tokens": ["Gern", "den", "Fi\u00b7scher", "wie\u00b7der", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber ich mu\u00df fort nach Thule,", "tokens": ["A\u00b7ber", "ich", "mu\u00df", "fort", "nach", "Thu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKVZ", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchen auf des Meeres Grund", "tokens": ["Su\u00b7chen", "auf", "des", "Mee\u00b7res", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Becher, meine Buhle", "tokens": ["Ei\u00b7nen", "Be\u00b7cher", ",", "mei\u00b7ne", "Buh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinkt sich nur aus ihm gesund.", "tokens": ["Trinkt", "sich", "nur", "aus", "ihm", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Wo die Sch\u00e4tze sind begraben", "tokens": ["Wo", "die", "Sch\u00e4t\u00b7ze", "sind", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich l\u00e4ngst, Geduld, Geduld,", "tokens": ["Wei\u00df", "ich", "l\u00e4ngst", ",", "Ge\u00b7duld", ",", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Sch\u00e4tze werd' ich haben", "tokens": ["Al\u00b7le", "Sch\u00e4t\u00b7ze", "werd'", "ich", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu bezahlen alle Schuld.", "tokens": ["Zu", "be\u00b7zah\u00b7len", "al\u00b7le", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "W\u00e4hrend ich dies Lied gesungen,", "tokens": ["W\u00e4h\u00b7rend", "ich", "dies", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahet sich des Waldes Rand,", "tokens": ["Na\u00b7het", "sich", "des", "Wal\u00b7des", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Laubes D\u00e4mmerungen", "tokens": ["Aus", "des", "Lau\u00b7bes", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trete ich ins offne Land.", "tokens": ["Tre\u00b7te", "ich", "ins", "off\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aus den Eichen zu den Myrten,", "tokens": ["Aus", "den", "Ei\u00b7chen", "zu", "den", "Myr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Laube in das Zelt,", "tokens": ["Aus", "der", "Lau\u00b7be", "in", "das", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hat der J\u00e4ger sich dem Hirten,", "tokens": ["Hat", "der", "J\u00e4\u00b7ger", "sich", "dem", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00f6te sich dem Horn gesellt.", "tokens": ["Fl\u00f6\u00b7te", "sich", "dem", "Horn", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da\u00df du leicht die L\u00e4mmer h\u00fctest", "tokens": ["Da\u00df", "du", "leicht", "die", "L\u00e4m\u00b7mer", "h\u00fc\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00e4hm' ich dir des Wolfes Wut,", "tokens": ["Z\u00e4hm'", "ich", "dir", "des", "Wol\u00b7fes", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil du fromm die H\u00e4nde bietest,", "tokens": ["Weil", "du", "fromm", "die", "H\u00e4n\u00b7de", "bie\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' ich deines Herdes Glut.", "tokens": ["Werd'", "ich", "dei\u00b7nes", "Her\u00b7des", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und willst du die Arme schlingen", "tokens": ["Und", "willst", "du", "die", "Ar\u00b7me", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um dein Liebchen zwei und zwei,", "tokens": ["Um", "dein", "Lieb\u00b7chen", "zwei", "und", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "CARD", "KON", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich dir den Fels schon zwingen,", "tokens": ["Will", "ich", "dir", "den", "Fels", "schon", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er eine Laube sei.", "tokens": ["Da\u00df", "er", "ei\u00b7ne", "Lau\u00b7be", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Du kannst Kr\u00e4nze schlingen, singen,", "tokens": ["Du", "kannst", "Kr\u00e4n\u00b7ze", "schlin\u00b7gen", ",", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnitzen, spitzen Pfeile s\u00fc\u00df,", "tokens": ["Schnit\u00b7zen", ",", "spit\u00b7zen", "Pfei\u00b7le", "s\u00fc\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich kann ringen, klingen, schwingen", "tokens": ["Ich", "kann", "rin\u00b7gen", ",", "klin\u00b7gen", ",", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlank und blank den J\u00e4gerspie\u00df.", "tokens": ["Schlank", "und", "blank", "den", "J\u00e4\u00b7ger\u00b7spie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Gieb die Pfeile, nimm den Bogen,", "tokens": ["Gieb", "die", "Pfei\u00b7le", ",", "nimm", "den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir ist's Ernst und dir ist's Scherz,", "tokens": ["Mir", "ist's", "Ernst", "und", "dir", "ist's", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "KON", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' die Senne ich gezogen", "tokens": ["Hab'", "die", "Sen\u00b7ne", "ich", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du gezielt, so trifft's ins Herz.", "tokens": ["Du", "ge\u00b7zielt", ",", "so", "trifft's", "ins", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Durch den Wald mit raschen Schritten", "tokens": ["Durch", "den", "Wald", "mit", "ra\u00b7schen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trage ich die Laute hin,", "tokens": ["Tra\u00b7ge", "ich", "die", "Lau\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude singt, was Leid gelitten,", "tokens": ["Freu\u00b7de", "singt", ",", "was", "Leid", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweres Herz hat leichten Sinn.", "tokens": ["Schwe\u00b7res", "Herz", "hat", "leich\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Durch die B\u00fcsche mu\u00df ich dringen", "tokens": ["Durch", "die", "B\u00fc\u00b7sche", "mu\u00df", "ich", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder zu dem Felsenborn,", "tokens": ["Nie\u00b7der", "zu", "dem", "Fel\u00b7sen\u00b7born", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es schlingen sich mit Klingen", "tokens": ["Und", "es", "schlin\u00b7gen", "sich", "mit", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Saiten Ros' und Dorn.", "tokens": ["In", "die", "Sai\u00b7ten", "Ros'", "und", "Dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "In der Wildnis wild Gew\u00e4sser", "tokens": ["In", "der", "Wild\u00b7nis", "wild", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breche ich mir k\u00fchne Bahn,", "tokens": ["Bre\u00b7che", "ich", "mir", "k\u00fch\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klimm' ich aufw\u00e4rts in die Schl\u00f6sser,", "tokens": ["Klimm'", "ich", "auf\u00b7w\u00e4rts", "in", "die", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaun sie mich befreundet an.", "tokens": ["Schaun", "sie", "mich", "be\u00b7freun\u00b7det", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Weil ich alles Leben ehre,", "tokens": ["Weil", "ich", "al\u00b7les", "Le\u00b7ben", "eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheuen mich die Geister nicht,", "tokens": ["Scheu\u00b7en", "mich", "die", "Geis\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich spring' durch ihre Ch\u00f6re", "tokens": ["Und", "ich", "spring'", "durch", "ih\u00b7re", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein irrend Zauberlicht.", "tokens": ["Wie", "ein", "ir\u00b7rend", "Zau\u00b7ber\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Haus' ich n\u00e4chtlich in Kapellen", "tokens": ["Haus'", "ich", "n\u00e4cht\u00b7lich", "in", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6rt sich kein Gespenst an mir,", "tokens": ["St\u00f6rt", "sich", "kein", "Ge\u00b7spenst", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sich Wandrer gern gesellen,", "tokens": ["Weil", "sich", "Wand\u00b7rer", "gern", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn auch ich bin nicht von hier.", "tokens": ["Denn", "auch", "ich", "bin", "nicht", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PTKNEG", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Geister reichen mir den Becher,", "tokens": ["Geis\u00b7ter", "rei\u00b7chen", "mir", "den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichen mir die kalte Hand,", "tokens": ["Rei\u00b7chen", "mir", "die", "kal\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich bin ein frommer Zecher,", "tokens": ["Denn", "ich", "bin", "ein", "from\u00b7mer", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheue nicht den gl\u00fchen Rand.", "tokens": ["Scheu\u00b7e", "nicht", "den", "gl\u00fc\u00b7hen", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Die Sirene in den Wogen,", "tokens": ["Die", "Si\u00b7re\u00b7ne", "in", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' sie mich im Wasserschlo\u00df,", "tokens": ["H\u00e4tt'", "sie", "mich", "im", "Was\u00b7ser\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4be, den sie hingezogen,", "tokens": ["G\u00e4\u00b7be", ",", "den", "sie", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern den Fischer wieder los.", "tokens": ["Gern", "den", "Fi\u00b7scher", "wie\u00b7der", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Aber ich mu\u00df fort nach Thule,", "tokens": ["A\u00b7ber", "ich", "mu\u00df", "fort", "nach", "Thu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKVZ", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchen auf des Meeres Grund", "tokens": ["Su\u00b7chen", "auf", "des", "Mee\u00b7res", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Becher, meine Buhle", "tokens": ["Ei\u00b7nen", "Be\u00b7cher", ",", "mei\u00b7ne", "Buh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinkt sich nur aus ihm gesund.", "tokens": ["Trinkt", "sich", "nur", "aus", "ihm", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.24": {"line.1": {"text": "Wo die Sch\u00e4tze sind begraben", "tokens": ["Wo", "die", "Sch\u00e4t\u00b7ze", "sind", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich l\u00e4ngst, Geduld, Geduld,", "tokens": ["Wei\u00df", "ich", "l\u00e4ngst", ",", "Ge\u00b7duld", ",", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Sch\u00e4tze werd' ich haben", "tokens": ["Al\u00b7le", "Sch\u00e4t\u00b7ze", "werd'", "ich", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu bezahlen alle Schuld.", "tokens": ["Zu", "be\u00b7zah\u00b7len", "al\u00b7le", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "W\u00e4hrend ich dies Lied gesungen,", "tokens": ["W\u00e4h\u00b7rend", "ich", "dies", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahet sich des Waldes Rand,", "tokens": ["Na\u00b7het", "sich", "des", "Wal\u00b7des", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Laubes D\u00e4mmerungen", "tokens": ["Aus", "des", "Lau\u00b7bes", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trete ich ins offne Land.", "tokens": ["Tre\u00b7te", "ich", "ins", "off\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Aus den Eichen zu den Myrten,", "tokens": ["Aus", "den", "Ei\u00b7chen", "zu", "den", "Myr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Laube in das Zelt,", "tokens": ["Aus", "der", "Lau\u00b7be", "in", "das", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hat der J\u00e4ger sich dem Hirten,", "tokens": ["Hat", "der", "J\u00e4\u00b7ger", "sich", "dem", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00f6te sich dem Horn gesellt.", "tokens": ["Fl\u00f6\u00b7te", "sich", "dem", "Horn", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Da\u00df du leicht die L\u00e4mmer h\u00fctest", "tokens": ["Da\u00df", "du", "leicht", "die", "L\u00e4m\u00b7mer", "h\u00fc\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00e4hm' ich dir des Wolfes Wut,", "tokens": ["Z\u00e4hm'", "ich", "dir", "des", "Wol\u00b7fes", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil du fromm die H\u00e4nde bietest,", "tokens": ["Weil", "du", "fromm", "die", "H\u00e4n\u00b7de", "bie\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' ich deines Herdes Glut.", "tokens": ["Werd'", "ich", "dei\u00b7nes", "Her\u00b7des", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Und willst du die Arme schlingen", "tokens": ["Und", "willst", "du", "die", "Ar\u00b7me", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um dein Liebchen zwei und zwei,", "tokens": ["Um", "dein", "Lieb\u00b7chen", "zwei", "und", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "CARD", "KON", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich dir den Fels schon zwingen,", "tokens": ["Will", "ich", "dir", "den", "Fels", "schon", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er eine Laube sei.", "tokens": ["Da\u00df", "er", "ei\u00b7ne", "Lau\u00b7be", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Du kannst Kr\u00e4nze schlingen, singen,", "tokens": ["Du", "kannst", "Kr\u00e4n\u00b7ze", "schlin\u00b7gen", ",", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnitzen, spitzen Pfeile s\u00fc\u00df,", "tokens": ["Schnit\u00b7zen", ",", "spit\u00b7zen", "Pfei\u00b7le", "s\u00fc\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich kann ringen, klingen, schwingen", "tokens": ["Ich", "kann", "rin\u00b7gen", ",", "klin\u00b7gen", ",", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlank und blank den J\u00e4gerspie\u00df.", "tokens": ["Schlank", "und", "blank", "den", "J\u00e4\u00b7ger\u00b7spie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Gieb die Pfeile, nimm den Bogen,", "tokens": ["Gieb", "die", "Pfei\u00b7le", ",", "nimm", "den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir ist's Ernst und dir ist's Scherz,", "tokens": ["Mir", "ist's", "Ernst", "und", "dir", "ist's", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "KON", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' die Senne ich gezogen", "tokens": ["Hab'", "die", "Sen\u00b7ne", "ich", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du gezielt, so trifft's ins Herz.", "tokens": ["Du", "ge\u00b7zielt", ",", "so", "trifft's", "ins", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}