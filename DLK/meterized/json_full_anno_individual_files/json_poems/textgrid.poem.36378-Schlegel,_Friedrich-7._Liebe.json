{"textgrid.poem.36378": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7. Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz fr\u00fch, wann sich entz\u00fcndet", "tokens": ["Ganz", "fr\u00fch", ",", "wann", "sich", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der silberwei\u00dfe Tag,", "tokens": ["Der", "sil\u00b7ber\u00b7wei\u00b7\u00dfe", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und uns die Sonn' verk\u00fcndet,", "tokens": ["Und", "uns", "die", "Sonn'", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was nachts verborgen lag,", "tokens": ["Was", "nachts", "ver\u00b7bor\u00b7gen", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Lieb' in meinem Herzen", "tokens": ["Die", "Lieb'", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Fl\u00e4mmlein z\u00fcndet an,", "tokens": ["Ein", "Fl\u00e4mm\u00b7lein", "z\u00fcn\u00b7det", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das brennt gleich einer Kerzen,", "tokens": ["Das", "brennt", "gleich", "ei\u00b7ner", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So niemand l\u00f6schen kann.", "tokens": ["So", "nie\u00b7mand", "l\u00f6\u00b7schen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wann schon ich's trag im Winde", "tokens": ["Wann", "schon", "ich's", "trag", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Ost und Norden draus,", "tokens": ["Nach", "Ost", "und", "Nor\u00b7den", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch Ruh noch Rast ich finde,", "tokens": ["Noch", "Ruh", "noch", "Rast", "ich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es l\u00f6schet nimmer aus.", "tokens": ["Es", "l\u00f6\u00b7schet", "nim\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O weh der Qual und Peinen,", "tokens": ["O", "weh", "der", "Qual", "und", "Pei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo soll mich wenden hin?", "tokens": ["Wo", "soll", "mich", "wen\u00b7den", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich immerdar mu\u00df weinen,", "tokens": ["Ich", "im\u00b7mer\u00b7dar", "mu\u00df", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stets ich in Schmerzen bin.", "tokens": ["Stets", "ich", "in", "Schmer\u00b7zen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wann wieder dann entflogen", "tokens": ["Wann", "wie\u00b7der", "dann", "ent\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tag zur Nacht hinein,", "tokens": ["Der", "Tag", "zur", "Nacht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unter sich gebogen", "tokens": ["Und", "un\u00b7ter", "sich", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonn' und Sonnenschein;", "tokens": ["Die", "Sonn'", "und", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Fl\u00e4mmlein so mich qu\u00e4let", "tokens": ["Das", "Fl\u00e4mm\u00b7lein", "so", "mich", "qu\u00e4\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch bleibt in voller Glut,", "tokens": ["Noch", "bleibt", "in", "vol\u00b7ler", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All' Stund', so viel man z\u00e4hlet,", "tokens": ["All'", "Stund'", ",", "so", "viel", "man", "z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es mich noch brennen tut.", "tokens": ["Es", "mich", "noch", "bren\u00b7nen", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Fl\u00e4mmlein, so ich meine,", "tokens": ["Das", "Fl\u00e4mm\u00b7lein", ",", "so", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Jesus s\u00fc\u00dfer Nam';", "tokens": ["Ist", "Je\u00b7sus", "s\u00fc\u00b7\u00dfer", "Nam'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zehret Mark und Beine,", "tokens": ["Es", "zeh\u00b7ret", "Mark", "und", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fri\u00dft ein gar wundersam.", "tokens": ["Fri\u00dft", "ein", "gar", "wun\u00b7der\u00b7sam", "."], "toke