{"dta.poem.13468": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Sonne lieblich schiene", "tokens": ["Wenn", "die", "Son\u00b7ne", "lieb\u00b7lich", "schie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie in W\u00e4lschland, lau und blau.", "tokens": ["Wie", "in", "W\u00e4l\u00b7schland", ",", "lau", "und", "blau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ging' ich mit der Mandoline", "tokens": ["Ging'", "ich", "mit", "der", "Man\u00b7do\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "+--+----", "measure": "dactylic.di.plus"}, "line.4": {"text": "Durch die \u00fcbergl\u00e4nzte Au.", "tokens": ["Durch", "die", "\u00fc\u00b7ber\u00b7gl\u00e4nz\u00b7te", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der Nacht dann Liebchen lauschte", "tokens": ["In", "der", "Nacht", "dann", "Lieb\u00b7chen", "lauschte"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Fenster s\u00fc\u00df verwacht,", "tokens": ["An", "dem", "Fens\u00b7ter", "s\u00fc\u00df", "ver\u00b7wacht", ","], "token_info