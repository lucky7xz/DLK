{"dta.poem.2059": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sondern uns Gefl\u00fcgel jagt. Aus der Luft, auch aus", "tokens": ["Son\u00b7dern", "uns", "Ge\u00b7fl\u00fc\u00b7gel", "jagt", ".", "Aus", "der", "Luft", ",", "auch", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "NN", "VVFIN", "$.", "APPR", "ART", "NN", "$,", "ADV", "APPR"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "der Flut,", "tokens": ["der", "Flut", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Was geschossen, holt und bringet, und so viele Dinge", "tokens": ["Was", "ge\u00b7schos\u00b7sen", ",", "holt", "und", "brin\u00b7get", ",", "und", "so", "vie\u00b7le", "Din\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$,", "KON", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "thut,", "tokens": ["thut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die nicht alle zu erz\u00e4hlen. Zu so viel verschiednen Sa-", "tokens": ["Die", "nicht", "al\u00b7le", "zu", "er\u00b7z\u00e4h\u00b7len", ".", "Zu", "so", "viel", "ver\u00b7schied\u00b7nen", "Sa"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "PIS", "PTKZU", "VVINF", "$.", "APPR", "ADV", "PIAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "chen", "tokens": ["chen"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Dieses wundersame Thier uns zu Gut geschickt zu machen,", "tokens": ["Die\u00b7ses", "wun\u00b7der\u00b7sa\u00b7me", "Thier", "uns", "zu", "Gut", "ge\u00b7schickt", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PPER", "PTKA", "ADJD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Hat es die Natur f\u00fcr uns so verschiedentlich formiret,", "tokens": ["Hat", "es", "die", "Na\u00b7tur", "f\u00fcr", "uns", "so", "ver\u00b7schie\u00b7dent\u00b7lich", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Wie man es an keinem Thier auf der ganzen Welt ver-", "tokens": ["Wie", "man", "es", "an", "kei\u00b7nem", "Thier", "auf", "der", "gan\u00b7zen", "Welt", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "APPR", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "sp\u00fcret:", "tokens": ["sp\u00fc\u00b7ret", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Viele sind besonders gro\u00df, da hingegen andre klein,", "tokens": ["Vie\u00b7le", "sind", "be\u00b7son\u00b7ders", "gro\u00df", ",", "da", "hin\u00b7ge\u00b7gen", "and\u00b7re", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ADV", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Viele stark und schwer von Gliedern, andre schlank, ge-", "tokens": ["Vie\u00b7le", "stark", "und", "schwer", "von", "Glie\u00b7dern", ",", "and\u00b7re", "schlank", ",", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "ADJD", "KON", "ADJD", "APPR", "NN", "$,", "PIS", "VVFIN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "lenk, gewandt,", "tokens": ["lenk", ",", "ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Zotticht, stroblicht, kahl und glatt: ja es scheint ein\u2019 Art", "tokens": ["Zot\u00b7ticht", ",", "strob\u00b7licht", ",", "kahl", "und", "glatt", ":", "ja", "es", "scheint", "ein'", "Art"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$.", "ADV", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Verstand", "tokens": ["Ver\u00b7stand"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Diesem Thier fast beyzuwohnen. Da\u00df es keck, gehor-", "tokens": ["Die\u00b7sem", "Thier", "fast", "bey\u00b7zu\u00b7woh\u00b7nen", ".", "Da\u00df", "es", "keck", ",", "ge\u00b7hor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "ADV", "VVIZU", "$.", "KOUS", "PPER", "ADJD", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "sam, treu,", "tokens": ["sam", ",", "treu", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Munter, freundlich, k\u00fchn, verschmitzt, schmeichelnd", "tokens": ["Mun\u00b7ter", ",", "freund\u00b7lich", ",", "k\u00fchn", ",", "ver\u00b7schmitzt", ",", "schmei\u00b7chelnd"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "VVPP", "$,", "ADJD"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.19": {"text": "und gelehrig sey,", "tokens": ["und", "ge\u00b7leh\u00b7rig", "sey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "Da\u00df es tausend K\u00fcnste lernt, ist uns allen ja bekannt.", "tokens": ["Da\u00df", "es", "tau\u00b7send", "K\u00fcns\u00b7te", "lernt", ",", "ist", "uns", "al\u00b7len", "ja", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PIS", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Witz, Ged\u00e4chtni\u00df, Schlauigkeit mu\u00df man ihnen zuge-", "tokens": ["Witz", ",", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ",", "Schlau\u00b7ig\u00b7keit", "mu\u00df", "man", "ih\u00b7nen", "zu\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VMFIN", "PIS", "PPER", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "stehen;", "tokens": ["ste\u00b7hen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Sonderlich hat die Natur sie mit einer Kraft versehen", "tokens": ["Son\u00b7der\u00b7lich", "hat", "die", "Na\u00b7tur", "sie", "mit", "ei\u00b7ner", "Kraft", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.24": {"text": "Jm Geruch, der unbegreiflich, da sie Wege finden k\u00f6n-", "tokens": ["Jm", "Ge\u00b7ruch", ",", "der", "un\u00b7be\u00b7greif\u00b7lich", ",", "da", "sie", "We\u00b7ge", "fin\u00b7den", "k\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "$,", "KOUS", "PPER", "NN", "VVINF", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "nen,", "tokens": ["nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Und die Spuren unterscheiden, Dinge die man fast nicht", "tokens": ["Und", "die", "Spu\u00b7ren", "un\u00b7ter\u00b7schei\u00b7den", ",", "Din\u00b7ge", "die", "man", "fast", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "NN", "ART", "PIS", "ADV", "PTKNEG"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.27": {"text": "nennen,", "tokens": ["nen\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.28": {"text": "Z\u00e4hlen noch begreifen kann, t\u00fccht- und f\u00e4hig auszu-", "tokens": ["Z\u00e4h\u00b7len", "noch", "be\u00b7grei\u00b7fen", "kann", ",", "t\u00fccht", "und", "f\u00e4\u00b7hig", "aus\u00b7zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVINF", "VMFIN", "$,", "TRUNC", "KON", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "sp\u00e4hn;", "tokens": ["sp\u00e4hn", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Ja so gar selbst in Gem\u00e4hlden ihren eignen Herrn zu", "tokens": ["Ja", "so", "gar", "selbst", "in", "Ge\u00b7m\u00e4hl\u00b7den", "ih\u00b7ren", "eig\u00b7nen", "Herrn", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ADV", "ADV", "APPR", "NN", "PPOSAT", "ADJA", "NN", "PTKZU"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.31": {"text": "kennen,", "tokens": ["ken\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Wie ich solches mit Verwundrung selbst mit Augen an-", "tokens": ["Wie", "ich", "sol\u00b7ches", "mit", "Ver\u00b7wund\u00b7rung", "selbst", "mit", "Au\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "APPR", "NN", "ADV", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.33": {"text": "gesehn.", "tokens": ["ge\u00b7sehn", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}