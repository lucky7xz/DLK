": ["Er", ",", "nicht", "die", "Lust", ",", "sei", "Grund\u00b7ton", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PTKNEG", "ART", "NN", "$,", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der l\u00f6schte mir das s\u00fc\u00dfe Bild durch Tr\u00e4nen,", "tokens": ["Der", "l\u00f6schte", "mir", "das", "s\u00fc\u00b7\u00dfe", "Bild", "durch", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Und wieder zu den Schatten geht mein Sehnen.", "tokens": ["Und", "wie\u00b7der", "zu", "den", "Schat\u00b7ten", "geht", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ade! Lichtblume Du auf sonn'gen H\u00f6hen!", "tokens": ["A\u00b7de", "!", "Licht\u00b7blu\u00b7me", "Du", "auf", "sonn'\u00b7gen", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Den S\u00e4nger la\u00df nur zu den Toten gehen.", "tokens": ["Den", "S\u00e4n\u00b7ger", "la\u00df", "nur", "zu", "den", "To\u00b7ten", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}