{"textgrid.poem.41095": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An die Prinzessin Marie von W\u00fcrttemberg", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Lichtbild, das, wenn's einmal nur erscheint,", "tokens": ["Du", "Licht\u00b7bild", ",", "das", ",", "wenn's", "ein\u00b7mal", "nur", "er\u00b7scheint", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PDS", "$,", "KOUS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen anfacht, selbst im welken, kalten,", "tokens": ["Im", "Her\u00b7zen", "an\u00b7facht", ",", "selbst", "im", "wel\u00b7ken", ",", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "APPRART", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das man f\u00fcr Lust und Schmerz erstorben meint,", "tokens": ["Das", "man", "f\u00fcr", "Lust", "und", "Schmerz", "ers\u00b7tor\u00b7ben", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "NN", "KON", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Maienlicht ein neues Sichentfalten,", "tokens": ["Wie", "Mai\u00b7en\u00b7licht", "ein", "neu\u00b7es", "Si\u00b7ch\u00b7ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Hast Lebensfunken selbst in jenem alten,", "tokens": ["Hast", "Le\u00b7bens\u00b7fun\u00b