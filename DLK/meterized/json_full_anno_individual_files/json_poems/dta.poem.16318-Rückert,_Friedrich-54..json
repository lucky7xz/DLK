{"dta.poem.16318": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast der Freunde viel, und geizest nicht um einen;", "tokens": ["Du", "hast", "der", "Freun\u00b7de", "viel", ",", "und", "gei\u00b7zest", "nicht", "um", "ei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe wenige, und nannte dich den meinen,", "tokens": ["Ich", "ha\u00b7be", "we\u00b7ni\u00b7ge", ",", "und", "nann\u00b7te", "dich", "den", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "KON", "VVFIN", "PPER", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und mu\u00df im Herzen noch den meinigen dich nennen,", "tokens": ["Und", "mu\u00df", "im", "Her\u00b7zen", "noch", "den", "mei\u00b7ni\u00b7gen", "dich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ADV", "ART", "PPOSS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und darf es, wenn nicht dir, mir selber wol bekennen.", "tokens": ["Und", "darf", "es", ",", "wenn", "nicht", "dir", ",", "mir", "sel\u00b7ber", "wol", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "PTKNEG", "PPER", "$,", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was dich entfremden konnt', hab' ich nicht Lust zu fragen;", "tokens": ["Was", "dich", "ent\u00b7frem\u00b7den", "konnt'", ",", "hab'", "ich", "nicht", "Lust", "zu", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch da\u00df es m\u00f6glich war, das hab' ich zu beklagen.", "tokens": ["Doch", "da\u00df", "es", "m\u00f6g\u00b7lich", "war", ",", "das", "hab'", "ich", "zu", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}