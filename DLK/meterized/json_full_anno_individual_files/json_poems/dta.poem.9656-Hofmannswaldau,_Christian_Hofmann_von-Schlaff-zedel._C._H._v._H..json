{"dta.poem.9656": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schlaff-zedel.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer liegt den vollerey und wollust hat bestricket/", "tokens": ["Hjer", "liegt", "den", "vol\u00b7le\u00b7rey", "und", "wol\u00b7lust", "hat", "be\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "KON", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unter Bachus fahn sich wohl gehalten hat/", "tokens": ["Der", "un\u00b7ter", "Ba\u00b7chus", "fahn", "sich", "wohl", "ge\u00b7hal\u00b7ten", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "PRF", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den noch die s\u00fcsse last von starckem weine dr\u00fccket/", "tokens": ["Den", "noch", "die", "s\u00fcs\u00b7se", "last", "von", "star\u00b7ckem", "wei\u00b7ne", "dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stetig wohl gefolgt der gurgel nassem rath.", "tokens": ["Und", "ste\u00b7tig", "wohl", "ge\u00b7folgt", "der", "gur\u00b7gel", "nas\u00b7sem", "rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den manche Delila in ihrer scho\u00df geschlossen/", "tokens": ["Den", "man\u00b7che", "De\u00b7li\u00b7la", "in", "ih\u00b7rer", "scho\u00df", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dessen thaten noch das haupt von Elsa\u00df kennt/", "tokens": ["Und", "des\u00b7sen", "tha\u00b7ten", "noch", "das", "haupt", "von", "El\u00b7sa\u00df", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der manches reine feld mit seiner brunst begossen/", "tokens": ["Der", "man\u00b7ches", "rei\u00b7ne", "feld", "mit", "sei\u00b7ner", "brunst", "be\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und so mich recht bed\u00fcnckt/ auch in dem schlaffe brennt.", "tokens": ["Und", "so", "mich", "recht", "be\u00b7d\u00fcnckt", "/", "auch", "in", "dem", "schlaf\u00b7fe", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "VVPP", "$(", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schlaf/ schlaf/ es wachet doch das uhrwerck im gehirne/", "tokens": ["Schlaf", "/", "schlaf", "/", "es", "wa\u00b7chet", "doch", "das", "uhr\u00b7werck", "im", "ge\u00b7hir\u00b7ne", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Venus schl\u00fcpffrigkeit/ des Bacchus toller wein", "tokens": ["Der", "Ve\u00b7nus", "schl\u00fcpf\u00b7frig\u00b7keit", "/", "des", "Bac\u00b7chus", "tol\u00b7ler", "wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$(", "ART", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bestricket dir itzund die bo\u00dfheit-reiche stirne/", "tokens": ["Be\u00b7stri\u00b7cket", "dir", "it\u00b7zund", "die", "bo\u00df\u00b7heit\u00b7rei\u00b7che", "stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und zeiget dir mit flei\u00df/ was beyde g\u00f6tter seyn.", "tokens": ["Und", "zei\u00b7get", "dir", "mit", "flei\u00df", "/", "was", "bey\u00b7de", "g\u00f6t\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$(", "PWS", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}