{"textgrid.poem.39803": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An denselben", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir singen, F\u00fcrst, zu deinen F\u00fc\u00dfen", "tokens": ["Wir", "sin\u00b7gen", ",", "F\u00fcrst", ",", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied, das uns die Liebe lehrt,", "tokens": ["Ein", "Lied", ",", "das", "uns", "die", "Lie\u00b7be", "lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Neugebornen zu begr\u00fc\u00dfen,", "tokens": ["Den", "Neu\u00b7ge\u00b7bor\u00b7nen", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den dir und uns der Herr bescheert.", "tokens": ["Den", "dir", "und", "uns", "der", "Herr", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sind des Hauses freud'ge S\u00f6hne,", "tokens": ["Wir", "sind", "des", "Hau\u00b7ses", "freu\u00b7d'\u00b7ge", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auch du dr\u00fcckst an die Brust den Sohn;", "tokens": ["Auch", "du", "dr\u00fcckst", "an", "die", "Brust", "den", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Drum h\u00f6rst du l\u00e4chelnd unsre T\u00f6ne,", "tokens": ["Drum", "h\u00f6rst", "du", "l\u00e4\u00b7chelnd", "uns\u00b7re", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Jugend Jubel um den Thron.", "tokens": ["Der", "Ju\u00b7gend", "Ju\u00b7bel", "um", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast manch Licht uns leuchten lassen", "tokens": ["Du", "hast", "manch", "Licht", "uns", "leuch\u00b7ten", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Kindheit D\u00e4mmerung,", "tokens": ["In", "uns\u00b7rer", "Kind\u00b7heit", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wir werden's erst als M\u00e4nner fassen: \u2013", "tokens": ["Wir", "wer\u00b7den's", "erst", "als", "M\u00e4n\u00b7ner", "fas\u00b7sen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch heute freut sich Alt und Jung;", "tokens": ["Doch", "heu\u00b7te", "freut", "sich", "Alt", "und", "Jung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein kindlich Herz kann heut empfinden,", "tokens": ["Ein", "kind\u00b7lich", "Herz", "kann", "heut", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was uns der Himmel schmecken l\u00e4\u00dft;", "tokens": ["Was", "uns", "der", "Him\u00b7mel", "schme\u00b7cken", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, keine Kunst ist's, zu erfinden", "tokens": ["Ach", ",", "kei\u00b7ne", "Kunst", "ist's", ",", "zu", "er\u00b7fin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Lied zu solchem Wiegenfest.", "tokens": ["Ein", "Lied", "zu", "sol\u00b7chem", "Wie\u00b7gen\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Lied, das jedes Herz gesungen", "tokens": ["Ein", "Lied", ",", "das", "je\u00b7des", "Herz", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu des Gesch\u00fctzes Freudenhall,", "tokens": ["Zu", "des", "Ge\u00b7sch\u00fct\u00b7zes", "Freu\u00b7den\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lied, das durch die Luft geklungen", "tokens": ["Ein", "Lied", ",", "das", "durch", "die", "Luft", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aller Glocken frommem Schall.", "tokens": ["Mit", "al\u00b7ler", "Glo\u00b7cken", "from\u00b7mem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollten zu der Schule wallen,", "tokens": ["Wir", "woll\u00b7ten", "zu", "der", "Schu\u00b7le", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Vaterlandes weiser Zier:", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "wei\u00b7ser", "Zier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da h\u00f6rten wir's vom Thurme schallen,", "tokens": ["Da", "h\u00f6r\u00b7ten", "wir's", "vom", "Thur\u00b7me", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zu dem Tempel eilten wir.", "tokens": ["Und", "zu", "dem", "Tem\u00b7pel", "eil\u00b7ten", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dort scholl ein Sang, aus Lust gewoben,", "tokens": ["Dort", "scholl", "ein", "Sang", ",", "aus", "Lust", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Hoffnung, Dankbarkeit und Mut", "tokens": ["Von", "Hoff\u00b7nung", ",", "Dank\u00b7bar\u00b7keit", "und", "Mut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum treuen Himmel aufgehoben,", "tokens": ["Zum", "treu\u00b7en", "Him\u00b7mel", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und flehte f\u00fcr das neue Gut", "tokens": ["Und", "fleh\u00b7te", "f\u00fcr", "das", "neu\u00b7e", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aus der Zeit bewegten Fluten", "tokens": ["Und", "aus", "der", "Zeit", "be\u00b7weg\u00b7ten", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stieg vor uns auf der Zukunft Reich:", "tokens": ["Stieg", "vor", "uns", "auf", "der", "Zu\u00b7kunft", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sah'n ein Volk von Freien, Guten,", "tokens": ["Wir", "sah'n", "ein", "Volk", "von", "Frei\u00b7en", ",", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und einen Sohn, dem Vater gleich!", "tokens": ["Und", "ei\u00b7nen", "Sohn", ",", "dem", "Va\u00b7ter", "gleich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das schauten wir am frohen Morgen,", "tokens": ["Das", "schau\u00b7ten", "wir", "am", "fro\u00b7hen", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herr! sei deiner G\u00fcte Lohn,", "tokens": ["Das", "Herr", "!", "sei", "dei\u00b7ner", "G\u00fc\u00b7te", "Lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So m\u00f6gen sich der Mutter Sorgen", "tokens": ["So", "m\u00f6\u00b7gen", "sich", "der", "Mut\u00b7ter", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bezahlen an dem theuren Sohn;", "tokens": ["Be\u00b7zah\u00b7len", "an", "dem", "theu\u00b7ren", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit deinem Volk ist Gottes Segen,", "tokens": ["Mit", "dei\u00b7nem", "Volk", "ist", "Got\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es geht getrost der Ferne zu;", "tokens": ["Es", "geht", "ge\u00b7trost", "der", "Fer\u00b7ne", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch wir, wir wachsen ihr entgegen,", "tokens": ["Auch", "wir", ",", "wir", "wach\u00b7sen", "ihr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So Furchtlos und so Treu, wie du!", "tokens": ["So", "Furcht\u00b7los", "und", "so", "Treu", ",", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir singen, F\u00fcrst, zu deinen F\u00fc\u00dfen", "tokens": ["Wir", "sin\u00b7gen", ",", "F\u00fcrst", ",", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied, das uns die Liebe lehrt,", "tokens": ["Ein", "Lied", ",", "das", "uns", "die", 