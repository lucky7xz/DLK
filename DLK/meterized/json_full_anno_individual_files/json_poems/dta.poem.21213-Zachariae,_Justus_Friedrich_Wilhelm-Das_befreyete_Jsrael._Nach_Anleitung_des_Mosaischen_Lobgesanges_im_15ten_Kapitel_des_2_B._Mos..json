{"dta.poem.21213": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das befreyete Jsrael.  \n  Nach Anleitung des Mosaischen Lobgesanges  \n im 15ten Kapitel des 2 B. Mos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft uns dem Herrn lobsingen,", "tokens": ["La\u00dft", "uns", "dem", "Herrn", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hat die gr\u00f6\u00dfte der Thaten gethan!", "tokens": ["Er", "hat", "die", "gr\u00f6\u00df\u00b7te", "der", "Tha\u00b7ten", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Meer fuhr hinweg auf des Ostwindes Schwingen;", "tokens": ["Das", "Meer", "fuhr", "hin\u00b7weg", "auf", "des", "Ost\u00b7win\u00b7des", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Kam wieder in schrecklichen St\u00fcrmen heran,", "tokens": ["Kam", "wie\u00b7der", "in", "schreck\u00b7li\u00b7chen", "St\u00fcr\u00b7men", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und deckte Ro\u00df, und Wagen, und Mann.", "tokens": ["Und", "deck\u00b7te", "Ro\u00df", ",", "und", "Wa\u00b7gen", ",", "und", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Noch lag von Mann, und Wagen, und Ro\u00df,", "tokens": ["Noch", "lag", "von", "Mann", ",", "und", "Wa\u00b7gen", ",", "und", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Des Schilfmeers Gestade bedeckt;", "tokens": ["Des", "Schilf\u00b7meers", "Ge\u00b7sta\u00b7de", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Denn Gott ergriff sein t\u00f6dtlich Gescho\u00df,", "tokens": ["Denn", "Gott", "er\u00b7griff", "sein", "t\u00f6dt\u00b7lich", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Womit er die K\u00f6nige schreckt.", "tokens": ["Wo\u00b7mit", "er", "die", "K\u00f6\u00b7ni\u00b7ge", "schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die Wagen brausten; auf Leichnamen stunden", "tokens": ["Die", "Wa\u00b7gen", "braus\u00b7ten", ";", "auf", "Leich\u00b7na\u00b7men", "stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kinder Abrams, und schauten umher;", "tokens": ["Die", "Kin\u00b7der", "Ab\u00b7rams", ",", "und", "schau\u00b7ten", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und sieh, ihre Feinde waren verschwunden,", "tokens": ["Und", "sieh", ",", "ih\u00b7re", "Fein\u00b7de", "wa\u00b7ren", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und Pharaons Heerschaaren waren nicht mehr.", "tokens": ["Und", "Pha\u00b7raons", "Heer\u00b7schaa\u00b7ren", "wa\u00b7ren", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da kam der Geist des Herrn mit heilgem Ungest\u00fcm", "tokens": ["Da", "kam", "der", "Geist", "des", "Herrn", "mit", "heil\u00b7gem", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Mosen, seinen Knecht, herab.", "tokens": ["Auf", "Mo\u00b7sen", ",", "sei\u00b7nen", "Knecht", ",", "her\u00b7ab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sang den Sieg, den Gottes Hand ietzt gab,", "tokens": ["Er", "sang", "den", "Sieg", ",", "den", "Got\u00b7tes", "Hand", "ietzt", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alles Jsrael sang im Triumph mit ihm.", "tokens": ["Und", "al\u00b7les", "Js\u00b7rael", "sang", "im", "Tri\u00b7umph", "mit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NE", "VVFIN", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Chor von einem versammleten Volke", "tokens": ["Das", "Chor", "von", "ei\u00b7nem", "ver\u00b7samm\u00b7le\u00b7ten", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erf\u00fcllte die W\u00fcste mit Jubelgeschrey;", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "die", "W\u00fcs\u00b7te", "mit", "Ju\u00b7bel\u00b7ge\u00b7schrey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und Jubel stieg auf zur besch\u00fctzenden Wolke,", "tokens": ["Und", "Ju\u00b7bel", "stieg", "auf", "zur", "be\u00b7sch\u00fct\u00b7zen\u00b7den", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und Engel stimmten dem Jubelton bey.", "tokens": ["Und", "En\u00b7gel", "stimm\u00b7ten", "dem", "Ju\u00b7bel\u00b7ton", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich will dem Herrn lobsingen,", "tokens": ["Ich", "will", "dem", "Herrn", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er hat die gr\u00f6\u00dfte der Thaten gethan!", "tokens": ["Er", "hat", "die", "gr\u00f6\u00df\u00b7te", "der", "Tha\u00b7ten", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Meer fuhr hinweg auf des Ostwindes Schwingen;", "tokens": ["Das", "Meer", "fuhr", "hin\u00b7weg", "auf", "des", "Ost\u00b7win\u00b7des", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Kam wieder im schrecklichen Sturme heran,", "tokens": ["Kam", "wie\u00b7der", "im", "schreck\u00b7li\u00b7chen", "Stur\u00b7me", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und deckte Ro\u00df, und Wagen, und Mann.", "tokens": ["Und", "deck\u00b7te", "Ro\u00df", ",", "und", "Wa\u00b7gen", ",", "und", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Ich will dem Herrn lobsingen;", "tokens": ["Ich", "will", "dem", "Herrn", "lob\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herr ist meine St\u00e4rke,", "tokens": ["Der", "Herr", "ist", "mei\u00b7ne", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist mein Heil, mein Lobgesang.", "tokens": ["Er", "ist", "mein", "Heil", ",", "mein", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verk\u00fcndiget, ihr Himmel, seine Werke", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7get", ",", "ihr", "Him\u00b7mel", ",", "sei\u00b7ne", "Wer\u00b7ke"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vom Aufgang bis zum Niedergang!", "tokens": ["Vom", "Auf\u00b7gang", "bis", "zum", "Nie\u00b7der\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich will dem Herrn lobsingen,", "tokens": ["Ich", "will", "dem", "Herrn", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er ist der rechte Kriegesmann.", "tokens": ["Er", "ist", "der", "rech\u00b7te", "Krie\u00b7ges\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Mund gebot dem Meere zu verschlingen,", "tokens": ["Sein", "Mund", "ge\u00b7bot", "dem", "Mee\u00b7re", "zu", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und es verschlang Ro\u00df, Wagen und Mann.", "tokens": ["Und", "es", "ver\u00b7schlang", "Ro\u00df", ",", "Wa\u00b7gen", "und", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Aegypten stand auf, und die rollenden Wagen,", "tokens": ["A\u00b7e\u00b7gyp\u00b7ten", "stand", "auf", ",", "und", "die", "rol\u00b7len\u00b7den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die eisernen Reuter bedeckten das Feld.", "tokens": ["Die", "ei\u00b7ser\u00b7nen", "Reu\u00b7ter", "be\u00b7deck\u00b7ten", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die W\u00fcste stieg auf im Staub;", "tokens": ["Die", "W\u00fcs\u00b7te", "stieg", "auf", "im", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ganz Jsrael war schon ihr Raub;", "tokens": ["Ganz", "Js\u00b7rael", "war", "schon", "ihr", "Raub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Krieger befiel Entsetzen und Zagen;", "tokens": ["Die", "Krie\u00b7ger", "be\u00b7fiel", "Ent\u00b7set\u00b7zen", "und", "Za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da schaute der Herr von seinem Gezelt.", "tokens": ["Da", "schau\u00b7te", "der", "Herr", "von", "sei\u00b7nem", "Ge\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er stie\u00df die R\u00e4der mit Ungest\u00fcm", "tokens": ["Er", "stie\u00df", "die", "R\u00e4\u00b7der", "mit", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Von ihren Axen herab;", "tokens": ["Von", "ih\u00b7ren", "A\u00b7xen", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Jm dunklen Sturme kam sein Grimm,", "tokens": ["Jm", "dunk\u00b7len", "Stur\u00b7me", "kam", "sein", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das wallende Weltmeer ward ihr Grab.", "tokens": ["Das", "wal\u00b7len\u00b7de", "Welt\u00b7meer", "ward", "ihr", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Wir wollen sie erjagen,", "tokens": ["Wir", "wol\u00b7len", "sie", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedachte voller Stolz der Feind.", "tokens": ["Ge\u00b7dach\u00b7te", "vol\u00b7ler", "Stolz", "der", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Schwertes Sch\u00e4rfe soll sie schlagen;", "tokens": ["Des", "Schwer\u00b7tes", "Sch\u00e4r\u00b7fe", "soll", "sie", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aegyptens Hand soll sie verderben,", "tokens": ["A\u00b7e\u00b7gyp\u00b7tens", "Hand", "soll", "sie", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sie sollen sterben!", "tokens": ["Sie", "sol\u00b7len", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Aegyptens Hand soll sie verderben,", "tokens": ["A\u00b7e\u00b7gyp\u00b7tens", "Hand", "soll", "sie", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "wo