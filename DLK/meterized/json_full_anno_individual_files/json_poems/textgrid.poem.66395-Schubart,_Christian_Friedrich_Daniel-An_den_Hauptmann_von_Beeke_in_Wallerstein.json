{"textgrid.poem.66395": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An den Hauptmann von Beeke in Wallerstein", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Du \u00fcber die Tasten des strahlenbesaiteten Fl\u00fcgels", "tokens": ["Der", "Du", "\u00fc\u00b7ber", "die", "Tas\u00b7ten", "des", "strah\u00b7len\u00b7be\u00b7sai\u00b7te\u00b7ten", "Fl\u00fc\u00b7gels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Bald dem Weste gleich schl\u00fcpfst, wenn er den Bl\u00fcthenbusch f\u00e4chelt;", "tokens": ["Bald", "dem", "Wes\u00b7te", "gleich", "schl\u00fcpfst", ",", "wenn", "er", "den", "Bl\u00fc\u00b7then\u00b7busch", "f\u00e4\u00b7chelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+---+-++-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bald dem Sturme gleich rasest, wenn er die Wipfel der Tannen", "tokens": ["Bald", "dem", "Stur\u00b7me", "gleich", "ra\u00b7sest", ",", "wenn", "er", "die", "Wip\u00b7fel", "der", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "M\u00e4chtig r\u00fcttelt; Dir, ", "tokens": ["M\u00e4ch\u00b7tig", "r\u00fct\u00b7telt", ";", "Dir", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "L\u00e4chelt mein Genius von der wallumg\u00fcrteten Veste, \u2013", "tokens": ["L\u00e4\u00b7chelt", "mein", "Ge\u00b7nius", "von", "der", "wal\u00b7lum\u00b7g\u00fcr\u00b7te\u00b7ten", "Ves\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wo die blutige Sehnsucht in Felsenquader sich klammert;", "tokens": ["Wo", "die", "blu\u00b7ti\u00b7ge", "Sehn\u00b7sucht", "in", "Fel\u00b7sen\u00b7qua\u00b7der", "sich", "klam\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Wo des Berges Geist, von schwarzen Locken umflattert,", "tokens": ["Wo", "des", "Ber\u00b7ges", "Geist", ",", "von", "schwar\u00b7zen", "Lo\u00b7cken", "um\u00b7flat\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mit dem gr\u00e4ulichen Scheitel streift an n\u00e4chtliche Wolken,", "tokens": ["Mit", "dem", "gr\u00e4u\u00b7li\u00b7chen", "Schei\u00b7tel", "streift", "an", "n\u00e4cht\u00b7li\u00b7che", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Und die Silberstimme der Freud' im Eisengeklirre", "tokens": ["Und", "die", "Sil\u00b7ber\u00b7stim\u00b7me", "der", "Freud'", "im", "Ei\u00b7sen\u00b7ge\u00b7klir\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ewiger Fesseln verschlingt. \u2013 ", "tokens": ["E\u00b7wi\u00b7ger", "Fes\u00b7seln", "ver\u00b7schlingt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Ha, drum zucken \u00e4therische Funken vom Auge dir nieder,", "tokens": ["Ha", ",", "drum", "zu\u00b7cken", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "Fun\u00b7ken", "vom", "Au\u00b7ge", "dir", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PAV", "VVINF", "ADJA", "NN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Wenn dein feuriger Geist den H\u00e4nden Eile gebietet;", "tokens": ["Wenn", "dein", "feu\u00b7ri\u00b7ger", "Geist", "den", "H\u00e4n\u00b7den", "Ei\u00b7le", "ge\u00b7bie\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Wenn du Engeln abgehorchte Urharmonien", "tokens": ["Wenn", "du", "En\u00b7geln", "ab\u00b7ge\u00b7horch\u00b7te", "Ur\u00b7har\u00b7mo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+---+", "measure": "unknown.measure.hexa"}, "line.14": {"text": "Goldnen Saiten entlockst; und wenn du am herrschenden Fl\u00fcgel", "tokens": ["Gold\u00b7nen", "Sai\u00b7ten", "ent\u00b7lockst", ";", "und", "wenn", "du", "am", "herr\u00b7schen\u00b7den", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$.", "KON", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Strich, und Hauch, und Sang in einen gro\u00dfen Gedanken", "tokens": ["Strich", ",", "und", "Hauch", ",", "und", "Sang", "in", "ei\u00b7nen", "gro\u00b7\u00dfen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "NN", "$,", "KON", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "M\u00e4chtig wirkend verfl\u00f6\u00df'st. \u2013 Geister des Himmels umschweben", "tokens": ["M\u00e4ch\u00b7tig", "wir\u00b7kend", "ver\u00b7fl\u00f6\u00df'", "st.", "\u2013", "Geis\u00b7ter", "des", "Him\u00b7mels", "um\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "abbreviation", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "NN", "ART", "NN", "VVINF"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Deinen Steinischen Fl\u00fcgel und nicken dir Beifall. Dir lauschen", "tokens": ["Dei\u00b7nen", "Stei\u00b7ni\u00b7schen", "Fl\u00fc\u00b7gel", "und", "ni\u00b7cken", "dir", "Bei\u00b7fall", ".", "Dir", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "VVFIN", "PPER", "NN", "$.", "PPER", "VVINF"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Kenner von Kopf und Herz. Auch ich belauschte dich, Zauberer,", "tokens": ["Ken\u00b7ner", "von", "Kopf", "und", "Herz", ".", "Auch", "ich", "be\u00b7lauschte", "dich", ",", "Zau\u00b7be\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "ADV", "PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+--", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Schl\u00fcrfte die Noten in mich, die von den Fingern dir trofen;", "tokens": ["Schl\u00fcrf\u00b7te", "die", "No\u00b7ten", "in", "mich", ",", "die", "von", "den", "Fin\u00b7gern", "dir", "tro\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.20": {"text": "Silberfunkelnden Fr\u00fchthau schl\u00fcrft so die lechzende Blume.", "tokens": ["Sil\u00b7ber\u00b7fun\u00b7keln\u00b7den", "Fr\u00fch\u00b7thau", "schl\u00fcrft", "so", "die", "lech\u00b7zen\u00b7de", "Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Ach, da war ich noch frei! \u2013 Wie war ich so selig, o", "tokens": ["Ach", ",", "da", "war", "ich", "noch", "frei", "!", "\u2013", "Wie", "war", "ich", "so", "se\u00b7lig", ",", "o"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "$(", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "FM"], "meter": "+-+-++-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Hoch auf l\u00fcpft ich das Glas, vom rheinischen Traubengold blinkend,", "tokens": ["Hoch", "auf", "l\u00fcpft", "ich", "das", "Glas", ",", "vom", "rhei\u00b7ni\u00b7schen", "Trau\u00b7ben\u00b7gold", "blin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "VVFIN", "PPER", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Dr\u00fcckte dir freudig die Rechte, vom feurigen Spiele noch gl\u00fchend.", "tokens": ["Dr\u00fcck\u00b7te", "dir", "freu\u00b7dig", "die", "Rech\u00b7te", ",", "vom", "feu\u00b7ri\u00b7gen", "Spie\u00b7le", "noch", "gl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Harmonia's Lieblinge unter dem biederen Volke", "tokens": ["Har\u00b7mo\u00b7nia's", "Lieb\u00b7lin\u00b7ge", "un\u00b7ter", "dem", "bie\u00b7de\u00b7ren", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.25": {"text": "Deutschlands \u2013 (Orpheus selbst, der Griechen Lehrer, ist unser!)", "tokens": ["Deutschlands", "\u2013", "(", "Or\u00b7pheus", "selbst", ",", "der", "Grie\u00b7chen", "Leh\u00b7rer", ",", "ist", "un\u00b7ser", "!", ")"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "$(", "NE", "ADV", "$,", "ART", "NN", "NN", "$,", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Gingen vor uns, wie in Wolken vor\u00fcber. Die Bardenges\u00e4nge", "tokens": ["Gin\u00b7gen", "vor", "uns", ",", "wie", "in", "Wol\u00b7ken", "vor\u00b7\u00fc\u00b7ber", ".", "Die", "Bar\u00b7den\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "PWAV", "APPR", "NN", "PTKVZ", "$.", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Voll von Tiefsinn, der Mode Affenspr\u00fcnge verachtend.", "tokens": ["Voll", "von", "Tief\u00b7sinn", ",", "der", "Mo\u00b7de", "Af\u00b7fen\u00b7spr\u00fcn\u00b7ge", "ver\u00b7ach\u00b7tend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Alle Geweihte des sanften Klaviers erschienen. Da flogen", "tokens": ["Al\u00b7le", "Ge\u00b7weih\u00b7te", "des", "sanf\u00b7ten", "Kla\u00b7viers", "er\u00b7schie\u00b7nen", ".", "Da", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$.", "ADV", "VVFIN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.29": {"text": "S\u00fc\u00dfe goldbeschwingte Stunden l\u00e4chelnd vor\u00fcber.", "tokens": ["S\u00fc\u00b7\u00dfe", "gold\u00b7be\u00b7schwing\u00b7te", "Stun\u00b7den", "l\u00e4\u00b7chelnd", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "\u2013 Aber ", "tokens": ["\u2013", "A\u00b7ber"], "token_info": ["punct", "word"], "pos": ["$(", "KON"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Meines Saitenspiels. Vom wiedert\u00f6nenden Boden", "tokens": ["Mei\u00b7nes", "Sai\u00b7ten\u00b7spiels", ".", "Vom", "wie\u00b7der\u00b7t\u00f6\u00b7nen\u00b7den", "Bo\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+