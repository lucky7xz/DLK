{"textgrid.poem.33841": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Fest-Ode bei Einweihung der Schiller-Statue in M\u00fcnchen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schmerzlich den letzten und bangen Hauch ausatmend, der Nachtigall gleich, noch im Tode,", "tokens": ["Schmerz\u00b7lich", "den", "letz\u00b7ten", "und", "ban\u00b7gen", "Hauch", "aus\u00b7at\u00b7mend", ",", "der", "Nach\u00b7ti\u00b7gall", "gleich", ",", "noch", "im", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "KON", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-+-+--+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "So starb, was an Schiller von Staub war. Fr\u00fchlingslicht", "tokens": ["So", "starb", ",", "was", "an", "Schil\u00b7ler", "von", "Staub", "war", ".", "Fr\u00fch\u00b7lings\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPR", "NE", "APPR", "NN", "VAFIN", "$.", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ruht \u00fcber des Toten entschlafenem Antlitz, aus ist alles beengende Leid,", "tokens": ["Ruht", "\u00fc\u00b7ber", "des", "To\u00b7ten", "ent\u00b7schla\u00b7fe\u00b7nem", "Ant\u00b7litz", ",", "aus", "ist", "al\u00b7les", "be\u00b7en\u00b7gen\u00b7de", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,", "APPR", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+-+--+--+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Aus aller Kampf in tr\u00fcber Erdennacht; Tr\u00e4nen, tief empfundene Tr\u00e4nen, geheiligt durch ihn selbst,", "tokens": ["Aus", "al\u00b7ler", "Kampf", "in", "tr\u00fc\u00b7ber", "Er\u00b7den\u00b7nacht", ";", "Tr\u00e4\u00b7nen", ",", "tief", "emp\u00b7fun\u00b7de\u00b7ne", "Tr\u00e4\u00b7nen", ",", "ge\u00b7hei\u00b7ligt", "durch", "ihn", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ADJA", "NN", "$.", "NN", "$,", "ADJD", "ADJA", "NN", "$,", "VVPP", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Dem sie galten, weihte Deutschland seinem S\u00e4nger.", "tokens": ["Dem", "sie", "gal\u00b7ten", ",", "weih\u00b7te", "Deutschland", "sei\u00b7nem", "S\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du heilige Nacht um den Abgrund alles Seins,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Nacht", "um", "den", "Ab\u00b7grund", "al\u00b7les", "Seins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Durchleuchtet von Strahlen des Lichtmeers, h\u00f6chste Sch\u00f6nheit, Geheimnis der Welt,", "tokens": ["Durch\u00b7leuch\u00b7tet", "von", "Strah\u00b7len", "des", "Licht\u00b7meers", ",", "h\u00f6chs\u00b7te", "Sch\u00f6n\u00b7heit", ",", "Ge\u00b7heim\u00b7nis", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$,", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Seele, selbst sich R\u00e4tsel, denkt voll Sehnsucht viel sich Inniges \u00fcber ihr Los hienieden aus,", "tokens": ["Die", "See\u00b7le", ",", "selbst", "sich", "R\u00e4t\u00b7sel", ",", "denkt", "voll", "Sehn\u00b7sucht", "viel", "sich", "In\u00b7ni\u00b7ges", "\u00fc\u00b7ber", "ihr", "Los", "hien\u00b7ie\u00b7den", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "NN", "$,", "VVFIN", "ADJD", "NN", "PIAT", "PRF", "ADV", "APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+----+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Oft in schwermutvollen, stets anmut'gen Bildern.", "tokens": ["Oft", "in", "schwer\u00b7mut\u00b7vol\u00b7len", ",", "stets", "an\u00b7mut'\u00b7gen", "Bil\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVINF", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Dichtend sah sie viel voll Tiefsinns schon, was der erforschende Geist", "tokens": ["Dich\u00b7tend", "sah", "sie", "viel", "voll", "Tief\u00b7sinns", "schon", ",", "was", "der", "er\u00b7for\u00b7schen\u00b7de", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADJD", "NN", "ADV", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sp\u00e4t hernach erst fand, vollbringend den Sieg der Idee.", "tokens": ["Sp\u00e4t", "her\u00b7nach", "erst", "fand", ",", "voll\u00b7brin\u00b7gend", "den", "Sieg", "der", "I\u00b7dee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "VVFIN", "$,", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sie tr\u00e4gt dann, wie ihr eigenes Bild,", "tokens": ["Sie", "tr\u00e4gt", "dann", ",", "wie", "ihr", "ei\u00b7ge\u00b7nes", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie die gefl\u00fcgelte Sphinx an sich das Memento noch hat, so auch, und jetzt", "tokens": ["Wie", "die", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Sphinx", "an", "sich", "das", "Me\u00b7men\u00b7to", "noch", "hat", ",", "so", "auch", ",", "und", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PRF", "ART", "NE", "ADV", "VAFIN", "$,", "ADV", "ADV", "$,", "KON", "ADV"], "meter": "-+-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Hehr und befreit im Triumph, die Totenmaske hoch in die bl\u00fchende Welt,", "tokens": ["Hehr", "und", "be\u00b7freit", "im", "Tri\u00b7umph", ",", "die", "To\u00b7ten\u00b7mas\u00b7ke", "hoch", "in", "die", "bl\u00fc\u00b7hen\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$,", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+--+--+", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Als das Sinnbild ihrer Abkunft von den Sternen.", "tokens": ["Als", "das", "Sinn\u00b7bild", "ih\u00b7rer", "Ab\u00b7kunft", "von", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Wie wir entstanden, woher wir kommen, welches der Ersten des Menschengeschlechts", "tokens": ["Wie", "wir", "ent\u00b7stan\u00b7den", ",", "wo\u00b7her", "wir", "kom\u00b7men", ",", "wel\u00b7ches", "der", "Ers\u00b7ten", "des", "Men\u00b7schen\u00b7ge\u00b7schlechts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "$,", "PWAV", "PPER", "VVINF", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+-+--+--+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Urspr\u00fcngliche Heimat und ihr Schicksal war, traumhaft", "tokens": ["Ur\u00b7spr\u00fcng\u00b7li\u00b7che", "Hei\u00b7mat", "und", "ihr", "Schick\u00b7sal", "war", ",", "traum\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$,", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die Mythe nur sagt es und wirft dar\u00fcber blitzschnell spr\u00fchendes Streiflicht und weckt", "tokens": ["Die", "My\u00b7the", "nur", "sagt", "es", "und", "wirft", "da\u00b7r\u00fc\u00b7ber", "blitz\u00b7schnell", "spr\u00fc\u00b7hen\u00b7des", "Streif\u00b7licht", "und", "weckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PAV", "ADJD", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+--+--+-+-+-+--+--+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Ein Vorgef\u00fchl vollkommener Einsicht; Sinnbild wird ihr alles Erscheinende, jede Lebensspur", "tokens": ["Ein", "Vor\u00b7ge\u00b7f\u00fchl", "voll\u00b7kom\u00b7me\u00b7ner", "Ein\u00b7sicht", ";", "Sinn\u00b7bild", "wird", "ihr", "al\u00b7les", "Er\u00b7schei\u00b7nen\u00b7de", ",", "je\u00b7de", "Le\u00b7bens\u00b7spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "NN", "VAFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+--+-+-+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Wird der Ausdruck einer Weltalloffenbarung.", "tokens": ["Wird", "der", "Aus\u00b7druck", "ei\u00b7ner", "Welt\u00b7al\u00b7lof\u00b7fen\u00b7ba\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Welchen Gedanken flocht deutungsvoll die Gottheit, welchen Gedanken an sich", "tokens": ["Wel\u00b7chen", "Ge\u00b7dan\u00b7ken", "flocht", "deu\u00b7tungs\u00b7voll", "die", "Got\u00b7theit", ",", "wel\u00b7chen", "Ge\u00b7dan\u00b7ken", "an", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$,", "PWAT", "NN", "APPR", "PRF"], "meter": "+--+-++-+-+-+--+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "In ihrer Erkornen Sendung? Denn die sind", "tokens": ["In", "ih\u00b7rer", "Er\u00b7kor\u00b7nen", "Sen\u00b7dung", "?", "Denn", "die", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "KON", "ART", "NN"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Mit aller erhabnen Erinnerung eines Volks aufs tiefste verwoben und stets", "tokens": ["Mit", "al\u00b7ler", "er\u00b7hab\u00b7nen", "E\u00b7rin\u00b7ne\u00b7rung", "ei\u00b7nes", "Volks", "aufs", "tiefs\u00b7te", "ver\u00b7wo\u00b7ben", "und", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN", "APPRART", "ADJA", "VVINF", "KON", "ADV"], "meter": "-+--+--+--+-+-+--+--+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Der Gr\u00f6\u00dfe Mitvollbringer, weil ja ewig, Zeugschaft gebend von Geist- und Gem\u00fctskraft, unverf\u00e4lscht", "tokens": ["Der", "Gr\u00f6\u00b7\u00dfe", "Mit\u00b7voll\u00b7brin\u00b7ger", ",", "weil", "ja", "e\u00b7wig", ",", "Zeug\u00b7schaft", "ge\u00b7bend", "von", "Geist", "und", "Ge\u00b7m\u00fcts\u00b7kraft", ",", "un\u00b7ver\u00b7f\u00e4lscht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "ADV", "ADJD", "$,", "NN", "VVPP", "APPR", "TRUNC", "KON", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Als ein Erbgut stets der Dichtung Sprache fortlebt.", "tokens": ["Als", "ein", "Erb\u00b7gut", "stets", "der", "Dich\u00b7tung", "Spra\u00b7che", "fort\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.5": {"line.1": {"text": "K\u00fchn wie der Adler vor Deutschlands Heereszug zu der Kaiser gewaltiger Zeit,", "tokens": ["K\u00fchn", "wie", "der", "Ad\u00b7ler", "vor", "Deutschlands", "Hee\u00b7res\u00b7zug", "zu", "der", "Kai\u00b7ser", "ge\u00b7wal\u00b7ti\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NE", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+--+--+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "So weht wie vom hohen Gebirg dein Flug! Dich mit Stolz", "tokens": ["So", "weht", "wie", "vom", "ho\u00b7hen", "Ge\u00b7birg", "dein", "Flug", "!", "Dich", "mit", "Stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$.", "PPER", "APPR", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Voll innigen Dankes nennt sein das Vaterland, ja jedes Jahrhundert bekennt sich", "tokens": ["Voll", "in\u00b7ni\u00b7gen", "Dan\u00b7kes", "nennt", "sein", "das", "Va\u00b7ter\u00b7land", ",", "ja", "je\u00b7des", "Jahr\u00b7hun\u00b7dert", "be\u00b7kennt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPOSAT", "ART", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "PRF"], "meter": "--+-+--+-+-+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Zu deinem Ruhm, die Gegenwart strebt, tiefger\u00fchrt von deines Gesanges Wohllaut, wetteifernd dich", "tokens": ["Zu", "dei\u00b7nem", "Ruhm", ",", "die", "Ge\u00b7gen\u00b7wart", "strebt", ",", "tief\u00b7ge\u00b7r\u00fchrt", "von", "dei\u00b7nes", "Ge\u00b7san\u00b7ges", "Wohl\u00b7laut", ",", "wett\u00b7ei\u00b7fernd", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Auch im Erzbild ragend stets vor sich zu schauen,", "tokens": ["Auch", "im", "Erz\u00b7bild", "ra\u00b7gend", "stets", "vor", "sich", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Komm denn zu uns, wo, deines Genius Fl\u00fcgen verwandt,", "tokens": ["Komm", "denn", "zu", "uns", ",", "wo", ",", "dei\u00b7nes", "Ge\u00b7nius", "Fl\u00fc\u00b7gen", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "PWAV", "$,", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "S\u00e4ulenbau dich gr\u00fc\u00dft, wo der Antike geweiht, o Schiller, dich an die Welt", "tokens": ["S\u00e4u\u00b7len\u00b7bau", "dich", "gr\u00fc\u00dft", ",", "wo", "der", "An\u00b7ti\u00b7ke", "ge\u00b7weiht", ",", "o", "Schil\u00b7ler", ",", "dich", "an", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$,", "FM", "NE", "$,", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Edle Gestalten und Formen reiner Sch\u00f6nheit, wo mit heiliger Glut", "tokens": ["Ed\u00b7le", "Ge\u00b7stal\u00b7ten", "und", "For\u00b7men", "rei\u00b7ner", "Sch\u00f6n\u00b7heit", ",", "wo", "mit", "hei\u00b7li\u00b7ger", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "ADJA", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-+-+--+", "measure": "dactylic.di.plus"},