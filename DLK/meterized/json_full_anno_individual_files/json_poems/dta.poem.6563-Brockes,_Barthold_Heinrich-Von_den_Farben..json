{"dta.poem.6563": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Corpern siehet man, wenn sie die Gluht durchdringt,", "tokens": ["Auf", "Cor\u00b7pern", "sie\u00b7het", "man", ",", "wenn", "sie", "die", "Gluht", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df roth, schwartz, gelb, und wei\u00df auf selbigen entspringt,", "tokens": ["Da\u00df", "roth", ",", "schwartz", ",", "gelb", ",", "und", "wei\u00df", "auf", "sel\u00b7bi\u00b7gen", "ent\u00b7springt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "VVFIN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja alles, was wir nur von bunten C\u00f6rpern kennen,", "tokens": ["Ja", "al\u00b7les", ",", "was", "wir", "nur", "von", "bun\u00b7ten", "C\u00f6r\u00b7pern", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entstehet vom Geweb\u2019 und Z\u00e4serchen allein,", "tokens": ["Ent\u00b7ste\u00b7het", "vom", "Ge\u00b7web'", "und", "Z\u00e4\u00b7ser\u00b7chen", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So in dem C\u00f6rper sind, die wir gef\u00e4rbet nennen.", "tokens": ["So", "in", "dem", "C\u00f6r\u00b7per", "sind", ",", "die", "wir", "ge\u00b7f\u00e4r\u00b7bet", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Man sp\u00fcret, da\u00df sie mehr und minder dunckel seyn,", "tokens": ["Man", "sp\u00fc\u00b7ret", ",", "da\u00df", "sie", "mehr", "und", "min\u00b7der", "dun\u00b7ckel", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "KON", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nachdem ein jeglicher, in unterschiednem Grad,", "tokens": ["Nach\u00b7dem", "ein", "jeg\u00b7li\u00b7cher", ",", "in", "un\u00b7ter\u00b7schied\u00b7nem", "Grad", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mehr glatt-und rauch-mehr fest-und loses hat.", "tokens": ["Mehr", "glat\u00b7t\u00b7und", "rauch\u00b7mehr", "fest\u00b7\u00b7und", "lo\u00b7ses", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "FM", "FM", "FM", "VAFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}