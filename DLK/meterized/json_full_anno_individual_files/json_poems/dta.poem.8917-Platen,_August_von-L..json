{"dta.poem.8917": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die aber nie an dir der Himmel r\u00e4che,", "tokens": ["Die", "a\u00b7ber", "nie", "an", "dir", "der", "Him\u00b7mel", "r\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sonst m\u00fc\u00dften flie\u00dfen deine Thr\u00e4nenb\u00e4che,", "tokens": ["Sonst", "m\u00fc\u00df\u00b7ten", "flie\u00b7\u00dfen", "dei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7b\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn von der Lippe dir mein Name gleitet.", "tokens": ["Wenn", "von", "der", "Lip\u00b7pe", "dir", "mein", "Na\u00b7me", "glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Doch bis Gewi\u00dfheit jeden Wahn bestreitet,", "tokens": ["Doch", "bis", "Ge\u00b7wi\u00df\u00b7heit", "je\u00b7den", "Wahn", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will gern ich dich, und th\u00e4t' ich es aus Schw\u00e4che,", "tokens": ["Will", "gern", "ich", "dich", ",", "und", "th\u00e4t'", "ich", "es", "aus", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PRF", "$,", "KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vertheid'gen, Freund! von auf der Oberfl\u00e4che", "tokens": ["Vert\u00b7hei\u00b7d'\u00b7gen", ",", "Freund", "!", "von", "auf", "der", "O\u00b7berf\u00b7l\u00e4\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "APPR", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gesch\u00f6pften Zufallsgr\u00fcnden nie verleitet.", "tokens": ["Ge\u00b7sch\u00f6pf\u00b7ten", "Zu\u00b7falls\u00b7gr\u00fcn\u00b7den", "nie", "ver\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar w\u00fcrd' ich kaum dir zum Vertheid'ger taugen,", "tokens": ["Zwar", "w\u00fcrd'", "ich", "kaum", "dir", "zum", "Vert\u00b7hei\u00b7d'\u00b7ger", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch stets bedienst du dich als deiner beyden", "tokens": ["Doch", "stets", "be\u00b7dienst", "du", "dich", "als", "dei\u00b7ner", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "KOUS", "PPOSAT", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcrsprecher listig meiner beyden Augen:", "tokens": ["F\u00fcr\u00b7spre\u00b7cher", "lis\u00b7tig", "mei\u00b7ner", "bey\u00b7den", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So lang sie sich an deinem Blicke weiden,", "tokens": ["So", "lang", "sie", "sich", "an", "dei\u00b7nem", "Bli\u00b7cke", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00fcssen Liebe sie aus ihm sich saugen,", "tokens": ["So", "m\u00fcs\u00b7sen", "Lie\u00b7be", "sie", "aus", "ihm", "sich", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du aber lies in ihrem Blick mein Leiden!", "tokens": ["Du", "a\u00b7ber", "lies", "in", "ih\u00b7rem", "Blick", "mein", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}