{"dta.poem.21065": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Weihgesang Zur Er\u00f6ffnung des neuen anatomischen Theaters in  \n Rostock. .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwache zu erhabner Geistesweide,               ", "tokens": ["Er\u00b7wa\u00b7che", "zu", "er\u00b7hab\u00b7ner", "Geis\u00b7tes\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erwache, gottgeliebtes Warnathen!", "tokens": ["Er\u00b7wa\u00b7che", ",", "gott\u00b7ge\u00b7lieb\u00b7tes", "War\u00b7na\u00b7then", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eil', in deinem sch\u00f6nsten Feyerkleide,", "tokens": ["Und", "eil'", ",", "in", "dei\u00b7nem", "sch\u00f6ns\u00b7ten", "Fe\u00b7yer\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das neue Heil, das dir ergl\u00e4nzt, zu sehn.", "tokens": ["Das", "neu\u00b7e", "Heil", ",", "das", "dir", "er\u00b7gl\u00e4nzt", ",", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es lodern schon des Fr\u00fchroths Purpurgluthen;", "tokens": ["Es", "lo\u00b7dern", "schon", "des", "Fr\u00fch\u00b7roths", "Pur\u00b7pur\u00b7glu\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es wallt schon aus des Osten Flammenmeer", "tokens": ["Es", "wallt", "schon", "aus", "des", "Os\u00b7ten", "Flam\u00b7men\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der sch\u00f6ngelockte Tag, den Weisen und den Guten", "tokens": ["Der", "sch\u00f6n\u00b7ge\u00b7lock\u00b7te", "Tag", ",", "den", "Wei\u00b7sen", "und", "den", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein vielgew\u00fcnschter Tag, daher.", "tokens": ["Ein", "viel\u00b7ge\u00b7w\u00fcnschter", "Tag", ",", "da\u00b7her", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Zu unsers Musageten Preis und Ehre,", "tokens": ["Zu", "un\u00b7sers", "Mu\u00b7sa\u00b7ge\u00b7ten", "Preis", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir, s\u00fcsses Vaterland, zu Lob' und Ruhm,", "tokens": ["Dir", ",", "s\u00fcs\u00b7ses", "Va\u00b7ter\u00b7land", ",", "zu", "Lob'", "und", "Ruhm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Euch, S\u00f6hnen Aeskulaps, zu Lust und Lehre,", "tokens": ["Euch", ",", "S\u00f6h\u00b7nen", "A\u00b7es\u00b7ku\u00b7laps", ",", "zu", "Lust", "und", "Leh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NE", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Entriegelt heut ein neues Heiligthum", "tokens": ["Ent\u00b7rie\u00b7gelt", "heut", "ein", "neu\u00b7es", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich unserm Blick \u2014 Wem flammt die Altarlohe?", "tokens": ["Sich", "un\u00b7serm", "Blick", "Wem", "flammt", "die", "Al\u00b7tar\u00b7lo\u00b7he", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wem wallt die lichte Weihrauchwolke hin?", "tokens": ["Wem", "wallt", "die", "lich\u00b7te", "Weih\u00b7rauch\u00b7wol\u00b7ke", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dir, Anatome, dir, du Hehre, Holde, Hohe,", "tokens": ["Dir", ",", "A\u00b7na\u00b7to\u00b7me", ",", "dir", ",", "du", "Heh\u00b7re", ",", "Hol\u00b7de", ",", "Ho\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "PPER", "$,", "PPER", "NN", "$,", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des ewgen Bildners Heroldin!", "tokens": ["Des", "ew\u00b7gen", "Bild\u00b7ners", "He\u00b7rol\u00b7din", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Entschwebet euren stillen Regionen,", "tokens": ["Ent\u00b7schwe\u00b7bet", "eu\u00b7ren", "stil\u00b7len", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Genien der grauen Wissenschaft,", "tokens": ["Ihr", "Ge\u00b7ni\u00b7en", "der", "grau\u00b7en", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die ihr in ", "tokens": ["Die", "ihr", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "An des ", "tokens": ["An", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Entwallt, Heroen, euren Lorberschatten,", "tokens": ["Ent\u00b7wallt", ",", "He\u00b7roen", ",", "eu\u00b7ren", "Lor\u00b7ber\u00b7schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und helfet unser Heiligthum uns weihn!", "tokens": ["Und", "hel\u00b7fet", "un\u00b7ser", "Hei\u00b7lig\u00b7thum", "uns", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verlasse, Greis von ", "tokens": ["Ver\u00b7las\u00b7se", ",", "Greis", "von"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und stimm' in unsre Feyer ein!", "tokens": ["Und", "stimm'", "in", "uns\u00b7re", "Fe\u00b7yer", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die ihr aus tausendj\u00e4hr'ger Nacht erstandet,", "tokens": ["Die", "ihr", "aus", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ger", "Nacht", "er\u00b7stan\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fchn im Zwielicht eines sch\u00f6nern Tags", "tokens": ["Und", "k\u00fchn", "im", "Zwie\u00b7licht", "ei\u00b7nes", "sch\u00f6\u00b7nern", "Tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die hohe Kunst der Barbarey entwandet,", "tokens": ["Die", "ho\u00b7he", "Kunst", "der", "Bar\u00b7ba\u00b7rey", "ent\u00b7wan\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Schatten ", "tokens": ["Ihr", "Schat\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "stach's,               ", "tokens": ["stach's", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Linn&#228;us, Meteor aus Uplands &#214;den,               ", "tokens": ["Linn", "&#228;", "us", ",", "Me\u00b7te\u00b7or", "aus", "Up\u00b7lands", "&#214;", "den", ","], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "$(", "NE", "$,", "NN", "APPR", "NE", "$(", "ART", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Verkl\u00e4rter ", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Entschwebt, ihr Herrlichen, entschwebet eurem", "tokens": ["Ent\u00b7schwebt", ",", "ihr", "Herr\u00b7li\u00b7chen", ",", "ent\u00b7schwe\u00b7bet", "eu\u00b7rem"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Eden,", "tokens": ["E\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Und weihet unser Heiligthum.", "tokens": ["Und", "wei\u00b7het", "un\u00b7ser", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Heran, wem Liebe zu der ernsten Muse", "tokens": ["He\u00b7ran", ",", "wem", "Lie\u00b7be", "zu", "der", "erns\u00b7ten", "Mu\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "PWS", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Brust mit Sehnsucht und Verlangen schwellt,", "tokens": ["Die", "Brust", "mit", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heran, und sch\u00f6pft aus unsrer Arethuse", "tokens": ["He\u00b7ran", ",", "und", "sch\u00f6pft", "aus", "uns\u00b7rer", "A\u00b7ret\u00b7hu\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die jedem Lechzer volle Labung quellt!", "tokens": ["Die", "je\u00b7dem", "Lech\u00b7zer", "vol\u00b7le", "La\u00b7bung", "quellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heran, wem theuer seines Menschenseyns Kunde,", "tokens": ["He\u00b7ran", ",", "wem", "theu\u00b7er", "sei\u00b7nes", "Men\u00b7schen\u00b7seyns", "Kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWS", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wem wichtig inhaltreiche Wissenschaft,", "tokens": ["Wem", "wich\u00b7tig", "in\u00b7hal\u00b7trei\u00b7che", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Heran, und schauet hier im festverschlungnen Bunde", "tokens": ["He\u00b7ran", ",", "und", "schau\u00b7et", "hier", "im", "fest\u00b7ver\u00b7schlung\u00b7nen", "Bun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Raumlose Weisheit, Huld und Kraft.", "tokens": ["Raum\u00b7lo\u00b7se", "Weis\u00b7heit", ",", "Huld", "und", "Kraft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Seht hier die morsche H\u00fcls' erhabner Seelen", "tokens": ["Seht", "hier", "die", "mor\u00b7sche", "H\u00fcls'", "er\u00b7hab\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Stoffen tausendfacher Art gewebt,", "tokens": ["Aus", "Stof\u00b7fen", "tau\u00b7send\u00b7fa\u00b7cher", "Art", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Salzen, S\u00e4uren, Kalken, Erden, \u00d6len,", "tokens": ["Aus", "Sal\u00b7zen", ",", "S\u00e4u\u00b7ren", ",", "Kal\u00b7ken", ",", "Er\u00b7den", ",", "\u00d6\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Licht und Luft und Feuerstoff belebt!", "tokens": ["Durch", "Licht", "und", "Luft", "und", "Feu\u00b7er\u00b7stoff", "be\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seht hier das labyrinthische Ge\u00e4der,", "tokens": ["Seht", "hier", "das", "la\u00b7by\u00b7rin\u00b7thi\u00b7sche", "Ge\u00b7\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dadurch des Blutes Kugelwoge rollt!", "tokens": ["Da\u00b7durch", "des", "Blu\u00b7tes", "Ku\u00b7gel\u00b7wo\u00b7ge", "rollt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Seht hier des Daseyns Born, das grosse Rad der", "tokens": ["Seht", "hier", "des", "Da\u00b7seyns", "Born", ",", "das", "gros\u00b7se", "Rad", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "R\u00e4der,", "tokens": ["R\u00e4\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Dem jede Ader zinst und zollt!", "tokens": ["Dem", "je\u00b7de", "A\u00b7der", "zinst", "und", "zollt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bist du es, kleiner Muskel unsers Lebens,", "tokens": ["Bist", "du", "es", ",", "klei\u00b7ner", "Mus\u00b7kel", "un\u00b7sers", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rastloses Triebwerk, Unruh unsrer Brust?", "tokens": ["Rast\u00b7lo\u00b7ses", "Trieb\u00b7werk", ",", "Un\u00b7ruh", "uns\u00b7rer", "Brust", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punc