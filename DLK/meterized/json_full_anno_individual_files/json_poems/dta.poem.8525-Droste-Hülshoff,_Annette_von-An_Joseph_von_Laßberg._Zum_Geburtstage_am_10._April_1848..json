{"dta.poem.8525": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An Joseph von La\u00dfberg.  \n Zum Geburtstage am 10. April 1848.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grad heute, wo ich gar zu gern", "tokens": ["Grad", "heu\u00b7te", ",", "wo", "ich", "gar", "zu", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PWAV", "PPER", "ADV", "PTKA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir h\u00e4tt\u2019 ein herzlich Wort gesagt,", "tokens": ["Dir", "h\u00e4tt'", "ein", "herz\u00b7lich", "Wort", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Grad heute hat mein b\u00f6ser Stern", "tokens": ["Grad", "heu\u00b7te", "hat", "mein", "b\u00f6\u00b7ser", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit argem Husten mich geplagt;", "tokens": ["Mit", "ar\u00b7gem", "Hus\u00b7ten", "mich", "ge\u00b7plagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch w\u00e4r\u2019 ich wohl hinaufgeklommen,", "tokens": ["Doch", "w\u00e4r'", "ich", "wohl", "hin\u00b7auf\u00b7ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4r\u2019 nicht mein Schwesterlein gekommen,", "tokens": ["W\u00e4r'", "nicht", "mein", "Schwes\u00b7ter\u00b7lein", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00e4tt\u2019 es ernst mir untersagt.", "tokens": ["Und", "h\u00e4tt'", "es", "ernst", "mir", "un\u00b7ter\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was send\u2019 ich meinem Gru\u00dfe nach?", "tokens": ["Was", "send'", "ich", "mei\u00b7nem", "Gru\u00b7\u00dfe", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein buntes Gl\u00f6ckchen, arm und klein;", "tokens": ["Ein", "bun\u00b7tes", "Gl\u00f6ck\u00b7chen", ",", "arm", "und", "klein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ist sein Stimmchen zart und schwach,", "tokens": ["Wohl", "ist", "sein", "Stimm\u00b7chen", "zart", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ist es silberhell und rein;", "tokens": ["Doch", "ist", "es", "sil\u00b7ber\u00b7hell", "und", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo du l\u00e4\u00dft es klingelnd rauschen,", "tokens": ["Und", "wo", "du", "l\u00e4\u00dft", "es", "klin\u00b7gelnd", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da wird das Ohr der Liebe lauschen,", "tokens": ["Da", "wird", "das", "Ohr", "der", "Lie\u00b7be", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, glaub\u2019 es mir, das h\u00f6rt gar fein!", "tokens": ["Und", ",", "glaub'", "es", "mir", ",", "das", "h\u00f6rt", "gar", "fein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PPER", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}