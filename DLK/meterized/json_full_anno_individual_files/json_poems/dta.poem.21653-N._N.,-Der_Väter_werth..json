{"dta.poem.21653": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Der V\u00e4ter werth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Himmel rauscht gewaltig", "tokens": ["Vom", "Him\u00b7mel", "rauscht", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein wundersames Lied,", "tokens": ["Ein", "wun\u00b7der\u00b7sa\u00b7mes", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ahnen vielgestaltig", "tokens": ["Ein", "Ah\u00b7nen", "viel\u00b7ge\u00b7stal\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch meine Seele zieht.", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Adlerschwingen klingt es,", "tokens": ["Wie", "Ad\u00b7ler\u00b7schwin\u00b7gen", "klingt", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie sturmdurchbrauster Wald,", "tokens": ["Wie", "sturm\u00b7durch\u00b7braus\u00b7ter", "Wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie Jubelhymnen singt es:", "tokens": ["Wie", "Ju\u00b7bel\u00b7hym\u00b7nen", "singt", "es", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was Du ersehnt, kommt bald!", "tokens": ["Was", "Du", "er\u00b7sehnt", ",", "kommt", "bald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fest steht der Bau gegr\u00fcndet,", "tokens": ["Fest", "steht", "der", "Bau", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das heil\u2019ge deutsche Reich,", "tokens": ["Das", "heil'\u00b7ge", "deut\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zinnen gluthentz\u00fcndet", "tokens": ["Die", "Zin\u00b7nen", "glu\u00b7thent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erstrahlen sonnengleich.", "tokens": ["Er\u00b7strah\u00b7len", "son\u00b7nen\u00b7gleich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die stolzen Banner wogen", "tokens": ["Die", "stol\u00b7zen", "Ban\u00b7ner", "wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In alle Lande weit,", "tokens": ["In", "al\u00b7le", "Lan\u00b7de", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und droben gl\u00e4nzt der Bogen", "tokens": ["Und", "dro\u00b7ben", "gl\u00e4nzt", "der", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erhab\u2019ner Einigkeit!", "tokens": ["Er\u00b7hab'\u00b7ner", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun schwillt die Seele bebend", "tokens": ["Nun", "schwillt", "die", "See\u00b7le", "be\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Drang nach deutscher Art,", "tokens": ["Im", "Drang", "nach", "deut\u00b7scher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun jubelt auf, was strebend", "tokens": ["Nun", "ju\u00b7belt", "auf", ",", "was", "stre\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich treu zusammen schaart:", "tokens": ["Sich", "treu", "zu\u00b7sam\u00b7men", "schaart", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Reden und im Handeln", "tokens": ["Im", "Re\u00b7den", "und", "im", "Han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Hohn dem L\u00fcgendunst", "tokens": ["Zum", "Hohn", "dem", "L\u00fc\u00b7gen\u00b7dunst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Deutsch allezeit zu wandeln", "tokens": ["Deutsch", "al\u00b7le\u00b7zeit", "zu", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Leben und in Kunst!", "tokens": ["In", "Le\u00b7ben", "und", "in", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hell schmettern die Fanfaren", "tokens": ["Hell", "schmet\u00b7tern", "die", "Fan\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Thal und Bergrevier:", "tokens": ["Durch", "Thal", "und", "Berg\u00b7re\u00b7vier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer will die Treue wahren", "tokens": ["Wer", "will", "die", "Treu\u00b7e", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem deutschen Reichspanier?", "tokens": ["Dem", "deut\u00b7schen", "Reich\u00b7spa\u00b7nier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir heben hoch die H\u00e4nde", "tokens": ["Wir", "he\u00b7ben", "hoch", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und kreuzen Schwert mit Schwert:", "tokens": ["Und", "kreu\u00b7zen", "Schwert", "mit", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun hat die Schmach ein Ende,", "tokens": ["Nun", "hat", "die", "Schmach", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir sind der V\u00e4ter werth!", "tokens": ["Wir", "sind", "der", "V\u00e4\u00b7ter", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}