{"dta.poem.21660": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Schwankend ragen \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Schwankend ragen finst\u2019re B\u00e4ume,", "tokens": ["Schwan\u00b7kend", "ra\u00b7gen", "finst'\u00b7re", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die L\u00fcfte zieht es schaurig,", "tokens": ["Durch", "die", "L\u00fcf\u00b7te", "zieht", "es", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte, tiefe, dunk\u2019le Tr\u00e4ume", "tokens": ["Al\u00b7te", ",", "tie\u00b7fe", ",", "dunk'\u00b7le", "Tr\u00e4u\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittern durch die Seele traurig.", "tokens": ["Zit\u00b7tern", "durch", "die", "See\u00b7le", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welke Bl\u00e4tter rascheln st\u00f6hnend,", "tokens": ["Wel\u00b7ke", "Bl\u00e4t\u00b7ter", "ra\u00b7scheln", "st\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was am Mark verdorrt, mu\u00df brechen,", "tokens": ["Was", "am", "Mark", "ver\u00b7dorrt", ",", "mu\u00df", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVPP", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steinern Schicksal schl\u00e4gt uns h\u00f6hnend,", "tokens": ["Stei\u00b7nern", "Schick\u00b7sal", "schl\u00e4gt", "uns", "h\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was verschuldet, mu\u00df sich r\u00e4chen.", "tokens": ["Was", "ver\u00b7schul\u00b7det", ",", "mu\u00df", "sich", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, die n\u00e4chtig schweren Sorgen", "tokens": ["Ach", ",", "die", "n\u00e4ch\u00b7tig", "schwe\u00b7ren", "Sor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohnen wachend im Gem\u00fcthe,", "tokens": ["Woh\u00b7nen", "wa\u00b7chend", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die S\u00fcnde liegt verborgen", "tokens": ["Und", "die", "S\u00fcn\u00b7de", "liegt", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Keim der sch\u00f6nsten Bl\u00fcthe.", "tokens": ["In", "dem", "Keim", "der", "sch\u00f6ns\u00b7ten", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}