{"dta.poem.7264": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Vogelh\u00fctte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Regen, Regen, immer Regen! will nicht das Gepl\u00e4tscher", "tokens": ["Re\u00b7gen", ",", "Re\u00b7gen", ",", "im\u00b7mer", "Re\u00b7gen", "!", "will", "nicht", "das", "Ge\u00b7pl\u00e4t\u00b7scher"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADV", "NN", "$.", "VMFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "enden,", "tokens": ["en\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df ich aus dem Sarge brechen kann, aus diesen Bretter-", "tokens": ["Da\u00df", "ich", "aus", "dem", "Sar\u00b7ge", "bre\u00b7chen", "kann", ",", "aus", "die\u00b7sen", "Bret\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,", "APPR", "PDAT", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "w\u00e4nden?", "tokens": ["w\u00e4n\u00b7den", "?"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Sieben Schuhe ins Gevierte, das ist doch ein \u00e4rmlich", "tokens": ["Sie\u00b7ben", "Schu\u00b7he", "ins", "Ge\u00b7vier\u00b7te", ",", "das", "ist", "doch", "ein", "\u00e4rm\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPRART", "NN", "$,", "PDS", "VAFIN", "ADV", "ART", "ADJD"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "R\u00e4umchen", "tokens": ["R\u00e4um\u00b7chen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "F\u00fcr ein Menschenkind, und w\u00e4r' es schlank auch wie ein", "tokens": ["F\u00fcr", "ein", "Men\u00b7schen\u00b7kind", ",", "und", "w\u00e4r'", "es", "schlank", "auch", "wie", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "VVFIN", "ADV", "KOKOM", "ART"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Rosenb\u00e4umchen!", "tokens": ["Ro\u00b7sen\u00b7b\u00e4um\u00b7chen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "O was lie\u00df ich mich gel\u00fcsten, in den Vogelheerd zu fl\u00fcchten,", "tokens": ["O", "was", "lie\u00df", "ich", "mich", "ge\u00b7l\u00fcs\u00b7ten", ",", "in", "den", "Vo\u00b7gel\u00b7heerd", "zu", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PPER", "PRF", "VVPP", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als nur schwach die Wolke tropfte, als noch fl\u00fcsterten die", "tokens": ["Als", "nur", "schwach", "die", "Wol\u00b7ke", "tropf\u00b7te", ",", "als", "noch", "fl\u00fcs\u00b7ter\u00b7ten", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Fichten:", "tokens": ["Fich\u00b7ten", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}