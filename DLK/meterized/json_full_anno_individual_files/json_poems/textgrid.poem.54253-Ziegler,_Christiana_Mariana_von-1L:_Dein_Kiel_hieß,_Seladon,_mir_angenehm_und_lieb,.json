ord", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ins k\u00fcnfftige nicht mehr in dein Concept mit sehen.", "tokens": ["Ins", "k\u00fcnff\u00b7ti\u00b7ge", "nicht", "mehr", "in", "dein", "Con\u00b7cept", "mit", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was heimlich bleiben soll, vertraut man Knaben nicht,", "tokens": ["Was", "heim\u00b7lich", "blei\u00b7ben", "soll", ",", "ver\u00b7traut", "man", "Kna\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$,", "ADJD", "PIS", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch deren Plaudern offt Verr\u00e4therey geschicht.", "tokens": ["Durch", "de\u00b7ren", "Plau\u00b7dern", "offt", "Ver\u00b7r\u00e4\u00b7the\u00b7rey", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du kanst ja, wie du wilst, die nette Feder r\u00fchren,", "tokens": ["Du", "kanst", "ja", ",", "wie", "du", "wilst", ",", "die", "net\u00b7te", "Fe\u00b7der", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was l\u00e4st du dir ein Kind die Hand im Schreiben f\u00fchren?", "tokens": ["Was", "l\u00e4st", "du", "dir", "ein", "Kind", "die", "Hand", "im", "Schrei\u00b7ben", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Liebe hat mit mir wahrhafftig nichts zu thun,", "tokens": ["Die", "Lie\u00b7be", "hat", "mit", "mir", "wahr\u00b7haff\u00b7tig", "nichts", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADJD", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Drum la\u00df den Amor doch mit seiner Mutter ruhn.", "tokens": ["Drum", "la\u00df", "den", "A\u00b7mor", "doch", "mit", "sei\u00b7ner", "Mut\u00b7ter", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er hat schon so genung mit seinen Liebes-Waffen,", "tokens": ["Er", "hat", "schon", "so", "ge\u00b7nung", "mit", "sei\u00b7nen", "Lie\u00b7bes\u00b7Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Welt ist gar zu gro\u00df, so hier als dar zu schaffen.", "tokens": ["Die", "Welt", "ist", "gar", "zu", "gro\u00df", ",", "so", "hier", "als", "dar", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "ADV", "ADV", "KOUS", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach traue nicht dem Schalck, der sich zwar ehrbar stellt,", "tokens": ["Ach", "trau\u00b7e", "nicht", "dem", "Schalck", ",", "der", "sich", "zwar", "ehr\u00b7bar", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch bey der Glei\u00dfnerey den Trug zur\u00fccke h\u00e4lt.", "tokens": ["Doch", "bey", "der", "Glei\u00df\u00b7ne\u00b7rey", "den", "Trug", "zu\u00b7r\u00fc\u00b7cke", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es ist mit selbigen wahrhafftig nicht zu schertzen,", "tokens": ["Es", "ist", "mit", "sel\u00b7bi\u00b7gen", "wahr\u00b7haff\u00b7tig", "nicht", "zu", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "ADJD", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er raubt, so arg er kan, und stiehlt der Menschen Hertzen.", "tokens": ["Er", "raubt", ",", "so", "arg", "er", "kan", ",", "und", "stiehlt", "der", "Men\u00b7schen", "Hert\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du siehest, Geladon, li\u00df es nur mit Bedacht,", "tokens": ["Du", "sie\u00b7hest", ",", "Ge\u00b7la\u00b7don", ",", "li\u00df", "es", "nur", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wie sehr Asterie vor deine Ruhe wacht.", "tokens": ["Wie", "sehr", "As\u00b7te\u00b7rie", "vor", "dei\u00b7ne", "Ru\u00b7he", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.23": {"text": "Wie leichte k\u00f6nt er dir, bey so gestallten Sachen,", "tokens": ["Wie", "leich\u00b7te", "k\u00f6nt", "er", "dir", ",", "bey", "so", "ge\u00b7stall\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Dein Hertz gantz unvermerckt zum Mammelucken machen;", "tokens": ["Dein", "Hertz", "gantz", "un\u00b7ver\u00b7merckt", "zum", "Mam\u00b7me\u00b7lu\u00b7cken", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und st\u00e4hl er selbges dir, so d\u00fcrffte wohl die Welt,", "tokens": ["Und", "st\u00e4hl", "er", "selb\u00b7ges", "dir", ",", "so", "d\u00fcrff\u00b7te", "wohl", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "PPER", "$,", "ADV", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die dich, du bist es auch, vor gantz vollkommen h\u00e4lt,", "tokens": ["Die", "dich", ",", "du", "bist", "es", "auch", ",", "vor", "gantz", "voll\u00b7kom\u00b7men", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,", "APPR", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Beliebter Seladon, so dann verst\u00fcmmelt heissen,", "tokens": ["Be\u00b7lieb\u00b7ter", "Se\u00b7la\u00b7don", ",", "so", "dann", "ver\u00b7st\u00fcm\u00b7melt", "heis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du k\u00f6ntst dein Hertz nicht mehr, das sch\u00f6nste St\u00fcckgen, weissen.", "tokens": ["Du", "k\u00f6ntst", "dein", "Hertz", "nicht", "mehr", ",", "das", "sch\u00f6ns\u00b7te", "St\u00fcck\u00b7gen", ",", "weis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}