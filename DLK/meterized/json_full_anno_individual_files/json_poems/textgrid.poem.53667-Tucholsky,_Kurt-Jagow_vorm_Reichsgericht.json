{"textgrid.poem.53667": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Jagow vorm Reichsgericht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hier in Deutschland etwas Protektion hat,", "tokens": ["Wer", "hier", "in", "Deutschland", "et\u00b7was", "Pro\u00b7tek\u00b7ti\u00b7on", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "dem wird kein Str\u00e4flings-Kahlkopf je rasiert.", "tokens": ["dem", "wird", "kein", "Str\u00e4f\u00b7lings\u00b7Kahl\u00b7kopf", "je", "ra\u00b7siert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pa\u00df auf: man l\u00e4\u00dft ihm einundzwanzig Monat,", "tokens": ["Pa\u00df", "auf", ":", "man", "l\u00e4\u00dft", "ihm", "ein\u00b7und\u00b7zwan\u00b7zig", "Mo\u00b7nat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "damit er den Justizschwank gut probiert.", "tokens": ["da\u00b7mit", "er", "den", "Jus\u00b7tiz\u00b7schwank", "gut", "pro\u00b7biert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Kommunisten Posten, Wassertr\u00e4ger", "tokens": ["Der", "Kom\u00b7mu\u00b7nis\u00b7ten", "Pos\u00b7ten", ",", "Was\u00b7ser\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sperrt man am liebsten vorm Delikte ein;", "tokens": ["sperrt", "man", "am", "liebs\u00b7ten", "vorm", "De\u00b7lik\u00b7te", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKA", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "die Toller-Leute leben wie die Neger . . .", "tokens": ["die", "Tol\u00b7ler\u00b7Leu\u00b7te", "le\u00b7ben", "wie", "die", "Ne\u00b7ger", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich bin ein Preu\u00dfe, will ein Preu\u00dfe sein \u2013!", "tokens": ["Ich", "bin", "ein", "Preu\u00b7\u00dfe", ",", "will", "ein", "Preu\u00b7\u00dfe", "sein", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VMFIN", "ART", "NN", "VAINF", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das kl\u00f6nt von Bolschewismus, der nicht da war,", "tokens": ["Das", "kl\u00f6nt", "von", "Bol\u00b7sche\u00b7wis\u00b7mus", ",", "der", "nicht", "da", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "das hat auf Ebert den Patrizierzorn;", "tokens": ["das", "hat", "auf", "E\u00b7bert", "den", "Pat\u00b7ri\u00b7zier\u00b7zorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "wenn da nur Drahtverhau und viel Hurra war:", "tokens": ["wenn", "da", "nur", "Draht\u00b7ver\u00b7hau", "und", "viel", "Hur\u00b7ra", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sie in B\u00fcros \u2013 und die Soldaten vorn.", "tokens": ["sie", "in", "B\u00fc\u00b7ros", "\u2013", "und", "die", "Sol\u00b7da\u00b7ten", "vorn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "$(", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch Ludendorff \u2013 bei welchem Reinfall darf der fehlen! \u2013", "tokens": ["Auch", "Lu\u00b7den\u00b7dorff", "\u2013", "bei", "wel\u00b7chem", "Re\u00b7in\u00b7fall", "darf", "der", "feh\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NE", "$(", "APPR", "PWAT", "NN", "VMFIN", "ART", "VVINF", "$.", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "erg\u00e4nzt den M\u00e4nnerbund der frumben Seelen;", "tokens": ["er\u00b7g\u00e4nzt", "den", "M\u00e4n\u00b7ner\u00b7bund", "der", "frum\u00b7ben", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und heute kannst du im Berichte lesen:", "tokens": ["und", "heu\u00b7te", "kannst", "du", "im", "Be\u00b7rich\u00b7te", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie Lausejungen \u2013 keiner ists gewesen.", "tokens": ["Wie", "Lau\u00b7se\u00b7jun\u00b7gen", "\u2013", "kei\u00b7ner", "ists", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$(", "PIS", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht mal zum L\u00fcgen hat das viel Talent!", "tokens": ["Nicht", "mal", "zum", "L\u00fc\u00b7gen", "hat", "das", "viel", "Ta\u00b7lent", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sowas war Regierungspr\u00e4sident!", "tokens": ["Und", "so\u00b7was", "war", "Re\u00b7gie\u00b7rungs\u00b7pr\u00e4\u00b7si\u00b7dent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was da verhandelt wird, ist ganz belanglos.", "tokens": ["Was", "da", "ver\u00b7han\u00b7delt", "wird", ",", "ist", "ganz", "be\u00b7lang\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sie hatten so viel Zeit \u2013 die armen drei!", "tokens": ["Sie", "hat\u00b7ten", "so", "viel", "Zeit", "\u2013", "die", "ar\u00b7men", "drei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$(", "ART", "ADJA", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie konnten eindreiviertel Jahre zwanglos", "tokens": ["Sie", "konn\u00b7ten", "ein\u00b7drei\u00b7vier\u00b7tel", "Jah\u00b7re", "zwang\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "vertuschen \u2013 denn sie waren frei.", "tokens": ["ver\u00b7tu\u00b7schen", "\u2013", "denn", "sie", "wa\u00b7ren", "frei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn von der kleinsten Nutte sie was munkeln,", "tokens": ["Wenn", "von", "der", "kleins\u00b7ten", "Nut\u00b7te", "sie", "was", "mun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dann sperrt man Nutte, Louis, Kundschaft ein:", "tokens": ["dann", "sperrt", "man", "Nut\u00b7te", ",", "Lou\u00b7is", ",", "Kund\u00b7schaft", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "$,", "NE", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "es k\u00f6nnte sonst den Tatbestand verdunkeln \u2013", "tokens": ["es", "k\u00f6nn\u00b7te", "sonst", "den", "Tat\u00b7be\u00b7stand", "ver\u00b7dun\u00b7keln", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "das mu\u00df beim Kapp-Putsch wohl nicht n\u00f6tig sein.", "tokens": ["das", "mu\u00df", "beim", "Kapp\u00b7Putsch", "wohl", "nicht", "n\u00f6\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und keiner traut sich, Ludendorff zu fassen,", "tokens": ["Und", "kei\u00b7ner", "traut", "sich", ",", "Lu\u00b7den\u00b7dorff", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "$,", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und L\u00fcttwitz nicht und Kapp und Traub \u2013 die Massen,", "tokens": ["und", "L\u00fctt\u00b7witz", "nicht", "und", "Kapp", "und", "Traub", "\u2013", "die", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "PTKNEG", "KON", "NN", "KON", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sie bleiben frei.", "tokens": ["sie", "blei\u00b7ben", "frei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nachtw\u00e4chterstaat \u2013!", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter\u00b7staat", "\u2013", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du s\u00e4st dir eine schwarz-wei\u00df-rote Saat!", "tokens": ["Du", "s\u00e4st", "dir", "ei\u00b7ne", "schwa\u00b7rz\u00b7wei\u00df\u00b7ro\u00b7te", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und die geht auf.", "tokens": ["Und", "die", "geht", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sie h\u00f6hnen dich mit Recht.", "tokens": ["Sie", "h\u00f6h\u00b7nen", "dich", "mit", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du bist durchaus von zwittrigem Geschlecht!", "tokens": ["Du", "bist", "durc\u00b7haus", "von", "zwitt\u00b7ri\u00b7gem", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Levin\u00e9 ist tot. Und Jagow meckert.", "tokens": ["Der", "Le\u00b7vin\u00e9", "ist", "tot", ".", "Und", "Ja\u00b7gow", "me\u00b7ckert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "NE", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Du hast dich stets nur links mit Blut bekleckert \u2013", "tokens": ["Du", "hast", "dich", "stets", "nur", "links", "mit", "Blut", "be\u00b7kle\u00b7ckert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "die wirklichen Verbrecher henkst du nicht . . .", "tokens": ["die", "wirk\u00b7li\u00b7chen", "Ver\u00b7bre\u00b7cher", "henkst", "du", "nicht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Was Leipzig! Kinderspiel und Reichsgericht!", "tokens": ["Was", "Leip\u00b7zig", "!", "Kin\u00b7der\u00b7spiel", "und", "Reichs\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wir haben feste Monarchistenreihn \u2013", "tokens": ["Wir", "ha\u00b7ben", "fes\u00b7te", "Mon\u00b7ar\u00b7chis\u00b7ten\u00b7reihn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und dann im Reichstag den Gesangverein.", "tokens": ["und", "dann", "im", "Reichs\u00b7tag", "den", "Ge\u00b7sang\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und so viel kluge M\u00e4nner in der Politik.", "tokens": ["Und", "so", "viel", "klu\u00b7ge", "M\u00e4n\u00b7ner", "in", "der", "Po\u00b7li\u00b7tik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Zuf\u00e4llig keinen Rex.", "tokens": ["Zu\u00b7f\u00e4l\u00b7lig", "kei\u00b7nen", "Rex", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und keine Republik.", "tokens": ["Und", "kei\u00b7ne", "Re\u00b7pub\u00b7lik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer hier in Deutschland etwas Protektion hat,", "tokens": ["Wer", "hier", "in", "Deutschland", "et\u00b7was", "Pro\u00b7tek\u00b7ti\u00b7on", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "dem wird kein Str\u00e4flings-Kahlkopf je rasiert.", "tokens": ["dem", "wird", "kein", "Str\u00e4f\u00b7lings\u00b7Kahl\u00b7kopf", "je", "ra\u00b7siert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pa\u00df auf: man l\u00e4\u00dft ihm einundzwanzig Monat,", "tokens": ["Pa\u00df", "auf", ":", "man", "l\u00e4\u00dft", "ihm", "ein\u00b7und\u00b7zwan\u00b7zig", "Mo\u00b7nat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "damit er den Justizschwank gut probiert.", "tokens": ["da\u00b7mit", "er", "den", "Jus\u00b7tiz\u00b7schwank", "gut", "pro\u00b7biert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Kommunisten Posten, Wassertr\u00e4ger", "tokens": ["Der", "Kom\u00b7mu\u00b7nis\u00b7ten", "Pos\u00b7ten", ",", "Was\u00b7ser\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sperrt man am liebsten vorm Delikte ein;", "tokens": ["sperrt", "man", "am", "liebs\u00b7ten", "vorm", "De\u00b7lik\u00b7te", "ein", ";"], "token_info": ["word",