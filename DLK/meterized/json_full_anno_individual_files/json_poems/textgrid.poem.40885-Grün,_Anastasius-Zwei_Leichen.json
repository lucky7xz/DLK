{"textgrid.poem.40885": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Leichen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei theure Leichen liegen im Schweizerland zumal,", "tokens": ["Zwei", "theu\u00b7re", "Lei\u00b7chen", "lie\u00b7gen", "im", "Schwei\u00b7zer\u00b7land", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die ein' im Feld bei Frastenz, die andr' in Dornecks Thal!", "tokens": ["Die", "ein'", "im", "Feld", "bei", "Fras\u00b7tenz", ",", "die", "andr'", "in", "Dor\u00b7necks", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "APPR", "NE", "$,", "PRELS", "PIS", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Allbeide edel, doch haben sie sonst wohl nichts gemein", "tokens": ["All\u00b7bei\u00b7de", "e\u00b7del", ",", "doch", "ha\u00b7ben", "sie", "sonst", "wohl", "nichts", "ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "PIS", "ADJD"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Als blut'ge Herzenswunden und ew'gen Schlaf allein.", "tokens": ["Als", "blut'\u00b7ge", "Her\u00b7zens\u00b7wun\u00b7den", "und", "ew'\u00b7gen", "Schlaf", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der eine schien gesunken als starker Felsenthurm,", "tokens": ["Der", "ei\u00b7ne", "schien", "ge\u00b7sun\u00b7ken", "als", "star\u00b7ker", "Fel\u00b7sen\u00b7thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVPP", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der k\u00fchn im Sturm gestanden, doch auch gest\u00fcrzt im Sturm;", "tokens": ["Der", "k\u00fchn", "im", "Sturm", "ge\u00b7stan\u00b7den", ",", "doch", "auch", "ge\u00b7st\u00fcrzt", "im", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVPP", "$,", "ADV", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sein Herz, nun welk und f\u00fchllos, ein ausgebrannter Vulkan,", "tokens": ["Sein", "Herz", ",", "nun", "welk", "und", "f\u00fchl\u00b7los", ",", "ein", "aus\u00b7ge\u00b7brann\u00b7ter", "Vul\u00b7kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Einst herrlich, flammenstrahlend! Tod dem, der's wagt zu nahn!", "tokens": ["Einst", "herr\u00b7lich", ",", "flam\u00b7men\u00b7strah\u00b7lend", "!", "Tod", "dem", ",", "der's", "wagt", "zu", "nahn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVPP", "$.", "NN", "ART", "$,", "PIS", "VVFIN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Andr' ein uralter zertr\u00fcmmerter Altar,", "tokens": ["Der", "Andr'", "ein", "ur\u00b7al\u00b7ter", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ter", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Drauf einst die Opferflamme gelodert rein und klar;", "tokens": ["Drauf", "einst", "die", "Op\u00b7fer\u00b7flam\u00b7me", "ge\u00b7lo\u00b7dert", "rein", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "VVPP", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sein Herz das milde Abbild der Sonn', ein Regenbogen,", "tokens": ["Sein", "Herz", "das", "mil\u00b7de", "Ab\u00b7bild", "der", "Sonn'", ",", "ein", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Dem schlie\u00dft ein Weib das Auge, und ihre Thr\u00e4ne rinnt;", "tokens": ["Dem", "schlie\u00dft", "ein", "Weib", "das", "Au\u00b7ge", ",", "und", "ih\u00b7re", "Thr\u00e4\u00b7ne", "rinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die\u00df Weib, ist's nicht die Freiheit? Es ist des Greises Kind!", "tokens": ["Die\u00df", "Weib", ",", "ist's", "nicht", "die", "Frei\u00b7heit", "?", "Es", "ist", "des", "Grei\u00b7ses", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "VAFIN", "PTKNEG", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Herzen seines Volkes, die sind sein Todtenbuch,", "tokens": ["Die", "Her\u00b7zen", "sei\u00b7nes", "Vol\u00b7kes", ",", "die", "sind", "sein", "Tod\u00b7ten\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Die freie Heimaterde, die ist sein Leichentuch.", "tokens": ["Die", "frei\u00b7e", "Hei\u00b7ma\u00b7ter\u00b7de", ",", "die", "ist", "sein", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wer dr\u00fcckt ihm zu die Augen, wer wird ihm Thr\u00e4nen weihn?", "tokens": ["Wer", "dr\u00fcckt", "ihm", "zu", "die", "Au\u00b7gen", ",", "wer", "wird", "ihm", "Thr\u00e4\u00b7nen", "weihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWS", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Blieb nichts ihm treu? O sehet, sein traurig Schlachtro\u00df dort", "tokens": ["Blieb", "nichts", "ihm", "treu", "?", "O", "se\u00b7het", ",", "sein", "trau\u00b7rig", "Schlach\u00b7tro\u00df", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "$.", "NE", "VVFIN", "$,", "PPOSAT", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Scheucht ihm vom Haupt die Raben, die ungeduldigen, fort.", "tokens": ["Scheucht", "ihm", "vom", "Haupt", "die", "Ra\u00b7ben", ",", "die", "un\u00b7ge\u00b7dul\u00b7di\u00b7gen", ",", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "PTKVZ", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Wie K\u00f6n'ge stolz war dieser und war doch nur ein Knecht,", "tokens": ["Wie", "K\u00f6n'\u00b7ge", "stolz", "war", "die\u00b7ser", "und", "war", "doch", "nur", "ein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VAFIN", "PDAT", "KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Frei jener wie kein K\u00f6nig, doch eben schlecht und recht;", "tokens": ["Frei", "je\u00b7ner", "wie", "kein", "K\u00f6\u00b7nig", ",", "doch", "e\u00b7ben", "schlecht", "und", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "KOKOM", "PIAT", "NN", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbdort liegt Wohlleb!\u00ab Der Schweizer zeigt's, ruhmerr\u00f6thend, euch,", "tokens": ["\u00bb", "dort", "liegt", "Wohl\u00b7leb", "!", "\u00ab", "Der", "Schwei\u00b7zer", "zeigt's", ",", "ruh\u00b7mer\u00b7r\u00f6\u00b7thend", ",", "euch", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "NE", "$.", "$(", "ART", "NN", "VVFIN", "$,", "VVPP", "$,", "PPER", "$,"], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbund hier der F\u00fcrstenberger!\u00ab Da bebt er und wird bleich.", "tokens": ["\u00bb", "und", "hier", "der", "F\u00fcrs\u00b7ten\u00b7ber\u00b7ger", "!", "\u00ab", "Da", "bebt", "er", "und", "wird", "bleich", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein K\u00e4stlein, drein die Freiheit gern ihren Brautring legt,", "tokens": ["Ein", "K\u00e4st\u00b7lein", ",", "drein", "die", "Frei\u00b7heit", "gern", "ih\u00b7ren", "Brau\u00b7tring", "legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das scheint der Sarg des Einen, der solche Worte tr\u00e4gt:", "tokens": ["Das", "scheint", "der", "Sarg", "des", "Ei\u00b7nen", ",", "der", "sol\u00b7che", "Wor\u00b7te", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbich bin ein freier Schweizer, Heinz Wohlleb zubenannt,", "tokens": ["\u00bb", "ich", "bin", "ein", "frei\u00b7er", "Schwei\u00b7zer", ",", "Heinz", "Wohl\u00b7leb", "zu\u00b7be\u00b7nannt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die\u00df H\u00e4uschen und sein Sasse stehn beid' in Gottes Hand.\u00ab", "tokens": ["Die\u00df", "H\u00e4usc\u00b7hen", "und", "sein", "Sas\u00b7se", "stehn", "beid'", "in", "Got\u00b7tes", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PIS", "APPR", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Sarg des Andern aber schien eines F\u00fcrsten Schrein,", "tokens": ["Der", "Sarg", "des", "An\u00b7dern", "a\u00b7ber", "schien", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Voll bluterkaufter Juwelen, drauf grub die\u00df Wort man ein:", "tokens": ["Voll", "blu\u00b7ter\u00b7kauf\u00b7ter", "Ju\u00b7we\u00b7len", ",", "drauf", "grub", "die\u00df", "Wort", "man", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PAV", "VVFIN", "PDS", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "\u00bbdem K\u00f6nig war mein Leben, die Seele Gott allein,", "tokens": ["\u00bb", "dem", "K\u00f6\u00b7nig", "war", "mein", "Le\u00b7ben", ",", "die", "See\u00b7le", "Gott", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mein Herz den Fraun ergeben, die Ehre nur blieb mein.\u00ab", "tokens": ["Mein", "Herz", "den", "Fraun", "er\u00b7ge\u00b7ben", ",", "die", "Eh\u00b7re", "nur", "blieb", "mein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "O Ehre, F\u00fcrsten, Frauen! ha, gebt ihr solchen Lohn?", "tokens": ["O", "Eh\u00b7re", ",", "F\u00fcrs\u00b7ten", ",", "Frau\u00b7en", "!", "ha", ",", "gebt", "ihr", "sol\u00b7chen", "Lohn", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$.", "ITJ", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Speist selbst auf Grabessteine, o Welt, du deinen Hohn?", "tokens": ["Speist", "selbst", "auf", "Gra\u00b7bess\u00b7tei\u00b7ne", ",", "o", "Welt", ",", "du", "dei\u00b7nen", "Hohn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "$,", "FM", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schlaft sanft, ihr Zwei! Ihr aber, die ihr noch jetzo wacht:", "tokens": ["Schlaft", "sanft", ",", "ihr", "Zwei", "!", "Ihr", "a\u00b7ber", ",", "die", "ihr", "noch", "jet\u00b7zo", "wacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "CARD", "$.", "PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "An wessen Stelle lieber schlieft ihr die ew'ge Nacht?", "tokens": ["An", "wes\u00b7sen", "Stel\u00b7le", "lie\u00b7ber", "schlieft", "ihr", "die", "ew'\u00b7ge", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.10": {"line.1": {"text": "Zwei theure Leichen liegen im Schweizerland zumal,", "tokens": ["Zwei", "theu\u00b7re", "Lei\u00b7chen", "lie\u00b7gen", "im", "Schwei\u00b7zer\u00b7land", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die ein' im Feld bei Frastenz, die andr' in Dornecks Thal!", "tokens": ["Die", "ein'", "im", "Feld", "bei", "Fras\u00b7tenz", ",", "die", "andr'", "in", "Dor\u00b7necks", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "APPR", "NE", "$,", "PRELS", "PIS", "APPR", "NE", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Allbeide edel, doch haben sie sonst wohl nichts gemein", "tokens": ["All\u00b7bei\u00b7de", "e\u00b7del", ",", "doch", "ha\u00b7ben", "sie", "sonst", "wohl", "nichts", "ge\u00b7mein"], "token_