{"dta.poem.19220": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst ward dem Zauberer in abergl\u00e4ubischen Tagen", "tokens": ["Sonst", "ward", "dem", "Zau\u00b7be\u00b7rer", "in", "a\u00b7berg\u00b7l\u00e4u\u00b7bi\u00b7schen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Vorderzahn des Munds ein obrer eingeschlagen,", "tokens": ["Ein", "Vor\u00b7der\u00b7zahn", "des", "Munds", "ein", "ob\u00b7rer", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Schlange gleichsam so der Giftzahn ausgebrochen,", "tokens": ["Der", "Schlan\u00b7ge", "gleich\u00b7sam", "so", "der", "Gift\u00b7zahn", "aus\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df kraftlos sei das Wort, undeutlich ausgesprochen.", "tokens": ["Da\u00df", "kraft\u00b7los", "sei", "das", "Wort", ",", "un\u00b7deut\u00b7lich", "aus\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch schl\u00fcgst du Zahn und Zahn dem Ohrenbl\u00e4ser ein,", "tokens": ["Doch", "schl\u00fcgst", "du", "Zahn", "und", "Zahn", "dem", "Oh\u00b7ren\u00b7bl\u00e4\u00b7ser", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Ohrenblasen wird nicht minder giftig seyn.", "tokens": ["Sein", "Oh\u00b7ren\u00b7bla\u00b7sen", "wird", "nicht", "min\u00b7der", "gif\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}