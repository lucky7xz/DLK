{"textgrid.poem.50848": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Am Himmelfahrtstag", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgestorben scheint die Stadt,", "tokens": ["Aus\u00b7ge\u00b7stor\u00b7ben", "scheint", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil, was Freude f\u00fchlt und Leben", "tokens": ["Weil", ",", "was", "Freu\u00b7de", "f\u00fchlt", "und", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWS", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein gl\u00e4ubig Herz sich heben,", "tokens": ["Und", "ein", "gl\u00e4u\u00b7big", "Herz", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich hinaus begeben hat", "tokens": ["Sich", "hin\u00b7aus", "be\u00b7ge\u00b7ben", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APZR", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den See und auf die Berge;", "tokens": ["Auf", "den", "See", "und", "auf", "die", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Angef\u00fcllt wird jedes Tal;", "tokens": ["An\u00b7ge\u00b7f\u00fcllt", "wird", "je\u00b7des", "Tal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00fchren mu\u00df sich Wirt und Ferge", "tokens": ["R\u00fch\u00b7ren", "mu\u00df", "sich", "Wirt", "und", "Fer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem warmen Maienstrahl.", "tokens": ["In", "dem", "war\u00b7men", "Mai\u00b7en\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von des Daches Giebel schau", "tokens": ["Von", "des", "Da\u00b7ches", "Gie\u00b7bel", "schau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich hinaus, o welch Gewimmel!", "tokens": ["Ich", "hin\u00b7aus", ",", "o", "welch", "Ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "FM", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die Erde tr\u00e4gt gen Himmel", "tokens": ["Ja", ",", "die", "Er\u00b7de", "tr\u00e4gt", "gen", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschenherz und gr\u00fcne Au!", "tokens": ["Men\u00b7schen\u00b7herz", "und", "gr\u00fc\u00b7ne", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehe, wie lebend'ge Fahnen", "tokens": ["Sie\u00b7he", ",", "wie", "le\u00b7ben\u00b7d'\u00b7ge", "Fah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Flattern dort am Berggel\u00e4nder", "tokens": ["Flat\u00b7tern", "dort", "am", "Berg\u00b7ge\u00b7l\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kinder, bunte Lenzgew\u00e4nder,", "tokens": ["Kin\u00b7der", ",", "bun\u00b7te", "Lenz\u00b7ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter gr\u00fcnenden Platanen!", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7nen\u00b7den", "Pla\u00b7ta\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einsam wehen hier die Linden", "tokens": ["Ein\u00b7sam", "we\u00b7hen", "hier", "die", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Stadt um stille D\u00e4cher \u2013", "tokens": ["Die\u00b7ser", "Stadt", "um", "stil\u00b7le", "D\u00e4\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wie einen leeren Becher", "tokens": ["Ach", ",", "wie", "ei\u00b7nen", "lee\u00b7ren", "Be\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich die verla\u00dfne finden,", "tokens": ["Mu\u00df", "ich", "die", "ver\u00b7la\u00df\u00b7ne", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Becher, dessen Schein", "tokens": ["Ei\u00b7nen", "Be\u00b7cher", ",", "des\u00b7sen", "Schein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird geflohn von jedem Munde", "tokens": ["Wird", "ge\u00b7flohn", "von", "je\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf dessen dunklem Grunde", "tokens": ["Und", "auf", "des\u00b7sen", "dunk\u00b7lem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich der letzte Tropfen Wein!", "tokens": ["Ich", "der", "letz\u00b7te", "Trop\u00b7fen", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "In die k\u00fchle D\u00e4mmernacht", "tokens": ["In", "die", "k\u00fch\u00b7le", "D\u00e4m\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Hauses steig ich nieder,", "tokens": ["Mei\u00b7nes", "Hau\u00b7ses", "steig", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mir meine jungen Lieder", "tokens": ["Wo", "mir", "mei\u00b7ne", "jun\u00b7gen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlummern, bis ihr Tag erwacht;", "tokens": ["Schlum\u00b7mern", ",", "bis", "ihr", "Tag", "er\u00b7wacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ein Strau\u00df von Bl\u00fctenzweigen", "tokens": ["Wo", "ein", "Strau\u00df", "von", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcber nickt mit stillem Neigen,", "tokens": ["Dr\u00fc\u00b7ber", "nickt", "mit", "stil\u00b7lem", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit erwartungsvollem Schweigen", "tokens": ["Mit", "er\u00b7war\u00b7tungs\u00b7vol\u00b7lem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Junge Rosen halten Wacht.", "tokens": ["Jun\u00b7ge", "Ro\u00b7sen", "hal\u00b7ten", "Wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ich lange z\u00f6gernd mied:", "tokens": ["Was", "ich", "lan\u00b7ge", "z\u00f6\u00b7gernd", "mied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun in tiefer Einsamkeit", "tokens": ["Nun", "in", "tie\u00b7fer", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schreib ich dieses letzte Lied,", "tokens": ["Schreib", "ich", "die\u00b7ses", "letz\u00b7te", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hoffnung sei's geweiht,", "tokens": ["Und", "der", "Hoff\u00b7nung", "sei's", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Preis ihr, wenn sie endlich h\u00e4lt", "tokens": ["Preis", "ihr", ",", "wenn", "sie", "end\u00b7lich", "h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich zur Himmelfahrt bereit!", "tokens": ["Sich", "zur", "Him\u00b7mel\u00b7fahrt", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O sie braucht nicht weit zu fahren,", "tokens": ["O", "sie", "braucht", "nicht", "weit", "zu", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Himmel in sich wahrt:", "tokens": ["Die", "den", "Him\u00b7mel", "in", "sich", "wahrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ist wie ein Heil'genschrein:", "tokens": ["Sie", "ist", "wie", "ein", "Heil'\u00b7gen\u00b7schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Au\u00dfen lieblich bunt bemalet,", "tokens": ["Au\u00b7\u00dfen", "lieb\u00b7lich", "bunt", "be\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch verdeckt im Innern strahlet", "tokens": ["Doch", "ver\u00b7deckt", "im", "In\u00b7nern", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pures Gold und Edelstein.", "tokens": ["Pu\u00b7res", "Gold", "und", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tu dich auf, o sch\u00f6ner Schrein,", "tokens": ["Tu", "dich", "auf", ",", "o", "sch\u00f6\u00b7ner", "Schrein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasse deine Sch\u00e4tze funkeln!", "tokens": ["Las\u00b7se", "dei\u00b7ne", "Sch\u00e4t\u00b7ze", "fun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie, blitzend hell, verdunkeln", "tokens": ["La\u00df", "sie", ",", "blit\u00b7zend", "hell", ",", "ver\u00b7dun\u00b7keln"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "$,", "ADJD", "ADJD", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der M\u00e4rtyrer bla\u00df Gebein! \u2013", "tokens": ["Der", "M\u00e4r\u00b7ty\u00b7rer", "bla\u00df", "Ge\u00b7bein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Freiheitschwanger sind die L\u00fcfte:", "tokens": ["Frei\u00b7heitsc\u00b7hwan\u00b7ger", "sind", "die", "L\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieg hinaus, mein Schwalbenzug!", "tokens": ["Flieg", "hin\u00b7aus", ",", "mein", "Schwal\u00b7ben\u00b7zug", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APZR", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flattre hin, mein Liederflug,", "tokens": ["Flatt\u00b7re", "hin", ",", "mein", "Lie\u00b7der\u00b7flug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Klingend durch die Fr\u00fchlingsd\u00fcfte!", "tokens": ["Klin\u00b7gend", "durch", "die", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ausgestorben scheint die Stadt,", "tokens": ["Aus\u00b7ge\u00b7stor\u00b7ben", "scheint", "die", "Stadt", ","], "token_info": ["word", "word", 