rd", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Wei\u00df immer anzuregen.", "tokens": ["Wei\u00df", "im\u00b7mer", "an\u00b7zu\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es gibt, dem Leoparden gleich,", "tokens": ["Es", "gibt", ",", "dem", "Le\u00b7o\u00b7par\u00b7den", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen Herrn, der, stolz und reich,", "tokens": ["So", "man\u00b7chen", "Herrn", ",", "der", ",", "stolz", "und", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als einziges Talent", "tokens": ["Als", "ein\u00b7zi\u00b7ges", "Ta\u00b7lent"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sein Kleid nur hat und kennt.", "tokens": ["Sein", "Kleid", "nur", "hat", "und", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}