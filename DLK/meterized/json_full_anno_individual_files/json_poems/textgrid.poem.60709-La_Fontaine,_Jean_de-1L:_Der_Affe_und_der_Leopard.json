{"textgrid.poem.60709": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Affe und der Leopard", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Affe und der Leopard", "tokens": ["Der", "Af\u00b7fe", "und", "der", "Le\u00b7o\u00b7pard"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdienten auf der Messe Geld,", "tokens": ["Ver\u00b7dien\u00b7ten", "auf", "der", "Mes\u00b7se", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem sie sich zur Schau gestellt,", "tokens": ["In\u00b7dem", "sie", "sich", "zur", "Schau", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einer des andern Widerpart.", "tokens": ["Ei\u00b7ner", "des", "an\u00b7dern", "Wi\u00b7der\u00b7part", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der eine rief: \u00bbIhr lieben Leute,", "tokens": ["Der", "ei\u00b7ne", "rief", ":", "\u00bb", "Ihr", "lie\u00b7ben", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Ruf und Ruhm ist nicht von heute,", "tokens": ["Mein", "Ruf", "und", "Ruhm", "ist", "nicht", "von", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "PTKNEG", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin bekannt an h\u00f6chster Stell;", "tokens": ["Ich", "bin", "be\u00b7kannt", "an", "h\u00f6chs\u00b7ter", "Stell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der K\u00f6nig kam, um mich zu sehen,", "tokens": ["Der", "K\u00f6\u00b7nig", "kam", ",", "um", "mich", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sterb ich einst, dann soll mein Fell", "tokens": ["Und", "sterb", "ich", "einst", ",", "dann", "soll", "mein", "Fell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als Muff sich um die H\u00e4nde drehen", "tokens": ["Als", "Muff", "sich", "um", "die", "H\u00e4n\u00b7de", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der allergn\u00e4digsten K\u00f6nigin.", "tokens": ["Der", "al\u00b7lerg\u00b7n\u00e4\u00b7digs\u00b7ten", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Betrachtet mich, wie bunt ich bin:", "tokens": ["Be\u00b7trach\u00b7tet", "mich", ",", "wie", "bunt", "ich", "bin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gestreift, gesprenkelt und gescheckt,", "tokens": ["Ge\u00b7streift", ",", "ge\u00b7spren\u00b7kelt", "und", "ge\u00b7scheckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Getupft, beringelt und gefleckt.\u00ab", "tokens": ["Ge\u00b7tupft", ",", "be\u00b7rin\u00b7gelt", "und", "ge\u00b7fleckt", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "VVFIN", "KON", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das bunte Spiel der Haut gefiel", "tokens": ["Das", "bun\u00b7te", "Spiel", "der", "Haut", "ge\u00b7fiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "F\u00fcr einen Blick als Augenziel,", "tokens": ["F\u00fcr", "ei\u00b7nen", "Blick", "als", "Au\u00b7gen\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein jeder sah es fl\u00fcchtig an", "tokens": ["Ein", "je\u00b7der", "sah", "es", "fl\u00fcch\u00b7tig", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und wendete sich weiter dann.", "tokens": ["Und", "wen\u00b7de\u00b7te", "sich", "wei\u00b7ter", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Affe rief: \u00bbHerbei, herbei,", "tokens": ["Der", "Af\u00b7fe", "rief", ":", "\u00bb", "Her\u00b7bei", ",", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ihr edlen Herrn und sch\u00f6nen Damen!", "tokens": ["Ihr", "ed\u00b7len", "Herrn", "und", "sch\u00f6\u00b7nen", "Da\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ich kenne K\u00fcnste mancherlei", "tokens": ["Ich", "ken\u00b7ne", "K\u00fcns\u00b7te", "man\u00b7cher\u00b7lei"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und nicht nur nach dem Namen.", "tokens": ["Und", "nicht", "nur", "nach", "dem", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die Buntheit, die ihr hier", "tokens": ["Die", "Bunt\u00b7heit", ",", "die", "ihr", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Bei meinem Nachbarn schaut,", "tokens": ["Bei", "mei\u00b7nem", "Nach\u00b7barn", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Die trag ich auch bei mir,", "tokens": ["Die", "trag", "ich", "auch", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Doch nicht auf meiner Haut,", "tokens": ["Doch", "nicht", "auf", "mei\u00b7ner", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Vielmehr in meinem Geist.", "tokens": ["Viel\u00b7mehr", "in", "mei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Weit bin ich hergereist,", "tokens": ["Weit", "bin", "ich", "her\u00b7ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.29": {"text": "Drei Schiffe haben mich getragen.", "tokens": ["Drei", "Schif\u00b7fe", "ha\u00b7ben", "mich", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Mu\u00df ich noch meinen Namen sagen?", "tokens": ["Mu\u00df", "ich", "noch", "mei\u00b7nen", "Na\u00b7men", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Hier steht Hans Wurst! Zwiefach verwandt", "tokens": ["Hier", "steht", "Hans", "Wurst", "!", "Zwie\u00b7fach", "ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "VAFIN", "$.", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Mit Bertrand, der des Papstes Affe.", "tokens": ["Mit", "Ber\u00b7trand", ",", "der", "des", "Paps\u00b7tes", "Af\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Gesegelt ist er und gerannt,", "tokens": ["Ge\u00b7se\u00b7gelt", "ist", "er", "und", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Damit er hier euch Kurzweil schaffe.", "tokens": ["Da\u00b7mit", "er", "hier", "euch", "Kurz\u00b7weil", "schaf\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Er tut's! Man h\u00f6re, schau und staune:", "tokens": ["Er", "tut's", "!", "Man", "h\u00f6\u00b7re", ",", "schau", "und", "stau\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIS", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Er ist ein T\u00e4nzer, ist ein Hexer,", "tokens": ["Er", "ist", "ein", "T\u00e4n\u00b7zer", ",", "ist", "ein", "He\u00b7xer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Springt Seil und Reifen, bl\u00e4st Posaune \u2013", "tokens": ["Springt", "Seil", "und", "Rei\u00b7fen", ",", "bl\u00e4st", "Po\u00b7sau\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und alles dies f\u00fcr einen Sechser.", "tokens": ["Und", "al\u00b7les", "dies", "f\u00fcr", "ei\u00b7nen", "Sech\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Was sag ich? Schon f\u00fcr einen Sou!", "tokens": ["Was", "sag", "ich", "?", "Schon", "f\u00fcr", "ei\u00b7nen", "Sou", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und wenn euch seine Kunst mi\u00dff\u00e4llt,", "tokens": ["Und", "wenn", "euch", "sei\u00b7ne", "Kunst", "mi\u00df\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "So geht und schaut nicht l\u00e4nger zu,", "tokens": ["So", "geht", "und", "schaut", "nicht", "l\u00e4n\u00b7ger", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ihr kriegt zur\u00fcck das Eintrittsgeld.\u00ab", "tokens": ["Ihr", "kriegt", "zu\u00b7r\u00fcck", "das", "Ein\u00b7tritts\u00b7geld", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Affe redete gescheit.", "tokens": ["Der", "Af\u00b7fe", "re\u00b7de\u00b7te", "ge\u00b7scheit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Es gilt die Mannigfaltigkeit", "tokens": ["Es", "gilt", "die", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Des Geistes mehr als die am Kleid:", "tokens": ["Des", "Geis\u00b7tes", "mehr", "als", "die", "am", "Kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Ein buntes Fell", "tokens": ["Ein", "bun\u00b7tes", "Fell"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Erm\u00fcdet schnell,", "tokens": ["Er\u00b7m\u00fc\u00b7det", "schnell", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.48": {"text": "Ein bunter Geist dagegen", "tokens": ["Ein", "bun\u00b7ter", "Geist", "da\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Wei\u00df immer anzuregen.", "tokens": ["Wei\u00df", "im\u00b7mer", "an\u00b7zu\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es gibt, dem Leoparden gleich,", "tokens": ["Es", "gibt", ",", "dem", "Le\u00b7o\u00b7par\u00b7den", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen Herrn, der, stolz und reich,", "tokens": ["So", "man\u00b7chen", "Herrn", ",", "der", ",", "stolz", "und", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als einziges Talent", "tokens": ["Als", "ein\u00b7zi\u00b7ges", "Ta\u00b7lent"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sein Kleid nur hat und kennt.", "tokens": ["Sein", "Kleid", "nur", "hat", "und", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Affe und der Leopard", "tokens": ["Der", "Af\u00b7fe", "und", "der", "Le\u00b7o\u00b7pard"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdienten auf der Messe Geld,", "tokens": ["Ver\u00b7dien\u00b7ten", "auf", "der", "Mes\u00b7se", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem sie sich zur Schau gestellt,", "tokens": ["In\u00b7dem", "sie", "sich", "zur", "Schau", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einer des andern Widerpart.", "tokens": ["Ei\u00b7ner", "des", "an\u00b7dern", "Wi\u00b7der\u00b7part", "."], "token_info": ["word", "word", 