{"textgrid.poem.61142": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach so viel tr\u00fcben, tr\u00fcben Nebeltagen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach so viel tr\u00fcben, tr\u00fcben Nebeltagen,", "tokens": ["Nach", "so", "viel", "tr\u00fc\u00b7ben", ",", "tr\u00fc\u00b7ben", "Ne\u00b7bel\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du goldner Schein, der aus dem Blauen flie\u00dft", "tokens": ["Du", "gold\u00b7ner", "Schein", ",", "der", "aus", "dem", "Blau\u00b7en", "flie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und klar durch meine Seele sich ergie\u00dft,", "tokens": ["Und", "klar", "durch", "mei\u00b7ne", "See\u00b7le", "sich", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Schein des Trosts, la\u00df meinen Gru\u00df dir sagen!", "tokens": ["O", "Schein", "des", "Trosts", ",", "la\u00df", "mei\u00b7nen", "Gru\u00df", "dir", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich war mit Angst und Traurigkeit geschlagen,", "tokens": ["Ich", "war", "mit", "Angst", "und", "Trau\u00b7rig\u00b7keit", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch nun ist's gut, da sich der Strahl erschlie\u00dft;", "tokens": ["Doch", "nun", "ist's", "gut", ",", "da", "sich", "der", "Strahl", "er\u00b7schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und leise, leise, wie die Rose sprie\u00dft,", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", ",", "wie", "die", "Ro\u00b7se", "sprie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darf Lust und Hoffnung aufzubl\u00fchen wagen.", "tokens": ["Darf", "Lust", "und", "Hoff\u00b7nung", "auf\u00b7zu\u00b7bl\u00fc\u00b7hen", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O scheltet nicht, da\u00df ich, ein Sohn der Erde", "tokens": ["O", "schel\u00b7tet", "nicht", ",", "da\u00df", "ich", ",", "ein", "Sohn", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tief im Wesen der Natur vereint,", "tokens": ["Und", "tief", "im", "We\u00b7sen", "der", "Na\u00b7tur", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von ihrem Angesicht geleitet werde!", "tokens": ["Von", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", "ge\u00b7lei\u00b7tet", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr seht ja doch, da\u00df, wenn die Mutter weint,", "tokens": ["Ihr", "seht", "ja", "doch", ",", "da\u00df", ",", "wenn", "die", "Mut\u00b7ter", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Kind verstummt mit trauriger Geb\u00e4rde", "tokens": ["Das", "Kind", "ver\u00b7stummt", "mit", "trau\u00b7ri\u00b7ger", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder l\u00e4chelt, wenn sie froh erscheint.", "tokens": ["Und", "wie\u00b7der", "l\u00e4\u00b7chelt", ",", "wenn", "sie", "froh", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nach so viel tr\u00fcben, tr\u00fcben Nebeltagen,", "tokens": ["Nach", "so", "viel", "tr\u00fc\u00b7ben", ",", "tr\u00f