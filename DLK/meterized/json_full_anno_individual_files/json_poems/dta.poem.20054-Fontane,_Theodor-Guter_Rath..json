{"dta.poem.20054": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "An einem Sommermorgen", "tokens": ["An", "ei\u00b7nem", "Som\u00b7mer\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da nimm den Wanderstab,", "tokens": ["Da", "nimm", "den", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fallen Deine Sorgen", "tokens": ["Es", "fal\u00b7len", "Dei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Nebel von Dir ab.", "tokens": ["Wie", "Ne\u00b7bel", "von", "Dir", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Himmels heitere Bl\u00e4ue", "tokens": ["Des", "Him\u00b7mels", "hei\u00b7te\u00b7re", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lacht Dir in\u2019s Herz hinein,", "tokens": ["Lacht", "Dir", "in's", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schlie\u00dft, wie Gottes Treue,", "tokens": ["Und", "schlie\u00dft", ",", "wie", "Got\u00b7tes", "Treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Dach Dich ein.", "tokens": ["Mit", "sei\u00b7nem", "Dach", "Dich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rings Bl\u00fcthen nur und Triebe", "tokens": ["Rings", "Bl\u00fc\u00b7then", "nur", "und", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Halme von Segen schwer,", "tokens": ["Und", "Hal\u00b7me", "von", "Se\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dir ist als z\u00f6ge die Liebe", "tokens": ["Dir", "ist", "als", "z\u00f6\u00b7ge", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Des Weges nebenher.", "tokens": ["Des", "We\u00b7ges", "ne\u00b7ben\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "So heimisch alles klinget", "tokens": ["So", "hei\u00b7misch", "al\u00b7les", "klin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie im Vaterhaus,", "tokens": ["Als", "wie", "im", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber die Lerchen schwinget", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Ler\u00b7chen", "schwin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Seele sich hinaus.", "tokens": ["Die", "See\u00b7le", "sich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}