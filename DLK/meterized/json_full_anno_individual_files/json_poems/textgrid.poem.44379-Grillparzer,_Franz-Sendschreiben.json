{"textgrid.poem.44379": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Sendschreiben", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sehr auch unser Freund, der Grenadier und Dichter,", "tokens": ["So", "sehr", "auch", "un\u00b7ser", "Freund", ",", "der", "Gre\u00b7na\u00b7di\u00b7er", "und", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Qual geschildert hat, die gr\u00e4mlichen Gesichter,", "tokens": ["Die", "Qual", "ge\u00b7schil\u00b7dert", "hat", ",", "die", "gr\u00e4m\u00b7li\u00b7chen", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Langeweile, die im Wildbad nun regiert,", "tokens": ["Die", "Lan\u00b7ge\u00b7wei\u00b7le", ",", "die", "im", "Wild\u00b7bad", "nun", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seit ihr mit euch das Sch\u00f6nste weggef\u00fchrt;", "tokens": ["Seit", "ihr", "mit", "euch", "das", "Sch\u00f6ns\u00b7te", "weg\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hat er doch \u2013 vielleicht aus Furcht, sich zu verraten,", "tokens": ["So", "hat", "er", "doch", "\u2013", "viel\u00b7leicht", "aus", "Furcht", ",", "sich", "zu", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$(", "ADV", "APPR", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil man auf Feuer schlie\u00dft, da wo man Rauch erblickt \u2013", "tokens": ["Weil", "man", "auf", "Feu\u00b7er", "schlie\u00dft", ",", "da", "wo", "man", "Rauch", "er\u00b7blickt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,", "ADV", "PWAV", "PIS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur halb geschildert, was uns ganz bedr\u00fcckt.", "tokens": ["Nur", "halb", "ge\u00b7schil\u00b7dert", ",", "was", "uns", "ganz", "be\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht recht von einem Dichter und Soldaten!", "tokens": ["Nicht", "recht", "von", "ei\u00b7nem", "Dich\u00b7ter", "und", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O wi\u00dft es nur, wi\u00dft nur die Wahrheit ganz!", "tokens": ["O", "wi\u00dft", "es", "nur", ",", "wi\u00dft", "nur", "die", "Wahr\u00b7heit", "ganz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zerrissen ist der Freude Blumenkranz,", "tokens": ["Zer\u00b7ris\u00b7sen", "ist", "der", "Freu\u00b7de", "Blu\u00b7men\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und Erde, Luft und Wasser haben sich verschworen,", "tokens": ["Und", "Er\u00b7de", ",", "Luft", "und", "Was\u00b7ser", "ha\u00b7ben", "sich", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Seitdem sie euch, die G\u00fcnstlinge, verloren.", "tokens": ["Seit\u00b7dem", "sie", "euch", ",", "die", "G\u00fcnst\u00b7lin\u00b7ge", ",", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Es hat der Himmel sich mit schwarzem Flor beh\u00e4ngt", "tokens": ["Es", "hat", "der", "Him\u00b7mel", "sich", "mit", "schwar\u00b7zem", "Flor", "be\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und weint in dicken, schweren Tropfen;", "tokens": ["Und", "weint", "in", "di\u00b7cken", ",", "schwe\u00b7ren", "Trop\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So sehr man ihn mit Flehn und Bitten dr\u00e4ngt,", "tokens": ["So", "sehr", "man", "ihn", "mit", "Flehn", "und", "Bit\u00b7ten", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nichts kann die Schleusen seines Zorns verstopfen.", "tokens": ["Nichts", "kann", "die", "Schleu\u00b7sen", "sei\u00b7nes", "Zorns", "ver\u00b7stop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Es tobt der Wasserfall mit doppler Macht,", "tokens": ["Es", "tobt", "der", "Was\u00b7ser\u00b7fall", "mit", "dopp\u00b7ler", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er br\u00fcllt wie ein verwundet Ungeheuer", "tokens": ["Er", "br\u00fcllt", "wie", "ein", "ver\u00b7wun\u00b7det", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und weil er mich im irrigen Verdacht,", "tokens": ["Und", "weil", "er", "mich", "im", "ir\u00b7ri\u00b7gen", "Ver\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df eine andre Frau noch au\u00dfer euch mir teuer,", "tokens": ["Da\u00df", "ei\u00b7ne", "and\u00b7re", "Frau", "noch", "au\u00b7\u00dfer", "euch", "mir", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Pocht er an mein Gemach bei stiller Nacht,", "tokens": ["Pocht", "er", "an", "mein", "Ge\u00b7mach", "bei", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Als w\u00e4re Tod und Untergang mir zugedacht;", "tokens": ["Als", "w\u00e4\u00b7re", "Tod", "und", "Un\u00b7ter\u00b7gang", "mir", "zu\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Auch hat er mir in seines Zornes Feuer", "tokens": ["Auch", "hat", "er", "mir", "in", "sei\u00b7nes", "Zor\u00b7nes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Vorl\u00e4ufig nur ein t\u00fcchtig Halsweh schon gebracht.", "tokens": ["Vor\u00b7l\u00e4u\u00b7fig", "nur", "ein", "t\u00fcch\u00b7tig", "Hals\u00b7weh", "schon", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Ordnung der Natur hat sich verkehrt,", "tokens": ["Die", "Ord\u00b7nung", "der", "Na\u00b7tur", "hat", "sich", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sogar bei Tisch ist nichts an seiner Stelle,", "tokens": ["So\u00b7gar", "bei", "Tisch", "ist", "nichts", "an", "sei\u00b7ner", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Zur Gr\u00e4te schrumpft die leckere Forelle,", "tokens": ["Zur", "Gr\u00e4\u00b7te", "schrumpft", "die", "le\u00b7cke\u00b7re", "Fo\u00b7rel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Das Rindfleisch riecht, weil ihrs nicht mehr verzehrt,", "tokens": ["Das", "Rind\u00b7fleisch", "riecht", ",", "weil", "ihrs", "nicht", "mehr", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und wer nach so viel Unheil \u00fcbrig noch geblieben,", "tokens": ["Und", "wer", "nach", "so", "viel", "Un\u00b7heil", "\u00fcb\u00b7rig", "noch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADV", "PIAT", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der wird vom K\u00e4lberbraten schm\u00e4hlich aufgerieben.", "tokens": ["Der", "wird", "vom", "K\u00e4l\u00b7ber\u00b7bra\u00b7ten", "schm\u00e4h\u00b7lich", "auf\u00b7ge\u00b7rie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gastein ist nur ein gro\u00dfer Sarg,", "tokens": ["Gas\u00b7tein", "ist", "nur", "ein", "gro\u00b7\u00dfer", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Es klagt der Held, es klagt der S\u00e4nger", "tokens": ["Es", "klagt", "der", "Held", ",", "es", "klagt", "der", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Um euch, je l\u00e4nger, um so b\u00e4nger,", "tokens": ["Um", "euch", ",", "je", "l\u00e4n\u00b7ger", ",", "um", "so", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "ADV", "ADJD", "$,", "KOUI", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Trotz seines Z\u00f6libats, der Patriarch.", "tokens": ["Trotz", "sei\u00b7nes", "Z\u00f6\u00b7li\u00b7bats", ",", "der", "Pat\u00b7ri\u00b7arch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Nichts kann uns Trost, Ersatz uns geben,", "tokens": ["Nichts", "kann", "uns", "Trost", ",", "Er\u00b7satz", "uns", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NN", "$,", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Lehrts doch die Welt, das ganze Dasein so,", "tokens": ["Lehrts", "doch", "die", "Welt", ",", "das", "gan\u00b7ze", "Da\u00b7sein", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Da\u00df, wo die Charis und die Kunst entfloh,", "tokens": ["Da\u00df", ",", "wo", "die", "Cha\u00b7ris", "und", "die", "Kunst", "ent\u00b7floh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Nichts W\u00fcnschenswertes mehr sich zeigt im Leben.", "tokens": ["Nichts", "W\u00fcn\u00b7schens\u00b7wer\u00b7tes", "mehr", "sich", "zeigt", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADV", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So sehr auch unser Freund, der Grenadier und Dichter,", "tokens": ["So", "sehr", "auch", "un\u00b7ser", "Freund", ",", "der", "Gre\u00b7na\u00b7di\u00b7er", "und", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Qual geschildert hat, die gr\u00e4mlichen Gesichter,", "tokens": ["Die", "Qual", "ge\u00b7schil\u00b7dert", "hat", ",", "die", "gr\u00e4m\u00b7li\u00b7chen", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Langeweile, die im Wildbad nun regiert,", "tokens": ["Die", "Lan\u00b7ge\u00b7wei\u00b7le", ",", "die", "im", "Wild\u00b7bad", "nun", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seit ihr mit euch das Sch\u00f6nste weggef\u00fchrt;", "tokens": ["Seit", "ihr", "mit", "euch", "das", "Sch\u00f6ns\u00b7te", "weg\u00b7ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hat er doch \u2013 vielleicht aus Furcht, sich zu verraten,", "tokens": ["So", "hat", "er", "doch", "\u2013", "viel\u00b7leicht", "aus", "Furcht", ",", "sich", "zu", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$(", "ADV", "APPR", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil man auf Feuer schlie\u00dft, da wo man Rauch erblickt \u2013", "tokens": ["Weil", "man", "auf", "Feu\u00b7er", "schlie\u00dft", ",", "da", "wo", "man", "Rauch", "er\u00b7blickt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,", "ADV", "PWAV", "PIS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur halb geschildert, was uns ganz bedr\u00fcckt.", "tokens": ["Nur", "halb", "ge\u00b7schil\u00b7dert", ",", "was", "uns", "ganz", "be\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht recht von einem Dichter und Soldaten!", "tokens": ["Nicht", "recht", "von", "ei\u00b7nem", "Dich\u00b7ter", "und", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O wi\u00dft es nur, wi\u00dft nur die Wahrheit ganz!", "tokens": ["O", "wi\u00dft", "es", "nur", ",", "wi\u00dft", "nur", "die", "Wahr\u00b7heit", "ganz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zerrissen ist der Freude Blumenkranz,", "tokens": ["Zer\u00b7ris\u00b7sen", "ist", "der", "Freu\u00b7de", "Blu\u00b7men\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und Erde, Luft und Wasser haben sich verschworen,", "tokens": ["Und", "Er\u00b7de", ",", "Luft", "und", "Was\u00b7ser", "ha\u00b7ben", "sich", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Seitdem sie euch, die G\u00fcnstlinge, verloren.", "tokens": ["Seit\u00b7dem", "sie", "euch", ",", "die", "G\u00fcnst\u00b7lin\u00b7ge", ",", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Es hat der Himmel sich mit schwarzem Flor beh\u00e4ngt", "tokens": ["Es", "hat", "der", "Him\u00b7mel", "sich", "mit", "schwar\u00b7zem", "Flor", "be\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und weint in dicken, schweren Tropfen;", "tokens": ["Und", "weint", "in", "di\u00b7cken", ",", "schwe\u00b7ren", "Trop\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So sehr man ihn mit Flehn und Bitten dr\u00e4ngt,", "tokens": ["So", "sehr", "man", "ihn", "mit", "Flehn", "und", "Bit\u00b7ten", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nichts kann die Schleusen seines Zorns verstopfen.", "tokens": ["Nichts", "kann", "die", "Schleu\u00b7sen", "sei\u00b7nes", "Zorns", "ver\u00b7stop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-