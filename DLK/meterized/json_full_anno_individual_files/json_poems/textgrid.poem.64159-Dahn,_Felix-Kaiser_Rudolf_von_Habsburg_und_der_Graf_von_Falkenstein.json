{"textgrid.poem.64159": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Rudolf von Habsburg und der Graf von Falkenstein", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorm Falkenstein, vor'm Falkenstein des Reiches Herold rief:", "tokens": ["Vorm", "Fal\u00b7ken\u00b7stein", ",", "vor'm", "Fal\u00b7ken\u00b7stein", "des", "Rei\u00b7ches", "He\u00b7rold", "rief", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbherr Ruppert, la\u00dft das Rauben sein! Lest Kaiser Rudolfs Brief:", "tokens": ["\u00bb", "herr", "Rup\u00b7pert", ",", "la\u00dft", "das", "Rau\u00b7ben", "sein", "!", "Lest", "Kai\u00b7ser", "Ru\u00b7dolfs", "Brief", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "VVIMP", "ART", "NN", "VAINF", "$.", "NE", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wer Friede bricht im Land und Recht mit Schwertgewalt und Zwang,", "tokens": ["Wer", "Frie\u00b7de", "bricht", "im", "Land", "und", "Recht", "mit", "Schwert\u00b7ge\u00b7walt", "und", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der h\u00e4ngt, sei's Ritter oder Knecht, als R\u00e4uber an dem Strang.\u00ab", "tokens": ["Der", "h\u00e4ngt", ",", "sei's", "Rit\u00b7ter", "o\u00b7der", "Knecht", ",", "als", "R\u00e4u\u00b7ber", "an", "dem", "Strang", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "KOUS", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Da warf vom hohen Falkenstein der Graf ein h\u00e4nfen Seil:", "tokens": ["Da", "warf", "vom", "ho\u00b7hen", "Fal\u00b7ken\u00b7stein", "der", "Graf", "ein", "h\u00e4n\u00b7fen", "Seil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "\u00bbdem kleinen Schweizergr\u00e4felein, dem Kr\u00e4merkaiser Heil!", "tokens": ["\u00bb", "dem", "klei\u00b7nen", "Schwei\u00b7zer\u00b7gr\u00e4\u00b7fe\u00b7lein", ",", "dem", "Kr\u00e4\u00b7mer\u00b7kai\u00b7ser", "Heil", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Er h\u00e4nge mich mit meinem Strang in meinem eignen Tor:", "tokens": ["Er", "h\u00e4n\u00b7ge", "mich", "mit", "mei\u00b7nem", "Strang", "in", "mei\u00b7nem", "eig\u00b7nen", "Tor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Doch bring' den Galgen, stark und lang, er hier heraus zuvor.\u00ab", "tokens": ["Doch", "bring'", "den", "Gal\u00b7gen", ",", "stark", "und", "lang", ",", "er", "hier", "he\u00b7raus", "zu\u00b7vor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "PPER", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Der Herold nahm das Seil und ging. Der Graf schickt Boten aus:", "tokens": ["Der", "He\u00b7rold", "nahm", "das", "Seil", "und", "ging", ".", "Der", "Graf", "schickt", "Bo\u00b7ten", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "\u00bbvogt Geierstein, Graf Drachenring, ich lad' euch in mein Haus.", "tokens": ["\u00bb", "vogt", "Gei\u00b7er\u00b7stein", ",", "Graf", "Dra\u00b7chen\u00b7ring", ",", "ich", "lad'", "euch", "in", "mein", "Haus", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "$,", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Die Etschbr\u00fcck' h\u00e4lt mein Bruder gut, sonst f\u00fchrt kein Pa\u00df herein:", "tokens": ["Die", "Etschbr\u00fcck", "h\u00e4lt", "mein", "Bru\u00b7der", "gut", ",", "sonst", "f\u00fchrt", "kein", "Pa\u00df", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "So lang noch Wasser ers\u00e4ufen tut, ist sicher der Falkenstein.\u00ab", "tokens": ["So", "lang", "noch", "Was\u00b7ser", "er\u00b7s\u00e4u\u00b7fen", "tut", ",", "ist", "si\u00b7cher", "der", "Fal\u00b7ken\u00b7stein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADV", "NN", "VVPP", "VVFIN", "$,", "VAFIN", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Und es zog mit zwanzigtausend Mann der Kaiser landaus, landein:", "tokens": ["Und", "es", "zog", "mit", "zwan\u00b7zig\u00b7tau\u00b7send", "Mann", "der", "Kai\u00b7ser", "lan\u00b7daus", ",", "lan\u00b7de\u00b7in", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "CARD", "NN", "ART", "NN", "PTKVZ", "$,", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Zw\u00f6lf Richter zogen ihm voran, zw\u00f6lf Henker hinterdrein.", "tokens": ["Zw\u00f6lf", "Rich\u00b7ter", "zo\u00b7gen", "ihm", "vo\u00b7ran", ",", "zw\u00f6lf", "Hen\u00b7ker", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Er zog mit Macht durch alles Land: er kam wie Sonnenschein,", "tokens": ["Er", "zog", "mit", "Macht", "durch", "al\u00b7les", "Land", ":", "er", "kam", "wie", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PIAT", "NN", "$.", "PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Und wo er eine Raubburg fand, \u2013 gebrochen mu\u00dfte sie sein;", "tokens": ["Und", "wo", "er", "ei\u00b7ne", "Raub\u00b7burg", "fand", ",", "\u2013", "ge\u00b7bro\u00b7chen", "mu\u00df\u00b7te", "sie", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "$(", "VVINF", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.17": {"text": "Und es segneten Witwen und Waisen ihn, was schwach und schirmlos war", "tokens": ["Und", "es", "seg\u00b7ne\u00b7ten", "Wit\u00b7wen", "und", "Wai\u00b7sen", "ihn", ",", "was", "schwach", "und", "schirm\u00b7los", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "NN", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN"], "meter": "+-+--++-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Und alle Geier mu\u00dften fliehn vor dem kaiserlichen Aar.", "tokens": ["Und", "al\u00b7le", "Gei\u00b7er", "mu\u00df\u00b7ten", "fliehn", "vor", "dem", "kai\u00b7ser\u00b7li\u00b7chen", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "Doch als er kam gen Falkenschlo\u00df, ein Wolkenbruch geschah:", "tokens": ["Doch", "als", "er", "kam", "gen", "Fal\u00b7ken\u00b7schlo\u00df", ",", "ein", "Wol\u00b7ken\u00b7bruch", "ge\u00b7schah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Die Etsch geschwellt wie w\u00fctend scho\u00df, kein Steg war fern und nah.", "tokens": ["Die", "Etsch", "ge\u00b7schwellt", "wie", "w\u00fc\u00b7tend", "scho\u00df", ",", "kein", "Steg", "war", "fern", "und", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "ADJD", "VVFIN", "$,", "PIAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Da hob der Kaiser fromm und rein die H\u00e4nde gen Himmel auf:", "tokens": ["Da", "hob", "der", "Kai\u00b7ser", "fromm", "und", "rein", "die", "H\u00e4n\u00b7de", "gen", "Him\u00b7mel", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "\u00bbla\u00df hemmen nicht dies W\u00e4sserlein, Herr, deines Rechtes Lauf.", "tokens": ["\u00bb", "la\u00df", "hem\u00b7men", "nicht", "dies", "W\u00e4s\u00b7ser\u00b7lein", ",", "Herr", ",", "dei\u00b7nes", "Rech\u00b7tes", "Lauf", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "VVFIN", "PTKNEG", "PDS", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Ich selbst, der ich kann kein Wunder tun, mein Ro\u00df einst schenkt' ich dir: \u2013", "tokens": ["Ich", "selbst", ",", "der", "ich", "kann", "kein", "Wun\u00b7der", "tun", ",", "mein", "Ro\u00df", "einst", "schenkt'", "ich", "dir", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.24": {"text": "Du hilf mir durch dies Wasser nun, wie in der Schweiz ich dir.\u00ab", "tokens": ["Du", "hilf", "mir", "durch", "dies", "Was\u00b7ser", "nun", ",", "wie", "in", "der", "Schweiz", "ich", "dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDS", "NN", "ADV", "$,", "PWAV", "APPR", "ART", "NE", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "Sieh, da kam goldner Sonnenschein und vom Himmel kam Mittagsbrand:", "tokens": ["Sieh", ",", "da", "kam", "gold\u00b7ner", "Son\u00b7nen\u00b7schein", "und", "vom", "Him\u00b7mel", "kam", "Mit\u00b7tags\u00b7brand", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADJA", "NN", "KON", "APPRART", "NN", "VVFIN", "NN", "$."], "meter": "---+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Die wilde Etsch war zahm und klein, eh' der Abend ging ins Land.", "tokens": ["Die", "wil\u00b7de", "Etsch", "war", "zahm", "und", "klein", ",", "eh'", "der", "A\u00b7bend", "ging", "ins", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "Am zweiten Tag durch ihr Bette ging der Kaiser trocken und heil,", "tokens": ["Am", "zwei\u00b7ten", "Tag", "durch", "ihr", "Bet\u00b7te", "ging", "der", "Kai\u00b7ser", "tro\u00b7cken", "und", "heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Am dritten Tag Graf Ruppert hing in seinem eignen Seil:", "tokens": ["Am", "drit\u00b7ten", "Tag", "Graf", "Rup\u00b7pert", "hing", "in", "sei\u00b7nem", "eig\u00b7nen", "Seil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.29": {"text": "In seinem eignen Tor er hing: doch hing er nicht allein:", "tokens": ["In", "sei\u00b7nem", "eig\u00b7nen", "Tor", "er", "hing", ":", "doch", "hing", "er", "nicht", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.30": {"text": "Es hing dabei Graf Drachenring und der Vogt von Geierstein.", "tokens": ["Es", "hing", "da\u00b7bei", "Graf", "Dra\u00b7chen\u00b7ring", "und", "der", "Vogt", "von", "Gei\u00b7er\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "NE", "NE", "KON", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--++-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Vorm Falkenstein, vor'm Falkenstein des Reiches Herold rief:", "tokens": ["Vorm", "Fal\u00b7ken\u00b7stein", ",", "vor'm", "Fal\u00b7ken\u00b7stein", "des", "Rei\u00b7ches", "He\u00b7rold