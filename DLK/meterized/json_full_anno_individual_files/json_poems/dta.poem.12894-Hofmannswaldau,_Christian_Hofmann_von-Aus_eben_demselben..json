{"dta.poem.12894": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus eben demselben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die vergn\u00fcgungen, dich, Allersch\u00f6nstes kind!", "tokens": ["Da\u00df", "die", "ver\u00b7gn\u00fc\u00b7gun\u00b7gen", ",", "dich", ",", "Al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zu h\u00f6ren und zu sehn, gar s\u00fc\u00df und niedlich sind;", "tokens": ["Zu", "h\u00f6\u00b7ren", "und", "zu", "sehn", ",", "gar", "s\u00fc\u00df", "und", "nied\u00b7lich", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df hundert buhler sich durchaus gel\u00fccklich nennen,", "tokens": ["Da\u00df", "hun\u00b7dert", "buh\u00b7ler", "sich", "durc\u00b7haus", "ge\u00b7l\u00fcck\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sie nur Daphnen sehn und Daphnen h\u00f6ren k\u00f6nnen;", "tokens": ["Wenn", "sie", "nur", "Daph\u00b7nen", "sehn", "und", "Daph\u00b7nen", "h\u00f6\u00b7ren", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVINF", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das nimmt Leander zwar nicht ohne beyfall an:", "tokens": ["Das", "nimmt", "Le\u00b7an\u00b7der", "zwar", "nicht", "oh\u00b7ne", "bey\u00b7fall", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "ADV", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat aber hand und mund denn allen werth verlohren?", "tokens": ["Hat", "a\u00b7ber", "hand", "und", "mund", "denn", "al\u00b7len", "werth", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "KON", "PIAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist weiter nichts an mir, was Daphne laben kan,", "tokens": ["Ist", "wei\u00b7ter", "nichts", "an", "mir", ",", "was", "Daph\u00b7ne", "la\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "PPER", "$,", "PWS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als nur der zarte durst der augen und der ohren?", "tokens": ["Als", "nur", "der", "zar\u00b7te", "durst", "der", "au\u00b7gen", "und", "der", "oh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}