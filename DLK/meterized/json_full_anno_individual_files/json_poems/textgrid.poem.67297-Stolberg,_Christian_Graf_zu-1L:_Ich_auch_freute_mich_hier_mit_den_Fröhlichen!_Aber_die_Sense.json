{"textgrid.poem.67297": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich auch freute mich hier mit den Fr\u00f6hlichen! Aber die Sense", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich auch freute mich hier mit den Fr\u00f6hlichen! Aber die Sense", "tokens": ["Ich", "auch", "freu\u00b7te", "mich", "hier", "mit", "den", "Fr\u00f6h\u00b7li\u00b7chen", "!", "A\u00b7ber", "die", "Sen\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$.", "KON", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "M\u00e4hte den J\u00fcngling hinweg, fern an dem s\u00fcdlichem Strand.", "tokens": ["M\u00e4h\u00b7te", "den", "J\u00fcng\u00b7ling", "hin\u00b7weg", ",", "fern", "an", "dem", "s\u00fcd\u00b7li\u00b7chem", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Unverg\u00e4nglich ist Liebe; die bleibt mir. Oft ungesehen", "tokens": ["Un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "ist", "Lie\u00b7be", ";", "die", "bleibt", "mir", ".", "Oft", "un\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "$.", "ART", "VVFIN", "PPER", "$.", "ADV", "ADJA"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Schwebt mein Gebild' in dem Chor treuer Gespielen umher.", "tokens": ["Schwebt", "mein", "Ge\u00b7bild'", "in", "dem", "Chor", "treu\u00b7er", "Ge\u0