{"textgrid.poem.49649": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die kranke Mama", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Europia, die gute Alte,", "tokens": ["Eu\u00b7ro\u00b7pia", ",", "die", "gu\u00b7te", "Al\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Ist neulings nicht mehr bei Humor.", "tokens": ["Ist", "neu\u00b7lings", "nicht", "mehr", "bei", "Hu\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sie zeigt uns eine Kummerfalte", "tokens": ["Sie", "zeigt", "uns", "ei\u00b7ne", "Kum\u00b7mer\u00b7fal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kommt mir so ver\u00e4ndert vor.", "tokens": ["Und", "kommt", "mir", "so", "ver\u00b7\u00e4n\u00b7dert", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst, wenn die Kinder heftig waren,", "tokens": ["Sonst", ",", "wenn", "die", "Kin\u00b7der", "hef\u00b7tig", "wa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwies sie ihnen sanft den Streit.", "tokens": ["Ver\u00b7wies", "sie", "ih\u00b7nen", 