word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die F\u00fcsse ebenfals sind weislich ausersehn,", "tokens": ["Die", "F\u00fcs\u00b7se", "e\u00b7ben\u00b7fals", "sind", "weis\u00b7lich", "au\u00b7ser\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Womit sie auf und ab, an glatten Fl\u00e4chen gehn,", "tokens": ["Wo\u00b7mit", "sie", "auf", "und", "ab", ",", "an", "glat\u00b7ten", "Fl\u00e4\u00b7chen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "KON", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Gelenkigt eingericht; an dem Gelenk der Krallen,", "tokens": ["Ge\u00b7len\u00b7kigt", "ein\u00b7ge\u00b7richt", ";", "an", "dem", "Ge\u00b7lenk", "der", "Kral\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ist ein gedoppelter, ein schwammigt sanffter Ballen.", "tokens": ["Ist", "ein", "ge\u00b7dop\u00b7pel\u00b7ter", ",", "ein", "schwam\u00b7migt", "sanff\u00b7ter", "Bal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.53": {"text": "Die F\u00fcsse haben auch sehr spize N\u00e4gelein,", "tokens": ["Die", "F\u00fcs\u00b7se", "ha\u00b7ben",