ter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.63": {"text": "Die sch\u00fctteln, str\u00e4uben sie, um damit abzukeh-", "tokens": ["Die", "sch\u00fct\u00b7teln", ",", "str\u00e4u\u00b7ben", "sie", ",", "um", "da\u00b7mit", "ab\u00b7zu\u00b7keh"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUI", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Die St\u00e4ubgen welche sonst die Fl\u00fcgel leicht beschwe-", "tokens": ["Die", "St\u00e4ub\u00b7gen", "wel\u00b7che", "sonst", "die", "Fl\u00fc\u00b7gel", "leicht", "be\u00b7schwe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRELS", "ADV", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Sie wischt damit gar offt der Augen Spiegel aus,", "tokens": ["Sie", "wischt", "da\u00b7mit", "gar", "offt", "der", "Au\u00b7gen", "Spie\u00b7gel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Wenn etwa in der Lufft, bei einem Windebraus;", "tokens": ["Wenn", "et\u00b7wa", "in", "der", "Lufft", ",", "bei", "ei\u00b7nem", "Win\u00b7de\u00b7braus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Ein Staub sich drauf gesezt, auf da\u00df sie nicht ver-", "tokens": ["Ein", "Staub", "sich", "drauf", "ge\u00b7sezt", ",", "auf", "da\u00df", "sie", "nicht", "ver"], "token_info": ["word", "word", "word", "word", "word", "p