{"dta.poem.8332": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die k\u00fcnstlichen Fliegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fliegen, welche wir nur mit Verdrus\nansehn,", "tokens": ["Die", "Flie\u00b7gen", ",", "wel\u00b7che", "wir", "nur", "mit", "Ver\u00b7drus", "an\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darauf wir keinen Blik, mit Achtsam-", "tokens": ["Da\u00b7rauf", "wir", "kei\u00b7nen", "Blik", ",", "mit", "Acht\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "PPER", "PIAT", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Als wie wir leider thun. Wir m\u00fcssen sie betrachten,", "tokens": ["Als", "wie", "wir", "lei\u00b7der", "thun", ".", "Wir", "m\u00fcs\u00b7sen", "sie", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "VVINF", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu ihres Sch\u00f6pfers Ruhm. Auf! seht dies Thier-", "tokens": ["Zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ruhm", ".", "Auf", "!", "seht", "dies", "Thier"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "APPR", "$.", "VVFIN", "PDS", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das, ob es noch so klein, uns dennoch zeigen kan,", "tokens": ["Das", ",", "ob", "es", "noch", "so", "klein", ",", "uns", "den\u00b7noch", "zei\u00b7gen", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es sey ein grosser ", "tokens": ["Es", "sey", "ein", "gros\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Als herrlich, wunderbar, als w\u00fcrklich gros erschei-", "tokens": ["Als", "herr\u00b7lich", ",", "wun\u00b7der\u00b7bar", ",", "als", "w\u00fcrk\u00b7lich", "gros", "er\u00b7schei"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "$,", "ADJD", "$,", "KOUS", "ADJD", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Seht durchs Vergr\u00f6\u00dfrungs Glas, o! was vor", "tokens": ["Seht", "durchs", "Ver\u00b7gr\u00f6\u00df\u00b7rungs", "Glas", ",", "o", "!", "was", "vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "NN", "$,", "FM", "$.", "PWS", "APPR"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Ein Schimmerreiches Gold strahlt an dem Kopf", "tokens": ["Ein", "Schim\u00b7mer\u00b7rei\u00b7ches", "Gold", "strahlt", "an", "dem", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Augen die daran als halbe Monden stehen,", "tokens": ["Die", "Au\u00b7gen", "die", "da\u00b7ran", "als", "hal\u00b7be", "Mon\u00b7den", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sind kunstreich angesezt, und werth sie zu besehen.", "tokens": ["Sind", "kuns\u00b7treich", "an\u00b7ge\u00b7sezt", ",", "und", "werth", "sie", "zu", "be\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "KON", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der P\u00fcnctgen kleine Meng, wie Linsen ausges\u00e4t,", "tokens": ["Der", "P\u00fcnct\u00b7gen", "klei\u00b7ne", "Meng", ",", "wie", "Lin\u00b7sen", "aus\u00b7ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NE", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sind zarte Spiegelchen, worin der Lichtstrahl geht,", "tokens": ["Sind", "zar\u00b7te", "Spie\u00b7gel\u00b7chen", ",", "wo\u00b7rin", "der", "Licht\u00b7strahl", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man sieht der Augen Zahl mit innigen Vergn\u00fcgen,", "tokens": ["Man", "sieht", "der", "Au\u00b7gen", "Zahl", "mit", "in\u00b7ni\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "In Reihen ordentlich als an einander liegen.", "tokens": ["In", "Rei\u00b7hen", "or\u00b7dent\u00b7lich", "als", "an", "ein\u00b7an\u00b7der", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KOKOM", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Reihen sind zertheilt und wie ins Kreuz ge-", "tokens": ["Die", "Rei\u00b7hen", "sind", "zer\u00b7theilt", "und", "wie", "ins", "Kreuz", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "PWAV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wie ein Gegitterwerk, das durcheinander geht,", "tokens": ["Wie", "ein", "Ge\u00b7git\u00b7ter\u00b7werk", ",", "das", "durch\u00b7ein\u00b7an\u00b7der", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So viele Augen sind; so viele Spiegelfl\u00e4chen,", "tokens": ["So", "vie\u00b7le", "Au\u00b7gen", "sind", ";", "so", "vie\u00b7le", "Spie\u00b7gel\u00b7fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "$.", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wird man zugleich gewahr, worin die Strahlen", "tokens": ["Wird", "man", "zu\u00b7gleich", "ge\u00b7wahr", ",", "wo\u00b7rin", "die", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Von einem \u00e4usrem Licht, von einem \u00e4usren Schein:", "tokens": ["Von", "ei\u00b7nem", "\u00e4us\u00b7rem", "Licht", ",", "von", "ei\u00b7nem", "\u00e4us\u00b7ren", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und diese Einrichtung kan schon ein Zeuge seyn,", "tokens": ["Und", "die\u00b7se", "Ein\u00b7rich\u00b7tung", "kan", "schon", "ein", "Zeu\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.23": {"text": "Das eine weise Macht, die Fliegen hat gebildet,", "tokens": ["Das", "ei\u00b7ne", "wei\u00b7se", "Macht", ",", "die", "Flie\u00b7gen", "hat", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit einer grossen Meng von Augen \u00fcberg\u00fcldet.", "tokens": ["Mit", "ei\u00b7ner", "gros\u00b7sen", "Meng", "von", "Au\u00b7gen", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bei Thieren andrer Art, woran das Aug sich regt,", "tokens": ["Bei", "Thie\u00b7ren", "an\u00b7drer", "Art", ",", "wo\u00b7ran", "das", "Aug", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Vermehret sich der Strahl, der in dasselbe schl\u00e4gt:", "tokens": ["Ver\u00b7meh\u00b7ret", "sich", "der", "Strahl", ",", "der", "in", "das\u00b7sel\u00b7be", "schl\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "APPR", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allein bei Fliegen nicht, die keine Dinge sehen,", "tokens": ["Al\u00b7lein", "bei", "Flie\u00b7gen", "nicht", ",", "die", "kei\u00b7ne", "Din\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als die vor ihnen sind, gerade \u00fcber stehen.", "tokens": ["Als", "die", "vor", "ih\u00b7nen", "sind", ",", "ge\u00b7ra\u00b7de", "\u00fc\u00b7ber", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPER", "VAFIN", "$,", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum hat die Weisheit auch der Augen Zahl ver-", "tokens": ["Drum", "hat", "die", "Weis\u00b7heit", "auch", "der", "Au\u00b7gen", "Zahl", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Das sie von jeder Seit, was vorgeht, gleich er-", "tokens": ["Das", "sie", "von", "je\u00b7der", "Seit", ",", "was", "vor\u00b7geht", ",", "gleich", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PIAT", "NN", "$,", "PWS", "VVFIN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Die Fliege ist ein Raub von vielen andern Thieren,", "tokens": ["Die", "Flie\u00b7ge", "ist", "ein", "Raub", "von", "vie\u00b7len", "an\u00b7dern", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bei ihrer Augen Meng kan sie nun leichtlich sp\u00fcren,", "tokens": ["Bei", "ih\u00b7rer", "Au\u00b7gen", "Meng", "kan", "sie", "nun", "leicht\u00b7lich", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was hie, was da herfleugt. Die drohende Gefahr", "tokens": ["Was", "hie", ",", "was", "da", "her\u00b7fleugt", ".", "Die", "dro\u00b7hen\u00b7de", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "$,", "PRELS", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Von vorn und hinten zu, wird sie so gleich gewahr,", "tokens": ["Von", "vorn", "und", "hin\u00b7ten", "zu", ",", "wird", "sie", "so", "gleich", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "PTKVZ", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Auch was von oben komt, und was zu beiden Sei-", "tokens": ["Auch", "was", "von", "o\u00b7ben", "komt", ",", "und", "was", "zu", "bei\u00b7den", "Sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "APPR", "ADV", "VVFIN", "$,", "KON", "PWS", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Merkt sie im Gegenschein, und kan dawider streiten.", "tokens": ["Merkt", "sie", "im", "Ge\u00b7gen\u00b7schein", ",", "und", "kan", "da\u00b7wi\u00b7der", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "KON", "VMFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie k\u00fcnstlich ist das nicht vom Sch\u00f6pfer ausge-", "tokens": ["Wie", "k\u00fcnst\u00b7lich", "ist", "das", "nicht", "vom", "Sch\u00f6p\u00b7fer", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PDS", "PTKNEG", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wie weislich ist nur nicht der Fliegen Kopf ge-", "tokens": ["Wie", "weis\u00b7lich", "ist", "nur", "nicht", "der", "Flie\u00b7gen", "Kopf", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Daran man noch bemerkt, da\u00df diese vielen Augen,", "tokens": ["Da\u00b7ran", "man", "noch", "be\u00b7merkt", ",", "da\u00df", "die\u00b7se", "vie\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADV", "VVPP", "$,", "KOUS", "PDAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Bilder nicht verwirrn, die sie zu sehen taugen,", "tokens": ["Die", "Bil\u00b7der", "nicht", "ver\u00b7wirrn", ",", "die", "sie", "zu", "se\u00b7hen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie sehen einerlei mit vielen Augen an,", "tokens": ["Sie", "se\u00b7hen", "ei\u00b7ner\u00b7lei", "mit", "vie\u00b7len", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Doch so da\u00df dieser Schein sie nicht verwirren kan.", "tokens": ["Doch", "so", "da\u00df", "die\u00b7ser", "Schein", "sie", "nicht", "ver\u00b7wir\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PDAT", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die Fl\u00fcgel zeigen uns ein Spiegelglat Gespinste,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "zei\u00b7gen", "uns", "ein", "Spie\u00b7gel\u00b7glat", "Ge\u00b7spins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alex