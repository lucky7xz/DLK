{"textgrid.poem.32617": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heiter str\u00f6mt es nach dem gr\u00fcnen Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiter str\u00f6mt es nach dem gr\u00fcnen Wald,", "tokens": ["Hei\u00b7ter", "str\u00f6mt", "es", "nach", "dem", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Untergehen wird die Sonne bald,", "tokens": ["Un\u00b7ter\u00b7ge\u00b7hen", "wird", "die", "Son\u00b7ne", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und schon sieht man ihre letzten Strahlen", "tokens": ["Und", "schon", "sieht", "man", "ih\u00b7re", "letz\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rote Rosen in die Wipfel malen.", "tokens": ["Ro\u00b7te", "Ro\u00b7sen", "in", "die", "Wip\u00b7fel", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Goldnes Leuchten durch die Zweige bricht,", "tokens": ["Gold\u00b7nes", "Leuch\u00b7ten", "durch", "die", "Zwei\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch die Menge achtet dessen nicht,", "tokens": ["Doch", "die", "Men\u00b7ge", "ach\u00b7tet", "des\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PDS", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Horch, da girren Tauben just und kosen,", "tokens": ["Horch", ",", "da", "gir\u00b7ren", "Tau\u00b7ben", "just", "und", "ko\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADJA", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das ist lustiger, als jene Rosen.", "tokens": ["Das", "ist", "lus\u00b7ti\u00b7ger", ",", "als", "je\u00b7ne", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "KOUS", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, des Waldes gr\u00fcne Herrlichkeit,", "tokens": ["Ach", ",", "des", "Wal\u00b7des", "gr\u00fc\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Seine w\u00fcrz'ge Pracht auch mich erfreut,", "tokens": ["Sei\u00b7ne", "w\u00fcrz'\u00b7ge", "Pracht", "auch", "mich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gern seh' ich der Abendsonne Strahlen", "tokens": ["Gern", "seh'", "ich", "der", "A\u00b7bend\u00b7son\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wilde Rosen in das Laubwerk malen.", "tokens": ["Wil\u00b7de", "Ro\u00b7sen", "in", "das", "Laub\u00b7werk", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Girrten nur die Tauben nicht so sehr!", "tokens": ["Girr\u00b7ten", "nur", "die", "Tau\u00b7ben", "nicht", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Leiden macht's mich immer gar so schwer:", "tokens": ["Lei\u00b7den", "macht's", "mich", "im\u00b7mer", "gar", "so", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seh' ich, wie so gl\u00fccklich sie gemeinsam,", "tokens": ["Seh'", "ich", ",", "wie", "so", "gl\u00fcck\u00b7lich", "sie", "ge\u00b7mein\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mu\u00df ich weinen, da\u00df ich gar so einsam!", "tokens": ["Mu\u00df", "ich", "wei\u00b7nen", ",", "da\u00df", "ich", "gar", "so", "ein\u00b7sam", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}