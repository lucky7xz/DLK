{"dta.poem.321": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der  Bellandra  Abfall schmertzet ihn nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.85", "fr:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was traurest du mein Hertze!", "tokens": ["Was", "trau\u00b7rest", "du", "mein", "Hert\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frisch auf/ verzage nicht!", "tokens": ["Frisch", "auf", "/", "ver\u00b7za\u00b7ge", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$(", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df diese dunckle Kertze/", "tokens": ["La\u00df", "die\u00b7se", "dunck\u00b7le", "Kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und such ein ander Licht.", "tokens": ["Und", "such", "ein", "an\u00b7der", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hast sie zwar geliebet", "tokens": ["Du", "hast", "sie", "zwar", "ge\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch nur zum Zeit-vertreib/", "tokens": ["Doch", "nur", "zum", "Zeit\u00b7ver\u00b7treib", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ists/ das dich betr\u00fcbet?", "tokens": ["Was", "ists", "/", "das", "dich", "be\u00b7tr\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein ungetreues Weib.", "tokens": ["Ein", "un\u00b7ge\u00b7treu\u00b7es", "Weib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was ist das Frauen-Zimmer?", "tokens": ["Was", "ist", "das", "Frau\u00b7en\u00b7Zim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was heist bey ihnen Treu?", "tokens": ["Was", "heist", "bey", "ih\u00b7nen", "Treu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ein falscher Schimmer", "tokens": ["Es", "ist", "ein", "fal\u00b7scher", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Betrug und Heucheley.", "tokens": ["Be\u00b7trug", "und", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer ihren Worten trauet", "tokens": ["Wer", "ih\u00b7ren", "Wor\u00b7ten", "trau\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der wandert bey der Nacht", "tokens": ["Der", "wan\u00b7dert", "bey", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis er das Ungl\u00fcck schauet", "tokens": ["Bis", "er", "das", "Un\u00b7gl\u00fcck", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darinn sie ihn gebracht.", "tokens": ["Da\u00b7rinn", "sie", "ihn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie das Aprillen-Wetter", "tokens": ["Wie", "das", "A\u00b7pril\u00b7len\u00b7Wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Unsiett und st\u00fcrmisch ist", "tokens": ["Un\u00b7siett", "und", "st\u00fcr\u00b7misch", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wie die Rosen-Bl\u00e4tter", "tokens": ["Und", "wie", "die", "Ro\u00b7sen\u00b7Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald die Verwesung k\u00fc\u00dft.", "tokens": ["Bald", "die", "Ver\u00b7we\u00b7sung", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So steht auch ihre Liebe", "tokens": ["So", "steht", "auch", "ih\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf lauter Unbestand/", "tokens": ["Auf", "lau\u00b7ter", "Un\u00b7be\u00b7stand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sind verlogne Diebe", "tokens": ["Sie", "sind", "ver\u00b7log\u00b7ne", "Die\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Betriegen Mund und Hand.", "tokens": ["Be\u00b7trie\u00b7gen", "Mund", "und", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie gleichen den ", "tokens": ["Sie", "glei\u00b7chen", "den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die unser Fall erfreut/", "tokens": ["Die", "un\u00b7ser", "Fall", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will man sich darnach sehnen", "tokens": ["Will", "man", "sich", "dar\u00b7nach", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PRF", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist der Todt nicht weit.", "tokens": ["So", "ist", "der", "Todt", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Weil sie nun solche Leute/", "tokens": ["Weil", "sie", "nun", "sol\u00b7che", "Leu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sch\u00e4tze dich vergn\u00fcgt/", "tokens": ["So", "sch\u00e4t\u00b7ze", "dich", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du zu einer Beute", "tokens": ["Da\u00df", "du", "zu", "ei\u00b7ner", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Tod nicht weg gekriegt.", "tokens": ["Den", "Tod", "nicht", "weg", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du hast sie schon berochen/", "tokens": ["Du", "hast", "sie", "schon", "be\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du weist ja was sie hat/", "tokens": ["Du", "weist", "ja", "was", "sie", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rose ist gebrochen/", "tokens": ["Die", "Ro\u00b7se", "ist", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und du bist ihrer satt.", "tokens": ["Und", "du", "bist", "ih\u00b7rer", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "La\u00df diese falsche Seele", "tokens": ["La\u00df", "die\u00b7se", "fal\u00b7sche", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf! gib ihr gute Nacht/", "tokens": ["Auf", "!", "gib", "ihr", "gu\u00b7te", "Nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flieh ihre Laster-H\u00f6hle", "tokens": ["Flieh", "ih\u00b7re", "Las\u00b7ter\u00b7H\u00f6h\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo dein Verderben wacht.", "tokens": ["Wo", "dein", "Ver\u00b7der\u00b7ben", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So danck ich denn der Liebe", "tokens": ["So", "danck", "ich", "denn", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ihren Plagen ab", "tokens": ["Und", "ih\u00b7ren", "Pla\u00b7gen", "ab"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schwinge meine Triebe", "tokens": ["Und", "schwin\u00b7ge", "mei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.12": {"line.1": {"text": "We\u00f1 Dolch und Degen blincken", "tokens": ["We\u00f1", "Dolch", "und", "De\u00b7gen", "blin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So freut sich meine Brust/", "tokens": ["So", "freut", "sich", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Ro\u00df und Mann versincken", "tokens": ["Wo", "Ro\u00df", "und", "Mann", "ver\u00b7sin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da find\u2019 ich meine Lust.", "tokens": ["Da", "find'", "ich", "mei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Paucken und Trompeten", "tokens": ["Die", "Pau\u00b7cken", "und", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh ich den K\u00fcssen f\u00fcr/", "tokens": ["Zieh", "ich", "den", "K\u00fcs\u00b7sen", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Pistohlen und ", "tokens": ["Pis\u00b7toh\u00b7len", "und"], "token_info": ["word", "word"], "pos": ["NN", "KON"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sind meine beste Zier.", "tokens": ["Sind", "mei\u00b7ne", "bes\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wenn die Carthaunen knallen", "tokens": ["Wenn", "die", "Car\u00b7thau\u00b7nen", "knal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Feur und Pulver kracht", "tokens": ["Wenn", "Feur", "und", "Pul\u00b7ver", "kracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn veste Mauren fallen", "tokens": ["Wenn", "ves\u00b7te", "Mau\u00b7ren", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wenn man Mienen macht.", "tokens": ["Und", "wenn", "man", "Mie\u00b7nen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Denn find ich mein Vergn\u00fcgen", "tokens": ["Denn", "find", "ich", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dem erhitzten Streit/", "tokens": ["In", "dem", "er\u00b7hitz\u00b7ten", "Streit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Fechten und das Kriegen", "tokens": ["Das", "Fech\u00b7ten", "und", "das", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist lauter Fr\u00f6lichkeit.", "tokens": ["Ist", "lau\u00b7ter", "Fr\u00f6\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Nun gute Nacht du Sch\u00f6ne", "tokens": ["Nun", "gu\u00b7te", "Nacht", "du", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch falsche auch dabey/", "tokens": ["Doch", "fal\u00b7sche", "auch", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich liebe das Geth\u00f6ne", "tokens": ["Ich", "lie\u00b7be", "das", "Ge\u00b7th\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}}}}