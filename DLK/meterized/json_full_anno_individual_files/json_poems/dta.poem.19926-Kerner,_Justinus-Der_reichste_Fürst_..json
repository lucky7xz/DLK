{"dta.poem.19926": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der reichste F\u00fcrst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Prei\u00dfend mit viel sch\u00f6nen Reden", "tokens": ["Prei\u00b7\u00dfend", "mit", "viel", "sch\u00f6\u00b7nen", "Re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer L\u00e4nder Werth und Zahl.", "tokens": ["Ih\u00b7rer", "L\u00e4n\u00b7der", "Werth", "und", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen viele deutsche F\u00fcrsten", "tokens": ["Sa\u00b7\u00dfen", "vie\u00b7le", "deut\u00b7sche", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst zu Worms im Kaisersaal.", "tokens": ["Einst", "zu", "Worms", "im", "Kai\u00b7ser\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herrlich, sprach der F\u00fcrst von Sachsen,", "tokens": ["Herr\u00b7lich", ",", "sprach", "der", "F\u00fcrst", "von", "Sach\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein Land und seine Macht,", "tokens": ["Ist", "mein", "Land", "und", "sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Silber hegen seine Berge", "tokens": ["Sil\u00b7ber", "he\u00b7gen", "sei\u00b7ne", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl in manchem tiefen Schacht.", "tokens": ["Wohl", "in", "man\u00b7chem", "tie\u00b7fen", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht mein Land in \u00fcpp'ger F\u00fclle,", "tokens": ["Seht", "mein", "Land", "in", "\u00fcpp'\u00b7ger", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach der Churf\u00fcrst von dem Rhein,", "tokens": ["Sprach", "der", "Chur\u00b7f\u00fcrst", "von", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Goldne Saaten in den Th\u00e4lern,", "tokens": ["Gold\u00b7ne", "Saa\u00b7ten", "in", "den", "Th\u00e4\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Bergen edlen Wein!", "tokens": ["Auf", "den", "Ber\u00b7gen", "ed\u00b7len", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gro\u00dfe St\u00e4dte, reiche Kl\u00f6ster,", "tokens": ["Gro\u00b7\u00dfe", "St\u00e4d\u00b7te", ",", "rei\u00b7che", "Kl\u00f6s\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ludwig Herr zu Bayern sprach,", "tokens": ["Lud\u00b7wig", "Herr", "zu", "Bay\u00b7ern", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffen, da\u00df mein Land den euren", "tokens": ["Schaf\u00b7fen", ",", "da\u00df", "mein", "Land", "den", "eu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl nicht steht an Sch\u00e4tzen nach.", "tokens": ["Wohl", "nicht", "steht", "an", "Sch\u00e4t\u00b7zen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eberhardt, der mit dem Barte,", "tokens": ["E\u00b7ber\u00b7hardt", ",", "der", "mit", "dem", "Bar\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrtembergs geliebter Herr,", "tokens": ["W\u00fcr\u00b7tem\u00b7bergs", "ge\u00b7lieb\u00b7ter", "Herr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach: mein Land hat kleine St\u00e4dte", "tokens": ["Sprach", ":", "mein", "Land", "hat", "klei\u00b7ne", "St\u00e4d\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt nicht Berge silberschwer.", "tokens": ["Tr\u00e4gt", "nicht", "Ber\u00b7ge", "sil\u00b7ber\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da\u00df in W\u00e4ldern noch so gro\u00df", "tokens": ["Da\u00df", "in", "W\u00e4l\u00b7dern", "noch", "so", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mein Haupt kann k\u00fchnlich legen", "tokens": ["Ich", "mein", "Haupt", "kann", "k\u00fchn\u00b7lich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Unterthan' in Schoo\u00df.", "tokens": ["Je\u00b7dem", "Un\u00b7tert\u00b7han'", "in", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und es rief der Herr von Sachsen,", "tokens": ["Und", "es", "rief", "der", "Herr", "von", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der von Bayern, der vom Rhein.", "tokens": ["Der", "von", "Bay\u00b7ern", ",", "der", "vom", "Rhein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "$,", "PRELS", "APPRART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Graf im Bart! ", "tokens": ["Graf", "im", "Bart", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Euer Land tr\u00e4gt ", "tokens": ["Eu\u00b7er", "Land", "tr\u00e4gt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}}}}}