{"textgrid.poem.44300": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Bauer, der sein Kalb verloren hat", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um ein verlornes Kalb zu suchen,", "tokens": ["Um", "ein", "ver\u00b7lor\u00b7nes", "Kalb", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging einst ein Bauer in den Wald,", "tokens": ["Ging", "einst", "ein", "Bau\u00b7er", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bestieg die h\u00f6chste aller Buchen", "tokens": ["Be\u00b7stieg", "die", "h\u00f6chs\u00b7te", "al\u00b7ler", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lauschte rings umher. Doch bald", "tokens": ["Und", "lauschte", "rings", "um\u00b7her", ".", "Doch", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$.", "KON", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Naht ihm sich ein verliebtes P\u00e4rchen", "tokens": ["Naht", "ihm", "sich", "ein", "ver\u00b7lieb\u00b7tes", "P\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und strecket sich ins weiche Gras,", "tokens": ["Und", "stre\u00b7cket"