{"dta.poem.5351": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die unaufh\u00f6rlich uns, wes wir in diesem Leben", "tokens": ["Die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "uns", ",", "wes", "wir", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "$,", "PWS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bed\u00fcrftig seyn, gesch\u00e4ftig ist zu geben.", "tokens": ["Be\u00b7d\u00fcrf\u00b7tig", "seyn", ",", "ge\u00b7sch\u00e4f\u00b7tig", "ist", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch w\u00fcrdiget man alle Wunder-Wercke", "tokens": ["Und", "den\u00b7noch", "w\u00fcr\u00b7di\u00b7get", "man", "al\u00b7le", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht eins so viel, da\u00df man so Kr\u00e4ft\u2019 als Wirckung mercke,", "tokens": ["Nicht", "eins", "so", "viel", ",", "da\u00df", "man", "so", "Kr\u00e4ft'", "als", "Wir\u00b7ckung", "mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "ADV", "ADV", "$,", "KOUS", "PIS", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df man an den, der uns die\u00df alles schenckt,", "tokens": ["Da\u00df", "man", "an", "den", ",", "der", "uns", "die\u00df", "al\u00b7les", "schenckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "$,", "PRELS", "PPER", "PDS", "PIS", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Durch Nutz und Lust ger\u00fchrt, nicht einst gedenckt.", "tokens": ["Durch", "Nutz", "und", "Lust", "ge\u00b7r\u00fchrt", ",", "nicht", "einst", "ge\u00b7denckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist wol, f\u00fcr unsrer Seelen Kr\u00e4ffte,", "tokens": ["Ist", "wol", ",", "f\u00fcr", "uns\u00b7rer", "See\u00b7len", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein n\u00fctz- und n\u00f6thiger Gesch\u00e4ffte,", "tokens": ["Ein", "n\u00fctz", "und", "n\u00f6\u00b7thi\u00b7ger", "Ge\u00b7sch\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als GOtt, f\u00fcr so viel Guts, ein fr\u00f6lich Hertz zu g\u00f6nnen?", "tokens": ["Als", "Gott", ",", "f\u00fcr", "so", "viel", "Guts", ",", "ein", "fr\u00f6\u00b7lich", "Hertz", "zu", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "$,", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auf welche Weise wird man sonst, f\u00fcr so viel Gaben,", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "wird", "man", "sonst", ",", "f\u00fcr", "so", "viel", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PIS", "ADV", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die wir aus lauter Gnad\u2019 und Huld empfangen haben,", "tokens": ["Die", "wir", "aus", "lau\u00b7ter", "Gnad'", "und", "Huld", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Undancks Vorwurfs sich entsch\u00fctten k\u00f6nnen!", "tokens": ["Des", "Un\u00b7dancks", "Vor\u00b7wurfs", "sich", "ent\u00b7sch\u00fct\u00b7ten", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}