{"textgrid.poem.56378": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchle mehr und mehr die Kr\u00e4fte schwinden;", "tokens": ["Ich", "f\u00fch\u00b7le", "mehr", "und", "mehr", "die", "Kr\u00e4f\u00b7te", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der Tod, der mir am Herzen nagt,", "tokens": ["Das", "ist", "der", "Tod", ",", "der", "mir", "am", "Her\u00b7zen", "nagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df es schon und, was ihr immer sagt,", "tokens": ["Ich", "wei\u00df", "es", "schon", "und", ",", "was", "ihr", "im\u00b7mer", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr werdet mir die Augen nicht verbinden.", "tokens": ["Ihr", "wer\u00b7det", "mir", "die", "Au\u00b7gen", "nicht", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich werde m\u00fcd und m\u00fcder so mich winden,", "tokens": ["Ich", "wer\u00b7de", "m\u00fcd", "und", "m\u00fc\u00b7der", "so", "mich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis endlich der verh\u00e4ngte Morgen tagt,", "tokens": ["Bis", "end\u00b7lich", "der", "ver\u00b7h\u00e4ng\u00b7te", "Mor\u00b7gen", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann sinkt der Abend und, wer nach mir fragt,", "tokens": ["Dann", "sinkt", "der", "A\u00b7bend", "und", ",", "wer", "nach", "mir", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "$,", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der wird nur einen stillen Mann noch finden.", "tokens": ["Der", "wird", "nur", "ei\u00b7nen", "stil\u00b7len", "Mann", "noch", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df so vom Tod ich sprechen mag und Sterben,", "tokens": ["Da\u00df", "so", "vom", "Tod", "ich", "spre\u00b7chen", "mag", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch sich meine Wangen nicht entf\u00e4rben,", "tokens": ["Und", "doch", "sich", "mei\u00b7ne", "Wan\u00b7gen", "nicht", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es d\u00fcnkt euch mutig, \u00fcbermutig fast.", "tokens": ["Es", "d\u00fcnkt", "euch", "mu\u00b7tig", ",", "\u00fc\u00b7ber\u00b7mu\u00b7tig", "fast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Tod! \u2013 der Tod? Das Wort erschreckt mich nicht,", "tokens": ["Der", "Tod", "!", "\u2013", "der", "Tod", "?", "Das", "Wort", "er\u00b7schreckt", "mich", "nicht", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch hab ich im Gem\u00fct ihn nicht erfa\u00dft,", "tokens": ["Doch", "hab", "ich", "im", "Ge\u00b7m\u00fct", "ihn", "nicht", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und noch ihm nicht geschaut ins Angesicht.", "tokens": ["Und", "noch", "ihm", "nicht", "ge\u00b7schaut", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PTKNEG", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich f\u00fchle mehr und mehr die Kr\u00e4fte schwinden;", "tokens": ["Ich