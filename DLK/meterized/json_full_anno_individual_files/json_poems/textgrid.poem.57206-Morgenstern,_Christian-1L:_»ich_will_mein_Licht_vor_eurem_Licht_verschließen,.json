{"textgrid.poem.57206": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbich will mein Licht vor eurem Licht verschlie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich will mein Licht vor eurem Licht verschlie\u00dfen,", "tokens": ["\u00bb", "ich", "will", "mein", "Licht", "vor", "eu\u00b7rem", "Licht", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich will euch nicht, ihr sollt mich nicht genie\u00dfen,", "tokens": ["ich", "will", "euch", "nicht", ",", "ihr", "sollt", "mich", "nicht", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bevor ich nicht ein Eigenlicht geworden.", "tokens": ["be\u00b7vor", "ich", "nicht", "ein", "Ei\u00b7gen\u00b7licht", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So bring ich wohl das B\u00f6se zur Erscheinung,", "tokens": ["So", "bring", "ich", "wohl", "das", "B\u00f6\u00b7se", "zur", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als Geist der Sonderheit und der Verneinung,", "tokens": ["als", "Geist", "der", "Son\u00b7der\u00b7heit", "und", "der", "Ver\u00b7nei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "doch neue Welt erschafft mein Geisterorden.", "tokens": ["doch", "neu\u00b7e", "Welt", "er\u00b7schafft", "mein", "Geis\u00b7ter\u00b7or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Aus Widerspruch zum unbeirrten Wesen,", "tokens": ["Aus", "Wi\u00b7der\u00b7spruch", "zum", "un\u00b7be\u00b7irr\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus Irr-tum soll ein G\u00f6tterstamm genesen,", "tokens": ["aus", "Irr\u00b7tum", "soll", "ein", "G\u00f6t\u00b7ter\u00b7stamm", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der sich aus sich \u2013 und nicht aus euch \u2013 entscheidet.", "tokens": ["der", "sich", "aus", "sich", "\u2013", "und", "nicht", "aus", "euch", "\u2013", "ent\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PRF", "$(", "KON", "PTKNEG", "APPR", "PPER", "$(", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Der nicht von Anbeginn in Wahrheit wand