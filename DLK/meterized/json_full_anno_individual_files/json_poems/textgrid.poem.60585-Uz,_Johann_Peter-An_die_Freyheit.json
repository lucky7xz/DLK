{"textgrid.poem.60585": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die Freyheit", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die den nackten Wilden", "tokens": ["Du", ",", "die", "den", "nack\u00b7ten", "Wil\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In W\u00e4ldern gl\u00fccklich macht,", "tokens": ["In", "W\u00e4l\u00b7dern", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unter k\u00f6niglicher Pracht", "tokens": ["Und", "un\u00b7ter", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch in Britanniens Gefilden", "tokens": ["Noch", "in", "Bri\u00b7tan\u00b7ni\u00b7ens", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Vom g\u00fcldnen Thron gebeut,", "tokens": ["Vom", "g\u00fcld\u00b7nen", "Thron", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im Schoo\u00dfe stolzer Sicherheit:", "tokens": ["Im", "Schoo\u00b7\u00dfe", "stol\u00b7zer", "Si\u00b7cher\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Mutter wahrer Freuden,", "tokens": ["Du", "Mut\u00b7ter", "wah\u00b7rer", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht blo\u00df im Ueberflu\u00df,", "tokens": ["Nicht", "blo\u00df", "im", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Freyheit, unter deren Fu\u00df", "tokens": ["O", "Frey\u00b7heit", ",", "un\u00b7ter", "de\u00b7ren", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Felsen und verbrannte Heiden", "tokens": ["Auch", "Fel\u00b7sen", "und", "ver\u00b7brann\u00b7te", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von ungewohntem Gr\u00fcn", "tokens": ["Von", "un\u00b7ge\u00b7wohn\u00b7tem", "Gr\u00fcn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und tausend Bluhmen duftend bl\u00fchn!", "tokens": ["Und", "tau\u00b7send", "Bluh\u00b7men", "duf\u00b7tend", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erstaunte V\u00f6lker melden", "tokens": ["Er\u00b7staun\u00b7te", "V\u00f6l\u00b7ker", "mel\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Wunder deiner Hand:", "tokens": ["Die", "Wun\u00b7der", "dei\u00b7ner", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du schm\u00fcckest ein geliebtes Land", "tokens": ["Du", "schm\u00fc\u00b7ckest", "ein", "ge\u00b7lieb\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Patrioten, Weisen, Helden:", "tokens": ["Mit", "Pat\u00b7ri\u00b7o\u00b7ten", ",", "Wei\u00b7sen", ",", "Hel\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Derselben Arm und Rath", "tokens": ["Der\u00b7sel\u00b7ben", "Arm", "und", "Rath"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sind ehrne Mauern um den Staat.", "tokens": ["Sind", "ehr\u00b7ne", "Mau\u00b7ern", "um", "den", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Beseelt von deinem Feuer,", "tokens": ["Be\u00b7seelt", "von", "dei\u00b7nem", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denkt jeder B\u00fcrger gro\u00df.", "tokens": ["Denkt", "je\u00b7der", "B\u00fcr\u00b7ger", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Muse flieht in deinen Schoo\u00df,", "tokens": ["Die", "Mu\u00b7se", "flieht", "in", "dei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre hochgestimmte Leyer", "tokens": ["Und", "ih\u00b7re", "hoch\u00b7ges\u00b7timm\u00b7te", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "T\u00f6nt g\u00f6ttlichen Gesang,", "tokens": ["T\u00f6nt", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie sonst am Tieberstrom erklang.", "tokens": ["Wie", "sonst", "am", "Tie\u00b7bers\u00b7trom", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch tr\u00e4g, in dunkler H\u00f6hle,", "tokens": ["Doch", "tr\u00e4g", ",", "in", "dunk\u00b7ler", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Liegt feige Sklaverey:", "tokens": ["Liegt", "fei\u00b7ge", "Skla\u00b7ve\u00b7rey", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie l\u00e4hmt im Joch der Tyranney", "tokens": ["Sie", "l\u00e4hmt", "im", "Joch", "der", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die k\u00fchnen Schwingen unsrer Seele:", "tokens": ["Die", "k\u00fch\u00b7nen", "Schwin\u00b7gen", "uns\u00b7rer", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie wischt erhabne Lust", "tokens": ["Sie", "wischt", "er\u00b7hab\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zum wahren Ruhm aus unsrer Brust.", "tokens": ["Zum", "wah\u00b7ren", "Ruhm", "aus", "uns\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie hat des Menschen Leben", "tokens": ["Sie", "hat", "des", "Men\u00b7schen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und was ihm heilig hei\u00dft,", "tokens": ["Und", "was", "ihm", "hei\u00b7lig", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seinen freygebohrnen Geist", "tokens": ["Und", "sei\u00b7nen", "frey\u00b7ge\u00b7bohr\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der frechen Willk\u00fchr preis gegeben,", "tokens": ["Der", "fre\u00b7chen", "Will\u00b7k\u00fchr", "preis", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die unser Blut vergie\u00dft,", "tokens": ["Die", "un\u00b7ser", "Blut", "ver\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie Wasser, das am Wege flie\u00dft.", "tokens": ["Wie", "Was\u00b7ser", ",", "das", "am", "We\u00b7ge", "flie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gieb, G\u00f6ttinn, deinen Freunden,", "tokens": ["Gieb", ",", "G\u00f6t\u00b7tinn", ",", "dei\u00b7nen", "Freun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Alemannen Muth!", "tokens": ["Den", "A\u00b7le\u00b7man\u00b7nen", "Muth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie? Eigennutz und blinde Wuth", "tokens": ["Wie", "?", "Ei\u00b7gen\u00b7nutz", "und", "blin\u00b7de", "Wuth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verrathen uns verschmitzten Feinden?", "tokens": ["Ver\u00b7ra\u00b7then", "uns", "ver\u00b7schmitz\u00b7ten", "Fein\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Spricht uns ein Fremder schon", "tokens": ["Spricht", "uns", "ein", "Frem\u00b7der", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In unsern festen St\u00e4dten Hohn?", "tokens": ["In", "un\u00b7sern", "fes\u00b7ten", "St\u00e4d\u00b7ten", "Hohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ist Leiden, nicht, sich R\u00e4chen,", "tokens": ["Ist", "Lei\u00b7den", ",", "nicht", ",", "sich", "R\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PTKNEG", "$,", "PRF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun freyer Deutschen Pflicht?", "tokens": ["Nun", "frey\u00b7er", "Deut\u00b7schen", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie wird von unsrer Schande nicht", "tokens": ["Wie", "wird", "von", "uns\u00b7rer", "Schan\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nachwelt einst err\u00f6thend sprechen", "tokens": ["Die", "Nach\u00b7welt", "einst", "er\u00b7r\u00f6\u00b7thend", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und z\u00fcrnen, wann sie h\u00f6rt,", "tokens": ["Und", "z\u00fcr\u00b7nen", ",", "wann", "sie", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Deutschland seine Feinde n\u00e4hrt:", "tokens": ["Da\u00df", "Deutschland", "sei\u00b7ne", "Fein\u00b7de", "n\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Wo seine F\u00fcrsten wohnten,", "tokens": ["Wo", "sei\u00b7ne", "F\u00fcrs\u00b7ten", "wohn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun einsam Elend ist,", "tokens": ["Nun", "ein\u00b7sam", "E\u00b7lend", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und r\u00e4uberische Flamme fri\u00dft,", "tokens": ["Und", "r\u00e4u\u00b7be\u00b7ri\u00b7sche", "Flam\u00b7me", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Geiz und Pl\u00fcnderung verschonten,", "tokens": ["Was", "Geiz", "und", "Pl\u00fcn\u00b7de\u00b7rung", "ver\u00b7schon\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis Deutschland keine Stadt,", "tokens": ["Bis", "Deutschland", "kei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Nur seiner St\u00e4dte Leichen hat!", "tokens": ["Nur", "sei\u00b7ner", "St\u00e4d\u00b7te", "Lei\u00b7chen", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So tief sind wir gesunken!", "tokens": ["So", "tief", "sind", "wir", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer diese Frevel sieht,", "tokens": ["Wer", "die\u00b7se", "Fre\u00b7vel", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nicht von edlem Unmuth gl\u00fcht,", "tokens": ["Und", "nicht", "von", "ed\u00b7lem", "Un\u00b7muth", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat der an deutscher Brust getrunken?", "tokens": ["Hat", "der", "an", "deut\u00b7scher", "Brust", "ge\u00b7trun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit nahem Joch bedroht,", "tokens": ["Mit", "na\u00b7hem", "Joch", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Scheut ein Germanier den Tod?", "tokens": ["Scheut", "ein", "Ger\u00b7ma\u00b7nier", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.11": {"line.1": {"text": "Du, die den nackten Wilden", "tokens": ["Du", ",", "die", "den", "nack\u00b7ten", "Wil\u00b7den"], "token_info": ["word", "punct", "word", "word",