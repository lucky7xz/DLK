{"dta.poem.33": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Anreden an GOtt uns seine Ei-  \n genschaften auf dem Feldern se-  \n hen zu lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsrer Felder reiffe Bl\u00fcte,", "tokens": ["Uns\u00b7rer", "Fel\u00b7der", "reif\u00b7fe", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeuget ", "tokens": ["Zeu\u00b7get"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Womit du die Menschen n\u00e4hrst:", "tokens": ["Wo\u00b7mit", "du", "die", "Men\u00b7schen", "n\u00e4hrst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! la\u00df uns bey diesem Seegen,", "tokens": ["Ach", "!", "la\u00df", "uns", "bey", "die\u00b7sem", "See\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den du giebest wol erwegen,", "tokens": ["Den", "du", "gie\u00b7best", "wol", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie du uns auch dadurch lehrst,", "tokens": ["Wie", "du", "uns", "auch", "da\u00b7durch", "lehrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns zum Guten zu erwekken,", "tokens": ["Uns", "zum", "Gu\u00b7ten", "zu", "er\u00b7wek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da wir deine G\u00fcte schmekken.", "tokens": ["Da", "wir", "dei\u00b7ne", "G\u00fc\u00b7te", "schmek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott du kanst uns in den Aehren,", "tokens": ["Gott", "du", "kanst", "uns", "in", "den", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die wir eingesammlet lehren,", "tokens": ["Die", "wir", "ein\u00b7ge\u00b7samm\u00b7let", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine grosse Wundermacht;", "tokens": ["Dei\u00b7ne", "gros\u00b7se", "Wun\u00b7der\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gib da\u00df wir stets mit Bedacht", "tokens": ["Gib", "da\u00df", "wir", "stets", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An den Fr\u00fcchten die wir essen", "tokens": ["An", "den", "Fr\u00fcch\u00b7ten", "die", "wir", "es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Andachtsvoll die Gr\u00f6\u00df ermessen,", "tokens": ["An\u00b7dachts\u00b7voll", "die", "Gr\u00f6\u00df", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mit Seegen unser Feld,", "tokens": ["Die", "mit", "See\u00b7gen", "un\u00b7ser", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dadurch auch uns erh\u00e4lt.", "tokens": ["Und", "da\u00b7durch", "auch", "uns", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weiser GOtt! wie wunderbar,", "tokens": ["Wei\u00b7ser", "Gott", "!", "wie", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebest du uns alle Jahr,", "tokens": ["Gie\u00b7best", "du", "uns", "al\u00b7le", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Nahrung aus der Erden,", "tokens": ["Uns\u00b7re", "Nah\u00b7rung", "aus", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da, wenn wir den Saamen streun,", "tokens": ["Da", ",", "wenn", "wir", "den", "Saa\u00b7men", "streun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unter himmlischen Gedein,", "tokens": ["Un\u00b7ter", "himm\u00b7li\u00b7schen", "Ge\u00b7dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Felder fruchtbar werden;", "tokens": ["Uns\u00b7re", "Fel\u00b7der", "frucht\u00b7bar", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df uns deine Weisheit sehn,", "tokens": ["La\u00df", "uns", "dei\u00b7ne", "Weis\u00b7heit", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "An den Fr\u00fcchten die entstehn,", "tokens": ["An", "den", "Fr\u00fcch\u00b7ten", "die", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Damit wir an deinen Gaben,", "tokens": ["Da\u00b7mit", "wir", "an", "dei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "M\u00f6gen Leib und Seele laben.", "tokens": ["M\u00f6\u00b7gen", "Leib", "und", "See\u00b7le", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gerechter GOtt! mit deiner Ruthe,", "tokens": ["Ge\u00b7rech\u00b7ter", "Gott", "!", "mit", "dei\u00b7ner", "Ru\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "St\u00e4upest du das Akkerfeld,", "tokens": ["St\u00e4u\u00b7pest", "du", "das", "Ak\u00b7ker\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Damit die verfluchte Welt,", "tokens": ["Da\u00b7mit", "die", "ver\u00b7fluch\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Jhrer S\u00fcnden wegen blute:", "tokens": ["Ih\u00b7rer", "S\u00fcn\u00b7den", "we\u00b7gen", "blu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df uns HErr! doch nicht erfahren,", "tokens": ["La\u00df", "uns", "Herr", "!", "doch", "nicht", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$.", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Strafgerechtigkeit,", "tokens": ["Dei\u00b7ne", "Straf\u00b7ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie in den vergangnen Jahren,", "tokens": ["Wie", "in", "den", "ver\u00b7gang\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern gib uns Gnadenzeit.", "tokens": ["Son\u00b7dern", "gib", "uns", "Gna\u00b7den\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wunder GOtt! du hast versprochen,", "tokens": ["Wun\u00b7der", "Gott", "!", "du", "hast", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00e4hrlich eine Erndte-Zeit;", "tokens": ["J\u00e4hr\u00b7lich", "ei\u00b7ne", "Ernd\u00b7te\u00b7Zeit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das hast du noch nie gebrochen,", "tokens": ["Das", "hast", "du", "noch", "nie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "La\u00df uns die Warhaftigkeit,", "tokens": ["La\u00df", "uns", "die", "War\u00b7haf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Worts an deinen Werken,", "tokens": ["Dei\u00b7nes", "Worts", "an", "dei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Felde wol bemerken:", "tokens": ["Auf", "dem", "Fel\u00b7de", "wol", "be\u00b7mer\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Damit uns dies alle Jahr,", "tokens": ["Da\u00b7mit", "uns", "dies", "al\u00b7le", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mache deine Warheit klar.", "tokens": ["Ma\u00b7che", "dei\u00b7ne", "War\u00b7heit", "klar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6pfer! deine Vorsehung,", "tokens": ["Sch\u00f6p\u00b7fer", "!", "dei\u00b7ne", "Vor\u00b7se\u00b7hung", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan man mit Bewunderung,", "tokens": ["Kan", "man", "mit", "Be\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den wunderbaren Werken,", "tokens": ["An", "den", "wun\u00b7der\u00b7ba\u00b7ren", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Sch\u00f6pfung stets bemerken:", "tokens": ["Dei\u00b7ner", "Sch\u00f6p\u00b7fung", "stets", "be\u00b7mer\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeig uns auch auf unsrer Flur,", "tokens": ["Zeig", "uns", "auch", "auf", "uns\u00b7rer", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhre Andachtsvolle Spur,", "tokens": ["Ih\u00b7re", "An\u00b7dachts\u00b7vol\u00b7le", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da dein Gnaden-volles Walten,", "tokens": ["Da", "dein", "Gna\u00b7den\u00b7vol\u00b7les", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns darauf die Frucht erhalten.", "tokens": ["Uns", "da\u00b7rauf", "die", "Frucht", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}