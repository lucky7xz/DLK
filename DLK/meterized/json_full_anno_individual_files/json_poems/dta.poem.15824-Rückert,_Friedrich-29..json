{"dta.poem.15824": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte wissen, wo der Freund zur Stunde weilt,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wis\u00b7sen", ",", "wo", "der", "Freund", "zur", "Stun\u00b7de", "weilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach welchem in die Welt hinaus mein Denken eilt.", "tokens": ["Nach", "wel\u00b7chem", "in", "die", "Welt", "hin\u00b7aus", "mein", "Den\u00b7ken", "eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "APZR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem unst\u00e4t schweifenden hats unst\u00e4t nachzuschweifen,", "tokens": ["Dem", "un\u00b7st\u00e4t", "schwei\u00b7fen\u00b7den", "hats", "un\u00b7st\u00e4t", "nach\u00b7zu\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wei\u00df die St\u00e4tte nicht, wo es ihn soll ergreifen.", "tokens": ["Und", "wei\u00df", "die", "St\u00e4t\u00b7te", "nicht", ",", "wo", "es", "ihn", "soll", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn auf der L\u00e4nderkart' ich s\u00e4he nur den Ort;", "tokens": ["Wenn", "auf", "der", "L\u00e4n\u00b7der\u00b7kart'", "ich", "s\u00e4\u00b7he", "nur", "den", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist er, spr\u00e4ch' ich, jetzt! und w\u00e4r' im Geiste dort.", "tokens": ["Da", "ist", "er", ",", "spr\u00e4ch'", "ich", ",", "jetzt", "!", "und", "w\u00e4r'", "im", "Geis\u00b7te", "dort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "$.", "KON", "VAFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}