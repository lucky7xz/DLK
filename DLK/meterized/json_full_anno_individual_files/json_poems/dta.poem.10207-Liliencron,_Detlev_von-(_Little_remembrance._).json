{"dta.poem.10207": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Little remembrance. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Z\u00e4hne aufeinander, weit die Augen,", "tokens": ["Die", "Z\u00e4h\u00b7ne", "auf\u00b7ein\u00b7an\u00b7der", ",", "weit", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willst du das Ungeheuer \u201eLeben\u201c binden.", "tokens": ["Willst", "du", "das", "Un\u00b7ge\u00b7heu\u00b7er", "\u201e", "Le\u00b7ben", "\u201c", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gilt! Nimm Waffen, die zum Kampfe taugen,", "tokens": ["Es", "gilt", "!", "Nimm", "Waf\u00b7fen", ",", "die", "zum", "Kamp\u00b7fe", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schlaffes Volk, das gleich sich giebt den Winden.", "tokens": ["Ein", "schlaf\u00b7fes", "Volk", ",", "das", "gleich", "sich", "giebt", "den", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Voran denn! Bade dich in scharfen Laugen,", "tokens": ["Vo\u00b7ran", "denn", "!", "Ba\u00b7de", "dich", "in", "schar\u00b7fen", "Lau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nage, mu\u00df es sein, an harten Rinden.", "tokens": ["Und", "na\u00b7ge", ",", "mu\u00df", "es", "sein", ",", "an", "har\u00b7ten", "Rin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VMFIN", "PPER", "VAINF", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Geduld! Am Ende wirst du Honig saugen,", "tokens": ["Ge\u00b7duld", "!", "Am", "En\u00b7de", "wirst", "du", "Ho\u00b7nig", "sau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wohnen unter selbstgepflanzten Linden.", "tokens": ["Und", "woh\u00b7nen", "un\u00b7ter", "selbst\u00b7ge\u00b7pflanz\u00b7ten", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}