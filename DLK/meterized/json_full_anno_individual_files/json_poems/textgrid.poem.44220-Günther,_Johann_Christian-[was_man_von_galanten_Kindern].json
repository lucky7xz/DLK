{"textgrid.poem.44220": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[was man von galanten Kindern]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was man von galanten Kindern,", "tokens": ["Was", "man", "von", "ga\u00b7lan\u00b7ten", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verg\u00e4lltem Munde spricht,", "tokens": ["Mit", "ver\u00b7g\u00e4ll\u00b7tem", "Mun\u00b7de", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan die Sehnsucht nicht verhindern,", "tokens": ["Kan", "die", "Sehn\u00b7sucht", "nicht", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Werth ins Auge sticht,", "tokens": ["Die", "der", "Werth", "ins", "Au\u00b7ge", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie dir bey stiller Ruh", "tokens": ["Da\u00df", "sie", "dir", "bey", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein geheimes Opfer thu.", "tokens": ["Ein", "ge\u00b7hei\u00b7mes", "Op\u00b7fer", "thu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als ich dich in unsern Ga\u00dfen", "tokens": ["Als", "ich", "dich", "in", "un\u00b7sern", "Ga\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur verstohlen angeblickt,", "tokens": ["Nur", "ver\u00b7stoh\u00b7len", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fing ich alles an zu ha\u00dfen,", "tokens": ["Fing", "ich", "al\u00b7les", "an", "zu", "ha\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sich hier mit Ehrgeiz schm\u00fcckt;", "tokens": ["Was", "sich", "hier", "mit", "Ehr\u00b7geiz", "schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Verwundrung nahm mich ein,", "tokens": ["Die", "Ver\u00b7wund\u00b7rung", "nahm", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir ein stilles Lied zu weihn.", "tokens": ["Dir", "ein", "stil\u00b7les", "Lied", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir gefiel dein freyes Wesen,", "tokens": ["Mir", "ge\u00b7fiel", "dein", "frey\u00b7es", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches Blick und Gang bewies,", "tokens": ["Wel\u00b7ches", "Blick", "und", "Gang", "be\u00b7wies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich w\u00fcntschte dem den Beesen,", "tokens": ["Und", "ich", "w\u00fcnt\u00b7schte", "dem", "den", "Bee\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der es neulich Frechheit hies,", "tokens": ["Der", "es", "neu\u00b7lich", "Frech\u00b7heit", "hies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach der Thorheit unsrer Stadt,", "tokens": ["Nach", "der", "Thor\u00b7heit", "uns\u00b7rer", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die viel falsche Meinung hat.", "tokens": ["Die", "viel", "fal\u00b7sche", "Mei\u00b7nung", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tadler- und Verleumdungsme\u00dfer", "tokens": ["Tad\u00b7ler", "und", "Ver\u00b7leum\u00b7dungs\u00b7me\u00b7\u00dfer"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Biegen wie geschlifnes Bley,", "tokens": ["Bie\u00b7gen", "wie", "ge\u00b7schlif\u00b7nes", "Bley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint dein Bild doch sch\u00f6n und be\u00dfer", "tokens": ["Scheint", "dein", "Bild", "doch", "sch\u00f6n", "und", "be\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als das neidische Geschrey.", "tokens": ["Als", "das", "nei\u00b7di\u00b7sche", "Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer dich sieht und das nicht glaubt,", "tokens": ["Wer", "dich", "sieht", "und", "das", "nicht", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist wohl des Geschmacks beraubt.", "tokens": ["Ist", "wohl", "des", "Ge\u00b7schmacks", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wendung, Gang, Person und Lachen", "tokens": ["Wen\u00b7dung", ",", "Gang", ",", "Per\u00b7son", "und", "La\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dfen mich zum \u00dcberflu\u00df", "tokens": ["La\u00b7\u00dfen", "mich", "zum", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey mir selbst die Rechnung machen,", "tokens": ["Bey", "mir", "selbst", "die", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was wohl der genie\u00dfen mu\u00df,", "tokens": ["Was", "wohl", "der", "ge\u00b7nie\u00b7\u00dfen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den der Stern vergn\u00fcgter Nacht", "tokens": ["Den", "der", "Stern", "ver\u00b7gn\u00fcg\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Umgangs w\u00fcrdig macht.", "tokens": ["Dei\u00b7nes", "Um\u00b7gangs", "w\u00fcr\u00b7dig", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf dergleichen Marmor gleiten,", "tokens": ["Auf", "derg\u00b7lei\u00b7chen", "Mar\u00b7mor", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den man aus dem Busen gr\u00e4bt,", "tokens": ["Den", "man", "aus", "dem", "Bu\u00b7sen", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ein Fall von gro\u00dfen Leuten,", "tokens": ["Ist", "ein", "Fall", "von", "gro\u00b7\u00dfen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ins Paradies erhebt;", "tokens": ["Der", "ins", "Pa\u00b7ra\u00b7dies", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gift aus feuervoller Hand", "tokens": ["Gift", "aus", "feu\u00b7er\u00b7vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird ein s\u00fc\u00dfer Tod genand.", "tokens": ["Wird", "ein", "s\u00fc\u00b7\u00dfer", "Tod", "ge\u00b7nand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie z\u00e4rtlich mag sichs k\u00fc\u00dfen,", "tokens": ["O", "wie", "z\u00e4rt\u00b7lich", "mag", "sichs", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man deine Zunge f\u00fchlt", "tokens": ["Wenn", "man", "dei\u00b7ne", "Zun\u00b7ge", "f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Scherz mit sanften Bi\u00dfen", "tokens": ["Und", "ihr", "Scherz", "mit", "sanf\u00b7ten", "Bi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die hei\u00dfe Lippen spielt;", "tokens": ["Um", "die", "hei\u00b7\u00dfe", "Lip\u00b7pen", "spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So ein k\u00fczlich Aus und Ein", "tokens": ["So", "ein", "k\u00fcz\u00b7lich", "Aus", "und", "Ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "APPR", "KON", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag des Himmels Vorschmack seyn.", "tokens": ["Mag", "des", "Him\u00b7mels", "Vor\u00b7schmack", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "W\u00e4r auch zehnmahl deine Liebe", "tokens": ["W\u00e4r", "auch", "zehn\u00b7mahl", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein vor mich verbothner Baum,", "tokens": ["Ein", "vor", "mich", "ver\u00b7both\u00b7ner", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4b ich doch dem starcken Triebe", "tokens": ["G\u00e4b", "ich", "doch", "dem", "star\u00b7cken", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solcher s\u00fc\u00dfen S\u00fcnden Raum,", "tokens": ["Sol\u00b7cher", "s\u00fc\u00b7\u00dfen", "S\u00fcn\u00b7den", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die Schuld, so es verlezt,", "tokens": ["Weil", "die", "Schuld", ",", "so", "es", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Strafen Qual ersezt.", "tokens": ["Al\u00b7ler", "Stra\u00b7fen", "Qual", "er\u00b7sezt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lieben achtet kein Geseze,", "tokens": ["Lie\u00b7ben", "ach\u00b7tet", "kein", "Ge\u00b7se\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die angenehme Spur", "tokens": ["Und", "die", "an\u00b7ge\u00b7neh\u00b7me", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So entz\u00fcndter Liebessch\u00e4ze", "tokens": ["So", "ent\u00b7z\u00fcnd\u00b7ter", "Lie\u00b7bes\u00b7sch\u00e4\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Antrieb der Natur,", "tokens": ["Ist", "ein", "An\u00b7trieb", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die uns nicht zuwieder spricht;", "tokens": ["Die", "uns", "nicht", "zu\u00b7wie\u00b7der", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das versteht der P\u00f6bel nicht.", "tokens": ["Das", "ver\u00b7steht", "der", "P\u00f6\u00b7bel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Drum so lache, kluge Sch\u00f6ne,", "tokens": ["Drum", "so", "la\u00b7che", ",", "klu\u00b7ge", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So vern\u00fcnftig, als du thust,", "tokens": ["So", "ver\u00b7n\u00fcnf\u00b7tig", ",", "als", "du", "thust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du irgend das Geh\u00f6hne", "tokens": ["Wenn", "du", "ir\u00b7gend", "das", "Ge\u00b7h\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tummer Sp\u00f6tter h\u00f6ren must,", "tokens": ["Tum\u00b7mer", "Sp\u00f6t\u00b7ter", "h\u00f6\u00b7ren", "must", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil dein ungebundner Geist", "tokens": ["Weil", "dein", "un\u00b7ge\u00b7bund\u00b7ner", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier und da mit K\u00fc\u00dfen speist.", "tokens": ["Hier", "und", "da", "mit", "K\u00fc\u00b7\u00dfen", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Jugend, Lust und sch\u00f6ne Wangen", "tokens": ["Ju\u00b7gend", ",", "Lust", "und", "sch\u00f6\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn fast st\u00fcndlich auf der Flucht,", "tokens": ["Stehn", "fast", "st\u00fcnd\u00b7lich", "auf", "der", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind die einmahl weggegangen,", "tokens": ["Sind", "die", "ein\u00b7mahl", "weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden sie umsonst gesucht;", "tokens": ["Wer\u00b7den", "sie", "um\u00b7sonst", "ge\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wer die Bahn der Klugheit tritt,", "tokens": ["Wer", "die", "Bahn", "der", "Klug\u00b7heit", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmt sie fein bey Zeiten mit.", "tokens": ["Nimmt", "sie", "fein", "bey", "Zei\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Was man von galanten Kindern,", "tokens": ["Was", "man", "von", "ga\u00b7lan\u00b7ten", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verg\u00e4lltem Munde spricht,", "tokens": ["Mit", "ver\u00b7g\u00e4ll\u00b7tem", "Mun\u00b7de", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan die Sehnsucht nicht verhindern,", "tokens": ["Kan", "die", "Sehn\u00b7sucht", "nicht", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Werth ins Auge sticht,", "tokens": ["Die", "der", "Werth", "ins", "Au\u00b7ge", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie dir bey stiller Ruh", "tokens": ["Da\u00df", "sie", "dir", "bey", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein geheimes Opfer thu.", "tokens": ["Ein", "ge\u00b7hei\u00b7mes", "Op\u00b7fer", "thu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Als ich dich in unsern Ga\u00dfen", "tokens": ["Als", "ich", "dich", "in", "un\u00b7sern", "Ga\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur verstohlen angeblickt,", "tokens": ["Nur", "ver\u00b7stoh\u00b7len", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fing ich alles an zu ha\u00dfen,", "tokens": ["Fing", "ich", "al\u00b7les", "an", "zu", "ha\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sich hier mit Ehrgeiz schm\u00fcckt;", "tokens": ["Was", "sich", "hier", "mit", "Ehr\u00b7geiz", "schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Verwundrung nahm mich ein,", "tokens": ["Die", "Ver\u00b7wund\u00b7rung", "nahm", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir ein stilles Lied zu weihn.", "tokens": ["Dir", "ein", "stil\u00b7les", "Lied", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Mir gefiel dein freyes Wesen,", "tokens": ["Mir", "ge\u00b7fiel", "dein", "frey\u00b7es", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches Blick und Gang bewies,", "tokens": ["Wel\u00b7ches", "Blick", "und", "Gang", "be\u00b7wies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich w\u00fcntschte dem den Beesen,", "tokens": ["Und", "ich", "w\u00fcnt\u00b7schte", "dem", "den", "Bee\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der es neulich Frechheit hies,", "tokens": ["Der", "es", "neu\u00b7lich", "Frech\u00b7heit", "hies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach der Thorheit unsrer Stadt,", "tokens": ["Nach", "der", "Thor\u00b7heit", "uns\u00b7rer", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die viel falsche Meinung hat.", "tokens": ["Die", "viel", "fal\u00b7sche", "Mei\u00b7nung", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Tadler- und Verleumdungsme\u00dfer", "tokens": ["Tad\u00b7ler", "und", "Ver\u00b7leum\u00b7dungs\u00b7me\u00b7\u00dfer"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Biegen wie geschlifnes Bley,", "tokens": ["Bie\u00b7gen", "wie", "ge\u00b7schlif\u00b7nes", "Bley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint dein Bild doch sch\u00f6n und be\u00dfer", "tokens": ["Scheint", "dein", "Bild", "doch", "sch\u00f6n", "und", "be\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als das neidische Geschrey.", "tokens": ["Als", "das", "nei\u00b7di\u00b7sche", "Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer dich sieht und das nicht glaubt,", "tokens": ["Wer", "dich", "sieht", "und", "das", "nicht", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist wohl des Geschmacks beraubt.", "tokens": ["Ist", "wohl", "des", "Ge\u00b7schmacks", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Wendung, Gang, Person und Lachen", "tokens": ["Wen\u00b7dung", ",", "Gang", ",", "Per\u00b7son", "und", "La\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dfen mich zum \u00dcberflu\u00df", "tokens": ["La\u00b7\u00dfen", "mich", "zum", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey mir selbst die Rechnung machen,", "tokens": ["Bey", "mir", "selbst", "die", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was wohl der genie\u00dfen mu\u00df,", "tokens": ["Was", "wohl", "der", "ge\u00b7nie\u00b7\u00dfen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den der Stern vergn\u00fcgter Nacht", "tokens": ["Den", "der", "Stern", "ver\u00b7gn\u00fcg\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Umgangs w\u00fcrdig macht.", "tokens": ["Dei\u00b7nes", "Um\u00b7gangs", "w\u00fcr\u00b7dig", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Auf dergleichen Marmor gleiten,", "tokens": ["Auf", "derg\u00b7lei\u00b7chen", "Mar\u00b7mor", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den man aus dem Busen gr\u00e4bt,", "tokens": ["Den", "man", "aus", "dem", "Bu\u00b7sen", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ein Fall von gro\u00dfen Leuten,", "tokens": ["Ist", "ein", "Fall", "von", "gro\u00b7\u00dfen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ins Paradies erhebt;", "tokens": ["Der", "ins", "Pa\u00b7ra\u00b7dies", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gift aus feuervoller Hand", "tokens": ["Gift", "aus", "feu\u00b7er\u00b7vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird ein s\u00fc\u00dfer Tod genand.", "tokens": ["Wird", "ein", "s\u00fc\u00b7\u00dfer", "Tod", "ge\u00b7nand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "O wie z\u00e4rtlich mag sichs k\u00fc\u00dfen,", "tokens": ["O", "wie", "z\u00e4rt\u00b7lich", "mag", "sichs", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man deine Zunge f\u00fchlt", "tokens": ["Wenn", "man", "dei\u00b7ne", "Zun\u00b7ge", "f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Scherz mit sanften Bi\u00dfen", "tokens": ["Und", "ihr", "Scherz", "mit", "sanf\u00b7ten", "Bi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die hei\u00dfe Lippen spielt;", "tokens": ["Um", "die", "hei\u00b7\u00dfe", "Lip\u00b7pen", "spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So ein k\u00fczlich Aus und Ein", "tokens": ["So", "ein", "k\u00fcz\u00b7lich", "Aus", "und", "Ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "APPR", "KON", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag des Himmels Vorschmack seyn.", "tokens": ["Mag", "des", "Him\u00b7mels", "Vor\u00b7schmack", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "W\u00e4r auch zehnmahl deine Liebe", "tokens": ["W\u00e4r", "auch", "zehn\u00b7mahl", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein vor mich verbothner Baum,", "tokens": ["Ein", "vor", "mich", "ver\u00b7both\u00b7ner", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4b ich doch dem starcken Triebe", "tokens": ["G\u00e4b", "ich", "doch", "dem", "star\u00b7cken", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solcher s\u00fc\u00dfen S\u00fcnden Raum,", "tokens": ["Sol\u00b7cher", "s\u00fc\u00b7\u00dfen", "S\u00fcn\u00b7den", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die Schuld, so es verlezt,", "tokens": ["Weil", "die", "Schuld", ",", "so", "es", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Strafen Qual ersezt.", "tokens": ["Al\u00b7ler", "Stra\u00b7fen", "Qual", "er\u00b7sezt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Lieben achtet kein Geseze,", "tokens": ["Lie\u00b7ben", "ach\u00b7tet", "kein", "Ge\u00b7se\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die angenehme Spur", "tokens": ["Und", "die", "an\u00b7ge\u00b7neh\u00b7me", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So entz\u00fcndter Liebessch\u00e4ze", "tokens": ["So", "ent\u00b7z\u00fcnd\u00b7ter", "Lie\u00b7bes\u00b7sch\u00e4\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Antrieb der Natur,", "tokens": ["Ist", "ein", "An\u00b7trieb", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die uns nicht zuwieder spricht;", "tokens": ["Die", "uns", "nicht", "zu\u00b7wie\u00b7der", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das versteht der P\u00f6bel nicht.", "tokens": ["Das", "ver\u00b7steht", "der", "P\u00f6\u00b7bel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Drum so lache, kluge Sch\u00f6ne,", "tokens": ["Drum", "so", "la\u00b7che", ",", "klu\u00b7ge", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So vern\u00fcnftig, als du thust,", "tokens": ["So", "ver\u00b7n\u00fcnf\u00b7tig", ",", "als", "du", "thust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du irgend das Geh\u00f6hne", "tokens": ["Wenn", "du", "ir\u00b7gend", "das", "Ge\u00b7h\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tummer Sp\u00f6tter h\u00f6ren must,", "tokens": ["Tum\u00b7mer", "Sp\u00f6t\u00b7ter", "h\u00f6\u00b7ren", "must", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil dein ungebundner Geist", "tokens": ["Weil", "dein", "un\u00b7ge\u00b7bund\u00b7ner", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier und da mit K\u00fc\u00dfen speist.", "tokens": ["Hier", "und", "da", "mit", "K\u00fc\u00b7\u00dfen", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Jugend, Lust und sch\u00f6ne Wangen", "tokens": ["Ju\u00b7gend", ",", "Lust", "und", "sch\u00f6\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn fast st\u00fcndlich auf der Flucht,", "tokens": ["Stehn", "fast", "st\u00fcnd\u00b7lich", "auf", "der", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind die einmahl weggegangen,", "tokens": ["Sind", "die", "ein\u00b7mahl", "weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden sie umsonst gesucht;", "tokens": ["Wer\u00b7den", "sie", "um\u00b7sonst", "ge\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wer die Bahn der Klugheit tritt,", "tokens": ["Wer", "die", "Bahn", "der", "Klug\u00b7heit", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmt sie fein bey Zeiten mit.", "tokens": ["Nimmt", "sie", "fein", "bey", "Zei\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "+-