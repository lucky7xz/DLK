{"dta.poem.6526": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mu\u00df sodann von seiner gleichen Strassen,", "tokens": ["Es", "mu\u00df", "so\u00b7dann", "von", "sei\u00b7ner", "glei\u00b7chen", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Strahlen Heer sich seitwerts lencken lassen.", "tokens": ["Der", "Strah\u00b7len", "Heer", "sich", "seit\u00b7werts", "len\u00b7cken", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne, deren Schein", "tokens": ["Die", "Son\u00b7ne", ",", "de\u00b7ren", "Schein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sodann noch nicht entdecket,", "tokens": ["So\u00b7dann", "noch", "nicht", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und, unter dem Gesicht-Creis noch verstecket,", "tokens": ["Und", ",", "un\u00b7ter", "dem", "Ge\u00b7sicht\u00b7Creis", "noch", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verborgen sollte seyn;", "tokens": ["Ver\u00b7bor\u00b7gen", "soll\u00b7te", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Scheint durch gebrochnen Strahl zu uns sich schon zu f\u00fcgen,", "tokens": ["Scheint", "durch", "ge\u00b7broch\u00b7nen", "Strahl", "zu", "uns", "sich", "schon", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Indem die Wolcken ihn zu uns, herabwerts biegen:", "tokens": ["In\u00b7dem", "die", "Wol\u00b7cken", "ihn", "zu", "uns", ",", "her\u00b7ab\u00b7werts", "bie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPER", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man glaubt, man s\u00e4he schon die Sonne prangen,", "tokens": ["Man", "glaubt", ",", "man", "s\u00e4\u00b7he", "schon", "die", "Son\u00b7ne", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Noch eher, als sie aufgegangen.", "tokens": ["Noch", "e\u00b7her", ",", "als", "sie", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}