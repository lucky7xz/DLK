{"dta.poem.19414": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "147.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4rchen von dem Schatz, den dort der Mann verhie\u00df", "tokens": ["Das", "M\u00e4r\u00b7chen", "von", "dem", "Schatz", ",", "den", "dort", "der", "Mann", "ver\u00b7hie\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei S\u00f6hnen, welchen er den Weinberg hinterlie\u00df,", "tokens": ["Drei", "S\u00f6h\u00b7nen", ",", "wel\u00b7chen", "er", "den", "Wein\u00b7berg", "hin\u00b7ter\u00b7lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PWAT", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vom Schatze, welchen sie im Weinberg sollten graben,", "tokens": ["Vom", "Schat\u00b7ze", ",", "wel\u00b7chen", "sie", "im", "Wein\u00b7berg", "soll\u00b7ten", "gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAT", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauf sie umgew\u00fchlt den ganzen Weinberg haben,", "tokens": ["Wo\u00b7rauf", "sie", "um\u00b7ge\u00b7w\u00fchlt", "den", "gan\u00b7zen", "Wein\u00b7berg", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der, so im Grund erneut, dreif\u00e4ltig Fr\u00fcchte trug,", "tokens": ["Der", ",", "so", "im", "Grund", "er\u00b7neut", ",", "drei\u00b7f\u00e4l\u00b7tig", "Fr\u00fcch\u00b7te", "trug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "VVFIN", "$,", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alle drei am Schatz auf einmal hatten gnug;", "tokens": ["Da\u00df", "al\u00b7le", "drei", "am", "Schatz", "auf", "ein\u00b7mal", "hat\u00b7ten", "gnug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "CARD", "APPRART", "NN", "APPR", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das M\u00e4rchen setzt voraus vers\u00e4umten Weinbergsgrund,", "tokens": ["Das", "M\u00e4r\u00b7chen", "setzt", "vo\u00b7raus", "ver\u00b7s\u00e4um\u00b7ten", "Wein\u00b7bergs\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem solche Heilung so gewaltsam war gesund.", "tokens": ["Dem", "sol\u00b7che", "Hei\u00b7lung", "so", "ge\u00b7walt\u00b7sam", "war", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch h\u00e4tt' ihn nach Geb\u00fchr der Mann gebaut im Leben,", "tokens": ["Doch", "h\u00e4tt'", "ihn", "nach", "Ge\u00b7b\u00fchr", "der", "Mann", "ge\u00b7baut", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Flei\u00df'ger Flei\u00dfigen ihn sterbend \u00fcbergeben;", "tokens": ["Ein", "Flei\u00df'\u00b7ger", "Flei\u00b7\u00dfi\u00b7gen", "ihn", "ster\u00b7bend", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die S\u00f6hne braucht' er nicht zu t\u00e4uschen noch zu plagen,", "tokens": ["Die", "S\u00f6h\u00b7ne", "braucht'", "er", "nicht", "zu", "t\u00e4u\u00b7schen", "noch", "zu", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weinberg ohne Zwang h\u00e4tt' einen Schatz getragen.", "tokens": ["Der", "Wein\u00b7berg", "oh\u00b7ne", "Zwang", "h\u00e4tt'", "ei\u00b7nen", "Schatz", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}