{"textgrid.poem.59960": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frisch auf, mein Sinn, ermuntre dich,", "genre": "verse", "period": "N.A.", "pub_year": 1653, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, mein Sinn, ermuntre dich,", "tokens": ["Frisch", "auf", ",", "mein", "Sinn", ",", "er\u00b7munt\u00b7re", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil dort die Morgensonne sich", "tokens": ["Weil", "dort", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zeigt auf verg\u00fcld'tem H\u00fcgel.", "tokens": ["Zeigt", "auf", "ver\u00b7g\u00fcld'\u00b7tem", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es h\u00fcpfet ob den B\u00fcschen \u00fcmm,", "tokens": ["Es", "h\u00fcp\u00b7fet", "ob", "den", "B\u00fc\u00b7schen", "\u00fcmm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und singet Gott mit krauser Stimm'", "tokens": ["Und", "sin\u00b7get", "Gott", "mit", "krau\u00b7ser", "Stimm'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das leichte Luftgefl\u00fcgel.", "tokens": ["Das", "leich\u00b7te", "Luft\u00b7ge\u00b7fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schl\u00e4fer, Sch\u00e4fer,", "tokens": ["Schl\u00e4\u00b7fer", ",", "Sch\u00e4\u00b7fer", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sind geflissen,", "tokens": ["Sind", "ge\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Zu begr\u00fc\u00dfen", "tokens": ["Zu", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Trift und Auen,", "tokens": ["Trift", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Dir und ihnen sich zu trauen.", "tokens": ["Dir", "und", "ih\u00b7nen", "sich", "zu", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir, dir, dir hier, o Gott, stimmt an,", "tokens": ["Dir", ",", "dir", ",", "dir", "hier", ",", "o", "Gott", ",", "stimmt", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schwebt, was webt, was beben kann,", "tokens": ["Was", "schwebt", ",", "was", "webt", ",", "was", "be\u00b7ben", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Loblied deiner G\u00fcte.", "tokens": ["Ein", "Lob\u00b7lied", "dei\u00b7ner", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch mich soll nichts besch\u00e4men nicht,", "tokens": ["Auch", "mich", "soll", "nichts", "be\u00b7sch\u00e4\u00b7men", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PIS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich vergesse meine Pflicht", "tokens": ["Da\u00df", "ich", "ver\u00b7ges\u00b7se", "mei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dankbares Gem\u00fcthe.", "tokens": ["Und", "dank\u00b7ba\u00b7res", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "H\u00f6re, mehre", "tokens": ["H\u00f6\u00b7re", ",", "meh\u00b7re"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Dies Erklingen,", "tokens": ["Dies", "Er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "La\u00df mein Singen,", "tokens": ["La\u00df", "mein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Dich jetzt preisen,", "tokens": ["Dich", "jetzt", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Und dir Ruhm und Ehr' erweisen.", "tokens": ["Und", "dir", "Ruhm", "und", "Ehr'", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Leid der Nacht ist \u00fcberhin.", "tokens": ["Das", "Leid", "der", "Nacht", "ist", "\u00fc\u00b7berh\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer macht, da\u00df ich entkommen bin", "tokens": ["Wer", "macht", ",", "da\u00df", "ich", "ent\u00b7kom\u00b7men", "bin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus tausendfachen Stricken?", "tokens": ["Aus", "tau\u00b7send\u00b7fa\u00b7chen", "Stri\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da mich umfing des Todes Bild,", "tokens": ["Da", "mich", "um\u00b7fing", "des", "To\u00b7des", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War deine Hand mein starker Schild,", "tokens": ["War", "dei\u00b7ne", "Hand", "mein", "star\u00b7ker", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Schutz wollt' mich begl\u00fccken.", "tokens": ["Dein", "Schutz", "wollt'", "mich", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Pfeilen, Seilen", "tokens": ["Pfei\u00b7len", ",", "Sei\u00b7len"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "B\u00f6ser Leute,", "tokens": ["B\u00f6\u00b7ser", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Die zur Beute", "tokens": ["Die", "zur", "Beu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Mich erw\u00e4hlet,", "tokens": ["Mich", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Hat ihr Werk der Nacht gefehlet.", "tokens": ["Hat", "ihr", "Werk", "der", "Nacht", "ge\u00b7feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Held und H\u00fcter unsrer Wacht,", "tokens": ["Du", "Held", "und", "H\u00fc\u00b7ter", "uns\u00b7rer", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der du nicht schl\u00e4fest in der Nacht,", "tokens": ["Der", "du", "nicht", "schl\u00e4\u00b7fest", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Gnadenaug' bleib' offen;", "tokens": ["Dein", "Gna\u00b7de\u00b7naug'", "bleib'", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beug' ferner allem Unfall f\u00fcr,", "tokens": ["Beug'", "fer\u00b7ner", "al\u00b7lem", "Un\u00b7fall", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PIS", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00f6ffne meines Herzens Th\u00fcr", "tokens": ["Und", "\u00f6ff\u00b7ne", "mei\u00b7nes", "Her\u00b7zens", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu fest gefa\u00dftem Hoffen.", "tokens": ["Zu", "fest", "ge\u00b7fa\u00df\u00b7tem", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ende, wende", "tokens": ["En\u00b7de", ",", "wen\u00b7de"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Meine Schmerzen", "tokens": ["Mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "In dem Herzen", "tokens": ["In", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "Ob den S\u00fcnden,", "token