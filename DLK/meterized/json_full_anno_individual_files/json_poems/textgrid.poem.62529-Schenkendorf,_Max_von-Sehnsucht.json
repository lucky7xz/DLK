ure": "trochaic.tetra"}, "line.5": {"text": "Geb' ich meinem Freunde hin.", "tokens": ["Geb'", "ich", "mei\u00b7nem", "Freun\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Seinen Schwestern, seinen Br\u00fcdern", "tokens": ["Sei\u00b7nen", "Schwes\u00b7tern", ",", "sei\u00b7nen", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich mich in Treue nahn,", "tokens": ["Will", "ich", "mich", "in", "Treu\u00b7e", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Armen, Bl\u00f6den, Niedern", "tokens": ["An", "den", "Ar\u00b7men", ",", "Bl\u00f6\u00b7den", ",", "Nie\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich dankend ihm erwiedern,", "tokens": ["Will", "ich", "dan\u00b7kend", "ihm", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er liebend mir gethan.", "tokens": ["Was", "er", "lie\u00b7bend", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Einst erklingen andre Stunden,", "tokens": ["Einst", "er\u00b7klin\u00b7gen", "and\u00b7re", "Stun\