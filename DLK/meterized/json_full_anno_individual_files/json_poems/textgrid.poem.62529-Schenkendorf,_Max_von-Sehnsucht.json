{"textgrid.poem.62529": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die Ferne m\u00f6cht' ich ziehen,", "tokens": ["In", "die", "Fer\u00b7ne", "m\u00f6cht'", "ich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit von meines Vaters Haus;", "tokens": ["Weit", "von", "mei\u00b7nes", "Va\u00b7ters", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Bergesspitzen gl\u00fchen,", "tokens": ["Wo", "die", "Ber\u00b7ges\u00b7spit\u00b7zen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die fremden Blumen bl\u00fchen,", "tokens": ["Wo", "die", "frem\u00b7den", "Blu\u00b7men", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhte meine Seele aus.", "tokens": ["Ruh\u00b7te", "mei\u00b7ne", "See\u00b7le", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' ich Fl\u00fcgel, h\u00e4tt' ich Fl\u00fcgel,", "tokens": ["H\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", ",", "h\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00f6g' ich auf zu meinem Stern,", "tokens": ["Fl\u00f6g'", "ich", "auf", "zu", "mei\u00b7nem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber Meere, Th\u00e4ler, H\u00fcgel,", "tokens": ["Ue\u00b7ber", "Mee\u00b7re", ",", "Th\u00e4\u00b7ler", ",", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonder Schranke, sonder Z\u00fcgel", "tokens": ["Son\u00b7der", "Schran\u00b7ke", ",", "son\u00b7der", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Folgt' ich immer meinem Herrn.", "tokens": ["Folgt'", "ich", "im\u00b7mer", "mei\u00b7nem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Still und selig mit Marien", "tokens": ["Still", "und", "se\u00b7lig", "mit", "Ma\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ihm zu F\u00fc\u00dfen s\u00e4\u00df' ich da,", "tokens": ["Ihm", "zu", "F\u00fc\u00b7\u00dfen", "s\u00e4\u00df'", "ich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer m\u00f6cht' ich vor ihm knieen,", "tokens": ["Im\u00b7mer", "m\u00f6cht'", "ich", "vor", "ihm", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In mich seine Worte ziehen,", "tokens": ["In", "mich", "sei\u00b7ne", "Wor\u00b7te", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4tt' ihn immer hold und nah.", "tokens": ["H\u00e4tt'", "ihn", "im\u00b7mer", "hold", "und", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! das war ein sch\u00f6ner Segen,", "tokens": ["Ach", "!", "das", "war", "ein", "sch\u00f6\u00b7ner", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann er mit den J\u00fcngern ging,", "tokens": ["Wann", "er", "mit", "den", "J\u00fcn\u00b7gern", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Feldern, auf den Wegen", "tokens": ["Auf", "den", "Fel\u00b7dern", ",", "auf", "den", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Herz, wie Maienregen,", "tokens": ["Je\u00b7des", "Herz", ",", "wie", "Mai\u00b7en\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Trost, sein Wort empfing.", "tokens": ["Sei\u00b7nen", "Trost", ",", "sein", "Wort", "emp\u00b7fing", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ander Loos ward uns bereitet:", "tokens": ["An\u00b7der", "Loos", "ward", "uns", "be\u00b7rei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie auch bl\u00fchet rings das Land,", "tokens": ["Wie", "auch", "bl\u00fc\u00b7het", "rings", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sich rings die Ferne breitet \u2013", "tokens": ["Wie", "sich", "rings", "die", "Fer\u00b7ne", "brei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns rufet, der uns leitet,", "tokens": ["Der", "uns", "ru\u00b7fet", ",", "der", "uns", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unser holder Freund verschwand.", "tokens": ["Un\u00b7ser", "hol\u00b7der", "Freund", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aufgehoben, aufgenommen", "tokens": ["Auf\u00b7ge\u00b7ho\u00b7ben", ",", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Himmel ist er nur;", "tokens": ["In", "den", "Him\u00b7mel", "ist", "er", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrlich will er wiederkommen,", "tokens": ["Herr\u00b7lich", "will", "er", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Treuen, Stillen, Frommen,", "tokens": ["Sei\u00b7ne", "Treu\u00b7en", ",", "Stil\u00b7len", ",", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Folgen immer seiner Spur.", "tokens": ["Fol\u00b7gen", "im\u00b7mer", "sei\u00b7ner", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Will mich denn zufrieden geben,", "tokens": ["Will", "mich", "denn", "zu\u00b7frie\u00b7den", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fassen mich im stillen Sinn;", "tokens": ["Fas\u00b7sen", "mich", "im", "stil\u00b7len", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Denken, Sehnen, Streben,", "tokens": ["All", "mein", "Den\u00b7ken", ",", "Seh\u00b7nen", ",", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Lieb' und auch mein Leben", "tokens": ["Mei\u00b7ne", "Lieb'", "und", "auch", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geb' ich meinem Freunde hin.", "tokens": ["Geb'", "ich", "mei\u00b7nem", "Freun\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seinen Schwestern, seinen Br\u00fcdern", "tokens": ["Sei\u00b7nen", "Schwes\u00b7tern", ",", "sei\u00b7nen", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich mich in Treue nahn,", "tokens": ["Will", "ich", "mich", "in", "Treu\u00b7e", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Armen, Bl\u00f6den, Niedern", "tokens": ["An", "den", "Ar\u00b7men", ",", "Bl\u00f6\u00b7den", ",", "Nie\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich dankend ihm erwiedern,", "tokens": ["Will", "ich", "dan\u00b7kend", "ihm", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er liebend mir gethan.", "tokens": ["Was", "er", "lie\u00b7bend", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Einst erklingen andre Stunden,", "tokens": ["Einst", "er\u00b7klin\u00b7gen", "and\u00b7re", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Herz nimmt andern Lauf,", "tokens": ["Und", "das", "Herz", "nimmt", "an\u00b7dern", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erd' und Heimat ist verschwunden,", "tokens": ["Erd'", "und", "Hei\u00b7mat", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den sel'gen Liebeswunden", "tokens": ["In", "den", "sel'\u00b7gen", "Lie\u00b7bes\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6set aller Schmerz sich auf.", "tokens": ["L\u00f6\u00b7set", "al\u00b7ler", "Schmerz", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Meine Seele, gleich der Taube,", "tokens": ["Mei\u00b7ne", "See\u00b7le", ",", "gleich", "der", "Tau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich birgt im Felsenstein,", "tokens": ["Die", "sich", "birgt", "im", "Fel\u00b7sen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Erde nicht zum Raube:", "tokens": ["Wird", "der", "Er\u00b7de", "nicht", "zum", "Rau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Himmel dringt mein Glaube,", "tokens": ["In", "den", "Him\u00b7mel", "dringt", "mein", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Lieb' und Sehnsucht ein.", "tokens": ["Mei\u00b7ne", "Lieb'", "und", "Sehn\u00b7sucht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dort ist Gnade, dort Erbarmen,", "tokens": ["Dort", "ist", "Gna\u00b7de", ",", "dort", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ew'ge F\u00fcll' und reiche Lust.", "tokens": ["Ew'\u00b7ge", "F\u00fcll'", "und", "rei\u00b7che", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All' ihr Kranken, all' ihr Armen,", "tokens": ["All'", "ihr", "Kran\u00b7ken", ",", "all'", "ihr", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Genesen, zum Erwarmen", "tokens": ["Zum", "Ge\u00b7ne\u00b7sen", ",", "zum", "Er\u00b7war\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Kommt an eures Heilands Brust!", "tokens": ["Kommt", "an", "eu\u00b7res", "Hei\u00b7lands", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "In die Ferne m\u00f6cht' ich ziehen,", "tokens": ["In", "die", "Fer\u00b7ne", "m\u00f6cht'", "ich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit von meines Vaters Haus;", "tokens": ["Weit", "von", "mei\u00b7nes", "Va\u00b7ters", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Bergesspitzen gl\u00fchen,", "tokens": ["Wo", "die", "Ber\u00b7ges\u00b7spit\u00b7zen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die fremden Blumen bl\u00fchen,", "tokens": ["Wo", "die", "frem\u00b7den", "Blu\u00b7men", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhte meine Seele aus.", "tokens": ["Ruh\u00b7te", "mei\u00b7ne", "See\u00b7le", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "H\u00e4tt' ich Fl\u00fcgel, h\u00e4tt' ich Fl\u00fcgel,", "tokens": ["H\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", ",", "h\u00e4tt'", "ich", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00f6g' ich auf zu meinem Stern,", "tokens": ["Fl\u00f6g'", "ich", "auf", "zu", "mei\u00b7nem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber Meere, Th\u00e4ler, H\u00fcgel,", "tokens": ["Ue\u00b7ber", "Mee\u00b7re", ",", "Th\u00e4\u00b7ler", ",", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonder Schranke, sonder Z\u00fcgel", "tokens": ["Son\u00b7der", "Schran\u00b7ke", ",", "son\u00b7der", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Folgt' ich immer meinem Herrn.", "tokens": ["Folgt'", "ich", "im\u00b7mer", "mei\u00b7nem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Still und selig mit Marien", "tokens": ["Still", "und", "se\u00b7lig", "mit", "Ma\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ihm zu F\u00fc\u00dfen s\u00e4\u00df' ich da,", "tokens": ["Ihm", "zu", "F\u00fc\u00b7\u00dfen", "s\u00e4\u00df'", "ich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer m\u00f6cht' ich vor ihm knieen,", "tokens": ["Im\u00b7mer", "m\u00f6cht'", "ich", "vor", "ihm", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In mich seine Worte ziehen,", "tokens": ["In", "mich", "sei\u00b7ne", "Wor\u00b7te", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4tt' ihn immer hold und nah.", "tokens": ["H\u00e4tt'", "ihn", "im\u00b7mer", "hold", "und", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ach! das war ein sch\u00f6ner Segen,", "tokens": ["Ach", "!", "das", "war", "ein", "sch\u00f6\u00b7ner", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann er mit den J\u00fcngern ging,", "tokens": ["Wann", "er", "mit", "den", "J\u00fcn\u00b7gern", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Feldern, auf den Wegen", "tokens": ["Auf", "den", "Fel\u00b7dern", ",", "auf", "den", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Herz, wie Maienregen,", "tokens": ["Je\u00b7des", "Herz", ",", "wie", "Mai\u00b7en\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Trost, sein Wort empfing.", "tokens": ["Sei\u00b7nen", "Trost", ",", "sein", "Wort", "emp\u00b7fing", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ander Loos ward uns bereitet:", "tokens": ["An\u00b7der", "Loos", "ward", "uns", "be\u00b7rei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie auch bl\u00fchet rings das Land,", "tokens": ["Wie", "auch", "bl\u00fc\u00b7het", "rings", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sich rings die Ferne breitet \u2013", "tokens": ["Wie", "sich", "rings", "die", "Fer\u00b7ne", "brei\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns rufet, der uns leitet,", "tokens": ["Der", "uns", "ru\u00b7fet", ",", "der", "uns", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unser holder Freund verschwand.", "tokens": ["Un\u00b7ser", "hol\u00b7der", "Freund", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Aufgehoben, aufgenommen", "tokens": ["Auf\u00b7ge\u00b7ho\u00b7ben", ",", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Himmel ist er nur;", "tokens": ["In", "den", "Him\u00b7mel", "ist", "er", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrlich will er wiederkommen,", "tokens": ["Herr\u00b7lich", "will", "er", "wie\u00b7der\u00b7kom\u0