"Wird der Erde nicht zum Raube:", "tokens": ["Wird", "der", "Er\u00b7de", "nicht", "zum", "Rau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Himmel dringt mein Glaube,", "tokens": ["In", "den", "Him\u00b7mel", "dringt", "mein", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Lieb' und Sehnsucht ein.", "tokens": ["Mei\u00b7ne", "Lieb'", "und", "Sehn\u00b7sucht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Dort ist Gnade, dort Erbarmen,", "tokens": ["Dort", "ist", "Gna\u00b7de", ",", "dort", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ew'ge F\u00fcll' und reiche Lust.", "tokens": ["Ew'\u00b7ge", "F\u00fcll'", "und", "rei\u00b7che", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All' ihr Kranken, all' ihr Armen,", "tokens": ["All'", "ihr", "Kran\u00b7ken", ",", "all'", "ihr", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Genesen, zum Erwarmen", "tokens": ["Zum", "Ge\u00b7ne\u00b7sen", ",", "zum", "Er\u00b7war\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Kommt an eures Heilands Brust!", "tokens": ["Kommt", "an", "eu\u00b7res", "Hei\u00b7lands", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}