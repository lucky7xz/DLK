{"textgrid.poem.47370": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Calderon und seine Bearbeiter", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Calderon mit seiner steifen", "tokens": ["Cal\u00b7de\u00b7ron", "mit", "sei\u00b7ner", "stei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Formenpracht kann ich begreifen,", "tokens": ["For\u00b7men\u00b7pracht", "kann", "ich", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch an seinem immer neuen", "tokens": ["Auch", "an", "sei\u00b7nem", "im\u00b7mer", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Farbenschmelz mein Aug' erfreuen,", "tokens": ["Far\u00b7ben\u00b7schmelz", "mein", "Aug'", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst Phantome seiner krassen", "tokens": ["Selbst", "Phan\u00b7to\u00b7me", "sei\u00b7ner", "kras\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kloster-Hofluft gelten lassen.", "tokens": ["Klos\u00b7ter\u00b7Hofluft", "gel\u00b7ten", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Aber wer ihn heut noch gelten", "tokens": ["A\u00b7ber", "wer", "ihn", "heut", "noch", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Machen will, den mu\u00df ich schelten.", "tokens": ["Ma\u00b7chen", "will", ",", "den", "mu\u00df", "ich", "schel\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "ART", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo er stehn will auf den Brettern,", "tokens": ["Wo", "er", "stehn", "will", "auf", "den", "Bret\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wird die Zeit herab ihn schmettern,", "tokens": ["Wird", "die", "Zeit", "her\u00b7ab", "ihn", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die mit F\u00fcrstenknecht und Pfaffen", "tokens": ["Die", "mit", "F\u00fcrs\u00b7ten\u00b7knecht", "und", "Pfaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "K\u00fcnftig nichts mehr hat zu schaffen.", "tokens": ["K\u00fcnf\u00b7tig", "nichts", "mehr", "hat", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "ADV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Calderon mit seiner steifen", "tokens