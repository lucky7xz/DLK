{"textgrid.poem.44180": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[brich an, erfreutes Licht, las deine Freudenstunden]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brich an, erfreutes Licht, las deine Freudenstunden", "tokens": ["Brich", "an", ",", "er\u00b7freu\u00b7tes", "Licht", ",", "las", "dei\u00b7ne", "Freu\u00b7den\u00b7stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit angenehmer Pracht an diesem Morgen sehn;", "tokens": ["Mit", "an\u00b7ge\u00b7neh\u00b7mer", "Pracht", "an", "die\u00b7sem", "Mor\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sich des Jahres Kern, der Fr\u00fchling, eingefunden,", "tokens": ["Da", "sich", "des", "Jah\u00b7res", "Kern", ",", "der", "Fr\u00fch\u00b7ling", ",", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So m\u00fc\u00dfen von nun an nur Zephyrl\u00fcfte wehn.", "tokens": ["So", "m\u00fc\u00b7\u00dfen", "von", "nun", "an", "nur", "Ze\u00b7phyr\u00b7l\u00fcf\u00b7te", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ADV", "APPR", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Glanz, begl\u00fcckter Tag, dein angenehmes Wesen", "tokens": ["Dein", "Glanz", ",", "be\u00b7gl\u00fcck\u00b7ter", "Tag", ",", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00e4hrt mit vollkommner Lust in meine Th\u00e4ler ein,", "tokens": ["F\u00e4hrt", "mit", "voll\u00b7komm\u00b7ner", "Lust", "in", "mei\u00b7ne", "Th\u00e4\u00b7ler", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es heist mich Opferholz zu seinem Feste lesen", "tokens": ["Es", "heist", "mich", "Op\u00b7fer\u00b7holz", "zu", "sei\u00b7nem", "Fes\u00b7te", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und auf den Danckaltar geweihte Myrthen streun.", "tokens": ["Und", "auf", "den", "Dan\u00b7ckal\u00b7tar", "ge\u00b7weih\u00b7te", "Myr\u00b7then", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich k\u00f6nte hier gar viel von alten Opfern schreiben,", "tokens": ["Ich", "k\u00f6n\u00b7te", "hier", "gar", "viel", "von", "al\u00b7ten", "Op\u00b7fern", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Rom und Griechenland den G\u00f6ttern dargebracht,", "tokens": ["Die", "Rom", "und", "Grie\u00b7chen\u00b7land", "den", "G\u00f6t\u00b7tern", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch mu\u00df ich diesesmahl bey meinem Zwecke bleiben,", "tokens": ["Doch", "mu\u00df", "ich", "die\u00b7ses\u00b7mahl", "bey", "mei\u00b7nem", "Zwe\u00b7cke", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den mein Gehorsam mir und meine Pflicht gemacht.", "tokens": ["Den", "mein", "Ge\u00b7hor\u00b7sam", "mir", "und", "mei\u00b7ne", "Pflicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erlaube, Gn\u00e4digster, dies k\u00fcrzlich aufzusezen,", "tokens": ["Er\u00b7lau\u00b7be", ",", "Gn\u00e4\u00b7digs\u00b7ter", ",", "dies", "k\u00fcrz\u00b7lich", "auf\u00b7zu\u00b7se\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PDS", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was meine Danckbarkeit und den Gehorsam zeigt,", "tokens": ["Was", "mei\u00b7ne", "Dan\u00b7ck\u00b7bar\u00b7keit", "und", "den", "Ge\u00b7hor\u00b7sam", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Ich wollte, k\u00f6nt ich nur, den Tag in Marmor \u00e4zen,", "tokens": ["Ich", "woll\u00b7te", ",", "k\u00f6nt", "ich", "nur", ",", "den", "Tag", "in", "Mar\u00b7mor", "\u00e4\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VMFIN", "PPER", "ADV", "$,", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich wollte, w\u00e4re mir der Himmel nur geneigt,", "tokens": ["Ich", "woll\u00b7te", ",", "w\u00e4\u00b7re", "mir", "der", "Him\u00b7mel", "nur", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auch selbst der Ewigkeit die Wohlthat einverleiben,", "tokens": ["Auch", "selbst", "der", "E\u00b7wig\u00b7keit", "die", "Wohlt\u00b7hat", "ein\u00b7ver\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die mir dies hohe Haus zu jeder Zeit erweist;", "tokens": ["Die", "mir", "dies", "ho\u00b7he", "Haus", "zu", "je\u00b7der", "Zeit", "er\u00b7weist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "ADJA", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch will ich selbige tief in mein Herze schreiben,", "tokens": ["Doch", "will", "ich", "sel\u00b7bi\u00b7ge", "tief", "in", "mein", "Her\u00b7ze", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Aus welchem sie niemahls der Zeiten Wechsel rei\u00dft.", "tokens": ["Aus", "wel\u00b7chem", "sie", "nie\u00b7mahls", "der", "Zei\u00b7ten", "Wech\u00b7sel", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Mich lehrt Thalia zwar vor dich zu schlechte Lieder,", "tokens": ["Mich", "lehrt", "Tha\u00b7lia", "zwar", "vor", "dich", "zu", "schlech\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.22": {"text": "Und mein geringer Geist kennt nur der Einfalt Ei\u00df;", "tokens": ["Und", "mein", "ge\u00b7rin\u00b7ger", "Geist", "kennt", "nur", "der", "Ein\u00b7falt", "Ei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch tr\u00f6stet de\u00dfen sich die stumpfe Feder wieder,", "tokens": ["Doch", "tr\u00f6s\u00b7tet", "de\u00b7\u00dfen", "sich", "die", "stump\u00b7fe", "Fe\u00b7der", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PRF", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df sie von Schmincke nicht noch Heuchelworten weis.", "tokens": ["Da\u00df", "sie", "von", "Schmin\u00b7cke", "nicht", "noch", "Heu\u00b7chel\u00b7wor\u00b7ten", "weis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dies Opfer, so mein Kiel vor diesesmahl will bringen,", "tokens": ["Dies", "Op\u00b7fer", ",", "so", "mein", "Kiel", "vor", "die\u00b7ses\u00b7mahl", "will", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "ADV", "PPOSAT", "NN", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Soll blos ein hei\u00dfer Wuntsch in schlechten Worten seyn,", "tokens": ["Soll", "blos", "ein", "hei\u00b7\u00dfer", "Wunt\u00b7sch", "in", "schlech\u00b7ten", "Wor\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Der H\u00f6chste la\u00dfe ihn nur durch die Wolcken dringen,", "tokens": ["Der", "H\u00f6chs\u00b7te", "la\u00b7\u00dfe", "ihn", "nur", "durch", "die", "Wol\u00b7cken", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es gehe de\u00dfen Kraft mir selbst durch Marck und Bein:", "tokens": ["Es", "ge\u00b7he", "de\u00b7\u00dfen", "Kraft", "mir", "selbst", "durch", "Marck", "und", "Bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Gott wolle, Gn\u00e4digster, vor Dero Wohlseyn wachen,", "tokens": ["Gott", "wol\u00b7le", ",", "Gn\u00e4\u00b7digs\u00b7ter", ",", "vor", "De\u00b7ro", "Wohl\u00b7seyn", "wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "NN", "$,", "APPR", "PDS", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.30": {"text": "Damit kein Ungemach uns unsre Freude st\u00f6rt,", "tokens": ["Da\u00b7mit", "kein", "Un\u00b7ge\u00b7mach", "uns", "uns\u00b7re", "Freu\u00b7de", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er wolle diesen Tag stets viel begl\u00fcckter machen,", "tokens": ["Er", "wol\u00b7le", "die\u00b7sen", "Tag", "stets", "viel", "be\u00b7gl\u00fcck\u00b7ter", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df man an ihm von Lust und lauter Freude h\u00f6rt.", "tokens": ["Da\u00df", "man", "an", "ihm", "von", "Lust", "und", "lau\u00b7ter", "Freu\u00b7de", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPR", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Es soll dies hohe Haus noch viele Jahre bl\u00fchen,", "tokens": ["Es", "soll", "dies", "ho\u00b7he", "Haus", "noch", "vie\u00b7le", "Jah\u00b7re", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dem schon das Alterthum, der ahnenvolle Schild,", "tokens": ["Dem", "schon", "das", "Al\u00b7ter\u00b7thum", ",", "der", "ah\u00b7nen\u00b7vol\u00b7le", "Schild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der keinen Wandel kennt, den Nachruhm hat verliehen:", "tokens": ["Der", "kei\u00b7nen", "Wan\u00b7del", "kennt", ",", "den", "Nach\u00b7ruhm", "hat", "ver\u00b7lie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Es bleibe jedermann ein wahres Tugendbild.", "tokens": ["Es", "blei\u00b7be", "je\u00b7der\u00b7mann", "ein", "wah\u00b7res", "Tu\u00b7gend\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Herr, der alles kan, vermehre Dero Jahre", "tokens": ["Der", "Herr", ",", "der", "al\u00b7les", "kan", ",", "ver\u00b7meh\u00b7re", "De\u00b7ro", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VMFIN", "$,", "VVFIN", "PDS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und seze selben was von meinen Tagen bey,", "tokens": ["Und", "se\u00b7ze", "sel\u00b7ben", "was", "von", "mei\u00b7nen", "Ta\u00b7gen", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PWS", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Damit ein jeglicher noch mit der Zeit erfahre,", "tokens": ["Da\u00b7mit", "ein", "jeg\u00b7li\u00b7cher", "noch", "mit", "der", "Zeit", "er\u00b7fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PIAT", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df hier des Herren Hand und St\u00e4rcke m\u00e4chtig sey.", "tokens": ["Da\u00df", "hier", "des", "Her\u00b7ren", "Hand", "und", "St\u00e4r\u00b7cke", "m\u00e4ch\u00b7tig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er ist es, welchen du stets deine H\u00fclfe nennest;", "tokens": ["Er", "ist", "es", ",", "wel\u00b7chen", "du", "stets", "dei\u00b7ne", "H\u00fcl\u00b7fe", "nen\u00b7nest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAT", "PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Drum las er seiner Huld auch die empfohlen seyn,", "tokens": ["Drum", "las", "er", "sei\u00b7ner", "Huld", "auch", "die", "emp\u00b7foh\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ART", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die ich als G\u00f6nnerin, du als Gemahlin kennest,", "tokens": ["Die", "ich", "als", "G\u00f6n\u00b7ne\u00b7rin", ",", "du", "als", "Ge\u00b7mah\u00b7lin", "ken\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "$,", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So trift mein hei\u00dfer Wuntsch und alle Hofnung ein.", "tokens": ["So", "trift", "mein", "hei\u00b7\u00dfer", "Wunt\u00b7sch", "und", "al\u00b7le", "Hof\u00b7nung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Brich an, erfreutes Licht, las deine Freudenstunden", "tokens": ["Brich", "an", ",", "er\u00b7freu\u00b7tes", "Licht", ",", "las", "dei\u00b7ne", "Freu\u00b7den\u00b7stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit angenehmer Pracht an diesem Morgen sehn;", "tokens": ["Mit", "an\u00b7ge\u00b7neh\u00b7mer", "Pracht", "an", "die\u00b7sem", "Mor\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sich des Jahres Kern, der Fr\u00fchling, eingefunden,", "tokens": ["Da", "sich", "des", "Jah\u00b7res", "Kern", ",", "der", "Fr\u00fch\u00b7ling", ",", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So m\u00fc\u00dfen von nun an nur Zephyrl\u00fcfte wehn.", "tokens": ["So", "m\u00fc\u00b7\u00dfen", "von", "nun", "an", "nur", "Ze\u00b7phyr\u00b7l\u00fcf\u00b7te", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ADV", "APPR", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Glanz, begl\u00fcckter Tag, dein angenehmes Wesen", "tokens": ["Dein", "Glanz", ",", "be\u00b7gl\u00fcck\u00b7ter", "Tag", ",", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00e4hrt mit vollkommner Lust in meine Th\u00e4ler ein,", "tokens": ["F\u00e4hrt", "mit", "voll\u00b7komm\u00b7ner", "Lust", "in", "mei\u00b7ne", "Th\u00e4\u00b7ler", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"tex