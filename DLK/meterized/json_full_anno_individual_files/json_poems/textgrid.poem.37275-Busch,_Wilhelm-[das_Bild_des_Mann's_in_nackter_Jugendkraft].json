{"textgrid.poem.37275": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[das Bild des Mann's in nackter Jugendkraft]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Bild des Mann's in nackter Jugendkraft,", "tokens": ["Das", "Bild", "des", "Mann's", "in", "nack\u00b7ter", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So stolz in Ruhe und bewegt so edel,", "tokens": ["So", "stolz", "in", "Ru\u00b7he", "und", "be\u00b7wegt", "so", "e\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl ist's ein Anblick, der Bewundrung schafft;", "tokens": ["Wohl", "ist's", "ein", "An\u00b7blick", ",", "der", "Be\u00b7wund\u00b7rung", "schafft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drum Licht herbei! Und merke dir's, o Sch\u00e4del!", "tokens": ["Drum", "Licht", "her\u00b7bei", "!", "Und", "mer\u00b7ke", "dir's", ",", "o", "Sch\u00e4\u00b7del", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PIS", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jedoch ein Weib, ein unverh\u00fclltes Weib \u2013", "tokens": ["Je\u00b7doch", "ein", "Weib", ",", "ein", "un\u00b7ver\u00b7h\u00fcll\u00b7tes", "Weib", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da wird dir's doch ganz anders, alter Junge.", "tokens": ["Da", "wird", "dir's", "doch", "ganz", "an\u00b7ders", ",", "al\u00b7ter", "Jun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bewundrung zieht sich durch den ganzen Leib", "tokens": ["Be\u00b7wund\u00b7rung", "zieht", "sich", "durch", "den", "gan\u00b7zen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und greift mit Wonneschreck an Herz und Lunge.", "tokens": ["Und", "greift", "mit", "Won\u00b7ne\u00b7schreck", "an", "Herz", "und", "Lun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und pl\u00f6tzlich jagt das losgelassne Blut", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "jagt", "das", "los\u00b7ge\u00b7lass\u00b7ne", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle Gassen, wie die Feuerreiter.", "tokens": ["Durch", "al\u00b7le", "Gas\u00b7sen", ",", "wie", "die", "Feu\u00b7er\u00b7rei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ganze Kerl ist eine helle Glut;", "tokens": ["Der", "gan\u00b7ze", "Kerl", "ist", "ei\u00b7ne", "hel\u00b7le", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sieht nichts mehr und tappt nur noch so weiter.", "tokens": ["Er", "sieht", "nichts", "mehr", "und", "tappt", "nur", "noch", "so", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "KON", "VVFIN", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Bild des Mann's in nackter Jugendkraft,", "tokens": ["Das", "Bild", "des", "Mann's", "in", "nack\u00b7ter", "Ju\u00b7gend\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So stolz in Ruhe und bewegt so edel,", "tokens": ["So", "stolz", "in", "Ru\u00b7he", "und", "be\u00b7wegt", "so", "e\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl ist's ein Anblick, der Bewundrung schafft;", "tokens": ["Wohl", "ist's", "ein", "An\u00b7blick", ",", "der", "Be\u00b7wund\u00b7rung", "schafft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "meas