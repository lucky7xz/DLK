ure": "iambic.tri"}, "line.4": {"text": "Den Braut-Tantz, heller klingen.", "tokens": ["Den", "Braut\u00b7Tantz", ",", "hel\u00b7ler", "klin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Thu auff, o Himmel, deinen Scho\u00df,", "tokens": ["Thu", "auff", ",", "o", "Him\u00b7mel", ",", "dei\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df dich mit ihnen ein,", "tokens": ["La\u00df", "dich", "mit", "ih\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd mach, da\u00df sie an Saamen gro\u00df", "tokens": ["Vnd", "mach", ",", "da\u00df", "sie", "an", "Saa\u00b7men", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd reich an G\u00fctern seyn.", "tokens": ["Vnd", "reich", "an", "G\u00fc\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vmbgebet ewer Leid", "tokens": ["Vmb\u00b7ge\u00b7bet", "e\u00b7wer", "Leid"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Jetzt mit gewissen Schrancken:", "tokens": ["Jetzt", "mit", "ge\u00b7wis\u00b7sen", "Schran\u00b7cken", ":"], "token_info": ["word", "w