NN", "PIAT", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Br\u00e4utgam und der Braut.", "tokens": ["Dem", "Br\u00e4ut\u00b7gam", "und", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sieh ihre Gaben an,", "tokens": ["Sieh", "ih\u00b7re", "Ga\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erkenn', ob auch auff Erden", "tokens": ["Er\u00b7kenn'", ",", "ob", "auch", "auff", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was angenehmers kan", "tokens": ["Was", "an\u00b7ge\u00b7neh\u00b7mers", "kan"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als sie gepaaret werden,", "tokens": ["Als", "sie", "ge\u00b7paa\u00b7ret", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott hat in ungef\u00e4rbter Trew", "tokens": ["Gott", "hat", "in", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Trew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst sie zu hauff gebracht,", "tokens": ["Selbst", "sie", "zu", "hauff", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Plato h\u00e4tte diese Frey,", "tokens": ["Kein", "Pla\u00b7to", "h\u00e4t\u00b7te", "die\u00b7se", "Frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NE", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Sokrates erdacht.", "tokens": ["Kein", "Sok\u00b7ra\u00b7tes", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drumb la\u00df den Liebes-Wind", "tokens": ["Drumb", "la\u00df", "den", "Lie\u00b7bes\u00b7Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch beyder Hertzen dringen,", "tokens": ["Durch", "bey\u00b7der", "Hert\u00b7zen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vnd ihn, das Freuden-Kind,", "tokens": ["Vnd", "ihn", ",", "das", "Freu\u00b7den\u00b7Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Braut-Tantz, heller klingen.", "tokens": ["Den", "Braut\u00b7Tantz", ",", "hel\u00b7ler", "klin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Thu auff, o Himmel, deinen Scho\u00df,", "tokens": ["Thu", "auff", ",", "o", "Him\u00b7mel", ",", "dei\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df dich mit ihnen ein,", "tokens": ["La\u00df", "dich", "mit", "ih\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd mach, da\u00df sie an Saamen gro\u00df", "tokens": ["Vnd", "mach", ",", "da\u00df", "sie", "an", "Saa\u00b7men", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd reich an G\u00fctern seyn.", "tokens": ["Vnd", "reich", "an", "G\u00fc\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}