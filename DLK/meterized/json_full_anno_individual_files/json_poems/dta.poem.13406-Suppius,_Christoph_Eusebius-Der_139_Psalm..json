{"dta.poem.13406": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der 139 Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR! dein alles sehend Auge", "tokens": ["HeRR", "!", "dein", "al\u00b7les", "se\u00b7hend", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "PIS", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Forscht mein Inneres genau,", "tokens": ["Forscht", "mein", "In\u00b7ne\u00b7res", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was es etwa vor Dir tauge,", "tokens": ["Was", "es", "et\u00b7wa", "vor", "Dir", "tau\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts entrinnet seiner Schau.", "tokens": ["Nichts", "ent\u00b7rin\u00b7net", "sei\u00b7ner", "Schau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinem allerh\u00f6chsten Wesen", "tokens": ["Dei\u00b7nem", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "We\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich, schlechter Gegenstand,", "tokens": ["Bin", "ich", ",", "schlech\u00b7ter", "Ge\u00b7gen\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Den Du aus dem Nichts erlesen,", "tokens": ["Den", "Du", "aus", "dem", "Nichts", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seit ich ward, genau bekannt.", "tokens": ["Seit", "ich", "ward", ",", "ge\u00b7nau", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie mein Wunderbau der Glieder", "tokens": ["Wie", "mein", "Wun\u00b7der\u00b7bau", "der", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in Angeln schmeidig dreht,", "tokens": ["Sich", "in", "An\u00b7geln", "schmei\u00b7dig", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Setzet sich mein K\u00f6rper nieder,", "tokens": ["Set\u00b7zet", "sich", "mein", "K\u00f6r\u00b7per", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder wenn er sich erh\u00f6ht,", "tokens": ["O\u00b7der", "wenn", "er", "sich", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Fugen und Gelenke,", "tokens": ["Sei\u00b7ne", "Fu\u00b7gen", "und", "Ge\u00b7len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Thaten sind Dir nah,", "tokens": ["Mei\u00b7ne", "Tha\u00b7ten", "sind", "Dir", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ich erst noch k\u00fcnftig denke,", "tokens": ["Was", "ich", "erst", "noch", "k\u00fcnf\u00b7tig", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Steht schon jetzo vor Dir da.", "tokens": ["Steht", "schon", "jet\u00b7zo", "vor", "Dir", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Wenn ich mich erm\u00fcdet dehne,", "tokens": ["Wenn", "ich", "mich", "er\u00b7m\u00fc\u00b7det", "deh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Lager ausgestreckt,", "tokens": ["Auf", "dem", "La\u00b7ger", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach sanfter Ruhe sehne,", "tokens": ["Und", "nach", "sanf\u00b7ter", "Ru\u00b7he", "seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder, da\u00df ich aufgeweckt,", "tokens": ["O\u00b7der", ",", "da\u00df", "ich", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meinen Ort zu \u00e4ndern pflege,", "tokens": ["Mei\u00b7nen", "Ort", "zu", "\u00e4n\u00b7dern", "pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da bin ich von Dir umstrahlt,", "tokens": ["Da", "bin", "ich", "von", "Dir", "um\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Alle meine Schritt und Wege", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Schritt", "und", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stehn Dir deutlich vorgemalt.", "tokens": ["Stehn", "Dir", "deut\u00b7lich", "vor\u00b7ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rollet \u00fcber meine Zunge", "tokens": ["Rol\u00b7let", "\u00fc\u00b7ber", "mei\u00b7ne", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein Wort, ein Laut, ein Ton,", "tokens": ["Nur", "ein", "Wort", ",", "ein", "Laut", ",", "ein", "Ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgesandt durch Schlund und Lunge,", "tokens": ["Aus\u00b7ge\u00b7sandt", "durch", "Schlund", "und", "Lun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehe, HErr! Du weist es schon!", "tokens": ["Sie\u00b7he", ",", "Herr", "!", "Du", "weist", "es", "schon", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles, was ich jetzt verrichte,", "tokens": ["Al\u00b7les", ",", "was", "ich", "jetzt", "ver\u00b7rich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was hernach, das wird durch Dich,", "tokens": ["Was", "her\u00b7nach", ",", "das", "wird", "durch", "Dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PDS", "VAFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Wunderhand Gewichte", "tokens": ["Dei\u00b7ner", "Wun\u00b7der\u00b7hand", "Ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Breitet sich hoch \u00fcber mich.", "tokens": ["Brei\u00b7tet", "sich", "hoch", "\u00fc\u00b7ber", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solch allwissendes Erkennen", "tokens": ["Solch", "all\u00b7wis\u00b7sen\u00b7des", "Er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir schlechterdings zu hoch;", "tokens": ["Ist", "mir", "schlech\u00b7ter\u00b7dings", "zu", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wunderbar ist es zu nennen,", "tokens": ["Wun\u00b7der\u00b7bar", "ist", "es", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wo begreif ich solches doch?", "tokens": ["Wo", "be\u00b7greif", "ich", "sol\u00b7ches", "doch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein! ich kann es nicht ergr\u00fcnden,", "tokens": ["Nein", "!", "ich", "kann", "es", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ey! wie mu\u00df das m\u00f6glich seyn?", "tokens": ["Ey", "!", "wie", "mu\u00df", "das", "m\u00f6g\u00b7lich", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VMFIN", "PDS", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo ist solch ein Geist zu finden,", "tokens": ["Wo", "ist", "solch", "ein", "Geist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIAT", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und wer siehet so was ein?", "tokens": ["Und", "wer", "sie\u00b7het", "so", "was", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "PWS", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "HeRR! allgegenw\u00e4rtig Wesen!", "tokens": ["HeRR", "!", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "We\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! wohin vor deinem Geist,", "tokens": ["Ach", "!", "wo\u00b7hin", "vor", "dei\u00b7nem", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir zur Flucht den Ort erlesen,", "tokens": ["Mir", "zur", "Flucht", "den", "Ort", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchen man den Himmel hei\u00dft?", "tokens": ["Wel\u00b7chen", "man", "den", "Him\u00b7mel", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O! da bist Du gegenw\u00e4rtig!", "tokens": ["O", "!", "da", "bist", "Du", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In die H\u00f6lle will ich, ja!", "tokens": ["In", "die", "H\u00f6l\u00b7le", "will", "ich", ",", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "$,", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zum Entfliehn ist alles fertig:", "tokens": ["Zum", "Ent\u00b7fliehn", "ist", "al\u00b7les", "fer\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch, ich finde Dich auch da.", "tokens": ["Doch", ",", "ich", "fin\u00b7de", "Dich", "auch", "da", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn sich doch ein Ort erb\u00f6te", "tokens": ["Wenn", "sich", "doch", "ein", "Ort", "er\u00b7b\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Am entfernetesten Meer!", "tokens": ["Am", "ent\u00b7fer\u00b7ne\u00b7tes\u00b7ten", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hurtig! schnelle Morgenr\u00f6the!", "tokens": ["Hur\u00b7tig", "!", "schnel\u00b7le", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieb mir deine Fl\u00fcgel her!", "tokens": ["Gieb", "mir", "dei\u00b7ne", "Fl\u00fc\u00b7gel", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar, ged\u00e4cht ich da zu leben,", "tokens": ["Zwar", ",", "ge\u00b7d\u00e4cht", "ich", "da", "zu", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O so w\u00e4re deine Hand", "tokens": ["O", "so", "w\u00e4\u00b7re", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem F\u00fchren, Leiten, Heben", "tokens": ["In", "dem", "F\u00fch\u00b7ren", ",", "Lei\u00b7ten", ",", "He\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch nicht da von mir gewandt!", "tokens": ["Auch", "nicht", "da", "von", "mir", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Spr\u00e4ch ich zu den Finsternissen,", "tokens": ["Spr\u00e4ch", "ich", "zu", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der schwarzen Mitternacht,", "tokens": ["Zu", "der", "schwar\u00b7zen", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich in euren Arm zu schliessen,", "tokens": ["Mich", "in", "eu\u00b7ren", "Arm", "zu", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seyd anjetzo nur bedacht!", "tokens": ["Seyd", "an\u00b7je\u00b7tzo", "nur", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch wird mich nichts verhehlen,", "tokens": ["Den\u00b7noch", "wird", "mich", "nichts", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es bedarf ja nur allein", "tokens": ["Es", "be\u00b7darf", "ja", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deines m\u00e4chtigen Befehlen,", "tokens": ["Dei\u00b7nes", "m\u00e4ch\u00b7ti\u00b7gen", "Be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Nacht mu\u00df lichte seyn!", "tokens": ["Und", "die", "Nacht", "mu\u00df", "lich\u00b7te", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Es verliehren ja die Schatten", "tokens": ["Es", "ver\u00b7lieh\u00b7ren", "ja", "die", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey Dir alle Dunkelheit,", "tokens": ["Bey", "Dir", "al\u00b7le", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist, die Pflichten abzustatten,", "tokens": ["Ist", ",", "die", "Pflich\u00b7ten", "ab\u00b7zu\u00b7stat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor Dir selbst die Nacht bereit,", "tokens": ["Vor", "Dir", "selbst", "die", "Nacht", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alsdenn wird ihr schwarz Gesichte", "tokens": ["Als\u00b7denn", "wird", "ihr", "schwarz", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Helle, wie der Tag, gemalt,", "tokens": ["Hel\u00b7le", ",", "wie", "der", "Tag", ",", "ge\u00b7malt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur von deinem Wunderlichte", "tokens": ["Nur", "von", "dei\u00b7nem", "Wun\u00b7der\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird ihr Schleyer so durchstrahlt.", "tokens": ["Wird", "ihr", "Schle\u00b7yer", "so", "durch\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Aus dem allen kann ich sp\u00fchren", "tokens": ["Aus", "dem", "al\u00b7len", "kann", "ich", "sp\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Gr\u00f6sse alsobald,", "tokens": ["Dei\u00b7ne", "Gr\u00f6s\u00b7se", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sogar auch meine Nieren", "tokens": ["Denn", "so\u00b7gar", "auch", "mei\u00b7ne", "Nie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast Du unter der Gewalt;", "tokens": ["Hast", "Du", "un\u00b7ter", "der", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ich noch in dem Gew\u00f6lbe", "tokens": ["Als", "ich", "noch", "in", "dem", "Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Mutter Leibes lag,", "tokens": ["Mei\u00b7ner", "Mut\u00b7ter", "Lei\u00b7bes", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Warest Du es schon, derselbe,", "tokens": ["Wa\u00b7rest", "Du", "es", "schon", ",", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$,", "PDAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der mich zu umgeben pflag.", "tokens": ["Der", "mich", "zu", "um\u00b7ge\u00b7ben", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dir allein geb\u00fchrt mein Danken,", "tokens": ["Dir", "al\u00b7lein", "ge\u00b7b\u00fchrt", "mein", "Dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich wunderbar gebaut", "tokens": ["Da\u00df", "ich", "wun\u00b7der\u00b7bar", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den unerkannten Schranken,", "tokens": ["In", "den", "un\u00b7er\u00b7kann\u00b7ten", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dein Auge schon durchschaut!", "tokens": ["Die", "dein", "Au\u00b7ge", "schon", "durch\u00b7schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, so oft ich es bemerke,", "tokens": ["Und", ",", "so", "oft", "ich", "es", "be\u00b7mer\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ruf ich gleichsam ganz entz\u00fcckt:", "tokens": ["Ruf", "ich", "gleich\u00b7sam", "ganz", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wunderbar sind deine Werke,", "tokens": ["Wun\u00b7der\u00b7bar", "sind", "dei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie mein Geist sehr wohl erblickt:", "tokens": ["Wie", "mein", "Geist", "sehr", "wohl", "er\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Als, vor aller Welt verborgen,", "tokens": ["Als", ",", "vor", "al\u00b7ler", "Welt", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich erst an zu werden fieng,", "tokens": ["Ich", "erst", "an", "zu", "wer\u00b7den", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und da meiner Sch\u00f6pffung Morgen", "tokens": ["Und", "da", "mei\u00b7ner", "Sch\u00f6pf\u00b7fung", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der H\u00f6hle vor sich gieng,", "tokens": ["In", "der", "H\u00f6h\u00b7le", "vor", "sich", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bey der Bildung in das Kleine,", "tokens": ["Bey", "der", "Bil\u00b7dung", "in", "das", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der ersten Monden Zahl,", "tokens": ["In", "der", "ers\u00b7ten", "Mon\u00b7den", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "So war Dir schon mein Gebeine", "tokens": ["So", "war", "Dir", "schon", "mein", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht verhohlen dazumahl.", "tokens": ["Nicht", "ver\u00b7hoh\u00b7len", "da\u00b7zu\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ja, bevor ich Etwas worden,", "tokens": ["Ja", ",", "be\u00b7vor", "ich", "Et\u00b7was", "wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch vermischt mit anderm Thon,", "tokens": ["Noch", "ver\u00b7mischt", "mit", "an\u00b7derm", "Thon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbereit zum Menschen-Orden,", "tokens": ["Un\u00b7be\u00b7reit", "zum", "Men\u00b7schen\u00b7Or\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sahe mich dein Auge schon;", "tokens": ["Sa\u00b7he", "mich", "dein", "Au\u00b7ge", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keinen Tag hatt ich erfahren,", "tokens": ["Kei\u00b7nen", "Tag", "hatt", "ich", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die doch schon dem ohngeacht", "tokens": ["Die", "doch", "schon", "dem", "ohn\u00b7ge\u00b7acht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dein Buch geschrieben waren,", "tokens": ["In", "dein", "Buch", "ge\u00b7schrie\u00b7ben", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Du mir einst zugedacht.", "tokens": ["Die", "Du", "mir", "einst", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Deine weiseste Gedanken,", "tokens": ["Dei\u00b7ne", "wei\u00b7ses\u00b7te", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "GoTT! wie k\u00f6stlich sind sie mir!", "tokens": ["GoTT", "!", "wie", "k\u00f6st\u00b7lich", "sind", "sie", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O! wer setzet ihnen Schranken?", "tokens": ["O", "!", "wer", "set\u00b7zet", "ih\u00b7nen", "Schran\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und welch eine Meng ist ihr!", "tokens": ["Und", "welch", "ei\u00b7ne", "Meng", "ist", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NE", "VAFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "W\u00e4r es m\u00f6glich, sie zu z\u00e4hlen,", "tokens": ["W\u00e4r", "es", "m\u00f6g\u00b7lich", ",", "sie", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4me man damit zum Ziel,", "tokens": ["K\u00e4\u00b7me", "man", "da\u00b7mit", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Vermuthung wird nicht fehlen,", "tokens": ["Die", "Ver\u00b7mu\u00b7thung", "wird", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhr sind wie des Sandes viel.", "tokens": ["Ihr", "sind", "wie", "des", "San\u00b7des", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wenn ich von dem Schlaf erwache,", "tokens": ["Wenn", "ich", "von", "dem", "Schlaf", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich noch bey Dir, mein GOtt!", "tokens": ["Bin", "ich", "noch", "bey", "Dir", ",", "mein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebe doch an B\u00f6sen Rache!", "tokens": ["Ue\u00b7be", "doch", "an", "B\u00f6\u00b7sen", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4ren doch Blutgierge fort!", "tokens": ["W\u00e4\u00b7ren", "doch", "Blut\u00b7gier\u00b7ge", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Die Dir nicht die Ehre geben,", "tokens": ["Die", "Dir", "nicht", "die", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Jhre Reden l\u00e4stern Dich,", "tokens": ["Ih\u00b7re", "Re\u00b7den", "l\u00e4s\u00b7tern", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sich deine Feind erheben,", "tokens": ["Wie", "sich", "dei\u00b7ne", "Feind", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn ohn Ursach bl\u00e4hn sie sich.", "tokens": ["Denn", "ohn", "Ur\u00b7sach", "bl\u00e4hn", "sie", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "HeRR! mein Ha\u00df ist gegen Leute,", "tokens": ["HeRR", "!", "mein", "Ha\u00df", "ist", "ge\u00b7gen", "Leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Dich hassen, angeflammt.", "tokens": ["Die", "Dich", "has\u00b7sen", ",", "an\u00b7ge\u00b7flammt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich verdreu\u00dft nicht nur auf heute,", "tokens": ["Mich", "ver\u00b7dreu\u00dft", "nicht", "nur", "auf", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADV", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Sondern stets, da\u00df insgesammt", "tokens": ["Son\u00b7dern", "stets", ",", "da\u00df", "ins\u00b7ge\u00b7sammt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie sich wider Dich entr\u00fcsten,", "tokens": ["Sie", "sich", "wi\u00b7der", "Dich", "ent\u00b7r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ernstlich ist mein Ha\u00df gemeynt!", "tokens": ["Ernst\u00b7lich", "ist", "mein", "Ha\u00df", "ge\u00b7meynt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie sich deswegen br\u00fcsten,", "tokens": ["Da\u00df", "sie", "sich", "des\u00b7we\u00b7gen", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn daher sind sie mir feind!", "tokens": ["Denn", "da\u00b7her", "sind", "sie", "mir", "feind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PPER", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.17": {"line.1": {"text": "GoTT! du Kenner der Gedanken,", "tokens": ["GoTT", "!", "du", "Ken\u00b7ner", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin da! erforsche mich!", "tokens": ["Ich", "bin", "da", "!", "er\u00b7for\u00b7sche", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "VVFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Untersuch des Herzens Schranken,", "tokens": ["Un\u00b7ter\u00b7such", "des", "Her\u00b7zens", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehe! sie er\u00f6ffnen sich.", "tokens": ["Sie\u00b7he", "!", "sie", "er\u00b7\u00f6ff\u00b7nen", "sich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "PRF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Pr\u00fcf, erfahre Sinn und Handeln,", "tokens": ["Pr\u00fcf", ",", "er\u00b7fah\u00b7re", "Sinn", "und", "Han\u00b7deln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gieb Du acht, ob ich bereit", "tokens": ["Gieb", "Du", "acht", ",", "ob", "ich", "be\u00b7reit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "CARD", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6cht auf b\u00f6sen Wege wandeln;", "tokens": ["M\u00f6cht", "auf", "b\u00f6\u00b7sen", "We\u00b7ge", "wan\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchre mich zur Ewigkeit!", "tokens": ["F\u00fch\u00b7re", "mich", "zur", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}