{"textgrid.poem.53586": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Verfehlte Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute wollte die Gn\u00e4dige bei mir schlafen \u2013", "tokens": ["Heu\u00b7te", "woll\u00b7te", "die", "Gn\u00e4\u00b7di\u00b7ge", "bei", "mir", "schla\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und ich freute mich auf unsres Gl\u00fcckes Hafen.", "tokens": ["und", "ich", "freu\u00b7te", "mich", "auf", "uns\u00b7res", "Gl\u00fc\u00b7ckes", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Aber die, die l\u00e4ngst in den Gr\u00e4bern ruhen,", "tokens": ["A\u00b7ber", "die", ",", "die", "l\u00e4ngst", "in", "den", "Gr\u00e4\u00b7bern", "ru\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "wei\u00df betogat und mit wei\u00dfen Schuhen,", "tokens": ["wei\u00df", "be\u00b7to\u00b7gat", "und", "mit", "wei\u00b7\u00dfen", "Schu\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "jene alten, weisen, w\u00fcrdigen Kirchenv\u00e4ter", "tokens": ["je\u00b7ne", "al\u00b7ten", ",", "wei\u00b7sen", ",", "w\u00fcr\u00b7di\u00b7gen", "Kir\u00b7chen\u00b7v\u00e4\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "wandern schaurig hinteinander durch den \u00c4ther . . .", "tokens": ["wan\u00b7dern", "schau\u00b7rig", "hin\u00b7tein\u00b7an\u00b7der", "durch", "den", "\u00c4\u00b7ther", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, ich mu\u00df sie alle, alle lernen,", "tokens": ["Ach", ",", "ich", "mu\u00df", "sie", "al\u00b7le", ",", "al\u00b7le", "ler\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PPER", "PIS", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und dann ziehn sie wieder in die nebelhaften Fernen.", "tokens": ["und", "dann", "ziehn", "sie", "wie\u00b7der", "in", "die", "ne\u00b7bel\u00b7haf\u00b7ten", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Meine Nacht beim Teufel \u2013 die verfluchten Frommen!", "tokens": ["Mei\u00b7ne", "Nacht", "beim", "Teu\u00b7fel", "\u2013", "die", "ver\u00b7fluch\u00b7ten", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wirst du n\u00e4chste Woche zu mir kommen? \u2013", "tokens": ["Wirst", "du", "n\u00e4chs\u00b7te", "Wo\u00b7che", "zu", "mir", "kom\u00b7men", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sieh, dann sind sie fest in meinem Kopf gefangen.", "tokens": ["Sieh", ",", "dann", "sind", "sie", "fest", "in", "mei\u00b7nem", "Kopf", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "und ich will vergn\u00fcgt nach deinen Br\u00fcsten langen!", "tokens": ["und", "ich", "will", "ver\u00b7gn\u00fcgt", "nach", "dei\u00b7nen", "Br\u00fcs\u00b7ten", "lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Heute wollte die Gn\u00e4dige bei mir schlafen \u2013", "tokens": ["Heu\u00b7te", "woll\u00b7te", "die", "Gn\u00e4\u00b7di\u00b7ge", "bei", "mir", "schla\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und ich freute mich auf unsres Gl\u00fcckes Hafen.", "tokens": ["und", "ich", "freu\u00b7te", "mich", "auf", "uns\u00b7res", "Gl\u00fc\u00b7ckes", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "wo