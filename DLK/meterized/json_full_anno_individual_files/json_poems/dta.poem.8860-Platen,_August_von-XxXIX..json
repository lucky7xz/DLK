{"dta.poem.8860": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Was etwa d\u00fcrft ihr sonst noch, o meine Begleiter,", "tokens": ["Was", "et\u00b7wa", "d\u00fcrft", "ihr", "sonst", "noch", ",", "o", "mei\u00b7ne", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADV", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr r\u00fcckt mir nur mit Unrecht ein m\u00fcssiges Treiben", "tokens": ["Ihr", "r\u00fcckt", "mir", "nur", "mit", "Un\u00b7recht", "ein", "m\u00fcs\u00b7si\u00b7ges", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn da das Schiff zu Grund ging, was sollen die", "tokens": ["Denn", "da", "das", "Schiff", "zu", "Grund", "ging", ",", "was", "sol\u00b7len", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "PWS", "VMFIN", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich wei\u00df ein Volk, das ehemals zum Muster gedient", "tokens": ["Ich", "wei\u00df", "ein", "Volk", ",", "das", "e\u00b7he\u00b7mals", "zum", "Mus\u00b7ter", "ge\u00b7dient"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Was wollt' ich, w\u00e4r's ein Volk noch, als Ritter und", "tokens": ["Was", "wollt'", "ich", ",", "w\u00e4r's", "ein", "Volk", "noch", ",", "als", "Rit\u00b7ter", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "$,", "VAFIN", "ART", "NN", "ADV", "$,", "KOUS", "NN", "KON"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Doch greif' ich zum Pokal nun, und \u00fcbe Gesang, und", "tokens": ["Doch", "greif'", "ich", "zum", "Po\u00b7kal", "nun", ",", "und", "\u00fc\u00b7be", "Ge\u00b7sang", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,", "KON", "ADJA", "NN", "$,", "KON"], "meter": "-+--++--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Was hart und unabweisbar, gef\u00e4llig und heiter thun!", "tokens": ["Was", "hart", "und", "un\u00b7ab\u00b7weis\u00b7bar", ",", "ge\u00b7f\u00e4l\u00b7lig", "und", "hei\u00b7ter", "thun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Den Himmel, wenn an's Herz euch ich dr\u00fccke, begehr'", "tokens": ["Den", "Him\u00b7mel", ",", "wenn", "an's", "Herz", "euch", "ich", "dr\u00fc\u00b7cke", ",", "be\u00b7gehr'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPRART", "NN", "PPER", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Was sollt' ich, w\u00e4r' ich Jakob, mit Staffel und Leiter", "tokens": ["Was", "sollt'", "ich", ",", "w\u00e4r'", "ich", "Ja\u00b7kob", ",", "mit", "Staf\u00b7fel", "und", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "$,", "VAFIN", "PPER", "NE", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}