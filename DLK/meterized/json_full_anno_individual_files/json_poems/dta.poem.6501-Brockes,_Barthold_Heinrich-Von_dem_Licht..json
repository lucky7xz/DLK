{"dta.poem.6501": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Himmels-Lichter, die so sch\u00f6n,", "tokens": ["Die", "Him\u00b7mels\u00b7Lich\u00b7ter", ",", "die", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die das vortrefflichste von allem, was zu sehn,", "tokens": ["Die", "das", "vor\u00b7treff\u00b7lichs\u00b7te", "von", "al\u00b7lem", ",", "was", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "APPR", "PIS", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Besitzen so ein schnell-und f\u00fchlbares Bewegen,", "tokens": ["Be\u00b7sit\u00b7zen", "so", "ein", "schnell\u00b7\u00b7und", "f\u00fchl\u00b7ba\u00b7res", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sie in uns ein zitternd Licht erregen,", "tokens": ["Wenn", "sie", "in", "uns", "ein", "zit\u00b7ternd", "Licht", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "VVPP", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor dessen schnellem Schein", "tokens": ["Vor", "des\u00b7sen", "schnel\u00b7lem", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Nerven des Gesichts empfindlich seyn,", "tokens": ["Die", "Ner\u00b7ven", "des", "Ge\u00b7sichts", "emp\u00b7find\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wie, wenn C\u00f6rper sonst an andre Nerven r\u00fchren,", "tokens": ["So", "wie", ",", "wenn", "C\u00f6r\u00b7per", "sonst", "an", "and\u00b7re", "Ner\u00b7ven", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "$,", "KOUS", "NE", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dieselben solchen Druck versp\u00fcren.", "tokens": ["Die\u00b7sel\u00b7ben", "sol\u00b7chen", "Druck", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit Recht vergleichet man die Strahlen, kleinen Ballen,", "tokens": ["Mit", "Recht", "ver\u00b7glei\u00b7chet", "man", "die", "Strah\u00b7len", ",", "klei\u00b7nen", "Bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auch etwan kleinen zarten Spiessen,", "tokens": ["Auch", "et\u00b7wan", "klei\u00b7nen", "zar\u00b7ten", "Spies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die im geraden Lauf herunter schiessen,", "tokens": ["Die", "im", "ge\u00b7ra\u00b7den", "Lauf", "her\u00b7un\u00b7ter", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und ohne Zwischen-Raum uns in die Augen fallen.", "tokens": ["Und", "oh\u00b7ne", "Zwi\u00b7schen\u00b7Raum", "uns", "in", "die", "Au\u00b7gen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auf die Art sp\u00fcren wir, da\u00df von dem Licht", "tokens": ["Auf", "die", "Art", "sp\u00fc\u00b7ren", "wir", ",", "da\u00df", "von", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Die W\u00fcrckung stets auf uns geschicht.", "tokens": ["Die", "W\u00fcr\u00b7ckung", "stets", "auf", "uns", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nachdem die Strahlen nun von solchem Schein", "tokens": ["Nach\u00b7dem", "die", "Strah\u00b7len", "nun", "von", "sol\u00b7chem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mehr oder minder spitzig seyn,", "tokens": ["Mehr", "o\u00b7der", "min\u00b7der", "spit\u00b7zig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KON", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und tieffer sich in unsre Augen dringen;", "tokens": ["Und", "tief\u00b7fer", "sich", "in", "uns\u00b7re", "Au\u00b7gen", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Versp\u00fcren wir, da\u00df sie uns andre W\u00fcrckung bringen.", "tokens": ["Ver\u00b7sp\u00fc\u00b7ren", "wir", ",", "da\u00df", "sie", "uns", "and\u00b7re", "W\u00fcr\u00b7ckung", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}