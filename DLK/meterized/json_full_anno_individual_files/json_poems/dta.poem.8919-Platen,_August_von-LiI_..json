{"dta.poem.8919": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Geschieht's, indem ich bey mir selber sage:", "tokens": ["Ge\u00b7schieht's", ",", "in\u00b7dem", "ich", "bey", "mir", "sel\u00b7ber", "sa\u00b7ge", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er wei\u00df ja nicht, wie sehr ich meiner Tage", "tokens": ["Er", "wei\u00df", "ja", "nicht", ",", "wie", "sehr", "ich", "mei\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zufriedenheit an seinen Namen reihe!", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "an", "sei\u00b7nen", "Na\u00b7men", "rei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er wei\u00df ja nicht, wie sehr ich ihm verleihe,", "tokens": ["Er", "wei\u00df", "ja", "nicht", ",", "wie", "sehr", "ich", "ihm", "ver\u00b7lei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Liebevolles ich im Herzen trage,", "tokens": ["Was", "Lie\u00b7be\u00b7vol\u00b7les", "ich", "im", "Her\u00b7zen", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was gerne theilt des Lebens Lust und Plage,", "tokens": ["Was", "ger\u00b7ne", "theilt", "des", "Le\u00b7bens", "Lust", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja, was dem Leben giebt die h\u00f6chste Weihe!", "tokens": ["Ja", ",", "was", "dem", "Le\u00b7ben", "giebt", "die", "h\u00f6chs\u00b7te", "Wei\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft es nicht, und soll ich dir's beschw\u00f6ren?", "tokens": ["Du", "wei\u00dft", "es", "nicht", ",", "und", "soll", "ich", "dir's", "be\u00b7schw\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O nein! Ich wage kaum, mit dir zu sprechen,", "tokens": ["O", "nein", "!", "Ich", "wa\u00b7ge", "kaum", ",", "mit", "dir", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um nicht den Traum, der mich begl\u00fcckt, zu st\u00f6ren.", "tokens": ["Um", "nicht", "den", "Traum", ",", "der", "mich", "be\u00b7gl\u00fcckt", ",", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie sehr mich Sch\u00f6nheit auch und Reiz bestechen,", "tokens": ["Wie", "sehr", "mich", "Sch\u00f6n\u00b7heit", "auch", "und", "Reiz", "be\u00b7ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "NN", "ADV", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00fcrcht' ich doch, sie k\u00f6nnten mich beth\u00f6ren,", "tokens": ["So", "f\u00fcrcht'", "ich", "doch", ",", "sie", "k\u00f6nn\u00b7ten", "mich", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es k\u00f6nnte doch an Liebe dir gebrechen!", "tokens": ["Es", "k\u00f6nn\u00b7te", "doch", "an", "Lie\u00b7be", "dir", "ge\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}