{"dta.poem.11711": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Irrsal kam in die Mondscheing\u00e4rten", "tokens": ["Ein", "Irr\u00b7sal", "kam", "in", "die", "Mond\u00b7schein\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einer einst heiligen Liebe.", "tokens": ["Ei\u00b7ner", "einst", "hei\u00b7li\u00b7gen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Schaudernd entdeckt' ich verj\u00e4hrten Betrug.", "tokens": ["Schau\u00b7dernd", "ent\u00b7deckt'", "ich", "ver\u00b7j\u00e4hr\u00b7ten", "Be\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und mit weinendem Blick, doch grausam,", "tokens": ["Und", "mit", "wei\u00b7nen\u00b7dem", "Blick", ",", "doch", "grau\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Hie\u00df ich das schlanke,", "tokens": ["Hie\u00df", "ich", "das", "schlan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zauberhafte M\u00e4dchen", "tokens": ["Zau\u00b7ber\u00b7haf\u00b7te", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ferne gehen von mir.", "tokens": ["Fer\u00b7ne", "ge\u00b7hen", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Ach, ihre hohe Stirn,", "tokens": ["Ach", ",", "ih\u00b7re", "ho\u00b7he", "Stirn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Drin ein sch\u00f6ner, s\u00fcndhafter Wahnsinn", "tokens": ["Drin", "ein", "sch\u00f6\u00b7ner", ",", "s\u00fcnd\u00b7haf\u00b7ter", "Wahn\u00b7sinn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Aus dem dunkelen Auge blickte,", "tokens": ["Aus", "dem", "dun\u00b7ke\u00b7len", "Au\u00b7ge", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "War gesenkt, denn sie liebte mich;", "tokens": ["War", "ge\u00b7senkt", ",", "denn", "sie", "lieb\u00b7te", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Aber sie zog mit Schweigen", "tokens": ["A\u00b7ber", "sie", "zog", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Fort in die graue,", "tokens": ["Fort", "in", "die", "grau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.14": {"text": "Stille Welt hinaus.", "tokens": ["Stil\u00b7le", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Von der Zeit an", "tokens": ["Von", "der", "Zeit", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.2": {"text": "Kamen mir Tr\u00e4ume voll sch\u00f6ner Tr\u00fcbe,", "tokens": ["Ka\u00b7men", "mir", "Tr\u00e4u\u00b7me", "voll", "sch\u00f6\u00b7ner", "Tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie gesponnen auf Nebelgrund;", "tokens": ["Wie", "ge\u00b7spon\u00b7nen", "auf", "Ne\u00b7bel\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wu\u00dfte nimmer, wie mir geschah,", "tokens": ["Wu\u00df\u00b7te", "nim\u00b7mer", ",", "wie", "mir", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "War nur schmachtend seliger Krankheit voll.", "tokens": ["War", "nur", "schmach\u00b7tend", "se\u00b7li\u00b7ger", "Krank\u00b7heit", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Oft in den Tr\u00e4umen zog sich ein Vorhang", "tokens": ["Oft", "in", "den", "Tr\u00e4u\u00b7men", "zog", "sich", "ein", "Vor\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Finster und gro\u00df in's Unendliche", "tokens": ["Fins\u00b7ter", "und", "gro\u00df", "in's", "Un\u00b7end\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.3": {"text": "Zwischen mich und die dunkle Welt;", "tokens": ["Zwi\u00b7schen", "mich", "und", "die", "dunk\u00b7le", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Hinter ihm ahnt' ich ein Haideland,", "tokens": ["Hin\u00b7ter", "ihm", "ahnt'", "ich", "ein", "Hai\u00b7de\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Hinter ihm h\u00f6rt' ich's wie Nachtwind sausen;", "tokens": ["Hin\u00b7ter", "ihm", "h\u00f6rt'", "ich's", "wie", "Nacht\u00b7wind", "sau\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Auch die Falten des Vorhangs", "tokens": ["Auch", "die", "Fal\u00b7ten", "des", "Vor\u00b7hangs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Fingen bald an, sich im Sturme zu regen:", "tokens": ["Fin\u00b7gen", "bald", "an", ",", "sich", "im", "Stur\u00b7me", "zu", "re\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "PRF", "APPRART", "NN", "APPR", "ADJA", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Gleich einer Ahnung strich er dahinten,", "tokens": ["Gleich", "ei\u00b7ner", "Ah\u00b7nung", "strich", "er", "da\u00b7hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ruhig blieb ich und bange doch:", "tokens": ["Ru\u00b7hig", "blieb", "ich", "und", "ban\u00b7ge", "doch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Immer leiser wurde der Haidesturm \u2014", "tokens": ["Im\u00b7mer", "lei\u00b7ser", "wur\u00b7de", "der", "Hai\u00b7des\u00b7turm"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Siehe! da kam's.", "tokens": ["Sie\u00b7he", "!", "da", "kam'", "s."], "token_info": ["word", "punct", "word", "word", "abbreviation"], "pos": ["VVIMP", "$.", "ADV", "VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Aus einer Spalte des Vorhangs guckte", "tokens": ["Aus", "ei\u00b7ner", "Spal\u00b7te", "des", "Vor\u00b7hangs", "guck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pl\u00f6tzlich der Kopf des Zauberm\u00e4dchens,", "tokens": ["Pl\u00f6tz\u00b7lich", "der", "Kopf", "des", "Zau\u00b7ber\u00b7m\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lieblich war er und doch so be\u00e4ngstend.", "tokens": ["Lieb\u00b7lich", "war", "er", "und", "doch", "so", "be\u00b7\u00e4ngs\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Sollt' ich die Hand ihr nicht geben", "tokens": ["Sollt'", "ich", "die", "Hand", "ihr", "nicht", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "PTKNEG", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "In ihre liebe Hand?", "tokens": ["In", "ih\u00b7re", "lie\u00b7be", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bat denn ihr Auge nicht,", "tokens": ["Bat", "denn", "ihr", "Au\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sagend: da bin ich wieder", "tokens": ["Sa\u00b7gend", ":", "da", "bin", "ich", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Hergekommen aus weiter Welt?", "tokens": ["Her\u00b7ge\u00b7kom\u00b7men", "aus", "wei\u00b7ter", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}