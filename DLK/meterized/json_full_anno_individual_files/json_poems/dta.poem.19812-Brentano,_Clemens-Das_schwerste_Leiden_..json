{"dta.poem.19812": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das schwerste Leiden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist auf Erden kein schwerers Leiden,               ", "tokens": ["Es", "ist", "auf", "Er\u00b7den", "kein", "schwe\u00b7rers", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als wann sich einer auf ein neu's mu\u00df kleiden.", "tokens": ["Als", "wann", "sich", "ei\u00b7ner", "auf", "ein", "neu's", "mu\u00df", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PRF", "ART", "APPR", "ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein neues Paar Schuh,", "tokens": ["Ein", "neu\u00b7es", "Paar", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein Wammes darzu", "tokens": ["Ein", "Wam\u00b7mes", "dar\u00b7zu"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PAV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ein Rock dabei, hat kein Falten.", "tokens": ["Ein", "Rock", "da\u00b7bei", ",", "hat", "kein", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Hosen sind hinten und vorne zerspalten,", "tokens": ["Die", "Ho\u00b7sen", "sind", "hin\u00b7ten", "und", "vor\u00b7ne", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Str\u00fcmpf h\u00e4ngen wohl \u00fcber die Schuh,", "tokens": ["Die", "Str\u00fcmpf", "h\u00e4n\u00b7gen", "wohl", "\u00fc\u00b7ber", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Gleichwie ich auch thue,", "tokens": ["Gleich\u00b7wie", "ich", "auch", "thue", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Hab ich kein anders zu kaufen.", "tokens": ["Hab", "ich", "kein", "an\u00b7ders", "zu", "kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wann ich \u00fcber die Gassen gehe,", "tokens": ["Wann", "ich", "\u00fc\u00b7ber", "die", "Gas\u00b7sen", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Wind thut mir von Herzen wehe,", "tokens": ["Der", "Wind", "thut", "mir", "von", "Her\u00b7zen", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man siehet mir hinten und vorne ein,", "tokens": ["Man", "sie\u00b7het", "mir", "hin\u00b7ten", "und", "vor\u00b7ne", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das stehet nicht fein,", "tokens": ["Das", "ste\u00b7het", "nicht", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ein jeder thut meiner lachen.", "tokens": ["Ein", "je\u00b7der", "thut", "mei\u00b7ner", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Linz ist gar eine feine Stadt,", "tokens": ["Linz", "ist", "gar", "ei\u00b7ne", "fei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darin es gar viel Schneider hat,", "tokens": ["Da\u00b7rin", "es", "gar", "viel", "Schnei\u00b7der", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt' ich Geld, so z\u00f6ge ich hinein,", "tokens": ["H\u00e4tt'", "ich", "Geld", ",", "so", "z\u00f6\u00b7ge", "ich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und kaufet ein.", "tokens": ["Und", "kau\u00b7fet", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Also mu\u00df ichs lassen bleiben.", "tokens": ["Al\u00b7so", "mu\u00df", "ichs", "las\u00b7sen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also geschicht den kostfreien Gesellen,", "tokens": ["Al\u00b7so", "ge\u00b7schicht", "den", "kost\u00b7frei\u00b7en", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wann sie stets banketieren w\u00f6llen,", "tokens": ["Wann", "sie", "stets", "ban\u00b7ke\u00b7tie\u00b7ren", "w\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fressen und saufen wohl bei dem Wein,", "tokens": ["Fres\u00b7sen", "und", "sau\u00b7fen", "wohl", "bei", "dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wollen die besten seyn,", "tokens": ["Wol\u00b7len", "die", "bes\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "F\u00fcr einen jeden thun sie auszahlen.", "tokens": ["F\u00fcr", "ei\u00b7nen", "je\u00b7den", "thun", "sie", "aus\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dies Liedlein ist den jungen Gesellen gemacht;", "tokens": ["Dies", "Lied\u00b7lein", "ist", "den", "jun\u00b7gen", "Ge\u00b7sel\u00b7len", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die gern spaziren gehn bei der Nacht,", "tokens": ["Die", "gern", "spa\u00b7zi\u00b7ren", "gehn", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Wenig erwerben,", "tokens": ["We\u00b7nig", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und viel verderben.", "tokens": ["Und", "viel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}