{"dta.poem.9096": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "See, See, sonnigste See, soweit du siehst!", "tokens": ["See", ",", "See", ",", "son\u00b7nigs\u00b7te", "See", ",", "so\u00b7weit", "du", "siehst", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ueber die rollenden Wasser hin, jauchzend, tausend Tritonen.", "tokens": ["Ue\u00b7ber", "die", "rol\u00b7len\u00b7den", "Was\u00b7ser", "hin", ",", "jauch\u00b7zend", ",", "tau\u00b7send", "Tri\u00b7to\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,", "VVPP", "$,", "CARD", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf ihren Schultern,", "tokens": ["Auf", "ih\u00b7ren", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "muschelempor,", "tokens": ["mu\u00b7schel\u00b7em\u00b7por", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "hoch,", "tokens": ["hoch", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "ein Weib.", "tokens": ["ein", "Weib", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Ihre Nacktheit", "tokens": ["Ih\u00b7re", "Nackt\u00b7heit"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "in die Sonne.", "tokens": ["in", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Unter ihr,", "tokens": ["Un\u00b7ter", "ihr", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "triefend,", "tokens": ["trie\u00b7fend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "die blendenden Perlmutterw\u00e4nde immer wieder von Neuem hoch,", "tokens": ["die", "blen\u00b7den\u00b7den", "Perl\u00b7mut\u00b7ter\u00b7w\u00e4n\u00b7de", "im\u00b7mer", "wie\u00b7der", "von", "Neu\u00b7em", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+--+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "dick, feist, verliebt,", "tokens": ["dick", ",", "feist", ",", "ver\u00b7liebt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "wie Kr\u00f6ten,", "tokens": ["wie", "Kr\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "sieben alte, glamsrige Meertaper.", "tokens": ["sie\u00b7ben", "al\u00b7te", ",", "glams\u00b7ri\u00b7ge", "Meer\u00b7ta\u00b7per", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Gesichter! Das Gest\u00f6hn und das Gepruste!         ", "tokens": ["Die", "Ge\u00b7sich\u00b7ter", "!", "Das", "Ge\u00b7st\u00f6hn", "und", "das", "Ge\u00b7prus\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Da,", "tokens": ["Da", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "pl\u00f6tzlich,", "tokens": ["pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "w\u00fctend aus der Tiefe,", "tokens": ["w\u00fc\u00b7tend", "aus", "der", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Neptun.", "tokens": ["Nep\u00b7tun", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Sein Bart", "tokens": ["Sein", "Bart"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "blitzt.", "tokens": ["blitzt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "\u201ehallunken!\u201c", "tokens": ["\u201e", "hal\u00b7lun\u00b7ken", "!", "\u201c"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "Und, plitschplatsch, sein Dreizack den sieben Schlappschw\u00e4nzen um die Glatzen.", "tokens": ["Und", ",", "plitschplatsch", ",", "sein", "Drei\u00b7zack", "den", "sie\u00b7ben", "Schlapp\u00b7schw\u00e4n\u00b7zen", "um", "die", "Glat\u00b7zen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$,", "PPOSAT", "NN", "ART", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Die br\u00fcllen!", "tokens": ["Die", "br\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Dann, schnell,", "tokens": ["Dann", ",", "schnell", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "hier noch ein paar Tatschen, dort noch ein Bauch \u2014", "tokens": ["hier", "noch", "ein", "paar", "Tat\u00b7schen", ",", "dort", "noch", "ein", "Bauch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "$,", "ADV", "ADV", "ART", "NN", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "weg sind sie.", "tokens": ["weg", "sind", "sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Die Sch\u00f6ne", "tokens": ["Die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "l\u00e4chelt.", "tokens": ["l\u00e4\u00b7chelt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.12": {"line.1": {"text": "Neptun", "tokens": ["Nep\u00b7tun"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "verbeugt sich.", "tokens": ["ver\u00b7beugt", "sich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.13": {"line.1": {"text": "\u201emadam?\u201c", "tokens": ["\u201e", "ma\u00b7dam", "?", "\u201c"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}}}}}