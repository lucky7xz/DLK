{"dta.poem.282": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Von der Gr\u00f6sse der Liebe.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Alles kan der Mensche enden/", "tokens": ["Al\u00b7les", "kan", "der", "Men\u00b7sche", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur der Liebe s\u00fcsses Ziel", "tokens": ["Nur", "der", "Lie\u00b7be", "s\u00fcs\u00b7ses", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebt ihm stets was unter H\u00e4nden;", "tokens": ["Giebt", "ihm", "stets", "was", "un\u00b7ter", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er findt allzeit noch so viel/", "tokens": ["Er", "findt", "all\u00b7zeit", "noch", "so", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als er k\u00f6nnte ewig machen", "tokens": ["Als", "er", "k\u00f6nn\u00b7te", "e\u00b7wig", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den angenehmsten Sachen.", "tokens": ["Von", "den", "an\u00b7ge\u00b7nehms\u00b7ten", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie ist der Liebe Macht", "tokens": ["Nie", "ist", "der", "Lie\u00b7be", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In enge Gr\u00e4ntzen bracht.", "tokens": ["In", "en\u00b7ge", "Gr\u00e4nt\u00b7zen", "bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Himmel brennt/", "tokens": ["Der", "Him\u00b7mel", "brennt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Welt ernehren Flammen/", "tokens": ["Die", "Welt", "er\u00b7neh\u00b7ren", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und stehen unzertrennt", "tokens": ["Und", "ste\u00b7hen", "un\u00b7zer\u00b7trennt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In reiner Gluth beysammen.", "tokens": ["In", "rei\u00b7ner", "Gluth", "bey\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Lust", "tokens": ["Die", "Lust"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der Brust", "tokens": ["Der", "Brust"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Kan niemand nicht aussprechen", "tokens": ["Kan", "nie\u00b7mand", "nicht", "aus\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die stets ein s\u00fcsses Lieben sp\u00fchrt.", "tokens": ["Die", "stets", "ein", "s\u00fcs\u00b7ses", "Lie\u00b7ben", "sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kein Schmertz", "tokens": ["Kein", "Schmertz"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Ins Hertz", "tokens": ["Ins", "Hertz"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Derselben Seelen bricht", "tokens": ["Der\u00b7sel\u00b7ben", "See\u00b7len", "bricht"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Darinnen scheint das frohe Freuden-Licht.", "tokens": ["Da\u00b7rin\u00b7nen", "scheint", "das", "fro\u00b7he", "Freu\u00b7den\u00b7Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So durch die Liebe wird", "tokens": ["So", "durch", "die", "Lie\u00b7be", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Gezwungen anzubrechen.", "tokens": ["Ge\u00b7zwun\u00b7gen", "an\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So starck sind deine Triebe", "tokens": ["So", "starck", "sind", "dei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergn\u00fcgungs-reiche Liebe/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gungs\u00b7rei\u00b7che", "Lie\u00b7be", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df kein Schmertz", "tokens": ["Da\u00df", "kein", "Schmertz"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Kommt ins Hertz", "tokens": ["Kommt", "ins", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wo deine Allmacht wohnet/", "tokens": ["Wo", "dei\u00b7ne", "All\u00b7macht", "woh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Liebes-Wesen lohnet", "tokens": ["Dein", "Lie\u00b7bes\u00b7We\u00b7sen", "loh\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Allzeit die Brust", "tokens": ["All\u00b7zeit", "die", "Brust"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Mit lauter Lust.", "tokens": ["Mit", "lau\u00b7ter", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So starck sind deine Triebe", "tokens": ["So", "starck", "sind", "dei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vergn\u00fcgungs-reiche Liebe.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gungs\u00b7rei\u00b7che", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}