{"dta.poem.18305": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An Friedrich R\u00fcckert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und mehr als einmal auch ein Erzphilister,", "tokens": ["Und", "mehr", "als", "ein\u00b7mal", "auch", "ein", "Erz\u00b7phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum trauern, da\u00df du schon gestorben bist,", "tokens": ["Drum", "trau\u00b7ern", ",", "da\u00df", "du", "schon", "ge\u00b7stor\u00b7ben", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch heute alle Unterrichtsminister.", "tokens": ["Noch", "heu\u00b7te", "al\u00b7le", "Un\u00b7ter\u00b7richts\u00b7mi\u00b7nis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn lebtest du noch, ", "tokens": ["Denn", "leb\u00b7test", "du", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich schw\u00f6r's bei allen abgehaunen Z\u00f6pfen,", "tokens": ["Ich", "schw\u00f6r's", "bei", "al\u00b7len", "ab\u00b7ge\u00b7hau\u00b7nen", "Z\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Mandarin der deutschen Poesie,", "tokens": ["Zum", "Man\u00b7da\u00b7rin", "der", "deut\u00b7schen", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Mandarin mit dreizehn Kn\u00f6pfen!", "tokens": ["Zum", "Man\u00b7da\u00b7rin", "mit", "drei\u00b7zehn", "Kn\u00f6p\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}