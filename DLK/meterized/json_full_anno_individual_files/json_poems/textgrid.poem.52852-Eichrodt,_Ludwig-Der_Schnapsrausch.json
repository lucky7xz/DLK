re": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit dem Schnaps des Herrn zurecht,", "tokens": ["Mit", "dem", "Schnaps", "des", "Herrn", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mit B\u00f6sem Gutes lohnete", "tokens": ["Da", "mit", "B\u00f6\u00b7sem", "Gu\u00b7tes", "loh\u00b7ne\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Raps der hinterlist'ge Knecht.", "tokens": ["Raps", "der", "hin\u00b7ter\u00b7list'\u00b7ge", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und noch eh das halbe Jahr um,", "tokens": ["Und", "noch", "eh", "das", "hal\u00b7be", "Jahr", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.6": {"text": "Niemand ahnete, wie warum,", "tokens": ["Nie\u00b7mand", "ah\u00b7ne\u00b7te", ",", "wie", "wa\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PWAV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "War, o Schreck, im Fuderfa\u00df", "tokens": ["War", ",", "o", "Schreck", ",", "im", "Fu\u00b7der\u00b7fa\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6chstens noch der Boden na\u00df.", "tokens": ["H\u00f6chs\u00b7tens", "noch", "der", "Bo\u00b7den", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie Herr Menke Mauses merkete", "tokens": ["Wie", "Herr", "Men\u00b7ke", "Mau\u00b7ses", "mer\u00b7ke\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "NE", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den abscheulichen Verlurst,", "tokens": ["Den", "ab\u00b7scheu\u00b7li\u00b7chen", "Ver\u00b7lurst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da geschah's, da\u00df er sich \u00e4rgerte", "tokens": ["Da", "ge\u00b7scha\u00b7h's", ",", "da\u00df", "er", "sich", "\u00e4r\u00b7ger\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ueber solches Diebes Durst;", "tokens": ["Ue\u00b7ber", "sol\u00b7ches", "Die\u00b7bes", "Durst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruft die Boten seines Dienstes", "tokens": ["Ruft", "die", "Bo\u00b7ten", "sei\u00b7nes", "Diens\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringst es 'raus, sagt Menke, bringst es", "tokens": ["Bringst", "es", "'raus", ",", "sagt", "Men\u00b7ke", ",", "bringst", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "$,", "VVFIN", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Raus, wo nicht, so bringst es nicht -", "tokens": ["Raus", ",", "wo", "nicht", ",", "so", "bringst", "es", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Johann, halt' einmal das Licht!", "tokens": ["Jo\u00b7hann", ",", "halt'", "ein\u00b7mal", "das", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Johann, o Du dummer Teufu\u00eal,", "tokens": ["Jo\u00b7hann", ",", "o", "Du", "dum\u00b7mer", "Teu\u00b7fu\u00eal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bring' die Flamme nicht vor's Maul,", "tokens": ["Bring'", "die", "Flam\u00b7me", "nicht", "vor's", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Johann, Du bist's ohne Zweifu\u00eal,", "tokens": ["Jo\u00b7hann", ",", "Du", "bist's", "oh\u00b7ne", "Zwei\u00b7fu\u00eal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Denn der Fusel ist nicht faul,", "tokens": ["Denn", "der", "Fu\u00b7sel", "ist", "nicht", "faul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau, er schl\u00e4gt Dir aus dem Rachen", "tokens": ["Schau", ",", "er", "schl\u00e4gt", "Dir", "aus", "dem", "Ra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Lichterloh, es ist zum Lachen,", "tokens": ["Lich\u00b7ter\u00b7loh", ",", "es", "ist", "zum", "La\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, es ist zum Lachen nicht -", "tokens": ["Nein", ",", "es", "ist", "zum", "La\u00b7chen", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mensch, o halte nie das Licht!", "tokens": ["Mensch", ",", "o", "hal\u00b7te", "nie", "das", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}