{"textgrid.poem.52852": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnapsrausch", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niedrig ist der Rausch des Schnapso\u00ebs", "tokens": ["Nied\u00b7rig", "ist", "der", "Rausch", "des", "Schnap\u00b7so\u00ebs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kein Mensch hat Ruhm davon,", "tokens": ["Und", "kein", "Mensch", "hat", "Ruhm", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Johann Raps, des alten Rapso\u00ebs", "tokens": ["Jo\u00b7hann", "Raps", ",", "des", "al\u00b7ten", "Rap\u00b7so\u00ebs"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sohn war ein mi\u00dfrathner Sohn.", "tokens": ["Sohn", "war", "ein", "mi\u00df\u00b7ra\u00b7th\u00b7ner", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "La\u00dft mich tief betr\u00fcbt erz\u00e4hlen", "tokens": ["La\u00dft", "mich", "tief", "be\u00b7tr\u00fcbt", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "VVPP", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sich Raps verlegt auf's Stehlen", "tokens": ["Wie", "sich", "Raps", "ver\u00b7legt", "auf's", "Steh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, wiewohl ihm das verha\u00dft,", "tokens": ["Und", ",", "wie\u00b7wohl", "ihm", "das", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ward im Schnapsrausch abgefa\u00dft.", "tokens": ["Ward", "im", "Schnaps\u00b7rausch", "ab\u00b7ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eingestellt als Knecht des Hausu\u00ebs,", "tokens": ["Ein\u00b7ge\u00b7stellt", "als", "Knecht", "des", "Hau\u00b7su\u00ebs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlank, was die Gestalt betrifft,", "tokens": ["Schlank", ",", "was", "die", "Ge\u00b7stalt", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dient er treu bei Menke Mausu\u00f6s,", "tokens": ["Dient", "er", "treu", "bei", "Men\u00b7ke", "Mau\u00b7su\u00b7\u00f6s", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bis ihn reizt das Fuselgift:", "tokens": ["Bis", "ihn", "reizt", "das", "Fu\u00b7sel\u00b7gift", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn in dem Gesch\u00e4ft war dieses", "tokens": ["Denn", "in", "dem", "Ge\u00b7sch\u00e4ft", "war", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4hrend einer Handelskrises,", "tokens": ["W\u00e4h\u00b7rend", "ei\u00b7ner", "Han\u00b7dels\u00b7kri\u00b7ses", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Im Gesch\u00e4ftslokal des Herrn,", "tokens": ["Im", "Ge\u00b7sch\u00e4fts\u00b7lo\u00b7kal", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Darum naschte Johann gern.", "tokens": ["Da\u00b7rum", "naschte", "Jo\u00b7hann", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "ADV", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Also kam er viele Monate", "tokens": ["Al\u00b7so", "kam", "er", "vie\u00b7le", "Mo\u00b7na\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit dem Schnaps des Herrn zurecht,", "tokens": ["Mit", "dem", "Schnaps", "des", "Herrn", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mit B\u00f6sem Gutes lohnete", "tokens": ["Da", "mit", "B\u00f6\u00b7sem", "Gu\u00b7tes", "loh\u00b7ne\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Raps der hinterlist'ge Knecht.", "tokens": ["Raps", "der", "hin\u00b7ter\u00b7list'\u00b7ge", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und noch eh das halbe Jahr um,", "tokens": ["Und", "noch", "eh", "das", "hal\u00b7be", "Jahr", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.6": {"text": "Niemand ahnete, wie warum,", "tokens": ["Nie\u00b7mand", "ah\u00b7ne\u00b7te", ",", "wie", "wa\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PWAV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "War, o Schreck, im Fuderfa\u00df", "tokens": ["War", ",", "o", "Schreck", ",", "im", "Fu\u00b7der\u00b7fa\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6chstens noch der Boden na\u00df.", "tokens": ["H\u00f6chs\u00b7tens", "noch", "der", "Bo\u00b7den", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie Herr Menke Mauses merkete", "tokens": ["Wie", "Herr", "Men\u00b7ke", "Mau\u00b7ses", "mer\u00b7ke\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "NE", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den abscheulichen Verlurst,", "tokens": ["Den", "ab\u00b7scheu\u00b7li\u00b7chen", "Ver\u00b7lurst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da geschah's, da\u00df er sich \u00e4rgerte", "tokens": ["Da", "ge\u00b7scha\u00b7h's", ",", "da\u00df", "er", "sich", "\u00e4r\u00b7ger\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ueber solches Diebes Durst;", "tokens": ["Ue\u00b7ber", "sol\u00b7ches", "Die\u00b7bes", "Durst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruft die Boten seines Dienstes", "tokens": ["Ruft", "die", "Bo\u00b7ten", "sei\u00b7nes", "Diens\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringst es 'raus, sagt Menke, bringst es", "tokens": ["Bringst", "es", "'raus", ",", "sagt", "Men\u00b7ke", ",", "bringst", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "$,", "VVFIN", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Raus, wo nicht, so bringst es nicht -", "tokens": ["Raus", ",", "wo", "nicht", ",", "so", "bringst", "es", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Johann, halt' einmal das Licht!", "tokens": ["Jo\u00b7hann", ",", "halt'", "ein\u00b7mal", "das", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Johann, o Du dummer Teufu\u00eal,", "tokens": ["Jo\u00b7hann", ",", "o", "Du", "dum\u00b7mer", "Teu\u00b7fu\u00eal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Bring' die Flamme nicht vor's Maul,", "tokens": ["Bring'", "die", "Flam\u00b7me", "nicht", "vor's", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Johann, Du bist's ohne Zweifu\u00eal,", "tokens": ["Jo\u00b7hann", ",", "Du", "bist's", "oh\u00b7ne", "Zwei\u00b7fu\u00eal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Denn der Fusel ist nicht faul,", "tokens": ["Denn", "der", "Fu\u00b7sel", "ist", "nicht", "faul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau, er schl\u00e4gt Dir aus dem Rachen", "tokens": ["Schau", ",", "er", "schl\u00e4gt", "Dir", "aus", "dem", "Ra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Lichterloh, es ist zum Lachen,", "tokens": ["Lich\u00b7ter\u00b7loh", ",", "es", "ist", "zum", "La\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, es ist zum Lachen nicht -", "tokens": ["Nein", ",", "es", "ist", "zum", "La\u00b7chen", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mensch, o halte nie das Licht!", "tokens": ["Mensch", ",", "o", "hal\u00b7te", "nie", "das", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Niedrig ist der Rausch des Schnapso\u00ebs", "tokens": ["Nied\u00b7rig", "ist", "der", "Rausch", "des", "Schnap\u00b7so\u00ebs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kein Mensch hat Ruhm davon,", "tokens": ["Und", "kein", "Mensch", "hat", "Ruhm", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Johann Raps, des alten Rapso\u00ebs", "tokens": ["Jo\u00b7hann", "Raps", ",", "des", "al\u00b7ten", "Rap\u00b7so\u00ebs"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sohn war ein mi\u00dfrathner Sohn.", "tokens": ["Sohn", "war", "ein", "mi\u00df\u00b7ra\u00b7th\u00b7ner", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "me