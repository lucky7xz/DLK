{"textgrid.poem.54263": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich einbildete / sie w\u00e4re sch\u00f6n", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Climene, wenn du denckst, da\u00df du so sch\u00f6ne bist,", "tokens": ["Cli\u00b7me\u00b7ne", ",", "wenn", "du", "denckst", ",", "da\u00df", "du", "so", "sch\u00f6\u00b7ne", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denckst du wie der Hund, der das Gesp-- fri\u00dft,", "tokens": ["So", "denckst", "du", "wie", "der", "Hund", ",", "der", "das", "Ge\u00b7sp\u00b7", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.3": {"text": "Derselbe bild sich ein, er hab ein sch\u00f6n Gerichte.", "tokens": ["Der\u00b7sel\u00b7be", "bild", "sich", "ein", ",", "er", "hab", "ein", "sch\u00f6n", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PRF", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du fliehst zum Spiegel hin, und hast kein sch\u00f6n Gesichte.", "tokens": ["Du", "fliehst", "zum", "Spie\u00b7gel", "hin", ",", "und", "hast", "kein", "sch\u00f6n", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "KON", "VAFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach packe, bitt ich dich, mit deiner Sch\u00f6nheit ein,", "tokens": ["Ach", "pa\u00b7cke", ",", "bitt", "ich", "dich", ",", "mit", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach der kein Paris leicht wohl d\u00fcrffte l\u00fcstern seyn.", "tokens": ["Nach", "der", "kein", "Pa\u00b7ris", "leicht", "wohl", "d\u00fcrff\u00b7te", "l\u00fcs\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NE", "ADJD", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer dich genau beguckt, wird auf den Argwohn kommen,", "tokens": ["Wer", "dich", "ge\u00b7nau", "be\u00b7guckt", ",", "wird", "auf", "den", "Arg\u00b7wohn", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "$,", "VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du h\u00e4ttst von Hecuben die Masque vorgenommen.", "tokens": ["Du", "h\u00e4ttst", "von", "He\u00b7cu\u00b7ben", "die", "Mas\u00b7que", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP