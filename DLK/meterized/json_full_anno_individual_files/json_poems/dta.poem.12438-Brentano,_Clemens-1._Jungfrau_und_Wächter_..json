{"dta.poem.12438": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1.  Jungfrau und W\u00e4chter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von hoher Art ein Fr\u00e4ulein zart,                 ", "tokens": ["Von", "ho\u00b7her", "Art", "ein", "Fr\u00e4u\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt ich dem W\u00e4chter klagen,", "tokens": ["H\u00f6rt", "ich", "dem", "W\u00e4ch\u00b7ter", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Herzens-Qual, zum erstenmal", "tokens": ["Aus", "Her\u00b7zens\u00b7Qual", ",", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wollt sie die Liebe wagen,", "tokens": ["Wollt", "sie", "die", "Lie\u00b7be", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sprach: \u201eGeselle mein Ungef\u00e4lle", "tokens": ["Sie", "sprach", ":", "\u201e", "Ge\u00b7sel\u00b7le", "mein", "Un\u00b7ge\u00b7f\u00e4l\u00b7le"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201eist nah und bringt mir Schmerzen,", "tokens": ["\u201e", "ist", "nah", "und", "bringt", "mir", "Schmer\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201each W\u00e4chter gut, ein argen Muth", "tokens": ["\u201e", "ach", "W\u00e4ch\u00b7ter", "gut", ",", "ein", "ar\u00b7gen", "Muth"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "XY", "NE", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201etrag ich in meinem Herzen.\u201c", "tokens": ["\u201e", "trag", "ich", "in", "mei\u00b7nem", "Her\u00b7zen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eeinem werthen Mann, dem w\u00fcnsch ich an,", "tokens": ["\u201e", "ei\u00b7nem", "wert\u00b7hen", "Mann", ",", "dem", "w\u00fcnsch", "ich", "an", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u201eviel Gl\u00fcck und Heil mit Treuen,", "tokens": ["\u201e", "viel", "Gl\u00fcck", "und", "Heil", "mit", "Treu\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201esein Tugend gro\u00df findt niemand blos,", "tokens": ["\u201e", "sein", "Tu\u00b7gend", "gro\u00df", "findt", "nie\u00b7mand", "blos", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJD", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eauf ihn ist wohl zu bauen,", "tokens": ["\u201e", "auf", "ihn", "ist", "wohl", "zu", "bau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u201eda\u00df er wohl sey alles Wandels frey,", "tokens": ["\u201e", "da\u00df", "er", "wohl", "sey", "al\u00b7les", "Wan\u00b7dels", "frey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201eein Mann von hohen Ehren.\u201c", "tokens": ["\u201e", "ein", "Mann", "von", "ho\u00b7hen", "Eh\u00b7ren", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201eo W\u00e4chter mein, mag es wohl seyn,", "tokens": ["\u201e", "o", "W\u00e4ch\u00b7ter", "mein", ",", "mag", "es", "wohl", "seyn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "PPOSAT", "$,", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "\u201eso hilf mir Freude mehren.", "tokens": ["\u201e", "so", "hilf", "mir", "Freu\u00b7de", "meh\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVIMP", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201egut, W\u00e4chter! ich kann ihn ohne dich,", "tokens": ["\u201e", "gut", ",", "W\u00e4ch\u00b7ter", "!", "ich", "kann", "ihn", "oh\u00b7ne", "dich", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NE", "$.", "PPER", "VMFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201ein mein Gemach nicht bringen,", "tokens": ["\u201e", "in", "mein", "Ge\u00b7mach", "nicht", "brin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eo wolle mir nach meiner Begier,", "tokens": ["\u201e", "o", "wol\u00b7le", "mir", "nach", "mei\u00b7ner", "Be\u00b7gier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u201emein Leid nun helfen wenden,", "tokens": ["\u201e", "mein", "Leid", "nun", "hel\u00b7fen", "wen\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eich sag f\u00fcrwahr, da\u00df immerdar", "tokens": ["\u201e", "ich", "sag", "f\u00fcr\u00b7wahr", ",", "da\u00df", "im\u00b7mer\u00b7dar"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201emit Gab ich dir's vergelte,", "tokens": ["\u201e", "mit", "Gab", "ich", "dir's", "ver\u00b7gel\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "VVFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ek\u00f6mmt er herbey, gut W\u00e4chter frey,", "tokens": ["\u201e", "k\u00f6mmt", "er", "her\u00b7bey", ",", "gut", "W\u00e4ch\u00b7ter", "frey", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "NE", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "\u201eden Gast gen niemand melde.\u201c", "tokens": ["\u201e", "den", "Gast", "gen", "nie\u00b7mand", "mel\u00b7de", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}