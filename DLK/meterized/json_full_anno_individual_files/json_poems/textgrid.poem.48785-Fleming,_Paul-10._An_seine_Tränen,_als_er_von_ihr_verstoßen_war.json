{"textgrid.poem.48785": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. An seine Tr\u00e4nen, als er von ihr versto\u00dfen war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flie\u00dft, flie\u00dft so, wie ihr tut, ihr zweier Brunnen B\u00e4che,", "tokens": ["Flie\u00dft", ",", "flie\u00dft", "so", ",", "wie", "ihr", "tut", ",", "ihr", "zwei\u00b7er", "Brun\u00b7nen", "B\u00e4\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "flie\u00dft ferner, wie bisher mit zweimal st\u00e4rkrer Flut,", "tokens": ["flie\u00dft", "fer\u00b7ner", ",", "wie", "bis\u00b7her", "mit", "zwei\u00b7mal", "st\u00e4r\u00b7krer", "Flut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "flie\u00dft, wie ihr habt getan und wie ihr itzt noch tut,", "tokens": ["flie\u00dft", ",", "wie", "ihr", "habt", "ge\u00b7tan", "und", "wie", "ihr", "itzt", "noch", "tut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "KON", "PWAV", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df ich mich recht an der, die euch erpresset, r\u00e4che.", "tokens": ["da\u00df", "ich", "mich", "recht", "an", "der", ",", "die", "euch", "er\u00b7pres\u00b7set", ",", "r\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Flie\u00dft immer Nacht und Tag, ob sich ihr Sinn, der freche,", "tokens": ["Flie\u00dft", "im\u00b7mer", "Nacht", "und", "Tag", ",", "ob", "sich", "ihr", "Sinn", ",", "der", "fre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,", "KOUS", "PRF", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der feindgesinnte Freund, das hochgeherzte Blut,", "tokens": ["der", "feind\u00b7ge\u00b7sinn\u00b7te", "Freund", ",", "das", "hoch\u00b7ge\u00b7herz\u00b7te", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das mich um dieses ha\u00dft, dieweil ich ihm bin gut,", "tokens": ["das", "mich", "um", "die\u00b7ses", "ha\u00dft", ",", "die\u00b7weil", "ich", "ihm", "bin", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PDAT", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "durch eine Stetigkeit und gro\u00dfe St\u00e4rke breche.", "tokens": ["durch", "ei\u00b7ne", "Ste\u00b7tig\u00b7keit", "und", "gro\u00b7\u00dfe", "St\u00e4r\u00b7ke", "bre\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Tropfen waschen aus den festen Marmelstein,", "tokens": ["Die", "Trop\u00b7fen", "wa\u00b7schen", "aus", "den", "fes\u00b7ten", "Mar\u00b7mel\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das weiche Wasser zwingt das harte Helfenbein,", "tokens": ["das", "wei\u00b7che", "Was\u00b7ser", "zwingt", "das", "har\u00b7te", "Hel\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auch Eisen und Demant mu\u00df feuchten Sachen weichen.", "tokens": ["auch", "Ei\u00b7sen", "und", "De\u00b7mant", "mu\u00df", "feuch\u00b7ten", "Sa\u00b7chen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Flie\u00dft ewig, wie ihr flie\u00dft! Es ist ja m\u00fcglich, nicht,", "tokens": ["Flie\u00dft", "e\u00b7wig", ",", "wie", "ihr", "flie\u00dft", "!", "Es", "ist", "ja", "m\u00fcg\u00b7lich", ",", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u0