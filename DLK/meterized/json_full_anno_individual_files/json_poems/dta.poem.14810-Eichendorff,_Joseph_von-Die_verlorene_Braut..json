{"dta.poem.14810": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die verlorene Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sprach: \u201eWie wirres Klingen", "tokens": ["Sie", "sprach", ":", "\u201e", "Wie", "wir\u00b7res", "Klin\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt durch die Einsamkeit,", "tokens": ["Kommt", "durch", "die", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Lied wohl h\u00f6rt' ich singen", "tokens": ["Das", "Lied", "wohl", "h\u00f6rt'", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In alter, sch\u00f6ner Zeit.\u201c", "tokens": ["In", "al\u00b7ter", ",", "sch\u00f6\u00b7ner", "Zeit", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}