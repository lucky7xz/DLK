{"textgrid.poem.57586": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Heide der Ostwind zieht,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "der", "Ost\u00b7wind", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Kiefern rauschen leise,", "tokens": ["Die", "Kie\u00b7fern", "rau\u00b7schen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Goldammer singt ihr blasiertes Lied,", "tokens": ["Die", "Gold\u00b7am\u00b7mer", "singt", "ihr", "bla\u00b7sier\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die alte, gleichg\u00fcltige Weise:", "tokens": ["Die", "al\u00b7te", ",", "gleich\u00b7g\u00fcl\u00b7ti\u00b7ge", "Wei\u00b7se", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Fr\u00fchling ist tot, die Blumen sind krank,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "tot", ",", "die", "Blu\u00b7men", "sind", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verstummt ist der lustige Finkengesang,", "tokens": ["Ver\u00b7stummt", "ist", "der", "lus\u00b7ti\u00b7ge", "Fin\u00b7ken\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Nur die Ammer noch singt voller Traurigkeit:", "tokens": ["Nur", "die", "Am\u00b7mer", "noch", "singt", "vol\u00b7ler", "Trau\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "\u00bbmein Nest ist so weit, so weit.\u00ab", "tokens": ["\u00bb", "mein", "Nest", "ist", "so", "weit", ",", "so", "weit", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Von meiner Brust ein Seufzer weicht \u2013", "tokens": ["Von", "mei\u00b7ner", "Brust", "ein", "Seuf\u00b7zer", "weicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu der Kampf und die S\u00fcnde?", "tokens": ["Wo\u00b7zu", "der", "Kampf", "und", "die", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hab' nie den Saum des Gl\u00fccks erreicht,", "tokens": ["Hab'", "nie", "den", "Saum", "des", "Gl\u00fccks", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word