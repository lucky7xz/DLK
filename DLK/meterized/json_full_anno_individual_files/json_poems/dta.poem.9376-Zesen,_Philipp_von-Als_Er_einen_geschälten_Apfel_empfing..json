{"dta.poem.9376": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als Er einen gesch\u00e4lten Apfel empfing.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste/ soll der Apfel hier/ den sie mier gegeben/", "tokens": ["Sch\u00f6ns\u00b7te", "/", "soll", "der", "Ap\u00b7fel", "hier", "/", "den", "sie", "mier", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VMFIN", "ART", "NN", "ADV", "$(", "ART", "PPER", "PPER", "VVPP", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jhrer Liebe zeichen seyn", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", "zei\u00b7chen", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "nur allein?", "tokens": ["nur", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Oder wil Sie opfern mier in Best\u00e4ndigkeit jhr Leben?", "tokens": ["O\u00b7der", "wil", "Sie", "op\u00b7fern", "mier", "in", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "jhr", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "ist der apfel rund?", "tokens": ["ist", "der", "ap\u00b7fel", "rund", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ey so wird die Treue kunt;", "tokens": ["Ey", "so", "wird", "die", "Treu\u00b7e", "kunt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn was rund ist/ hatt kein Ende.", "tokens": ["Denn", "was", "rund", "ist", "/", "hatt", "kein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$(", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr gem\u00fcth ist ohne falsch/ dr\u00fcmb ist er geschelet/", "tokens": ["Ihr", "ge\u00b7m\u00fcth", "ist", "oh\u00b7ne", "falsch", "/", "dr\u00fcmb", "ist", "er", "ge\u00b7sche\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ADJD", "$(", "PAV", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+----", "measure": "unknown.measure.penta"}, "line.2": {"text": "aller tr\u00fcg- und glei\u00dfnerey", "tokens": ["al\u00b7ler", "tr\u00fcg", "und", "glei\u00df\u00b7ne\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "TRUNC", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ist Sie frey/", "tokens": ["ist", "Sie", "frey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "dr\u00fcmb ich zur Freundin Sie schon vorl\u00e4ngst erwehlet/", "tokens": ["dr\u00fcmb", "ich", "zur", "Freun\u00b7din", "Sie", "schon", "vor\u00b7l\u00e4ngst", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "NN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "da\u00df der apfel steht", "tokens": ["da\u00df", "der", "ap\u00b7fel", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "auf der spitzen und sich dreht/", "tokens": ["auf", "der", "spit\u00b7zen", "und", "sich", "dreht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "KON", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "ist die Lieb im Tod\u2019 und Leiden.", "tokens": ["ist", "die", "Lieb", "im", "Tod'", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}