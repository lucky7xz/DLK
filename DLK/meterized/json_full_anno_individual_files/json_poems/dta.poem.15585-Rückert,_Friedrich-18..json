{"dta.poem.15585": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der nat\u00fcrlichen Religion geboren", "tokens": ["In", "der", "na\u00b7t\u00fcr\u00b7li\u00b7chen", "Re\u00b7li\u00b7gi\u00b7on", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wird jeder Mensch, und nie geht sie ihm ganz verloren.", "tokens": ["Wird", "je\u00b7der", "Mensch", ",", "und", "nie", "geht", "sie", "ihm", "ganz", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihm angezogen wird ein \u00e4u\u00dfres Glaubenthum,", "tokens": ["Ihm", "an\u00b7ge\u00b7zo\u00b7gen", "wird", "ein", "\u00e4u\u00df\u00b7res", "Glau\u00b7ben\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nimmt im Leben er wie einen Mantel um.", "tokens": ["Das", "nimmt", "im", "Le\u00b7ben", "er", "wie", "ei\u00b7nen", "Man\u00b7tel", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PPER", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er trag' es, weil er lebt; im Tode legt ers ab,", "tokens": ["Er", "trag'", "es", ",", "weil", "er", "lebt", ";", "im", "To\u00b7de", "legt", "ers", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$.", "APPRART", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da bleibt der Glauben ihm, den Gott ihm selber gab.", "tokens": ["Da", "bleibt", "der", "Glau\u00b7ben", "ihm", ",", "den", "Gott", "ihm", "sel\u00b7ber", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}