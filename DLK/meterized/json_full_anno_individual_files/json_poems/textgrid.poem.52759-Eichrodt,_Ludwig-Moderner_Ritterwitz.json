{"textgrid.poem.52759": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Moderner Ritterwitz", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah sie unter Fratzen statt Gesichtern,", "tokens": ["Ich", "sah", "sie", "un\u00b7ter", "Frat\u00b7zen", "statt", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entbehrend eines Herzens gr\u00fcnen Gru\u00df,", "tokens": ["Ent\u00b7beh\u00b7rend", "ei\u00b7nes", "Her\u00b7zens", "gr\u00fc\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie unter einer Brut von Afterdichtern,", "tokens": ["Wie", "un\u00b7ter", "ei\u00b7ner", "Brut", "von", "Af\u00b7ter\u00b7dich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Feuergenius ersticken mu\u00df!!", "tokens": ["Ein", "Feu\u00b7er\u00b7ge\u00b7nius", "er\u00b7sti\u00b7cken", "mu\u00df", "!!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mir bluteten die sporngeschm\u00fcckten Fersen:", "tokens": ["Mir", "blu\u00b7te\u00b7ten", "die", "sporn\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Fer\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fort ritt' ich, ha! in w\u00fcster Wetternacht.", "tokens": ["Fort", "ritt'", "ich", ",", "ha", "!", "in", "w\u00fcs\u00b7ter", "Wet\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ITJ", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich den Gedanken, meinen wilddiversen,", "tokens": ["Gleich", "den", "Ge\u00b7dan\u00b7ken", ",", "mei\u00b7nen", "wild\u00b7di\u00b7ver\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "B\u00e4umt sich der Hengst, als ging es in die Schlacht.", "tokens": ["B\u00e4umt", "sich", "der", "Hengst", ",", "als", "ging", "es", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das stolze Thier verletzt am rauhen Kies,", "tokens": ["Das", "stol\u00b7ze", "Thier", "ver\u00b7letzt", "am", "rau\u00b7hen", "Kies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das edle. O, zur\u00fcck jetzt in die l\u00fcst'ren", "tokens": ["Das", "ed\u00b7le", ".", "O", ",", "zu\u00b7r\u00fcck", "jetzt", "in", "die", "l\u00fcst'\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$.", "NE", "$,", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Glanzfluthenden Gem\u00e4cher, die ich lie\u00df,", "tokens": ["Glanz\u00b7flut\u00b7hen\u00b7den", "Ge\u00b7m\u00e4\u00b7cher", ",", "die", "ich", "lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Jetzt oder nie mu\u00df sich das Bild entd\u00fcstren.", "tokens": ["Jetzt", "o\u00b7der", "nie", "mu\u00df", "sich", "das", "Bild", "ent\u00b7d\u00fcs\u00b7tren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch ging die Strahlende nicht zum Altar,", "tokens": ["Noch", "ging", "die", "Strah\u00b7len\u00b7de", "nicht", "zum", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verlobung nur ist, was sie heute feiern:", "tokens": ["Ver\u00b7lo\u00b7bung", "nur", "ist", ",", "was", "sie", "heu\u00b7te", "fei\u00b7ern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jetzt klirr' ich mitten in die schale Schaar \u2013", "tokens": ["Jetzt", "klirr'", "ich", "mit\u00b7ten", "in", "die", "scha\u00b7le", "Schaar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure"