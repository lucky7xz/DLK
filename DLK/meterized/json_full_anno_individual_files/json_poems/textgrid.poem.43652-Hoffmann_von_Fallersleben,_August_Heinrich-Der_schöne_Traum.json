{"textgrid.poem.43652": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6ne Traum", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun steht in frischer Gr\u00fcne", "tokens": ["Nun", "steht", "in", "fri\u00b7scher", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hohe Rosenstrauch,", "tokens": ["Der", "ho\u00b7he", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und um die Knospen spielet", "tokens": ["Und", "um", "die", "Knos\u00b7pen", "spie\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein linder Morgenhauch.", "tokens": ["Ein", "lin\u00b7der", "Mor\u00b7gen\u00b7hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und jede schl\u00e4ft und tr\u00e4umet", "tokens": ["Und", "je\u00b7de", "schl\u00e4ft", "und", "tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ihrem gr\u00fcnen Schrein,", "tokens": ["In", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Schrein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und denkt: es wird hienieden", "tokens": ["Und", "denkt", "