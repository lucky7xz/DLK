{"dta.poem.17984": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "227.  Der Weisen Kr\u00e4nkung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wei\u00df\u2019 ist st\u00e4ts in Freud/ er wird von nichts betr\u00fcbt.", "tokens": ["Der", "Wei\u00df'", "ist", "st\u00e4ts", "in", "Freud", "/", "er", "wird", "von", "nichts", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "$(", "PPER", "VAFIN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df einge kr\u00e4nkt ihn nur da\u00df GOtt nicht wird geliebt.", "tokens": ["Di\u00df", "ein\u00b7ge", "kr\u00e4nkt", "ihn", "nur", "da\u00df", "Gott", "nicht", "wird", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "ADV", "KOUS", "NN", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}