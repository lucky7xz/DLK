{"dta.poem.14089": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiwerber .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ringsum viel' Schl\u00f6sser schimmernd steh'n,", "tokens": ["Ring\u00b7sum", "viel'", "Schl\u00f6s\u00b7ser", "schim\u00b7mernd", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So silbern geht der Str\u00f6me Lauf,", "tokens": ["So", "sil\u00b7bern", "geht", "der", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch, weit rings Lerchenlieder weh'n,", "tokens": ["Hoch", ",", "weit", "rings", "Ler\u00b7chen\u00b7lie\u00b7der", "weh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlie\u00df' Fenster, Herz und Aeuglein auf!", "tokens": ["Schlie\u00df'", "Fens\u00b7ter", ",", "Herz", "und", "A\u00b7e\u00b7u\u00b7glein", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}