{"textgrid.poem.65640": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Alexander Ypsilanti aus Munkacs", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alexander Ypsilanti sa\u00df in Munkacs", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "Yp\u00b7si\u00b7lan\u00b7ti", "sa\u00df", "in", "Mun\u00b7kacs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "An den morschen Fenstergittern r\u00fcttelte der wilde Sturm,", "tokens": ["An", "den", "mor\u00b7schen", "Fens\u00b7ter\u00b7git\u00b7tern", "r\u00fct\u00b7tel\u00b7te", "der", "wil\u00b7de", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schwarze Wolkenz\u00fcge flogen \u00fcber Mond und Sterne hin \u2013", "tokens": ["Schwar\u00b7ze", "Wol\u00b7ken\u00b7z\u00fc\u00b7ge", "flo\u00b7gen", "\u00fc\u00b7ber", "Mond", "und", "Ster\u00b7ne", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und der Griechenf\u00fcrst erseufzte: Ach, da\u00df ich gefangen bin!", "tokens": ["Und", "der", "Grie\u00b7chen\u00b7f\u00fcrst", "er\u00b7seufz\u00b7te", ":", "Ach", ",", "da\u00df", "ich", "ge\u00b7fan\u00b7gen", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "ITJ", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "An des Mittags Horizonte hing sein Auge unverwandt:", "tokens": ["An", "des", "Mit\u00b7tags", "Ho\u00b7ri\u00b7zon\u00b7te", "hing", "sein", "Au\u00b7ge", "un\u00b7ver\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "L\u00e4g' ich doch in deiner Erde, mein geliebtes Vaterland!", "tokens": ["L\u00e4g'", "ich", "doch", "in", "dei\u00b7ner", "Er\u00b7de", ",", "mein", "ge\u00b7lieb\u00b7tes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und er \u00f6ffnete das Fenster, sah in's \u00f6de Land hinein;", "tokens": ["Und", "er", "\u00f6ff\u00b7ne\u00b7te", "das", "Fens\u00b7ter", ",", "sah", "in's", "\u00f6\u00b7de", "Land", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Kr\u00e4hen schw\u00e4rmten in den Gr\u00fcnden, Adler um das Felsgestein.", "tokens": ["Kr\u00e4\u00b7hen", "schw\u00e4rm\u00b7ten", "in", "den", "Gr\u00fcn\u00b7den", ",", "Ad\u00b7ler", "um", "das", "Fels\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wieder fing er an zu seufzen: Bringt mir Keiner Botschaft her", "tokens": ["Wie\u00b7der", "fing", "er", "an", "zu", "seuf\u00b7zen", ":", "Bringt", "mir", "Kei\u00b7ner", "Bot\u00b7schaft", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$.", "VVFIN", "PPER", "PIAT", "NN", "APZR"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Aus dem Lande meiner V\u00e4ter? \u2013 Und die Wimper ward ihm schwer \u2013", "tokens": ["Aus", "dem", "Lan\u00b7de", "mei\u00b7ner", "V\u00e4\u00b7ter", "?", "\u2013", "Und", "die", "Wim\u00b7per", "ward", "ihm", "schwer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$.", "$(", "KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "War's von Thr\u00e4nen? war's von Schlummer? und sein Haupt sank in die Hand.", "tokens": ["Wa\u00b7r's", "von", "Thr\u00e4\u00b7nen", "?", "wa\u00b7r's", "von", "Schlum\u00b7mer", "?", "und", "sein", "Haupt", "sank", "in", "die", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$.", "VAFIN", "APPR", "NN", "$.", "KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.12": {"text": "Seht, sein Antlitz wird so helle \u2013 Tr\u00e4umt er von dem Vaterland?", "tokens": ["Seht", ",", "sein", "Ant\u00b7litz", "wird", "so", "hel\u00b7le", "\u2013", "Tr\u00e4umt", "er", "von", "dem", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Also sa\u00df er, und zum Schl\u00e4fer trat ein schlichter Heldenmann,", "tokens": ["Al\u00b7so", "sa\u00df", "er", ",", "und", "zum", "Schl\u00e4\u00b7fer", "trat", "ein", "schlich\u00b7ter", "Hel\u00b7den\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Sah mit freudig ernstem Blicke lange den Betr\u00fcbten an:", "tokens": ["Sah", "mit", "freu\u00b7dig", "erns\u00b7tem", "Bli\u00b7cke", "lan\u00b7ge", "den", "Be\u00b7tr\u00fcb\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Alexander Ypsilanti, sei gegr\u00fc\u00dft und fasse Muth!", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "Yp\u00b7si\u00b7lan\u00b7ti", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "und", "fas\u00b7se", "Muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VAFIN", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "In dem engen Felsenpasse, wo geflossen ist mein Blut,", "tokens": ["In", "dem", "en\u00b7gen", "Fel\u00b7sen\u00b7pas\u00b7se", ",", "wo", "ge\u00b7flos\u00b7sen", "ist", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Wo in ", "tokens": ["Wo", "in"], "token_info": ["word", "word"], "pos": ["PWAV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Haben \u00fcber die Barbaren freie Griechen heut' gesiegt", "tokens": ["Ha\u00b7ben", "\u00fc\u00b7ber", "die", "Bar\u00b7ba\u00b7ren", "frei\u00b7e", "Grie\u00b7chen", "heut'", "ge\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN", "ADV", "VVPP"], "meter": "+-+-----+-+-+-+", "measure": "unknown.measure.hexa"}, "line.19": {"text": "Diese Botschaft dir zu bringen ward mein Geist herabgesandt.", "tokens": ["Die\u00b7se", "Bot\u00b7schaft", "dir", "zu", "brin\u00b7gen", "ward", "mein", "Geist", "her\u00b7ab\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "PTKZU", "VVINF", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Alexander Ypsilanti, frei wird Hellas heil'ges Land!", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "Yp\u00b7si\u00b7lan\u00b7ti", ",", "frei", "wird", "Hel\u00b7las", "heil'\u00b7ges", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "VAFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Da erwacht der F\u00fcrst vom Schlummer, ruft entz\u00fcckt: Leonidas!", "tokens": ["Da", "er\u00b7wacht", "der", "F\u00fcrst", "vom", "Schlum\u00b7mer", ",", "ruft", "ent\u00b7z\u00fcckt", ":", "Leo\u00b7ni\u00b7das", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "VVPP", "$.", "NE", "$."], "meter": "+-+-+-+-+-++-+", "measure": "unknown.measure.octa.plus"}, "line.22": {"text": "Und er f\u00fchlt, von Freudenthr\u00e4nen sind ihm Aug' und Wange na\u00df.", "tokens": ["Und", "er", "f\u00fchlt", ",", "von", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "sind", "ihm", "Aug'", "und", "Wan\u00b7ge", "na\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Horch, es rauscht ob seinem Haupte, und ein K\u00f6nigsadler fliegt", "tokens": ["Horch", ",", "es", "rauscht", "ob", "sei\u00b7nem", "Haup\u00b7te", ",", "und", "ein", "K\u00f6\u00b7ni\u00b7gsad\u00b7ler", "fliegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Aus dem Fenster, und die Schwingen in dem Mondenstrahl er wiegt.", "tokens": ["Aus", "dem", "Fens\u00b7ter", ",", "und", "die", "Schwin\u00b7gen", "in", "dem", "Mon\u00b7dens\u00b7trahl", "er", "wiegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Alexander Ypsilanti sa\u00df in Munkacs", "tokens": ["A\u00b7lex\u00b7an\u00b7der", "Yp\u00b7si\u00b7lan\u00b7ti", "sa\u00df", "in", "Mun\u00b7kacs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", 