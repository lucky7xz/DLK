{"textgrid.poem.29059": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, der ich Haus und Stadt im Kriege hulffe st\u00fcrmen,", "tokens": ["Ich", ",", "der", "ich", "Haus", "und", "Stadt", "im", "Krie\u00b7ge", "hulf\u00b7fe", "st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bau ietzund hier ein Haus: so sieget Zeit und W\u00fcrmen", "tokens": ["Bau", "ie\u00b7tzund", "hier", "ein", "Haus", ":", "so", "sie\u00b7get", "Zeit", "und", "W\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Namen ziemlich ob. Nach viermal f\u00fcnfftem Jahr,", "tokens": ["Mein", "Na\u00b7men", "ziem\u00b7lich", "ob", ".", "Nach", "vier\u00b7mal", "f\u00fcnff\u00b7tem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KOUS", "$.", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da deutsch und schwedisch Haupt nun wieder friedlich war,", "tokens": ["Da", "deutsch", "und", "schwe\u00b7disch", "Haupt", "nun", "wie\u00b7der", "fried\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ward dieser Grund gelegt; die M\u00fcntze beyder Parten", "tokens": ["Ward", "die\u00b7ser", "Grund", "ge\u00b7legt", ";", "die", "M\u00fcnt\u00b7ze", "bey\u00b7der", "Par\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "VVPP", "$.", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Liegt zum Ged\u00e4chtn\u00fc\u00df bey. Geh, wilstu mehr nicht warten,", "tokens": ["Liegt", "zum", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "bey", ".", "Geh", ",", "wils\u00b7tu", "mehr", "nicht", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$.", "NE", "$,", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der du hieher gelangt: hier steht ein Glas voll Wein,", "tokens": ["Der", "du", "hie\u00b7her", "ge\u00b7langt", ":", "hier", "steht", "ein", "Glas", "voll", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PAV", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Trinck, bilde dir dabey, was dir beliebet, ein!", "tokens": ["Trinck", ",", "bil\u00b7de", "dir", "da\u00b7bey", ",", "was", "dir", "be\u00b7lie\u00b7bet", ",", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PAV", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa