{"dta.poem.7740": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schauspielerin Natur tritt auf in allen Rollen", "tokens": ["Schau\u00b7spie\u00b7le\u00b7rin", "Na\u00b7tur", "tritt", "auf", "in", "al\u00b7len", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorm Geist, die t\u00e4uschen ihn und ihn ergetzen sollen.", "tokens": ["Vorm", "Geist", ",", "die", "t\u00e4u\u00b7schen", "ihn", "und", "ihn", "er\u00b7get\u00b7zen", "sol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVFIN", "PPER", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn sie sich erkannt in jeder Maske sieht,", "tokens": ["Und", "wenn", "sie", "sich", "er\u00b7kannt", "in", "je\u00b7der", "Mas\u00b7ke", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tritt sie besch\u00e4mt zur\u00fcck, und alle T\u00e4uschung flieht.", "tokens": ["Tritt", "sie", "be\u00b7sch\u00e4mt", "zu\u00b7r\u00fcck", ",", "und", "al\u00b7le", "T\u00e4u\u00b7schung", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "PIAT", "NN", "VVFIN", "