{"dta.poem.11164": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine ins kloster gehende courtisanin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00fcllest muth und gluth in einer kutten ein,", "tokens": ["Du", "h\u00fcl\u00b7lest", "muth", "und", "gluth", "in", "ei\u00b7ner", "kut\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wilst hinf\u00fcro stein in fleisch und blute seyn.", "tokens": ["Und", "wilst", "hin\u00b7f\u00fc\u00b7ro", "stein", "in", "fleisch", "und", "blu\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "ADJD", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer nach der ursach fragt, der nehme diese lehre:", "tokens": ["Wer", "nach", "der", "ur\u00b7sach", "fragt", ",", "der", "neh\u00b7me", "die\u00b7se", "leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es fehlte Teutschland noch an einer Valiere.", "tokens": ["Es", "fehl\u00b7te", "Teutschland", "noch", "an", "ei\u00b7ner", "Va\u00b7lie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "-