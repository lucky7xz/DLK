{"textgrid.poem.46621": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es war in meinem Hause]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war in meinem Hause", "tokens": ["Es", "war", "in", "mei\u00b7nem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch niemals mir zum Grause", "tokens": ["Noch", "nie\u00b7mals", "mir", "zum", "Grau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geworden der Gedanke,", "tokens": ["Ge\u00b7wor\u00b7den", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wieviel in dieser Schranke", "tokens": ["Wie\u00b7viel", "in", "die\u00b7ser", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon ihres Lebens Spiele", "tokens": ["Schon", "ih\u00b7res", "Le\u00b7bens", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Getrieben bis zum Ziele.", "tokens": ["Ge\u00b7trie\u00b7ben", "bis", "zum", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie man darin geworben", "tokens": ["Wie", "man", "da\u00b7rin", "ge\u00b7wor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor mir und drin gestorben,", "tokens": ["Vor", "mir", "und", "drin", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist in diesen Hallen", "tokens": ["Das", "ist", "in", "die\u00b7sen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir niemals eingefallen,", "tokens": ["Mir", "nie\u00b7mals", "ein\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die nur gebaut mir schienen,", "tokens": ["Die", "nur", "ge\u00b7baut", "mir", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Leben mir zu dienen.", "tokens": ["Zum", "Le\u00b7ben", "mir", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun ist mir's auch zum Grause", "tokens": ["Nun", "ist", "mir's", "auch", "zum", "Grau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geworden nicht im Hause,", "tokens": ["Ge\u00b7wor\u00b7den", "nicht", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df drin die Meinen starben,", "tokens": ["Da\u00df", "drin", "die", "Mei\u00b7nen", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch scheinen andre Farben", "tokens": ["Doch", "schei\u00b7nen", "and\u00b7re", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu tragen nun die Zimmer,", "tokens": ["Zu", "tra\u00b7gen", "nun", "die", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht mehr so frischen Schimmer.", "tokens": ["Nicht", "mehr", "so", "fri\u00b7schen", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist als ob ein Schatte", "tokens": ["Es", "ist", "als", "ob", "ein", "Schat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich mit dem Lichte gatte,", "tokens": ["Sich", "mit", "dem", "Lich\u00b7te", "gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das durch die Fenster strahlet", "tokens": ["Das", "durch", "die", "Fens\u00b7ter", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tr