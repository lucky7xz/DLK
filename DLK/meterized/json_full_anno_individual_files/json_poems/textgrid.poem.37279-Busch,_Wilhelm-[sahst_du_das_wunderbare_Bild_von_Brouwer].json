{"textgrid.poem.37279": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[sahst du das wunderbare Bild von Brouwer]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sahst du das wunderbare Bild von Brouwer?", "tokens": ["Sahst", "du", "das", "wun\u00b7der\u00b7ba\u00b7re", "Bild", "von", "Brou\u00b7wer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es zieht dich an wie ein Magnet.", "tokens": ["Es", "zieht", "dich", "an", "wie", "ein", "Mag\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du l\u00e4chelst wohl, derweil ein Schreckensschauer", "tokens": ["Du", "l\u00e4\u00b7chelst", "wohl", ",", "der\u00b7weil", "ein", "Schre\u00b7ckens\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch deine Wirbels\u00e4ule geht.", "tokens": ["Durch", "dei\u00b7ne", "Wir\u00b7bel\u00b7s\u00e4u\u00b7le", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein k\u00fchler Doktor \u00f6ffnet einem Manne", "tokens": ["Ein", "k\u00fch\u00b7ler", "Dok\u00b7tor", "\u00f6ff\u00b7net", "ei\u00b7nem", "Man\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schw\u00e4re hinten im Genick;", "tokens": ["Die", "Schw\u00e4\u00b7re", "hin\u00b7ten", "im", "Ge\u00b7nick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Daneben steht ein Weib mit einer Kanne,", "tokens": ["Da\u00b7ne\u00b7ben", "steht", "ein", "Weib", "mit", "ei\u00b7ner", "Kan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vertieft in dieses Mi\u00dfgeschick.", "tokens": ["Ver\u00b7tieft", "in", "die\u00b7ses", "Mi\u00df\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, alter Freund, wir haben unsre Schw\u00e4re", "tokens": ["Ja", ",", "al\u00b7ter", "Freund", ",", "wir", "ha\u00b7ben", "uns\u00b7re", "Schw\u00e4\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Meist hinten. Und voll Seelenruh", "tokens": ["Meist", "hin\u00b7ten", ".", "Und", "voll", "See\u00b7len\u00b7ruh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcckt sie ein andrer auf. Es rinnt die Z\u00e4hre", "tokens": ["Dr\u00fcckt", "sie", "ein", "an\u00b7drer", "auf", ".", "Es", "rinnt", "die", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "