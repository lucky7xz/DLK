{"textgrid.poem.26457": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Keine arbeitsparende Maschine,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine arbeitsparende Maschine,", "tokens": ["Kei\u00b7ne", "ar\u00b7beits\u00b7pa\u00b7ren\u00b7de", "Ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Keine Entdeckung oder Erfindung", "tokens": ["Kei\u00b7ne", "Ent\u00b7de\u00b7ckung", "o\u00b7der", "Er\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00e4gt meinen Namen;", "tokens": ["Tr\u00e4gt", "mei\u00b7nen", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Auch werde ich keine Stiftung f\u00fcr ein Krankenhaus oder eine Bibliothek hinterlassen k\u00f6nnen,", "tokens": ["Auch", "wer\u00b7de", "ich", "kei\u00b7ne", "Stif\u00b7tung", "f\u00fcr", "ein", "Kran\u00b7ken\u00b7haus", "o\u00b7der", "ei\u00b7ne", "Bib\u00b7lio\u00b7thek", "hin\u00b7ter\u00b7las\u00b7sen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "KON", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+--+-+-+-+-+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Keine Heldentat f\u00fcr Amerika,", "tokens": ["Kei\u00b7ne", "Hel\u00b7den\u00b7tat", "f\u00fcr", "A\u00b7me\u00b7ri\u00b7ka", ","], "token_in