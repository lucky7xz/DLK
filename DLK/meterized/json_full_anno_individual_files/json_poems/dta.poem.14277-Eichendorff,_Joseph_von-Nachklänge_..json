{"dta.poem.14277": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Lust'ge V\u00f6gel in dem Wald,", "tokens": ["Lust'\u00b7ge", "V\u00f6\u00b7gel", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sing't, so lang es gr\u00fcn,", "tokens": ["Sing't", ",", "so", "lang", "es", "gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach wer wei\u00df, wie bald, wie bald", "tokens": ["Ach", "wer", "wei\u00df", ",", "wie", "bald", ",", "wie", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PWS", "VVFIN", "$,", "PWAV", "ADV", "$,", "PWAV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles mu\u00df verbl\u00fch'n!", "tokens": ["Al\u00b7les", "mu\u00df", "ver\u00b7bl\u00fch'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}