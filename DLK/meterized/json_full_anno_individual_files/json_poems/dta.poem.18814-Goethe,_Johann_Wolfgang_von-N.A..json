{"dta.poem.18814": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "In tausend Formen magst du dich verstecken,", "tokens": ["In", "tau\u00b7send", "For\u00b7men", "magst", "du", "dich", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch, Allerliebste, gleich erkenn\u2019 ich dich,", "tokens": ["Doch", ",", "Al\u00b7ler\u00b7liebs\u00b7te", ",", "gleich", "er\u00b7kenn'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du magst mit Zauberschleyern dich bedecken,", "tokens": ["Du", "magst", "mit", "Zau\u00b7ber\u00b7schley\u00b7ern", "dich", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allgegenw\u00e4rtige, gleich erkenn\u2019 ich dich.", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ge", ",", "gleich", "er\u00b7kenn'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "An der Cypresse reinstem, jungen Streben,", "tokens": ["An", "der", "Cyp\u00b7res\u00b7se", "reins\u00b7tem", ",", "jun\u00b7gen", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Allsch\u00f6ngewaschne, gleich erkenn\u2019 ich dich,", "tokens": ["All\u00b7sch\u00f6n\u00b7ge\u00b7waschne", ",", "gleich", "er\u00b7kenn'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In des Canales reinem Wellenleben,", "tokens": ["In", "des", "Ca\u00b7na\u00b7les", "rei\u00b7nem", "Wel\u00b7len\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allschmeichelhafte, wohl erkenn\u2019 ich dich.", "tokens": ["A\u00b7llschmei\u00b7chel\u00b7haf\u00b7te", ",", "wohl", "er\u00b7kenn'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn steigend sich der Wasserstrahl entfaltet,", "tokens": ["Wenn", "stei\u00b7gend", "sich", "der", "Was\u00b7ser\u00b7strahl", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allspielende, wie froh erkenn\u2019 ich dich.", "tokens": ["All\u00b7spie\u00b7len\u00b7de", ",", "wie", "froh", "er\u00b7kenn'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Wolke sich gestaltend umgestaltet,", "tokens": ["Wenn", "Wol\u00b7ke", "sich", "ge\u00b7stal\u00b7tend", "um\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allmannigfaltige, dort erkenn\u2019 ich dich.", "tokens": ["All\u00b7man\u00b7nig\u00b7fal\u00b7ti\u00b7ge", ",", "dort", "er\u00b7kenn'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "An des gebl\u00fcmten Schleyers Wiesenteppich,", "tokens": ["An", "des", "ge\u00b7bl\u00fcm\u00b7ten", "Schley\u00b7ers", "Wie\u00b7sen\u00b7tep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Allbuntbesternte, sch\u00f6n erkenn\u2019 ich dich.", "tokens": ["A\u00b7llbunt\u00b7bes\u00b7tern\u00b7te", ",", "sch\u00f6n", "er\u00b7kenn'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und greift umher ein tausendarmger Eppich,", "tokens": ["Und", "greift", "um\u00b7her", "ein", "tau\u00b7sen\u00b7darm\u00b7ger", "Ep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "O! Allumklammernde, da kenn\u2019 ich dich.", "tokens": ["O", "!", "Al\u00b7lum\u00b7klam\u00b7mern\u00b7de", ",", "da", "kenn'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn am Gebirg der Morgen sich entz\u00fcndet,", "tokens": ["Wenn", "am", "Ge\u00b7birg", "der", "Mor\u00b7gen", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich, Allerheiternde, begr\u00fcss\u2019 ich dich,", "tokens": ["Gleich", ",", "Al\u00b7ler\u00b7hei\u00b7tern\u00b7de", ",", "be\u00b7gr\u00fcss'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann \u00fcber mir der Himmel rein sich r\u00fcndet,", "tokens": ["Dann", "\u00fc\u00b7ber", "mir", "der", "Him\u00b7mel", "rein", "sich", "r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allherzerweiternde, dann athm\u2019 ich dich.", "tokens": ["All\u00b7her\u00b7zer\u00b7wei\u00b7tern\u00b7de", ",", "dann", "athm'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was ich mit \u00e4usserm Sinn, mit innerm kenne,", "tokens": ["Was", "ich", "mit", "\u00e4us\u00b7serm", "Sinn", ",", "mit", "in\u00b7nerm", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Allbelehrende, kenn\u2019 ich durch dich.", "tokens": ["Du", "All\u00b7be\u00b7leh\u00b7ren\u00b7de", ",", "kenn'", "ich", "durch", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wenn ich Allahs Namenhundert nenne,", "tokens": ["Und", "wenn", "ich", "Al\u00b7lahs", "Na\u00b7men\u00b7hun\u00b7dert", "nen\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit jedem klingt ein Name nach f\u00fcr dich.", "tokens": ["Mit", "je\u00b7dem", "klingt", "ein", "Na\u00b7me", "nach", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}