{"textgrid.poem.53354": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "2. Klaglied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war nun umb die Nacht und umb das volle Liecht,", "tokens": ["Es", "war", "nun", "umb", "die", "Nacht", "und", "umb", "das", "vol\u00b7le", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Monde wich' am Glantz auch schier der Sonnen nicht.", "tokens": ["Der", "Mon\u00b7de", "wich'", "am", "Glantz", "auch", "schier", "der", "Son\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie stehen auff, der Herr selbst vierter ist verschwiegen", "tokens": ["Sie", "ste\u00b7hen", "auff", ",", "der", "Herr", "selbst", "vier\u00b7ter", "ist", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Oeleberg hinauff sehr hoch betr\u00fcbt gestiegen,", "tokens": ["Den", "O\u00b7e\u00b7le\u00b7berg", "hin\u00b7auff", "sehr", "hoch", "be\u00b7tr\u00fcbt", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Den Berg, der ausserhalb der Stadt viel Oele tr\u00e4gt,", "tokens": ["Den", "Berg", ",", "der", "aus\u00b7ser\u00b7halb", "der", "Stadt", "viel", "O\u00b7e\u00b7le", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Vnd eine Garten-Lust auff seinem Gipffel hegt.", "tokens": ["Vnd", "ei\u00b7ne", "Gar\u00b7ten\u00b7Lust", "auff", "sei\u00b7nem", "Gipf\u00b7fel", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hie ist zu weinen fug, hie irrt ihn kein Get\u00fcmmel,", "tokens": ["Hie", "ist", "zu", "wei\u00b7nen", "fug", ",", "hie", "irrt", "ihn", "kein", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die reinen H\u00e4nde hebt er hie empor gen Himmel.", "tokens": ["Die", "rei\u00b7nen", "H\u00e4n\u00b7de", "hebt", "er", "hie", "em\u00b7por", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er denckt an die Gestalt der tausendfachen Noht,", "tokens": ["Er", "denckt", "an", "die", "Ge\u00b7stalt", "der", "tau\u00b7send\u00b7fa\u00b7chen", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schmach, Geisseln, Backenstreich, Creutz, Dornen, Bande, Tod.", "tokens": ["Schmach", ",", "Geis\u00b7seln", ",", "Ba\u00b7cken\u00b7streich", ",", "Creutz", ",", "Dor\u00b7nen", ",", "Ban\u00b7de", ",", "Tod", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "In dem er nun beginnt der Laster Grimm und Grawen,", "tokens": ["In", "dem", "er", "nun", "be\u00b7ginnt", "der", "Las\u00b7ter", "Grimm", "und", "Gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "ART", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sein unschuldigs Haupt ihm drucken, anzuschawen,", "tokens": ["Die", "sein", "un\u00b7schul\u00b7digs", "Haupt", "ihm", "dru\u00b7cken", ",", "an\u00b7zu\u00b7scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Erseufftzt er tieff, sein Hertz ist Angst und Schreckens voll,", "tokens": ["Er\u00b7seufftzt", "er", "tieff", ",", "sein", "Hertz", "ist", "Angst", "und", "Schre\u00b7ckens", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch Trawrigkeit, die Gott auch selbst empfinden sol.", "tokens": ["Durch", "Traw\u00b7rig\u00b7keit", ",", "die", "Gott", "auch", "selbst", "emp\u00b7fin\u00b7den", "sol", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sein Leib mu\u00df Schwei\u00df, sein H\u00e4upt viel Trh\u00e4nen ihm gewehren,", "tokens": ["Sein", "Leib", "mu\u00df", "Schwei\u00df", ",", "sein", "H\u00e4upt", "viel", "Tr\u00b7h\u00e4\u00b7nen", "ihm", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "$,", "PPOSAT", "NN", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Der Schwei\u00df war aber Blut, Blut waren auch die Z\u00e4hren.", "tokens": ["Der", "Schwei\u00df", "war", "a\u00b7ber", "Blut", ",", "Blut", "wa\u00b7ren", "auch", "die", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo Vater, spricht er: dich je Blut vers\u00f6hnen kan,", "tokens": ["Wo", "Va\u00b7ter", ",", "spricht", "er", ":", "dich", "je", "Blut", "ver\u00b7s\u00f6h\u00b7nen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "PPER", "$.", "PPER", "ADV", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Schaw her, dein lieber Sohn vergeust es, nimm es an.", "tokens": ["Schaw", "her", ",", "dein", "lie\u00b7ber", "Sohn", "ver\u00b7geust", "es", ",", "nimm", "es", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Legst du das h\u00f6chst' auff mich? hier ist das h\u00f6chste Leiden,", "tokens": ["Legst", "du", "das", "h\u00f6chst'", "auff", "mich", "?", "hier", "ist", "das", "h\u00f6chs\u00b7te", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "APPR", "PPER", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Aus dessen Furcht ich auch von hinnen schier mu\u00df scheiden.", "tokens": ["Aus", "des\u00b7sen", "Furcht", "ich", "auch", "von", "hin\u00b7nen", "schier", "mu\u00df", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "APPR", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch ist der Schwei\u00df nicht gnug, vers\u00f6hn' ich erst durch Blut,", "tokens": ["Doch", "ist", "der", "Schwei\u00df", "nicht", "gnug", ",", "ver\u00b7s\u00f6hn'", "ich", "erst", "durch", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Geisseln sampt dem Speer vergiessen, deinen Muth?", "tokens": ["Das", "Geis\u00b7seln", "sampt", "dem", "Speer", "ver\u00b7gies\u00b7sen", ",", "dei\u00b7nen", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hie bin ich; wenn man mich auch wird get\u00f6dtet haben,", "tokens": ["Hie", "bin", "ich", ";", "wenn", "man", "mich", "auch", "wird", "ge\u00b7t\u00f6d\u00b7tet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$.", "KOUS", "PIS", "PRF", "ADV", "VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So mag der Spies mein Hertz ausleeren und durchgraben.", "tokens": ["So", "mag", "der", "Spies", "mein", "Hertz", "aus\u00b7lee\u00b7ren", "und", "durch\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Vater selbst erbarmt sich \u00fcber seinen Sohn,", "tokens": ["Der", "Va\u00b7ter", "selbst", "er\u00b7barmt", "sich", "\u00fc\u00b7ber", "sei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Schickt einen Engel her von seinem hohen Thron,", "tokens": ["Schickt", "ei\u00b7nen", "En\u00b7gel", "her", "von", "sei\u00b7nem", "ho\u00b7hen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der ihn sol tr\u00f6sten, ihm sol trucknen seine Wangen,", "tokens": ["Der", "ihn", "sol", "tr\u00f6s\u00b7ten", ",", "ihm", "sol", "truck\u00b7nen", "sei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "VVINF", "$,", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von dem er doch dabey ein hart Gebot empfangen.", "tokens": ["Von", "dem", "er", "doch", "da\u00b7bey", "ein", "hart", "Ge\u00b7bot", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PAV", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was holst du, Christe, Trost vom Himmel? hat denn Gott", "tokens": ["Was", "holst", "du", ",", "Chris\u00b7te", ",", "Trost", "vom", "Him\u00b7mel", "?", "hat", "denn", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "$,", "NN", "APPRART", "NN", "$.", "VAFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nicht Krafft gehabt behertzt zu leiden diesen Todt?", "tokens": ["Nicht", "Krafft", "ge\u00b7habt", "be\u00b7hertzt", "zu", "lei\u00b7den", "die\u00b7sen", "Todt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAPP", "ADJD", "PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich merck es, ach! Du wilst dir selber Trost versagen,", "tokens": ["Ich", "merck", "es", ",", "ach", "!", "Du", "wilst", "dir", "sel\u00b7ber", "Trost", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ITJ", "$.", "PPER", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auff da\u00df du kanst Gedult mit unsrer Schwachheit tragen.", "tokens": ["Auff", "da\u00df", "du", "kanst", "Ge\u00b7dult", "mit", "uns\u00b7rer", "Schwach\u00b7heit", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es war nun umb die Nacht und umb das volle Liecht,", "tokens": ["Es", "war", "nun", "umb", "die", "Nacht", "und", "umb", "das", "vol\u00b7le", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Monde wich' am Glantz auch schier der Sonnen nicht.", "tokens": ["Der", "Mon\u00b7de", "wich'", "am", "Glantz", "auch", "schier", "der", "Son\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie stehen auff, der Herr selbst vierter ist verschwiegen", "tokens": ["Sie", "ste\u00b7hen", "auff", ",", "der", "Herr", "selbst", "vier\u00b7ter", "ist", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Oeleberg hinauff sehr hoch betr\u00fcbt gestiegen,", "tokens": ["Den", "O\u00b7e\u00b7le\u00b7berg", "hin\u00b7auff", "sehr", "hoch", "be\u00b7tr\u00fcbt", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Den Berg, der ausserhalb der Stadt viel Oele tr\u00e4gt,", "tokens": ["Den", "Berg", ",", "der", "aus\u00b7ser\u00b7halb", "der", "Stadt", "viel", "O\u00b7e\u00b7le", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Vnd eine Garten-Lust auff seinem Gipffel hegt.", "tokens": ["Vnd", "ei\u00b7ne", "Gar\u00b7ten\u00b7Lust", "auff", "sei\u00b7nem", "Gipf\u00b7fel", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hie ist zu weinen fug, hie irrt ihn kein Get\u00fcmmel,", "tokens": ["Hie", "ist", "zu", "wei\u00b7nen", "fug", ",", "hie", "irrt", "ihn", "kein", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die reinen H\u00e4nde hebt er hie empor gen Himmel.", "tokens": ["Die", "rei\u00b7nen", "H\u00e4n\u00b7de", "hebt", "er", "hie", "em\u00b7por", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er denckt an die Gestalt der tausendfachen Noht,", "tokens": ["Er", "denckt", "an", "die", "Ge\u00b7stalt", "der", "tau\u00b7send\u00b7fa\u00b7chen", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schmach, Geisseln, Backenstreich, Creutz, Dornen, Bande, Tod.", "tokens": ["Schmach", ",", "Geis\u00b7seln", ",", "Ba\u00b7cken\u00b7streich", ",", "Creutz", ",", "Dor\u00b7nen", ",", "Ban\u00b7de", ",", "Tod", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "In dem er nun beginnt der Laster Grimm und Grawen,", "tokens": ["In", "dem", "er", "nun", "be\u00b7ginnt", "der", "Las\u00b7ter", "Grimm", "und", "Gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "ART", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sein unschuldigs Haupt ihm drucken, anzuschawen,", "tokens": ["Die", "sein", "un\u00b7schul\u00b7digs", "Haupt", "ihm", "dru\u00b7cken", ",", "an\u00b7zu\u00b7scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Erseufftzt er tieff, sein Hertz ist Angst und Schreckens voll,", "tokens": ["Er\u00b7seufftzt", "er", "tieff", ",", "sein", "Hertz", "ist", "Angst", "und", "Schre\u00b7ckens", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch Trawrigkeit, die Gott auch selbst empfinden sol.", "tokens": ["Durch", "Traw\u00b7rig\u00b7keit", ",", "die", "Gott", "auch", "selbst", "emp\u00b7fin\u00b7den", "sol", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sein Leib mu\u00df Schwei\u00df, sein H\u00e4upt viel Trh\u00e4nen ihm gewehren,", "tokens": ["Sein", "Leib", "mu\u00df", "Schwei\u00df", ",", "sein", "H\u00e4upt", "viel", "Tr\u00b7h\u00e4\u00b7nen", "ihm", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "$,", "PPOSAT", "NN", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Der Schwei\u00df war aber Blut, Blut waren auch die Z\u00e4hren.", "tokens": ["Der", "Schwei\u00df", "war", "a\u00b7ber", "Blut", ",", "Blut", "wa\u00b7ren", "auch", "die", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo Vater, spricht er: dich je Blut vers\u00f6hnen kan,", "tokens": ["Wo", "Va\u00b7ter", ",", "spricht", "er", ":", "dich", "je", "Blut", "ver\u00b7s\u00f6h\u00b7nen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "PPER", "$.", "PPER", "ADV", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Schaw her, dein lieber Sohn vergeust es, nimm es an.", "tokens": ["Schaw", "her", ",", "dein", "lie\u00b7ber", "Sohn", "ver\u00b7geust", "es", ",", "nimm", "es", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Legst du das h\u00f6chst' auff mich? hier ist das h\u00f6chste Leiden,", "tokens": ["Legst", "du", "das", "h\u00f6chst'", "auff", "mich", "?", "hier", "ist", "das", "h\u00f6chs\u00b7te", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "APPR", "PPER", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Aus dessen Furcht ich auch von hinnen schi