{"textgrid.poem.64833": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht dort den Regentropfen beben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht dort den Regentropfen beben", "tokens": ["Seht", "dort", "den", "Re\u00b7gen\u00b7trop\u00b7fen", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An jenes Baumes dunkelm Stamm!", "tokens": ["An", "je\u00b7nes", "Bau\u00b7mes", "dun\u00b7kelm", "Stamm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Demant gl\u00e4nzt er hell im Schweben,", "tokens": ["Als", "De\u00b7mant", "gl\u00e4nzt", "er", "hell", "im", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sinkt er nieder, wird er Schlamm. \u2013", "tokens": ["Doch", "sinkt", "er", "nie\u00b7der", ",", "wird", "er", "Schlamm", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, ihn wieder aufzuraffen", "tokens": ["Al\u00b7lein", ",", "ihn", "wie\u00b7der", "auf\u00b7zu\u00b7raf\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihn, der farblos erst und 