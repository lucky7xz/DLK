{"dta.poem.20559": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "18.  \n  Landwehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "O Zeit der Kraft, wo an des Thrones Stufen", "tokens": ["O", "Zeit", "der", "Kraft", ",", "wo", "an", "des", "Thro\u00b7nes", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Volk Du, K\u00f6nig Preu\u00dfens, fest vereint;", "tokens": ["Dein", "Volk", "Du", ",", "K\u00f6\u00b7nig", "Preu\u00b7\u00dfens", ",", "fest", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "NN", "NE", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Landwehr mit den Freiwill\u2019gen Du gerufen,", "tokens": ["Land\u00b7wehr", "mit", "den", "Frei\u00b7will'\u00b7gen", "Du", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Und jeder wahr es mit dem Andern meint!", "tokens": ["Und", "je\u00b7der", "wahr", "es", "mit", "dem", "An\u00b7dern", "meint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vom M\u00e4nnertritt bebt Erde, wie von Hufen,", "tokens": ["Vom", "M\u00e4n\u00b7ner\u00b7tritt", "bebt", "Er\u00b7de", ",", "wie", "von", "Hu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das ganze Heer in r\u00fcst\u2019ger Kraft erscheint.", "tokens": ["Das", "gan\u00b7ze", "Heer", "in", "r\u00fcst'\u00b7ger", "Kraft", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des stet\u2019gen S\u00f6ldners Ruhmtrieb wird entz\u00fcndet,", "tokens": ["Des", "stet'\u00b7gen", "S\u00f6ld\u00b7ners", "Ruhm\u00b7trieb", "wird", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da er den Landwehrmann sich \u00e4hnlich findet.", "tokens": ["Da", "er", "den", "Land\u00b7wehr\u00b7mann", "sich", "\u00e4hn\u00b7lich", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Landwehrmann tr\u00e4gt des Erl\u00f6sers Zeichen", "tokens": ["Der", "Land\u00b7wehr\u00b7mann", "tr\u00e4gt", "des", "Er\u00b7l\u00f6\u00b7sers", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Hut; ist so f\u00fcr Sieg und Tod geschm\u00fcckt.", "tokens": ["Am", "Hut", ";", "ist", "so", "f\u00fcr", "Sieg", "und", "Tod", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Gott f\u00fcr K\u00f6nig und f\u00fcr\u2019s Land der Eichen", "tokens": ["Mit", "Gott", "f\u00fcr", "K\u00f6\u00b7nig", "und", "f\u00fcr's", "Land", "der", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steht auf dem Kreuz, das auf zum Himmel blickt.", "tokens": ["Steht", "auf", "dem", "Kreuz", ",", "das", "auf", "zum", "Him\u00b7mel", "blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Bauern, wohlge\u00fcbt im Sensenstreichen,", "tokens": ["Die", "Bau\u00b7ern", ",", "wohl\u00b7ge\u00b7\u00fcbt", "im", "Sen\u00b7sen\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind schnell zur Hand. \u2014 In Reihen anger\u00fcckt,", "tokens": ["Sind", "schnell", "zur", "Hand", ".", "In", "Rei\u00b7hen", "an\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "$.", "$(", "APPR", "NN", "VVPP", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Als Exercierkunstkund\u2019ge j\u00fcngst entlassen,", "tokens": ["Als", "E\u00b7xer\u00b7cier\u00b7kunst\u00b7kun\u00b7d'\u00b7ge", "j\u00fcngst", "ent\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Str\u00f6men die lust\u2019gen Kr\u00fcmper zu in Massen.", "tokens": ["Str\u00f6\u00b7men", "die", "lust'\u00b7gen", "Kr\u00fcm\u00b7per", "zu", "in", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKZU", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}