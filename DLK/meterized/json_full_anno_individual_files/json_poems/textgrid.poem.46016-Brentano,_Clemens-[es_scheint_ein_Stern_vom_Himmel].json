{"textgrid.poem.46016": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[es scheint ein Stern vom Himmel]", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es scheint ein Stern vom Himmel,", "tokens": ["Es", "scheint", "ein", "Stern", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein einz'ger in mein Herz,", "tokens": ["Ein", "einz'\u00b7ger", "in", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00f6nnte wohl was Bessers tun,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da h\u00e4tt' ich Nacht und Schmerz.", "tokens": ["Da", "h\u00e4tt'", "ich", "Nacht", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es spritzt ein Quell vom Felsen", "tokens": ["Es", "spritzt", "ein", "Quell", "vom", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Tr\u00f6pfchen zu mir her,", "tokens": ["Ein", "Tr\u00f6pf\u00b7chen", "zu", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00f6nnte wohl was Bessers tun,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich verschmachtet w\u00e4r'.", "tokens": ["Da\u00df", "ich", "ver\u00b7schmach\u00b7tet", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es singt ein Himmelv\u00f6gelein", "tokens": ["Es", "singt", "ein", "Him\u00b7mel\u00b7v\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meiner Kerkerwand,", "tokens": ["An", "mei\u00b7ner", "Ker\u00b7ker\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00f6nnte wohl was Bessers tun,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich k\u00e4m' um den Verstand.", "tokens": ["Ich", "k\u00e4m'", "um", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es bl\u00fcht ein einz'ges Bl\u00fcmlein", "tokens": ["Es", "bl\u00fcht", "ein", "einz'\u00b7ges", "Bl\u00fcm\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf meinem Wege w\u00fcst,", "tokens": ["Auf", "mei\u00b7nem", "We\u00b7ge", "w\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00f6nnte wohl was Bessers tun,", "tokens": ["Es", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich verzweifeln m\u00fc\u00dft'.", "tokens": ["Da\u00df", "ich", "ver\u00b7zwei\u00b7feln", "m\u00fc\u00dft'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vor mir streicht hin ein wei\u00dfer Hirsch", "tokens": ["Vor", "mir", "streicht", "hin", "ein", "wei\u00b7\u00dfer", "Hirsch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit goldenem Geweih,", "tokens": ["Mit", "gol\u00b7de\u00b7nem", "Ge\u00b7weih", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00f6nnte wohl was Bessers tun,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich verirret sei.", "tokens": ["Da\u00df", "ich", "ver\u00b7ir\u00b7ret", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es scheint ein bi\u00dfchen Sonnenschein", "tokens": ["Es", "scheint", "ein", "bi\u00df\u00b7chen", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir in die Nacht herein,", "tokens": ["Mir", "in", "die", "Nacht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00f6nnte wohl was Bessers tun,", "tokens": ["Es", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich st\u00fcrb' in dunkler Pein.", "tokens": ["Ich", "st\u00fcrb'", "in", "dunk\u00b7ler", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es f\u00e4llt mir eine Bl\u00fcte", "tokens": ["Es", "f\u00e4llt", "mir", "ei\u00b7ne", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Segens ohne Frucht,", "tokens": ["Des", "Se\u00b7gens", "oh\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnte wohl was Bessers tun,", "tokens": ["Sie", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich glaubte mich verflucht.", "tokens": ["Ich", "glaub\u00b7te", "mich", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es sieht mit Himmelsg\u00fcte", "tokens": ["Es", "sieht", "mit", "Him\u00b7mels\u00b7g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein reines Aug' mich an,", "tokens": ["Ein", "rei\u00b7nes", "Aug'", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00f6nnte wohl was Bessers tun,", "tokens": ["Es", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann w\u00e4r's um mich getan.", "tokens": ["Dann", "w\u00e4r's", "um", "mich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es mahnet an dem Abgrund mich", "tokens": ["Es", "mah\u00b7net", "an", "dem", "Ab\u00b7grund", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein frommer Liedermund", "tokens": ["Ein", "from\u00b7mer", "Lie\u00b7der\u00b7mund"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er k\u00f6nnte wohl was Bessers tun,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich st\u00fcrzte in den Schlund.", "tokens": ["Ich", "st\u00fcrz\u00b7te", "in", "den", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es tut ein frommes M\u00e4gdlein", "tokens": ["Es", "tut", "ein", "from\u00b7mes", "M\u00e4gd\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl Engeldienst an mir,", "tokens": ["Wohl", "En\u00b7gel\u00b7dienst", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnte wohl was Bessers tun,", "tokens": ["Sie", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mein Heil verlier'.", "tokens": ["Da\u00df", "ich", "mein", "Heil", "ver\u00b7lier'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mich hat der Herr geliebet,", "tokens": ["Mich", "hat", "der", "Herr", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er zeigte mir, was sch\u00f6n,", "tokens": ["Er", "zeig\u00b7te", "mir", ",", "was", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er k\u00f6nnte wohl was Bessers tun,", "tokens": ["Er", "k\u00f6nn\u00b7te", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann m\u00fc\u00dft' ich untergehn.", "tokens": ["Dann", "m\u00fc\u00dft'", "ich", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Heut schienen Stern und Lichter", "tokens": ["Heut", "schie\u00b7nen", "Stern", "und", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir, was ich liebe, an,", "tokens": ["Mir", ",", "was", "ich", "lie\u00b7be", ",", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnten wohl was Bessers tun,", "tokens": ["Sie", "k\u00f6nn\u00b7ten", "wohl", "was", "Bes\u00b7sers", "tun", ","], "token_info": ["word", "w