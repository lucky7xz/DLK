{"textgrid.poem.53428": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df Christus mich befreyt von S\u00fcnden]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Christus mich befreyt von S\u00fcnden,", "tokens": ["Da\u00df", "Chris\u00b7tus", "mich", "be\u00b7freyt", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Vater mir vers\u00f6hnet hat,", "tokens": ["Den", "Va\u00b7ter", "mir", "ver\u00b7s\u00f6h\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di\u00df kan bey mir zwar Stelle finden:", "tokens": ["Di\u00df", "kan", "bey", "mir", "zwar", "Stel\u00b7le", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Hertz ist aber tr\u00e4g und matt.", "tokens": ["Mein", "Hertz", "ist", "a\u00b7ber", "tr\u00e4g", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie mach ich dieses mir zu eigen!", "tokens": ["Wie", "mach", "ich", "die\u00b7ses", "mir", "zu", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer z\u00fcndet meinen Glauben an?", "tokens": ["Wer", "z\u00fcn\u00b7det", "mei\u00b7nen", "Glau\u00b7ben", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der meinen Sinn k\u00f6nn \u00fcberzeugen,", "tokens": ["Der", "mei\u00b7nen", "Sinn", "k\u00f6nn", "\u00fc\u00b7berz\u00b7eu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das alles sey f\u00fcr mich gethan.", "tokens": ["Das", "al\u00b7les", "sey", "f\u00fcr", "mich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Di\u00df mu\u00df der Heilge Geist verrichten,", "tokens": ["Di\u00df", "mu\u00df", "der", "Heil\u00b7ge", "Geist", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der spricht mir Trost und Freude ein,", "tokens": ["Der", "spricht", "mir", "Trost", "und", "Freu\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich dir Christe bey mu\u00df pflichten,", "tokens": ["Da\u00df", "ich", "dir", "Chris\u00b7te", "bey", "mu\u00df", "pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und halten was du bist, f\u00fcr mein.", "tokens": ["Und", "hal\u00b7ten", "was", "du", "bist", ",", "f\u00fcr", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er ist des Kleinmuths Sieg und Zwinger,", "tokens": ["Er", "ist", "des", "Klein\u00b7muths", "Sieg", "und", "Zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt mein Gem\u00fcth empor zu dir,", "tokens": ["Hebt", "mein", "Ge\u00b7m\u00fcth", "em\u00b7por", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gottes rechten Hand ein Finger,", "tokens": ["Von", "Got\u00b7tes", "rech\u00b7ten", "Hand", "ein", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schreibt dich tieff ins Hertz hinein.", "tokens": ["Und", "schreibt", "dich", "tieff", "ins", "Hertz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er dr\u00fcckt nach der Verhei\u00dfung, Siegel", "tokens": ["Er", "dr\u00fcckt", "nach", "der", "Ver\u00b7hei\u00b7\u00dfung", ",", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und deines Wortes Schirm hinauff,", "tokens": ["Und", "dei\u00b7nes", "Wor\u00b7tes", "Schirm", "hin\u00b7auff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schiebet vor des Glaubens Riegel,", "tokens": ["Und", "schie\u00b7bet", "vor", "des", "Glau\u00b7bens", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erinnert mich auch meiner Tauff.", "tokens": ["E\u00b7rin\u00b7nert", "mich", "auch", "mei\u00b7ner", "Tauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wil ein Unmuth mich bestreiten,", "tokens": ["Und", "wil", "ein", "Un\u00b7muth", "mich", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So stehet er mir kr\u00e4ftig bey;", "tokens": ["So", "ste\u00b7het", "er", "mir", "kr\u00e4f\u00b7tig", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4sset meinen Fu\u00df nicht gleiten,", "tokens": ["Er", "l\u00e4s\u00b7set", "mei\u00b7nen", "Fu\u00df", "nicht", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist meine Zuversicht und Treu.", "tokens": ["Ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du warest von dem Todt erstanden", "tokens": ["Du", "wa\u00b7rest", "von", "dem", "Todt", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zeugtest deinen J\u00fcngern dich,", "tokens": ["Und", "zeug\u00b7test", "dei\u00b7nen", "J\u00fcn\u00b7gern", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lagen in des Zweiffels Banden,", "tokens": ["Sie", "la\u00b7gen", "in", "des", "Zweif\u00b7fels", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder traumte was f\u00fcr sich.", "tokens": ["Ein", "je\u00b7der", "traum\u00b7te", "was", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PIS", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du fuhrest auf, versprachest ihnen", "tokens": ["Du", "fuh\u00b7rest", "auf", ",", "ver\u00b7spra\u00b7chest", "ih\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tr\u00f6ster, welcher jederzeit", "tokens": ["Den", "Tr\u00f6s\u00b7ter", ",", "wel\u00b7cher", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Solt ihren bl\u00f6den Hertzen dienen", "tokens": ["Solt", "ih\u00b7ren", "bl\u00f6\u00b7den", "Hert\u00b7zen", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Unverstand und Traurigkeit.", "tokens": ["F\u00fcr", "Un\u00b7ver\u00b7stand", "und", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie hatten selber dich gesehen,", "tokens": ["Sie", "hat\u00b7ten", "sel\u00b7ber", "dich", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch was durch dich und deine Hand", "tokens": ["Auch", "was", "durch", "dich", "und", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr grosse Wunderthat geschehen,", "tokens": ["F\u00fcr", "gros\u00b7se", "Wun\u00b7der\u00b7that", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch bleibst du ihnen unbekant.", "tokens": ["Noch", "bleibst", "du", "ih\u00b7nen", "un\u00b7be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie sassen nun in Furcht und Zagen,", "tokens": ["Sie", "sas\u00b7sen", "nun", "in", "Furcht", "und", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es h\u00e4tte sie ein rauschend Blat,", "tokens": ["Es", "h\u00e4t\u00b7te", "sie", "ein", "rau\u00b7schend", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein blosser Schatten k\u00f6nnen jagen,", "tokens": ["Ein", "blos\u00b7ser", "Schat\u00b7ten", "k\u00f6n\u00b7nen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df dein Wort zu Wercke trat.", "tokens": ["Bis", "da\u00df", "dein", "Wort", "zu", "Wer\u00b7cke", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ein Brausen k\u00f6mmt auf sie gedrungen,", "tokens": ["Ein", "Brau\u00b7sen", "k\u00f6mmt", "auf", "sie", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das gantze Hau\u00df bebt durch den Wind,", "tokens": ["Das", "gant\u00b7ze", "Hau\u00df", "bebt", "durch", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stracks sehn sie an einander Zungen,", "tokens": ["Stracks", "sehn", "sie", "an", "ein\u00b7an\u00b7der", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PRF", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die feurig und gespalten sind.", "tokens": ["Die", "feu\u00b7rig", "und", "ge\u00b7spal\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie reden aller V\u00f6lcker Sprachen", "tokens": ["Sie", "re\u00b7den", "al\u00b7ler", "V\u00f6l\u00b7cker", "Spra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zeugen Gottes Wunder an,", "tokens": ["Und", "zeu\u00b7gen", "Got\u00b7tes", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dessen grosses Thun und machen,", "tokens": ["Auch", "des\u00b7sen", "gros\u00b7ses", "Thun", "und", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df sich entsetzet jederman.", "tokens": ["da\u00df", "sich", "ent\u00b7set\u00b7zet", "je\u00b7der\u00b7man", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Sie kommen vor das Volck getreten", "tokens": ["Sie", "kom\u00b7men", "vor", "das", "Volck", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und machen dich in Salem kund:", "tokens": ["Und", "ma\u00b7chen", "dich", "in", "Sa\u00b7lem", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man dreuet ihnen Strick und Ketten,", "tokens": ["Man", "dreu\u00b7et", "ih\u00b7nen", "Strick", "und", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts aber stopffet ihren Mund.", "tokens": ["Nichts", "a\u00b7ber", "stopf\u00b7fet", "ih\u00b7ren", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da\u00df Christus mich befreyt von S\u00fcnden,", "tokens": ["Da\u00df", "Chris\u00b7tus", "mich", "be\u00b7freyt", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Vater mir vers\u00f6hnet hat,", "tokens": ["Den", "Va\u00b7ter", "mir", "ver\u00b7s\u00f6h\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER"