a"}}, "stanza.25": {"line.1": {"text": "Sie reden aller V\u00f6lcker Sprachen", "tokens": ["Sie", "re\u00b7den", "al\u00b7ler", "V\u00f6l\u00b7cker", "Spra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zeugen Gottes Wunder an,", "tokens": ["Und", "zeu\u00b7gen", "Got\u00b7tes", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dessen grosses Thun und machen,", "tokens": ["Auch", "des\u00b7sen", "gros\u00b7ses", "Thun", "und", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df sich entsetzet jederman.", "tokens": ["da\u00df", "sich", "ent\u00b7set\u00b7zet", "je\u00b7der\u00b7man", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Sie kommen vor das Volck getreten", "tokens": ["Sie", "kom\u00b7men", "vor", "das", "Volck", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und machen dich in Salem kund:", "tokens": ["Und", "ma\u00b7chen", "dich", "in", "Sa\u00b7lem", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man dreuet ihnen Strick und Ketten,", "tokens": ["Man", "dreu\u00b7et", "ih\u00b7nen", "Strick", "und", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts aber stopffet ihren Mund.", "tokens": ["Nichts", "a\u00b7ber", "stopf\u00b7fet", "ih\u00b7ren", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}