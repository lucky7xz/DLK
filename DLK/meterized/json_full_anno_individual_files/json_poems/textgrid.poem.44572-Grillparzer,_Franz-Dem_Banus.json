ra"}, "line.2": {"text": "Doch auch der Wurzel Mark,", "tokens": ["Doch", "auch", "der", "Wur\u00b7zel", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Machst du der Pfeile Sinnspruch wahr,", "tokens": ["Machst", "du", "der", "Pfei\u00b7le", "Sinns\u00b7pruch", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nur verbunden stark.", "tokens": ["Die", "nur", "ver\u00b7bun\u00b7den", "stark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Wozu kein F\u00fcrst, kein Heer gen\u00fcgt,", "tokens": ["Wo\u00b7zu", "kein", "F\u00fcrst", ",", "kein", "Heer", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Zutraun wird es leicht,", "tokens": ["Dem", "Zu\u00b7traun", "wird", "es", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer f\u00fcgt sich nicht, wenn der sich f\u00fcgt,", "tokens": ["Wer", "f\u00fcgt", "sich", "nicht", ",", "wenn", "der", "sich", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PTKNEG", "$,", "KOUS", "ART", "PRF", "VVFIN", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Dem keiner sich vergleicht.", "tokens": ["Dem", "kei\u00b