r", ",", "du", "bist", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und sieh in unsres Dankes Lohn", "tokens": ["Und", "sieh", "in", "uns\u00b7res", "Dan\u00b7kes", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zugleich der Warnung Bild:", "tokens": ["Zu\u00b7gleich", "der", "War\u00b7nung", "Bild", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Heldenschwert, du hast es schon,", "tokens": ["Das", "Hel\u00b7den\u00b7schwert", ",", "du", "hast", "es", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm noch dazu den Schild.", "tokens": ["Nimm", "noch", "da\u00b7zu", "den", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Geboren, um ein Schwert zu sein,", "tokens": ["Ge\u00b7bo\u00b7ren", ",", "um", "ein", "Schwert", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Heldenmutes Bild,", "tokens": ["Des", "Hel\u00b7den\u00b7mu\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure":