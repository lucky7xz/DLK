{"textgrid.poem.31211": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Ein Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Freund, der Th\u00fcrmer, war noch wach,", "tokens": ["Sein", "Freund", ",", "der", "Th\u00fcr\u00b7mer", ",", "war", "noch", "wach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Silber gleisste das Rathhausdach,", "tokens": ["Wie", "Sil\u00b7ber", "gleiss\u00b7te", "das", "Rath\u00b7haus\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dr\u00fcber stand der Mond.", "tokens": ["Und", "dr\u00fc\u00b7ber", "stand", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er wusste kaum, wie schwer er litt,", "tokens": ["Er", "wuss\u00b7te", "kaum", ",", "wie", "schwer", "er", "litt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch schlug ihm das Herz bei jedem Schritt,", "tokens": ["Doch", "schlug", "ihm", "das", "Herz", "bei", "je\u00b7dem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und das R\u00e4nzel dr\u00fcckte ihn.", "tokens": ["Und", "das", "R\u00e4n\u00b7zel", "dr\u00fcck\u00b7te", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Gasse war so lang, so lang,", "tokens": ["Die", "Gas\u00b7se", "war", "so", "lang", ",", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dazu noch die Stimme, die \u00fcber ihm sang:", "tokens": ["Und", "da\u00b7zu", "noch", "die", "Stim\u00b7me", ",", "die", "\u00fc\u00b7ber", "ihm", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wann's Mail\u00fcfterl weht!", "tokens": ["Wann's", "Mai\u00b7l\u00fcf\u00b7terl", "weht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Jetzt bog sich ein Fliederstrauch \u00fcber den Zaun,", "tokens": ["Jetzt", "bog", "sich", "ein", "Flie\u00b7der\u00b7strauch", "\u00fc\u00b7ber", "den", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und die Mutter Gottes, aus Stein gehaun,", "tokens": ["Und", "die", "Mut\u00b7ter", "Got\u00b7tes", ",", "aus", "Stein", "ge\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stand weiss vor dem Domportal.", "tokens": ["Stand", "weiss", "vor", "dem", "Dom\u00b7por\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Hier stand er eine Weile still", "tokens": ["Hier", "stand", "er", "ei\u00b7ne", "Wei\u00b7le", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rte, wie eine Dohle schrill", "tokens": ["Und", "h\u00f6r\u00b7te", ",", "wie", "ei\u00b7ne", "Doh\u00b7le", "schrill"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hoch oben ums Thurmkreuz pfiff.", "tokens": ["Hoch", "o\u00b7ben", "ums", "Thurm\u00b7kreuz", "pfiff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Dann l\u00f6schte links in dem kleinen Haus", "tokens": ["Dann", "l\u00f6schte", "links", "in", "dem", "klei\u00b7nen", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der L\u00f6wenwirth seine Lichter aus,", "tokens": ["Der", "L\u00f6\u00b7wen\u00b7wirth", "sei\u00b7ne", "Lich\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die Domuhr schlug langsam zehn.", "tokens": ["Und", "die", "Do\u00b7muhr", "schlug", "lang\u00b7sam", "zehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Brunnen rauschten wie im Traum,", "tokens": ["Die", "Brun\u00b7nen", "rauschten", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Nachtigall schlug im Lindenbaum,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "schlug", "im", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Alles war wie sonst!", "tokens": ["Und", "Al\u00b7les", "war", "wie", "sonst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da riss er die Rose sich aus dem Rock", "tokens": ["Da", "riss", "er", "die", "Ro\u00b7se", "sich", "aus", "dem", "Rock"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und stiess sie ins Pflaster mit seinem Stock,", "tokens": ["Und", "stiess", "sie", "ins", "Pflas\u00b7ter", "mit", "sei\u00b7nem", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dass die Funken stoben, und ging.", "tokens": ["Dass", "die", "Fun\u00b7ken", "sto\u00b7ben", ",", "und", "ging", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Das L\u00e4mpchen flackerte roth \u00fcberm Thor,", "tokens": ["Das", "L\u00e4mp\u00b7chen", "fla\u00b7cker\u00b7te", "roth", "\u00fc\u00b7berm", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und der Wald, in den sich sein Weg verlor,", "tokens": ["Und", "der", "Wald", ",", "in", "den", "sich", "sein", "Weg", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Stand schwarz im Mondlicht da ...", "tokens": ["Stand", "schwarz", "im", "Mond\u00b7licht", "da", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Erst droben auf dem Heiligenstein", "tokens": ["Erst", "dro\u00b7ben", "auf", "dem", "Hei\u00b7li\u00b7gen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Fiel ihm noch einmal Alles ein,", "tokens": ["Fiel", "ihm", "noch", "ein\u00b7mal", "Al\u00b7les", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als der Weg um die Buche bog.", "tokens": ["Als", "der", "Weg", "um", "die", "Bu\u00b7che", "bog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Die Bl\u00e4tter rauschten, er stand und stand", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "rauschten", ",", "er", "stand", "und", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah hinunter unverwandt,", "tokens": ["Und", "sah", "hin\u00b7un\u00b7ter", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo die D\u00e4cher funkelten!", "tokens": ["Wo", "die", "D\u00e4\u00b7cher", "fun\u00b7kel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.12": {"line.1": {"text": "Dort stand der Garten und dort das Haus,", "tokens": ["Dort", "stand", "der", "Gar\u00b7ten", "und", "dort", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und jetzt war das aus, und jetzt war das aus,", "tokens": ["Und", "jetzt", "war", "das", "aus", ",", "und", "jetzt", "war", "das", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDS", "PTKVZ", "$,", "KON", "ADV", "VAFIN", "PDS", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und \u2013 die D\u00e4cher funkelten!", "tokens": ["Und", "\u2013", "die", "D\u00e4\u00b7cher", "fun\u00b7kel\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.13": {"line.1": {"text": "Sein Herz schlug wild, sein Herz schlug nicht fromm:", "tokens": ["Sein", "Herz", "schlug", "wild", ",", "sein", "Herz", "schlug", "nicht", "fromm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wann i komm, wann i komm, wann i wiederkomm!", "tokens": ["Wann", "i", "komm", ",", "wann", "i", "komm", ",", "wann", "i", "wie\u00b7der\u00b7komm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "PWAV", "NE", "VVFIN", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch er kam nie wieder.", "tokens": ["Doch", "er", "kam", "nie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Sein Freund, der Th\u00fcrmer, war noch wach,", "tokens": ["Sein", "Freund", ",", "der", "Th\u00fcr\u00b7mer", ",", "war", "noch", "wach", ","], "token_info": ["word", "word", "punct", "word", "wor