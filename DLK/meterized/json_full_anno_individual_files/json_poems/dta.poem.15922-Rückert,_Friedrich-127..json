{"dta.poem.15922": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "127.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Ewiges, das wandelt und das bleibt,", "tokens": ["Es", "ist", "ein", "E\u00b7wi\u00b7ges", ",", "das", "wan\u00b7delt", "und", "das", "bleibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PDS", "VVFIN", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das in sich selber ruht und ruhlos Alles treibt.", "tokens": ["Das", "in", "sich", "sel\u00b7ber", "ruht", "und", "ruh\u00b7los", "Al\u00b7les", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRF", "ADV", "VVFIN", "KON", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft Erregungen und Leidenschaften lassen,", "tokens": ["Du", "mu\u00dft", "Er\u00b7re\u00b7gun\u00b7gen", "und", "Lei\u00b7den\u00b7schaf\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn du das Ewige, das ruhet, willst erfassen.", "tokens": ["Wenn", "du", "das", "E\u00b7wi\u00b7ge", ",", "das", "ru\u00b7het", ",", "willst", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "PDS", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du mu\u00dft Erregungen und Leidenschaften hegen,", "tokens": ["Du", "mu\u00dft", "Er\u00b7re\u00b7gun\u00b7gen", "und", "Lei\u00b7den\u00b7schaf\u00b7ten", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn dich das Ewige, das wandelt, soll bewegen.", "tokens": ["Wenn", "dich", "das", "E\u00b7wi\u00b7ge", ",", "das", "wan\u00b7delt", ",", "soll", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "PDS", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erfassend und erfa\u00dft, erregend und erregt,", "tokens": ["Er\u00b7fas\u00b7send", "und", "er\u00b7fa\u00dft", ",", "er\u00b7re\u00b7gend", "und", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei gleich dem Ew'gen selbst, bewegt und unbewegt.", "tokens": ["Sei", "gleich", "dem", "Ew'\u00b7gen", "selbst", ",", "be\u00b7wegt", "und", "un\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}