{"textgrid.poem.64053": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Pflanzenleben", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt Ihr schon in junges Gr\u00fcn gekleidet,", "tokens": ["Habt", "Ihr", "schon", "in", "jun\u00b7ges", "Gr\u00fcn", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eure Knospen jugendlich empor?", "tokens": ["Eu\u00b7re", "Knos\u00b7pen", "ju\u00b7gend\u00b7lich", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zarte Pflanzen, die ich oft beneidet,", "tokens": ["Zar\u00b7te", "Pflan\u00b7zen", ",", "die", "ich", "oft", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn mein Auge sich an Euch geweidet,", "tokens": ["Wenn", "mein", "Au\u00b7ge", "sich", "an", "Euch", "ge\u00b7wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mein Sinn sich tief in Euch verlor.", "tokens": ["Und", "mein", "Sinn", "sich", "tief", "in", "Euch", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Denn umweht von warmen Fr\u00fchlingsl\u00fcften,", "tokens": ["Denn", "um\u00b7weht", "von", "war\u00b7men", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oeffnet sich der Erde dunkler Schooss.", "tokens": ["Oeff\u00b7net", "sich", "der", "Er\u00b7de", "dunk\u00b7ler", "Scho\u00b7oss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus des m\u00fctterlichen Bodens Gr\u00fcften,", "tokens": ["Aus", "des", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Bo\u00b7dens", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ringen sich, geweckt zu s\u00fcssen D\u00fcften,", "tokens": ["Rin\u00b7gen", "sich", ",", "ge\u00b7weckt", "zu", "s\u00fcs\u00b7sen", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "$,", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eure still entsprossten Keime los.", "tokens": ["Eu\u00b7re", "still", "ent\u00b7spross\u00b7ten", "Kei\u00b7me", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und der Sonne milde Blicke schweben", "tokens": ["Und", "der", "Son\u00b7ne", "mil\u00b7de", "Bli\u00b7cke", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebend um das neue, frische Gr\u00fcn.", "tokens": ["Lie\u00b7bend", "um", "das", "neu\u00b7e", ",", "fri\u00b7sche", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kraftvoll regt sich Euer innres Leben,", "tokens": ["Kraft\u00b7voll", "regt", "sich", "Eu\u00b7er", "inn\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit ungehemmtem, raschem Streben,", "tokens": ["Und", "mit", "un\u00b7ge\u00b7hemm\u00b7tem", ",", "ra\u00b7schem", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Keimt die Knospe nur um aufzubl\u00fchn.", "tokens": ["Keimt", "die", "Knos\u00b7pe", "nur", "um", "auf\u00b7zu\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KOUI", "VVIZU", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "S\u00fcsse Wohlger\u00fcche zu verhauchen,", "tokens": ["S\u00fcs\u00b7se", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "zu", "ver\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bricht bewusstlos dann die Blume auf;", "tokens": ["Bricht", "be\u00b7wusst\u00b7los", "dann", "die", "Blu\u00b7me", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und des Tages helle Strahlen tauchen", "tokens": ["Und", "des", "Ta\u00b7ges", "hel\u00b7le", "Strah\u00b7len", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schmeichelnd sich in ihren Kelch und saugen", "tokens": ["Schmei\u00b7chelnd", "sich", "in", "ih\u00b7ren", "Kelch", "und", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Leis' und schnell ihr junges Leben auf.", "tokens": ["Leis'", "und", "schnell", "ihr", "jun\u00b7ges", "Le\u00b7ben", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "K\u00f6nnt' ich, stille Pflanzen, mit euch tauschen,", "tokens": ["K\u00f6nnt'", "ich", ",", "stil\u00b7le", "Pflan\u00b7zen", ",", "mit", "euch", "tau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schmerzlos leben und vergehn, wie Ihr!", "tokens": ["Schmerz\u00b7los", "le\u00b7ben", "und", "ver\u00b7gehn", ",", "wie", "Ihr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Leise nur ber\u00fchrt, wenn St\u00fcrme rauschen,", "tokens": ["Lei\u00b7se", "nur", "be\u00b7r\u00fchrt", ",", "wenn", "St\u00fcr\u00b7me", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00fcrd' ich auf der Sonne L\u00e4cheln lauschen,", "tokens": ["W\u00fcrd'", "ich", "auf", "der", "Son\u00b7ne", "L\u00e4\u00b7cheln", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und das Daseyn w\u00e4re freundlich mir.", "tokens": ["Und", "das", "Da\u00b7seyn", "w\u00e4\u00b7re", "freund\u00b7lich", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Euer Loos \u2013 so einfach und bescheiden \u2013", "tokens": ["Eu\u00b7er", "Loos", "\u2013", "so", "ein\u00b7fach", "und", "be\u00b7schei\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach warum ward es dem Menschen nicht? \u2013", "tokens": ["Ach", "wa\u00b7rum", "ward", "es", "dem", "Men\u00b7schen", "nicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "PWAV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und die einzeln uns geschenkten Freuden", "tokens": ["Und", "die", "ein\u00b7zeln", "uns", "ge\u00b7schenk\u00b7ten", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stillen unsern Durst der Seele nicht.", "tokens": ["Stil\u00b7len", "un\u00b7sern", "Durst", "der", "See\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Bis der Schleier schimmernd sich erhebet,", "tokens": ["Bis", "der", "Schlei\u00b7er", "schim\u00b7mernd", "sich", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der das matte Auge jetzt umh\u00fcllt:", "tokens": ["Der", "das", "mat\u00b7te", "Au\u00b7ge", "jetzt", "um\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bis das Sehnen, das uns oft durchbebet,", "tokens": ["Bis", "das", "Seh\u00b7nen", ",", "das", "uns", "oft", "durch\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und das Ahnen, das uns leis' umschwebet", "tokens": ["Und", "das", "Ah\u00b7nen", ",", "das", "uns", "leis'", "um\u00b7schwe\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der Verkl\u00e4rung Himmelsglanz erf\u00fcllt.", "tokens": ["Der", "Ver\u00b7kl\u00e4\u00b7rung", "Him\u00b7mels\u00b7glanz", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Habt Ihr schon in junges Gr\u00fcn gekleidet,", "tokens": ["Habt", "Ihr", "schon", "in", "jun\u00b7ges", "Gr\u00fcn", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eure Knospen jugendlich empor?", "tokens": ["Eu\u00b7re", "Knos\u00b7pen", "ju\u00b7gend\u00b7lich", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zarte Pflanzen, die ich oft beneidet,", "tokens": ["Zar\u00b7te", "Pflan\u00b7zen", ",", "die", "ich", "oft", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn mein Auge sich an Euch geweidet,", "tokens": ["Wenn", "mein", "Au\u00b7ge", "sich", "an", "Euch", "ge\u00b7wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mein Sinn sich tief in Euch verlor.", "tokens": ["Und", "mein", "Sinn", "sich", "tief", "in", "Euch", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punc