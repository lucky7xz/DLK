{"dta.poem.7972": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fchlt jener schauervolle Sturm aus Norden", "tokens": ["W\u00fchlt", "je\u00b7ner", "schau\u00b7er\u00b7vol\u00b7le", "Sturm", "aus", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerst\u00f6rend auch im frischen Liederkranze?", "tokens": ["Zer\u00b7st\u00f6\u00b7rend", "auch", "im", "fri\u00b7schen", "Lie\u00b7der\u00b7kran\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist der Gesang ein feiges Spiel geworden?", "tokens": ["Ist", "der", "Ge\u00b7sang", "ein", "fei\u00b7ges", "Spiel", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wiegt f\u00fcrder nur der Degen und die Lanze?", "tokens": ["Wiegt", "f\u00fcr\u00b7der", "nur", "der", "De\u00b7gen", "und", "die", "Lan\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mu\u00df schamroth abw\u00e4rts fliehn der S\u00e4ngerorden,", "tokens": ["Mu\u00df", "scham\u00b7roth", "ab\u00b7w\u00e4rts", "fliehn", "der", "S\u00e4n\u00b7ge\u00b7ror\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wann Kriegerscharen ziehn im Waffenglanze?", "tokens": ["Wann", "Krie\u00b7ger\u00b7scha\u00b7ren", "ziehn", "im", "Waf\u00b7fen\u00b7glan\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Darf nicht der Harfner wie in vor\u2019gen Zeiten,", "tokens": ["Darf", "nicht", "der", "Harf\u00b7ner", "wie", "in", "vor'\u00b7gen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Willkommen selbst durch Feindeslager schreiten?", "tokens": ["Will\u00b7kom\u00b7men", "selbst", "durch", "Fein\u00b7des\u00b7la\u00b7ger", "schrei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bleibt Poesie zu Wald und Kluft verdrungen,", "tokens": ["Bleibt", "Poe\u00b7sie", "zu", "Wald", "und", "Kluft", "ver\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis nirgends Kampf der V\u00f6lker Ruhe st\u00f6ret,", "tokens": ["Bis", "nir\u00b7gends", "Kampf", "der", "V\u00f6l\u00b7ker", "Ru\u00b7he", "st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis das vulkan\u2019sche Feuer ausgerungen,", "tokens": ["Bis", "das", "vul\u00b7kan'\u00b7sche", "Feu\u00b7er", "aus\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das stets sich neu im Erdenschoo\u00df emp\u00f6ret:", "tokens": ["Das", "stets", "sich", "neu", "im", "Er\u00b7den\u00b7schoo\u00df", "em\u00b7p\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So ist bis heute noch kein Lied erklungen,", "tokens": ["So", "ist", "bis", "heu\u00b7te", "noch", "kein", "Lied", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wird auch keins in k\u00fcnft\u2019ger Zeit geh\u00f6ret.", "tokens": ["Und", "wird", "auch", "keins", "in", "k\u00fcnft'\u00b7ger", "Zeit", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nein! \u00fcber ew\u2019gen K\u00e4mpfen schwebt im Liede,", "tokens": ["Nein", "!", "\u00fc\u00b7ber", "ew'\u00b7gen", "K\u00e4mp\u00b7fen", "schwebt", "im", "Lie\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gleichwie in Goldgew\u00f6lk, der ew\u2019ge Friede.", "tokens": ["Gleich\u00b7wie", "in", "Gold\u00b7ge\u00b7w\u00f6lk", ",", "der", "ew'\u00b7ge", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein jedes weltlich Ding hat seine Zeit,", "tokens": ["Ein", "je\u00b7des", "welt\u00b7lich", "Ding", "hat", "sei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PWAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Dichtung lebet ewig im Gem\u00fcthe,", "tokens": ["Die", "Dich\u00b7tung", "le\u00b7bet", "e\u00b7wig", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich ewig in erhabner Herrlichkeit,", "tokens": ["Gleich", "e\u00b7wig", "in", "er\u00b7hab\u00b7ner", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie in der tiefen Lieb\u2019 und stillen G\u00fcte,", "tokens": ["Wie", "in", "der", "tie\u00b7fen", "Lieb'", "und", "stil\u00b7len", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gleich ewig in des Ernstes D\u00fcsterheit,", "tokens": ["Gleich", "e\u00b7wig", "in", "des", "Erns\u00b7tes", "D\u00fcs\u00b7ter\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie in dem Spiel und in des Scherzes Bl\u00fcthe.", "tokens": ["Wie", "in", "dem", "Spiel", "und", "in", "des", "Scher\u00b7zes", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ob Donner rollen, ob Orkane w\u00fchlen,", "tokens": ["Ob", "Don\u00b7ner", "rol\u00b7len", ",", "ob", "Or\u00b7ka\u00b7ne", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Sonne wankt nicht und die Sterne spielen.", "tokens": ["Die", "Son\u00b7ne", "wankt", "nicht", "und", "die", "Ster\u00b7ne", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schon r\u00fcsten sich die Heere zum Verderben,", "tokens": ["Schon", "r\u00fcs\u00b7ten", "sich", "die", "Hee\u00b7re", "zum", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fchling r\u00fcstet sich zu Spiel und Reigen;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "r\u00fcs\u00b7tet", "sich", "zu", "Spiel", "und", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Trommeln wirbeln, die Trommeten werben,", "tokens": ["Die", "Trom\u00b7meln", "wir\u00b7beln", ",", "die", "Trom\u00b7me\u00b7ten", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Inde\u00df die wilden Winterst\u00fcrme schweigen;", "tokens": ["In\u00b7de\u00df", "die", "wil\u00b7den", "Win\u00b7ter\u00b7st\u00fcr\u00b7me", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Blute wild der Krieg die Erde f\u00e4rben,", "tokens": ["Mit", "Blu\u00b7te", "wild", "der", "Krieg", "die", "Er\u00b7de", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sich mit Blumen schm\u00fcckt und Bl\u00fcthenzweigen:", "tokens": ["Die", "sich", "mit", "Blu\u00b7men", "schm\u00fcckt", "und", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Darf so der ird\u2019sche Lenz sich frei erschlie\u00dfen,", "tokens": ["Darf", "so", "der", "ird'\u00b7sche", "Lenz", "sich", "frei", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So m\u00f6g\u2019 auch unser Dichterfr\u00fchling sprie\u00dfen!", "tokens": ["So", "m\u00f6g'", "auch", "un\u00b7ser", "Dich\u00b7ter\u00b7fr\u00fch\u00b7ling", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}