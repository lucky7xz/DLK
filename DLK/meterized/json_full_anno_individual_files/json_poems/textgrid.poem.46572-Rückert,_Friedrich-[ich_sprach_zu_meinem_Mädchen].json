, "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Genug war ein goldnes F\u00e4dchen,", "tokens": ["Ge\u00b7nug", "war", "ein", "gold\u00b7nes", "F\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um es hinaufzuziehn.", "tokens": ["Um", "es", "hin\u00b7auf\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Uns aber, schwerer beladen", "tokens": ["Uns", "a\u00b7ber", ",", "schwe\u00b7rer", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Kummer oder von Schuld,", "tokens": ["Von", "Kum\u00b7mer", "o\u00b7der", "von", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auch uns am goldenen Faden", "tokens": ["Auch", "uns", "am", "gol\u00b7de\u00b7nen", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird hinaufziehn die Huld.", "tokens": ["Wird", "hin\u00b7auf\u00b7ziehn", "die", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}