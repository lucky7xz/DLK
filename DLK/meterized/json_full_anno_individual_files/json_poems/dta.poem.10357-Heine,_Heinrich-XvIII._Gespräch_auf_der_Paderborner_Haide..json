{"dta.poem.10357": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  \n Gespr\u00e4ch auf der Paderborner Haide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du nicht die fernen T\u00f6ne,", "tokens": ["H\u00f6rst", "du", "nicht", "die", "fer\u00b7nen", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von Brummba\u00df und von Geigen?", "tokens": ["Wie", "von", "Brumm\u00b7ba\u00df", "und", "von", "Gei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorten tanzt wohl manche Sch\u00f6ne", "tokens": ["Dor\u00b7ten", "tanzt", "wohl", "man\u00b7che", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den gefl\u00fcgelt leichten Reigen.", "tokens": ["Den", "ge\u00b7fl\u00fc\u00b7gelt", "leich\u00b7ten", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eei, mein Freund, das nenn' ich irren,", "tokens": ["\u201e", "ei", ",", "mein", "Freund", ",", "das", "nenn'", "ich", "ir\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Geigen h\u00f6r' ich keine,", "tokens": ["Von", "den", "Gei\u00b7gen", "h\u00f6r'", "ich", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Ferklein h\u00f6r' ich quirren,", "tokens": ["Nur", "die", "Fer\u00b7klein", "h\u00f6r'", "ich", "quir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grunzen nur h\u00f6r' ich die Schweine.\u201c", "tokens": ["Grun\u00b7zen", "nur", "h\u00f6r'", "ich", "die", "Schwei\u00b7ne", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6rst du nicht das Waldhorn blasen?", "tokens": ["H\u00f6rst", "du", "nicht", "das", "Wald\u00b7horn", "bla\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00e4ger sich des Waidwerks freuen;", "tokens": ["J\u00e4\u00b7ger", "sich", "des", "Waid\u00b7werks", "freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromme L\u00e4mmer seh ich grasen,", "tokens": ["From\u00b7me", "L\u00e4m\u00b7mer", "seh", "ich", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4fer spielen auf Schallmeien.", "tokens": ["Sch\u00e4\u00b7fer", "spie\u00b7len", "auf", "Schall\u00b7mei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201eei, mein Freund, was du vernommen", "tokens": ["\u201e", "ei", ",", "mein", "Freund", ",", "was", "du", "ver\u00b7nom\u00b7men"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist kein Waldhorn, noch Schallmeie;", "tokens": ["Ist", "kein", "Wald\u00b7horn", ",", "noch", "Schall\u00b7mei\u00b7e", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Nur den Sauhirt seh' ich kommen,", "tokens": ["Nur", "den", "Sau\u00b7hirt", "seh'", "ich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimw\u00e4rts treibt er seine S\u00e4ue.\u201c", "tokens": ["Heim\u00b7w\u00e4rts", "treibt", "er", "sei\u00b7ne", "S\u00e4u\u00b7e", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6rst du nicht das ferne Singen,", "tokens": ["H\u00f6rst", "du", "nicht", "das", "fer\u00b7ne", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von s\u00fc\u00dfen Wettges\u00e4ngen?", "tokens": ["Wie", "von", "s\u00fc\u00b7\u00dfen", "Wett\u00b7ge\u00b7s\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Englein schlagen mit den Schwingen", "tokens": ["En\u00b7glein", "schla\u00b7gen", "mit", "den", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauten Beifall solchen Kl\u00e4ngen.", "tokens": ["Lau\u00b7ten", "Bei\u00b7fall", "sol\u00b7chen", "Kl\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eei, was dort so h\u00fcbsch geklungen,", "tokens": ["\u201e", "ei", ",", "was", "dort", "so", "h\u00fcbsch", "ge\u00b7klun\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist kein Wettgesang, mein Lieber!", "tokens": ["Ist", "kein", "Wett\u00b7ge\u00b7sang", ",", "mein", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singend treiben G\u00e4nsejungen", "tokens": ["Sin\u00b7gend", "trei\u00b7ben", "G\u00e4n\u00b7se\u00b7jun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre G\u00e4nselein vor\u00fcber.\u201c", "tokens": ["Ih\u00b7re", "G\u00e4n\u00b7sel\u00b7ein", "vor\u00b7\u00fc\u00b7ber", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6rst du nicht die Glocken l\u00e4uten,", "tokens": ["H\u00f6rst", "du", "nicht", "die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderlieblich, wunderhelle?", "tokens": ["Wun\u00b7der\u00b7lieb\u00b7lich", ",", "wun\u00b7der\u00b7hel\u00b7le", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromme Kircheng\u00e4nger schreiten", "tokens": ["From\u00b7me", "Kir\u00b7chen\u00b7g\u00e4n\u00b7ger", "schrei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Andachtsvoll zur Dorfkapelle.", "tokens": ["An\u00b7dachts\u00b7voll", "zur", "Dorf\u00b7ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201eei, mein Freund, das sind die Schellen", "tokens": ["\u201e", "ei", ",", "mein", "Freund", ",", "das", "sind", "die", "Schel\u00b7len"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Ochsen, von den K\u00fchen,", "tokens": ["Von", "den", "Och\u00b7sen", ",", "von", "den", "K\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nach ihren dunkeln St\u00e4llen", "tokens": ["Die", "nach", "ih\u00b7ren", "dun\u00b7keln", "St\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gesenktem Kopfe ziehen.\u201c", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", "Kop\u00b7fe", "zie\u00b7hen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Siehst du nicht den Schleier wehen?", "tokens": ["Siehst", "du", "nicht", "den", "Schlei\u00b7er", "we\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du nicht das leise Nicken?", "tokens": ["Siehst", "du", "nicht", "das", "lei\u00b7se", "Ni\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort seh' ich die Liebste stehen,", "tokens": ["Dort", "seh'", "ich", "die", "Liebs\u00b7te", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Feuchte Wehmuth in den Blicken.", "tokens": ["Feuch\u00b7te", "Weh\u00b7muth", "in", "den", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201eei! mein Freund, dort seh' ich nicken", "tokens": ["\u201e", "ei", "!", "mein", "Freund", ",", "dort", "seh'", "ich", "ni\u00b7cken"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$.", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur das Waldweib, nur die Lise;", "tokens": ["Nur", "das", "Wald\u00b7weib", ",", "nur", "die", "Li\u00b7se", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bla\u00df und hager an den Kr\u00fccken", "tokens": ["Bla\u00df", "und", "ha\u00b7ger", "an", "den", "Kr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinkt sie weiter nach der Wiese.\u201c", "tokens": ["Hinkt", "sie", "wei\u00b7ter", "nach", "der", "Wie\u00b7se", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun, mein Freund, so magst du lachen", "tokens": ["Nun", ",", "mein", "Freund", ",", "so", "magst", "du", "la\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber des Phantasten Frage;", "tokens": ["Ue\u00b7ber", "des", "Phan\u00b7tas\u00b7ten", "Fra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannst doch nicht zur T\u00e4uschung machen,", "tokens": ["Kannst", "doch", "nicht", "zur", "T\u00e4u\u00b7schung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich fest im Busen trage.", "tokens": ["Was", "ich", "fest", "im", "Bu\u00b7sen", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}