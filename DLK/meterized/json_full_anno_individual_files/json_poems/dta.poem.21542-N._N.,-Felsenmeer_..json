{"dta.poem.21542": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Felsenmeer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Felsen reiht sich an Felsen,", "tokens": ["Fel\u00b7sen", "reiht", "sich", "an", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und dem Auge des Menschen", "tokens": ["Und", "dem", "Au\u00b7ge", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Erscheint es, als seien es", "tokens": ["Er\u00b7scheint", "es", ",", "als", "sei\u00b7en", "es"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "M\u00e4cht\u2019ge erstarrte Wellen,", "tokens": ["M\u00e4cht'\u00b7ge", "er\u00b7starr\u00b7te", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Als h\u00e4tte des allm\u00e4chtigen Bildners", "tokens": ["Als", "h\u00e4t\u00b7te", "des", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Bild\u00b7ners"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wuchtige Hand", "tokens": ["Wuch\u00b7ti\u00b7ge", "Hand"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ein wogendes, brausendes,", "tokens": ["Ein", "wo\u00b7gen\u00b7des", ",", "brau\u00b7sen\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sch\u00e4umendes Meer", "tokens": ["Sch\u00e4u\u00b7men\u00b7des", "Meer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "In Stein gehauen.", "tokens": ["In", "Stein", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und in dem Volke schreitet die Sage,", "tokens": ["Und", "in", "dem", "Vol\u00b7ke", "schrei\u00b7tet", "die", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df hier dereinst eine See gebrandet,", "tokens": ["Da\u00df", "hier", "de\u00b7reinst", "ei\u00b7ne", "See", "ge\u00b7bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Eine durch Zauberers Spruch", "tokens": ["Ei\u00b7ne", "durch", "Zau\u00b7be\u00b7rers", "Spruch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Erstarrte, in Stein verwandelte", "tokens": ["Er\u00b7starr\u00b7te", ",", "in", "Stein", "ver\u00b7wan\u00b7del\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wilde, brausende See \u2026", "tokens": ["Wil\u00b7de", ",", "brau\u00b7sen\u00b7de", "See", "\u2026"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ach! wo einst das rastlose Leben", "tokens": ["Ach", "!", "wo", "einst", "das", "rast\u00b7lo\u00b7se", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Fluthen gebraust und gebrandet", "tokens": ["Der", "Flut\u00b7hen", "ge\u00b7braust", "und", "ge\u00b7bran\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wo sie in neckendem, fr\u00f6hlichem Spiel", "tokens": ["Wo", "sie", "in", "ne\u00b7cken\u00b7dem", ",", "fr\u00f6h\u00b7li\u00b7chem", "Spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sich \u00fcberst\u00fcrzten", "tokens": ["Sich", "\u00fc\u00b7bers\u00b7t\u00fcrz\u00b7ten"], "token_info": ["word", "word"], "pos": ["PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Oder voll titanischer Wuth", "tokens": ["O\u00b7der", "voll", "ti\u00b7ta\u00b7ni\u00b7scher", "Wuth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Sch\u00e4umten und rangen", "tokens": ["Sch\u00e4um\u00b7ten", "und", "ran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit dem Gestade in grausem Kriege,", "tokens": ["Mit", "dem", "Ge\u00b7sta\u00b7de", "in", "grau\u00b7sem", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Gef\u00fchrt durch den Sturmgott \u2014", "tokens": ["Ge\u00b7f\u00fchrt", "durch", "den", "Sturm\u00b7gott"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Dehnet sich heute ein steinernes,", "tokens": ["Deh\u00b7net", "sich", "heu\u00b7te", "ein", "stei\u00b7ner\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJA", "$,"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.10": {"text": "Graues, lebloses Abbild.", "tokens": ["Grau\u00b7es", ",", "leb\u00b7lo\u00b7ses", "Ab\u00b7bild", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4umend steh\u2019 ich, sinne und gr\u00fcble,", "tokens": ["Tr\u00e4u\u00b7mend", "steh'", "ich", ",", "sin\u00b7ne", "und", "gr\u00fcb\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und wie ich sinne, d\u00fcnkt mich, ich h\u00f6re", "tokens": ["Und", "wie", "ich", "sin\u00b7ne", ",", "d\u00fcnkt", "mich", ",", "ich", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den alten Mahnruf", "tokens": ["Den", "al\u00b7ten", "Mahn\u00b7ruf"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Zeit, der H\u00fcnin,", "tokens": ["Der", "Zeit", ",", "der", "H\u00fc\u00b7nin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die ewig geht und dennoch bleibt,", "tokens": ["Die", "e\u00b7wig", "geht", "und", "den\u00b7noch", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Alles verg\u00e4nglich", "tokens": ["Da\u00df", "Al\u00b7les", "ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und Alles eitel.", "tokens": ["Und", "Al\u00b7les", "ei\u00b7tel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ja! auch an uns", "tokens": ["Ja", "!", "auch", "an", "uns"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADV", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und unser F\u00fchlen", "tokens": ["Und", "un\u00b7ser", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und unsere Thaten", "tokens": ["Und", "un\u00b7se\u00b7re", "Tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mahnt nach einer winzigen Zahl", "tokens": ["Mahnt", "nach", "ei\u00b7ner", "win\u00b7zi\u00b7gen", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Winziger Jahre", "tokens": ["Win\u00b7zi\u00b7ger", "Jah\u00b7re"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nur solch ein starres", "tokens": ["Nur", "solch", "ein", "star\u00b7res"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Farbloses Abbild;", "tokens": ["Farb\u00b7lo\u00b7ses", "Ab\u00b7bild", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und kalt und lieblos", "tokens": ["Und", "kalt", "und", "lieb\u00b7los"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Schreitet ein neues Geschlecht", "tokens": ["Schrei\u00b7tet", "ein", "neu\u00b7es", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Ob unserm Grabe,", "tokens": ["Ob", "un\u00b7serm", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Das nichts mehr wei\u00df", "tokens": ["Das", "nichts", "mehr", "wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Von unserm Ringen", "tokens": ["Von", "un\u00b7serm", "Rin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Und unsern Qualen.", "tokens": ["Und", "un\u00b7sern", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Nur ein Gewaltiger noch,", "tokens": ["Nur", "ein", "Ge\u00b7wal\u00b7ti\u00b7ger", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Vielleicht ein F\u00fcrst oder Weiser,", "tokens": ["Viel\u00b7leicht", "ein", "F\u00fcrst", "o\u00b7der", "Wei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Ragt aus dem Schutte", "tokens": ["Ragt", "aus", "dem", "Schut\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Vergangener Zeiten.", "tokens": ["Ver\u00b7gan\u00b7ge\u00b7ner", "Zei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}