{"textgrid.poem.23906": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Fr\u00fchling kam, die Knospen sprangen,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling kam, die Knospen sprangen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kam", ",", "die", "Knos\u00b7pen", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da bin ich auf die Wiese,", "tokens": ["Da", "bin", "ich", "auf", "die", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja Wiese,", "tokens": ["Ja", "Wie\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Alleine hinausgegangen.", "tokens": ["Al\u00b7lei\u00b7ne", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich ging allein", "tokens": ["Ich", "ging", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und kam zu Zwein:", "tokens": ["Und", "kam", "zu", "Zwein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mit einem holden Kinde;", "tokens": ["Mit", "ei\u00b7nem", "hol\u00b7den", "Kin\u00b7de", ";"], "token_info": ["word", "w