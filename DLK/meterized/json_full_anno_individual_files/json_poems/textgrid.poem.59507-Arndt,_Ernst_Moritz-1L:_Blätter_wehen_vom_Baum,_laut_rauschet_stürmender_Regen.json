n Tod.", "tokens": ["Wo", "das", "Le\u00b7ben", "be\u00b7ginnt", ",", "su\u00b7che", "den", "lie\u00b7ben\u00b7den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Bl\u00e4tter wehen vom Baum, laut rauschet st\u00fcrmender Regen", "tokens": ["Bl\u00e4t\u00b7ter", "we\u00b7hen", "vom", "Baum", ",", "laut", "rau\u00b7schet", "st\u00fcr\u00b7men\u00b7der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ADJD", "VVFIN", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf die welken; sie fliehn mit ihm schwimmend hinab,", "tokens": ["Auf", "die", "wel\u00b7ken", ";", "sie", "fliehn", "mit", "ihm", "schwim\u00b7mend", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit ihm schwimmend zum Strom; fern tr\u00e4gt der starke zum Weltmeer,", "tokens": ["Mit", "ihm", "schwim\u00b7mend", "zum", "Strom", ";", "fern", "tr\u00e4gt", "der", "star\u00b7ke", "zum", "Welt\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN", "$.", "ADJD", "VVFIN", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Die im lustigen Gr\u00fcn s\u00e4uselten Liebenden einst,", "tokens": ["Die", "im", "lus\u00b7ti\u00b7gen", "Gr\u00fcn", "s\u00e4u\u00b7sel\u00b7ten", "Lie\u00b7ben\u00b7den