{"textgrid.poem.59507": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bl\u00e4tter wehen vom Baum, laut rauschet st\u00fcrmender Regen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00e4tter wehen vom Baum, laut rauschet st\u00fcrmender Regen", "tokens": ["Bl\u00e4t\u00b7ter", "we\u00b7hen", "vom", "Baum", ",", "laut", "rau\u00b7schet", "st\u00fcr\u00b7men\u00b7der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "ADJD", "VVFIN", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf die welken; sie fliehn mit ihm schwimmend hinab,", "tokens": ["Auf", "die", "wel\u00b7ken", ";", "sie", "fliehn", "mit", "ihm", "schwim\u00b7mend", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit ihm schwimmend zum Strom; fern tr\u00e4gt der starke zum Weltmeer,", "tokens": ["Mit", "ihm", "schwim\u00b7mend", "zum", "Strom", ";", "fern", "tr\u00e4gt", "der", "star\u00b7ke", "zum", "Welt\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN", "$.", "ADJD", "VVFIN", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Die im lustigen Gr\u00fcn s\u00e4uselten Liebenden einst,", "tokens": ["Die", "im", "lus\u00b7ti\u00b7gen", "Gr\u00fcn", "s\u00e4u\u00b7sel\u00b7ten", "Lie\u00b7ben\u00b7den", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "NN", "ADV", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die des V\u00f6geleins Nest in stille Schatten geborgen,", "tokens": ["Die", "des", "V\u00f6\u00b7ge\u00b7leins", "Nest", "in", "stil\u00b7le", "Schat\u00b7ten", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die des V\u00f6geleins Lied weckten im spielenden Reiz.", "tokens": ["Die", "des", "V\u00f6\u00b7ge\u00b7leins", "Lied", "weck\u00b7ten", "im", "spie\u00b7len\u00b7den", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Herbst, wie lehnst du dich ernst auf deine trauernde Urne!", "tokens": ["Herbst", ",", "wie", "lehnst", "du", "dich", "ernst", "auf", "dei\u00b7ne", "trau\u00b7ern\u00b7de", "Ur\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Wie erweckest du neu, was in dem Busen schon schlief!", "tokens": ["Wie", "er\u00b7we\u00b7ckest", "du", "neu", ",", "was", "in", "dem", "Bu\u00b7sen", "schon", "schlief", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Scheint das fr\u00f6hliche Leben nur gr\u00fcn im Spiegel des Todes?", "tokens": ["Scheint", "das", "fr\u00f6h\u00b7li\u00b7che", "Le\u00b7ben", "nur", "gr\u00fcn", "im", "Spie\u00b7gel", "des", "To\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Zeiget, was nichts ist, nur, was so lieblich einst war?", "tokens": ["Zei\u00b7get", ",", "was", "nichts", "ist", ",", "nur", ",", "was", "so", "lieb\u00b7lich", "einst", "war", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PIS", "VAFIN", "$,", "ADV", "$,", "PRELS", "ADV", "ADJD", "ADV", "VAFIN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Stirbt in dem Leben selbst des Lebens sch\u00f6nstes Ged\u00e4chtnis?", "tokens": ["Stirbt", "in", "dem", "Le\u00b7ben", "selbst", "des", "Le\u00b7bens", "sch\u00f6ns\u00b7tes", "Ge\u00b7d\u00e4cht\u00b7nis", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Gehn aus Verwesung allein G\u00f6tter und Manen empor?", "tokens": ["Gehn", "aus", "Ver\u00b7we\u00b7sung", "al\u00b7lein", "G\u00f6t\u00b7ter", "und", "Ma\u00b7nen", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "++-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "S\u00fc\u00dfe Liebe, du klingst mit den Schwanenfl\u00fcgeln des Lenzes", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7be", ",", "du", "klingst", "mit", "den", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7geln", "des", "Len\u00b7zes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Jugendlich hell um das Ohr, lange des Klanges entw\u00f6hnt?", "tokens": ["Ju\u00b7gend\u00b7lich", "hell", "um", "das", "Ohr", ",", "lan\u00b7ge", "des", "Klan\u00b7ges", "ent\u00b7w\u00f6hnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.15": {"text": "S\u00fc\u00dfe Liebe, du schlingst noch unverwelkliche Rosen", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7be", ",", "du", "schlingst", "noch", "un\u00b7ver\u00b7wel\u00b7kli\u00b7che", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+--+-+----+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Jugendlich frisch um die Stirn, welche die Trauer umh\u00fcllt?", "tokens": ["Ju\u00b7gend\u00b7lich", "frisch", "um", "die", "Stirn", ",", "wel\u00b7che", "die", "Trau\u00b7er", "um\u00b7h\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Ach! die holde Gestalt, womit du einst mich umfangen,", "tokens": ["Ach", "!", "die", "hol\u00b7de", "Ge\u00b7stalt", ",", "wo\u00b7mit", "du", "einst", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Liegt zerfallen als Staub unter den Modernden schon;", "tokens": ["Liegt", "zer\u00b7fal\u00b7len", "als", "Staub", "un\u00b7ter", "den", "Mo\u00b7dern\u00b7den", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KOKOM", "NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Blind wie lange das Aug', das mehr als Sterne des Himmels", "tokens": ["Blind", "wie", "lan\u00b7ge", "das", "Aug'", ",", "das", "mehr", "als", "Ster\u00b7ne", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "NN", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Blickt' Entz\u00fccken und Ruh' einst in das st\u00fcrmische Herz!", "tokens": ["Blickt'", "Ent\u00b7z\u00fc\u00b7cken", "und", "Ruh'", "einst", "in", "das", "st\u00fcr\u00b7mi\u00b7sche", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Und der Born des Gesangs, die Kehle, mit Erde gef\u00fcllet,", "tokens": ["Und", "der", "Born", "des", "Ge\u00b7sangs", ",", "die", "Keh\u00b7le", ",", "mit", "Er\u00b7de", "ge\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Die Philomelen gleich Fr\u00fchling und Liebe besang!", "tokens": ["Die", "Phi\u00b7lo\u00b7me\u00b7len", "gleich", "Fr\u00fch\u00b7ling", "und", "Lie\u00b7be", "be\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und die Lippen gebleicht wie lange, mit K\u00fcssen und Scherzen", "tokens": ["Und", "die", "Lip\u00b7pen", "ge\u00b7bleicht", "wie", "lan\u00b7ge", ",", "mit", "K\u00fcs\u00b7sen", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "KOKOM", "ADV", "$,", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Von den Grazien fr\u00fch schon in der Wiege getr\u00e4nkt!", "tokens": ["Von", "den", "Gra\u00b7zi\u00b7en", "fr\u00fch", "schon", "in", "der", "Wie\u00b7ge", "ge\u00b7tr\u00e4nkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Ach! umsonst lauscht oft das Ohr, den Klang zu vernehmen;", "tokens": ["Ach", "!", "um\u00b7sonst", "lauscht", "oft", "das", "Ohr", ",", "den", "Klang", "zu", "ver\u00b7neh\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Auch das g\u00f6ttliche Herz mu\u00dft' in das Dunkel hinab.", "tokens": ["Auch", "das", "g\u00f6tt\u00b7li\u00b7che", "Herz", "mu\u00dft'", "in", "das", "Dun\u00b7kel", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Komm' Erinnerung denn mit aller lieblichen Wehmut!", "tokens": ["Komm'", "E\u00b7rin\u00b7ne\u00b7rung", "denn", "mit", "al\u00b7ler", "lieb\u00b7li\u00b7chen", "Weh\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Urne, reiner entbl\u00fcht deinem Staube die Huld;", "tokens": ["Ur\u00b7ne", ",", "rei\u00b7ner", "ent\u00b7bl\u00fcht", "dei\u00b7nem", "Stau\u00b7be", "die", "Huld", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "G\u00f6ttlich steiget das Bild hell leuchtend \u00fcber den Lethe", "tokens": ["G\u00f6tt\u00b7lich", "stei\u00b7get", "das", "Bild", "hell", "leuch\u00b7tend", "\u00fc\u00b7ber", "den", "Le\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Auf, wenn den irdischen Staub f\u00fchrte die Welle hinweg.", "tokens": ["Auf", ",", "wenn", "den", "ir\u00b7di\u00b7schen", "Staub", "f\u00fchr\u00b7te", "die", "Wel\u00b7le", "hin\u00b7weg", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Aber meinen will ich in jedem bl\u00fchenden Lenze:", "tokens": ["A\u00b7ber", "mei\u00b7nen", "will", "ich", "in", "je\u00b7dem", "bl\u00fc\u00b7hen\u00b7den", "Len\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VMFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Als die Rose verdarb, starb mir das liebende Weib;", "tokens": ["Als", "die", "Ro\u00b7se", "ver\u00b7darb", ",", "starb", "mir", "das", "lie\u00b7ben\u00b7de", "Weib", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber weinen will ich in jedem welkenden Herbste,", "tokens": ["A\u00b7ber", "wei\u00b7nen", "will", "ich", "in", "je\u00b7dem", "wel\u00b7ken\u00b7den", "Herbs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Denn im Herbst erscheint, Tod, dein vielfaches Bild.", "tokens": ["Denn", "im", "Herbst", "er\u00b7scheint", ",", "Tod", ",", "dein", "viel\u00b7fa\u00b7ches", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Blumen will ich dir weihn, dir weihn die Gabe der Locken,", "tokens": ["Blu\u00b7men", "will", "ich", "dir", "weihn", ",", "dir", "weihn", "die", "Ga\u00b7be", "der", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und wenn Tr\u00e4nen auch euch wei\u00df die elysische Welt,", "tokens": ["Und", "wenn", "Tr\u00e4\u00b7nen", "auch", "euch", "wei\u00df", "die", "e\u00b7ly\u00b7si\u00b7sche", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Will ich das rieselnde Gras des stillen H\u00fcgels benetzen,", "tokens": ["Will", "ich", "das", "rie\u00b7seln\u00b7de", "Gras", "des", "stil\u00b7len", "H\u00fc\u00b7gels", "be\u00b7net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wann der einsame Mond sieht nur mich und das Grab.", "tokens": ["Wann", "der", "ein\u00b7sa\u00b7me", "Mond", "sieht", "nur", "mich", "und", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ach! du warst so hold, hingst lieb und liebend am Leben,", "tokens": ["Ach", "!", "du", "warst", "so", "hold", ",", "hingst", "lieb", "und", "lie\u00b7bend", "am", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "wo