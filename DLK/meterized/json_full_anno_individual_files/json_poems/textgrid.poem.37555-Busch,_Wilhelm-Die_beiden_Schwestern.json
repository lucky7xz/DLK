{"textgrid.poem.37555": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren mal zwei Schwestern,", "tokens": ["Es", "wa\u00b7ren", "mal", "zwei", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ich wei\u00df es noch wie gestern.", "tokens": ["ich", "wei\u00df", "es", "noch", "wie", "ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die eine namens Adelheid", "tokens": ["Die", "ei\u00b7ne", "na\u00b7mens", "A\u00b7del\u00b7heid"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "war faul und voller Eitelkeit.", "tokens": ["war", "faul", "und", "vol\u00b7ler", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die andre die hie\u00df K\u00e4thchen", "tokens": ["Die", "and\u00b7re", "die", "hie\u00df", "K\u00e4th\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ART", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und war ein gutes M\u00e4dchen.", "tokens": ["und", "war", "ein", "gu\u00b7tes", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie qu\u00e4lt sich ab von fr\u00fch bis sp\u00e4t,", "tokens": ["Sie", "qu\u00e4lt", "sich", "ab", "von", "fr\u00fch", "bis", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "APPR", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wenn Adelheid spazierengeht.", "tokens": ["wenn", "A\u00b7del\u00b7heid", "spa\u00b7zie\u00b7ren\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Adelheid trank roten Wein,", "tokens": ["Die", "A\u00b7del\u00b7heid", "trank", "ro\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "dem K\u00e4thchen schenkt sie Wasser ein.", "tokens": ["dem", "K\u00e4th\u00b7chen", "schenkt", "sie", "Was\u00b7ser", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst war dem K\u00e4thchen anbefohlen,", "tokens": ["Einst", "war", "dem", "K\u00e4th\u00b7chen", "an\u00b7be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "im Walde d\u00fcrres Holz zu holen.", "tokens": ["im", "Wal\u00b7de", "d\u00fcr\u00b7res", "Holz", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da sa\u00df an einem Wasser", "tokens": ["Da", "sa\u00df", "an", "ei\u00b7nem", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein Frosch, ein gr\u00fcner, nasser;", "tokens": ["ein", "Frosch", ",", "ein", "gr\u00fc\u00b7ner", ",", "nas\u00b7ser", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "der quackte ganz uns\u00e4glich,", "tokens": ["der", "quack\u00b7te", "ganz", "un\u00b7s\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "gottsj\u00e4mmerlich und kl\u00e4glich:", "tokens": ["gotts\u00b7j\u00e4m\u00b7mer\u00b7lich", "und", "kl\u00e4g\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bberbarme dich, erbarme dich,", "tokens": ["\u00bb", "er\u00b7bar\u00b7me", "dich", ",", "er\u00b7bar\u00b7me", "dich", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ach, k\u00fcsse und umarme mich!\u00ab", "tokens": ["ach", ",", "k\u00fcs\u00b7se", "und", "um\u00b7ar\u00b7me", "mich", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["XY", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das K\u00e4thchen denkt:", "tokens": ["Das", "K\u00e4th\u00b7chen", "denkt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "\u00bbich will's nur tun,", "tokens": ["\u00bb", "ich", "will's", "nur", "tun", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "sonst kann der arme", "tokens": ["sonst", "kann", "der", "ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Frosch nicht ruhn!\u00ab", "tokens": ["Frosch", "nicht", "ruhn", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der erste Ku\u00df", "tokens": ["Der", "ers\u00b7te", "Ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "schmeckt recht abscheulich.", "tokens": ["schmeckt", "recht", "ab\u00b7scheu\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Der gr\u00e4siggr\u00fcne Frosch", "tokens": ["Der", "gr\u00e4\u00b7sig\u00b7gr\u00fc\u00b7ne", "Frosch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "wird bl\u00e4ulich.", "tokens": ["wird", "bl\u00e4u\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Der zweite schmeckt schon", "tokens": ["Der", "zwei\u00b7te", "schmeckt", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "etwas besser;", "tokens": ["et\u00b7was", "bes\u00b7ser", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "der Frosch wird bunt", "tokens": ["der", "Frosch", "wird", "bunt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "und immer gr\u00f6\u00dfer.", "tokens": ["und", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Beim dritten gibt es ein Get\u00f6se,", "tokens": ["Beim", "drit\u00b7ten", "gibt", "es", "ein", "Ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als ob man die Kanonen l\u00f6se.", "tokens": ["als", "ob", "man", "die", "Ka\u00b7no\u00b7nen", "l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein hohes Schlo\u00df", "tokens": ["Ein", "ho\u00b7hes", "Schlo\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "steigt aus dem Moor,", "tokens": ["steigt", "aus", "dem", "Moor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "ein sch\u00f6ner Prinz", "tokens": ["ein", "sch\u00f6\u00b7ner", "Prinz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "steht vor dem Tor.", "tokens": ["steht", "vor", "dem", "Tor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er spricht:", "tokens": ["Er", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "\u00bblieb K\u00e4thchen, du allein", "tokens": ["\u00bb", "lieb", "K\u00e4th\u00b7chen", ",", "du", "al\u00b7lein"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "NE", "$,", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "sollst meine", "tokens": ["sollst", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Herzprinzessin sein!\u00ab", "tokens": ["Herz\u00b7prin\u00b7zes\u00b7sin", "sein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VAINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Nun ist das K\u00e4thchen hochbegl\u00fcckt,", "tokens": ["Nun", "ist", "das", "K\u00e4th\u00b7chen", "hoch\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "kriegt Kleider sch\u00f6n mit Gold gestickt", "tokens": ["kriegt", "Klei\u00b7der", "sch\u00f6n", "mit", "Gold", "ge\u00b7stickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADJD", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und trinkt mit ihrem Prinzgemahl", "tokens": ["und", "trinkt", "mit", "ih\u00b7rem", "Prinz\u00b7ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "aus einem goldenen Pokal.", "tokens": ["aus", "ei\u00b7nem", "gol\u00b7de\u00b7nen", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indessen ist die Adelheid", "tokens": ["In\u00b7des\u00b7sen", "ist", "die", "A\u00b7del\u00b7heid"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in ihrem neusten Sonntagskleid", "tokens": ["in", "ih\u00b7rem", "neus\u00b7ten", "Sonn\u00b7tags\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "herumspaziert an einem Weiher,", "tokens": ["her\u00b7um\u00b7spa\u00b7ziert", "an", "ei\u00b7nem", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "da sa\u00df ein Knabe mit der Leier.", "tokens": ["da", "sa\u00df", "ein", "Kna\u00b7be", "mit", "der", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Leier klang, der Knabe sang:", "tokens": ["Die", "Lei\u00b7er", "klang", ",", "der", "Kna\u00b7be", "sang", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbich liebe dich, bin treu gesinnt;", "tokens": ["\u00bb", "ich", "lie\u00b7be", "dich", ",", "bin", "treu", "ge\u00b7sinnt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "komm, k\u00fcsse mich, du h\u00fcbsches Kind!\u00ab", "tokens": ["komm", ",", "k\u00fcs\u00b7se", "mich", ",", "du", "h\u00fcb\u00b7sches", "Kind", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Kaum k\u00fc\u00dft sie ihn,", "tokens": ["Kaum", "k\u00fc\u00dft", "sie", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "so wird er gr\u00fcn,", "tokens": ["so", "wird", "er", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "so wird er struppig,", "tokens": ["so", "wird", "er", "strup\u00b7pig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "eiskalt und schuppig.", "tokens": ["eis\u00b7kalt", "und", "schup\u00b7pig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und ist, o Schreck!", "tokens": ["Und", "ist", ",", "o", "Schreck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "der alte, kalte Wasserneck.", "tokens": ["der", "al\u00b7te", ",", "kal\u00b7te", "Was\u00b7ser\u00b7neck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbha!\u00ab \u2013 lacht er \u2013 \u00bbdiese h\u00e4tten wir!!\u00ab", "tokens": ["\u00bb", "ha", "!", "\u00ab", "\u2013", "lacht", "er", "\u2013", "\u00bb", "die\u00b7se", "h\u00e4t\u00b7ten", "wir", "!!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "PDS", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00e4hrt bis auf den Grund mit ihr.", "tokens": ["Und", "f\u00e4hrt", "bis", "auf", "den", "Grund", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Da sitzt sie nun bei Wasserratzen,", "tokens": ["Da", "sitzt", "sie", "nun", "bei", "Was\u00b7ser\u00b7rat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mu\u00df Wassernickels Glatze kratzen,", "tokens": ["mu\u00df", "Was\u00b7ser\u00b7ni\u00b7ckels", "Glat\u00b7ze", "krat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "tr\u00e4gt einen Rock von rauhen Binsen,", "tokens": ["tr\u00e4gt", "ei\u00b7nen", "Rock", "von", "rau\u00b7hen", "Bin\u00b7sen", ","], "token_info": ["word", "word", "word", "word"