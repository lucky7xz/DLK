n bei Wasserratzen,", "tokens": ["Da", "sitzt", "sie", "nun", "bei", "Was\u00b7ser\u00b7rat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mu\u00df Wassernickels Glatze kratzen,", "tokens": ["mu\u00df", "Was\u00b7ser\u00b7ni\u00b7ckels", "Glat\u00b7ze", "krat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "tr\u00e4gt einen Rock von rauhen Binsen,", "tokens": ["tr\u00e4gt", "ei\u00b7nen", "Rock", "von", "rau\u00b7hen", "Bin\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "kriegt jeden Mittag Wasserlinsen;", "tokens": ["kriegt", "je\u00b7den", "Mit\u00b7tag", "Was\u00b7ser\u00b7lin\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und wenn sie etwas trinken mu\u00df,", "tokens": ["und", "wenn", "sie", "et\u00b7was", "trin\u00b7ken", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ist Wasser da im \u00dcberflu\u00df.", "tokens": ["ist", "Was\u00b7ser", "da", "im", "\u00dc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}