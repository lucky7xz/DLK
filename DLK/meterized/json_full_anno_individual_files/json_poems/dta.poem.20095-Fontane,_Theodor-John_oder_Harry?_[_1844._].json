{"dta.poem.20095": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "John oder Harry?  \n [ 1844. ]", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Da dr\u00fcben an des Rheines Borden,", "tokens": ["Da", "dr\u00fc\u00b7ben", "an", "des", "Rhei\u00b7nes", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Franzmanns ritterlicher Sinn", "tokens": ["Des", "Franz\u00b7manns", "rit\u00b7ter\u00b7li\u00b7cher", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ruft jenen Percy aus dem Norden,", "tokens": ["Ruft", "je\u00b7nen", "Per\u00b7cy", "aus", "dem", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Hei\u00dfsporn immer vor mich hin;", "tokens": ["Den", "Hei\u00df\u00b7sporn", "im\u00b7mer", "vor", "mich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Hei\u00dfsporn, der zum Zeitvertreibe", "tokens": ["Den", "Hei\u00df\u00b7sporn", ",", "der", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Die Schotten dutzendweis erstach,", "tokens": ["Die", "Schot\u00b7ten", "dut\u00b7zend\u00b7weis", "er\u00b7stach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jammernd dann zu seinem Weibe", "tokens": ["Und", "jam\u00b7mernd", "dann", "zu", "sei\u00b7nem", "Wei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von thatenlosem Dasein sprach.", "tokens": ["Von", "tha\u00b7ten\u00b7lo\u00b7sem", "Da\u00b7sein", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und seh ich dann auf Polstersitze", "tokens": ["Und", "seh", "ich", "dann", "auf", "Pols\u00b7ter\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich deutschen Michel hingestreckt:", "tokens": ["Dich", "deut\u00b7schen", "Mi\u00b7chel", "hin\u00b7ge\u00b7streckt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Mund voll selbstgef\u00e4llger Witze,", "tokens": ["Den", "Mund", "voll", "selbst\u00b7ge\u00b7f\u00e4ll\u00b7ger", "Wit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Hand \u2019nen Becher Sekt;", "tokens": ["Und", "in", "der", "Hand", "'nen", "Be\u00b7cher", "Sekt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird mir dazu die Augenweide", "tokens": ["Wird", "mir", "da\u00b7zu", "die", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von H\u00e4ngebauch und Doppelkinn,", "tokens": ["Von", "H\u00e4n\u00b7ge\u00b7bauch", "und", "Dop\u00b7pel\u00b7kinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will mir, zu meinem eignen Leide,", "tokens": ["Will", "mir", ",", "zu", "mei\u00b7nem", "eig\u00b7nen", "Lei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "John Falstaff gar nicht aus dem Sinn.", "tokens": ["John", "Fals\u00b7taff", "gar", "nicht", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wie, oder w\u00e4r dies Locker-leben", "tokens": ["Wie", ",", "o\u00b7der", "w\u00e4r", "dies", "Lo\u00b7cke\u00b7rle\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "VAFIN", "PDS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Klugheit Maske nur f\u00fcr Dich?", "tokens": ["Der", "Klug\u00b7heit", "Mas\u00b7ke", "nur", "f\u00fcr", "Dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst Du Dich aus dem Schlamm erheben", "tokens": ["Wirst", "Du", "Dich", "aus", "dem", "Schlamm", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wirst Du dereinst in Schlachtent\u00e4nzen,", "tokens": ["Wirst", "Du", "de\u00b7reinst", "in", "Schlach\u00b7ten\u00b7t\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bei Shrewsbury, auf blutgem Feld", "tokens": ["Bei", "Shrews\u00b7bu\u00b7ry", ",", "auf", "blut\u00b7gem", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Percy\u2019s Lorbeer Dich bekr\u00e4nzen?", "tokens": ["Mit", "Per\u00b7cy's", "Lor\u00b7beer", "Dich", "be\u00b7kr\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann gr\u00fc\u00df Dich Gott, Du Zukunftsheld.", "tokens": ["Dann", "gr\u00fc\u00df", "Dich", "Gott", ",", "Du", "Zu\u00b7kunfts\u00b7held", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}