{"dta.poem.6321": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Flammen irgendswo an einem Ort entstehn,", "tokens": ["Wenn", "Flam\u00b7men", "ir\u00b7gends\u00b7wo", "an", "ei\u00b7nem", "Ort", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Formiren sie leicht, hefftig und geschwinde,", "tokens": ["For\u00b7mi\u00b7ren", "sie", "leicht", ",", "heff\u00b7tig", "und", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Wofern sie frey, stets eine R\u00fcnde,", "tokens": ["Wo\u00b7fern", "sie", "frey", ",", "stets", "ei\u00b7ne", "R\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir w\u00fcrden sie in Form von einer Kugel sehn.", "tokens": ["Wir", "w\u00fcr\u00b7den", "sie", "in", "Form", "von", "ei\u00b7ner", "Ku\u00b7gel", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df aber sich das Feur, indem es aufw\u00e4rts steiget,", "tokens": ["Da\u00df", "a\u00b7ber", "sich", "das", "Feur", ",", "in\u00b7dem", "es", "auf\u00b7w\u00e4rts", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als eine Piramide zeiget;", "tokens": ["Als", "ei\u00b7ne", "Pi\u00b7ra\u00b7mi\u00b7de", "zei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Entsteht daher, da\u00df, von der Lufft umringet,", "tokens": ["Ent\u00b7steht", "da\u00b7her", ",", "da\u00df", ",", "von", "der", "Lufft", "um\u00b7rin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "KOUS", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die es zertheilt und durch sie dringet,", "tokens": ["Die", "es", "zer\u00b7theilt", "und", "durch", "sie", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich die Figur zusammen zieht,", "tokens": ["Sich", "die", "Fi\u00b7gur", "zu\u00b7sam\u00b7men", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "So da\u00df man an den Ort, durch welchen es sich zwinget,", "tokens": ["So", "da\u00df", "man", "an", "den", "Ort", ",", "durch", "wel\u00b7chen", "es", "sich", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "APPR", "ART", "NN", "$,", "APPR", "PWAT", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur einen spitzen Strich und keine Kugel sieht.", "tokens": ["Nur", "ei\u00b7nen", "spit\u00b7zen", "Strich", "und", "kei\u00b7ne", "Ku\u00b7gel", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}