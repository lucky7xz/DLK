{"textgrid.poem.40027": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein j\u00fcngst noch dick belaubter Baum", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein j\u00fcngst noch dick belaubter Baum", "tokens": ["Ein", "j\u00fcngst", "noch", "dick", "be\u00b7laub\u00b7ter", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah seines Wipfels Pracht erbleicht zu seinen F\u00fcssen,", "tokens": ["Sah", "sei\u00b7nes", "Wip\u00b7fels", "Pracht", "er\u00b7bleicht", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, wie des Bodens runder Raum,", "tokens": ["Und", ",", "wie", "des", "Bo\u00b7dens", "run\u00b7der", "Raum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den die so angenehm begr\u00fcnten Schatten", "tokens": ["Den", "die", "so", "an\u00b7ge\u00b7nehm", "be\u00b7gr\u00fcn\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So oft gesch\u00fctzt, so oft bedecket hatten,", "tokens": ["So", "oft", "ge\u00b7sch\u00fctzt", ",", "so", "oft", "be\u00b7de\u00b7cket", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den lieben Kinderchen zum Kirch-Hof werden m\u00fcssen.", "tokens": ["Den", "lie\u00b7ben", "Kin\u00b7der\u00b7chen", "zum", "Kirch\u00b7Hof", "wer\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ri\u00df der kalt' und rauhe Nord", "tokens": ["Es", "ri\u00df", "der", "kalt'", "und", "rau\u00b7he", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den d\u00fcnnen Ueberrest noch immer mit sich fort,", "tokens": ["Den", "d\u00fcn\u00b7nen", "Ue\u00b7ber\u00b7rest", "noch", "im\u00b7mer", "mit", "sich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie taumelten recht Schaaren-weis' herab,", "tokens": ["Sie", "tau\u00b7mel\u00b7ten", "recht", "Schaa\u00b7ren\u00b7weis'", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und suncken in das finstre Grab.", "tokens": ["Und", "sun\u00b7cken", "in", "das", "finst\u00b7re", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er schien, in dunckler Farb', ihr sterben zu betrauren,", "tokens": ["Er", "schien", ",", "in", "dunck\u00b7ler", "Fa\u00b7rb'", ",", "ihr", "ster\u00b7ben", "zu", "be\u00b7trau\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und, in der Kinder Fall, sich selber zu bedauren.", "tokens": ["Und", ",", "in", "der", "Kin\u00b7der", "Fall", ",", "sich", "sel\u00b7ber", "zu", "be\u00b7dau\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df heimliche Geseuftz, die\u00df still' und bange Klagen", "tokens": ["Die\u00df", "heim\u00b7li\u00b7che", "Ge\u00b7seuftz", ",", "die\u00df", "still'", "und", "ban\u00b7ge", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PDS", "PTKVZ", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vermochten einige der Bl\u00e4tter, die noch gr\u00fcn,", "tokens": ["Ver\u00b7moch\u00b7ten", "ei\u00b7ni\u00b7ge", "der", "Bl\u00e4t\u00b7ter", ",", "die", "noch", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und deren frische Farb' fast unverwercklich schien,", "tokens": ["Und", "de\u00b7ren", "fri\u00b7sche", "Fa\u00b7rb'", "fast", "un\u00b7ver\u00b7werck\u00b7lich", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nicht zu vertragen.", "tokens": ["Nicht", "zu", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie sprachen: Traure nicht! wir wollen bey dir bleiben,", "tokens": ["Sie", "spra\u00b7chen", ":", "Trau\u00b7re", "nicht", "!", "wir", "wol\u00b7len", "bey", "dir", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PTKNEG", "$.", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns wird kein Wind, kein Frost vertreiben.", "tokens": ["Uns", "wird", "kein", "Wind", ",", "kein", "Frost", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh nur, wie gr\u00fcn wir noch, wie frisch; wir f\u00fchlen nicht,", "tokens": ["Sieh", "nur", ",", "wie", "gr\u00fcn", "wir", "noch", ",", "wie", "frisch", ";", "wir", "f\u00fch\u00b7len", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "ADJD", "PPER", "ADV", "$,", "PWAV", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df uns, an Kraft, an Sch\u00f6nheit, was gebricht.", "tokens": ["Da\u00df", "uns", ",", "an", "Kraft", ",", "an", "Sch\u00f6n\u00b7heit", ",", "was", "ge\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein, fast in derselbigen Secunde,", "tokens": ["Al\u00b7lein", ",", "fast", "in", "der\u00b7sel\u00b7bi\u00b7gen", "Se\u00b7cun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "--+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erstarrt' ihr k\u00fchnes Wort in ihrem kleinen Munde.", "tokens": ["Er\u00b7starrt'", "ihr", "k\u00fch\u00b7nes", "Wort", "in", "ih\u00b7rem", "klei\u00b7nen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein kalter Hauch den Eurus von sich blie\u00df,", "tokens": ["Ein", "kal\u00b7ter", "Hauch", "den", "Eu\u00b7rus", "von", "sich", "blie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ihnen seine St\u00e4rck', und ihre Schw\u00e4che wies,", "tokens": ["Der", "ih\u00b7nen", "sei\u00b7ne", "St\u00e4rck", ",", "und", "ih\u00b7re", "Schw\u00e4\u00b7che", "wies", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Griff ihren zarten Leib so grimmig an,", "tokens": ["Griff", "ih\u00b7ren", "zar\u00b7ten", "Leib", "so", "grim\u00b7mig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ihnen Leben, Muth, und alle Kraft", "tokens": ["Da\u00df", "ih\u00b7nen", "Le\u00b7ben", ",", "Muth", ",", "und", "al\u00b7le", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vergieng, entwich, zerrann.", "tokens": ["Ver\u00b7gieng", ",", "ent\u00b7wich", ",", "zer\u00b7rann", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es stockt ihr Lebens-Saft;", "tokens": ["Es", "stockt", "ihr", "Le\u00b7bens\u00b7Saft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Es schrumpft ihr C\u00f6rper ein; sie zittern j\u00e4mmerlich;", "tokens": ["Es", "schrumpft", "ihr", "C\u00f6r\u00b7per", "ein", ";", "sie", "zit\u00b7tern", "j\u00e4m\u00b7mer\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein \u00e4ngstlich Seufzen scheint ihr lispelndes Gezische;", "tokens": ["Ein", "\u00e4ngst\u00b7lich", "Seuf\u00b7zen", "scheint", "ihr", "lis\u00b7peln\u00b7des", "Ge\u00b7zi\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Sie beben, und sie kr\u00fcmmen sich:", "tokens": ["Sie", "be\u00b7ben", ",", "und", "sie", "kr\u00fcm\u00b7men", "sich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es scheint, als ob man sie recht von den Zweigen wische.", "tokens": ["Es", "scheint", ",", "als", "ob", "man", "sie", "recht", "von", "den", "Zwei\u00b7gen", "wi\u00b7sche", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Sie hielten blo\u00df dar\u00fcm, dieweil die Reih", "tokens": ["Sie", "hiel\u00b7ten", "blo\u00df", "da\u00b7r\u00fcm", ",", "die\u00b7weil", "die", "Reih"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sie etwas sp\u00e4ter traff, sich fast vom welcken frey.", "tokens": ["Sie", "et\u00b7was", "sp\u00e4\u00b7ter", "traff", ",", "sich", "fast", "vom", "wel\u00b7cken", "frey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$,", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Lasst diese Bl\u00e4tterchen, ihr noch gesunden Alten,", "tokens": ["Lasst", "die\