{"dta.poem.21541": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Memnous Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Morgenstunde \u2014 noch ist Frieden", "tokens": ["Mor\u00b7gen\u00b7stun\u00b7de", "noch", "ist", "Frie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "VAFIN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Rings im Thal der Pyramiden,", "tokens": ["Rings", "im", "Thal", "der", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feurig durch des Ostens Thor,", "tokens": ["Feu\u00b7rig", "durch", "des", "Os\u00b7tens", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammen malen ihre Strahlen", "tokens": ["Flam\u00b7men", "ma\u00b7len", "ih\u00b7re", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An den Riesengr\u00e4bermalen,", "tokens": ["An", "den", "Rie\u00b7sen\u00b7gr\u00e4\u00b7ber\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steigt das Morgenroth empor.", "tokens": ["Steigt", "das", "Mor\u00b7gen\u00b7roth", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nun setzt es seinem Sohne", "tokens": ["Und", "nun", "setzt", "es", "sei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Memnon eine g\u00fcld\u2019ne Krone", "tokens": ["Mem\u00b7non", "ei\u00b7ne", "g\u00fcld'\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf das F\u00fcrstenhaupt von Stein:", "tokens": ["Auf", "das", "F\u00fcrs\u00b7ten\u00b7haupt", "von", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des G\u00f6ttersohnes Glieder", "tokens": ["Durch", "des", "G\u00f6t\u00b7ter\u00b7soh\u00b7nes", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geht ein Zittern, Klagelieder", "tokens": ["Geht", "ein", "Zit\u00b7tern", ",", "Kla\u00b7ge\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schallen schwermuthreich landein.", "tokens": ["Schal\u00b7len", "schwer\u00b7muth\u00b7reich", "lan\u00b7de\u00b7in", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Mutter, t\u00f6nt es von den kalten", "tokens": ["Mut\u00b7ter", ",", "t\u00f6nt", "es", "von", "den", "kal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lippen des Jahrtausendalten,", "tokens": ["Lip\u00b7pen", "des", "Jahr\u00b7tau\u00b7sen\u00b7dal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Ist er noch nicht da der Gott,", "tokens": ["Ist", "er", "noch", "nicht", "da", "der", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der der Dunkelm\u00e4nner Kronen", "tokens": ["Der", "der", "Dun\u00b7kel\u00b7m\u00e4n\u00b7ner", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bricht und Sch\u00e4cher st\u00fcrzt von Thronen", "tokens": ["Bricht", "und", "Sch\u00e4\u00b7cher", "st\u00fcrzt", "von", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Gro\u00dfen macht zum Spott?", "tokens": ["Und", "die", "Gro\u00b7\u00dfen", "macht", "zum", "Spott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Naht noch nicht der Wahrheitsender,", "tokens": ["Naht", "noch", "nicht", "der", "Wahr\u00b7hei\u00b7tsen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt noch nicht der Segenspender", "tokens": ["Kommt", "noch", "nicht", "der", "Se\u00b7gen\u00b7spen\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ormuzd auf der Lichtes Bahn,", "tokens": ["Or\u00b7muzd", "auf", "der", "Lich\u00b7tes", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er in des Orkus Kl\u00fcfte,", "tokens": ["Da\u00df", "er", "in", "des", "Or\u00b7kus", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Weltalls fernste Gr\u00fcfte", "tokens": ["In", "des", "Welt\u00b7alls", "ferns\u00b7te", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bannt den Todfeind Ahriman! \u2014", "tokens": ["Bannt", "den", "Tod\u00b7feind", "Ah\u00b7ri\u00b7man", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6her steigt der Sonne Wagen,", "tokens": ["H\u00f6\u00b7her", "steigt", "der", "Son\u00b7ne", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Weinen, leises Klagen", "tokens": ["Und", "in", "Wei\u00b7nen", ",", "lei\u00b7ses", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Endet Memnons Morgenlied;", "tokens": ["En\u00b7det", "Mem\u00b7nons", "Mor\u00b7gen\u00b7lied", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so wird es weiter t\u00f6nen", "tokens": ["Und", "so", "wird", "es", "wei\u00b7ter", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis herauf am gold\u2019nen sch\u00f6nen", "tokens": ["Bis", "her\u00b7auf", "am", "gold'\u00b7nen", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPRART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weltenmorgen Ormuzd zieht.", "tokens": ["Wel\u00b7ten\u00b7mor\u00b7gen", "Or\u00b7muzd", "zieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lange lag die Welt im Wahne,", "tokens": ["Lan\u00b7ge", "lag", "die", "Welt", "im", "Wah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner hielt empor die Fahne", "tokens": ["Kei\u00b7ner", "hielt", "em\u00b7por", "die", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenes Lichtgotts; nur allein", "tokens": ["Je\u00b7nes", "Licht\u00b7gotts", ";", "nur", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$.", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meldete des Lichts Gefunkel", "tokens": ["Mel\u00b7de\u00b7te", "des", "Lichts", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Weltnacht tiefem Dunkel", "tokens": ["In", "der", "Welt\u00b7nacht", "tie\u00b7fem", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Memnons kalter Mund von Stein.", "tokens": ["Mem\u00b7nons", "kal\u00b7ter", "Mund", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}