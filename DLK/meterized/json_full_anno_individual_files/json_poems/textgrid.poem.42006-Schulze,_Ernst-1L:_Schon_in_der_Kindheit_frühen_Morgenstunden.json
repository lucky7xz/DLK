{"textgrid.poem.42006": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon in der Kindheit fr\u00fchen Morgenstunden", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon in der Kindheit fr\u00fchen Morgenstunden", "tokens": ["Schon", "in", "der", "Kind\u00b7heit", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging nur auf dich mein Dichten und mein Streben;", "tokens": ["Ging", "nur", "auf", "dich", "mein", "Dich\u00b7ten", "und", "mein", "Stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch dich hat erst mein Geist dem niedern Leben,", "tokens": ["Durch", "dich", "hat", "erst", "mein", "Geist", "dem", "nie\u00b7dern", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Phantasie den Fesseln sich entwunden.", "tokens": ["Die", "Phan\u00b7ta\u00b7sie", "den", "Fes\u00b7seln", "sich", "ent\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als ich dich sah hab' ich zuerst empfunden,", "tokens": ["Als", "ich", "dich", "sah", "hab'", "ich", "zu\u00b7erst", "emp\u0