{"textgrid.poem.35651": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "14. An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, vergebens l\u00e4chelst du mir vom blauen", "tokens": ["Ach", ",", "ver\u00b7ge\u00b7bens", "l\u00e4\u00b7chelst", "du", "mir", "vom", "blau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Himmel, lieber Mond! Wie in Herbstgewittern,", "tokens": ["Him\u00b7mel", ",", "lie\u00b7ber", "Mond", "!", "Wie", "in", "Herbst\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "$.", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Ist voll d\u00fcstern Kummers mein Geist, voll tr\u00fcber", "tokens": ["Ist", "voll", "d\u00fcs\u00b7tern", "Kum\u00b7mers", "mein", "Geist", ",", "voll", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "VVFIN", "NN", "PPOSAT", "NN", "$,", "ADJD", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Z\u00e4hren mein Auge!", "tokens": ["Z\u00e4h\u00b7ren", "mein", "Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dreimal gingst du strahlenlos durch den \u00c4ther,", "tokens": ["Drei\u00b7mal", "gingst", "du", "strah\u00b7len\u00b7los", "durch", "den", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word