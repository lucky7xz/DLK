{"textgrid.poem.48208": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Fester Befehl", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Arkadien wurd' auch ", "tokens": ["In", "Ar\u00b7ka\u00b7di\u00b7en", "wurd'", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Ich ha\u00dfte Schranzen und F\u00fcrstenschmeichler,", "tokens": ["Ich", "ha\u00df\u00b7te", "Schran\u00b7zen", "und", "F\u00fcrs\u00b7ten\u00b7schmeich\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Glaubte beinah an Held und Eichler,", "tokens": ["Glaub\u00b7te", "bei\u00b7nah", "an", "Held", "und", "Eich\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und Herwegh, Karl Beck und Dingelsteten", "tokens": ["Und", "Her\u00b7wegh", ",", "Karl", "Beck", "und", "Din\u00b7gels\u00b7te\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "NE", "NE", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erhob ich zu meinen Leibpoeten.", "tokens": ["Er\u00b7hob", "ich", "zu", "mei\u00b7nen", "Leib\u00b7po\u00b7e\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bb ... Auf dem offenen Meere der Freiheit schwimmen ...", "tokens": ["\u00bb", "...", "Auf", "dem", "of\u00b7fe\u00b7nen", "Mee\u00b7re", "der", "Frei\u00b7heit", "schwim\u00b7men", "..."], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ein Volk mu\u00df immer sich selbst bestimmen,", "tokens": ["Ein", "Volk", "mu\u00df", "im\u00b7mer", "sich", "selbst", "be\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Volk geht immer die rechten Wege,", "tokens": ["Ein", "Volk", "geht", "im\u00b7mer", "die", "rech\u00b7ten", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nieder die Polizeigehege,", "tokens": ["Nie\u00b7der", "die", "Po\u00b7li\u00b7zei\u00b7ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nieder die possidentes beati \u2013\u00ab", "tokens": ["Nie\u00b7der", "die", "pos\u00b7si\u00b7den\u00b7tes", "be\u00b7a\u00b7ti", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NE", "$(", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So dacht' auch ich. Oh, tempi passati!", "tokens": ["So", "dacht'", "auch", "ich", ".", "Oh", ",", "tem\u00b7pi", "pas\u00b7sa\u00b7ti", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$.", "ITJ", "$,", "FM", "FM", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Freiheit freilich. Aber zum Schlimmen", "tokens": ["Frei\u00b7heit", "frei\u00b7lich", ".", "A\u00b7ber", "zum", "Schlim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "KON", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "F\u00fchrt der Masse sich selbst Bestimmen,", "tokens": ["F\u00fchrt", "der", "Mas\u00b7se", "sich", "selbst", "Be\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADV", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und das Kl\u00fcgste, das Beste, Bequemste,", "tokens": ["Und", "das", "Kl\u00fcgs\u00b7te", ",", "das", "Bes\u00b7te", ",", "Be\u00b7quems\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Das auch ", "tokens": ["Das", "auch"], "token_info": ["word", "word"], "pos": ["PDS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Hei\u00dft doch schlie\u00dflich, ich hab's nicht Hehl:", "tokens": ["Hei\u00dft", "doch", "schlie\u00df\u00b7lich", ",", "ich", "hab's", "nicht", "Hehl", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Festes Gesetz und fester Befehl.", "tokens": ["Fes\u00b7tes", "Ge\u00b7s