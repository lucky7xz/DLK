{"textgrid.poem.52491": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "11. Gott in der Natur", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gab mir, was ich hab' und bin?", "tokens": ["Wer", "gab", "mir", ",", "was", "ich", "hab'", "und", "bin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer schuf die weite Erde?", "tokens": ["Wer", "schuf", "die", "wei\u00b7te", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer pflanzte Felsenberge hin?", "tokens": ["Wer", "pflanz\u00b7te", "Fel\u00b7sen\u00b7ber\u00b7ge", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sprach zum Himmel: Werde!", "tokens": ["Wer", "sprach", "zum", "Him\u00b7mel", ":", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$.", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wem strahlt so flammend, gro\u00df und hehr", "tokens": ["Wem", "strahlt", "so", "flam\u00b7mend", ",", "gro\u00df", "und", "hehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der hohen Sonne Feuermeer?", "tokens": ["Der", "ho\u00b7hen", "Son\u00b7ne", "Feu\u00b7er\u00b7meer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem brausen mit so starker Macht", "tokens": ["Wem", "brau\u00b7sen", "mit", "so", "star\u00b7ker", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Waldstroms Silberwellen?", "tokens": ["Des", "Wald\u00b7stroms", "Sil\u00b7ber\u00b7wel\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer l\u00e4\u00dft den Blitz, die Wetternacht", "tokens": ["Wer", "l\u00e4\u00dft", "den", "Blitz", ",", "die", "Wet\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der fahlen Wolken hellen?", "tokens": ["Der", "fah\u00b7len", "Wol\u00b7ken", "hel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O sagt mir: Wessen Boten sind", "tokens": ["O", "sagt", "mir", ":", "Wes\u00b7sen", "Bo\u00b7ten", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Donner, der Gewitterwind?", "tokens": ["Der", "Don\u00b7ner", ",", "der", "Ge\u00b7wit\u00b7ter\u00b7wind", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er ist's! Er ist es, dessen Hand", "tokens": ["Er", "ist's", "!", "Er", "ist", "es", ",", "des\u00b7sen", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Abendr\u00f6te malet!", "tokens": ["Die", "A\u00b7bend\u00b7r\u00f6\u00b7te", "ma\u00b7let", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den Bogen ausgespannt,", "tokens": ["Er", "hat", "den", "Bo\u00b7gen", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der siebenfarbig strahlet.", "tokens": ["Der", "sie\u00b7ben\u00b7far\u00b7big", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er tr\u00e4nkt mit Regen und mit Tau", "tokens": ["Er", "tr\u00e4nkt", "mit", "Re\u00b7gen", "und", "mit", "Tau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ausged\u00f6rrte Halmenau.", "tokens": ["Die", "aus\u00b7ge\u00b7d\u00f6rr\u00b7te", "Hal\u00b7me\u00b7nau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er h\u00fcllt die Saat in wallend Gold,", "tokens": ["Er", "h\u00fcllt", "die", "Saat", "in", "wal\u00b7lend", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schwellt die vollen Garben.", "tokens": ["Er", "schwellt", "die", "vol\u00b7len", "Gar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schm\u00fcckt den Fr\u00fchling bunt und hold", "tokens": ["Er", "schm\u00fcckt", "den", "Fr\u00fch\u00b7ling", "bunt", "und", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gl\u00e4nzendlichten Farben.", "tokens": ["Mit", "gl\u00e4n\u00b7zend\u00b7lich\u00b7ten", "Far\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er l\u00e4\u00dft im Fr\u00fchling frisches Gr\u00fcn", "tokens": ["Er", "l\u00e4\u00dft", "im", "Fr\u00fch\u00b7ling", "fri\u00b7sches", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Haine und den Wald umziehn.", "tokens": ["Die", "Hai\u00b7ne", "und", "den", "Wald", "um\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es reift die Frucht auf sein Gebot", "tokens": ["Es", "reift", "die", "Frucht", "auf", "sein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am schwerbeladnen Baume;", "tokens": ["Am", "schwer\u00b7be\u00b7lad\u00b7nen", "Bau\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00e4rbt die s\u00fc\u00dfen Kirschen rot,", "tokens": ["Er", "f\u00e4rbt", "die", "s\u00fc\u00b7\u00dfen", "Kir\u00b7schen", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Violenblau die Pflaume.", "tokens": ["Vi\u00b7o\u00b7len\u00b7blau", "die", "Pflau\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Apfel schuf er voll und rund,", "tokens": ["Den", "Ap\u00b7fel", "schuf", "er", "voll", "und", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Birne saftig f\u00fcr den Mund.", "tokens": ["Die", "Bir\u00b7ne", "saf\u00b7tig", "f\u00fcr", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er streute, wie ein S\u00e4emann", "tokens": ["Er", "streu\u00b7te", ",", "wie", "ein", "S\u00e4e\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Furchenfeld die K\u00f6rner,", "tokens": ["Ins", "Fur\u00b7chen\u00b7feld", "die", "K\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sterne aus auf ihre Bahn;", "tokens": ["Die", "Ster\u00b7ne", "aus", "auf", "ih\u00b7re", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Mondes Silberh\u00f6rner", "tokens": ["Des", "Mon\u00b7des", "Sil\u00b7ber\u00b7h\u00f6r\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hing er leichtschwebend, wie ein Kahn,", "tokens": ["Hing", "er", "leicht\u00b7schwe\u00b7bend", ",", "wie", "ein", "Kahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An das Gew\u00f6lk des Himmels an.", "tokens": ["An", "das", "Ge\u00b7w\u00f6lk", "des", "Him\u00b7mels", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die ganze heilige Natur", "tokens": ["Die", "gan\u00b7ze", "hei\u00b7li\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist seiner Allmacht Zeuge;", "tokens": ["Ist", "sei\u00b7ner", "All\u00b7macht", "Zeu\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Anbeten, staunen kann ich nur \u2013", "tokens": ["An\u00b7be\u00b7ten", ",", "stau\u00b7nen", "kann", "ich", "nur", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "VMFIN", "PPER", "ADV", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich sinke hin und schweige.", "tokens": ["Ich", "sin\u00b7ke", "hin", "und", "schwei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tief, tief im Staube bin ich hier", "tokens": ["Tief", ",", "tief", "im", "Stau\u00b7be", "bin", "ich", "hier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "APPRART", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Gro\u00dfer, G\u00fctiger, vor dir!", "tokens": ["Du", "Gro\u00b7\u00dfer", ",", "G\u00fc\u00b7ti\u00b7ger", ",", "vor", "dir", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer gab mir, was ich hab' und bin?", "tokens": ["Wer", "gab", "mir", ",", "was", "ich", "hab'", "und", "bin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer schuf die weite Erde?", "tokens": ["Wer", "schuf", "die", "wei\u00b7te", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer pflanzte Felsenberge hin?", "tokens": ["Wer", "pflanz\u00b7te", "Fel\u00b7sen\u00b7ber\u00b7ge", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos":