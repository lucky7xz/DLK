{"textgrid.poem.33666": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Ich liebte dich", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebte dich, wie konnt' ich schweigen?", "tokens": ["Ich", "lieb\u00b7te", "dich", ",", "wie", "konnt'", "ich", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein tiefst Gem\u00fct lag frei vor dir.", "tokens": ["Mein", "tiefst", "Ge\u00b7m\u00fct", "lag", "frei", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wagt' es, dir mich ganz zu zeigen,", "tokens": ["Ich", "wagt'", "es", ",", "dir", "mich", "ganz", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du aber lie\u00dfest doch von mir.", "tokens": ["Du", "a\u00b7ber", "lie\u00b7\u00dfest", "doch", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ach, wie konnt' ich jemals hoffen,", "tokens": ["Doch", "ach", ",", "wie", "konnt'", "ich", "je\u00b7mals", "hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du w\u00fcrdest ruhn an einer Brust,", "tokens": ["Du", "w\u00fcr\u00b7dest", "ruhn", "an", "ei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die selbst noch allen St\u00fcrmen offen,", "tokens": ["Die", "selbst", "noch", "al\u00b7len", "St\u00fcr\u00b7men", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem Frieden je gewu\u00dft?", "tokens": ["Von", "kei\u00b7nem", "Frie\u00b7den", "je", "ge\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich liebte dich \u2013 ich darf es sagen!", "tokens": ["Ich", "lieb\u00b7te", "dich", "\u2013", "ich", "darf", "es", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hoffte \u2013 und mit welchem Schmerz!", "tokens": ["Ich", "hoff\u00b7te", "\u2013", "und", "mit", "wel\u00b7chem", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab' den Mut, mich anzuklagen;", "tokens": ["Ich", "hab'", "den", "Mut", ",", "mich", "an\u00b7zu\u00b7kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O, unaussprechlich litt mein Herz!", "tokens": ["O", ",", "un\u00b7aus\u00b7sprech\u00b7lich", "litt", "mein", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Leb wohl und m\u00f6g' dich Gott bewahren,", "tokens": ["Leb", "wohl", "und", "m\u00f6g'", "dich", "Gott", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "KON", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ein Atom nur jener Glut,", "tokens": ["Auch", "ein", "A\u00b7tom", "nur", "je\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich verzehrte, zu erfahren;", "tokens": ["Die", "mich", "ver\u00b7zehr\u00b7te", ",", "zu", "er\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur mein Herz dulde, bis es ruht!", "tokens": ["Nur", "mein", "Herz", "dul\u00b7de", ",", "bis", "es", "ruht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich liebte dich, wie konnt' ich schweigen?", "tokens": ["Ich", "lieb\u00b7te", "dich", ","