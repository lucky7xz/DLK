{"textgrid.poem.48113": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die frommen Jesuiten", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Lust der Welt ist eitel,", "tokens": ["Al\u00b7le", "Lust", "der", "Welt", "ist", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "s\u00fcndig ist ihr Glanz zu schaun \u2013", "tokens": ["s\u00fcn\u00b7dig", "ist", "ihr", "Glanz", "zu", "schaun", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "tuet in den Klingelbeutel", "tokens": ["tu\u00b7et", "in", "den", "Klin\u00b7gel\u00b7beu\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "eure Scherflein, fromme Fraun!", "tokens": ["eu\u00b7re", "Scher\u00b7flein", ",", "from\u00b7me", "Fraun", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt ihr auch das Brot nicht zahlen,", "tokens": ["K\u00f6nnt", "ihr", "auch", "das", "Brot", "nicht", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sch\u00fcttelt euch der K\u00e4lte Pein:", "tokens": ["sch\u00fct\u00b7telt", "euch", "der", "K\u00e4l\u00b7te",