{"textgrid.poem.61194": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der Hirsch, verletzt von Pfeil und Speer,", "tokens": ["So", "wie", "der", "Hirsch", ",", "ver\u00b7letzt", "von", "Pfeil", "und", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins Dickicht fleucht, um einsam zu verenden,", "tokens": ["Ins", "Di\u00b7ckicht", "fleucht", ",", "um", "ein\u00b7sam", "zu", "ver\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So flucht' ich mich zu deinen Felsenw\u00e4nden,", "tokens": ["So", "flucht'", "ich", "mich", "zu", "dei\u00b7nen", "Fel\u00b7sen\u00b7w\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu deinen stummen Grotten, ew'ges Meer.", "tokens": ["Zu", "dei\u00b7nen", "stum\u00b7men", "Grot\u00b7ten", ",", "ew'\u00b7ges", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist wund, und meine Seele schwer;", "tokens": ["Mein", "Herz", "ist", "wund", ",", "und", "mei\u00b7ne", "See\u00b7le", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort der Freiheit h\u00f6rt' ich t\u00e4glich sch\u00e4nden,", "tokens": ["Das", "Wort", "der", "Frei\u00b7heit", "h\u00f6rt'", "ich", "t\u00e4g\u00b7lich", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deren Amt es war, hier Trost zu spenden,", "tokens": ["Und", "de\u00b7ren", "Amt", "es", "war", ",", "hier", "Trost", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "VAFIN", "$,", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie trugen, sein zu walten, kein Begehr.", "tokens": ["Sie", "tru\u00b7gen", ",", "sein", "zu", "wal\u00b7ten", ",", "kein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAINF", "PTKZU", "VVINF", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum la\u00dft mich gehn! Hier, wo mit feuchten Schwingen", "tokens": ["Drum", "la\u00dft", "mich", "gehn", "!", "Hier", ",", "wo", "mit", "feuch\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "$.", "ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Winde tosen 