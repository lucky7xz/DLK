{"dta.poem.7016": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betrachte mich! schau' meine Pracht,", "tokens": ["Be\u00b7trach\u00b7te", "mich", "!", "schau'", "mei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein sch\u00f6nes Laub, den Stamm, die Zweige,", "tokens": ["Mein", "sch\u00f6\u00b7nes", "Laub", ",", "den", "Stamm", ",", "die", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "meine Bl\u00fchte,", "tokens": ["mei\u00b7ne", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Bewundre mich mit fr\u00f6lichem Gem\u00fcthe,", "tokens": ["Be\u00b7wund\u00b7re", "mich", "mit", "fr\u00f6\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und denck\u2019 in deiner Lust an Den, der mich gemacht!", "tokens": ["Und", "denck", "in", "dei\u00b7ner", "Lust", "an", "Den", ",", "der", "mich", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "APPR", "NE", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}