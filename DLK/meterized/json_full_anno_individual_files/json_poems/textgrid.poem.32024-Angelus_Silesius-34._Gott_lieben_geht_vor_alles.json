{"textgrid.poem.32024": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "34. Gott lieben geht vor alles", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df einen alle Lust der ganzen Welt genie\u00dfen", "tokens": ["La\u00df", "ei\u00b7nen", "al\u00b7le", "Lust", "der", "gan\u00b7zen", "Welt", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen dreimal mehr, als Salmon wu\u00dfte, wissen;", "tokens": ["Und", "ei\u00b7nen", "drei\u00b7mal", "mehr", ",", "als", "Sal\u00b7mon", "wu\u00df\u00b7te", ",", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "$,", "KOUS", "NE", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df einen sch\u00f6ner sein als Davids Absalon,", "tokens": ["La\u00df", "ei\u00b7nen", "sch\u00f6\u00b7ner", "sein", "als", "Da\u00b7vids", "Ab\u00b7sa\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "VAINF", "KOKOM", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gib einen, der mehr St\u00e4rk und Macht hat als Simson,", "tokens": ["Gib", "ei\u00b7nen", ",", "der", "mehr", "St\u00e4rk", "und", "Macht", "hat", "als", "Sim\u00b7son", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "$,", "PRELS", "PIAT", "NN", "KON", "NN", "VAFIN", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und einen, der mehr Gold als Kr\u00f6sus hat zu zeigen,", "tokens": ["Und", "ei\u00b7nen", ",", "der", "mehr", "Gold", "als", "Kr\u00f6\u00b7sus", "hat", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIAT", "NN", "KOUS", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und noch der alles kann, wie Alexander beugen,", "tokens": ["Und", "noch", "der", "al\u00b7les", "kann", ",", "wie", "A\u00b7lex\u00b7an\u00b7der", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "VMFIN", "$,", "PWAV", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, der dies alles ist, so sag ich doch ganz frei:", "tokens": ["Ja", ",", "der", "dies", "al\u00b7les", "ist", ",", "so", "sag", "ich", "doch", "ganz", "frei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PDS", "PIS", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df auch ein schlechter Mann, der Gott liebt, besser sei. ", "tokens": ["Da\u00df", "auch", "ein", "schlech\u00b7ter", "Mann", ",", "der", "Gott", "liebt", ",", "bes\u00b7ser", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"