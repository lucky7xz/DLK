{"dta.poem.10291": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Einer Toten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.71", "fr:0.28"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df du lebtest.             ", "tokens": ["Ach", ",", "da\u00df", "du", "leb\u00b7test", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die mich umflatterten auf allen Wegen,", "tokens": ["Die", "mich", "um\u00b7flat\u00b7ter\u00b7ten", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entflohen, wenn sich deine Tauben zeigten,", "tokens": ["Ent\u00b7flo\u00b7hen", ",", "wenn", "sich", "dei\u00b7ne", "Tau\u00b7ben", "zeig\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die wei\u00dfen Tauben deiner Fr\u00f6hlichkeit.", "tokens": ["Die", "wei\u00b7\u00dfen", "Tau\u00b7ben", "dei\u00b7ner", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df du noch lebtest.", "tokens": ["Da\u00df", "du", "noch", "leb\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Erde deinen Sarg und h\u00e4lt dich fest.", "tokens": ["Die", "Er\u00b7de", "dei\u00b7nen", "Sarg", "und", "h\u00e4lt", "dich", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich geh\u2019 nicht hin, ich finde dich nicht mehr.", "tokens": ["Ich", "geh'", "nicht", "hin", ",", "ich", "fin\u00b7de", "dich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Wiedersehn?", "tokens": ["Und", "Wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wenn wir zusammen Hosianna singen,", "tokens": ["Wenn", "wir", "zu\u00b7sam\u00b7men", "Ho\u00b7si\u00b7an\u00b7na", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ich dein Lachen nicht mehr h\u00f6ren kann?", "tokens": ["Und", "ich", "dein", "La\u00b7chen", "nicht", "mehr", "h\u00f6\u00b7ren", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Lachen, deine Sprache, deinen Trost:", "tokens": ["Dein", "La\u00b7chen", ",", "dei\u00b7ne", "Spra\u00b7che", ",", "dei\u00b7nen", "Trost", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Tag ist heut so sch\u00f6n, wo ist Chasseur,", "tokens": ["Der", "Tag", "ist", "heut", "so", "sch\u00f6n", ",", "wo", "ist", "Chas\u00b7seur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,", "PWAV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hol\u2019 aus dem Schranke deinen Lefaucheux,", "tokens": ["Hol'", "aus", "dem", "Schran\u00b7ke", "dei\u00b7nen", "Le\u00b7fauc\u00b7heux", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und geh\u2019 ins Feld, die H\u00fchner halten noch.", "tokens": ["Und", "geh'", "ins", "Feld", ",", "die", "H\u00fch\u00b7ner", "hal\u00b7ten", "noch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch bieg\u2019 nicht in das Buchenw\u00e4ldchen ein,", "tokens": ["Doch", "bieg'", "nicht", "in", "das", "Bu\u00b7chen\u00b7w\u00e4ld\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und leg\u2019 dich nicht ins Moos und tr\u00e4ume nicht.", "tokens": ["Und", "leg'", "dich", "nicht", "ins", "Moos", "und", "tr\u00e4u\u00b7me", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "APPRART", "NN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Pa\u00df auf die H\u00fchner und sei nicht zerstreut,", "tokens": ["Pa\u00df", "auf", "die", "H\u00fch\u00b7ner", "und", "sei", "nicht", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Blamir\u2019 dich nicht vor deinem Hund, ich bitte.", "tokens": ["Bla\u00b7mir'", "dich", "nicht", "vor", "dei\u00b7nem", "Hund", ",", "ich", "bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und alle Orgeldreher heut verw\u00fcnsch\u2019 ich,", "tokens": ["Und", "al\u00b7le", "Or\u00b7gel\u00b7dre\u00b7her", "heut", "ver\u00b7w\u00fcn\u00b7sch'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die luftgetragnen Ton von fernen D\u00f6rfern", "tokens": ["Die", "luft\u00b7ge\u00b7trag\u00b7nen", "Ton", "von", "fer\u00b7nen", "D\u00f6r\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dir zusenden, ich seh\u2019 dann keine H\u00fchner.", "tokens": ["Dir", "zu\u00b7sen\u00b7den", ",", "ich", "seh'", "dann", "kei\u00b7ne", "H\u00fch\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und doch, die braune Heide liegt so still,", "tokens": ["Und", "doch", ",", "die", "brau\u00b7ne", "Hei\u00b7de", "liegt", "so", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dich h\u00e4lt ihr Zauber, la\u00df dich nur bestricken.", "tokens": ["Dich", "h\u00e4lt", "ihr", "Zau\u00b7ber", ",", "la\u00df", "dich", "nur", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir essen heute Abend Erbsensuppe,", "tokens": ["Wir", "es\u00b7sen", "heu\u00b7te", "A\u00b7bend", "Erb\u00b7sen\u00b7sup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und der Margaux hat schon die Zimmerw\u00e4rme.", "tokens": ["Und", "der", "Mar\u00b7gaux", "hat", "schon", "die", "Zim\u00b7mer\u00b7w\u00e4r\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "--++-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Bring\u2019 also Hunger mit und gute Laune. \u2014", "tokens": ["Bring'", "al\u00b7so", "Hun\u00b7ger", "mit", "und", "gu\u00b7te", "Lau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "NN", "APPR", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann lieft du mir aus deinen Lieblingsdichtern.", "tokens": ["Dann", "lieft", "du", "mir", "aus", "dei\u00b7nen", "Lieb\u00b7lings\u00b7dich\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und willst du mehr, wir gehen an den Fl\u00fcgel,", "tokens": ["Und", "willst", "du", "mehr", ",", "wir", "ge\u00b7hen", "an", "den", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und singen Schumann, Robert Franz und Brahms.", "tokens": ["Und", "sin\u00b7gen", "Schu\u00b7mann", ",", "Ro\u00b7bert", "Franz", "und", "Brahms", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "NE", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Geldgeschichten lassen wir heut ruhn.", "tokens": ["Die", "Geld\u00b7ge\u00b7schich\u00b7ten", "las\u00b7sen", "wir", "heut", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du lieber Himmel, deine Gl\u00e4ubiger", "tokens": ["Du", "lie\u00b7ber", "Him\u00b7mel", ",", "dei\u00b7ne", "Gl\u00e4u\u00b7bi\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sind keine Teufel, die dich braten k\u00f6nnen,", "tokens": ["Sind", "kei\u00b7ne", "Teu\u00b7fel", ",", "die", "dich", "bra\u00b7ten", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und Alles wird sich machen.", "tokens": ["Und", "Al\u00b7les", "wird", "sich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich that dir guten Cognac in die Flasche.", "tokens": ["Ich", "that", "dir", "gu\u00b7ten", "Cog\u00b7nac", "in", "die", "Fla\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gr\u00fc\u00df Heide mir und Wald und all die Felder,", "tokens": ["Gr\u00fc\u00df", "Hei\u00b7de", "mir", "und", "Wald", "und", "all", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KON", "NN", "KON", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die abseits liegen und vergi\u00df die Schulden.", "tokens": ["Die", "ab\u00b7seits", "lie\u00b7gen", "und", "ver\u00b7gi\u00df", "die", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich seh\u2019 indessen in der K\u00fcche nach,", "tokens": ["Ich", "seh'", "in\u00b7des\u00b7sen", "in", "der", "K\u00fc\u00b7che", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df uns die Erbsensuppe nicht verbrennt. \u2014", "tokens": ["Da\u00df", "uns", "die", "Erb\u00b7sen\u00b7sup\u00b7pe", "nicht", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da\u00df du noch lebtest.", "tokens": ["Da\u00df", "du", "noch", "leb\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die mich umflatterten auf allen Wegen,", "tokens": ["Die", "mich", "um\u00b7flat\u00b7ter\u00b7ten", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entflohen, wenn sich deine Tauben zeigten,", "tokens": ["Ent\u00b7flo\u00b7hen", ",", "wenn", "sich", "dei\u00b7ne", "Tau\u00b7ben", "zeig\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wei\u00dfen Tauben deiner Fr\u00f6hlichkeit.", "tokens": ["Die", "wei\u00b7\u00dfen", "Tau\u00b7ben", "dei\u00b7ner", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach, da\u00df du lebtest.", "tokens": ["Ach", ",", "da\u00df", "du", "leb\u00b7test", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}