{"dta.poem.21574": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcde .  \n 1882.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O bange Stunden,", "tokens": ["O", "ban\u00b7ge", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wo alles Qual ist", "tokens": ["Wo", "al\u00b7les", "Qual", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und was empfunden,", "tokens": ["Und", "was", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Verrucht und schal ist.", "tokens": ["Ver\u00b7rucht", "und", "schal", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bald m\u00f6cht\u2019 in Thr\u00e4nen", "tokens": ["Bald", "m\u00f6cht'", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das Aug\u2019 zerflie\u00dfen,", "tokens": ["Das", "Aug'", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bald trotzig W\u00e4hnen", "tokens": ["Bald", "trot\u00b7zig", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das Herz verschlie\u00dfen.", "tokens": ["Das", "Herz", "ver\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "M\u00fcde zu hassen,", "tokens": ["M\u00fc\u00b7de", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "M\u00fcde der Liebe \u2014", "tokens": ["M\u00fc\u00b7de", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ach k\u00f6nnt\u2019 ich fassen,", "tokens": ["Ach", "k\u00f6nnt'", "ich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was ewig bliebe.", "tokens": ["Was", "e\u00b7wig", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}