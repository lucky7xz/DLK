{"dta.poem.6521": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag entweder schr\u00e4g\u2019, es mag gerade fallen,", "tokens": ["Es", "mag", "ent\u00b7we\u00b7der", "schr\u00e4g'", ",", "es", "mag", "ge\u00b7ra\u00b7de", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es mag vereint, getheilt, gebrochen, r\u00fcckwerts prallen;", "tokens": ["Es", "mag", "ver\u00b7eint", ",", "ge\u00b7theilt", ",", "ge\u00b7bro\u00b7chen", ",", "r\u00fcck\u00b7werts", "pral\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So theilt es sich", "tokens": ["So", "theilt", "es", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auch unsern Augen mit verschiedentlich.", "tokens": ["Auch", "un\u00b7sern", "Au\u00b7gen", "mit", "ver\u00b7schie\u00b7dent\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die C\u00f6rper, welche glatt und dicht,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "glatt", "und", "dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wodurch der Strahlen Sch\u00f6nheit nicht", "tokens": ["Wo\u00b7durch", "der", "Strah\u00b7len", "Sch\u00f6n\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschw\u00e4cht ist, dr\u00fccken sie hell, gl\u00e4ntzend, rein,", "tokens": ["Ge\u00b7schw\u00e4cht", "ist", ",", "dr\u00fc\u00b7cken", "sie", "hell", ",", "gl\u00e4nt\u00b7zend", ",", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schnell wiederum zur\u00fcck in hellen Wiederschein.", "tokens": ["Schnell", "wie\u00b7de\u00b7rum", "zu\u00b7r\u00fcck", "in", "hel\u00b7len", "Wie\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn aber sie die Krafft an andre C\u00f6rper brechen,", "tokens": ["Wenn", "a\u00b7ber", "sie", "die", "Krafft", "an", "and\u00b7re", "C\u00f6r\u00b7per", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sie zertheilen, biegen, schw\u00e4chen;", "tokens": ["Die", "sie", "zer\u00b7thei\u00b7len", ",", "bie\u00b7gen", ",", "schw\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So prallen sie auf unsre Blicke", "tokens": ["So", "pral\u00b7len", "sie", "auf", "uns\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So gl\u00e4ntzend lange nicht, und nicht so starck, zur\u00fccke.", "tokens": ["So", "gl\u00e4nt\u00b7zend", "lan\u00b7ge", "nicht", ",", "und", "nicht", "so", "starck", ",", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKNEG", "$,", "KON", "PTKNEG", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}