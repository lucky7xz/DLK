{"textgrid.poem.35765": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "4. An Grotthu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem J\u00fcngling zeigt die Welt ein Bild der Jugend;", "tokens": ["Dem", "J\u00fcng\u00b7ling", "zeigt", "die", "Welt", "ein", "Bild", "der", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sonnig wogt sein Weg bergab, bergan.", "tokens": ["Und", "son\u00b7nig", "wogt", "sein", "Weg", "berg\u00b7ab", ",", "ber\u00b7gan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Romantisch lacht ihm selbst die ernste Tugend;", "tokens": ["Ro\u00b7man\u00b7tisch", "lacht", "ihm", "selbst", "die", "erns\u00b7te", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie beut sich ihm mit ihren Kr\u00e4nzen an.", "tokens": ["Sie", "beut", "sich", "ihm", "mit", "ih\u00b7ren", "Kr\u00e4n\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er glaubt so gern bei frommen Huldigungen,", "tokens": ["Er", "glaubt", "so", "gern", "bei", "from\u00b7men", "Hul\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er habe sie, weil er sie liebt, errungen,", "tokens": ["Er", "ha\u00b7be", "sie", ",", "weil", "er", "sie", "liebt", ",", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ob auch f\u00fcr sie kein Schwei\u00df ihm noch entrann.", "tokens": ["Ob", "auch", "f\u00fcr", "sie", "kein", "Schwei\u00df", "ihm", "noch", "ent\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Begeistert schaut sein Blick in jene Ferne,", "tokens": ["Be\u00b7geis\u00b7tert", "schaut", "sein", "Blick", "in", "je\u00b7ne", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Labyrinth der Abendwelt hinaus;", "tokens": ["Ins", "La\u00b7by\u00b7rinth", "der", "Ab\u00b7end\u00b7welt", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Tag erscheint, und l\u00f6scht ihm seine Sterne,", "tokens": ["Der", "Tag", "er\u00b7scheint", ",", "und", "l\u00f6scht", "ihm", "sei\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wahrheit l\u00f6scht ihm seine Bilder aus.", "tokens": ["Die", "Wahr\u00b7heit", "l\u00f6scht", "ihm", "sei\u00b7ne", "Bil\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Morgenthal, wo ist es hingeschwunden?", "tokens": ["Das", "Mor\u00b7gen\u00b7thal", ",", "wo", "ist", "es", "hin\u00b7ge\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er fragt: wo sind die Nachtigallenstunden? \u2013", "tokens": ["Er", "fragt", ":", "wo", "sind", "die", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7stun\u00b7den", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zerflattert ist ihr kleines Bl\u00fctenhaus!", "tokens": ["Zer\u00b7flat\u00b7tert", "ist", "ihr", "klei\u00b7nes", "Bl\u00fc\u00b7ten\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Freund! unser Wandel ist ein Gang nach Morgen;", "tokens": ["Freund", "!", "un\u00b7ser", "Wan\u00b7del", "ist", "ein", "Gang", "nach", "Mor\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein langer Schatten l\u00e4uft uns lustig nach,", "tokens": ["Ein", "lan\u00b7ger", "Schat\u00b7ten", "l\u00e4uft", "uns", "lus\u00b7tig", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ist das Leben, mit verh\u00fcllten Sorgen;", "tokens": ["Es", "ist", "das", "Le\u00b7ben", ",", "mit", "ver\u00b7h\u00fcll\u00b7ten", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor uns die Welt, ein offnes Lustgemach;", "tokens": ["Vor", "uns", "die", "Welt", ",", "ein", "off\u00b7nes", "Lust\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch Abend wird's, und unsre Kr\u00e4ft' ermatten,", "tokens": ["Doch", "A\u00b7bend", "wird's", ",", "und", "uns\u00b7re", "Kr\u00e4ft'", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und vor uns schwebt der liebgewordne Schatten:", "tokens": ["Und", "vor", "uns", "schwebt", "der", "lieb\u00b7ge\u00b7word\u00b7ne", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA",