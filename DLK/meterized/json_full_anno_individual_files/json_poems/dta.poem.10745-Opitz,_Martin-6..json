{"dta.poem.10745": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ach liebster Schatz auff Erden/", "tokens": ["Ach", "liebs\u00b7ter", "Schatz", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warumb mich auelest so?", "tokens": ["Wa\u00b7rumb", "mich", "au\u00b7e\u00b7lest", "so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Zutheil la\u00df dich mir werden/", "tokens": ["Zu\u00b7theil", "la\u00df", "dich", "mir", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVIMP", "PPER", "PPER", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd mach mich endlich fro.", "tokens": ["Vnd", "mach", "mich", "end\u00b7lich", "fro", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein wil ich eigen sein.", "tokens": ["Dein", "wil", "ich", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In lieb vnd trew mich binde/", "tokens": ["In", "lieb", "vnd", "trew", "mich", "bin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit deiner hand mix winde", "tokens": ["Mit", "dei\u00b7ner", "hand", "mix", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Rosenkrentzelein.", "tokens": ["Ein", "Ro\u00b7sen\u00b7krent\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}