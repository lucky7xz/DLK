{"textgrid.poem.62248": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Gesetz \u00fcber die Pensionierung der Arbeiter", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Echo schall' es weit,", "tokens": ["Das", "E\u00b7cho", "schall'", "es", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es tagt Gerechtigkeit,", "tokens": ["Es", "tagt", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es lauschet Menschlichkeit:", "tokens": ["Es", "lau\u00b7schet", "Menschlich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kommt eine bessere Zeit?", "tokens": ["Kommt", "ei\u00b7ne", "bes\u00b7se\u00b7re", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ob Not und Elend flieht", "tokens": ["Ob", "Not", "und", "E\u00b7lend", "flieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus m\u00e4chtigstem Gebiet.", "tokens": ["Aus", "m\u00e4ch\u00b7tigs\u00b7tem", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es war ein Hohenzollernwort, \u2013", "tokens": ["Es", "war", "ein", "Ho\u00b7hen\u00b7zol\u00b7lern\u00b7wort", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kaiser Friedrich freut sich dort.", "tokens": ["Und", "Kai\u00b7ser", "Fried\u00b7rich", "freut", "sich", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}