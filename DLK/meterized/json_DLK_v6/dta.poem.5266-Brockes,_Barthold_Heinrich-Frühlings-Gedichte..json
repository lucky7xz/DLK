{"dta.poem.5266": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings-Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der strenge Winter ist vorbey, der laue Lentz erschei-\nnet wieder;", "tokens": ["Der", "stren\u00b7ge", "Win\u00b7ter", "ist", "vor\u00b7bey", ",", "der", "lau\u00b7e", "Lentz", "er\u00b7schei", "net", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "TRUNC", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Auf, auf, mein Geist! nimm alle Kraft und alle F\u00e4higkeit", "tokens": ["Auf", ",", "auf", ",", "mein", "Geist", "!", "nimm", "al\u00b7le", "Kraft", "und", "al\u00b7le", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Zu sehn, zu f\u00fchlen, zu bewundern! Auf bringe Danck-", "tokens": ["Zu", "sehn", ",", "zu", "f\u00fch\u00b7len", ",", "zu", "be\u00b7wun\u00b7dern", "!", "Auf", "brin\u00b7ge", "Dan\u00b7ck"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$.", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dem GOtt, aus dessen blossem Wollen, die Herrlichkei-", "tokens": ["Dem", "Gott", ",", "aus", "des\u00b7sen", "blos\u00b7sem", "Wol\u00b7len", ",", "die", "Herr\u00b7lich\u00b7kei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "La\u00dft uns von seiner G\u00fct und Lieb\u2019 und seiner weisen", "tokens": ["La\u00dft", "uns", "von", "sei\u00b7ner", "G\u00fct", "und", "Lieb'", "und", "sei\u00b7ner", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00dft uns, zu seinen heil\u2019gen Ehren, auch andern unsre", "tokens": ["La\u00dft", "uns", ",", "zu", "sei\u00b7nen", "heil'\u00b7gen", "Eh\u00b7ren", ",", "auch", "an\u00b7dern", "uns\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "PIS", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt zur holden Fr\u00fchlings-Zeit,", "tokens": ["Jetzt", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da sich die Natur erneuet,", "tokens": ["Da", "sich", "die", "Na\u00b7tur", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wird mit Lust und Lieblichkeit", "tokens": ["Wird", "mit", "Lust", "und", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Creatur erfreuet.", "tokens": ["Al\u00b7le", "Crea\u00b7tur", "er\u00b7freu\u00b7et", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Eine F\u00fclle von Vergn\u00fcgen", "tokens": ["Ei\u00b7ne", "F\u00fcl\u00b7le", "von", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seh\u2019 ich auf der Erde liegen,", "tokens": ["Seh'", "ich", "auf", "der", "Er\u00b7de", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf den klaren Fluten schwimmen,", "tokens": ["Auf", "den", "kla\u00b7ren", "Flu\u00b7ten", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den reinen L\u00fcften glimmen.", "tokens": ["In", "den", "rei\u00b7nen", "L\u00fcf\u00b7ten", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Es bebl\u00fcmen sich die Felder,", "tokens": ["Es", "be\u00b7bl\u00fc\u00b7men", "sich", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Es belauben sich die W\u00e4lder;", "tokens": ["Es", "be\u00b7lau\u00b7ben", "sich", "die", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jhre d\u00fcnn- und klaren Schatten", "tokens": ["Ih\u00b7re", "d\u00fcnn", "und", "kla\u00b7ren", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Zieren die begr\u00fcnten Matten.", "tokens": ["Zie\u00b7ren", "die", "be\u00b7gr\u00fcn\u00b7ten", "Mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "In der Thiere regem Blut", "tokens": ["In", "der", "Thie\u00b7re", "re\u00b7gem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Regt sich eine neue Glut,", "tokens": ["Regt", "sich", "ei\u00b7ne", "neu\u00b7e", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df sie fr\u00f6lich h\u00fcpfen, springen,", "tokens": ["Da\u00df", "sie", "fr\u00f6\u00b7lich", "h\u00fcp\u00b7fen", ",", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Fr\u00f6lich zwitschern, fr\u00f6lich singen.", "tokens": ["Fr\u00f6\u00b7lich", "zwit\u00b7schern", ",", "fr\u00f6\u00b7lich", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht das bl\u00fchende Geb\u00fcsche,", "tokens": ["Seht", "das", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht die Schuppen-reiche Fische,", "tokens": ["Seht", "die", "Schup\u00b7pen\u00b7rei\u00b7che", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt das Klingen, das Gezische", "tokens": ["H\u00f6rt", "das", "Klin\u00b7gen", ",", "das", "Ge\u00b7zi\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der gef\u00e4rbten V\u00f6gel an!", "tokens": ["Der", "ge\u00b7f\u00e4rb\u00b7ten", "V\u00f6\u00b7gel", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Riecht von so viel tausend Arten", "tokens": ["Riecht", "von", "so", "viel", "tau\u00b7send", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "ADV", "CARD", "NN"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.6": {"text": "Blumen in dem bunten Garten!", "tokens": ["Blu\u00b7men", "in", "dem", "bun\u00b7ten", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchlt das Schmeicheln lauer D\u00fcfte!", "tokens": ["F\u00fchlt", "das", "Schmei\u00b7cheln", "lau\u00b7er", "D\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6rt des S\u00e4useln linder L\u00fcfte!", "tokens": ["H\u00f6rt", "des", "S\u00e4u\u00b7seln", "lin\u00b7der", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seht, wie dort auf glatter Flut", "tokens": ["Seht", ",", "wie", "dort", "auf", "glat\u00b7ter", "Flut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Die Sapphirne Himmels Glut,", "tokens": ["Die", "Sap\u00b7phir\u00b7ne", "Him\u00b7mels", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "In schmaragdnen Ufern, ruht.", "tokens": ["In", "schma\u00b7ragd\u00b7nen", "U\u00b7fern", ",", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Seht wie ihr polierter Spiegel", "tokens": ["Seht", "wie", "ihr", "po\u00b7lier\u00b7ter", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Blumen, Kr\u00e4uter, Busch und H\u00fcgel", "tokens": ["Blu\u00b7men", ",", "Kr\u00e4u\u00b7ter", ",", "Busch", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Lieblich, nach dem Leben mahlt!", "tokens": ["Lieb\u00b7lich", ",", "nach", "dem", "Le\u00b7ben", "mahlt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Gleicht nicht die bebl\u00fchmte Wiese,", "tokens": ["Gleicht", "nicht", "die", "be\u00b7bl\u00fchm\u00b7te", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Von der Sonnen \u00fcberstrahlt,", "tokens": ["Von", "der", "Son\u00b7nen", "\u00fc\u00b7bers\u00b7trahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Gleichsam einem Paradiese?", "tokens": ["Gleich\u00b7sam", "ei\u00b7nem", "Pa\u00b7ra\u00b7die\u00b7se", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Alles was mein Auge siehet", "tokens": ["Al\u00b7les", "was", "mein", "Au\u00b7ge", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Pranget, funckelt, gl\u00e4ntzt und gl\u00fchet,", "tokens": ["Pran\u00b7get", ",", "fun\u00b7ckelt", ",", "gl\u00e4ntzt", "und", "gl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Scheinet, schimmert, gr\u00fcn\u2019t und bl\u00fchet.", "tokens": ["Schei\u00b7net", ",", "schim\u00b7mert", ",", "gr\u00fcn't", "und", "bl\u00fc\u00b7het", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Meine Seele wird erquickt,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "wird", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Wenn sie, wie die Welt geschm\u00fcckt,", "tokens": ["Wenn", "sie", ",", "wie", "die", "Welt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Sch\u00f6ner Lentz, in dir erblickt!", "tokens": ["Sch\u00f6\u00b7ner", "Lentz", ",", "in", "dir", "er\u00b7blickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Wenn ich an so mancher Stelle", "tokens": ["Wenn", "ich", "an", "so", "man\u00b7cher", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Dieser Wunder Meuge seh,", "tokens": ["Die\u00b7ser", "Wun\u00b7der", "Meu\u00b7ge", "seh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Zieht mein Geist sich in die H\u00f6h\u2019,", "tokens": ["Zieht", "mein", "Geist", "sich", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Suchet aller Wunder Quelle.", "tokens": ["Su\u00b7chet", "al\u00b7ler", "Wun\u00b7der", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Da nun f\u00e4llt der Sonnen Licht", "tokens": ["Da", "nun", "f\u00e4llt", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Alsobald mir ins Gesicht,", "tokens": ["Al\u00b7so\u00b7bald", "mir", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Diese giebt mir zu erkennen,", "tokens": ["Die\u00b7se", "giebt", "mir", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Da\u00df die Wunder auf der Erden", "tokens": ["Da\u00df", "die", "Wun\u00b7der", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und derselben holde Zier", "tokens": ["Und", "der\u00b7sel\u00b7ben", "hol\u00b7de", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Form und Farben blos von ihr", "tokens": ["Form", "und", "Far\u00b7ben", "blos", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wunderbar gewircket werden.", "tokens": ["Wun\u00b7der\u00b7bar", "ge\u00b7wir\u00b7cket", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dieweil der Sonnen Gl\u00e4ntzen", "tokens": ["Doch", "die\u00b7weil", "der", "Son\u00b7nen", "Gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Maasse, Schrancken hat und Gr\u00e4ntzen;", "tokens": ["Maas\u00b7se", ",", "Schran\u00b7cken", "hat", "und", "Gr\u00e4nt\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeigt sich, da\u00df ihr herrlich Licht", "tokens": ["Zeigt", "sich", ",", "da\u00df", "ihr", "herr\u00b7lich", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6n, doch keine GOttheit nicht.", "tokens": ["Sch\u00f6n", ",", "doch", "kei\u00b7ne", "Got\u00b7theit", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch f\u00fchrt sie uns am h\u00f6chsten", "tokens": ["Den\u00b7noch", "f\u00fchrt", "sie", "uns", "am", "h\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der GOttheit fast am n\u00e4chsten,", "tokens": ["Und", "der", "Got\u00b7theit", "fast", "am", "n\u00e4chs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPRART", "ADJA", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Welche meine Seel\u2019 in mir,", "tokens": ["Wel\u00b7che", "mei\u00b7ne", "Seel'", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie sich selbst, nicht sehen kann,", "tokens": ["Wie", "sich", "selbst", ",", "nicht", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "$,", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Darum bet\u2019 ich oft in ihr,", "tokens": ["Da\u00b7rum", "bet'", "ich", "oft", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "In der Sonnen Kraft und Zier,", "tokens": ["In", "der", "Son\u00b7nen", "Kraft", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Jhr, und meinen Sch\u00f6pfer an.", "tokens": ["Ihr", ",", "und", "mei\u00b7nen", "Sch\u00f6p\u00b7fer", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn wir also sehn und sp\u00fchren", "tokens": ["Wenn", "wir", "al\u00b7so", "sehn", "und", "sp\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alle Wunder, die uns r\u00fchren", "tokens": ["Al\u00b7le", "Wun\u00b7der", ",", "die", "uns", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "In der holden Fr\u00fchlings-Zeit,", "tokens": ["In", "der", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "La\u00dft, durch frohes Sehn und H\u00f6ren,", "tokens": ["La\u00dft", ",", "durch", "fro\u00b7hes", "Sehn", "und", "H\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Uns den grossen Ursprung ehren,", "tokens": ["Uns", "den", "gros\u00b7sen", "Ur\u00b7sprung", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Der so wol die Herrlichkeit", "tokens": ["Der", "so", "wol", "die", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Und der Sonnen Licht und Pracht,", "tokens": ["Und", "der", "Son\u00b7nen", "Licht", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.22": {"text": "Als die gantze Welt, gemacht,", "tokens": ["Als", "die", "gant\u00b7ze", "Welt", ",", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und aus dessen blossen Willen", "tokens": ["Und", "aus", "des\u00b7sen", "blos\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Aller Dinge Wesen quillen.", "tokens": ["Al\u00b7ler", "Din\u00b7ge", "We\u00b7sen", "quil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Grosse GOttheit, la\u00df die Lust", "tokens": ["Gros\u00b7se", "Got\u00b7theit", ",", "la\u00df", "die", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Unsrer von dem Wunderschein", "tokens": ["Uns\u00b7rer", "von", "dem", "Wun\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Deiner Werck\u2019 erf\u00fcllten Brust", "tokens": ["Dei\u00b7ner", "Werck", "er\u00b7f\u00fcll\u00b7ten", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Dir, durch dich, gef\u00e4llig seyn!", "tokens": ["Dir", ",", "durch", "dich", ",", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", 