{"textgrid.poem.33834": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4te Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4te Liebe \u2013 gl\u00e4nzt sie nicht,", "tokens": ["Sp\u00e4\u00b7te", "Lie\u00b7be", "\u2013", "gl\u00e4nzt", "sie", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie in Mitternacht ein Licht,", "tokens": ["Wie", "in", "Mit\u00b7ter\u00b7nacht", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das auf weites Schneegefild", "tokens": ["Das", "auf", "wei\u00b7tes", "Schnee\u00b7ge\u00b7fild"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Einsam leuchtet, still und mild?", "tokens": ["Ein\u00b7sam", "leuch\u00b7tet", ",", "still", "und", "mild", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Man w\u00e4hnt, man hofft,", "tokens": ["Man", "w\u00e4hnt", ",", "man", "hofft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Klug zu werden und wird nur zahm.", "tokens": ["Klug", "zu", "wer\u00b7den", "und", "wird", "nur", "zahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Man glaubt gar oft,", "tokens": ["Man", "glaubt", "gar", "oft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Weise zu sein und ist nur lahm.", "tokens": ["Wei\u00b7se", "zu", "sein", "und", "ist", "nur", "lahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "KON", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}