{"dta.poem.23744": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Seifenblase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Wie spielt die sch\u00f6ne Blase nicht", "tokens": ["Wie", "spielt", "die", "sch\u00f6\u00b7ne", "Bla\u00b7se", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bunt am goldnen Sonnenlicht?", "tokens": ["So", "bunt", "am", "gold\u00b7nen", "Son\u00b7nen\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein, ein Hauch! weg ist die Pracht,", "tokens": ["Al\u00b7lein", ",", "ein", "Hauch", "!", "weg", "ist", "die", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrer wird nicht mehr gedacht.", "tokens": ["Und", "ih\u00b7rer", "wird", "nicht", "mehr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr ist ein junges Herrchen gleich,", "tokens": ["Ihr", "ist", "ein", "jun\u00b7ges", "Herr\u00b7chen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stolz auf sein Kleid, vom Golde reich,", "tokens": ["Stolz", "auf", "sein", "Kleid", ",", "vom", "Gol\u00b7de", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst aber an Verdiensten leer:", "tokens": ["Selbst", "a\u00b7ber", "an", "Ver\u00b7diens\u00b7ten", "leer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man nehm es ihm, so bleibt nichts mehr.", "tokens": ["Man", "nehm", "es", "ihm", ",", "so", "bleibt", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}