{"textgrid.poem.26328": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wei\u00dfe Haare", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt funkeln mir im dunkeln Haar", "tokens": ["Jetzt", "fun\u00b7keln", "mir", "im", "dun\u00b7keln", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon wei\u00dfer Haare Spitzen.", "tokens": ["Schon", "wei\u00b7\u00dfer", "Haa\u00b7re", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist, als ob Erinnerungen blitzen", "tokens": ["Es", "ist", ",", "als", "ob", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "blit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dem, was einmal war.", "tokens": ["Von", "dem", ",", "was", "ein\u00b7mal", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und immer mehr wird ihre helle Schar.", "tokens": ["Und", "im\u00b7mer", "mehr", "wird", "ih\u00b7re", "hel\u00b7le", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Ich seh' mich bald mit wei\u00dfem Haare sitzen.", "tokens": ["Ich", "seh'", "mich", "bald", "mit", "wei\u00b7\u00dfem", "Haa\u00b7re", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben dringt dann nur noch durch die Ritzen.", "tokens": ["Das", "Le\u00b7ben", "dringt", "dann", "nur", "noch", "durch", "die", "Rit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stumm lausche ich, verschneit, dem letzten Lebensjahr.", "tokens": ["Stumm", "lau\u00b7sche", "ich", ",", "ver\u00b7schneit", ",", "dem", "letz\u00b7ten", "Le\u00b7bens\u00b7jahr", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}