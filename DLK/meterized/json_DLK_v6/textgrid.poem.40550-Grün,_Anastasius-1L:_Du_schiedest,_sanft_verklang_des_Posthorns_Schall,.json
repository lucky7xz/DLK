{"textgrid.poem.40550": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du schiedest, sanft verklang des Posthorns Schall,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schiedest, sanft verklang des Posthorns Schall,", "tokens": ["Du", "schie\u00b7dest", ",", "sanft", "ver\u00b7klang", "des", "Post\u00b7horns", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lang wiederholt von Fels und Wasserfall;", "tokens": ["Lang", "wie\u00b7der\u00b7holt", "von", "Fels", "und", "Was\u00b7ser\u00b7fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir aber schien's des alten Berggeists Sang,", "tokens": ["Mir", "a\u00b7ber", "schien's", "des", "al\u00b7ten", "Berg\u00b7geists", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der liebevoll dir nach zur Ferne klang:", "tokens": ["Der", "lie\u00b7be\u00b7voll", "dir", "nach", "zur", "Fer\u00b7ne", "klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbso lebe wohl denn, du mein liebster Gast,", "tokens": ["\u00bb", "so", "le\u00b7be", "wohl", "denn", ",", "du", "mein", "liebs\u00b7ter", "Gast", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "ADV", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der, was ich bieten kann, du selbst schon hast!", "tokens": ["Der", ",", "was", "ich", "bie\u00b7ten", "kann", ",", "du", "selbst", "schon", "hast", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht lieb' ich sieche Bettler, die nur flehn,", "tokens": ["Nicht", "lieb'", "ich", "sie\u00b7che", "Bett\u00b7ler", ",", "die", "nur", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADJA", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch M\u00e4nner, die als Gleiche vor mir stehn.", "tokens": ["Doch", "M\u00e4n\u00b7ner", ",", "die", "als", "Glei\u00b7che", "vor", "mir", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "KOUS", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erhaben sind, wie meiner Felsen Firn,", "tokens": ["Er\u00b7ha\u00b7ben", "sind", ",", "wie", "mei\u00b7ner", "Fel\u00b7sen", "Firn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Lichtgedanken einer Mannesstirn;", "tokens": ["Die", "Licht\u00b7ge\u00b7dan\u00b7ken", "ei\u00b7ner", "Man\u00b7ness\u00b7tirn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Blumenpracht im Alpenthal mir bl\u00fcht,", "tokens": ["Wie", "Blu\u00b7men\u00b7pracht", "im", "Al\u00b7pen\u00b7thal", "mir", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So wogt und gl\u00fcht Gef\u00fchl dir im Gem\u00fcth;", "tokens": ["So", "wogt", "und", "gl\u00fcht", "Ge\u00b7f\u00fchl", "dir", "im", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wie mein Busen birgt manch g\u00fclden Erz,", "tokens": ["Und", "wie", "mein", "Bu\u00b7sen", "birgt", "manch", "g\u00fcl\u00b7den", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So hegt manch Goldkorn tief und still dein Herz;", "tokens": ["So", "hegt", "manch", "Gold\u00b7korn", "tief", "und", "still", "dein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie sich mein Katarakt durch Felsen schl\u00e4gt,", "tokens": ["Wie", "sich", "mein", "Ka\u00b7ta\u00b7rakt", "durch", "Fel\u00b7sen", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wallt frei dein Manneswort, trifft und bewegt;", "tokens": ["Wallt", "frei", "dein", "Man\u00b7nes\u00b7wort", ",", "trifft", "und", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und wie mein Heilquell welke Blumen hebt,", "tokens": ["Und", "wie", "mein", "Heil\u00b7quell", "wel\u00b7ke", "Blu\u00b7men", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hat deine Huld manch trauernd Herz belebt. \u2013", "tokens": ["Hat", "dei\u00b7ne", "Huld", "manch", "trau\u00b7ernd", "Herz", "be\u00b7lebt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PIAT", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der so gesund an Seel' und K\u00f6rper ist,", "tokens": ["Der", "so", "ge\u00b7sund", "an", "Seel'", "und", "K\u00f6r\u00b7per", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nichts kann ich bieten dir; bleib' wie du bist!", "tokens": ["Nichts", "kann", "ich", "bie\u00b7ten", "dir", ";", "bleib'", "wie", "du", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Aufrecht und grad' wie meiner Tannen Schaft,", "tokens": ["Auf\u00b7recht", "und", "grad'", "wie", "mei\u00b7ner", "Tan\u00b7nen", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "KOKOM", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Behend wie meiner Gemsen Federkraft!", "tokens": ["Be\u00b7hend", "wie", "mei\u00b7ner", "Gem\u00b7sen", "Fe\u00b7der\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Das Schneehaupt selbst, wie meiner Gletscher Eis,", "tokens": ["Das", "Schnee\u00b7haupt", "selbst", ",", "wie", "mei\u00b7ner", "Glet\u00b7scher", "Eis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ist dir nicht Last, nein, Schmuck und Ehrenpreis!", "tokens": ["Ist", "dir", "nicht", "Last", ",", "nein", ",", "Schmuck", "und", "Eh\u00b7ren\u00b7preis", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,", "PTKANT", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ein ganzer Mann, dem meine Alpenwelt", "tokens": ["Ein", "gan\u00b7zer", "Mann", ",", "dem", "mei\u00b7ne", "Al\u00b7pen\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Den Spiegel eigner Gr\u00f6\u00df' entgegenh\u00e4lt!\u00ab", "tokens": ["Den", "Spie\u00b7gel", "eig\u00b7ner", "Gr\u00f6\u00df'", "ent\u00b7ge\u00b7gen\u00b7h\u00e4lt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}