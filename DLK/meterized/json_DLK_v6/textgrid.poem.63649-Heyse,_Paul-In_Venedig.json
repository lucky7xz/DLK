{"textgrid.poem.63649": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "In Venedig", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier unsre letzte Rast, im stillen Haus,", "tokens": ["Hier", "uns\u00b7re", "letz\u00b7te", "Rast", ",", "im", "stil\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Daran vorbei die schwarzen Gondeln gleiten.", "tokens": ["Da\u00b7ran", "vor\u00b7bei", "die", "schwar\u00b7zen", "Gon\u00b7deln", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie dumpfe Geisterklage t\u00f6nt daraus", "tokens": ["Wie", "dump\u00b7fe", "Geis\u00b7ter\u00b7kla\u00b7ge", "t\u00f6nt", "da\u00b7raus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Gondoliere Wechselruf zuzeiten.", "tokens": ["Der", "Gon\u00b7do\u00b7lie\u00b7re", "Wech\u00b7sel\u00b7ruf", "zu\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es schl\u00e4ft die Stadt, doch ihre Seele wacht,", "tokens": ["Es", "schl\u00e4ft", "die", "Stadt", ",", "doch", "ih\u00b7re", "See\u00b7le", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die sonnenscheue, wieder auf bei Nacht.", "tokens": ["Die", "son\u00b7nen\u00b7scheu\u00b7e", ",", "wie\u00b7der", "auf", "bei", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wir, wenn bei umflortem Sternenglanz", "tokens": ["Und", "wir", ",", "wenn", "bei", "um\u00b7flor\u00b7tem", "Ster\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir wandeln durch die schweigenden Arkaden,", "tokens": ["Wir", "wan\u00b7deln", "durch", "die", "schwei\u00b7gen\u00b7den", "Ar\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich Schatten unter Schatten, die zum Tanz", "tokens": ["Gleich", "Schat\u00b7ten", "un\u00b7ter", "Schat\u00b7ten", ",", "die", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Acheron die Spukgenossen laden \u2013", "tokens": ["Am", "A\u00b7che\u00b7ron", "die", "Spuk\u00b7ge\u00b7nos\u00b7sen", "la\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie der Gedank' uns l\u00e4hmend \u00fcberf\u00e4llt,", "tokens": ["Wie", "der", "Ge\u00b7dank'", "uns", "l\u00e4h\u00b7mend", "\u00fc\u00b7berf\u00b7\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fcck zu m\u00fcssen in die Oberwelt!", "tokens": ["Zu\u00b7r\u00fcck", "zu", "m\u00fcs\u00b7sen", "in", "die", "O\u00b7ber\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VMINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zur\u00fcck zur Heimat, die zur Fremde ward,", "tokens": ["Zu\u00b7r\u00fcck", "zur", "Hei\u00b7mat", ",", "die", "zur", "Frem\u00b7de", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo nicht mehr lockt, was einst so s\u00fc\u00df gewesen,", "tokens": ["Wo", "nicht", "mehr", "lockt", ",", "was", "einst", "so", "s\u00fc\u00df", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo unser nur die Freundesfrage harrt:", "tokens": ["Wo", "un\u00b7ser", "nur", "die", "Freun\u00b7des\u00b7fra\u00b7ge", "harrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kehrt ihr getr\u00f6stet wieder, gramgenesen? \u2013", "tokens": ["Kehrt", "ihr", "ge\u00b7tr\u00f6s\u00b7tet", "wie\u00b7der", ",", "gram\u00b7ge\u00b7ne\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "ADV", "$,", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn der Blick noch traurig suchend schweift,", "tokens": ["Und", "wenn", "der", "Blick", "noch", "trau\u00b7rig", "su\u00b7chend", "schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kaum einer, der sein Schmerzensrecht begreift!", "tokens": ["Kaum", "ei\u00b7ner", ",", "der", "sein", "Schmer\u00b7zens\u00b7recht", "be\u00b7greift", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn leicht beweglich flie\u00dft der Menschen Blut", "tokens": ["Denn", "leicht", "be\u00b7weg\u00b7lich", "flie\u00dft", "der", "Men\u00b7schen", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und scheidet hastig aus den fremden Tropfen.", "tokens": ["Und", "schei\u00b7det", "has\u00b7tig", "aus", "den", "frem\u00b7den", "Trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es str\u00e4ubt sich, Jahr und Jahr verlornem Gut,", "tokens": ["Es", "str\u00e4ubt", "sich", ",", "Jahr", "und", "Jahr", "ver\u00b7lor\u00b7nem", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "War's auch des Lebens Krone, nachzuklopfen.", "tokens": ["Wa\u00b7r's", "auch", "des", "Le\u00b7bens", "Kro\u00b7ne", ",", "nach\u00b7zu\u00b7klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,", "VVIZU", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wir aber, deren Blut der Gram verg\u00e4llt,", "tokens": ["Wir", "a\u00b7ber", ",", "de\u00b7ren", "Blut", "der", "Gram", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie taugten wir noch in die muntre Welt?", "tokens": ["Wie", "taug\u00b7ten", "wir", "noch", "in", "die", "mun\u00b7tre", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie g\u00f6nnt dem Ungl\u00fcck, eine Weile still", "tokens": ["Sie", "g\u00f6nnt", "dem", "Un\u00b7gl\u00fcck", ",", "ei\u00b7ne", "Wei\u00b7le", "still"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Einsamkeit sich trauernd abzuschlie\u00dfen.", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "sich", "trau\u00b7ernd", "ab\u00b7zu\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch da\u00df zuletzt nicht alles heilen will,", "tokens": ["Doch", "da\u00df", "zu\u00b7letzt", "nicht", "al\u00b7les", "hei\u00b7len", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PTKNEG", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht wiederkehrt die Sehnsucht, zu genie\u00dfen,", "tokens": ["Nicht", "wie\u00b7der\u00b7kehrt", "die", "Sehn\u00b7sucht", ",", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df Treue nicht zu sterben sich bequemt,", "tokens": ["Da\u00df", "Treu\u00b7e", "nicht", "zu", "ster\u00b7ben", "sich", "be\u00b7quemt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "PTKZU", "VVINF", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mu\u00df sie verdammen, weil es sie besch\u00e4mt.", "tokens": ["Mu\u00df", "sie", "ver\u00b7dam\u00b7men", ",", "weil", "es", "sie", "be\u00b7sch\u00e4mt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O wie sie grausam klug zu tr\u00f6sten wei\u00df,", "tokens": ["O", "wie", "sie", "grau\u00b7sam", "klug", "zu", "tr\u00f6s\u00b7ten", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADJD", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie ergaben spricht von Lebenspflichten:", "tokens": ["Wie", "sie", "er\u00b7ga\u00b7ben", "spricht", "von", "Le\u00b7bens\u00b7pflich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschenw\u00fcrde Feuerprobe sei's,", "tokens": ["Der", "Men\u00b7schen\u00b7w\u00fcr\u00b7de", "Feu\u00b7er\u00b7pro\u00b7be", "sei's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An neuer Hoffnung sich emporzurichten. \u2013", "tokens": ["An", "neu\u00b7er", "Hoff\u00b7nung", "sich", "em\u00b7por\u00b7zu\u00b7rich\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch wenn der Blitz des Baumes Mark verheert,", "tokens": ["Doch", "wenn", "der", "Blitz", "des", "Bau\u00b7mes", "Mark", "ver\u00b7heert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo ist ein Lenz, der neu ihn bl\u00fchen lehrt?", "tokens": ["Wo", "ist", "ein", "Lenz", ",", "der", "neu", "ihn", "bl\u00fc\u00b7hen", "lehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Gl\u00fccklichen! O, sie verstehn es nie", "tokens": ["Die", "Gl\u00fcck\u00b7li\u00b7chen", "!", "O", ",", "sie", "ver\u00b7stehn", "es", "nie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schelten \u00bbkrankhaft\u00ab den erkrankten Willen.", "tokens": ["Und", "schel\u00b7ten", "\u00bb", "krank\u00b7haft", "\u00ab", "den", "er\u00b7krank\u00b7ten", "Wil\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADJD", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die K\u00fchlgesunden! Nie begreifen sie,", "tokens": ["Die", "K\u00fchl\u00b7ge\u00b7sun\u00b7den", "!", "Nie", "be\u00b7grei\u00b7fen", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Wunsch und Wille nicht das Fieber stillen.", "tokens": ["Da\u00df", "Wunsch", "und", "Wil\u00b7le", "nicht", "das", "Fie\u00b7ber", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns aber la\u00df verstummen, wo uns nicht", "tokens": ["Uns", "a\u00b7ber", "la\u00df", "ver\u00b7stum\u00b7men", ",", "wo", "uns", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "$,", "PWAV", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Herz vernimmt, das unsre Sprache spricht.", "tokens": ["Ein", "Herz", "ver\u00b7nimmt", ",", "das", "uns\u00b7re", "Spra\u00b7che", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sieh diese Stadt, der Meere K\u00f6nigin,", "tokens": ["Sieh", "die\u00b7se", "Stadt", ",", "der", "Mee\u00b7re", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stolz, frei und gl\u00fccklich einst und allumworben.", "tokens": ["Stolz", ",", "frei", "und", "gl\u00fcck\u00b7lich", "einst", "und", "al\u00b7lum\u00b7wor\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Stern erblich, ihr Purpur sank dahin,", "tokens": ["Ihr", "Stern", "er\u00b7blich", ",", "ihr", "Pur\u00b7pur", "sank", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Macht, ihr Lebensatem, ist erstorben,", "tokens": ["Die", "Macht", ",", "ihr", "Le\u00b7ben\u00b7sa\u00b7tem", ",", "ist", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn die Sonne jetzt dem Meer entsteigt,", "tokens": ["Und", "wenn", "die", "Son\u00b7ne", "jetzt", "dem", "Meer", "ent\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "AR