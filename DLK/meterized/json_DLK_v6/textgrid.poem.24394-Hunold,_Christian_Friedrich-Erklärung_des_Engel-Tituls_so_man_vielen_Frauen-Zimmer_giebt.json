{"textgrid.poem.24394": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung des Engel-Tituls/ so man vielen Frauen-Zimmer giebt", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Engel heisset ihr/ das Wachen ist auch gro\u00df.", "tokens": ["Ein", "En\u00b7gel", "heis\u00b7set", "ihr", "/", "das", "Wa\u00b7chen", "ist", "auch", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil gut und b\u00f6se nun/ so fraget das Gewissen/", "tokens": ["Weil", "gut", "und", "b\u00f6\u00b7se", "nun", "/", "so", "fra\u00b7get", "das", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Worzu geh\u00f6ret ihr/ wohin tr\u00e4gt euer K\u00fcssen?", "tokens": ["Wor\u00b7zu", "ge\u00b7h\u00f6\u00b7ret", "ihr", "/", "wo\u00b7hin", "tr\u00e4gt", "eu\u00b7er", "K\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Guten tragen nur in Abrahami Schoo\u00df.", "tokens": ["Die", "Gu\u00b7ten", "tra\u00b7gen", "nur", "in", "Ab\u00b7ra\u00b7ha\u00b7mi", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VV