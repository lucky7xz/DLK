{"textgrid.poem.39018": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn her\u00fcber zu meinem Garten", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn her\u00fcber zu meinem Garten", "tokens": ["Wenn", "her\u00b7\u00fc\u00b7ber", "zu", "mei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die alten Lieder t\u00f6nen,", "tokens": ["Die", "al\u00b7ten", "Lie\u00b7der", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Pfeifer, die aus dem Gebirge kommend", "tokens": ["Der", "Pfei\u00b7fer", ",", "die", "aus", "dem", "Ge\u00b7bir\u00b7ge", "kom\u00b7mend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jeglich Marienbild mit Weisen gr\u00fc\u00dfen,", "tokens": ["Jeg\u00b7lich", "Ma\u00b7ri\u00b7en\u00b7bild", "mit", "Wei\u00b7sen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "So d\u00fcnk' ich mich in seltsame, ferne", "tokens": ["So", "d\u00fcnk'", "ich", "mich", "in", "selt\u00b7sa\u00b7me", ",", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$,", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wunderzeiten entr\u00fcckt,", "tokens": ["Wun\u00b7der\u00b7zei\u00b7ten", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und alte Legenden, und himmlische Sehnsucht,", "tokens": ["Und", "al\u00b7te", "Le\u00b7gen\u00b7den", ",", "und", "himm\u00b7li\u00b7sche", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.8": {"text": "Zarte Lieb' und gro\u00dfe Erinnerung", "tokens": ["Zar\u00b7te", "Lieb'", "und", "gro\u00b7\u00dfe", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Quellen aus den rauhen, einfachen T\u00f6nen.", "tokens": ["Quel\u00b7len", "aus", "den", "rau\u00b7hen", ",", "ein\u00b7fa\u00b7chen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.10": {"text": "Tiefer, und inniger", "tokens": ["Tie\u00b7fer", ",", "und", "in\u00b7ni\u00b7ger"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Spricht der Fr\u00f6mmigkeit Wort", "tokens": ["Spricht", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Die wunderliche Melodie,", "tokens": ["Die", "wun\u00b7der\u00b7li\u00b7che", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als in den Kirchen", "tokens": ["Als", "in", "den", "Kir\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Der neuen K\u00fcnstler Wirrwarr,", "tokens": ["Der", "neu\u00b7en", "K\u00fcnst\u00b7ler", "Wirr\u00b7warr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die alle T\u00f6ne keck aufbieten", "tokens": ["Die", "al\u00b7le", "T\u00f6\u00b7ne", "keck", "auf\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Um zu heucheln und zu grimassiren,", "tokens": ["Um", "zu", "heu\u00b7cheln", "und", "zu", "gri\u00b7mas\u00b7si\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Und mit weltlichem Prunk", "tokens": ["Und", "mit", "welt\u00b7li\u00b7chem", "Prunk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PWAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Das Heilige h\u00f6hnen.", "tokens": ["Das", "Hei\u00b7li\u00b7ge", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}