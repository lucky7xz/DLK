{"textgrid.poem.27319": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonntagskinder", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahrelang bildet der Meister und kann sich nimmer genugtun,", "tokens": ["Jah\u00b7re\u00b7lang", "bil\u00b7det", "der", "Meis\u00b7ter", "und", "kann", "sich", "nim\u00b7mer", "ge\u00b7nug\u00b7tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Dem genialen Geschlecht wird es im Traume beschert!", "tokens": ["Dem", "ge\u00b7ni\u00b7a\u00b7len", "Ge\u00b7schlecht", "wird", "es", "im", "Trau\u00b7me", "be\u00b7schert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}