{"textgrid.poem.25963": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcrzburgerisch", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mich an dei' B\u00e4ckle streich,", "tokens": ["Wenn", "ich", "mich", "an", "dei'", "B\u00e4ck\u00b7le", "streich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deine feine T\u00e4tzle k\u00fcss',", "tokens": ["Und", "dei\u00b7ne", "fei\u00b7ne", "T\u00e4tz\u00b7le", "k\u00fcss'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist kei Fleckle mehr so weich", "tokens": ["So", "ist", "kei", "Fleck\u00b7le", "mehr", "so", "weich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie 's Pl\u00e4tzle bei meim Fr\u00e4tzle.", "tokens": ["Wie", "'s", "Pl\u00e4tz\u00b7le", "bei", "meim", "Fr\u00e4tz\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und hinterm Hemd dei Br\u00fcstle, no,", "tokens": ["Und", "hin\u00b7term", "Hemd", "dei", "Br\u00fcst\u00b7le", ",", "no", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dran tapp ich voll Gel\u00fcstle,", "tokens": ["Dran", "tapp", "ich", "voll", "Ge\u00b7l\u00fcst\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wieg sie wie Tr\u00e4uble in der Hand,", "tokens": ["Wieg", "sie", "wie", "Tr\u00e4ub\u00b7le", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lab' mich dran, wie Moses froh,", "tokens": ["Und", "lab'", "mich", "dran", ",", "wie", "Mo\u00b7ses", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$,", "PWAV", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als er kam zum gelobten Land", "tokens": ["Als", "er", "kam", "zum", "ge\u00b7lob\u00b7ten", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Verdurstet aus dem W\u00fcstle.", "tokens": ["Ver\u00b7durs\u00b7tet", "aus", "dem", "W\u00fcst\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}