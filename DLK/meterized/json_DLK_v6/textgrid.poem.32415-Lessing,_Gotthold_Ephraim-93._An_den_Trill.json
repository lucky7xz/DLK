{"textgrid.poem.32415": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "93. An den Trill", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald willst du, Trill, und bald willst du dich nicht beweiben:", "tokens": ["Bald", "willst", "du", ",", "Trill", ",", "und", "bald", "willst", "du", "dich", "nicht", "be\u00b7wei\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NE", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald d\u00fcnkt dichs gut, bald nicht, ein Hagestolz zu bleiben.", "tokens": ["Bald", "d\u00fcnkt", "dichs", "gut", ",", "bald", "nicht", ",", "ein", "Ha\u00b7ge\u00b7stolz", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$,", "ADV", "PTKNEG", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich soll dir raten? Wohl! Tu, was dein Vater tat:", "tokens": ["Ich", "soll", "dir", "ra\u00b7ten", "?", "Wohl", "!", "Tu", ",", "was", "dein", "Va\u00b7ter", "tat", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$.", "ADV", "$.", "NE", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleib frei; heirate nicht! \u2013 Da hast du meinen Rat.", "tokens": ["Bleib", "frei", ";", "hei\u00b7ra\u00b7te", "nicht", "!", "\u2013", "Da", "hast", "du", "mei\u00b7nen", "Rat", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKNEG", "$.", "$(", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}