{"dta.poem.9497": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "An den  \n Seinen.  \n ODE/ darinnen die ersten vier Ver-  \n se Troch\u00e4isch/ die andern f\u00fcnffe Dactylisch/  \n die letzten viere Jambisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6stern als sich allbereit", "tokens": ["G\u00f6s\u00b7tern", "als", "sich", "all\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOUS", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mercken lie\u00df die Abends-zeit/", "tokens": ["Mer\u00b7cken", "lie\u00df", "die", "A\u00b7bends\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da der blasse Mond von sernen/", "tokens": ["Da", "der", "blas\u00b7se", "Mond", "von", "ser\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gesellte zu den Sternen;", "tokens": ["Sich", "ge\u00b7sell\u00b7te", "zu", "den", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da wir uns lustig erzeigten zusammen/", "tokens": ["Da", "wir", "uns", "lus\u00b7tig", "er\u00b7zeig\u00b7ten", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bachus entz\u00fcndte Poetische slammen.", "tokens": ["Ba\u00b7chus", "ent\u00b7z\u00fcnd\u00b7te", "Poe\u00b7ti\u00b7sche", "slam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch edelen Wein/", "tokens": ["Durch", "e\u00b7de\u00b7len", "Wein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Welchen mein Bruder mit kr\u00e4fftigen sachen", "tokens": ["Wel\u00b7chen", "mein", "Bru\u00b7der", "mit", "kr\u00e4ff\u00b7ti\u00b7gen", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "vers\u00fc\u00dfet allein/", "tokens": ["ver\u00b7s\u00fc\u00b7\u00dfet", "al\u00b7lein", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Kam mier ein Becher vor gesichte", "tokens": ["Kam", "mier", "ein", "Be\u00b7cher", "vor", "ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich wei\u00df nicht was ich schlie\u00df und richte?", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "schlie\u00df", "und", "rich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er mu\u00df ja l\u00f6chricht seyn/", "tokens": ["Er", "mu\u00df", "ja", "l\u00f6ch\u00b7richt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wo bliebe sonst der Wein?", "tokens": ["Wo", "blie\u00b7be", "sonst", "der", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was auf diesem Becher stund", "tokens": ["Was", "auf", "die\u00b7sem", "Be\u00b7cher", "stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War uns allen wohl vergunt;", "tokens": ["War", "uns", "al\u00b7len", "wohl", "ver\u00b7gunt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinck mich aus und leg mich nieder/", "tokens": ["Trinck", "mich", "aus", "und", "leg", "mich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "NE", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Richt mich auff und f\u00fcll mich wider;", "tokens": ["Richt", "mich", "auff", "und", "f\u00fcll", "mich", "wi\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses war eben am Becher geschrieben/", "tokens": ["Die\u00b7ses", "war", "e\u00b7ben", "am", "Be\u00b7cher", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Das uns zu trincken so wacker getrieben/", "tokens": ["Das", "uns", "zu", "trin\u00b7cken", "so", "wa\u00b7cker", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKZU", "VVFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den edelen Wein:", "tokens": ["Den", "e\u00b7de\u00b7len", "Wein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Wie? ist er denn unten und oben voll l\u00f6cher?", "tokens": ["Wie", "?", "ist", "er", "denn", "un\u00b7ten", "und", "o\u00b7ben", "voll", "l\u00f6\u00b7cher", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "ADV", "ADV", "KON", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Wie mag es doch seyn?", "tokens": ["Wie", "mag", "es", "doch", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Mein Sinn und w\u00e4hnen wird zu nichte/", "tokens": ["Mein", "Sinn", "und", "w\u00e4h\u00b7nen", "wird", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVINF", "VAFIN", "PTKA", "PIS", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich wei\u00df nicht/ was ich schlie\u00df und richte?", "tokens": ["Ich", "wei\u00df", "nicht", "/", "was", "ich", "schlie\u00df", "und", "rich\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er mu\u00df ja l\u00f6hricht seyn/", "tokens": ["Er", "mu\u00df", "ja", "l\u00f6h\u00b7richt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wo bliebe sonst der Wein?", "tokens": ["Wo", "blie\u00b7be", "sonst", "der", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}