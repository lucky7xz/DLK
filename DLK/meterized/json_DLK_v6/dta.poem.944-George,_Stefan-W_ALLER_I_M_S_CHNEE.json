{"dta.poem.944": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ich trat vor dich mit einem segenspruche               ", "tokens": ["Ich", "trat", "vor", "dich", "mit", "ei\u00b7nem", "se\u00b7gen\u00b7spru\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am abend wo f&#252;r dich die kerzen brannten               ", "tokens": ["Am", "a\u00b7bend", "wo", "f", "&#252;", "r", "dich", "die", "ker\u00b7zen", "brann\u00b7ten"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPRART", "KON", "PWAV", "NE", "$(", "VVIMP", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und reichte dir auf einem sammtnen tuche               ", "tokens": ["Und", "reich\u00b7te", "dir", "auf", "ei\u00b7nem", "sammt\u00b7nen", "tu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die h\u00f6chste meiner gaben: den demanten             ", "tokens": ["Die", "h\u00f6chs\u00b7te", "mei\u00b7ner", "ga\u00b7ben", ":", "den", "de\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "VVFIN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du aber weisst nichts von dem opferbrauche               ", "tokens": ["Du", "a\u00b7ber", "weisst", "nichts", "von", "dem", "op\u00b7fer\u00b7brau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von blanken leuchtern mit erhobnen &#228;rmen               ", "tokens": ["Von", "blan\u00b7ken", "leuch\u00b7tern", "mit", "er\u00b7hob\u00b7nen", "&#228;", "rmen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "$(", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von schalen die mit wolkenreinem rauche               ", "tokens": ["Von", "scha\u00b7len", "die", "mit", "wol\u00b7ken\u00b7rei\u00b7nem", "rau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der strengen tempel finsterniss erw&#228;rmen             ", "tokens": ["Der", "stren\u00b7gen", "tem\u00b7pel", "fins\u00b7ter\u00b7niss", "erw", "&#228;", "rmen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "$(", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von engeln die sich in den nischen sammeln               ", "tokens": ["Von", "en\u00b7geln", "die", "sich", "in", "den", "ni\u00b7schen", "sam\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sich bespiegeln am kristallnen l&#252;ster               ", "tokens": ["Und", "sich", "be\u00b7spie\u00b7geln", "am", "kris\u00b7tall\u00b7nen", "l", "&#252;", "ster"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$(", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Von gl&#252;hender und banger bitte stammeln               ", "tokens": ["Von", "gl", "&#252;", "hen\u00b7der", "und", "ban\u00b7ger", "bit\u00b7te", "stam\u00b7meln"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "ADJD", "KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von halben seufzern hingehaucht im d&#252;ster             ", "tokens": ["Von", "hal\u00b7ben", "seuf\u00b7zern", "hin\u00b7ge\u00b7haucht", "im", "d", "&#252;", "ster"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPRART", "XY", "XY", "XY"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Und nichts von w&#252;nschen die auf untern sprossen               ", "tokens": ["Und", "nichts", "von", "w", "&#252;", "nschen", "die", "auf", "un\u00b7tern", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "NE", "$(", "VVFIN", "ART", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des festlichen altars vernehmlich wimmern               ", "tokens": ["Des", "fest\u00b7li\u00b7chen", "al\u00b7tars", "ver\u00b7nehm\u00b7lich", "wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du fassest fragend kalt und unentschlossen               ", "tokens": ["Du", "fas\u00b7sest", "fra\u00b7gend", "kalt", "und", "un\u00b7ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den edelstein aus gluten thr&#228;nen schimmern.             ", "tokens": ["Den", "e\u00b7del\u00b7stein", "aus", "glu\u00b7ten", "thr", "&#228;", "nen", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$(", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}