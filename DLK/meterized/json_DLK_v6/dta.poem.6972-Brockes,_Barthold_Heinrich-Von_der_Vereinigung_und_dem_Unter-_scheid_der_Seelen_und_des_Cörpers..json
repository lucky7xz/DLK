{"dta.poem.6972": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie! fehlt uns das Gesicht in einer finstern Nacht,", "tokens": ["Wie", "!", "fehlt", "uns", "das", "Ge\u00b7sicht", "in", "ei\u00b7ner", "fins\u00b7tern", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat man so dann zu sehen auch wol Macht?", "tokens": ["Hat", "man", "so", "dann", "zu", "se\u00b7hen", "auch", "wol", "Macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "PTKZU", "VVINF", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nein, weilen Dunckelheit und Finsterni\u00df bestehn", "tokens": ["Nein", ",", "wei\u00b7len", "Dun\u00b7ckel\u00b7heit", "und", "Fins\u00b7ter\u00b7ni\u00df", "be\u00b7stehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sie in dunckler Nacht, vermag man nicht zu sehn.", "tokens": ["Und", "sie", "in", "dunck\u00b7ler", "Nacht", ",", "ver\u00b7mag", "man", "nicht", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$,", "VVFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$