{"textgrid.poem.53582": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Mi\u00dfachtung der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Tante Julla, du in Neu-Ruppin", "tokens": ["Ach", ",", "Tan\u00b7te", "Jul\u00b7la", ",", "du", "in", "Neu\u00b7Rup\u00b7pin"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "NE", "$,", "PPER", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "liest schaudernd von berliner Scheu\u00dflichkeiten,", "tokens": ["liest", "schau\u00b7dernd", "von", "ber\u00b7li\u00b7ner", "Scheu\u00df\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wie die Damen ihre Glieder spreiten,", "tokens": ["und", "wie", "die", "Da\u00b7men", "ih\u00b7re", "Glie\u00b7der", "sprei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und denkst: Dies S\u00fcndenbabylon Berlin!", "tokens": ["und", "denkst", ":", "Dies", "S\u00fcn\u00b7den\u00b7ba\u00b7by\u00b7lon", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und deine \u00c4uglein \u00f6ffnen sich in L\u00fcsten,", "tokens": ["Und", "dei\u00b7ne", "\u00c4ug\u00b7lein", "\u00f6ff\u00b7nen", "sich", "in", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "weil deine Kaffeeschwestern gerne w\u00fc\u00dften", "tokens": ["weil", "dei\u00b7ne", "Kaf\u00b7fee\u00b7schwes\u00b7tern", "ger\u00b7ne", "w\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "von einem Paar, gelagert Bein an Bein . . .", "tokens": ["von", "ei\u00b7nem", "Paar", ",", "ge\u00b7la\u00b7gert", "Bein", "an", "Bein", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie mag das sein?", "tokens": ["Wie", "mag", "das", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ach, Tante Julla \u2013 komm mal an die Spree.", "tokens": ["Ach", ",", "Tan\u00b7te", "Jul\u00b7la", "\u2013", "komm", "mal", "an", "die", "Spree", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "NE", "$(", "VVFIN", "ADV", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sieh dir dieses Wogen aus der N\u00e4he,", "tokens": ["Und", "sieh", "dir", "die\u00b7ses", "Wo\u00b7gen", "aus", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ganz aus der N\u00e4he an, wie ich es sehe.", "tokens": ["ganz", "aus", "der", "N\u00e4\u00b7he", "an", ",", "wie", "ich", "es", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und denk dir nur ein Chambre s\u00e9par\u00e9e.", "tokens": ["Und", "denk", "dir", "nur", "ein", "Chamb\u00b7re", "s\u00e9pa\u00b7r\u00e9e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sie quietscht. Der Kellner schummelt. D\u00fcnne Geigen", "tokens": ["Sie", "quietscht", ".", "Der", "Kell\u00b7ner", "schum\u00b7melt", ".", "D\u00fcn\u00b7ne", "Gei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "verleiten sie, sich ziemlich ganz zu zeigen.", "tokens": ["ver\u00b7lei\u00b7ten", "sie", ",", "sich", "ziem\u00b7lich", "ganz", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Mieder noch und noch ein Br\u00fcstchenlein . . .", "tokens": ["Ein", "Mie\u00b7der", "noch", "und", "noch", "ein", "Br\u00fcst\u00b7chen\u00b7lein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was kann da sein \u2013?", "tokens": ["Was", "kann", "da", "sein", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PPOSAT", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ach, Tante Julla \u2013 wir sind nicht blasiert.", "tokens": ["Ach", ",", "Tan\u00b7te", "Jul\u00b7la", "\u2013", "wir", "sind", "nicht", "bla\u00b7siert", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "NE", "$(", "PPER", "VAFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und doch: wie eng ist dieser Markt der Liebe!", "tokens": ["Und", "doch", ":", "wie", "eng", "ist", "die\u00b7ser", "Markt", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWAV", "ADJD", "VAFIN", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der liebt die Knaben, jener sch\u00e4tzt die Hiebe,", "tokens": ["Der", "liebt", "die", "Kna\u00b7ben", ",", "je\u00b7ner", "sch\u00e4tzt", "die", "Hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und der ist nur von Z\u00f6pfen enchantiert.", "tokens": ["und", "der", "ist", "nur", "von", "Z\u00f6p\u00b7fen", "en\u00b7chan\u00b7tiert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Themis bullert mit Moralgesetzen.", "tokens": ["Die", "The\u00b7mis", "bul\u00b7lert", "mit", "Mo\u00b7ral\u00b7ge\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man mu\u00df Erotik nicht so \u00fcbersch\u00e4tzen.", "tokens": ["Man", "mu\u00df", "E\u00b7ro\u00b7tik", "nicht", "so", "\u00fc\u00b7bersc\u00b7h\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bleib nur in deinen b\u00fcrgerlichen Tr\u00e4umen,", "tokens": ["Bleib", "nur", "in", "dei\u00b7nen", "b\u00fcr\u00b7ger\u00b7li\u00b7chen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "du hast hier n\u00e4mlich gar nichts zu vers\u00e4umen.", "tokens": ["du", "hast", "hier", "n\u00e4m\u00b7lich", "gar", "nichts", "zu", "ver\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bleib, Tante Julla, in dem St\u00fcbchen klein \u2013", "tokens": ["Bleib", ",", "Tan\u00b7te", "Jul\u00b7la", ",", "in", "dem", "St\u00fcb\u00b7chen", "klein", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "$,", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Was kann da sein \u2013?", "tokens": ["Was", "kann", "da", "sein", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PPOSAT", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Was kann da wirklich sein \u2013?", "tokens": ["Was", "kann", "da", "wirk\u00b7lich", "sein", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "VAINF", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}