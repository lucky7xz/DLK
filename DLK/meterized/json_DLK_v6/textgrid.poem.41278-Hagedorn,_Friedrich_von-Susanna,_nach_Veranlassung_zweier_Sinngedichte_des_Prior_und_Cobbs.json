{"textgrid.poem.41278": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Susanna, nach Veranlassung zweier Sinngedichte des Prior und Cobbs", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Susannens Keuschheit wird von allen hochgepriesen:", "tokens": ["Su\u00b7san\u00b7nens", "Keuschheit", "wird", "von", "al\u00b7len", "hoch\u00b7ge\u00b7prie\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das junge Weib, das jeder artig fand,", "tokens": ["Das", "jun\u00b7ge", "Weib", ",", "das", "je\u00b7der", "ar\u00b7tig", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "That beiden Greisen Widerstand,", "tokens": ["That", "bei\u00b7den", "Grei\u00b7sen", "Wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und hat sich keinem hold erwiesen.", "tokens": ["Und", "hat", "sich", "kei\u00b7nem", "hold", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lobe, was wir von ihr lesen;", "tokens": ["Ich", "lo\u00b7be", ",", "was", "wir", "von", "ihr", "le\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch r\u00e4umen alle Kenner ein,", "tokens": ["Doch", "r\u00e4u\u00b7men", "al\u00b7le", "Ken\u00b7ner", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Wunder w\u00fcrde gr\u00f6\u00dfer sein,", "tokens": ["Das", "Wun\u00b7der", "w\u00fcr\u00b7de", "gr\u00f6\u00b7\u00dfer", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn beide Buhler jung gewesen.", "tokens": ["Wenn", "bei\u00b7de", "Buh\u00b7ler", "jung", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}