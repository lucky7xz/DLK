{"textgrid.poem.67474": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An das Crucifix im Consistorium", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du Heiliger, bleibt Dir immer Dein trauriges Schicksal,", "tokens": ["O", "Du", "Hei\u00b7li\u00b7ger", ",", "bleibt", "Dir", "im\u00b7mer", "Dein", "trau\u00b7ri\u00b7ges", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zwischen Sch\u00e4chern geh\u00e4ngt, sterbend am Kreuze zu sein?", "tokens": ["Zwi\u00b7schen", "Sch\u00e4\u00b7chern", "ge\u00b7h\u00e4ngt", ",", "ster\u00b7bend", "am", "Kreu\u00b7ze", "zu", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "ADJD", "APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und zu Deinen F\u00fc\u00dfen erscheint das Wort des Propheten", "tokens": ["Und", "zu", "Dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "er\u00b7scheint", "das", "Wort", "des", "Pro\u00b7phe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von der Ochsen und Farrn feisten geselligen Schaar.", "tokens": ["Von", "der", "Och\u00b7sen", "und", "Farrn", "feis\u00b7ten", "ge\u00b7sel\u00b7li\u00b7gen", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Heiliger, blick auf mich und sprich auch mir in die Seele:", "tokens": ["Hei\u00b7li\u00b7ger", ",", "blick", "auf", "mich", "und", "sprich", "auch", "mir", "in", "die", "See\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "KON", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+--++-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "\u00bbvater, vergieb! denn ", "tokens": ["\u00bb", "va\u00b7ter", ",", "ver\u00b7gieb", "!", "denn"], "token_info": ["punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "NN", "$,", "VVIMP", "$.", "KON"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}