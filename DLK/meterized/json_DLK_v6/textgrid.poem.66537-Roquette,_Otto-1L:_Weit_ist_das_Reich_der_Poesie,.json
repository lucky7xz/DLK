{"textgrid.poem.66537": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit ist das Reich der Poesie,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit ist das Reich der Poesie,", "tokens": ["Weit", "ist", "das", "Reich", "der", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch wolle th\u00f6richt nicht verzagen,", "tokens": ["Doch", "wol\u00b7le", "th\u00f6\u00b7richt", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df das Geschick dir nur verlieh", "tokens": ["Da\u00df", "das", "Ge\u00b7schick", "dir", "nur", "ver\u00b7lieh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geringe Gabe f\u00fcr dein Wagen.", "tokens": ["Ge\u00b7rin\u00b7ge", "Ga\u00b7be", "f\u00fcr", "dein", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn eine einz'ge Melodie", "tokens": ["Wenn", "ei\u00b7ne", "einz'\u00b7ge", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von \u00e4chtem Klang du angeschlagen,", "tokens": ["Von", "\u00e4ch\u00b7tem", "Klang", "du", "an\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird von der Weltenharmonie", "tokens": ["Wird", "von", "der", "Wel\u00b7ten\u00b7har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zur Ewigkeit sie fortgetragen.", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "sie", "fort\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}