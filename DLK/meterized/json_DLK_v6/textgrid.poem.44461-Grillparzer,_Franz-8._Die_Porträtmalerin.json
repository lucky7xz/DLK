{"textgrid.poem.44461": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "8. Die Portr\u00e4tmalerin", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmalet keine toten Bilder,", "tokens": ["\u00bb", "ma\u00b7let", "kei\u00b7ne", "to\u00b7ten", "Bil\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tote Bilder des Lebendgen.\u00ab", "tokens": ["To\u00b7te", "Bil\u00b7der", "des", "Le\u00b7bend\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "So spricht Mahom der Prophete,", "tokens": ["So", "spricht", "Ma\u00b7hom", "der", "Pro\u00b7phe\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbdenn am Tage des Gerichtes", "tokens": ["\u00bb", "denn", "am", "Ta\u00b7ge", "des", "Ge\u00b7rich\u00b7tes"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden sie vor euch hintreten,", "tokens": ["Wer\u00b7den", "sie", "vor", "euch", "hin\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Leben fordernd, Seel und Geist.\u00ab", "tokens": ["Le\u00b7ben", "for\u00b7dernd", ",", "Seel", "und", "Geist", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, ich kenne Malerh\u00e4nde,", "tokens": ["Ach", ",", "ich", "ken\u00b7ne", "Ma\u00b7ler\u00b7h\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die beleben ihr Gem\u00e4lde", "tokens": ["Die", "be\u00b7le\u00b7ben", "ihr", "Ge\u00b7m\u00e4l\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sch\u00f6pferisch mit wahrem Leben.", "tokens": ["Sch\u00f6p\u00b7fe\u00b7risch", "mit", "wah\u00b7rem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Doch die Seele, die sie geben,", "tokens": ["Doch", "die", "See\u00b7le", ",", "die", "sie", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ward dem Urbild erst geraubt.", "tokens": ["Ward", "dem", "Ur\u00b7bild", "erst", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}