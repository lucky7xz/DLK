{"dta.poem.21011": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXV.  \n Eines Horntr\u00e4gers.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey H\u00f6rner liegen hier in dieser Grufft begra-", "tokens": ["Zwey", "H\u00f6r\u00b7ner", "lie\u00b7gen", "hier", "in", "die\u00b7ser", "Grufft", "be\u00b7gra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ben/ (haben.", "tokens": ["ben", "/", "(", "ha\u00b7ben", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVPP", "$(", "$(", "VAINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nicht dencket/ da\u00df ein Bock hier wird die Ruhstadt", "tokens": ["Nicht", "den\u00b7cket", "/", "da\u00df", "ein", "Bock", "hier", "wird", "die", "Ruh\u00b7stadt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$(", "KOUS", "ART", "NE", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier ruht ein guter Mann/ der H\u00f6rner hat bekom\u0303en/", "tokens": ["Hier", "ruht", "ein", "gu\u00b7ter", "Mann", "/", "der", "H\u00f6r\u00b7ner", "hat", "be\u00b7kom\u0303en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach dem ihm die Natur das Stossen hat benom\u0303en.", "tokens": ["Nach", "dem", "ihm", "die", "Na\u00b7tur", "das", "Stos\u00b7sen", "hat", "be\u00b7nom\u0303en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}