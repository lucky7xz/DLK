{"textgrid.poem.32831": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie schamhaft, wie bescheiden ist", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schamhaft, wie bescheiden ist", "tokens": ["Wie", "scham\u00b7haft", ",", "wie", "be\u00b7schei\u00b7den", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein M\u00e4dchen, die sanfte Blondine!", "tokens": ["Mein", "M\u00e4d\u00b7chen", ",", "die", "sanf\u00b7te", "Blon\u00b7di\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+----", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Als ich sie \u00f6ffentlich gek\u00fc\u00dft,", "tokens": ["Als", "ich", "sie", "\u00f6f\u00b7fent\u00b7lich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach sie mit zorniger Mine:", "tokens": ["Sprach", "sie", "mit", "zor\u00b7ni\u00b7ger", "Mi\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Wie? Unversch\u00e4mter, geh! was denkt die Welt von mir?", "tokens": ["Wie", "?", "Un\u00b7ver\u00b7sch\u00e4m\u00b7ter", ",", "geh", "!", "was", "denkt", "die", "Welt", "von", "mir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$,", "VVFIN", "$.", "PWS", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Heut Abends fordr' ich selbst noch Rechenschaft von dir.", "tokens": ["Heut", "A\u00b7bends", "fordr'", "ich", "selbst", "noch", "Re\u00b7chen\u00b7schaft", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie schamhaft sittsam ist sie nicht", "tokens": ["Wie", "scham\u00b7haft", "sitt\u00b7sam", "ist", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein M\u00e4dchen, die keusche Blondine!", "tokens": ["Mein", "M\u00e4d\u00b7chen", ",", "die", "keu\u00b7sche", "Blon\u00b7di\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich kam zu ihr: schon brannte Licht,", "tokens": ["Ich", "kam", "zu", "ihr", ":", "schon", "brann\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "$.", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wagts \u2013 \u2013 mit drohender Mine", "tokens": ["Ich", "wagts", "\u2013", "\u2013", "mit", "dro\u00b7hen\u00b7der", "Mi\u00b7ne"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Rief sie: o sch\u00e4m dich! sieh! der Nachbar guckt heraus:", "tokens": ["Rief", "sie", ":", "o", "sch\u00e4m", "dich", "!", "sieh", "!", "der", "Nach\u00b7bar", "guckt", "he\u00b7raus", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "FM", "FM", "PPER", "$.", "VVIMP", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie zog den Vorhang vor, und blies die Lichter aus.", "tokens": ["Sie", "zog", "den", "Vor\u00b7hang", "vor", ",", "und", "blies", "die", "Lich\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}