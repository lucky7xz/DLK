{"textgrid.poem.32700": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Minnepreis", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht! der Winter ist vergangen;", "tokens": ["Seht", "!", "der", "Win\u00b7ter", "ist", "ver\u00b7gan\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anger, Wald und Heide prangen;", "tokens": ["An\u00b7ger", ",", "Wald", "und", "Hei\u00b7de", "pran\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles freuet sich darob.", "tokens": ["Al\u00b7les", "freu\u00b7et", "sich", "da\u00b7rob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Minne lehrt die V\u00f6gel singen;", "tokens": ["Min\u00b7ne", "lehrt", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Minne lehrt die L\u00e4mmer springen;", "tokens": ["Min\u00b7ne", "lehrt", "die", "L\u00e4m\u00b7mer", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Minne sei mein Preis und Lob!", "tokens": ["Min\u00b7ne", "sei", "mein", "Preis", "und", "Lob", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Minne Lieb' und Leid erteilet;", "tokens": ["Min\u00b7ne", "Lieb'", "und", "Leid", "er\u00b7tei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Minne wundet, Minne heilet;", "tokens": ["Min\u00b7ne", "wun\u00b7det", ",", "Min\u00b7ne", "hei\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer trauren l\u00e4\u00dft sie nie;", "tokens": ["Im\u00b7mer", "trau\u00b7ren", "l\u00e4\u00dft", "sie", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Minne l\u00e4\u00dft zuweilen weinen,", "tokens": ["Min\u00b7ne", "l\u00e4\u00dft", "zu\u00b7wei\u00b7len", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber auch die Sonne scheinen;", "tokens": ["A\u00b7ber", "auch", "die", "Son\u00b7ne", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer preisen will ich sie.", "tokens": ["Im\u00b7mer", "prei\u00b7sen", "will", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}