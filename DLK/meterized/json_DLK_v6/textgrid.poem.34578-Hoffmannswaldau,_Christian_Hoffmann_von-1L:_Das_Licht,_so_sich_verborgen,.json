{"textgrid.poem.34578": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Licht, so sich verborgen,", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Licht, so sich verborgen,", "tokens": ["Das", "Licht", ",", "so", "sich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht itzt den neuen Morgen,", "tokens": ["Macht", "itzt", "den", "neu\u00b7en", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es sinkt die tr\u00fcbe Nacht;", "tokens": ["Es", "sinkt", "die", "tr\u00fc\u00b7be", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die bleichen Sterne weichen,", "tokens": ["Die", "blei\u00b7chen", "Ster\u00b7ne", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mond auch will verstreichen,", "tokens": ["Der", "Mond", "auch", "will", "ver\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ich bin aufgewacht.", "tokens": ["Und", "ich", "bin", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich mich kann bewegen,", "tokens": ["Da\u00df", "ich", "mich", "kann", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df Hand und Fu\u00df sich regen,", "tokens": ["Da\u00df", "Hand", "und", "Fu\u00df", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich noch leben kann,", "tokens": ["Da\u00df", "ich", "noch", "le\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Auge, Mund und Ohren", "tokens": ["Da\u00df", "Au\u00b7ge", ",", "Mund", "und", "Oh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht ihre Kraft verloren,", "tokens": ["Nicht", "ih\u00b7re", "Kraft", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast du, o Herr, gethan.", "tokens": ["Hast", "du", ",", "o", "Herr", ",", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich habe dies aus Gnaden,", "tokens": ["Ich", "ha\u00b7be", "dies", "aus", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich, der ich bin beladen", "tokens": ["Ich", ",", "der", "ich", "bin", "be\u00b7la\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit \u00fcberh\u00e4ufter Schuld;", "tokens": ["Mit", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ter", "Schuld", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es scheint, du willst die Flecken", "tokens": ["Es", "scheint", ",", "du", "willst", "die", "Fle\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit deinem Mantel decken", "tokens": ["Mit", "dei\u00b7nem", "Man\u00b7tel", "de\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und hast mit mir Geduld.", "tokens": ["Und", "hast", "mit", "mir", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr, rege Hand und Sinnen,", "tokens": ["Herr", ",", "re\u00b7ge", "Hand", "und", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Treib' selber mein Beginnen,", "tokens": ["Treib'", "sel\u00b7ber", "mein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei meines Geistes Licht!", "tokens": ["Sei", "mei\u00b7nes", "Geis\u00b7tes", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie kann mein Fu\u00df bestehen", "tokens": ["Wie", "kann", "mein", "Fu\u00df", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ohne Straucheln gehen,", "tokens": ["Und", "oh\u00b7ne", "Strau\u00b7cheln", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn mir dein Trieb gebricht?", "tokens": ["Wenn", "mir", "dein", "Trieb", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich bin in einer W\u00fcste", "tokens": ["Ich", "bin", "in", "ei\u00b7ner", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll tausend b\u00f6ser L\u00fcste;", "tokens": ["Voll", "tau\u00b7send", "b\u00f6\u00b7ser", "L\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, reich' mir deine Hand!", "tokens": ["Herr", ",", "reich'", "mir", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich kann hinaus nicht schreiten,", "tokens": ["Ich", "kann", "hin\u00b7aus", "nicht", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APZR", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird nicht dein Wort mich leiten", "tokens": ["Wird", "nicht", "dein", "Wort", "mich", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In ein bebautes Land.", "tokens": ["In", "ein", "be\u00b7bau\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich will mich zwar bem\u00fchen,", "tokens": ["Ich", "will", "mich", "zwar", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Glanz der Welt zu fliehen,", "tokens": ["Den", "Glanz", "der", "Welt", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die mich noch h\u00e4lt in Haft;", "tokens": ["Die", "mich", "noch", "h\u00e4lt", "in", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch weil's auf allen Seiten", "tokens": ["Doch", "weil's", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So leichtlich ist, zu gleiten,", "tokens": ["So", "leicht\u00b7lich", "ist", ",", "zu", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So gieb mir neue Kraft!", "tokens": ["So", "gieb", "mir", "neu\u00b7e", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Herr, lenk' mir mein Gesichte", "tokens": ["Herr", ",", "lenk'", "mir", "mein", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hin zu dem rechten Lichte", "tokens": ["Hin", "zu", "dem", "rech\u00b7ten", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Und zu dem rechten Schein;", "tokens": ["Und", "zu", "dem", "rech\u00b7ten", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Heb' du des Geistes Schwingen,", "tokens": ["Heb'", "du", "des", "Geis\u00b7tes", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wolken durchzudringen,", "tokens": ["Die", "Wol\u00b7ken", "durch\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So kann ich Adler sein!", "tokens": ["So", "kann", "ich", "Ad\u00b7ler", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}