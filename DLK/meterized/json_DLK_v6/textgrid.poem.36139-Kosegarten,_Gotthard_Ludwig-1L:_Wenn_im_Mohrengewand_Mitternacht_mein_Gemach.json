{"textgrid.poem.36139": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn im Mohrengewand Mitternacht mein Gemach", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im Mohrengewand Mitternacht mein Gemach", "tokens": ["Wenn", "im", "Moh\u00b7ren\u00b7ge\u00b7wand", "Mit\u00b7ter\u00b7nacht", "mein", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "NN", "PPOSAT", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Rings umsitzt, wenn die Welt still ist, als harrte sie", "tokens": ["Rings", "um\u00b7sitzt", ",", "wenn", "die", "Welt", "still", "ist", ",", "als", "harr\u00b7te", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,", "KOUS", "VVFIN", "PPER"], "meter": "--+--+++-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schon des kommenden Richters \u2013", "tokens": ["Schon", "des", "kom\u00b7men\u00b7den", "Rich\u00b7ters", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Oefter lausch' ich im Lager dann", "tokens": ["Oef\u00b7ter", "lausch'", "ich", "im", "La\u00b7ger", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wach und sinnig und ernst auf die Vergangenheit,", "tokens": ["Wach", "und", "sin\u00b7nig", "und", "ernst", "auf", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und bleichd\u00e4mmernd, wie Traum, kommt sie. Mein Aug' entdeckt", "tokens": ["Und", "bleich\u00b7d\u00e4m\u00b7mernd", ",", "wie", "Traum", ",", "kommt", "sie", ".", "Mein", "Aug'", "ent\u00b7deckt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "$,", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVPP"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Labyrinthengewebe,", "tokens": ["La\u00b7by\u00b7rin\u00b7then\u00b7ge\u00b7we\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Drinn ich, Knabe und J\u00fcngling, schritt.", "tokens": ["Drinn", "ich", ",", "Kna\u00b7be", "und", "J\u00fcng\u00b7ling", ",", "schritt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und ich wende mich, wag's, rufe der Zukunft auch \u2013", "tokens": ["Und", "ich", "wen\u00b7de", "mich", ",", "wag's", ",", "ru\u00b7fe", "der", "Zu\u00b7kunft", "auch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Scheltend bleibt sie zur\u00fcck. Riesengestalten gehn", "tokens": ["Schel\u00b7tend", "bleibt", "sie", "zu\u00b7r\u00fcck", ".", "Rie\u00b7sen\u00b7ge\u00b7stal\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVINF"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Z\u00fcrnend vor mir vor\u00fcber,", "tokens": ["Z\u00fcr\u00b7nend", "vor", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und ich schlie\u00dfe mein schwimmend Aug'.", "tokens": ["Und", "ich", "schlie\u00b7\u00dfe", "mein", "schwim\u00b7mend", "Aug'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Irr' und schreckenbet\u00e4ubt lauschet der Wanderer", "tokens": ["Irr'", "und", "schre\u00b7cken\u00b7be\u00b7t\u00e4ubt", "lau\u00b7schet", "der", "Wan\u00b7de\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "VVFIN", "ART", "NN"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So im graulichen Hain, wo ihn die Nacht ergriff,", "tokens": ["So", "im", "grau\u00b7li\u00b7chen", "Hain", ",", "wo", "ihn", "die", "Nacht", "er\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo ihn sibenfach Dunkel", "tokens": ["Wo", "ihn", "si\u00b7ben\u00b7fach", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aengstigt, bis ihm die Sonn' aufgeht.", "tokens": ["A\u00b7engs\u00b7tigt", ",", "bis", "ihm", "die", "Sonn'", "auf\u00b7geht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Zwar mir geht sie nicht auf. Aber mir d\u00e4mmert doch", "tokens": ["Zwar", "mir", "geht", "sie", "nicht", "auf", ".", "A\u00b7ber", "mir", "d\u00e4m\u00b7mert", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der Gedanke ins Herz: \u00bbSiehe, Vergangenheit", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", "ins", "Herz", ":", "\u00bb", "Sie\u00b7he", ",", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$(", "VVIMP", "$,", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "War vor kurzem die Zukunft\u00ab \u2013", "tokens": ["War", "vor", "kur\u00b7zem", "die", "Zu\u00b7kunft", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "ART", "NN", "$(", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und ich \u00f6ffne getrost mein Aug'.", "tokens": ["Und", "ich", "\u00f6ff\u00b7ne", "ge\u00b7trost", "mein", "Aug'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}