{"dta.poem.1240": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein einig Ku\u00df soll meiner Pein/", "tokens": ["Ein", "ei\u00b7nig", "Ku\u00df", "soll", "mei\u00b7ner", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll meiner Treue Zahlung seyn?", "tokens": ["Soll", "mei\u00b7ner", "Treu\u00b7e", "Zah\u00b7lung", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du weist ja/ da\u00df der Ku\u00df besiegelt das Versprechen", "tokens": ["Du", "weist", "ja", "/", "da\u00df", "der", "Ku\u00df", "be\u00b7sie\u00b7gelt", "das", "Ver\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der zugesagten Gunst/ da\u00df Liebe durch di\u00df Pfand", "tokens": ["Der", "zu\u00b7ge\u00b7sag\u00b7ten", "Gunst", "/", "da\u00df", "Lie\u00b7be", "durch", "di\u00df", "Pfand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "NN", "APPR", "PDS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein stilles Jawort auff die Lippen dr\u00fcckt.", "tokens": ["Ein", "stil\u00b7les", "Ja\u00b7wort", "auff", "die", "Lip\u00b7pen", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bist du gesinnt dein Wort/ der Freundschafft Recht/ zu bre-", "tokens": ["Bist", "du", "ge\u00b7sinnt", "dein", "Wort", "/", "der", "Freund\u00b7schafft", "Recht", "/", "zu", "bre"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "$(", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "chen?", "tokens": ["chen", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ein Ku\u00df und tausend noch thun schlechten Widerstand:", "tokens": ["Ein", "Ku\u00df", "und", "tau\u00b7send", "noch", "thun", "schlech\u00b7ten", "Wi\u00b7der\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "ADV", "VVINF", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo nicht? wie aus den treuen Augen blickt/", "tokens": ["Wo", "nicht", "?", "wie", "aus", "den", "treu\u00b7en", "Au\u00b7gen", "blickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "PWAV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Was schadet dirs/ wenn mir zu gutt", "tokens": ["Was", "scha\u00b7det", "dirs", "/", "wenn", "mir", "zu", "gutt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIS", "$(", "KOUS", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dein Mund noch mehr Versichrung thut?", "tokens": ["Dein", "Mund", "noch", "mehr", "Ver\u00b7sich\u00b7rung", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}