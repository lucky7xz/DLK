{"textgrid.poem.67839": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11. Wend', o wende diesen Blick", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wend', o wende diesen Blick,", "tokens": ["Wend'", ",", "o", "wen\u00b7de", "die\u00b7sen", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Aurora d\u00e4mmert nur!", "tokens": ["Dem", "Au\u00b7ro\u00b7ra", "d\u00e4m\u00b7mert", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und die Lippe zeuch zur\u00fcck,", "tokens": ["Und", "die", "Lip\u00b7pe", "zeuch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVIMP", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll so s\u00fcssem falschen Schwur;", "tokens": ["Voll", "so", "s\u00fcs\u00b7sem", "fal\u00b7schen", "Schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Treu nur, hier, ach! hier", "tokens": ["Mei\u00b7ne", "Treu", "nur", ",", "hier", ",", "ach", "!", "hier"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ADV", "$,", "ITJ", "$.", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vestgek\u00fc\u00dft, gib wieder mir!", "tokens": ["Vest\u00b7ge\u00b7k\u00fc\u00dft", ",", "gib", "wie\u00b7der", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00fcll, o h\u00fcll den Busen zart,", "tokens": ["H\u00fcll", ",", "o", "h\u00fcll", "den", "Bu\u00b7sen", "zart", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo auf H\u00fcgeln Schnee und kalt", "tokens": ["Wo", "auf", "H\u00fc\u00b7geln", "Schnee", "und", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN", "KON", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Kn\u00f6spchen bl\u00fchn \u2013 ach! jener Art,", "tokens": ["Kn\u00f6spc\u00b7hen", "bl\u00fchn", "\u2013", "ach", "!", "je\u00b7ner", "Art", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "ITJ", "$.", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie April sie niederwallt.", "tokens": ["Wie", "Ap\u00b7ril", "sie", "nie\u00b7der\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Armes Herz! in Eises Schoos", "tokens": ["Ar\u00b7mes", "Herz", "!", "in", "Ei\u00b7ses", "Schoos"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt es hier; ach, gib es los!", "tokens": ["Liegt", "es", "hier", ";", "ach", ",", "gib", "es", "los", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "XY", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}