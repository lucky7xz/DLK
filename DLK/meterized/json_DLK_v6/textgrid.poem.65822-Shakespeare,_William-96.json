{"textgrid.poem.65822": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "96", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sagt, dein Fehl ist Jugend, \u00dcbermut;", "tokens": ["Der", "sagt", ",", "dein", "Fehl", "ist", "Ju\u00b7gend", ",", "\u00dc\u00b7ber\u00b7mut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der rechnet Scherz und Jugend dir zur Zier:", "tokens": ["Der", "rech\u00b7net", "Scherz", "und", "Ju\u00b7gend", "dir", "zur", "Zier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch Zier wie Fehler, alle finden's gut;", "tokens": ["Doch", "Zier", "wie", "Feh\u00b7ler", ",", "al\u00b7le", "fin\u00b7den's", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "NN", "$,", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fehl wird zu Schmuck, wenn er sich zeigt an dir.", "tokens": ["Fehl", "wird", "zu", "Schmuck", ",", "wenn", "er", "sich", "zeigt", "an", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie an der Hand gesalbter K\u00f6niginnen", "tokens": ["Wie", "an", "der", "Hand", "ge\u00b7salb\u00b7ter", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des kleinsten Kleinods Wert zu steigen pflegt,", "tokens": ["Des", "kleins\u00b7ten", "Klein\u00b7ods", "Wert", "zu", "stei\u00b7gen", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So gilt an dir ein tadelhaft Beginnen", "tokens": ["So", "gilt", "an", "dir", "ein", "ta\u00b7del\u00b7haft", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr recht und wird zu Tugend umgepr\u00e4gt.", "tokens": ["F\u00fcr", "recht", "und", "wird", "zu", "Tu\u00b7gend", "um\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie viele L\u00e4mmer k\u00f6nnt' ein Wolf verzehren,", "tokens": ["Wie", "vie\u00b7le", "L\u00e4m\u00b7mer", "k\u00f6nnt'", "ein", "Wolf", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VMFIN", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn er des Lamms Geb\u00e4rden sich verschafft:", "tokens": ["Wenn", "er", "des", "Lamms", "Ge\u00b7b\u00e4r\u00b7den", "sich", "ver\u00b7schafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie viele Gaffer k\u00f6nntest du bet\u00f6ren,", "tokens": ["Wie", "vie\u00b7le", "Gaf\u00b7fer", "k\u00f6nn\u00b7test", "du", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gebrauchtest du des ganzen Wesens Kraft!", "tokens": ["Ge\u00b7brauch\u00b7test", "du", "des", "gan\u00b7zen", "We\u00b7sens", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch, tu es nicht! Ich halte so dich wert,", "tokens": ["Doch", ",", "tu", "es", "nicht", "!", "Ich", "hal\u00b7te", "so", "dich", "wert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df, wie du selbst, mein auch dein Ruf geh\u00f6rt.", "tokens": ["Da\u00df", ",", "wie", "du", "selbst", ",", "mein", "auch", "dein", "Ruf", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "ADV", "$,", "PPOSAT", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}