{"dta.poem.17238": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "88.  Au\u00df dem Hohen Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig f\u00fchrt die Braut inn Keller selbst hinein/", "tokens": ["Der", "K\u00f6\u00b7nig", "f\u00fchrt", "die", "Braut", "inn", "Kel\u00b7ler", "selbst", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie jhr mag erw\u00f6hln den allerbesten Wein.", "tokens": ["Da\u00df", "sie", "jhr", "mag", "er\u00b7w\u00f6hln", "den", "al\u00b7ler\u00b7bes\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So machts GOtt auch mit dir/ wann du bist seine Braut/", "tokens": ["So", "machts", "Gott", "auch", "mit", "dir", "/", "wann", "du", "bist", "sei\u00b7ne", "Braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPR", "PPER", "$(", "PWAV", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hat nichts in sich selbst/ da\u00df er dir nicht vertraut.", "tokens": ["Er", "hat", "nichts", "in", "sich", "selbst", "/", "da\u00df", "er", "dir", "nicht", "ver\u00b7traut", "."], "token_info"