{"textgrid.poem.38400": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Proklamation", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aennele wehr, Aennele wehr,", "tokens": ["A\u00b7en\u00b7ne\u00b7le", "wehr", ",", "A\u00b7en\u00b7ne\u00b7le", "wehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NE", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Buben sind im Garten,", "tokens": ["Bu\u00b7ben", "sind", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steck den hintern Riegel f\u00fcr,", "tokens": ["Steck", "den", "hin\u00b7tern", "Rie\u00b7gel", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und la\u00df die Narren warten.", "tokens": ["Und", "la\u00df", "die", "Nar\u00b7ren", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}