{"textgrid.poem.41071": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das Kalb", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Tier, im dunklen Stall geboren,", "tokens": ["Du", "Tier", ",", "im", "dunk\u00b7len", "Stall", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' du des Lebens recht bewu\u00dft,", "tokens": ["Eh'", "du", "des", "Le\u00b7bens", "recht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Greift dich ein Schl\u00e4chter bei den Ohren", "tokens": ["Greift", "dich", "ein", "Schl\u00e4ch\u00b7ter", "bei", "den", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rei\u00dft dich von der Mutterbrust.", "tokens": ["Und", "rei\u00dft", "dich", "von", "der", "Mut\u00b7ter\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein gro\u00dfes Auge, fromm und helle,", "tokens": ["Dein", "gro\u00b7\u00dfes", "Au\u00b7ge", ",", "fromm", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht da die Au zum erstenmal,", "tokens": ["Sieht", "da", "die", "Au", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch angstvoll; denn des Hunds Gebelle", "tokens": ["Doch", "angst\u00b7voll", ";", "denn", "des", "Hunds", "Ge\u00b7bel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Treibt rastlos dich durchs gr\u00fcne Tal.", "tokens": ["Treibt", "rast\u00b7los", "dich", "durchs", "gr\u00fc\u00b7ne", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald binden sie dir deine Glieder,", "tokens": ["Bald", "bin\u00b7den", "sie", "dir", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie achten nicht dein Angstgeschrei,", "tokens": ["Sie", "ach\u00b7ten", "nicht", "dein", "Angst\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man wirft dich auf die Schlachtbank nieder", "tokens": ["Man", "wirft", "dich", "auf", "die", "Schlacht\u00b7bank", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schneidet dir den Hals entzwei.", "tokens": ["Und", "schnei\u00b7det", "dir", "den", "Hals", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch bei dem letzten Hauch der Kehle", "tokens": ["Doch", "bei", "dem", "letz\u00b7ten", "Hauch", "der", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Glanz aus deinem Auge spricht:", "tokens": ["Ein", "Glanz", "aus", "dei\u00b7nem", "Au\u00b7ge", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbin mir auch wohnet eine Seele,", "tokens": ["\u00bb", "in", "mir", "auch", "woh\u00b7net", "ei\u00b7ne", "See\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr mich auch h\u00e4lt ein Gott Gericht.\u00ab", "tokens": ["F\u00fcr", "mich", "auch", "h\u00e4lt", "ein", "Gott", "Ge\u00b7richt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}