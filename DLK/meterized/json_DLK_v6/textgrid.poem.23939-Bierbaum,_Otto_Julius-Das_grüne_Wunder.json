{"textgrid.poem.23939": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Das gr\u00fcne Wunder", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Birkenhain stand wei\u00df und kahl,", "tokens": ["Mein", "Bir\u00b7ken\u00b7hain", "stand", "wei\u00df", "und", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die d\u00fcnnen St\u00e4mmchen fror,", "tokens": ["Die", "d\u00fcn\u00b7nen", "St\u00e4mm\u00b7chen", "fror", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam April und zauberte", "tokens": ["Da", "kam", "Ap\u00b7ril", "und", "zau\u00b7ber\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben gr\u00fcn hervor.", "tokens": ["Das", "Le\u00b7ben", "gr\u00fcn", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit einem Schleier angethan", "tokens": ["Mit", "ei\u00b7nem", "Schlei\u00b7er", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht nun mein Birkenhain;", "tokens": ["Steht", "nun", "mein", "Bir\u00b7ken\u00b7hain", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das gr\u00fcne Wunder ist geschehn,", "tokens": ["Das", "gr\u00fc\u00b7ne", "Wun\u00b7der", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun la\u00dft uns gl\u00e4ubig sein.", "tokens": ["Nun", "la\u00dft", "uns", "gl\u00e4u\u00b7big", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun la\u00dft uns glauben wiederum,", "tokens": ["Nun", "la\u00dft", "uns", "glau\u00b7ben", "wie\u00b7de\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Leben Sch\u00f6nheit hei\u00dft:", "tokens": ["Da\u00df", "Le\u00b7ben", "Sch\u00f6n\u00b7heit", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Birkicht ist ein Zauberwald,", "tokens": ["Mein", "Bir\u00b7kicht", "ist", "ein", "Zau\u00b7ber\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dem das Wunder krei\u00dft.", "tokens": ["In", "dem", "das", "Wun\u00b7der", "krei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}