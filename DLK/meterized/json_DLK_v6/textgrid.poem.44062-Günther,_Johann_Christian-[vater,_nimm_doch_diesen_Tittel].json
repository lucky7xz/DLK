{"textgrid.poem.44062": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[vater, nimm doch diesen Tittel]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater, nimm doch diesen Tittel", "tokens": ["Va\u00b7ter", ",", "nimm", "doch", "die\u00b7sen", "Tit\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem schwachen S\u00fcnder an,", "tokens": ["Von", "dem", "schwa\u00b7chen", "S\u00fcn\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher ohne dich kein Mittel", "tokens": ["Wel\u00b7cher", "oh\u00b7ne", "dich", "kein", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zur Bekehrung finden kan;", "tokens": ["Zur", "Be\u00b7keh\u00b7rung", "fin\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeige deine Kraft an mir", "tokens": ["Zei\u00b7ge", "dei\u00b7ne", "Kraft", "an", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erzeuge die Begier,", "tokens": ["Und", "er\u00b7zeu\u00b7ge", "die", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf so langes th\u00f6richt Handeln", "tokens": ["Auf", "so", "lan\u00b7ges", "th\u00f6\u00b7richt", "Han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einmahl klug und fromm zu wandeln.", "tokens": ["Ein\u00b7mahl", "klug", "und", "fromm", "zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hab ich gleich die sch\u00f6nsten Jahre", "tokens": ["Hab", "ich", "gleich", "die", "sch\u00f6ns\u00b7ten", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": ". . . . . . . . . . . . fortgeschickt", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "fort\u00b7ge\u00b7schickt"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "word"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und . . . . . . . . . . . . . . . .", "tokens": ["Und", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["KON", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vor der Zeit herzuger\u00fcckt,", "tokens": ["Vor", "der", "Zeit", "her\u00b7zu\u00b7ge\u00b7r\u00fcckt", ","], "token_info": [