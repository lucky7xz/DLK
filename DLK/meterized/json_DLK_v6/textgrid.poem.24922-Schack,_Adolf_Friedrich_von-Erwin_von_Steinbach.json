{"textgrid.poem.24922": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erwin von Steinbach", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdank dir, Ew'ger! Meine Sendung", "tokens": ["\u00bb", "dank", "dir", ",", "Ew'\u00b7ger", "!", "Mei\u00b7ne", "Sen\u00b7dung"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "PPER", "$,", "ADJA", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Erde ward vollbracht;", "tokens": ["Auf", "der", "Er\u00b7de", "ward", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in herrlicher Vollendung", "tokens": ["Denn", "in", "herr\u00b7li\u00b7cher", "Vol\u00b7len\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Strahlt das Werk, das ich erdacht,", "tokens": ["Strahlt", "das", "Werk", ",", "das", "ich", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um den ungebornen Jahren", "tokens": ["Um", "den", "un\u00b7ge\u00b7bor\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcnft'ger Zeit zu offenbaren,", "tokens": ["K\u00fcnft'\u00b7ger", "Zeit", "zu", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich nicht umsonst gelebt.\u00ab", "tokens": ["Da\u00df", "ich", "nicht", "um\u00b7sonst", "ge\u00b7lebt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Erwin also vor dem M\u00fcnster,", "tokens": ["Er\u00b7win", "al\u00b7so", "vor", "dem", "M\u00fcns\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Der zum Abendhimmel finster", "tokens": ["Der", "zum", "A\u00b7bend\u00b7him\u00b7mel", "fins\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seine Riesenmauern hebt.", "tokens": ["Sei\u00b7ne", "Rie\u00b7sen\u00b7mau\u00b7ern", "hebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber den gewalt'gen Zinnen", "tokens": ["Ue\u00b7ber", "den", "ge\u00b7walt'\u00b7gen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt der Mond ins Aetherblau;", "tokens": ["Steigt", "der", "Mond", "ins", "A\u00b7e\u00b7ther\u00b7blau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und noch lang in tiefem Sinnen", "tokens": ["Und", "noch", "lang", "in", "tie\u00b7fem", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht der Meister vor dem Bau,", "tokens": ["Steht", "der", "Meis\u00b7ter", "vor", "dem", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4hrend um ihn, stumm und stummer,", "tokens": ["W\u00e4h\u00b7rend", "um", "ihn", ",", "stumm", "und", "stum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon die Welt in sanften Schlummer", "tokens": ["Schon", "die", "Welt", "in", "sanf\u00b7ten", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre kleinen Sorgen wiegt", "tokens": ["Ih\u00b7re", "klei\u00b7nen", "Sor\u00b7gen", "wiegt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und auf ihren Menschenzwergen", "tokens": ["Und", "auf", "ih\u00b7ren", "Men\u00b7schenz\u00b7wer\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von dem Turme, hoch gleich Bergen,", "tokens": ["Von", "dem", "Tur\u00b7me", ",", "hoch", "gleich", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der erhabne Schatten liegt.", "tokens": ["Der", "er\u00b7hab\u00b7ne", "Schat\u00b7ten", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da, so wie im Jugendschwunge", "tokens": ["Da", ",", "so", "wie", "im", "Ju\u00b7gend\u00b7schwun\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dichterlippen zum Gesang,", "tokens": ["Dich\u00b7ter\u00b7lip\u00b7pen", "zum", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt des Domes Glockenzunge", "tokens": ["Hebt", "des", "Do\u00b7mes", "Glo\u00b7cken\u00b7zun\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zum ersten Feierklang;", "tokens": ["Sich", "zum", "ers\u00b7ten", "Fei\u00b7er\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schallend \u00f6ffnet am Portale", "tokens": ["Schal\u00b7lend", "\u00f6ff\u00b7net", "am", "Por\u00b7ta\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich das Thor der Kathedrale,", "tokens": ["Sich", "das", "Thor", "der", "Ka\u00b7thed\u00b7ra\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Und von innen dr\u00f6hnt ein Ruf;", "tokens": ["Und", "von", "in\u00b7nen", "dr\u00f6hnt", "ein", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohl versteht der Greis die Mahnung,", "tokens": ["Wohl", "ver\u00b7steht", "der", "Greis", "die", "Mah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und er tritt mit ernster Ahnung", "tokens": ["Und", "er", "tritt", "mit", "erns\u00b7ter", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In die Welt, die er erschuf.", "tokens": ["In", "die", "Welt", ",", "die", "er", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Festlich gr\u00fc\u00dfen ihn die hehren", "tokens": ["Fest\u00b7lich", "gr\u00fc\u00b7\u00dfen", "ihn", "die", "heh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hallen mit dem m\u00e4cht'gen Chor,", "tokens": ["Hal\u00b7len", "mit", "dem", "m\u00e4cht'\u00b7gen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den prangenden Alt\u00e4ren", "tokens": ["Von", "den", "pran\u00b7gen\u00b7den", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallt der Myrrhenrauch empor;", "tokens": ["Wallt", "der", "Myr\u00b7rhen\u00b7rauch", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mystisch aus der Fensterrose", "tokens": ["Mys\u00b7tisch", "aus", "der", "Fens\u00b7ter\u00b7ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht er durch die grenzenlose", "tokens": ["Sieht", "er", "durch", "die", "gren\u00b7zen\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00f6lbung einen Schimmer gl\u00fchn,", "tokens": ["W\u00f6l\u00b7bung", "ei\u00b7nen", "Schim\u00b7mer", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht ihn droben von den Kn\u00e4ufen", "tokens": ["Sieht", "ihn", "dro\u00b7ben", "von", "den", "Kn\u00e4u\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der gewalt'gen S\u00e4ulen tr\u00e4ufen,", "tokens": ["Der", "ge\u00b7walt'\u00b7gen", "S\u00e4u\u00b7len", "tr\u00e4u\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hier in den Kapellen bl\u00fchn.", "tokens": ["Hier", "in", "den", "Ka\u00b7pel\u00b7len", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ihm ist, zu allen Seiten", "tokens": ["Und", "ihm", "ist", ",", "zu", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rege sich der Bau um ihn,", "tokens": ["Re\u00b7ge", "sich", "der", "Bau", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die eh'rnen, langgereihten", "tokens": ["Wo", "die", "eh'r\u00b7nen", ",", "lang\u00b7ge\u00b7reih\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bilder in den Nischen knien;", "tokens": ["Bil\u00b7der", "in", "den", "Ni\u00b7schen", "kni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seltsam flimmert's an den W\u00e4nden;", "tokens": ["Selt\u00b7sam", "flim\u00b7mert's", "an", "den", "W\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Apostel in den Blenden", "tokens": ["Die", "A\u00b7pos\u00b7tel", "in", "den", "Blen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oeffnen ihr geweihtes Buch,", "tokens": ["Oeff\u00b7nen", "ihr", "ge\u00b7weih\u00b7tes", "Buch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und von Jungfraun, die zum Segen", "tokens": ["Und", "von", "Jung\u00b7fraun", ",", "die", "zum", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "$,", "PRELS", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Ihre Lippen sanft bewegen,", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "sanft", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00f6rt er rings den Atemzug.", "tokens": ["H\u00f6rt", "er", "rings", "den", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Horch! und von den H\u00f6hn des Domes", "tokens": ["Horch", "!", "und", "von", "den", "H\u00f6hn", "des", "Do\u00b7mes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Quillt herab der Orgelklang,", "tokens": ["Quillt", "her\u00b7ab", "der", "Or\u00b7gel\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wallt und flutet m\u00e4cht'gen Stromes", "tokens": ["Wallt", "und", "flu\u00b7tet", "m\u00e4cht'\u00b7gen", "Stro\u00b7mes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den Strebebogengang;", "tokens": ["Durch", "den", "Stre\u00b7be\u00b7bo\u00b7gen\u00b7gang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und aus allen Schiffen brechen,", "tokens": ["Und", "aus", "al\u00b7len", "Schif\u00b7fen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie das Meer in tausend B\u00e4chen", "tokens": ["Wie", "das", "Meer", "in", "tau\u00b7send", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ueber seine D\u00e4mme braust,", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "D\u00e4m\u00b7me", "braust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Echoreiche Katarakten,", "tokens": ["E\u00b7cho\u00b7rei\u00b7che", "Ka\u00b7ta\u00b7rak\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deren Fall an den gezackten", "tokens": ["De\u00b7ren", "Fall", "an", "den", "ge\u00b7zack\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Pfeilern in die Tiefe saust.", "tokens": ["Pfei\u00b7lern", "in", "die", "Tie\u00b7fe", "saust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": 