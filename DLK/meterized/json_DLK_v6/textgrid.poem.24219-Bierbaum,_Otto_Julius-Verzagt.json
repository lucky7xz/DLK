{"textgrid.poem.24219": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Verzagt", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich wieder schw\u00e4rmen, ich,", "tokens": ["Soll", "ich", "wie\u00b7der", "schw\u00e4r\u00b7men", ",", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich m\u00fcd bin und verdrossen,", "tokens": ["Der", "ich", "m\u00fcd", "bin", "und", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schicksalslaugen\u00fcbergossen,", "tokens": ["Schick\u00b7sals\u00b7lau\u00b7gen\u00b7\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traurig, tr\u00fcb und j\u00e4mmerlich?", "tokens": ["Trau\u00b7rig", ",", "tr\u00fcb", "und", "j\u00e4m\u00b7mer\u00b7lich", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll ich? Nein, ich dr\u00fccke mich.", "tokens": ["Soll", "ich", "?", "Nein", ",", "ich", "dr\u00fc\u00b7cke", "mich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$.", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Schw\u00e4rmer sind verschossen,", "tokens": ["Mei\u00b7ne", "Schw\u00e4r\u00b7mer", "sind", "ver\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Schicksal hats beschlossen:", "tokens": ["Und", "das", "Schick\u00b7sal", "hats", "be\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Wonnen mehr f\u00fcr dich.", "tokens": ["Kei\u00b7ne", "Won\u00b7nen", "mehr", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber deine Augen, Kind,", "tokens": ["A\u00b7ber", "dei\u00b7ne", "Au\u00b7gen", ",", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind bestimmt, das Gl\u00fcck zu schauen,", "tokens": ["Sind", "be\u00b7stimmt", ",", "das", "Gl\u00fcck", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das im sch\u00f6nsten Bogen geht:", "tokens": ["Das", "im", "sch\u00f6ns\u00b7ten", "Bo\u00b7gen", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Ruhe, Klarheit, Majest\u00e4t,", "tokens": ["Ru\u00b7he", ",", "Klar\u00b7heit", ",", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Davon deine Augenbrauen", "tokens": ["Da\u00b7von", "dei\u00b7ne", "Au\u00b7gen\u00b7brau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allersch\u00f6nstes Abbild sind.", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "Ab\u00b7bild", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}