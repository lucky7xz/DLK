{"textgrid.poem.36427": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Drittes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der rauhe Ost, der fr\u00fch nach Rom mich jagte,", "tokens": ["Der", "rau\u00b7he", "Ost", ",", "der", "fr\u00fch", "nach", "Rom", "mich", "jag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward dort zum Zephir hyacinthner L\u00fcste.", "tokens": ["Ward", "dort", "zum", "Ze\u00b7phir", "hya\u00b7cin\u00b7th\u00b7ner", "L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und keiner, der nur immer M\u00e4dchen k\u00fc\u00dfte,", "tokens": ["Und", "kei\u00b7ner", ",", "der", "nur", "im\u00b7mer", "M\u00e4d\u00b7chen", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "R\u00fchm seinen Schwanz, da\u00df er im Himmel ragte.", "tokens": ["R\u00fchm", "sei\u00b7nen", "Schwanz", ",", "da\u00df", "er", "im", "Him\u00b7mel", "rag\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch mich erregen noch die herben Br\u00fcste", "tokens": ["Auch", "mich", "er\u00b7re\u00b7gen", "noch", "die", "her\u00b7ben", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kampan'scher M\u00e4dchen, doch wie oft verzagte", "tokens": ["Kam\u00b7pan'\u00b7scher", "M\u00e4d\u00b7chen", ",", "doch", "wie", "oft", "ver\u00b7zag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "KOKOM", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Meerschaum an dem fremden Golf und klagte", "tokens": ["Mein", "Meer\u00b7schaum", "an", "dem", "frem\u00b7den", "Golf", "und", "klag\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ohne recht' Verst\u00e4ndnis diese K\u00fcste.", "tokens": ["Da\u00df", "oh\u00b7ne", "recht'", "Ver\u00b7st\u00e4nd\u00b7nis", "die\u00b7se", "K\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie anders schmiegte sich der Arsch des Knaben", "tokens": ["Wie", "an\u00b7ders", "schmieg\u00b7te", "sich", "der", "Arsch", "des", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dem Schwanz in lieblich-rundlichem Gehaben;", "tokens": ["dem", "Schwanz", "in", "lieb\u00b7lich\u00b7rund\u00b7li\u00b7chem", "Ge\u00b7ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Weib hat so behende mit der Zunge", "tokens": ["Kein", "Weib", "hat", "so", "be\u00b7hen\u00b7de", "mit", "der", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Eichel mir geleckt wie dieser Junge.", "tokens": ["Die", "Ei\u00b7chel", "mir", "ge\u00b7leckt", "wie", "die\u00b7ser", "Jun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KOKOM", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oh, k\u00f6nnt' ich doch an deinem Marmorhintern,", "tokens": ["Oh", ",", "k\u00f6nnt'", "ich", "doch", "an", "dei\u00b7nem", "Mar\u00b7mor\u00b7hin\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Knabe, viele Monde \u00fcberwintern ... !", "tokens": ["Mein", "Kna\u00b7be", ",", "vie\u00b7le", "Mon\u00b7de", "\u00fc\u00b7berw\u00b7in\u00b7tern", "...", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIAT", "NN", "VVINF", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}