{"textgrid.poem.47029": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und weil du dich so stolzen Sinns erhoben,", "tokens": ["Und", "weil", "du", "dich", "so", "stol\u00b7zen", "Sinns", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du vom sch\u00f6nsten nur willst sein gefreiet,", "tokens": ["Da\u00df", "du", "vom", "sch\u00f6ns\u00b7ten", "nur", "willst", "sein", "ge\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "VVFIN", "ADV", "VMFIN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00fcnsch' ich dir, da\u00df eigens dir geweihet", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "dir", ",", "da\u00df", "ei\u00b7gens", "dir", "ge\u00b7wei\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "ADV", "PPER", "VVPP"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Die Englein einen bringen her von droben,", "tokens": ["Die", "En\u00b7glein", "ei\u00b7nen", "brin\u00b7gen", "her", "von", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus allem Fr\u00fchlingsduft zusammgestoben,", "tokens": ["Aus", "al\u00b7lem", "Fr\u00fch\u00b7lings\u00b7duft", "zu\u00b7samm\u00b7ge\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus allem Perlentau zusammgereihet,", "tokens": ["Aus", "al\u00b7lem", "Per\u00b7len\u00b7tau", "zu\u00b7samm\u00b7ge\u00b7rei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus allem Bl\u00fctenschnee zusammgeschneiet,", "tokens": ["Aus", "al\u00b7lem", "Bl\u00fc\u00b7ten\u00b7schnee", "zu\u00b7samm\u00b7ge\u00b7schnei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus aller Herrlichkeit zusammgewoben.", "tokens": ["Aus", "al\u00b7ler", "Herr\u00b7lich\u00b7keit", "zu\u00b7samm\u00b7ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn du dann die goldne Schrift entdeckest", "tokens": ["Und", "wenn", "du", "dann", "die", "gold\u00b7ne", "Schrift", "ent\u00b7de\u00b7ckest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An seiner Stirn: Ich bin der sch\u00f6nst' von allen,", "tokens": ["An", "sei\u00b7ner", "Stirn", ":", "Ich", "bin", "der", "sch\u00f6nst'", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcr dich sch\u00f6nste bin ich hergesendet;", "tokens": ["Und", "f\u00fcr", "dich", "sch\u00f6ns\u00b7te", "bin", "ich", "her\u00b7ge\u00b7sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn du dann nach ihm die Arm' ausstreckest,", "tokens": ["Und", "wenn", "du", "dann", "nach", "ihm", "die", "Arm'", "aus\u00b7stre\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So soll der Duftmann dir in Duft zerwallen,", "tokens": ["So", "soll", "der", "Duft\u00b7mann", "dir", "in", "Duft", "zer\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dir die Sehnsucht bleiben, die nicht endet.", "tokens": ["Und", "dir", "die", "Sehn\u00b7sucht", "blei\u00b7ben", ",", "die", "nicht", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$,", "PRELS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}