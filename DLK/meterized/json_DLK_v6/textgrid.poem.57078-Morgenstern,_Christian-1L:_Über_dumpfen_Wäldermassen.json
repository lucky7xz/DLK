{"textgrid.poem.57078": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber dumpfen W\u00e4ldermassen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber dumpfen W\u00e4ldermassen", "tokens": ["\u00dc\u00b7ber", "dum\u00b7pfen", "W\u00e4l\u00b7der\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "baden steile erdenferne", "tokens": ["ba\u00b7den", "stei\u00b7le", "er\u00b7den\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6hen sich im rosenblassen", "tokens": ["H\u00f6\u00b7hen", "sich", "im", "ro\u00b7sen\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Firmament der ersten Sterne.", "tokens": ["Fir\u00b7ma\u00b7ment", "der", "ers\u00b7ten", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tageshelle will nicht scheiden,", "tokens": ["Ta\u00b7ges\u00b7hel\u00b7le", "will", "nicht", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ehe nicht der letzte flimmert;", "tokens": ["e\u00b7he", "nicht", "der", "letz\u00b7te", "flim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und so leuchten sie, von beiden", "tokens": ["und", "so", "leuch\u00b7ten", "sie", ",", "von", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "zwiefach feurig angeschimmert.", "tokens": ["zwie\u00b7fach", "feu\u00b7rig", "an\u00b7ge\u00b7schim\u00b7mert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bis des ehernen Geschickes", "tokens": ["Bis", "des", "e\u00b7her\u00b7nen", "Ge\u00b7schi\u00b7ckes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "strenger Schlu\u00df den Wettstreit endet", "tokens": ["stren\u00b7ger", "Schlu\u00df", "den", "Wett\u00b7streit", "en\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und der Tag sich langen Blickes", "tokens": ["und", "der", "Tag", "sich", "lan\u00b7gen", "Bli\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "von den vielgeliebten wendet.", "tokens": ["von", "den", "viel\u00b7ge\u00b7lieb\u00b7ten", "wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie beraubt der Seele, sinken", "tokens": ["Wie", "be\u00b7raubt", "der", "See\u00b7le", ",", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVPP", "ART", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "stumm die m\u00e4chtigen zusammen.", "tokens": ["stumm", "die", "m\u00e4ch\u00b7ti\u00b7gen", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Starre, kalte Strahlen blinken", "tokens": ["Star\u00b7re", ",", "kal\u00b7te", "Strah\u00b7len", "blin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "statt lebendiger Liebesflammen.", "tokens": ["statt", "le\u00b7ben\u00b7di\u00b7ger", "Lie\u00b7bes\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}