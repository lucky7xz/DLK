{"textgrid.poem.44732": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[auszeichnung hier erwarte nie]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auszeichnung hier erwarte nie,", "tokens": ["Aus\u00b7zeich\u00b7nung", "hier", "er\u00b7war\u00b7te", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn das System verbeuts,", "tokens": ["Denn", "das", "Sys\u00b7tem", "ver\u00b7beuts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Man h\u00e4ngt das Kreuz nicht ans Genie,", "tokens": ["Man", "h\u00e4ngt", "das", "Kreuz", "nicht", "ans", "Ge\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, das Genie ans Kreuz.", "tokens": ["Nein", ",", "das", "Ge\u00b7nie", "ans", "Kreuz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}