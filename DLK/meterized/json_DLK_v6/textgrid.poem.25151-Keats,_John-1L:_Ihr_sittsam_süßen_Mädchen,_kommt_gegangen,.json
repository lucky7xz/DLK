{"textgrid.poem.25151": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr sittsam s\u00fc\u00dfen M\u00e4dchen, kommt gegangen,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sittsam s\u00fc\u00dfen M\u00e4dchen, kommt gegangen,", "tokens": ["Ihr", "sitt\u00b7sam", "s\u00fc\u00b7\u00dfen", "M\u00e4d\u00b7chen", ",", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Senkt unter Wimpern blasser Augenlider", "tokens": ["Senkt", "un\u00b7ter", "Wim\u00b7pern", "blas\u00b7ser", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem\u00fctig keuschen Blick zur Erde nieder", "tokens": ["De\u00b7m\u00fc\u00b7tig", "keu\u00b7schen", "Blick", "zur", "Er\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und haltet milde Hand von Hand umfangen,", "tokens": ["Und", "hal\u00b7tet", "mil\u00b7de", "Hand", "von", "Hand", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als k\u00f6nntet ihr best\u00fcrzt nur und mit Bangen", "tokens": ["Als", "k\u00f6nn\u00b7tet", "ihr", "be\u00b7st\u00fcrzt", "nur", "und", "mit", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "ADV", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Opfer eurer Sch\u00f6nheit sehn, das nieder", "tokens": ["Ein", "Op\u00b7fer", "eu\u00b7rer", "Sch\u00f6n\u00b7heit", "sehn", ",", "das", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,", "PDS", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In nasse Nacht sinkt: niemals l\u00f6st ihn wieder", "tokens": ["In", "nas\u00b7se", "Nacht", "sinkt", ":", "nie\u00b7mals", "l\u00f6st", "ihn", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die junge Liebe aus den Wogenschlangen.", "tokens": ["Die", "jun\u00b7ge", "Lie\u00b7be", "aus", "den", "Wo\u00b7gen\u00b7schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Leander ist's, der sich zu Tode m\u00fcht.", "tokens": ["Le\u00b7an\u00b7der", "ist's", ",", "der", "sich", "zu", "To\u00b7de", "m\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ohnm\u00e4chtig l\u00e4cheln noch die matten Lippen", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "l\u00e4\u00b7cheln", "noch", "die", "mat\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den letzten Ku\u00df, den Sturm zu Hero trug.", "tokens": ["Den", "letz\u00b7ten", "Ku\u00df", ",", "den", "Sturm", "zu", "He\u00b7ro", "trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O schrecklich! Seht, wie seine Kraft verspr\u00fcht.", "tokens": ["O", "schreck\u00b7lich", "!", "Seht", ",", "wie", "sei\u00b7ne", "Kraft", "ver\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Leib l\u00f6scht aus wie Leuchten zwischen Klippen,", "tokens": ["Sein", "Leib", "l\u00f6scht", "aus", "wie", "Leuch\u00b7ten", "zwi\u00b7schen", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufperlt der Liebe letzter Atemzug.", "tokens": ["Auf\u00b7perlt", "der", "Lie\u00b7be", "letz\u00b7ter", "A\u00b7tem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}