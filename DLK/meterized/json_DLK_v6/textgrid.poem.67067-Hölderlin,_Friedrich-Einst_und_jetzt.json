{"textgrid.poem.67067": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einst und jetzt", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst, tr\u00e4nend Auge! sahst du so hell empor!", "tokens": ["Einst", ",", "tr\u00e4\u00b7nend", "Au\u00b7ge", "!", "sahst", "du", "so", "hell", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "NN", "$.", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Einst schlugst du mir so ruhig, emp\u00f6rtes Herz!", "tokens": ["Einst", "schlugst", "du", "mir", "so", "ru\u00b7hig", ",", "em\u00b7p\u00f6r\u00b7tes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "So, wie die Wallungen des B\u00e4chleins,", "tokens": ["So", ",", "wie", "die", "Wal\u00b7lun\u00b7gen", "des", "B\u00e4ch\u00b7leins", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo die Forell am Gestade hinschl\u00fcpft.", "tokens": ["Wo", "die", "Fo\u00b7rell", "am", "Ge\u00b7sta\u00b7de", "hin\u00b7schl\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Einst in des Vaters Scho\u00dfe \u2013 des liebenden,", "tokens": ["Einst", "in", "des", "Va\u00b7ters", "Scho\u00b7\u00dfe", "\u2013", "des", "lie\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$(", "ART", "ADJA", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Geliebten Vaters \u2013 aber der W\u00fcrger kam,", "tokens": ["Ge\u00b7lieb\u00b7ten", "Va\u00b7ters", "\u2013", "a\u00b7ber", "der", "W\u00fcr\u00b7ger", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir weinten, flehten, doch der W\u00fcrger", "tokens": ["Wir", "wein\u00b7ten", ",", "fleh\u00b7ten", ",", "doch", "der", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schnellte den Pfeil; und es sank die St\u00fctze!", "tokens": ["Schnell\u00b7te", "den", "Pfeil", ";", "und", "es", "sank", "die", "St\u00fct\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ha! du gerechte Vorsicht! so bald begann", "tokens": ["Ha", "!", "du", "ge\u00b7rech\u00b7te", "Vor\u00b7sicht", "!", "so", "bald", "be\u00b7gann"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "ADJA", "NN", "$.", "ADV", "ADV", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der Sturm, so bald? \u2013 Doch \u2013 straft mich des Undanks nicht,", "tokens": ["Der", "Sturm", ",", "so", "bald", "?", "\u2013", "Doch", "\u2013", "straft", "mich", "des", "Un\u00b7danks", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "$.", "$(", "KON", "$(", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Stunden meiner Knabenfreude,", "tokens": ["Ihr", "Stun\u00b7den", "mei\u00b7ner", "Kna\u00b7ben\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stunden des Spiels und des Ruhel\u00e4chelns!", "tokens": ["Stun\u00b7den", "des", "Spiels", "und", "des", "Ru\u00b7he\u00b7l\u00e4\u00b7chelns", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich seh euch wieder \u2013 herrlicher Augenblick!", "tokens": ["Ich", "seh", "euch", "wie\u00b7der", "\u2013", "herr\u00b7li\u00b7cher", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da f\u00fcttert ich mein H\u00fchnchen, da pflanzt ich Kohl", "tokens": ["Da", "f\u00fct\u00b7tert", "ich", "mein", "H\u00fchn\u00b7chen", ",", "da", "pflanzt", "ich", "Kohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Nelken \u2013 freute so des Fr\u00fchlings", "tokens": ["Und", "Nel\u00b7ken", "\u2013", "freu\u00b7te", "so", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$(", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich und der Ernt, und des Herbstgewimmels.", "tokens": ["Mich", "und", "der", "Ernt", ",", "und", "des", "Herbst\u00b7ge\u00b7wim\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Da sucht ich Maienbl\u00fcmchen im Walde mir,", "tokens": ["Da", "sucht", "ich", "Mai\u00b7en\u00b7bl\u00fcm\u00b7chen", "im", "Wal\u00b7de", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da w\u00e4lzt ich mich im duftenden Heu umher,", "tokens": ["Da", "w\u00e4lzt", "ich", "mich", "im", "duf\u00b7ten\u00b7den", "Heu", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da brockt ich Milch mit Schnittern ein, da", "tokens": ["Da", "brockt", "ich", "Milch", "mit", "Schnit\u00b7tern", "ein", ",", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "PTKVZ", "$,", "KOUS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schleudert ich Schw\u00e4rmer am Rebenberge.", "tokens": ["Schleu\u00b7dert", "ich", "Schw\u00e4r\u00b7mer", "am", "Re\u00b7ben\u00b7ber\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Und o! wie warm, wie hing ich so warm an euch", "tokens": ["Und", "o", "!", "wie", "warm", ",", "wie", "hing", "ich", "so", "warm", "an", "euch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "$.", "PWAV", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gespielen meiner Einfalt, wie st\u00fcrmten wir", "tokens": ["Ge\u00b7spie\u00b7len", "mei\u00b7ner", "Ein\u00b7falt", ",", "wie", "st\u00fcrm\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In offner Feldschlacht, lehrten uns den", "tokens": ["In", "off\u00b7ner", "Feld\u00b7schlacht", ",", "lehr\u00b7ten", "uns", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Strudel durchschwimmen, die Eich ersteigen!", "tokens": ["Stru\u00b7del", "durch\u00b7schwim\u00b7men", ",", "die", "Eich", "er\u00b7stei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Jetzt wandl ich einsam an dem Gestade hin,", "tokens": ["Jetzt", "wandl", "ich", "ein\u00b7sam", "an", "dem", "Ge\u00b7sta\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach keine Seele, keine f\u00fcr dieses Herz?", "tokens": ["Ach", "kei\u00b7ne", "See\u00b7le", ",", "kei\u00b7ne", "f\u00fcr", "die\u00b7ses", "Herz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "$,", "PIAT", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr frohen Reigen? Aber weh dir,", "tokens": ["Ihr", "fro\u00b7hen", "Rei\u00b7gen", "?", "A\u00b7ber", "weh", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "ADV", "PPER", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sehnender J\u00fcngling! sie gehn vor\u00fcber!", "tokens": ["Seh\u00b7nen\u00b7der", "J\u00fcng\u00b7ling", "!", "sie", "gehn", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Zur\u00fcck denn in die Zelle, Verachteter!", "tokens": ["Zu\u00b7r\u00fcck", "denn", "in", "die", "Zel\u00b7le", ",", "Ver\u00b7ach\u00b7te\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur\u00fcck zur Kummerst\u00e4tte, wo schlaflos du", "tokens": ["Zu\u00b7r\u00fcck", "zur", "Kum\u00b7mer\u00b7st\u00e4t\u00b7te", ",", "wo", "schlaf\u00b7los", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "PWAV", "ADJD", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So manche Mittern\u00e4chte weintest,", "tokens": ["So", "man\u00b7che", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te", "wein\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weintest im Durste nach Lieb und Lorbeer.", "tokens": ["Wein\u00b7test", "im", "Durs\u00b7te", "nach", "Lieb", "und", "Lor\u00b7beer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Lebt wohl, ihr g\u00fcldnen Stunden vergangner Zeit,", "tokens": ["Lebt", "wohl", ",", "ihr", "g\u00fcld\u00b7nen", "Stun\u00b7den", "ver\u00b7gang\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr lieben Kindertr\u00e4ume von Gr\u00f6\u00df und Ruhm,", "tokens": ["Ihr", "lie\u00b7ben", "Kin\u00b7der\u00b7tr\u00e4u\u00b7me", "von", "Gr\u00f6\u00df", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lebt wohl, lebt wohl, ihr Spielgenossen,", "tokens": ["Lebt", "wohl", ",", "lebt", "wohl", ",", "ihr", "Spiel\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weint um den J\u00fcngling, er ist verachtet!", "tokens": ["Weint", "um", "den", "J\u00fcng\u00b7ling", ",", "er", "ist", "ver\u00b7ach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}