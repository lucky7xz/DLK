{"textgrid.poem.25559": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Brause nicht mit deinen Fl\u00fcchen l\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brause nicht mit deinen Fl\u00fcchen l\u00e4nger", "tokens": ["Brau\u00b7se", "nicht", "mit", "dei\u00b7nen", "Fl\u00fc\u00b7chen", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In des abgeh\u00e4rmten M\u00e4dchens Ohr;", "tokens": ["In", "des", "ab\u00b7ge\u00b7h\u00e4rm\u00b7ten", "M\u00e4d\u00b7chens", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oeffne du nicht selbst, geliebter S\u00e4nger,", "tokens": ["Oeff\u00b7ne", "du", "nicht", "selbst", ",", "ge\u00b7lieb\u00b7ter", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihr das Todesthor.", "tokens": ["Ihr", "das", "To\u00b7dest\u00b7hor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Denn vielleicht, nur eben durchgegangen,", "tokens": ["Denn", "viel\u00b7leicht", ",", "nur", "e\u00b7ben", "durch\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "W\u00fcrd' es schon vor deinen Blicken klar,", "tokens": ["W\u00fcrd'", "es", "schon", "vor", "dei\u00b7nen", "Bli\u00b7cken", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und du s\u00e4hst zu sp\u00e4te, statt der Schlangen,", "tokens": ["Und", "du", "s\u00e4hst", "zu", "sp\u00e4\u00b7te", ",", "statt", "der", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKZU", "VVFIN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Liljen um mein Haar.", "tokens": ["Lil\u00b7jen", "um", "mein", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Steh' und poche dann! Wird er dich h\u00f6ren?", "tokens": ["Steh'", "und", "po\u00b7che", "dann", "!", "Wird", "er", "dich", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADV", "$.", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Lieg' und bettle deine Kniee wund:", "tokens": ["Lieg'", "und", "bett\u00b7le", "dei\u00b7ne", "Kni\u00b7ee", "wund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Werd' ich darum jemals wiederkehren", "tokens": ["Werd'", "ich", "da\u00b7rum", "je\u00b7mals", "wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Aus des Todes Schlund?", "tokens": ["Aus", "des", "To\u00b7des", "Schlund", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.13": {"text": "Mit Gewalt \u2013 ich kenne deine Hitze! \u2013", "tokens": ["Mit", "Ge\u00b7walt", "\u2013", "ich", "ken\u00b7ne", "dei\u00b7ne", "Hit\u00b7ze", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Sprengst du, mich zu suchen, wohl das Thor;", "tokens": ["Sprengst", "du", ",", "mich", "zu", "su\u00b7chen", ",", "wohl", "das", "Thor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Aber ach! du dringst zu meinem Sitze", "tokens": ["A\u00b7ber", "ach", "!", "du", "dringst", "zu", "mei\u00b7nem", "Sit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Mit Gewalt nicht vor.", "tokens": ["Mit", "Ge\u00b7walt", "nicht", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "In dem Lande, wo man nur die Treue", "tokens": ["In", "dem", "Lan\u00b7de", ",", "wo", "man", "nur", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PIS", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Und den Frieden, ihren Bruder, kennt,", "tokens": ["Und", "den", "Frie\u00b7den", ",", "ih\u00b7ren", "Bru\u00b7der", ",", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Wei\u00df man nicht, was dieser Erdball Reue", "tokens": ["Wei\u00df", "man", "nicht", ",", "was", "die\u00b7ser", "Erd\u00b7ball", "Reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "PWS", "PDAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Oder Thr\u00e4nen nennt.", "tokens": ["O\u00b7der", "Thr\u00e4\u00b7nen", "nennt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Hier, nur hier, ist's m\u00f6glich, deinen Jammer", "tokens": ["Hier", ",", "nur", "hier", ",", "ist's", "m\u00f6g\u00b7lich", ",", "dei\u00b7nen", "Jam\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "$,", "VAFIN", "ADJD", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Umzuschaffen in der Liebe Ruh';", "tokens": ["Um\u00b7zu\u00b7schaf\u00b7fen", "in", "der", "Lie\u00b7be", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "F\u00fchre denn, o Liebe! meiner Kammer", "tokens": ["F\u00fch\u00b7re", "denn", ",", "o", "Lie\u00b7be", "!", "mei\u00b7ner", "Kam\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "FM", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Heut' ihn wieder zu!", "tokens": ["Heut'", "ihn", "wie\u00b7der", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}