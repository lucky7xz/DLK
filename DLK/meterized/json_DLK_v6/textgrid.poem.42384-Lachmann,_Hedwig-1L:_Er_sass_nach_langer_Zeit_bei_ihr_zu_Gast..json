{"textgrid.poem.42384": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er sass nach langer Zeit bei ihr zu Gast.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er sass nach langer Zeit bei ihr zu Gast.", "tokens": ["Er", "sass", "nach", "lan\u00b7ger", "Zeit", "bei", "ihr", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schweigsam. Die beiden waren ehmals Gatten.", "tokens": ["Schweig\u00b7sam", ".", "Die", "bei\u00b7den", "wa\u00b7ren", "eh\u00b7mals", "Gat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ART", "PIS", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus l\u00e4ngst Erlebtem schoben sich verblasst", "tokens": ["Aus", "l\u00e4ngst", "Er\u00b7leb\u00b7tem", "scho\u00b7ben", "sich", "ver\u00b7blasst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Her\u00fcber die zur\u00fcckgedr\u00e4ngten Schatten.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "die", "zu\u00b7r\u00fcck\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er sass und sann: Ich habe sie gekr\u00e4nkt,", "tokens": ["Er", "sass", "und", "sann", ":", "Ich", "ha\u00b7be", "sie", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als uns noch Eide aneinander banden.", "tokens": ["Als", "uns", "noch", "Ei\u00b7de", "an\u00b7ein\u00b7an\u00b7der", "ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sprach in sich hinein: Kaum noch gedenkt", "tokens": ["Sie", "sprach", "in", "sich", "hin\u00b7ein", ":", "Kaum", "noch", "ge\u00b7denkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "PTKVZ", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz der Qualen, die es ausgestanden.", "tokens": ["Mein", "Herz", "der", "Qua\u00b7len", ",", "die", "es", "aus\u00b7ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie liebte mich. Gewiss hat sie verschmerzt,", "tokens": ["Sie", "lieb\u00b7te", "mich", ".", "Ge\u00b7wiss", "hat", "sie", "ver\u00b7schmerzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dass neues Schicksal unsre Bahnen st\u00f6rte. \u2013", "tokens": ["Dass", "neu\u00b7es", "Schick\u00b7sal", "uns\u00b7re", "Bah\u00b7nen", "st\u00f6r\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "- Ich habe alles in mir ausgemerzt", "tokens": ["Ich", "ha\u00b7be", "al\u00b7les", "in", "mir", "aus\u00b7ge\u00b7merzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PIS", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis auf mein eignes Selbst, das ihm geh\u00f6rte. \u2013", "tokens": ["Bis", "auf", "mein", "eig\u00b7nes", "Selbst", ",", "das", "ihm", "ge\u00b7h\u00f6r\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch immer ist sie meinem Herzen wert.", "tokens": ["Noch", "im\u00b7mer", "ist", "sie", "mei\u00b7nem", "Her\u00b7zen", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zeitlebens dank ich ihrem Edelmute. \u2013", "tokens": ["Zeit\u00b7le\u00b7bens", "dank", "ich", "ih\u00b7rem", "E\u00b7del\u00b7mu\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "- An meines Lebens fernstem Punkt verj\u00e4hrt", "tokens": ["An", "mei\u00b7nes", "Le\u00b7bens", "ferns\u00b7tem", "Punkt", "ver\u00b7j\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch nimmer der Verrat an meinem Blute.", "tokens": ["Doch", "nim\u00b7mer", "der", "Ver\u00b7rat", "an", "mei\u00b7nem", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}