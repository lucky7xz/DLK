{"dta.poem.11177": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die vereinigung des alten und neuen  \n calenders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun wird das alte neu, die welt verj\u00fcngt sich immer,", "tokens": ["Nun", "wird", "das", "al\u00b7te", "neu", ",", "die", "welt", "ver\u00b7j\u00fcngt", "sich", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJD", "$,", "ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man liebet neue pracht und neue kl\u00fcgeley.", "tokens": ["Man", "lie\u00b7bet", "neu\u00b7e", "pracht", "und", "neu\u00b7e", "kl\u00fc\u00b7ge\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwey st\u00fccke werden nur nicht wieder bey uns neu,", "tokens": ["Zwey", "st\u00fc\u00b7cke", "wer\u00b7den", "nur", "nicht", "wie\u00b7der", "bey", "uns", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die alte teutsche treu, und altes frauenzimmer.", "tokens": ["Die", "al\u00b7te", "teut\u00b7sche", "treu", ",", "und", "al\u00b7tes", "frau\u00b7en\u00b7zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJD", "$,", 