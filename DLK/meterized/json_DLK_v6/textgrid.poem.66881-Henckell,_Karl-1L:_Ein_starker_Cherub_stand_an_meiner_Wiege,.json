{"textgrid.poem.66881": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein starker Cherub stand an meiner Wiege,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein starker Cherub stand an meiner Wiege,", "tokens": ["Ein", "star\u00b7ker", "Che\u00b7rub", "stand", "an", "mei\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sprach: \u00bbIch lege dir ein Schwert zur Hand.", "tokens": ["Der", "sprach", ":", "\u00bb", "Ich", "le\u00b7ge", "dir", "ein", "Schwert", "zur", "Hand", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es geht dein Weg durch hartes Menschenland.", "tokens": ["Es", "geht", "dein", "Weg", "durch", "har\u00b7tes", "Men\u00b7schen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Sinn ist weich. Dies Schwert tut not zum Siege.", "tokens": ["Dein", "Sinn", "ist", "weich", ".", "Dies", "Schwert", "tut", "not", "zum", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PDS", "VVFIN", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und einen Schild vor deine Brust ich biege,", "tokens": ["Und", "ei\u00b7nen", "Schild", "vor", "dei\u00b7ne", "Brust", "ich", "bie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Mitte kr\u00f6nt ein Sternendiamant,", "tokens": ["Des", "Mit\u00b7te", "kr\u00f6nt", "ein", "Ster\u00b7nen\u00b7di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Hort der Ewigkeit dort festgebannt,", "tokens": ["Als", "Hort", "der", "E\u00b7wig\u00b7keit", "dort", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df, f\u00e4llst du einst, dein Herz beschirmet liege.\u00ab", "tokens": ["Da\u00df", ",", "f\u00e4llst", "du", "einst", ",", "dein", "Herz", "be\u00b7schir\u00b7met", "lie\u00b7ge", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sank ich den Widersachern nicht zum Raube,", "tokens": ["Sank", "ich", "den", "Wi\u00b7der\u00b7sa\u00b7chern", "nicht", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich mit Waffen aller Art bedr\u00e4ut,", "tokens": ["Die", "mich", "mit", "Waf\u00b7fen", "al\u00b7ler", "Art", "be\u00b7dr\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir dank ich's, dessen Stahl sich stets erneut,", "tokens": ["Dir", "dank", "ich's", ",", "des\u00b7sen", "Stahl", "sich", "stets", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "$,", "PRELAT", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schwert meines Geistes, der auf sich beharrt", "tokens": ["Schwert", "mei\u00b7nes", "Geis\u00b7tes", ",", "der", "auf", "sich", "be\u00b7harrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Im wilden Wirbelsturm der Gegenwart \u2013", "tokens": ["Im", "wil\u00b7den", "Wir\u00b7bels\u00b7turm", "der", "Ge\u00b7gen\u00b7wart", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Schild bist du, allm\u00e4chtiger Liebesglaube!", "tokens": ["Mein", "Schild", "bist", "du", ",", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Lie\u00b7bes\u00b7glau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}