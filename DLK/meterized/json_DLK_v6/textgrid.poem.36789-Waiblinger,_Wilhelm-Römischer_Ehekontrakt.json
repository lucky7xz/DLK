{"textgrid.poem.36789": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "R\u00f6mischer Ehekontrakt", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cazzo! ihr nehmet ein Weib, und k\u00f6nnt euch selbst nicht ern\u00e4hren? \u2013", "tokens": ["Caz\u00b7zo", "!", "ihr", "neh\u00b7met", "ein", "Weib", ",", "und", "k\u00f6nnt", "euch", "selbst", "nicht", "er\u00b7n\u00e4h\u00b7ren", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$.", "$("], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "\u00bbdas ist's eben, mein Freund, darum ern\u00e4hret sie mich.\u00ab", "tokens": ["\u00bb", "das", "ist's", "e\u00b7ben", ",", "mein", "Freund", ",", "da\u00b7rum", "er\u00b7n\u00e4h\u00b7ret", "sie", "mich", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PAV", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}