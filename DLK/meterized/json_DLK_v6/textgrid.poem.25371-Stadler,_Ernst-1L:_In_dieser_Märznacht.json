{"textgrid.poem.25371": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dieser M\u00e4rznacht", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser M\u00e4rznacht", "tokens": ["In", "die\u00b7ser", "M\u00e4rz\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.2": {"text": "trat ich sp\u00e4t aus meinem Haus.", "tokens": ["trat", "ich", "sp\u00e4t", "aus", "mei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Stra\u00dfen waren aufgew\u00fchlt von Lenzgeruch", "tokens": ["Die", "Stra\u00b7\u00dfen", "wa\u00b7ren", "auf\u00b7ge\u00b7w\u00fchlt", "von", "Lenz\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und gr\u00fcnem Saatregen.", "tokens": ["und", "gr\u00fc\u00b7nem", "Saat\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Winde schlugen an. Durch die verst\u00f6rte H\u00e4usersenkung", "tokens": ["Win\u00b7de", "schlu\u00b7gen", "an", ".", "Durch", "die", "ver\u00b7st\u00f6r\u00b7te", "H\u00e4u\u00b7ser\u00b7sen\u00b7kung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "gieng ich weit hinaus", "tokens": ["gieng", "ich", "weit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bis zu dem unbedeckten Wall und sp\u00fcrte:", "tokens": ["Bis", "zu", "dem", "un\u00b7be\u00b7deck\u00b7ten", "Wall", "und", "sp\u00fcr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "meinem Herzen schwoll ein neuer Takt entgegen.", "tokens": ["mei\u00b7nem", "Her\u00b7zen", "schwoll", "ein", "neu\u00b7er", "Takt", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "In jedem Lufthauch", "tokens": ["In", "je\u00b7dem", "Luft\u00b7hauch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "war ein junges Werden ausgespannt.", "tokens": ["war", "ein", "jun\u00b7ges", "Wer\u00b7den", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich lauschte,", "tokens": ["Ich", "lauschte", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "wie die starken Wirbel mir im Blute rollten.", "tokens": ["wie", "die", "star\u00b7ken", "Wir\u00b7bel", "mir", "im", "Blu\u00b7te", "roll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Schon dehnte sich bereitet Acker.", "tokens": ["Schon", "dehn\u00b7te", "sich", "be\u00b7rei\u00b7tet", "A\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In den Horizonten eingebrannt", "tokens": ["In", "den", "Ho\u00b7ri\u00b7zon\u00b7ten", "ein\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "War schon die Bl\u00e4ue hoher Morgenstunden,", "tokens": ["War", "schon", "die", "Bl\u00e4u\u00b7e", "ho\u00b7her", "Mor\u00b7gen\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die ins Weite f\u00fchren sollten.", "tokens": ["die", "ins", "Wei\u00b7te", "f\u00fch\u00b7ren", "soll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Schleusen knirschten.", "tokens": ["Die", "Schleu\u00b7sen", "knirschten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Abenteuer brach aus allen Fernen.", "tokens": ["A\u00b7bent\u00b7eu\u00b7er", "brach", "aus", "al\u00b7len", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberm Kanal, den junge Ausfahrtwinde wellten,", "tokens": ["\u00dc\u00b7berm", "Ka\u00b7nal", ",", "den", "jun\u00b7ge", "Aus\u00b7fahrt\u00b7win\u00b7de", "well\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "wuchsen helle Bahnen,", "tokens": ["wuch\u00b7sen", "hel\u00b7le", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "In deren Licht ich trieb.", "tokens": ["In", "de\u00b7ren", "Licht", "ich", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schicksal stand wartend in umwehten Sternen.", "tokens": ["Schick\u00b7sal", "stand", "war\u00b7tend", "in", "um\u00b7weh\u00b7ten", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In meinem Herzen lag ein St\u00fcrmen", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "lag", "ein", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "wie von aufgerollten Fahnen.", "tokens": ["wie", "von", "auf\u00b7ge\u00b7roll\u00b7ten", "Fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}