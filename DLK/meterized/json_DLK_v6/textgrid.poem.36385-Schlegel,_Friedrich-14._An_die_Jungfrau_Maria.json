{"textgrid.poem.36385": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14. An die Jungfrau Maria", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, edle K\u00f6nigin, wir ehren,", "tokens": ["Dich", ",", "ed\u00b7le", "K\u00f6\u00b7ni\u00b7gin", ",", "wir", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frau vom Himmel, dein Lob wir mehren;", "tokens": ["Frau", "vom", "Him\u00b7mel", ",", "dein", "Lob", "wir", "meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dich loben und ehren rechte,", "tokens": ["Dich", "lo\u00b7ben", "und", "eh\u00b7ren", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aller Kreaturen Geschlechte.", "tokens": ["Al\u00b7ler", "Kre\u00b7a\u00b7tu\u00b7ren", "Ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mutter der ew'gen Majestat,", "tokens": ["Mut\u00b7ter", "der", "ew'\u00b7gen", "Ma\u00b7jes\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gott selbst dich auserkoren hat.", "tokens": ["Gott", "selbst", "dich", "au\u00b7ser\u00b7ko\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich umgibt der Sonnenschein,", "tokens": ["Dich", "um\u00b7gibt", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Mond ist unter den F\u00fc\u00dfen dein,", "tokens": ["Der", "Mond", "ist", "un\u00b7ter", "den", "F\u00fc\u00b7\u00dfen", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Auf deinem Haupt ist schone", "tokens": ["Auf", "dei\u00b7nem", "Haupt", "ist", "scho\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von zw\u00f6lf Sternen die Krone.", "tokens": ["Von", "zw\u00f6lf", "Ster\u00b7nen", "die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Des ew'gen Vaters Tochter du bist,", "tokens": ["Des", "ew'\u00b7gen", "Va\u00b7ters", "Toch\u00b7ter", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Und Mutter des Sohnes Jesu Christ;", "tokens": ["Und", "Mut\u00b7ter", "des", "Soh\u00b7nes", "Je\u00b7su", "Christ", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "NE", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Des heiligen Geistes keusche Braut,", "tokens": ["Des", "hei\u00b7li\u00b7gen", "Geis\u00b7tes", "keu\u00b7sche", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dich loben die Engeln \u00fcberlaut.", "tokens": ["Dich", "lo\u00b7ben", "die", "En\u00b7geln", "\u00fc\u00b7berl\u00b7aut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die Cherubim dich umringen,", "tokens": ["Die", "Che\u00b7ru\u00b7bim", "dich", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Seraphim dir s\u00fc\u00dfiglich singen:", "tokens": ["Se\u00b7ra\u00b7phim", "dir", "s\u00fc\u00b7\u00dfig\u00b7lich", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVINF", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.17": {"text": "Du erfreuest Himmel und Erde", "tokens": ["Du", "er\u00b7freu\u00b7est", "Him\u00b7mel", "und", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Ave Maria, K\u00f6nigin werte!", "tokens": ["A\u00b7ve", "Ma\u00b7ria", ",", "K\u00f6\u00b7ni\u00b7gin", "wer\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}