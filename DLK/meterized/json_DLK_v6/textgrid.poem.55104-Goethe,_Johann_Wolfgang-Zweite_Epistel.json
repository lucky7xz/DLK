{"textgrid.poem.55104": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Zweite Epistel", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcrdiger Freund, du runzelst die Stirn; dir scheinen die Scherze", "tokens": ["W\u00fcr\u00b7di\u00b7ger", "Freund", ",", "du", "run\u00b7zelst", "die", "Stirn", ";", "dir", "schei\u00b7nen", "die", "Scher\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht am rechten Orte zu sein; die Frage war ernsthaft,", "tokens": ["Nicht", "am", "rech\u00b7ten", "Or\u00b7te", "zu", "sein", ";", "die", "Fra\u00b7ge", "war", "ernst\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "PTKZU", "VAINF", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+-+--+-+-++-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und besonnen verlangst du die Antwort; da wei\u00df ich, beim Himmel!", "tokens": ["Und", "be\u00b7son\u00b7nen", "ver\u00b7langst", "du", "die", "Ant\u00b7wort", ";", "da", "wei\u00df", "ich", ",", "beim", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Nicht, wie eben sich mir der Schalk im Busen bewegte.", "tokens": ["Nicht", ",", "wie", "e\u00b7ben", "sich", "mir", "der", "Schalk", "im", "Bu\u00b7sen", "be\u00b7weg\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ADV", "PRF", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Doch ich fahre bed\u00e4chtiger fort. Du sagst mir: \u00bbSo m\u00f6chte", "tokens": ["Doch", "ich", "fah\u00b7re", "be\u00b7d\u00e4ch\u00b7ti\u00b7ger", "fort", ".", "Du", "sagst", "mir", ":", "\u00bb", "So", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$.", "$(", "ADV", "VMFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Meinetwegen die Menge sich halten im Leben und Lesen,", "tokens": ["Mei\u00b7net\u00b7we\u00b7gen", "die", "Men\u00b7ge", "sich", "hal\u00b7ten", "im", "Le\u00b7ben", "und", "Le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Wie sie k\u00f6nnte; doch denke dir nur die T\u00f6chter im Hause,", "tokens": ["Wie", "sie", "k\u00f6nn\u00b7te", ";", "doch", "den\u00b7ke", "dir", "nur", "die", "T\u00f6ch\u00b7ter", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Die mir der kuppelnde Dichter mit allem B\u00f6sen bekannt macht.\u00ab", "tokens": ["Die", "mir", "der", "kup\u00b7peln\u00b7de", "Dich\u00b7ter", "mit", "al\u00b7lem", "B\u00f6\u00b7sen", "be\u00b7kannt", "macht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "PIS", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem ist leichter geholfen, versetz ich, als wohl ein andrer", "tokens": ["Dem", "ist", "leich\u00b7ter", "ge\u00b7hol\u00b7fen", ",", "ver\u00b7setz", "ich", ",", "als", "wohl", "ein", "an\u00b7drer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "VVPP", "$,", "VVIMP", "PPER", "$,", "KOUS", "ADV", "ART", "ADJA"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Denken m\u00f6chte. Die M\u00e4dchen sind gut und machen sich gerne", "tokens": ["Den\u00b7ken", "m\u00f6ch\u00b7te", ".", "Die", "M\u00e4d\u00b7chen", "sind", "gut", "und", "ma\u00b7chen", "sich", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "PRF", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Was zu schaffen. Da gib nur dem einen die Schl\u00fcssel zum Keller", "tokens": ["Was", "zu", "schaf\u00b7fen", ".", "Da", "gib", "nur", "dem", "ei\u00b7nen", "die", "Schl\u00fcs\u00b7sel", "zum", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKZU", "VVINF", "$.", "ADV", "VVIMP", "ADV", "ART", "ART", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df es die Weine des Vaters besorge, sobald sie, vom Winzer", "tokens": ["Da\u00df", "es", "die", "Wei\u00b7ne", "des", "Va\u00b7ters", "be\u00b7sor\u00b7ge", ",", "so\u00b7bald", "sie", ",", "vom", "Win\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "$,", "APPRART", "NN"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Oder vom Kaufmann geliefert, die weiten Gew\u00f6lbe bereichern.", "tokens": ["O\u00b7der", "vom", "Kauf\u00b7mann", "ge\u00b7lie\u00b7fert", ",", "die", "wei\u00b7ten", "Ge\u00b7w\u00f6l\u00b7be", "be\u00b7rei\u00b7chern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Manches zu schaffen hat ein M\u00e4dchen, die vielen Gef\u00e4\u00dfe,", "tokens": ["Man\u00b7ches", "zu", "schaf\u00b7fen", "hat", "ein", "M\u00e4d\u00b7chen", ",", "die", "vie\u00b7len", "Ge\u00b7f\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Leere F\u00e4sser und Flaschen in reinlicher Ordnung zu halten.", "tokens": ["Lee\u00b7re", "F\u00e4s\u00b7ser", "und", "Fla\u00b7schen", "in", "rein\u00b7li\u00b7cher", "Ord\u00b7nung", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Dann betrachtet sie oft des sch\u00e4umenden Mostes Bewegung,", "tokens": ["Dann", "be\u00b7trach\u00b7tet", "sie", "oft", "des", "sch\u00e4u\u00b7men\u00b7den", "Mos\u00b7tes", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Gie\u00dft das Fehlende zu, damit die wallenden Blasen", "tokens": ["Gie\u00dft", "das", "Feh\u00b7len\u00b7de", "zu", ",", "da\u00b7mit", "die", "wal\u00b7len\u00b7den", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Leicht die \u00d6ffnung des Fasses erreichen, trinkbar und helle", "tokens": ["Leicht", "die", "\u00d6ff\u00b7nung", "des", "Fas\u00b7ses", "er\u00b7rei\u00b7chen", ",", "trink\u00b7bar", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,", "ADJD", "KON", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Endlich der edelste Saft sich k\u00fcnftigen Jahren vollende.", "tokens": ["End\u00b7lich", "der", "e\u00b7dels\u00b7te", "Saft", "sich", "k\u00fcnf\u00b7ti\u00b7gen", "Jah\u00b7ren", "voll\u00b7en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PRF", "ADJA", "NN", "ADJA", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Unerm\u00fcdet ist sie alsdann, zu f\u00fcllen, zu sch\u00f6pfen,", "tokens": ["Un\u00b7er\u00b7m\u00fc\u00b7det", "ist", "sie", "als\u00b7dann", ",", "zu", "f\u00fcl\u00b7len", ",", "zu", "sch\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Da\u00df stets geistig der Trank und rein die Tafel belebe.", "tokens": ["Da\u00df", "stets", "geis\u00b7tig", "der", "Trank", "und", "rein", "die", "Ta\u00b7fel", "be\u00b7le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "La\u00df der andern die K\u00fcche zum Reich; da gibt es, wahrhaftig!", "tokens": ["La\u00df", "der", "an\u00b7dern", "die", "K\u00fc\u00b7che", "zum", "Reich", ";", "da", "gibt", "es", ",", "wahr\u00b7haf\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "ART", "NN", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "ADJD", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Arbeit genug, das t\u00e4gliche Mahl durch Sommer und Winter", "tokens": ["Ar\u00b7beit", "ge\u00b7nug", ",", "das", "t\u00e4g\u00b7li\u00b7che", "Mahl", "durch", "Som\u00b7mer", "und", "Win\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Schmackhaft stets zu bereiten und ohne Beschwerde des Beutels.", "tokens": ["Schmack\u00b7haft", "stets", "zu", "be\u00b7rei\u00b7ten", "und", "oh\u00b7ne", "Be\u00b7schwer\u00b7de", "des", "Beu\u00b7tels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "KON", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Denn im Fr\u00fchjahr sorget sie schon, im Hofe die K\u00fcchlein", "tokens": ["Denn", "im", "Fr\u00fch\u00b7jahr", "sor\u00b7get", "sie", "schon", ",", "im", "Ho\u00b7fe", "die", "K\u00fcch\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ART", "NN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Bald zu erziehen und bald die schnatternden Enten zu f\u00fcttern.", "tokens": ["Bald", "zu", "er\u00b7zie\u00b7hen", "und", "bald", "die", "schnat\u00b7tern\u00b7den", "En\u00b7ten", "zu", "f\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Alles, was ihr die Jahrszeit gibt, das bringt sie beizeiten", "tokens": ["Al\u00b7les", ",", "was", "ihr", "die", "Jahrs\u00b7zeit", "gibt", ",", "das", "bringt", "sie", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "VVINF"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Dir auf den Tisch und wei\u00df mit jeglichem Tage die Speisen", "tokens": ["Dir", "auf", "den", "Tisch", "und", "wei\u00df", "mit", "jeg\u00b7li\u00b7chem", "Ta\u00b7ge", "die", "Spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Klug zu wechseln, und reift nur eben der Sommer die Fr\u00fcchte,", "tokens": ["Klug", "zu", "wech\u00b7seln", ",", "und", "reift", "nur", "e\u00b7ben", "der", "Som\u00b7mer", "die", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Denkt sie an Vorrat schon f\u00fcr den Winter. Im k\u00fchlen", "tokens": ["Denkt", "sie", "an", "Vor\u00b7rat", "schon", "f\u00fcr", "den", "Win\u00b7ter", ".", "Im", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "APPR", "ART", "NN", "$.", "APPRART", "ADJA"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Gew\u00f6lbe G\u00e4rt ihr der kr\u00e4ftige Kohl und reifen in Essig die Gurken;", "tokens": ["Ge\u00b7w\u00f6l\u00b7be", "G\u00e4rt", "ihr", "der", "kr\u00e4f\u00b7ti\u00b7ge", "Kohl", "und", "rei\u00b7fen", "in", "Es\u00b7sig", "die", "Gur\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+--+--+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Aber die luftige Kammer bewahrt ihr die Gaben Pomonens.", "tokens": ["A\u00b7ber", "die", "luf\u00b7ti\u00b7ge", "Kam\u00b7mer", "be\u00b7wahrt", "ihr", "die", "Ga\u00b7ben", "Po\u00b7mo\u00b7nens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Gerne nimmt sie das Lob vom Vater und allen Geschwistern,", "tokens": ["Ger\u00b7ne", "nimmt", "sie", "das", "Lob", "vom", "Va\u00b7ter", "und", "al\u00b7len", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Und mi\u00dflingt ihr etwas, dann ist's ein gr\u00f6\u00dferes Ungl\u00fcck,", "tokens": ["Und", "mi\u00df\u00b7lingt", "ihr", "et\u00b7was", ",", "dann", "ist's", "ein", "gr\u00f6\u00b7\u00dfe\u00b7res", "Un\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Als wenn dir ein Schuldner entl\u00e4uft und den Wechsel zur\u00fcckl\u00e4\u00dft.", "tokens": ["Als", "wenn", "dir", "ein", "Schuld\u00b7ner", "ent\u00b7l\u00e4uft", "und", "den", "Wech\u00b7sel", "zu\u00b7r\u00fcck\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVPP", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.15": {"text": "Immer ist so das M\u00e4dchen besch\u00e4ftigt und reifet im stillen", "tokens": ["Im\u00b7mer", "ist", "so", "das", "M\u00e4d\u00b7chen", "be\u00b7sch\u00e4f\u00b7tigt", "und", "rei\u00b7fet", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "KON", "VVFIN", "APPRART", "ADJA"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "H\u00e4uslicher Tugend entgegen, den klugen Mann zu begl\u00fccken.", "tokens": ["H\u00e4us\u00b7li\u00b7cher", "Tu\u00b7gend", "ent\u00b7ge\u00b7gen", ",", "den", "klu\u00b7gen", "Mann", "zu", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "W\u00fcnscht sie dann endlich zu lesen, so w\u00e4hlt sie gewi\u00dflich ein Kochbuch,", "tokens": ["W\u00fcnscht", "sie", "dann", "end\u00b7lich", "zu", "le\u00b7sen", ",", "so", "w\u00e4hlt", "sie", "ge\u00b7wi\u00df\u00b7lich", "ein", "Koch\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Deren Hunderte schon die eifrigen Pressen uns gaben.", "tokens": ["De\u00b7ren", "Hun\u00b7der\u00b7te", "schon", "die", "eif\u00b7ri\u00b7gen", "Pres\u00b7sen", "uns", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "Eine Schwester besorget den Garten, der schwerlich zur Wildnis,", "tokens": ["Ei\u00b7ne", "Schwes\u00b7ter", "be\u00b7sor\u00b7get", "den", "Gar\u00b7ten", ",", "der", "schwer\u00b7lich", "zur", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Deine Wohnung romantisch und feucht zu umgeben, verdammt ist,", "tokens": ["Dei\u00b7ne", "Woh\u00b7nung", "ro\u00b7man\u00b7tisch", "und", "feucht", "zu", "um\u00b7ge\u00b7ben", ",", "ver\u00b7dammt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,", "VVPP", "VAFIN", "$,"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sondern in zierliche Beete geteilt, als Vorhof der K\u00fcche,", "tokens": ["Son\u00b7dern", "in", "zier\u00b7li\u00b7che", "Bee\u00b7te", "ge\u00b7teilt", ",", "als", "Vor\u00b7hof", "der", "K\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "N\u00fctzliche Kr\u00e4uter ern\u00e4hrt und jugendbegl\u00fcckende Fr\u00fcchte.", "tokens": ["N\u00fctz\u00b7li\u00b7che", "Kr\u00e4u\u00b7ter", "er\u00b7n\u00e4hrt", "und", "ju\u00b7gend\u00b7be\u00b7gl\u00fc\u00b7cken\u00b7de", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Patriarchalisch erzeuge so selbst dir ein kleines, gedr\u00e4ngtes", "tokens": ["Pat\u00b7ri\u00b7ar\u00b7cha\u00b7lisch", "er\u00b7zeu\u00b7ge", "so", "selbst", "dir", "ein", "klei\u00b7nes", ",", "ge\u00b7dr\u00e4ng\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ADV", "ADV", "PPER", "ART", "ADJA", "$,", "ADJA"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "K\u00f6nigreich, und bev\u00f6lkere dein Haus mit treuem Gesinde.", "tokens": ["K\u00f6\u00b7nig\u00b7reich", ",", "und", "be\u00b7v\u00f6l\u00b7ke\u00b7re", "dein", "Haus", "mit", "treu\u00b7em", "Ge\u00b7sin\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Hast du der T\u00f6chter noch mehr, die lieber sitzen und stille", "tokens": ["Hast", "du", "der", "T\u00f6ch\u00b7ter", "noch", "mehr", ",", "die", "lie\u00b7ber", "sit\u00b7zen", "und", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,", "PRELS", "ADV", "VVINF", "KON", "ADJA"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Weibliche Arbeit verrichten, da ist's noch besser; die Nadel", "tokens": ["Weib\u00b7li\u00b7che", "Ar\u00b7beit", "ver\u00b7rich\u00b7ten", ",", "da", "ist's", "noch", "bes\u00b7ser", ";", "die", "Na\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "KOUS", "NE", "ADV", "ADJD", "$.", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Ruht im Jahre nicht leicht: denn noch so h\u00e4uslich im Hause,", "tokens": ["Ruht", "im", "Jah\u00b7re", "nicht", "leicht", ":", "denn", "noch", "so", "h\u00e4us\u00b7lich", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKNEG", "ADJD", "$.", "KON", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "M\u00f6gen sie \u00f6ffentlich gern als m\u00fc\u00dfige Damen erscheinen.", "tokens": ["M\u00f6\u00b7gen", "sie", "\u00f6f\u00b7fent\u00b7lich", "gern", "als", "m\u00fc\u00b7\u00dfi\u00b7ge", "Da\u00b7men", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "+--++-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Wie sich das N\u00e4hen und Flicken vermehrt, das Waschen und Biegen,", "tokens": ["Wie", "sich", "das", "N\u00e4\u00b7hen", "und", "Fli\u00b7cken", "ver\u00b7mehrt", ",", "das", "Wa\u00b7schen", "und", "Bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "KON", "NN", "ADJD", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Hundertf\u00e4ltig, seitdem in wei\u00dfer, arkadischer H\u00fclle", "tokens": ["Hun\u00b7dert\u00b7f\u00e4l\u00b7tig", ",", "seit\u00b7dem", "in", "wei\u00b7\u00dfer", ",", "ar\u00b7ka\u00b7di\u00b7scher", "H\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Sich das M\u00e4dchen gef\u00e4llt, mit langen R\u00f6cken und Schleppen", "tokens": ["Sich", "das", "M\u00e4d\u00b7chen", "ge\u00b7f\u00e4llt", ",", "mit", "lan\u00b7gen", "R\u00f6\u00b7cken", "und", "Schlep\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Gassen kehret und G\u00e4rten, und Staub erreget im Tanzsaal.", "tokens": ["Gas\u00b7sen", "keh\u00b7ret", "und", "G\u00e4r\u00b7ten", ",", "und", "Staub", "er\u00b7re\u00b7get", "im", "Tanz\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$,", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Wahrlich! w\u00e4ren mir nur der M\u00e4dchen ein Dutzend im Hause,", "tokens": ["Wahr\u00b7lich", "!", "w\u00e4\u00b7ren", "mir", "nur", "der", "M\u00e4d\u00b7chen", "ein", "Dut\u00b7zend", "im", "Hau\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Niemals w\u00e4r ich verlegen um Arbeit, sie machen sich Arbeit", "tokens": ["Nie\u00b7mals", "w\u00e4r", "ich", "ver\u00b7le\u00b7gen", "um", "Ar\u00b7beit", ",", "sie", "ma\u00b7chen", "sich", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "APPR", "NN", "$,", "PPER", "VVFIN", "PRF", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Selber genug, es sollte kein Buch im Laufe des Jahres", "tokens": ["Sel\u00b7ber", "ge\u00b7nug", ",", "es", "soll\u00b7te", "kein", "Buch", "im", "Lau\u00b7fe", "des", "Jah\u00b7res"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PPER", "VMFIN", "PIAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "\u00dcber die Schwelle mir kommen, vom B\u00fccherverleiher gesendet.", "tokens": ["\u00dc\u00b7ber", "die", "Schwel\u00b7le", "mir", "kom\u00b7men", ",", "vom", "B\u00fc\u00b7cher\u00b7ver\u00b7lei\u00b7her", "ge\u00b7sen\u0