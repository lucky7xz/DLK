{"textgrid.poem.67266": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum dritten Mal tret' ich, hochw\u00fcrd'ge Frau,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum dritten Mal tret' ich, hochw\u00fcrd'ge Frau,", "tokens": ["Zum", "drit\u00b7ten", "Mal", "tret'", "ich", ",", "hoch\u00b7w\u00fcrd'\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In diesen heil'gen Hallen vor Dich hin,", "tokens": ["In", "die\u00b7sen", "heil'\u00b7gen", "Hal\u00b7len", "vor", "Dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchdrungen tief vom Ernste des Berufs,", "tokens": ["Durch\u00b7drun\u00b7gen", "tief", "vom", "Erns\u00b7te", "des", "Be\u00b7rufs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu dem die Freundschaft mich erkohr, am Fest", "tokens": ["Zu", "dem", "die", "Freund\u00b7schaft", "mich", "er\u00b7kohr", ",", "am", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Weihe Dir und diesem edlen Kreis',", "tokens": ["Der", "Wei\u00b7he", "Dir", "und", "die\u00b7sem", "ed\u00b7len", "Kreis'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem sich des Mannes Fu\u00df mit Ehrfurcht naht,", "tokens": ["Dem", "sich", "des", "Man\u00b7nes", "Fu\u00df", "mit", "Ehr\u00b7furcht", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Kleinod elterlichen Hauses, nun", "tokens": ["Ein", "Klei\u00b7nod", "el\u00b7ter\u00b7li\u00b7chen", "Hau\u00b7ses", ",", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Glied des hohen Ordens darzustelln,", "tokens": ["Ein", "Glied", "des", "ho\u00b7hen", "Or\u00b7dens", "dar\u00b7zu\u00b7stelln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dem Oberhaupt Du wurdest, Mutter bist.", "tokens": ["Dem", "O\u00b7ber\u00b7haupt", "Du", "wur\u00b7dest", ",", "Mut\u00b7ter", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "$,", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwiefach ist meines Amtes Ehre heut.", "tokens": ["Zwie\u00b7fach", "ist", "mei\u00b7nes", "Am\u00b7tes", "Eh\u00b7re", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur Seite stehn zwo edle Jungfraun mir,", "tokens": ["Zur", "Sei\u00b7te", "stehn", "zwo", "ed\u00b7le", "Jung\u00b7fraun", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "CARD", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Eine, schaut, ein Spr\u00f6\u00dfling jenes Stamms \u2013", "tokens": ["Die", "Ei\u00b7ne", ",", "schaut", ",", "ein", "Spr\u00f6\u00df\u00b7ling", "je\u00b7nes", "Stamms", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Eich' in Odins Hainen! \u2013 jenes Stamms,", "tokens": ["Die", "Eich'", "in", "O\u00b7dins", "Hai\u00b7nen", "!", "\u2013", "je\u00b7nes", "Stamms", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$.", "$(", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bew\u00e4hrt seit grauer Vorzeit in des Arms", "tokens": ["Be\u00b7w\u00e4hrt", "seit", "grau\u00b7er", "Vor\u00b7zeit", "in", "des", "Arms"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie in des Hauptes Thaten, minder nicht", "tokens": ["Wie", "in", "des", "Haup\u00b7tes", "Tha\u00b7ten", ",", "min\u00b7der", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In zarten Spindel-Tugenden; durch Zucht", "tokens": ["In", "zar\u00b7ten", "Spin\u00b7del\u00b7Tu\u00b7gen\u00b7den", ";", "durch", "Zucht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und fromme Sitte wohlbekannt auch hier", "tokens": ["Und", "from\u00b7me", "Sit\u00b7te", "wohl\u00b7be\u00b7kannt", "auch", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Im Heiligthum der Zellen. Nicht dem Feind", "tokens": ["Im", "Hei\u00b7lig\u00b7thum", "der", "Zel\u00b7len", ".", "Nicht", "dem", "Feind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Allein trotzt seines Ritterschildes Mau'r,", "tokens": ["Al\u00b7lein", "trotzt", "sei\u00b7nes", "Rit\u00b7ter\u00b7schil\u00b7des", "Mau'r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auch Bollwerk ist sie, d\u00e4mmt, wie's fester Muth,", "tokens": ["Auch", "Boll\u00b7werk", "ist", "sie", ",", "d\u00e4mmt", ",", "wie's", "fes\u00b7ter", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "$,", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie's Eifer nur, wie's Weisheit nur vermag,", "tokens": ["Wie's", "Ei\u00b7fer", "nur", ",", "wie's", "Weis\u00b7heit", "nur", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Den Strom entarteter, entnervter Zeit,", "tokens": ["Den", "Strom", "ent\u00b7ar\u00b7te\u00b7ter", ",", "ent\u00b7nerv\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die keiner Segensflur verheerend schont!", "tokens": ["Die", "kei\u00b7ner", "Se\u00b7gens\u00b7flur", "ver\u00b7hee\u00b7rend", "schont", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Gedankt sei's Gott, noch lodert unerl\u00f6scht,", "tokens": ["Ge\u00b7dankt", "sei's", "Gott", ",", "noch", "lo\u00b7dert", "un\u00b7er\u00b7l\u00f6scht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verborgen vor der Menge Sp\u00f6tteraug',", "tokens": ["Ver\u00b7bor\u00b7gen", "vor", "der", "Men\u00b7ge", "Sp\u00f6t\u00b7ter\u00b7aug'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "In mancher H\u00fctt', in manchem Goldpallast", "tokens": ["In", "man\u00b7cher", "H\u00fctt'", ",", "in", "man\u00b7chem", "Gold\u00b7pal\u00b7last"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein Fl\u00e4mmchen, hell und rein wie Silberblick,", "tokens": ["Ein", "Fl\u00e4mm\u00b7chen", ",", "hell", "und", "rein", "wie", "Sil\u00b7berb\u00b7lick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Auf Vesta's keuschem Heerd, der Jungfrau Brust,", "tokens": ["Auf", "Ve\u00b7sta's", "keu\u00b7schem", "Heerd", ",", "der", "Jung\u00b7frau", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sie selbst Altar, sie selbst auch Priesterinn", "tokens": ["Sie", "selbst", "Al\u00b7tar", ",", "sie", "selbst", "auch", "Pries\u00b7te\u00b7rinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "$,", "PPER", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Des heil'gen Feu'rs, das Andacht schirmt und n\u00e4hrt.\u2013", "tokens": ["Des", "heil'\u00b7gen", "Feu'rs", ",", "das", "An\u00b7dacht", "schirmt", "und", "n\u00e4hrt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hochw\u00fcrdige! Dein ahnend Auge sucht,", "tokens": ["Hoch\u00b7w\u00fcr\u00b7di\u00b7ge", "!", "Dein", "ah\u00b7nend", "Au\u00b7ge", "sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur Jungfrau dort sich wendend, ihren Blick,", "tokens": ["Zur", "Jung\u00b7frau", "dort", "sich", "wen\u00b7dend", ",", "ih\u00b7ren", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der sittsam niederschauend, so noch mehr", "tokens": ["Der", "sitt\u00b7sam", "nie\u00b7der\u00b7schau\u00b7end", ",", "so", "noch", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "$,", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verhei\u00dft. Gepriesen sei der Tag, da Sie", "tokens": ["Ver\u00b7hei\u00dft", ".", "Ge\u00b7prie\u00b7sen", "sei", "der", "Tag", ",", "da", "Sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "VVPP", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Unsern Eine ward!", "tokens": ["Der", "Un\u00b7sern", "Ei\u00b7ne", "ward", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht weit vom Baum, vom Baum Hesperia's,", "tokens": ["Nicht", "weit", "vom", "Baum", ",", "vom", "Baum", "Hes\u00b7pe\u00b7ria's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPRART", "NN", "$,", "APPRART", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Der, wie Ihr seht, die reife goldne Frucht", "tokens": ["Der", ",", "wie", "Ihr", "seht", ",", "die", "rei\u00b7fe", "gold\u00b7ne", "Frucht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit sanft bescheidner Liljenbl\u00fcthe paart,", "tokens": ["Mit", "sanft", "be\u00b7scheid\u00b7ner", "Lil\u00b7jen\u00b7bl\u00fct\u00b7he", "paart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie sich zum Kranz die Grazie w\u00e4hlt, wenn sie", "tokens": ["Wie", "sich", "zum", "Kranz", "die", "Gra\u00b7zie", "w\u00e4hlt", ",", "wenn", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Urania's Hymnus singt im Musen-Chor.", "tokens": ["U\u00b7ra\u00b7nia's", "Hym\u00b7nus", "singt", "im", "Mu\u00b7sen\u00b7Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Wie sollt' es meiner Anempfehlung wohl", "tokens": ["Wie", "sollt'", "es", "mei\u00b7ner", "An\u00b7emp\u00b7feh\u00b7lung", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bed\u00fcrfen? Doch auf meiner Zunge liegt", "tokens": ["Be\u00b7d\u00fcr\u00b7fen", "?", "Doch", "auf", "mei\u00b7ner", "Zun\u00b7ge", "liegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der M\u00fctter Wort, sie nah'n sich beide Dir", "tokens": ["Der", "M\u00fct\u00b7ter", "Wort", ",", "sie", "nah'n", "sich", "bei\u00b7de", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "PRF", "PIS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Bitten, und die Bitten, wie Du wei\u00dft,", "tokens": ["Mit", "Bit\u00b7ten", ",", "und", "die", "Bit\u00b7ten", ",", "wie", "Du", "wei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sind Gottes T\u00f6chter. Nimm, so flehn sie, nimm", "tokens": ["Sind", "Got\u00b7tes", "T\u00f6ch\u00b7ter", ".", "Nimm", ",", "so", "flehn", "sie", ",", "nimm"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "NN", "NN", "$.", "NE", "$,", "ADV", "VVFIN", "PPER", "$,", "VVIMP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit milder Huld, als treue Z\u00f6glinge,", "tokens": ["Mit", "mil\u00b7der", "Huld", ",", "als", "treu\u00b7e", "Z\u00f6g\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hier unter Dein geweihtes Dach sie auf,", "tokens": ["Hier", "un\u00b7ter", "Dein", "ge\u00b7weih\u00b7tes", "Dach", "sie", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Schleier ihnen reichend, der die Stirn", "tokens": ["Den", "Schlei\u00b7er", "ih\u00b7nen", "rei\u00b7chend", ",", "der", "die", "Stirn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.