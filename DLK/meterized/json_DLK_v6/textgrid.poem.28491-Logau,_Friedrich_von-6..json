{"textgrid.poem.28491": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr Leid, Creutz, Noth und Tod, die dir, O liebes Kind!", "tokens": ["F\u00fcr", "Leid", ",", "Creutz", ",", "Noth", "und", "Tod", ",", "die", "dir", ",", "O", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "PPER", "$,", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser schn\u00f6den Welt zu dulden etwa sind,", "tokens": ["In", "die\u00b7ser", "schn\u00f6\u00b7den", "Welt", "zu", "dul\u00b7den", "et\u00b7wa", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKZU", "VVINF", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist Jesu Christi Blut dein aller-bestes Heil;", "tokens": ["Ist", "Je\u00b7su", "Chris\u00b7ti", "Blut", "dein", "al\u00b7ler\u00b7bes\u00b7tes", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dadurch der Himmel dir verschrieben ist zu theil.", "tokens": ["Da\u00b7durch", "der", "Him\u00b7mel", "dir", "ver\u00b7schrie\u00b7ben", "ist", "zu", "theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVPP", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}