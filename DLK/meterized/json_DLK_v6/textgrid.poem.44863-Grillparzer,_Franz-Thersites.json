{"textgrid.poem.44863": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Thersites", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Hundsgesicht mit einer Hasenseele!", "tokens": ["Du", "Hunds\u00b7ge\u00b7sicht", "mit", "ei\u00b7ner", "Ha\u00b7sen\u00b7see\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was klammerst du dich an der F\u00fcrsten Rock?", "tokens": ["Was", "klam\u00b7merst", "du", "dich", "an", "der", "F\u00fcrs\u00b7ten", "Rock", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob auch das Wort an dir sein Ziel verfehle,", "tokens": ["Ob", "auch", "das", "Wort", "an", "dir", "sein", "Ziel", "ver\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Herrscherstab, bedenk, dient auch als Stock.", "tokens": ["Der", "Herr\u00b7scher\u00b7stab", ",", "be\u00b7denk", ",", "dient", "auch", "als", "Stock", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}