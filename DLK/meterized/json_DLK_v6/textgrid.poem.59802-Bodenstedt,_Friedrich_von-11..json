{"textgrid.poem.59802": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl wei\u00df ich einen Kranz zu winden", "tokens": ["Wohl", "wei\u00df", "ich", "ei\u00b7nen", "Kranz", "zu", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Blumen, die ich selbst gepfl\u00fcckt \u2013", "tokens": ["Aus", "Blu\u00b7men", ",", "die", "ich", "selbst", "ge\u00b7pfl\u00fcckt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl auch das rechte Wort zu finden,", "tokens": ["Wohl", "auch", "das", "rech\u00b7te", "Wort", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich betr\u00fcbt bin, ob begl\u00fcckt.", "tokens": ["Ob", "ich", "be\u00b7tr\u00fcbt", "bin", ",", "ob", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Solang' ich meiner Sinne Meister,", "tokens": ["So\u00b7lang'", "ich", "mei\u00b7ner", "Sin\u00b7ne", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Solang' ich wei\u00df, was mir gef\u00e4llt,", "tokens": ["So\u00b7lang'", "ich", "wei\u00df", ",", "was", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gehorchen dienstbar mir die Geister", "tokens": ["Ge\u00b7hor\u00b7chen", "dienst\u00b7bar", "mir", "die", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blumen- und der Feenwelt.", "tokens": ["Der", "Blu\u00b7men", "und", "der", "Feen\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch in der heil'gen Glut des Kusses,", "tokens": ["Doch", "in", "der", "heil'\u00b7gen", "Glut", "des", "Kus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wunderleuchten des Geschicks,", "tokens": ["Im", "Wun\u00b7der\u00b7leuch\u00b7ten", "des", "Ge\u00b7schicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Augenblick des Vollgenusses,", "tokens": ["Im", "Au\u00b7gen\u00b7blick", "des", "Voll\u00b7ge\u00b7nus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Vollgenu\u00df des Augenblicks:", "tokens": ["Im", "Voll\u00b7ge\u00b7nu\u00df", "des", "Au\u00b7gen\u00b7blicks", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da fehlen mir zum Lied die T\u00f6ne,", "tokens": ["Da", "feh\u00b7len", "mir", "zum", "Lied", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleichwie der Nachtigall der Schlag,", "tokens": ["Gleich\u00b7wie", "der", "Nach\u00b7ti\u00b7gall", "der", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil wohl der Mensch das h\u00f6chste Sch\u00f6ne", "tokens": ["Weil", "wohl", "der", "Mensch", "das", "h\u00f6chs\u00b7te", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Genie\u00dfen, doch nicht singen mag.", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", ",", "doch", "nicht", "sin\u00b7gen", "mag."], "token_info": ["word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "ADV", "PTKNEG", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer kann die helle Sonne malen", "tokens": ["Wer", "kann", "die", "hel\u00b7le", "Son\u00b7ne", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In h\u00f6chster Glut, im Mittagslicht?", "tokens": ["In", "h\u00f6chs\u00b7ter", "Glut", ",", "im", "Mit\u00b7tags\u00b7licht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer nur sie sehn mit ihren Strahlen", "tokens": ["Wer", "nur", "sie", "sehn", "mit", "ih\u00b7ren", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Angesicht zu Angesicht?", "tokens": ["Von", "An\u00b7ge\u00b7sicht", "zu", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}