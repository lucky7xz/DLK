{"textgrid.poem.24800": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Zum Ged\u00e4chtnis", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er losch auf einmal aus so wie ein Licht.", "tokens": ["Er", "losch", "auf", "ein\u00b7mal", "aus", "so", "wie", "ein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADV", "APPR", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir trugen alle wie von einem Blitz", "tokens": ["Wir", "tru\u00b7gen", "al\u00b7le", "wie", "von", "ei\u00b7nem", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Widerschein als Bl\u00e4sse im Gesicht.", "tokens": ["Den", "Wi\u00b7der\u00b7schein", "als", "Bl\u00e4s\u00b7se", "im", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er fiel: da fielen alle Puppen hin,", "tokens": ["Er", "fiel", ":", "da", "fie\u00b7len", "al\u00b7le", "Pup\u00b7pen", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deren Adern er sein Lebensblut", "tokens": ["In", "de\u00b7ren", "A\u00b7dern", "er", "sein", "Le\u00b7bens\u00b7blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gegossen hatte; lautlos starben sie,", "tokens": ["Ge\u00b7gos\u00b7sen", "hat\u00b7te", ";", "laut\u00b7los", "star\u00b7ben", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wo er lag, da lag ein Haufen Leichen,", "tokens": ["Und", "wo", "er", "lag", ",", "da", "lag", "ein", "Hau\u00b7fen", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "W\u00fcst hingestreckt: das Knie von einem S\u00e4ufer", "tokens": ["W\u00fcst", "hin\u00b7ge\u00b7streckt", ":", "das", "Knie", "von", "ei\u00b7nem", "S\u00e4u\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In eines K\u00f6nigs Aug gedr\u00fcckt, Don Philipp", "tokens": ["In", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Aug", "ge\u00b7dr\u00fcckt", ",", "Don", "Phi\u00b7lipp"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,", "NE", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit Caliban als Alp um seinen Hals,", "tokens": ["Mit", "Ca\u00b7li\u00b7ban", "als", "Alp", "um", "sei\u00b7nen", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und jeder tot.", "tokens": ["Und", "je\u00b7der", "tot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da wu\u00dften wir, wer uns gestorben war:", "tokens": ["Da", "wu\u00df\u00b7ten", "wir", ",", "wer", "uns", "ge\u00b7stor\u00b7ben", "war", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Zauberer, der gro\u00dfe, gro\u00dfe Gaukler!", "tokens": ["Der", "Zau\u00b7be\u00b7rer", ",", "der", "gro\u00b7\u00dfe", ",", "gro\u00b7\u00dfe", "Gauk\u00b7ler", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus den H\u00e4usern traten wir heraus", "tokens": ["Und", "aus", "den", "H\u00e4u\u00b7sern", "tra\u00b7ten", "wir", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und fingen an zu reden, wer er war.", "tokens": ["Und", "fin\u00b7gen", "an", "zu", "re\u00b7den", ",", "wer", "er", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer aber war er, und wer war er nicht?", "tokens": ["Wer", "a\u00b7ber", "war", "er", ",", "und", "wer", "war", "er", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "$,", "KON", "PWS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er kroch von einer Larve in die andre,", "tokens": ["Er", "kroch", "von", "ei\u00b7ner", "Lar\u00b7ve", "in", "die", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprang aus des Vaters in des Sohnes Leib", "tokens": ["Sprang", "aus", "des", "Va\u00b7ters", "in", "des", "Soh\u00b7nes", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und tauschte wie Gew\u00e4nder die Gestalten.", "tokens": ["Und", "tauschte", "wie", "Ge\u00b7w\u00e4n\u00b7der", "die", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Mit Schwertern, die er kreisen lie\u00df so schnell,", "tokens": ["Mit", "Schwer\u00b7tern", ",", "die", "er", "krei\u00b7sen", "lie\u00df", "so", "schnell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df niemand ihre Klinge funkeln sah,", "tokens": ["Da\u00df", "nie\u00b7mand", "ih\u00b7re", "Klin\u00b7ge", "fun\u00b7keln", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hieb er sich selbst in St\u00fccke: Jago war", "tokens": ["Hieb", "er", "sich", "selbst", "in", "St\u00fc\u00b7cke", ":", "Ja\u00b7go", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$.", "NE", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht das eine, und die andre H\u00e4lfte", "tokens": ["Viel\u00b7leicht", "das", "ei\u00b7ne", ",", "und", "die", "and\u00b7re", "H\u00e4lf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ART", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gab einen s\u00fc\u00dfen Narren oder Tr\u00e4umer.", "tokens": ["Gab", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Nar\u00b7ren", "o\u00b7der", "Tr\u00e4u\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein ganzer Leib war wie der Zauberschleier,", "tokens": ["Sein", "gan\u00b7zer", "Leib", "war", "wie", "der", "Zau\u00b7ber\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In dessen Falten alle Dinge wohnen:", "tokens": ["In", "des\u00b7sen", "Fal\u00b7ten", "al\u00b7le", "Din\u00b7ge", "woh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er holte Tiere aus sich selbst hervor:", "tokens": ["Er", "hol\u00b7te", "Tie\u00b7re", "aus", "sich", "selbst", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das Schaf, den L\u00f6wen, einen dummen Teufel", "tokens": ["Das", "Schaf", ",", "den", "L\u00f6\u00b7wen", ",", "ei\u00b7nen", "dum\u00b7men", "Teu\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und einen schrecklichen, und den, und jenen,", "tokens": ["Und", "ei\u00b7nen", "schreck\u00b7li\u00b7chen", ",", "und", "den", ",", "und", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "KON", "ART", "$,", "KON", "PDS", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und dich und mich. Sein ganzer Leib war gl\u00fchend,", "tokens": ["Und", "dich", "und", "mich", ".", "Sein", "gan\u00b7zer", "Leib", "war", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von innerlichem Schicksal durch und durch", "tokens": ["Von", "in\u00b7ner\u00b7li\u00b7chem", "Schick\u00b7sal", "durch", "und", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie Kohle gl\u00fchend, und er lebte drin", "tokens": ["Wie", "Koh\u00b7le", "gl\u00fc\u00b7hend", ",", "und", "er", "leb\u00b7te", "drin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und sah auf uns, die wir in H\u00e4usern wohnen,", "tokens": ["Und", "sah", "auf", "uns", ",", "die", "wir", "in", "H\u00e4u\u00b7sern", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Mit jenem undurchdringlich fremden Blick", "tokens": ["Mit", "je\u00b7nem", "un\u00b7durch\u00b7dring\u00b7lich", "frem\u00b7den", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Des Salamanders, der im Feuer wohnt.", "tokens": ["Des", "Sa\u00b7la\u00b7man\u00b7ders", ",", "der", "im", "Feu\u00b7er", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Er war ein wilder K\u00f6nig. Um die H\u00fcften", "tokens": ["Er", "war", "ein", "wil\u00b7der", "K\u00f6\u00b7nig", ".", "Um", "die", "H\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "KOUI", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trug er wie bunte Muscheln aufgereiht", "tokens": ["Trug", "er", "wie", "bun\u00b7te", "Mu\u00b7scheln", "auf\u00b7ge\u00b7reiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wahrheit und die L\u00fcge von uns allen.", "tokens": ["Die", "Wahr\u00b7heit", "und", "die", "L\u00fc\u00b7ge", "von", "uns", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seinen Augen flogen unsre Tr\u00e4ume", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "flo\u00b7gen", "uns\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor\u00fcber, wie von Scharen wilder V\u00f6gel", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "wie", "von", "Scha\u00b7ren", "wil\u00b7der", "V\u00f6\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Spiegelbild in einem tiefen Wasser.", "tokens": ["Das", "Spie\u00b7gel\u00b7bild", "in", "ei\u00b7nem", "tie\u00b7fen", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Hier trat er her, auf ebendiesen Fleck,", "tokens": ["Hier", "trat", "er", "her", ",", "auf", "e\u00b7ben\u00b7die\u00b7sen", "Fleck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich jetzt steh, und wie im Tritonshorn", "tokens": ["Wo", "ich", "jetzt", "steh", ",", "und", "wie", "im", "Tri\u00b7tons\u00b7horn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "KON", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der L\u00e4rm des Meeres eingefangen ist,", "tokens": ["Der", "L\u00e4rm", "des", "Mee\u00b7res", "ein\u00b7ge\u00b7fan\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So war in ihm die Stimme alles Lebens:", "tokens": ["So", "war", "in", "ihm", "die", "Stim\u00b7me", "al\u00b7les", "Le\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er wurde gro\u00df. Er war der ganze Wald,", "tokens": ["Er", "wur\u00b7de", "gro\u00df", ".", "Er", "war", "der", "gan\u00b7ze", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er war das Land, durch das die Stra\u00dfen laufen.", "tokens": ["Er", "war", "das", "Land", ",", "durch", "das", "die", "Stra\u00b7\u00dfen", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit Augen wie die Kinder sa\u00dfen wir", "tokens": ["Mit", "Au\u00b7gen", "wie", "die", "Kin\u00b7der", "sa\u00b7\u00dfen", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM",