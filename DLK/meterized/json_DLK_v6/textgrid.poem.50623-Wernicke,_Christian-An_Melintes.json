{"textgrid.poem.50623": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Melintes", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df auf Verfolgung nicht sich \u00e4ndern deine Triebe,", "tokens": ["La\u00df", "auf", "Ver\u00b7fol\u00b7gung", "nicht", "sich", "\u00e4n\u00b7dern", "dei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PTKNEG", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch sich in Zorn und Ha\u00df verkehren Gunst und Liebe;", "tokens": ["Noch", "sich", "in", "Zorn", "und", "Ha\u00df", "ver\u00b7keh\u00b7ren", "Gunst", "und", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Thu' jetzt mit ", "tokens": ["Thu'", "jetzt", "mit"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und mache durch Geduld die Unschuld offenbar;", "tokens": ["Und", "ma\u00b7che", "durch", "Ge\u00b7duld", "die", "Un\u00b7schuld", "of\u00b7fen\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sonst w\u00fcrden Die, die jetzt ihr Unrecht selbst erkennen,", "tokens": ["Sonst", "w\u00fcr\u00b7den", "Die", ",", "die", "jetzt", "ihr", "Un\u00b7recht", "selbst", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das, was die ", "tokens": ["Das", ",", "was", "die"], "token_info": ["word", "punct", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}