{"dta.poem.20874": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Weil unser Leben w\u00e4rt/ eh wir die Erde f\u00fcllen/", "tokens": ["Weil", "un\u00b7ser", "Le\u00b7ben", "w\u00e4rt", "/", "eh", "wir", "die", "Er\u00b7de", "f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist viel Schwerigkeit der Wei\u00dfheit Durst zu stillen/", "tokens": ["So", "ist", "viel", "Schwe\u00b7rig\u00b7keit", "der", "Wei\u00df\u00b7heit", "Durst", "zu", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So unsern Geist ber\u00fchrt/", "tokens": ["So", "un\u00b7sern", "Geist", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der kurtze Lebens-Lauff/ den wir allhier verrichten/", "tokens": ["Der", "kurt\u00b7ze", "Le\u00b7bens\u00b7Lauff", "/", "den", "wir", "all\u00b7hier", "ver\u00b7rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sey dahin gef\u00fchrt/", "tokens": ["Der", "sey", "da\u00b7hin", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PAV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df wir den dicken Dampff der schwartzen Nacht vernichten/", "tokens": ["Da\u00df", "wir", "den", "di\u00b7cken", "Dampff", "der", "schwart\u00b7zen", "Nacht", "ver\u00b7nich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und damit wir durch den Schein/", "tokens": ["Und", "da\u00b7mit", "wir", "durch", "den", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den der Flei\u00df uns angez\u00fcndet/", "tokens": ["Den", "der", "Flei\u00df", "uns", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf dem Wege m\u00f6gen seyn/", "tokens": ["Auf", "dem", "We\u00b7ge", "m\u00f6\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da man reine Warheit findet.", "tokens": ["Da", "man", "rei\u00b7ne", "War\u00b7heit", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}