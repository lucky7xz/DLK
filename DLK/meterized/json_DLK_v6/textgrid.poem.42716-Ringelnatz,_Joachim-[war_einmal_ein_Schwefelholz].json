{"textgrid.poem.42716": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[war einmal ein Schwefelholz]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War einmal ein Schwefelholz,", "tokens": ["War", "ein\u00b7mal", "ein", "Schwe\u00b7fel\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das sich mit erhabnem Stolz", "tokens": ["Das", "sich", "mit", "er\u00b7hab\u00b7nem", "Stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Anarchisten nannte", "tokens": ["Ei\u00b7nen", "An\u00b7ar\u00b7chis\u00b7ten", "nann\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein ganzes Haus verbrannte.", "tokens": ["Und", "ein", "gan\u00b7zes", "Haus", "ver\u00b7brann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses war schon ungew\u00f6hnlich,", "tokens": ["Die\u00b7ses", "war", "schon", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch es kannte auch pers\u00f6nlich", "tokens": ["Doch", "es", "kann\u00b7te", "auch", "per\u00b7s\u00f6n\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meyers Taschenlexika,", "tokens": ["Me\u00b7yers", "Ta\u00b7schen\u00b7le\u00b7xi\u00b7ka", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Ganz speziell das B\u00e4ndchen \u00bbA\u00ab,", "tokens": ["Ganz", "spe\u00b7zi\u00b7ell", "das", "B\u00e4nd\u00b7chen", "\u00bb", "A", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$(", "NE", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weshalb es sich nach dem Brande", "tokens": ["We\u00b7shalb", "es", "sich", "nach", "dem", "Bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "An besagtes B\u00e4ndchen wandte", "tokens": ["An", "be\u00b7sag\u00b7tes", "B\u00e4nd\u00b7chen", "wand\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit den Worten: \u00bbSag, was ist", "tokens": ["Mit", "den", "Wor\u00b7ten", ":", "\u00bb", "Sag", ",", "was", "ist"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "$(", "NN", "$,", "PWS", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Eigentlich ein Anarchist?\u00ab", "tokens": ["Ei\u00b7gent\u00b7lich", "ein", "An\u00b7ar\u00b7chist", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}