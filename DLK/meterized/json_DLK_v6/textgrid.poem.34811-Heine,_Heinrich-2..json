{"textgrid.poem.34811": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwollen Sie ihr nicht vorgestellt sein?\u00ab", "tokens": ["\u00bb", "wol\u00b7len", "Sie", "ihr", "nicht", "vor\u00b7ge\u00b7stellt", "sein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPER", "PTKNEG", "VVPP", "VAINF", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fl\u00fcsterte mir die Herzogin. \u2013", "tokens": ["Fl\u00fcs\u00b7ter\u00b7te", "mir", "die", "Her\u00b7zo\u00b7gin", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbbeileibe nicht, ich m\u00fc\u00dft ein Held sein,", "tokens": ["\u00bb", "bei\u00b7lei\u00b7be", "nicht", ",", "ich", "m\u00fc\u00dft", "ein", "Held", "sein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKNEG", "$,", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Anblick schon wirrt mir den Sinn.\u00ab", "tokens": ["Ihr", "An\u00b7blick", "schon", "wirrt", "mir", "den", "Sinn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das sch\u00f6ne Weib macht mich erbeben!", "tokens": ["Das", "sch\u00f6\u00b7ne", "Weib", "macht", "mich", "er\u00b7be\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ahnet mir, in ihrer N\u00e4h'", "tokens": ["Es", "ah\u00b7net", "mir", ",", "in", "ih\u00b7rer", "N\u00e4h'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beginnt f\u00fcr mich ein neues Leben,", "tokens": ["Be\u00b7ginnt", "f\u00fcr", "mich", "ein", "neu\u00b7es", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit neuer Lust, mit neuem Weh.", "tokens": ["Mit", "neu\u00b7er", "Lust", ",", "mit", "neu\u00b7em", "Weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es h\u00e4lt wie Angst mich von ihr ferne,", "tokens": ["Es", "h\u00e4lt", "wie", "Angst", "mich", "von", "ihr", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "PRF", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es treibt mich Sehnsucht hin zu ihr!", "tokens": ["Es", "treibt", "mich", "Sehn\u00b7sucht", "hin", "zu", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie meines Schicksals wilde Sterne", "tokens": ["Wie", "mei\u00b7nes", "Schick\u00b7sals", "wil\u00b7de", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erscheinen diese Augen mir.", "tokens": ["Er\u00b7schei\u00b7nen", "die\u00b7se", "Au\u00b7gen", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Stirn ist klar. Doch es gewittert", "tokens": ["Die", "Stirn", "ist", "klar", ".", "Doch", "es", "ge\u00b7wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dahinter schon der k\u00fcnft'ge Blitz,", "tokens": ["Da\u00b7hin\u00b7ter", "schon", "der", "k\u00fcnft'\u00b7ge", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der k\u00fcnft'ge Sturm, der mich ersch\u00fcttert", "tokens": ["Der", "k\u00fcnft'\u00b7ge", "Sturm", ",", "der", "mich", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis in der Seele tiefsten Sitz.", "tokens": ["Bis", "in", "der", "See\u00b7le", "tiefs\u00b7ten", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Mund ist fromm. Doch mit Entsetzen", "tokens": ["Der", "Mund", "ist", "fromm", ".", "Doch", "mit", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unter den Rosen seh ich schon", "tokens": ["Un\u00b7ter", "den", "Ro\u00b7sen", "seh", "ich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Schlangen, die mich einst verletzen", "tokens": ["Die", "Schlan\u00b7gen", ",", "die", "mich", "einst", "ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit falschem Ku\u00df, mit s\u00fc\u00dfem Hohn.", "tokens": ["Mit", "fal\u00b7schem", "Ku\u00df", ",", "mit", "s\u00fc\u00b7\u00dfem", "Hohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Sehnsucht treibt. \u2013 Ich mu\u00df mich n\u00e4h'ren", "tokens": ["Die", "Sehn\u00b7sucht", "treibt", ".", "\u2013", "Ich", "mu\u00df", "mich", "n\u00e4h'\u00b7ren"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem holden, unheilschwangern Ort \u2013", "tokens": ["Dem", "hol\u00b7den", ",", "un\u00b7heil\u00b7schwan\u00b7gern", "Ort", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon kann ich ihre Stimme h\u00f6ren \u2013", "tokens": ["Schon", "kann", "ich", "ih\u00b7re", "Stim\u00b7me", "h\u00f6\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Klingende Flamme ist ihr Wort.", "tokens": ["Klin\u00b7gen\u00b7de", "Flam\u00b7me", "ist", "ihr", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Sie fragt: \u00bbMonsieur, wie ist der Name", "tokens": ["Sie", "fragt", ":", "\u00bb", "Mon\u00b7si\u00b7eur", ",", "wie", "ist", "der", "Na\u00b7me"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "FM", "$,", "PWAV", "VAFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der S\u00e4ngerin, die eben sang?\u00ab", "tokens": ["Der", "S\u00e4n\u00b7ge\u00b7rin", ",", "die", "e\u00b7ben", "sang", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stotternd antworte ich der Dame:", "tokens": ["Stot\u00b7ternd", "ant\u00b7wor\u00b7te", "ich", "der", "Da\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbhab nichts geh\u00f6rt von dem Gesang.\u00ab", "tokens": ["\u00bb", "hab", "nichts", "ge\u00b7h\u00f6rt", "von", "dem", "Ge\u00b7sang", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PIS", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}}}}