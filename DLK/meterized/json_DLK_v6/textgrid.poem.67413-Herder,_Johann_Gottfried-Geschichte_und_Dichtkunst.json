{"textgrid.poem.67413": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Geschichte und Dichtkunst", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im sch\u00f6nen Musentempel, wo ich einst", "tokens": ["Im", "sch\u00f6\u00b7nen", "Mu\u00b7sen\u00b7tem\u00b7pel", ",", "wo", "ich", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Anschauend in Begeistrung mich verlor,", "tokens": ["An\u00b7schau\u00b7end", "in", "Be\u00b7geis\u00b7trung", "mich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbjungfr\u00e4uliche Gestalten,\u00ab sprach ich, \u00bblebtet,", "tokens": ["\u00bb", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Ge\u00b7stal\u00b7ten", ",", "\u00ab", "sprach", "ich", ",", "\u00bb", "leb\u00b7tet", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo lebtet Ihr? der reinen Menschheit Bilder,", "tokens": ["Wo", "leb\u00b7tet", "Ihr", "?", "der", "rei\u00b7nen", "Menschheit", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Woher nahm Euch der hohe Genius?\u00ab", "tokens": ["Wo\u00b7her", "nahm", "Euch", "der", "ho\u00b7he", "Ge\u00b7nius", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da f\u00fcllte des entz\u00fcckten Ph\u00f6bus mich,", "tokens": ["Da", "f\u00fcll\u00b7te", "des", "ent\u00b7z\u00fcck\u00b7ten", "Ph\u00f6\u00b7bus", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des sch\u00f6nen J\u00fcnglings, P\u00e4an, und das Chor", "tokens": ["Des", "sch\u00f6\u00b7nen", "J\u00fcng\u00b7lings", ",", "P\u00e4\u00b7an", ",", "und", "das", "Chor"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Musen mit Gesang und Fl\u00f6tenton,", "tokens": ["Der", "Mu\u00b7sen", "mit", "Ge\u00b7sang", "und", "Fl\u00f6\u00b7ten\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Psalterion und Leyer stimmten ein;", "tokens": ["Psal\u00b7te\u00b7ri\u00b7on", "und", "Le\u00b7yer", "stimm\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Kalliope mit aufgeschlagnem Buch,", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", "mit", "auf\u00b7ge\u00b7schlag\u00b7nem", "Buch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Euterp' und Erato, Terpsichore,", "tokens": ["Eu\u00b7ter\u00b7p'", "und", "E\u00b7ra\u00b7to", ",", "Ter\u00b7psi\u00b7cho\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NE", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.12": {"text": "Thalia; nur die Muse der Geschichte", "tokens": ["Tha\u00b7lia", ";", "nur", "die", "Mu\u00b7se", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Sa\u00df schweigend da mit weggewandtem Blick.", "tokens": ["Sa\u00df", "schwei\u00b7gend", "da", "mit", "weg\u00b7ge\u00b7wand\u00b7tem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich nahete mich ihr, und Geist zu Geist,", "tokens": ["Ich", "na\u00b7he\u00b7te", "mich", "ihr", ",", "und", "Geist", "zu", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Verstand sie mich, antwortete mir sanft:", "tokens": ["Ver\u00b7stand", "sie", "mich", ",", "ant\u00b7wor\u00b7te\u00b7te", "mir", "sanft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "\u00bbdu wunderst, Fremdling, Dich, da\u00df ich im Chor", "tokens": ["\u00bb", "du", "wun\u00b7derst", ",", "Fremd\u00b7ling", ",", "Dich", ",", "da\u00df", "ich", "im", "Chor"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "$,", "NN", "$,", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der lauten Schwestern schweig'? Ich horche zu", "tokens": ["Der", "lau\u00b7ten", "Schwes\u00b7tern", "schweig'", "?", "Ich", "hor\u00b7che", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und merk' auf unsres hohen F\u00fchrers Anklang", "tokens": ["Und", "merk'", "auf", "uns\u00b7res", "ho\u00b7hen", "F\u00fch\u00b7rers", "An\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und lern' an Jeder lebendem Gesang.", "tokens": ["Und", "lern'", "an", "Je\u00b7der", "le\u00b7ben\u00b7dem", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Kalliope stellt meinem Ohr vor Augen,", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", "stellt", "mei\u00b7nem", "Ohr", "vor", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Was einst geschah. Umfang und Ziel und Zweck,", "tokens": ["Was", "einst", "ge\u00b7schah", ".", "Um\u00b7fang", "und", "Ziel", "und", "Zweck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$.", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das Maa\u00df der Gegenwart und Leidenschaft", "tokens": ["Das", "Maa\u00df", "der", "Ge\u00b7gen\u00b7wart", "und", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Lern' ich aus ihrer und der Schwestern Weise.", "tokens": ["Lern'", "ich", "aus", "ih\u00b7rer", "und", "der", "Schwes\u00b7tern", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Doch steht auch schweigend dort Melpomene,", "tokens": ["Doch", "steht", "auch", "schwei\u00b7gend", "dort", "Mel\u00b7po\u00b7me\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Die ihren Fels hinansteigt; siehe dort", "tokens": ["Die", "ih\u00b7ren", "Fels", "hi\u00b7nan\u00b7steigt", ";", "sie\u00b7he", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$.", "VVIMP", "ADV"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.26": {"text": "Urania mit ihrem Stabe, mit", "tokens": ["U\u00b7ra\u00b7nia", "mit", "ih\u00b7rem", "Sta\u00b7be", ",", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Erhobnem Finger Polyhymnia;", "tokens": ["Er\u00b7hob\u00b7nem", "Fin\u00b7ger", "Po\u00b7ly\u00b7hym\u00b7nia", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Sie lehren mich die h\u00f6hre Harmonie", "tokens": ["Sie", "leh\u00b7ren", "mich", "die", "h\u00f6h\u00b7re", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Weltbegebenheiten. Horch!\u00ab Ich h\u00f6rte", "tokens": ["Der", "Welt\u00b7be\u00b7ge\u00b7ben\u00b7hei\u00b7ten", ".", "Horch", "!", "\u00ab", "Ich", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Welch einen andern als der Leyer Klang,", "tokens": ["Welch", "ei\u00b7nen", "an\u00b7dern", "als", "der", "Le\u00b7yer", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "PIS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Als Fl\u00f6te, Cither und Psalterion!", "tokens": ["Als", "Fl\u00f6\u00b7te", ",", "Cit\u00b7her", "und", "Psal\u00b7te\u00b7ri\u00b7on", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "\u00bbo Klio!\u00ab sprach ich. \u00bbNenne mich nicht Klio,", "tokens": ["\u00bb", "o", "Klio", "!", "\u00ab", "sprach", "ich", ".", "\u00bb", "Nen\u00b7ne", "mich", "nicht", "Klio", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$.", "$(", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "PTKNEG", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Ich l\u00e4ngst der Fama, die, die Wangen voll", "tokens": ["Ich", "l\u00e4ngst", "der", "Fa\u00b7ma", ",", "die", ",", "die", "Wan\u00b7gen", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Von Athem, Lob verk\u00fcndet. Meine Mutter", "tokens": ["Von", "A\u00b7them", ",", "Lob", "ver\u00b7k\u00fcn\u00b7det", ".", "Mei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "VVPP", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "War Mneme; ihre liebste Schwester hie\u00df", "tokens": ["War", "Mne\u00b7me", ";", "ih\u00b7re", "liebs\u00b7te", "Schwes\u00b7ter", "hie\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Melete, und Aoide war die j\u00fcngste;", "tokens": ["Me\u00b7le\u00b7te", ",", "und", "A\u00b7o\u00b7i\u00b7de", "war", "die", "j\u00fcngs\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.38": {"text": "Ihr aller Mutter war Mnemosyne.", "tokens": ["Ihr", "al\u00b7ler", "Mut\u00b7ter", "war", "Mne\u00b7mo\u00b7sy\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VAFIN", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Die Schwestern, die Aoidens Abkunft sind,", "tokens": ["Die", "Schwes\u00b7tern", ",", "die", "A\u00b7o\u00b7i\u00b7dens", "Ab\u00b7kunft", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "So sagt der G\u00f6tterspruch, sie werden einst", "tokens": ["So", "sagt", "der", "G\u00f6t\u00b7ter\u00b7spruch", ",", "sie", "wer\u00b7den", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Im Ansehn sinken; denn Mnemosyne", "tokens": ["Im", "An\u00b7sehn", "sin\u00b7ken", ";", "denn", "Mne\u00b7mo\u00b7sy\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$.", "KON", "NE"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Mit ihren T\u00f6chtern, Mneme, meine Mutter,", "tokens": ["Mit", "ih\u00b7ren", "T\u00f6ch\u00b7tern", ",", "Mne\u00b7me", ",", "mei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Melete und Aoide, die drei h\u00f6chsten", "tokens": ["Me\u00b7le\u00b7te", "und", "A\u00b7o\u00b7i\u00b7de", ",", "die", "drei", "h\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "PRELS", "CARD", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Und hehresten der Musen, kehren einst", "tokens": ["Und", "heh\u00b7res\u00b7ten", "der", "Mu\u00b7sen", ",", "keh\u00b7ren", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Dem bessern menschlichen Geschlecht zur\u00fcck.", "tokens": ["Dem", "bes\u00b7sern", "menschli\u00b7chen", "Ge\u00b7schlecht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Und sie erwarten meine n\u00e4hern Schwestern,", "tokens": ["Und", "sie", "er\u00b7war\u00b7ten", "mei\u00b7ne", "n\u00e4\u00b7hern", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Die schweigenden: hier Polyhymnia,", "tokens": ["Die", "schwei\u00b7gen\u00b7den", ":", "hier", "Po\u00b7ly\u00b7hym\u00b7nia", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "ADV", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Die mir der alten Gotteslehre Weisheit,", "tokens": ["Die", "mir", "der", "al\u00b7ten", "Got\u00b7tes\u00b7leh\u00b7re", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Urania, die mir der Welten Bau,", "tokens": ["U\u00b7ra\u00b7nia", ",", "die", "mir", "der", "Wel\u00b7ten", "Bau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.50": {"text": "Der Zeiten Ordnung, dort Melpomene,", "tokens": ["Der", "Zei\u00b