{"dta.poem.8230": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Seiner Herrin an der Seite;", "tokens": ["Sei\u00b7ner", "Her\u00b7rin", "an", "der", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Schulter gl\u00e4nzt die Sch\u00e4rpe", "tokens": ["Von", "der", "Schul\u00b7ter", "gl\u00e4nzt", "die", "Sch\u00e4r\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein freundlich Minnezeichen.", "tokens": ["Als", "ein", "freund\u00b7lich", "Min\u00b7ne\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieghaft schlingt er seine Arme", "tokens": ["Sieg\u00b7haft", "schlingt", "er", "sei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Leib des stolzen Weibes,", "tokens": ["Um", "den", "Leib", "des", "stol\u00b7zen", "Wei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unaufhaltsam s\u00fc\u00dfe Worte", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwatzt er, und die Dame schweiget.", "tokens": ["Schwatzt", "er", ",", "und", "die", "Da\u00b7me", "schwei\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will zu einem halben W\u00f6rtchen", "tokens": ["Will", "zu", "ei\u00b7nem", "hal\u00b7ben", "W\u00f6rt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oeffnen sie der Lippen Zeile,", "tokens": ["Oeff\u00b7nen", "sie", "der", "Lip\u00b7pen", "Zei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlie\u00dft er ihr den Mund mit K\u00fcssen,", "tokens": ["Schlie\u00dft", "er", "ihr", "den", "Mund", "mit", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Dame lauscht und schweiget.", "tokens": ["Und", "die", "Da\u00b7me", "lauscht", "und", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201es\u00fc\u00dfe Herrin, unerkl\u00e4rlich", "tokens": ["\u201e", "s\u00fc\u00b7\u00dfe", "Her\u00b7rin", ",", "un\u00b7er\u00b7kl\u00e4r\u00b7lich"], "token_info": ["punct", "word", "word", "punct", "word"], "pos": ["$(", "ADJA", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt mir eure stumme Weise!", "tokens": ["Bleibt", "mir", "eu\u00b7re", "stum\u00b7me", "Wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen eure rothen Lippen", "tokens": ["Wol\u00b7len", "eu\u00b7re", "ro\u00b7then", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiches zahlen mir mit Gleichem?", "tokens": ["Glei\u00b7ches", "zah\u00b7len", "mir", "mit", "Glei\u00b7chem", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oder lernten diese Lippen", "tokens": ["O\u00b7der", "lern\u00b7ten", "die\u00b7se", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblicher die Zeit vertreiben?", "tokens": ["Lieb\u00b7li\u00b7cher", "die", "Zeit", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gar beh\u00e4glich ist das Schwatzen;", "tokens": ["Gar", "be\u00b7h\u00e4g\u00b7lich", "ist", "das", "Schwat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ein Andres ist gescheidter.\u201c", "tokens": ["Doch", "ein", "And\u00b7res", "ist", "ge\u00b7scheid\u00b7ter", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drau\u00dfen auf den Mandelbl\u00fcthen", "tokens": ["Drau\u00b7\u00dfen", "auf", "den", "Man\u00b7del\u00b7bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht die Nacht im Mondenscheine;", "tokens": ["Ruht", "die", "Nacht", "im", "Mon\u00b7den\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unaufhaltsam schwatzt der Ritter,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "schwatzt", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Dame lauscht und schweiget.", "tokens": ["Und", "die", "Da\u00b7me", "lauscht", "und", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gab sie hin des Blickes Zauber?", "tokens": ["Gab", "sie", "hin", "des", "Bli\u00b7ckes", "Zau\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach sie aus die Zauberweise?", "tokens": ["Sprach", "sie", "aus", "die", "Zau\u00b7ber\u00b7wei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nicht f\u00fcrder klagt die Dame", "tokens": ["Doch", "nicht", "f\u00fcr\u00b7der", "klagt", "die", "Da\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber ihres Ritters Schweigen.", "tokens": ["Ue\u00b7ber", "ih\u00b7res", "Rit\u00b7ters", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}