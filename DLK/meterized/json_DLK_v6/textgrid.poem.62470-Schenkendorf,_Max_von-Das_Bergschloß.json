{"textgrid.poem.62470": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Das Bergschlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da droben auf jenem Berge,", "tokens": ["Da", "dro\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da stehet ein altes Haus,", "tokens": ["Da", "ste\u00b7het", "ein", "al\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schreiten zu Nacht und am Mittag", "tokens": ["Es", "schrei\u00b7ten", "zu", "Nacht", "und", "am", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Viel Rittergestalten heraus.", "tokens": ["Viel", "Rit\u00b7ter\u00b7ge\u00b7stal\u00b7ten", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die weilten in herrlichen Tagen", "tokens": ["Die", "weil\u00b7ten", "in", "herr\u00b7li\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hier fr\u00f6hlich am gastlichen Herd,", "tokens": ["Hier", "fr\u00f6h\u00b7lich", "am", "gast\u00b7li\u00b7chen", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie haben viel Schlachten geschlagen,", "tokens": ["Sie", "ha\u00b7ben", "viel", "Schlach\u00b7ten", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie haben viel Becher geleert.", "tokens": ["Sie", "ha\u00b7ben", "viel", "Be\u00b7cher", "ge\u00b7leert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Das alles ist leider! vor\u00fcber,", "tokens": ["Das", "al\u00b7les", "ist", "lei\u00b7der", "!", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "$.", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In Tr\u00fcmmern das alte Thor;", "tokens": ["In", "Tr\u00fcm\u00b7mern", "das", "al\u00b7te", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wer rufet aus Schutt und aus Gr\u00fcften", "tokens": ["Wer", "ru\u00b7fet", "aus", "Schutt", "und", "aus", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die m\u00e4chtige Zeit uns hervor?", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "Zeit", "uns", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und mag sie sich nimmer erheben,", "tokens": ["Und", "mag", "sie", "sich", "nim\u00b7mer", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und h\u00e4lt sie der ewige Neid,", "tokens": ["Und", "h\u00e4lt", "sie", "der", "e\u00b7wi\u00b7ge", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir wollen aufs Neue sie leben,", "tokens": ["Wir", "wol\u00b7len", "aufs", "Neu\u00b7e", "sie", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die alte, die selige Zeit.", "tokens": ["Die", "al\u00b7te", ",", "die", "se\u00b7li\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Wir sind hier zusammengekommen", "tokens": ["Wir", "sind", "hier", "zu\u00b7sam\u00b7men\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sprengen den k\u00f6stlichsten Wein,", "tokens": ["Und", "spren\u00b7gen", "den", "k\u00f6st\u00b7lichs\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zum Wohnsitz der Freien und Frommen", "tokens": ["Zum", "Wohn\u00b7sitz", "der", "Frei\u00b7en", "und", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Erbtheil der Deutschen zu weihn.", "tokens": ["Das", "E\u00b7rbtheil", "der", "Deut\u00b7schen", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Sieh' B\u00fcrger und Ritter aufs Neue", "tokens": ["Sieh'", "B\u00fcr\u00b7ger", "und", "Rit\u00b7ter", "aufs", "Neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "APPRART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erheben zum Schwure die Hand.", "tokens": ["Er\u00b7he\u00b7ben", "zum", "Schwu\u00b7re", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir meinen's recht in der Treue,", "tokens": ["Wir", "mei\u00b7nen's", "recht", "in", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du liebes, du heiliges Land!", "tokens": ["Du", "lie\u00b7bes", ",", "du", "hei\u00b7li\u00b7ges", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}