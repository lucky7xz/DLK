{"textgrid.poem.55665": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[mag sie sich immer erg\u00e4nzen]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag sie sich immer erg\u00e4nzen", "tokens": ["Mag", "sie", "sich", "im\u00b7mer", "er\u00b7g\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Eure br\u00fcchige Welt in sich!", "tokens": ["Eu\u00b7re", "br\u00fc\u00b7chi\u00b7ge", "Welt", "in", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Diese klaren Augen, sie gl\u00e4nzen,", "tokens": ["Die\u00b7se", "kla\u00b7ren", "Au\u00b7gen", ",", "sie", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dieses Herz, es schl\u00e4gt f\u00fcr mich!", "tokens": ["Die\u00b7ses", "Herz", ",", "es", "schl\u00e4gt", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}