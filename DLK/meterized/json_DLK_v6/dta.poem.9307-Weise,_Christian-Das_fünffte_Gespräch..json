{"dta.poem.9307": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach hertzerlieber - - K\u00fcster/", "tokens": ["Ach", "hert\u00b7zer\u00b7lie\u00b7ber", "K\u00fcs\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kenn euch an dem barte wol/", "tokens": ["Ich", "kenn", "euch", "an", "dem", "bar\u00b7te", "wol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach seht doch itzund ins register/", "tokens": ["Ach", "seht", "doch", "it\u00b7zund", "ins", "re\u00b7gis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich was guts bekommen soll/", "tokens": ["Ob", "ich", "was", "guts", "be\u00b7kom\u00b7men", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und solte mir was zugeh\u00f6ren/", "tokens": ["Und", "sol\u00b7te", "mir", "was", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lasst mirs nur fein bald bescheren.", "tokens": ["So", "lasst", "mirs", "nur", "fein", "bald", "be\u00b7sche\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich esse keine pfefferkuchen/", "tokens": ["Ich", "es\u00b7se", "kei\u00b7ne", "pfef\u00b7fer\u00b7ku\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mag auch keine mandelkern/", "tokens": ["Ich", "mag", "auch", "kei\u00b7ne", "man\u00b7del\u00b7kern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was die kleinen kinder suchen", "tokens": ["Und", "was", "die", "klei\u00b7nen", "kin\u00b7der", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dasselbe nehm ich gar nicht gern:", "tokens": ["Das\u00b7sel\u00b7be", "nehm", "ich", "gar", "nicht", "gern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich nur in mein kaltes bette", "tokens": ["Wenn", "ich", "nur", "in", "mein", "kal\u00b7tes", "bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So einen h\u00fcbschen w\u00e4rmstein h\u00e4tte.", "tokens": ["So", "ei\u00b7nen", "h\u00fcb\u00b7schen", "w\u00e4rms\u00b7tein", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich f\u00fcrchte mich so vor der k\u00e4lte/", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "so", "vor", "der", "k\u00e4l\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil ich armer hampelmann", "tokens": ["Die\u00b7weil", "ich", "ar\u00b7mer", "ham\u00b7pel\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des winters bo\u00dfheit stets entgelte/", "tokens": ["Des", "win\u00b7ters", "bo\u00df\u00b7heit", "stets", "ent\u00b7gel\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ich doch nicht verbieten kan.", "tokens": ["Die", "ich", "doch", "nicht", "ver\u00b7bie\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum mu\u00df ich mich im bette kr\u00e4ncken", "tokens": ["Drum", "mu\u00df", "ich", "mich", "im", "bet\u00b7te", "kr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und an den s\u00fcssen w\u00e4rmstein dencken.", "tokens": ["Und", "an", "den", "s\u00fcs\u00b7sen", "w\u00e4rms\u00b7tein", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Das feuer mag im ofen brennen/", "tokens": ["Das", "feu\u00b7er", "mag", "im", "o\u00b7fen", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das bette mag darneben stehn/", "tokens": ["Das", "bet\u00b7te", "mag", "dar\u00b7ne\u00b7ben", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df ich doch allzeit bekennen", "tokens": ["So", "mu\u00df", "ich", "doch", "all\u00b7zeit", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die k\u00e4lte wil mir nicht vergehn:", "tokens": ["Die", "k\u00e4l\u00b7te", "wil", "mir", "nicht", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn alles schl\u00e4gt mit grossem schmertzen", "tokens": ["Denn", "al\u00b7les", "schl\u00e4gt", "mit", "gros\u00b7sem", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir gar zu tieff zu meinem hertzen.", "tokens": ["Mir", "gar", "zu", "tieff", "zu", "mei\u00b7nem", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Und wo ich keinen w\u00e4rmstein habe/", "tokens": ["Und", "wo", "ich", "kei\u00b7nen", "w\u00e4rms\u00b7tein", "ha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00dft es nur ein pf\u00e4nngen seyn/", "tokens": ["So", "la\u00dft", "es", "nur", "ein", "pf\u00e4nn\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leget mir zur liebsten gabe", "tokens": ["Und", "le\u00b7get", "mir", "zur", "liebs\u00b7ten", "ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein bi\u00dfgen warme kohlen drein/", "tokens": ["Ein", "bi\u00df\u00b7gen", "war\u00b7me", "koh\u00b7len", "drein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit ich nicht vergebens harre", "tokens": ["Da\u00b7mit", "ich", "nicht", "ver\u00b7ge\u00b7bens", "har\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und als ein hartes scheit verstarre.", "tokens": ["Und", "als", "ein", "har\u00b7tes", "scheit", "ver\u00b7star\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. F\u00fcrwahr ich seh an meinen h\u00e4nden", "tokens": ["F\u00fcr\u00b7wahr", "ich", "seh", "an", "mei\u00b7nen", "h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ein gebackner pickling aus/", "tokens": ["Wie", "ein", "ge\u00b7back\u00b7ner", "pick\u00b7ling", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sperre mich an allen enden", "tokens": ["Und", "sper\u00b7re", "mich", "an", "al\u00b7len", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein verdorter nessel-strau\u00df:", "tokens": ["Wie", "ein", "ver\u00b7dor\u00b7ter", "nes\u00b7sel\u00b7strau\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn alles ist so trefflich theuer/", "tokens": ["Denn", "al\u00b7les", "ist", "so", "treff\u00b7lich", "theu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nichts so sehr als fremdes feuer.", "tokens": ["Und", "nichts", "so", "sehr", "als", "frem\u00b7des", "feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Wo ich ja gar vergessen bin/", "tokens": ["Wo", "ich", "ja", "gar", "ver\u00b7ges\u00b7sen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So setzt mich dennoch ins regiester/", "tokens": ["So", "setzt", "mich", "den\u00b7noch", "ins", "re\u00b7gies\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und helfft mir also zum gewinn/", "tokens": ["Und", "helfft", "mir", "al\u00b7so", "zum", "ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df/ wenn ich in dem bette liege", "tokens": ["Da\u00df", "/", "wenn", "ich", "in", "dem", "bet\u00b7te", "lie\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den w\u00e4rmstein in die arme kriege.", "tokens": ["Den", "w\u00e4rms\u00b7tein", "in", "die", "ar\u00b7me", "krie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}