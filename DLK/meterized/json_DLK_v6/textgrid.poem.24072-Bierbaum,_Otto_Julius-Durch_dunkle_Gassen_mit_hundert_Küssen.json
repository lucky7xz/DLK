{"textgrid.poem.24072": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Durch dunkle Gassen mit hundert K\u00fcssen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Heidenl\u00e4rm der Tanzmusik,", "tokens": ["Im", "Hei\u00b7den\u00b7l\u00e4rm", "der", "Tanz\u00b7mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Tabaksqualme, schwer und dick,", "tokens": ["Im", "Ta\u00b7baks\u00b7qual\u00b7me", ",", "schwer", "und", "dick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warf zu das Gl\u00fcck mir einen Blick,", "tokens": ["Warf", "zu", "das", "Gl\u00fcck", "mir", "ei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen goldenen Blick aus zwei hei\u00dfen Sonnen.", "tokens": ["Ei\u00b7nen", "gol\u00b7de\u00b7nen", "Blick", "aus", "zwei", "hei\u00b7\u00dfen", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Du warst an meiner Seite.", "tokens": ["Du", "warst", "an", "mei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der laute L\u00e4rm verschwamm, verrann,", "tokens": ["Der", "lau\u00b7te", "L\u00e4rm", "ver\u00b7schwamm", ",", "ver\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun huben erst ihr Leuchten an", "tokens": ["Nun", "hu\u00b7ben", "erst", "ihr", "Leuch\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonnen, da die Nacht begann,", "tokens": ["Die", "Son\u00b7nen", ",", "da", "die", "Nacht", "be\u00b7gann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die himmlischen Sonnen deiner Braunaugen.", "tokens": ["Die", "himm\u00b7li\u00b7schen", "Son\u00b7nen", "dei\u00b7ner", "Braun\u00b7au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du warst an meiner Seite.", "tokens": ["Du", "warst", "an", "mei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heil uns: die Nacht, die finstre Nacht.", "tokens": ["Heil", "uns", ":", "die", "Nacht", ",", "die", "finst\u00b7re", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun schnell uns auf den Weg gemacht!", "tokens": ["Nun", "schnell", "uns", "auf", "den", "Weg", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe dich nach Haus gebracht", "tokens": ["Ich", "ha\u00b7be", "dich", "nach", "Haus", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch dunkle Gassen mit hundert K\u00fcssen.", "tokens": ["Durch", "dunk\u00b7le", "Gas\u00b7sen", "mit", "hun\u00b7dert", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Warm nah du mir zur Seite.", "tokens": ["Warm", "nah", "du", "mir", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Leis klirrend schlug dein Hausthor zu.", "tokens": ["Leis", "klir\u00b7rend", "schlug", "dein", "Haus\u00b7thor", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Fenster Licht. Dann Nacht und Ruh.", "tokens": ["Am", "Fens\u00b7ter", "Licht", ".", "Dann", "Nacht", "und", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$.", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald lagst in Schlaf und Tr\u00e4umen du,", "tokens": ["Bald", "lagst", "in", "Schlaf", "und", "Tr\u00e4u\u00b7men", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber ging weiter durch n\u00e4chtige Felder,", "tokens": ["Ich", "a\u00b7ber", "ging", "wei\u00b7ter", "durch", "n\u00e4ch\u00b7ti\u00b7ge", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Liebe ging mir zur Seite.", "tokens": ["Die", "Lie\u00b7be", "ging", "mir", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}