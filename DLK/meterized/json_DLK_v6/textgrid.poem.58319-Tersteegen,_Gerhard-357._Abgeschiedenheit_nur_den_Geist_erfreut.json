{"textgrid.poem.58319": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "357. Abgeschiedenheit nur den Geist erfreut", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die wahre Abgeschiedenheit", "tokens": ["Die", "wah\u00b7re", "Ab\u00b7ge\u00b7schie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt unserm Geiste Fried' und Freud',", "tokens": ["Bringt", "un\u00b7serm", "Geis\u00b7te", "Fried'", "und", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein reines, sanft und freies Leben;", "tokens": ["Ein", "rei\u00b7nes", ",", "sanft", "und", "frei\u00b7es", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fleisch kriegt nichts auf dieser Bahn,", "tokens": ["Das", "Fleisch", "kriegt", "nichts", "auf", "die\u00b7ser", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Eigenheit geht's auch nicht an,", "tokens": ["Der", "Ei\u00b7gen\u00b7heit", "geht's", "auch", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum mu\u00dft du ihr auch gar nichts geben.", "tokens": ["Drum", "mu\u00dft", "du", "ihr", "auch", "gar", "nichts", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}