{"textgrid.poem.64210": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbschwarzamsel hoch im Ulmenast,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbschwarzamsel hoch im Ulmenast,", "tokens": ["\u00bb", "schwarz\u00b7am\u00b7sel", "hoch", "im", "Ul\u00b7me\u00b7nast", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Was ist's, das du gesungen hast,", "tokens": ["Was", "ist's", ",", "das", "du", "ge\u00b7sun\u00b7gen", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesungen im Sonnenuntergang?", "tokens": ["Ge\u00b7sun\u00b7gen", "im", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es war ein s\u00fc\u00dfer, frommer Klang.", "tokens": ["Es", "war", "ein", "s\u00fc\u00b7\u00dfer", ",", "from\u00b7mer", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Ulmenbaum, vom Wipfelast,", "tokens": ["Im", "Ul\u00b7men\u00b7baum", ",", "vom", "Wip\u00b7fe\u00b7last", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sag' an, was du gesungen hast:", "tokens": ["Sag'", "an", ",", "was", "du", "ge\u00b7sun\u00b7gen", "hast", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich m\u00f6cht' es gern erkunden \u2013", "tokens": ["Ich", "m\u00f6cht'", "es", "gern", "er\u00b7kun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vielleicht macht mich's gesunden.\u00ab", "tokens": ["Viel\u00b7leicht", "macht", "mich's", "ge\u00b7sun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbich singe froh aus voller Brust", "tokens": ["\u00bb", "ich", "sin\u00b7ge", "froh", "aus", "vol\u00b7ler", "Brust"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die reiche, reiche Sommerlust!", "tokens": ["Die", "rei\u00b7che", ",", "rei\u00b7che", "Som\u00b7mer\u00b7lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sing' sie in die weite Welt!", "tokens": ["Ich", "sing'", "sie", "in", "die", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gut ist alles rings bestellt:", "tokens": ["Wie", "gut", "ist", "al\u00b7les", "rings", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sind die roten Wolken sch\u00f6n", "tokens": ["Wie", "sind", "die", "ro\u00b7ten", "Wol\u00b7ken", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da droben in den blauen H\u00f6hn,", "tokens": ["Da", "dro\u00b7ben", "in", "den", "blau\u00b7en", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie warm der liebe Sonnenschein,", "tokens": ["Wie", "warm", "der", "lie\u00b7be", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Himmel, wie so klar und rein!", "tokens": ["Der", "Him\u00b7mel", ",", "wie", "so", "klar", "und", "rein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie flutet durch die laue Luft", "tokens": ["Wie", "flu\u00b7tet", "durch", "die", "lau\u00b7e", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der abendliche Maienduft", "tokens": ["Der", "a\u00b7bend\u00b7li\u00b7che", "Mai\u00b7en\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von Bl\u00fcten ohne Zahl:", "tokens": ["Von", "Bl\u00fc\u00b7ten", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wie friedlich ruht das Tal,", "tokens": ["Wie", "fried\u00b7lich", "ruht", "das", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie feierlich der Buchwald steht:", "tokens": ["Wie", "fei\u00b7er\u00b7lich", "der", "Buch\u00b7wald", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Rauschen durch die Wipfel geht,", "tokens": ["Ein", "Rau\u00b7schen", "durch", "die", "Wip\u00b7fel", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein Rauschen geht durch Rohr und Ried:", "tokens": ["Ein", "Rau\u00b7schen", "geht", "durch", "Rohr", "und", "Ried", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wird da die Seele nicht zum Lied?", "tokens": ["Wird", "da", "die", "See\u00b7le", "nicht", "zum", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Leg' ab auch du, betr\u00fcbter Gast,", "tokens": ["Leg'", "ab", "auch", "du", ",", "be\u00b7tr\u00fcb\u00b7ter", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "ADV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Last, die du zu tragen hast!\u00ab", "tokens": ["Die", "Last", ",", "die", "du", "zu", "tra\u00b7gen", "hast", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbschwarzamsel hoch im Ulmenast,", "tokens": ["\u00bb", "schwarz\u00b7am\u00b7sel", "hoch", "im", "Ul\u00b7me\u00b7nast", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der Sang, den du gesungen hast,", "tokens": ["Der", "Sang", ",", "den", "du", "ge\u00b7sun\u00b7gen", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist s\u00fc\u00df und hold gewesen: \u2013", "tokens": ["Ist", "s\u00fc\u00df", "und", "hold", "ge\u00b7we\u00b7sen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich macht er nicht genesen:", "tokens": ["Mich", "macht", "er", "nicht", "ge\u00b7ne\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn wiss', es gibt viel schlimmer Leid,", "tokens": ["Denn", "wiss'", ",", "es", "gibt", "viel", "schlim\u00b7mer", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Sturm und Schnee zur Winterzeit:", "tokens": ["Als", "Sturm", "und", "Schnee", "zur", "Win\u00b7ter\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Menschenbrust hegt tiefern Schmerz!", "tokens": ["Die", "Men\u00b7schen\u00b7brust", "hegt", "tie\u00b7fern", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein frohes, kleines Vogelherz", "tokens": ["Dein", "fro\u00b7hes", ",", "klei\u00b7nes", "Vo\u00b7gel\u00b7herz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kann sich's nicht tr\u00e4umen lassen!", "tokens": ["Kann", "sich's", "nicht", "tr\u00e4u\u00b7men", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Es w\u00fcrd' ihn gar nicht fassen:", "tokens": ["Es", "w\u00fcrd'", "ihn", "gar", "nicht", "fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und fa\u00dft' es ihn, so w\u00e4r's vorbei", "tokens": ["Und", "fa\u00dft'", "es", "ihn", ",", "so", "w\u00e4r's", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit seiner jauchzenden Melodei.", "tokens": ["Mit", "sei\u00b7ner", "jauch\u00b7zen\u00b7den", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Ach, was wei\u00dft du von Reu' und Schuld", "tokens": ["Ach", ",", "was", "wei\u00dft", "du", "von", "Reu'", "und", "Schuld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "APPR", "NE", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Und von verlorner Gotteshuld!", "tokens": ["Und", "von", "ver\u00b7lor\u00b7ner", "Got\u00b7tes\u00b7huld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Drum sing' du weiter froh und rein,", "tokens": ["Drum", "sing'", "du", "wei\u00b7ter", "froh", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sing' hell in Gottes Welt hinein", "tokens": ["Sing'", "hell", "in", "Got\u00b7tes", "Welt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "NN", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und la\u00df mit meinen Wehn", "tokens": ["Und", "la\u00df", "mit", "mei\u00b7nen", "Wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Mich meiner Stra\u00dfe gehn.\u00ab", "tokens": ["Mich", "mei\u00b7ner", "Stra\u00b7\u00dfe", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So sang ich einst, von Reu' gequ\u00e4lt!", "tokens": ["So", "sang", "ich", "einst", ",", "von", "Reu'", "ge\u00b7qu\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hat nie gegen Gott gefehlt?", "tokens": ["Wer", "hat", "nie", "ge\u00b7gen", "Gott", "ge\u00b7fehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedoch, ents\u00fchnt durch seine Gnade,", "tokens": ["Je\u00b7doch", ",", "ent\u00b7s\u00fchnt", "durch", "sei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Friedens wandl' ich meine Pfade:", "tokens": ["Voll", "Frie\u00b7dens", "wandl'", "ich", "mei\u00b7ne", "Pfa\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dankbar, wie der V\u00f6glein Schar,", "tokens": ["Und", "dank\u00b7bar", ",", "wie", "der", "V\u00f6\u00b7glein", "Schar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bring' ich ihm Lied und Leben dar. \u2013", "tokens": ["Bring'", "ich", "ihm", "Lied", "und", "Le\u00b7ben", "dar", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "PPER", "NN", "KON", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}