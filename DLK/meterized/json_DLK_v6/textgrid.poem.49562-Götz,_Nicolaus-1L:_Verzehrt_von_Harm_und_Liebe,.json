{"textgrid.poem.49562": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verzehrt von Harm und Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzehrt von Harm und Liebe,", "tokens": ["Ver\u00b7zehrt", "von", "Harm", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward ", "tokens": ["Ward"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und wer des Br\u00fcnngens trinket,", "tokens": ["Und", "wer", "des", "Br\u00fcnn\u00b7gens", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergi\u00dfet das Geliebte;", "tokens": ["Ver\u00b7gi\u00b7\u00dfet", "das", "Ge\u00b7lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vergi\u00dft selbst seinen Namen.", "tokens": ["Ver\u00b7gi\u00dft", "selbst", "sei\u00b7nen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Serenen zu verge\u00dfen,", "tokens": ["Se\u00b7re\u00b7nen", "zu", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wollt' ich des Br\u00fcnngens trinken.", "tokens": ["Wollt'", "ich", "des", "Br\u00fcnn\u00b7gens", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergebens. Denn sie hatte,", "tokens": ["Ver\u00b7ge\u00b7bens", ".", "Denn", "sie", "hat\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Weil sie so oft im Lieben", "tokens": ["Weil", "sie", "so", "oft", "im", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Gewechselt und getrunken,", "tokens": ["Ge\u00b7wech\u00b7selt", "und", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das Br\u00fcnngen ausgetrunken.", "tokens": ["Das", "Br\u00fcnn\u00b7gen", "aus\u00b7ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}