{"textgrid.poem.55311": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[motto]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weite Welt und breites Leben,", "tokens": ["Wei\u00b7te", "Welt", "und", "brei\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Langer Jahre redlich Streben,", "tokens": ["Lan\u00b7ger", "Jah\u00b7re", "red\u00b7lich", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets geforscht und stets gegr\u00fcndet,", "tokens": ["Stets", "ge\u00b7forscht", "und", "stets", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie geschlossen, oft ger\u00fcndet,", "tokens": ["Nie", "ge\u00b7schlos\u00b7sen", ",", "oft", "ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00c4ltestes bewahrt mit Treue,", "tokens": ["\u00c4l\u00b7tes\u00b7tes", "be\u00b7wahrt", "mit", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundlich aufgefa\u00dftes Neue,", "tokens": ["Freund\u00b7lich", "auf\u00b7ge\u00b7fa\u00df\u00b7tes", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heitern Sinn und reine Zwecke:", "tokens": ["Hei\u00b7tern", "Sinn", "und", "rei\u00b7ne", "Zwe\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun! man kommt wohl eine Strecke.", "tokens": ["Nun", "!", "man", "kommt", "wohl", "ei\u00b7ne", "Stre\u00b7cke", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}