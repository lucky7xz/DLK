{"dta.poem.18094": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Sonnete.  \n Aus dem Lateinischen Adeodati Seb\u00e4.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heint als der Monde war in seinen Krei\u00df gezogen,", "tokens": ["Heint", "als", "der", "Mon\u00b7de", "war", "in", "sei\u00b7nen", "Krei\u00df", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mich der s\u00fcsse Schlaf umfangen durch die Nacht,", "tokens": ["Und", "mich", "der", "s\u00fcs\u00b7se", "Schlaf", "um\u00b7fan\u00b7gen", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ward mir mein Augentrost im Traume f\u00fcrgebracht,", "tokens": ["Ward", "mir", "mein", "Au\u00b7gen\u00b7trost", "im", "Trau\u00b7me", "f\u00fcr\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als l\u00e4ge sie bey mir an meine Brust gebogen,", "tokens": ["Als", "l\u00e4\u00b7ge", "sie", "bey", "mir", "an", "mei\u00b7ne", "Brust", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Jhr Hertze war in mich, mein Hertz in sie geflogen;", "tokens": ["Ihr", "Hert\u00b7ze", "war", "in", "mich", ",", "mein", "Hertz", "in", "sie", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fand aber g\u00e4ntzlich nichts, wie ich des Morgens wacht,", "tokens": ["Fand", "a\u00b7ber", "g\u00e4ntz\u00b7lich", "nichts", ",", "wie", "ich", "des", "Mor\u00b7gens", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PIS", "$,", "PWAV", "PPER", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hielt die Lacken in den Armen, drum ich lacht,", "tokens": ["Und", "hielt", "die", "La\u00b7cken", "in", "den", "Ar\u00b7men", ",", "drum", "ich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ich recht innen ward, da\u00df ich so sehr betrogen.", "tokens": ["Als", "ich", "recht", "in\u00b7nen", "ward", ",", "da\u00df", "ich", "so", "sehr", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verr\u00e4ther, loser Traum, warum denn fleuchst du bald?", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", ",", "lo\u00b7ser", "Traum", ",", "wa\u00b7rum", "denn", "fleuchst", "du", "bald", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich doch l\u00e4nger sehn die liebliche Gestalt,", "tokens": ["La\u00df", "mich", "doch", "l\u00e4n\u00b7ger", "sehn", "die", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df sich doch mehr bey mir di\u00df sch\u00f6ne Vorbild s\u00e4umen.", "tokens": ["La\u00df", "sich", "doch", "mehr", "bey", "mir", "di\u00df", "sch\u00f6\u00b7ne", "Vor\u00b7bild", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "ADV", "ADV", "APPR", "PPER", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Betrieger, krieg ich nichts als Hohn und Spott von ihr,", "tokens": ["Be\u00b7trie\u00b7ger", ",", "krieg", "ich", "nichts", "als", "Hohn", "und", "Spott", "von", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihrer Sch\u00f6nheit Ros\u2019, ach bitt ich, la\u00df doch mir", "tokens": ["Und", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Ros'", ",", "ach", "bitt", "ich", ",", "la\u00df", "doch", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,", "ITJ", "VVFIN", "PPER", "$,", "VVIMP", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drey tausend Jahr so s\u00fc\u00df, ohn alles Wachen, tr\u00e4umen.", "tokens": ["Drey", "tau\u00b7send", "Jahr", "so", "s\u00fc\u00df", ",", "ohn", "al\u00b7les", "Wa\u00b7chen", ",", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["CARD", "CARD", "NN", "ADV", "ADJD", "$,", "KOUI", "PIAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}