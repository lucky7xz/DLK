{"dta.poem.404": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sich  Meurine  \u00fcber einen Ku\u00df erz\u00fcrnete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Meurine z\u00fcrnet nur/ ich will den Zorn ertragen/", "tokens": ["Meu\u00b7ri\u00b7ne", "z\u00fcr\u00b7net", "nur", "/", "ich", "will", "den", "Zorn", "er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Mund will sich dennoch zu euren Lippen wagen/", "tokens": ["Der", "Mund", "will", "sich", "den\u00b7noch", "zu", "eu\u00b7ren", "Lip\u00b7pen", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wer da Rosen pfl\u00fcckt/ der achtet es nicht viel", "tokens": ["Denn", "wer", "da", "Ro\u00b7sen", "pfl\u00fcckt", "/", "der", "ach\u00b7tet", "es", "nicht", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "NN", "VVFIN", "$(", "ART", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihm zuweilen schon ein D\u00f6rnlein stechen will.", "tokens": ["Wenn", "ihm", "zu\u00b7wei\u00b7len", "schon", "ein", "D\u00f6rn\u00b7lein", "ste\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Dornen ist der Weg des Lebens \u00fcberstreuet/", "tokens": ["Mit", "Dor\u00b7nen", "ist", "der", "Weg", "des", "Le\u00b7bens", "\u00fc\u00b7bers\u00b7treu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie/ meynt ihr/ da\u00df mein Geist sich vor den Dornen scheuet?", "tokens": ["Wie", "/", "meynt", "ihr", "/", "da\u00df", "mein", "Geist", "sich", "vor", "den", "Dor\u00b7nen", "scheu\u00b7et", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nein! denn der Ausgang macht da\u00df man den Schmertz vergi\u00dft/", "tokens": ["Nein", "!", "denn", "der", "Aus\u00b7gang", "macht", "da\u00df", "man", "den", "Schmertz", "ver\u00b7gi\u00dft", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "ART", "NN", "VVFIN", "KOUS", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So bald die Dornen weg/ alsdenn man Rosen k\u00fc\u00dft.", "tokens": ["So", "bald", "die", "Dor\u00b7nen", "weg", "/", "als\u00b7denn", "man", "Ro\u00b7sen", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$(", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}