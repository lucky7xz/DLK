{"textgrid.poem.60468": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durstig zieht die Karawane", "tokens": ["Durs\u00b7tig", "zieht", "die", "Ka\u00b7ra\u00b7wa\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die W\u00fcste, sucht die Quelle;", "tokens": ["Durch", "die", "W\u00fcs\u00b7te", ",", "sucht", "die", "Quel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch! da rauscht auf gr\u00fcner Matte", "tokens": ["Horch", "!", "da", "rauscht", "auf", "gr\u00fc\u00b7ner", "Mat\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ersehnte, frische, helle!", "tokens": ["Die", "er\u00b7sehn\u00b7te", ",", "fri\u00b7sche", ",", "hel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach dem s\u00fc\u00dfen Brunnenklange", "tokens": ["Nach", "dem", "s\u00fc\u00b7\u00dfen", "Brun\u00b7nen\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzen alle froh und eilig,", "tokens": ["St\u00fcr\u00b7zen", "al\u00b7le", "froh", "und", "ei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie sollen hier nicht trinken,", "tokens": ["Doch", "sie", "sol\u00b7len", "hier", "nicht", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es ist der Brunnen heilig.", "tokens": ["Denn", "es", "ist", "der", "Brun\u00b7nen", "hei\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auserw\u00e4hlte M\u00e4nner nahmen", "tokens": ["Au\u00b7ser\u00b7w\u00e4hl\u00b7te", "M\u00e4n\u00b7ner", "nah\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Oase sich zu eigen,", "tokens": ["Die", "O\u00b7a\u00b7se", "sich", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand sonst, wie hei\u00df er schmachte,", "tokens": ["Nie\u00b7mand", "sonst", ",", "wie", "hei\u00df", "er", "schmach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf zum Quell die Lippen neigen.", "tokens": ["Darf", "zum", "Quell", "die", "Lip\u00b7pen", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "W\u00e4chter stehen vor der Quelle", "tokens": ["W\u00e4ch\u00b7ter", "ste\u00b7hen", "vor", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichen, gottvergo\u00dfnen Wonnen;", "tokens": ["Rei\u00b7chen", ",", "gott\u00b7ver\u00b7go\u00df\u00b7nen", "Won\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der W\u00fcstendurst ist m\u00e4chtig,", "tokens": ["Doch", "der", "W\u00fcs\u00b7ten\u00b7durst", "ist", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schwerter klirren um den Bronnen.", "tokens": ["Schwer\u00b7ter", "klir\u00b7ren", "um", "den", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und mit kampferh\u00f6htem Durste", "tokens": ["Und", "mit", "kamp\u00b7fer\u00b7h\u00f6h\u00b7tem", "Durs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzen an den Quell die Sieger,", "tokens": ["St\u00fcr\u00b7zen", "an", "den", "Quell", "die", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie trinken gierig, hastig,", "tokens": ["Und", "sie", "trin\u00b7ken", "gie\u00b7rig", ",", "has\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie das Blut der hei\u00dfe Tiger.", "tokens": ["Wie", "das", "Blut", "der", "hei\u00b7\u00dfe", "Ti\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mancher, schon vom Schwert getroffen,", "tokens": ["Man\u00b7cher", ",", "schon", "vom", "Schwert", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00fcrft noch einen vollen Zug,", "tokens": ["Schl\u00fcrft", "noch", "ei\u00b7nen", "vol\u00b7len", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Seele zu erfrischen", "tokens": ["Um", "die", "See\u00b7le", "zu", "er\u00b7fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den weiten Scheideflug.", "tokens": ["Auf", "den", "wei\u00b7ten", "Schei\u00b7de\u00b7flug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tigerhaft gereizten Durstes", "tokens": ["Ti\u00b7ger\u00b7haft", "ge\u00b7reiz\u00b7ten", "Durs\u00b7tes"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmachten Ziskas Kampfgenossen", "tokens": ["Schmach\u00b7ten", "Zis\u00b7kas", "Kampf\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Kelch des Abendmahles,", "tokens": ["Nach", "dem", "Kelch", "des", "A\u00b7bend\u00b7mah\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den die Priester streng verschlossen.", "tokens": ["Den", "die", "Pries\u00b7ter", "streng", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Furchtbar rufen sie den Priestern:", "tokens": ["Furcht\u00b7bar", "ru\u00b7fen", "sie", "den", "Pries\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhabt ihr Christi Werk auf Erden,", "tokens": ["\u00bb", "habt", "ihr", "Chris\u00b7ti", "Werk", "auf", "Er\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "NE", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns das Sakrament verst\u00fcmmelt,", "tokens": ["Uns", "das", "Sak\u00b7ra\u00b7ment", "ver\u00b7st\u00fcm\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt ihr selbst verst\u00fcmmelt werden!\u00ab", "tokens": ["Sollt", "ihr", "selbst", "ver\u00b7st\u00fcm\u00b7melt", "wer\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jauchzend schwingen sie die Kelche", "tokens": ["Jauch\u00b7zend", "schwin\u00b7gen", "sie", "die", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Schlacht auf offner Wiese,", "tokens": ["Nach", "der", "Schlacht", "auf", "off\u00b7ner", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher sterbend riecht im Weine", "tokens": ["Man\u00b7cher", "ster\u00b7bend", "riecht", "im", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen schon vom Paradiese.", "tokens": ["Blu\u00b7men", "schon", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mit dem Blut des Liebevollsten", "tokens": ["Mit", "dem", "Blut", "des", "Lie\u00b7be\u00b7volls\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will des Hasses Glut sich laben;", "tokens": ["Will", "des", "Has\u00b7ses", "Glut", "sich", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcben aber werden Tote", "tokens": ["Dr\u00fc\u00b7ben", "a\u00b7ber", "wer\u00b7den", "To\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Verst\u00fcmmelten begraben.", "tokens": ["Von", "Ver\u00b7st\u00fcm\u00b7mel\u00b7ten", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn der lang und schwer Bedr\u00fcckte", "tokens": ["Wenn", "der", "lang", "und", "schwer", "Be\u00b7dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit sucht, so ha\u00dft der Wilde", "tokens": ["Frei\u00b7heit", "sucht", ",", "so", "ha\u00dft", "der", "Wil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zerbricht, wie andre Schranken,", "tokens": ["Und", "zer\u00b7bricht", ",", "wie", "and\u00b7re", "Schran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch das eignen Herzens Milde.", "tokens": ["Auch", "das", "eig\u00b7nen", "Her\u00b7zens", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}