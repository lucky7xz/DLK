{"textgrid.poem.23971": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mmerung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmerung mit den milden, grauen Augen", "tokens": ["D\u00e4m\u00b7me\u00b7rung", "mit", "den", "mil\u00b7den", ",", "grau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schreitet \u00fcber die Erde.", "tokens": ["Schrei\u00b7tet", "\u00fc\u00b7ber", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "K\u00fchl weht ihr Atem,", "tokens": ["K\u00fchl", "weht", "ihr", "A\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Weich und k\u00fchl,", "tokens": ["Weich", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Milde wie ruhiger Atemzug", "tokens": ["Mil\u00b7de", "wie", "ru\u00b7hi\u00b7ger", "A\u00b7tem\u00b7zug"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Eines schlummergek\u00fc\u00dften,", "tokens": ["Ei\u00b7nes", "schlum\u00b7mer\u00b7ge\u00b7k\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Backenroten Kindes.", "tokens": ["Ba\u00b7cken\u00b7ro\u00b7ten", "Kin\u00b7des", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "An lauschender Ferne ruhendem Rund", "tokens": ["An", "lau\u00b7schen\u00b7der", "Fer\u00b7ne", "ru\u00b7hen\u00b7dem", "Rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein goldenes Gl\u00e4nzen, matt verscheidend,", "tokens": ["Ein", "gol\u00b7de\u00b7nes", "Gl\u00e4n\u00b7zen", ",", "matt", "ver\u00b7schei\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Zerrinnend in zarten, grauen Duft ...", "tokens": ["Zer\u00b7rin\u00b7nend", "in", "zar\u00b7ten", ",", "grau\u00b7en", "Duft", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Oh Ruhe! Ruhe! Gabe der Seligkeit,", "tokens": ["Oh", "Ru\u00b7he", "!", "Ru\u00b7he", "!", "Ga\u00b7be", "der", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$.", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die du auf Fl\u00fcgeln der D\u00e4mmerung linde", "tokens": ["Die", "du", "auf", "Fl\u00fc\u00b7geln", "der", "D\u00e4m\u00b7me\u00b7rung", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Vom Himmel niederschwebst, gelinde", "tokens": ["Vom", "Him\u00b7mel", "nie\u00b7der\u00b7schwebst", ",", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Das Herz mit warmem Hauche,", "tokens": ["Das", "Herz", "mit", "war\u00b7mem", "Hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Sorgenscheuchend, r\u00fchrst;", "tokens": ["Sor\u00b7gen\u00b7scheu\u00b7chend", ",", "r\u00fchrst", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Oh Ruhe, Frieden, F\u00fclle des Seins!", "tokens": ["Oh", "Ru\u00b7he", ",", "Frie\u00b7den", ",", "F\u00fcl\u00b7le", "des", "Seins", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$,", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Heut aus grauen D\u00e4mmeraugen", "tokens": ["Heut", "aus", "grau\u00b7en", "D\u00e4m\u00b7mer\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Blickst du mich liebreich an und verhei\u00dfend,", "tokens": ["Blickst", "du", "mich", "lieb\u00b7reich", "an", "und", "ver\u00b7hei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "KON", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und mein Dank schwillt auf im Herzen,", "tokens": ["Und", "mein", "Dank", "schwillt", "auf", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wie im Auge der seligen Braut", "tokens": ["Wie", "im", "Au\u00b7ge", "der", "se\u00b7li\u00b7gen", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.21": {"text": "Warme, lachende Thr\u00e4nenflut, \u2013", "tokens": ["War\u00b7me", ",", "la\u00b7chen\u00b7de", "Thr\u00e4\u00b7nen\u00b7flut", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Aber mein Herz mu\u00df an verklungene", "tokens": ["A\u00b7ber", "mein", "Herz", "mu\u00df", "an", "ver\u00b7klun\u00b7ge\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "ADJA"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Tage h\u00f6heren Gl\u00fcckes denken,", "tokens": ["Ta\u00b7ge", "h\u00f6\u00b7he\u00b7ren", "Gl\u00fc\u00b7ckes", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Da ihm friedevolle Liebe", "tokens": ["Da", "ihm", "frie\u00b7de\u00b7vol\u00b7le", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "G\u00fctig fromm entgegenleuchtete", "tokens": ["G\u00fc\u00b7tig", "fromm", "ent\u00b7ge\u00b7gen\u00b7leuch\u00b7te\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Aus zwei braunen M\u00e4dchenaugen,", "tokens": ["Aus", "zwei", "brau\u00b7nen", "M\u00e4d\u00b7chen\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sonnen der Liebe.", "tokens": ["Son\u00b7nen", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}