{"textgrid.poem.41905": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling, der mit leisen Schwingen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling, der mit leisen Schwingen", "tokens": ["Fr\u00fch\u00b7ling", ",", "der", "mit", "lei\u00b7sen", "Schwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lau mir um die Wangen spielt,", "tokens": ["Lau", "mir", "um", "die", "Wan\u00b7gen", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, du kannst nicht wiederbringen,", "tokens": ["Ach", ",", "du", "kannst", "nicht", "wie\u00b7der\u00b7brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich einst in dir gef\u00fchlt!", "tokens": ["Was", "ich", "einst", "in", "dir", "ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In des Haines gr\u00fcner Halle,", "tokens": ["In", "des", "Hai\u00b7nes", "gr\u00fc\u00b7ner", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des Baches hellem Lauf", "tokens": ["An", "des", "Ba\u00b7ches", "hel\u00b7lem", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weckest du die Lieder alle,", "tokens": ["We\u00b7ckest", "du", "die", "Lie\u00b7der", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Blumen wieder auf;", "tokens": ["Al\u00b7le", "Blu\u00b7men", "wie\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch kehren jene Lieder,", "tokens": ["Und", "doch", "keh\u00b7ren", "je\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Gl\u00fccklichen entz\u00fcckt,", "tokens": ["Die", "den", "Gl\u00fcck\u00b7li\u00b7chen", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Blumen nimmer wieder,", "tokens": ["Je\u00b7ne", "Blu\u00b7men", "nim\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Haupt bleibt ungeschm\u00fcckt.", "tokens": ["Und", "mein", "Haupt", "bleibt", "un\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fremde Bilder seh' ich schweifen,", "tokens": ["Frem\u00b7de", "Bil\u00b7der", "seh'", "ich", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00e4thsel, neu und wunderbar,", "tokens": ["R\u00e4th\u00b7sel", ",", "neu", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Herz kann nicht begreifen,", "tokens": ["Und", "mein", "Herz", "kann", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihm sonst so deutlich war.", "tokens": ["Was", "ihm", "sonst", "so", "deut\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dieses Duften, dieses Prangen,", "tokens": ["Die\u00b7ses", "Duf\u00b7ten", ",", "die\u00b7ses", "Pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat es einst mich doch erg\u00f6tzt;", "tokens": ["Hat", "es", "einst", "mich", "doch", "er\u00b7g\u00f6tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum rinnt mir von den Wangen", "tokens": ["Wa\u00b7rum", "rinnt", "mir", "von", "den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die bittre Thr\u00e4ne jetzt?", "tokens": ["Denn", "die", "bitt\u00b7re", "Thr\u00e4\u00b7ne", "jetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6nes, dacht' ich, seh' ich bl\u00fchen,", "tokens": ["Sch\u00f6\u00b7nes", ",", "dacht'", "ich", ",", "seh'", "ich", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Sch\u00f6nre folgt ihm nach. \u2013", "tokens": ["Und", "das", "Sch\u00f6n\u00b7re", "folgt", "ihm", "nach", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, des Menschen W\u00fcnsche fliehen", "tokens": ["Ach", ",", "des", "Men\u00b7schen", "W\u00fcn\u00b7sche", "flie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spurlos wie die Well' im Bach!", "tokens": ["Spur\u00b7los", "wie", "die", "Well'", "im", "Bach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O du rasches junges Leben,", "tokens": ["O", "du", "ra\u00b7sches", "jun\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig wechselnd, ohne Ruh,", "tokens": ["E\u00b7wig", "wech\u00b7selnd", ",", "oh\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durftest du mir Treue geben,", "tokens": ["Durf\u00b7test", "du", "mir", "Treu\u00b7e", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Hoffnung nicht dazu?", "tokens": ["Und", "die", "Hoff\u00b7nung", "nicht", "da\u00b7zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}