{"dta.poem.2766": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die verschertzte Freyheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach unertr\u00e4glich Joch/", "tokens": ["Ach", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "Joch", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An dem ich Armer noch", "tokens": ["An", "dem", "ich", "Ar\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Leben mu\u00df verschliessen!", "tokens": ["Mein", "Le\u00b7ben", "mu\u00df", "ver\u00b7schlies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn werd ich doch entrissen", "tokens": ["Wenn", "werd", "ich", "doch", "ent\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der strengen Dieustbarkeit/", "tokens": ["Der", "stren\u00b7gen", "Die\u00b7ust\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die mir k\u00fcrtzt meine Zeit!", "tokens": ["Die", "mir", "k\u00fcrtzt", "mei\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ist denn ein freyer Muth", "tokens": ["Ist", "denn", "ein", "frey\u00b7er", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dienstschuldig was er thut?", "tokens": ["Dienst\u00b7schul\u00b7dig", "was", "er", "thut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mu\u00df denn mein Willen eben", "tokens": ["Mu\u00df", "denn", "mein", "Wil\u00b7len", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nach eurem Wollen leben?", "tokens": ["Nach", "eu\u00b7rem", "Wol\u00b7len", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bald sagen Ja/ bald Nein/", "tokens": ["Bald", "sa\u00b7gen", "Ja", "/", "bald", "Nein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKANT", "$(", "ADV", "PTKANT", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Das geht mir bitter ein.", "tokens": ["Das", "geht", "mir", "bit\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was thut nicht das Gel\u00fcck", "tokens": ["Was", "thut", "nicht", "das", "Ge\u00b7l\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und seine b\u00f6se T\u00fcck?", "tokens": ["Und", "sei\u00b7ne", "b\u00f6\u00b7se", "T\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Da ich jetzt solte siegen/", "tokens": ["Da", "ich", "jetzt", "sol\u00b7te", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Mu\u00df ich zu Hofe ligen/", "tokens": ["Mu\u00df", "ich", "zu", "Ho\u00b7fe", "li\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und was mich mehr geht an.", "tokens": ["Und", "was", "mich", "mehr", "geht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Selbst seyn ein Unterthan.", "tokens": ["Selbst", "seyn", "ein", "Un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Hilff Himmel! Ist es recht?", "tokens": ["Hilff", "Him\u00b7mel", "!", "Ist", "es", "recht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Dein Sohn wird jetzt ein Knecht/", "tokens": ["Dein", "Sohn", "wird", "jetzt", "ein", "Knecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "L\u00e4st sich in Schrancken treiben/", "tokens": ["L\u00e4st", "sich", "in", "Schran\u00b7cken", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.22": {"text": "Und ihm Befehl f\u00fcrschreiben/", "tokens": ["Und", "ihm", "Be\u00b7fehl", "f\u00fcrsc\u00b7hrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Thut was ein feyger Muth", "tokens": ["Thut", "was", "ein", "fey\u00b7ger", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Sonst aus Verzweifflung thut.", "tokens": ["Sonst", "aus", "Ver\u00b7zweif\u00b7flung", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Erz\u00f6rnte Himmels-Rach/", "tokens": ["Er\u00b7z\u00f6rn\u00b7te", "Him\u00b7mels\u00b7Rach", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "La\u00df doch nur einmahl nach", "tokens": ["La\u00df", "doch", "nur", "ein\u00b7mahl", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Auff meinen Kopff zu blitzen.", "tokens": ["Auff", "mei\u00b7nen", "Kopff", "zu", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wo du mich nicht willst sch\u00fctzen/", "tokens": ["Wo", "du", "mich", "nicht", "willst", "sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "So kan ich nicht bestehn/", "tokens": ["So", "kan", "ich", "nicht", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Und mu\u00df zu Grunde gehn.", "tokens": ["Und", "mu\u00df", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Dem vor die Welt zu klein/", "tokens": ["Dem", "vor", "die", "Welt", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Geht jetzt Gehorsam ein/", "tokens": ["Geht", "jetzt", "Ge\u00b7hor\u00b7sam", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.33": {"text": "Der auff die Freyheit trutzte/", "tokens": ["Der", "auff", "die", "Frey\u00b7heit", "trutz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "In ihrem Purpur stutzte/", "tokens": ["In", "ih\u00b7rem", "Pur\u00b7pur", "stutz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Tr\u00e4gt nun O Jammer-Stand/", "tokens": ["Tr\u00e4gt", "nun", "O", "Jam\u00b7mer\u00b7Stand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Der Fessel schweres Band.", "tokens": ["Der", "Fes\u00b7sel", "schwe\u00b7res", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Gefangen bin ich nicht/", "tokens": ["Ge\u00b7fan\u00b7gen", "bin", "ich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Und gleichwohl doch verpflicht/", "tokens": ["Und", "gleich\u00b7wohl", "doch", "ver\u00b7pflicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "So bald der Tag erschienen/", "tokens": ["So", "bald", "der", "Tag", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Mu\u00df ich zu Gnaden dienen/", "tokens": ["Mu\u00df", "ich", "zu", "Gna\u00b7den", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Und tret auf schl\u00fcpffrig Ei\u00df/", "tokens": ["Und", "tret", "auf", "schl\u00fcpf\u00b7frig", "Ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Das ich gebrechlich wei\u00df.", "tokens": ["Das", "ich", "ge\u00b7brech\u00b7lich", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Was ich zuvor lacht aus/", "tokens": ["Was", "ich", "zu\u00b7vor", "lacht", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Das kommt mir jetzt zu Hau\u00df.", "tokens": ["Das", "kommt", "mir", "jetzt", "zu", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Vor kont\u2019 ich Herrschafft \u00fcben/", "tokens": ["Vor", "kont'", "ich", "Herr\u00b7schafft", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Nun mu\u00df ich mich betr\u00fcben/", "tokens": ["Nun", "mu\u00df", "ich", "mich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Da\u00df ich nach frembdem Sinn", "tokens": ["Da\u00df", "ich", "nach", "fremb\u00b7dem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Fast ein Leibeigner bin.", "tokens": ["Fast", "ein", "Leib\u00b7eig\u00b7ner", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "Welt/ Ehre/ Ruhm und Pracht/", "tokens": ["Welt", "/", "Eh\u00b7re", "/", "Ruhm", "und", "Pracht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.50": {"text": "Euch sag ich gute Nacht.", "tokens": ["Euch", "sag", "ich", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Dich/ Dich/ such ich durch Lieder/", "tokens": ["Dich", "/", "Dich", "/", "such", "ich", "durch", "Lie\u00b7der", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "$(", "VVIMP", "PPER", "APPR", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.52": {"text": "Komm meine Freyheit wieder/", "tokens": ["Komm", "mei\u00b7ne", "Frey\u00b7heit", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Und mach mich doch nur frey", "tokens": ["Und", "mach", "mich", "doch", "nur", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.54": {"text": "Von dieser Sclaverey.", "tokens": ["Von", "die\u00b7ser", "Scla\u00b7ve\u00b7rey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Wie freudig wil ichs sehn/", "tokens": ["Wie", "freu\u00b7dig", "wil", "ichs", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.56": {"text": "Wenn dieses wird geschehn/", "tokens": ["Wenn", "die\u00b7ses", "wird", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Da\u00df mein vers\u00f6hnt Gel\u00fccke", "tokens": ["Da\u00df", "mein", "ver\u00b7s\u00f6hnt", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Mir abnimmt seine Stricke/", "tokens": ["Mir", "ab\u00b7nimmt", "sei\u00b7ne", "Stri\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Und l\u00e4ss\u2019t mich in der Ruh", "tokens": ["Und", "l\u00e4ss't", "mich", "in", "der", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.60": {"text": "Mein Leben bringen zu.", "tokens": ["Mein", "Le\u00b7ben", "brin\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}