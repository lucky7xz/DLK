{"dta.poem.4150": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Hirtenfeuer in der r\u00f6mischen Ebene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und reich' mir deine Hand!", "tokens": ["Und", "reich'", "mir", "dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich treibe meine Heerde", "tokens": ["Ich", "trei\u00b7be", "mei\u00b7ne", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinab in's Niederland.", "tokens": ["Hin\u00b7ab", "in's", "Nie\u00b7der\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Saaten sind gem\u00e4het,", "tokens": ["Die", "Saa\u00b7ten", "sind", "ge\u00b7m\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Stoppelfeld ist frei:", "tokens": ["Das", "Stop\u00b7pel\u00b7feld", "ist", "frei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df uns mit blauem Bande", "tokens": ["La\u00df", "uns", "mit", "blau\u00b7em", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Verkn\u00fcpfen Lieb' und Treu'.", "tokens": ["Ver\u00b7kn\u00fcp\u00b7fen", "Lieb'", "und", "Treu'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich trag' es auf dem Hute,", "tokens": ["Ich", "trag'", "es", "auf", "dem", "Hu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du tr\u00e4gst es auf der Brust,", "tokens": ["Du", "tr\u00e4gst", "es", "auf", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und pocht dein Herz dagegen,", "tokens": ["Und", "pocht", "dein", "Herz", "da\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich f\u00fchl's in banger Lust.", "tokens": ["Ich", "f\u00fchl's", "in", "ban\u00b7ger", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schaust du herab vom Berge", "tokens": ["Schaust", "du", "her\u00b7ab", "vom", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in der dunkeln Nacht,", "tokens": ["Wohl", "in", "der", "dun\u00b7keln", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tief unten brennt ein Feuer,", "tokens": ["Tief", "un\u00b7ten", "brennt", "ein", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo dein Geliebter wacht.", "tokens": ["Wo", "dein", "Ge\u00b7lieb\u00b7ter", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und h\u00f6her schl\u00e4gt die Lohe,", "tokens": ["Und", "h\u00f6\u00b7her", "schl\u00e4gt", "die", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und heller gl\u00fcht der Schein:", "tokens": ["Und", "hel\u00b7ler", "gl\u00fcht", "der", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann denk', es ist sein Herze,", "tokens": ["Dann", "denk'", ",", "es", "ist", "sein", "Her\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das will hier oben sein.", "tokens": ["Das", "will", "hier", "o\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}