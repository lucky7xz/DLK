{"textgrid.poem.47410": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was ist der Lenz? Ein Commentar]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Lenz? Ein Commentar;", "tokens": ["Was", "ist", "der", "Lenz", "?", "Ein", "Com\u00b7men\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er machet deine Sch\u00f6ne klar;", "tokens": ["Er", "ma\u00b7chet", "dei\u00b7ne", "Sch\u00f6\u00b7ne", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er legt, so weit es seinem M\u00fch'n,", "tokens": ["Er", "legt", ",", "so", "weit", "es", "sei\u00b7nem", "M\u00fch'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem freilich unzureichlichen,", "tokens": ["Dem", "frei\u00b7lich", "un\u00b7zu\u00b7reich\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Doch eifrigen, gelingen mag,", "tokens": ["Doch", "eif\u00b7ri\u00b7gen", ",", "ge\u00b7lin\u00b7gen", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die F\u00fclle deiner Reize dar;", "tokens": ["Die", "F\u00fcl\u00b7le", "dei\u00b7ner", "Rei\u00b7ze", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn nur von ihnen handeln", "tokens": ["Denn", "nur", "von", "ih\u00b7nen", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Licht, Blume, Duft all\u00fcberall", "tokens": ["Licht", ",", "Blu\u00b7me", ",", "Duft", "al\u00b7l\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wo meine F\u00fc\u00dfe wandeln.", "tokens": ["Wo", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}