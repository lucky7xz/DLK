{"dta.poem.11889": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlthaten sind wol gut, und wohl dem der sie thut!", "tokens": ["Wohlt\u00b7ha\u00b7ten", "sind", "wol", "gut", ",", "und", "wohl", "dem", "der", "sie", "thut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ADV", "ART", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohlwollen aber ist viel besser noch als gut.", "tokens": ["Wohl\u00b7wol\u00b7len", "a\u00b7ber", "ist", "viel", "bes\u00b7ser", "noch", "als", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADV", "ADJD", "ADV", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohlthat wird Armen nur und Lebenden entboten,", "tokens": ["Wohlt\u00b7hat", "wird", "Ar\u00b7men", "nur", "und", "Le\u00b7ben\u00b7den", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ADV", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohlwollen arm und reich den Lebenden und Todten.", "tokens": ["Wohl\u00b7wol\u00b7len", "arm", "und", "reich", "den", "Le\u00b7ben\u00b7den", "und", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}