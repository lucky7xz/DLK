{"textgrid.poem.59535": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lebt ein Geist, durch welchen alles lebt,", "tokens": ["Es", "lebt", "ein", "Geist", ",", "durch", "wel\u00b7chen", "al\u00b7les", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PWAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch den die Sonne kreist,", "tokens": ["Durch", "den", "die", "Son\u00b7ne", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Blumenbusch die goldnen K\u00f6pfchen hebt,", "tokens": ["Der", "Blu\u00b7men\u00b7busch", "die", "gold\u00b7nen", "K\u00f6pf\u00b7chen", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Lenz der Vogel preist;", "tokens": ["Den", "Lenz", "der", "Vo\u00b7gel", "preist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Durch den das Menschenherz, das Wunderding,", "tokens": ["Durch", "den", "das", "Men\u00b7schen\u00b7herz", ",", "das", "Wun\u00b7der\u00b7ding", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor eignen Wundern bebt,", "tokens": ["Vor", "eig\u00b7nen", "Wun\u00b7dern", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann er es m\u00e4chtig zu dem Sonnenring", "tokens": ["Wann", "er", "es", "m\u00e4ch\u00b7tig", "zu", "dem", "Son\u00b7nen\u00b7ring"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In tiefster Sehnsucht hebt.", "tokens": ["In", "tiefs\u00b7ter", "Sehn\u00b7sucht", "hebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Geist der Geister, knieend bet' ich an,", "tokens": ["O", "Geist", "der", "Geis\u00b7ter", ",", "kni\u00b7e\u00b7end", "bet'", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was keine Zunge spricht;", "tokens": ["Was", "kei\u00b7ne", "Zun\u00b7ge", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieh, ew'ges Licht, den kleinen Funken an,", "tokens": ["Zieh", ",", "ew'\u00b7ges", "Licht", ",", "den", "klei\u00b7nen", "Fun\u00b7ken", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er will zu deinem Licht.", "tokens": ["Er", "will", "zu", "dei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er flo\u00df vom sel'gen G\u00f6tterlande aus", "tokens": ["Er", "flo\u00df", "vom", "sel'\u00b7gen", "G\u00f6t\u00b7ter\u00b7lan\u00b7de", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Herab zur Erdenflur", "tokens": ["Her\u00b7ab", "zur", "Er\u00b7den\u00b7flur"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sehnt sich ewig nach dem Sonnenhaus,", "tokens": ["Und", "sehnt", "sich", "e\u00b7wig", "nach", "dem", "Son\u00b7nen\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach himmlischer Natur.", "tokens": ["Nach", "himm\u00b7li\u00b7scher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Geist der Geister, trage mich empor!", "tokens": ["O", "Geist", "der", "Geis\u00b7ter", ",", "tra\u00b7ge", "mich", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und mache ganz mich dein!", "tokens": ["Und", "ma\u00b7che", "ganz", "mich", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist mein Vaterland, was ich verlor:", "tokens": ["Es", "ist", "mein", "Va\u00b7ter\u00b7land", ",", "was", "ich", "ver\u00b7lor", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Himmel ist ja mein.", "tokens": ["Der", "Him\u00b7mel", "ist", "ja", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}