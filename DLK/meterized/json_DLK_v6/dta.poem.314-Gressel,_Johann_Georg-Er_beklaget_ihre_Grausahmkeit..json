{"dta.poem.314": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er beklaget ihre Grausahmkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Tyrannin meiner Brust la\u00df ab mich so zu qu\u00e4hlen/", "tokens": ["Ty\u00b7ran\u00b7nin", "mei\u00b7ner", "Brust", "la\u00df", "ab", "mich", "so", "zu", "qu\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVIMP", "PTKVZ", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6r\u2019 auf mit deinem Grimm\u2019/ und schone meiner Seelen/", "tokens": ["H\u00f6r'", "auf", "mit", "dei\u00b7nem", "Grimm'", "/", "und", "scho\u00b7ne", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es bringt zwar meine Angst dir/ ", "tokens": ["Es", "bringt", "zwar", "mei\u00b7ne", "Angst", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jedennoch mu\u00df ich stets von dir gefoltert seyn.", "tokens": ["Je\u00b7den\u00b7noch", "mu\u00df", "ich", "stets", "von", "dir", "ge\u00b7fol\u00b7tert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erbarm dich meiner Noht/ la\u00df ab mich zu versuchen", "tokens": ["Er\u00b7barm", "dich", "mei\u00b7ner", "Noht", "/", "la\u00df", "ab", "mich", "zu", "ver\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$(", "VVIMP", "PTKVZ", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie lange soll dein Zorn auf meine Liebe fluchen?", "tokens": ["Wie", "lan\u00b7ge", "soll", "dein", "Zorn", "auf", "mei\u00b7ne", "Lie\u00b7be", "flu\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kan ohndem nicht mehr ertragen jene Last", "tokens": ["Ich", "kan", "ohn\u00b7dem", "nicht", "mehr", "er\u00b7tra\u00b7gen", "je\u00b7ne", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PAV", "PTKNEG", "ADV", "VVINF", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Womit du mich/ mein Kind/ so sehr beladen hast.", "tokens": ["Wo\u00b7mit", "du", "mich", "/", "mein", "Kind", "/", "so", "sehr", "be\u00b7la\u00b7den", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$(", "PPOSAT", "NN", "$(", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein B\u00e4hr\u2019 noch Tyger kan so grausahm sich geb\u00e4hrden", "tokens": ["Kein", "B\u00e4hr'", "noch", "Ty\u00b7ger", "kan", "so", "grau\u00b7sahm", "sich", "ge\u00b7b\u00e4hr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "NN", "VMFIN", "ADV", "VVFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wie du dich verstelst/ ich mu\u00df gefoltert werden/", "tokens": ["Als", "wie", "du", "dich", "ver\u00b7stelst", "/", "ich", "mu\u00df", "ge\u00b7fol\u00b7tert", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "PRF", "VVFIN", "$(", "PPER", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Hertze ist ein Stein/ der niemahls m\u00fcrbe wird", "tokens": ["Dein", "Hert\u00b7ze", "ist", "ein", "Stein", "/", "der", "nie\u00b7mahls", "m\u00fcr\u00b7be", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ART", "NN", "$(", "ART", "ADV", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Ohr verstopffet sich wenn meine K\u00e4hle girrt.", "tokens": ["Dein", "Ohr", "ver\u00b7stopf\u00b7fet", "sich", "wenn", "mei\u00b7ne", "K\u00e4h\u00b7le", "girrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kan denn mein Jammer-Stand kein Bey-Leyd mir erwerben/", "tokens": ["Kan", "denn", "mein", "Jam\u00b7mer\u00b7Stand", "kein", "Bey\u00b7Leyd", "mir", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "PIAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ist alles Flehn umsonst? wohlan! nun will ich sterben/", "tokens": ["Ist", "al\u00b7les", "Flehn", "um\u00b7sonst", "?", "wo\u00b7hlan", "!", "nun", "will", "ich", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "ADV", "$.", "ADV", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gib nur drey Thr\u00e4nen her/ so wird die Todes-Pein", "tokens": ["Gib", "nur", "drey", "Thr\u00e4\u00b7nen", "her", "/", "so", "wird", "die", "To\u00b7des\u00b7Pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "CARD", "NN", "PTKVZ", "$(", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir lieb und angenehm/ und nicht verdrie\u00dflich seyn.", "tokens": ["Mir", "lieb", "und", "an\u00b7ge\u00b7nehm", "/", "und", "nicht", "ver\u00b7drie\u00df\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$(", "KON", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}