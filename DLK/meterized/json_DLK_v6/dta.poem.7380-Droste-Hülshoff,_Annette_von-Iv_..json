{"dta.poem.7380": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst stand ich unter den F\u00f6hren am See,", "tokens": ["J\u00fcngst", "stand", "ich", "un\u00b7ter", "den", "F\u00f6h\u00b7ren", "am", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meinen B\u00fcchsenspanner zur Seite.", "tokens": ["Mei\u00b7nen", "B\u00fcch\u00b7sen\u00b7span\u00b7ner", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vom Hange schm\u00e4hlte das br\u00fcnstige Reh,", "tokens": ["Vom", "Han\u00b7ge", "schm\u00e4hl\u00b7te", "das", "br\u00fcns\u00b7ti\u00b7ge", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und strich durch des Aufschlags Breite;", "tokens": ["Und", "strich", "durch", "des", "Auf\u00b7schlags", "Brei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich h\u00f6rte es knistern so nah und klar,", "tokens": ["Ich", "h\u00f6r\u00b7te", "es", "knis\u00b7tern", "so", "nah", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Grad' wo die Lichtung verd\u00e4mmert,", "tokens": ["Grad'", "wo", "die", "Lich\u00b7tung", "ver\u00b7d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Da\u00df mich gest\u00f6ret der Holzwurm gar,", "tokens": ["Da\u00df", "mich", "ge\u00b7st\u00f6\u00b7ret", "der", "Holz\u00b7wurm", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der unter'm Fu\u00dfe mir h\u00e4mmert.", "tokens": ["Der", "un\u00b7ter'm", "Fu\u00b7\u00dfe", "mir", "h\u00e4m\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann sprang es ab, es mochte die Luft", "tokens": ["Dann", "sprang", "es", "ab", ",", "es", "moch\u00b7te", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihm unsre Witterung tragen;", "tokens": ["Ihm", "uns\u00b7re", "Wit\u00b7te\u00b7rung", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eherr,\u201c sprach der Bursche: \u201elinks \u00fcber die Kluft!", "tokens": ["\u201e", "herr", ",", "\u201c", "sprach", "der", "Bur\u00b7sche", ":", "\u201e", "links", "\u00fc\u00b7ber", "die", "Kluft", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "$(", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wir m\u00fcssen zur Linken uns schlagen!", "tokens": ["Wir", "m\u00fcs\u00b7sen", "zur", "Lin\u00b7ken", "uns", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Hier naht kein Wild, wo sie eingescharrt", "tokens": ["Hier", "naht", "kein", "Wild", ",", "wo", "sie", "ein\u00b7ge\u00b7scharrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die tolle Gertrud vom Gestade,", "tokens": ["Die", "tol\u00b7le", "Ger\u00b7trud", "vom", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ich h\u00f6re genau wie der Holzwurm pocht", "tokens": ["Ich", "h\u00f6\u00b7re", "ge\u00b7nau", "wie", "der", "Holz\u00b7wurm", "pocht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "In ihrer zerfallenden Lade.\u201c", "tokens": ["In", "ih\u00b7rer", "zer\u00b7fal\u00b7len\u00b7den", "La\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Zur Seite sprang ich, eisig durchgraut,", "tokens": ["Zur", "Sei\u00b7te", "sprang", "ich", ",", "ei\u00b7sig", "durch\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mir war als hab ich ges\u00fcndigt,", "tokens": ["Mir", "war", "als", "hab", "ich", "ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Inde\u00df der Bursch mit fl\u00fcsterndem Laut", "tokens": ["In\u00b7de\u00df", "der", "Bursch", "mit", "fl\u00fcs\u00b7tern\u00b7dem", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die schaurige M\u00e4hre verk\u00fcndigt:", "tokens": ["Die", "schau\u00b7ri\u00b7ge", "M\u00e4h\u00b7re", "ver\u00b7k\u00fcn\u00b7digt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "\u201ewie Jene gesucht, bei Tag und Nacht,", "tokens": ["\u201e", "wie", "Je\u00b7ne", "ge\u00b7sucht", ",", "bei", "Tag", "und", "Nacht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PDS", "VVPP", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nach dem fremden ertrunkenen Weibe,", "tokens": ["Nach", "dem", "frem\u00b7den", "er\u00b7trun\u00b7ke\u00b7nen", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Das ihr der t\u00fcckische See gebracht,", "tokens": ["Das", "ihr", "der", "t\u00fc\u00b7cki\u00b7sche", "See", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verloren an Seele und Leibe.", "tokens": ["Ver\u00b7lo\u00b7ren", "an", "See\u00b7le", "und", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ob ihres Blutes? man wu\u00dfte es nicht!", "tokens": ["Ob", "ih\u00b7res", "Blu\u00b7tes", "?", "man", "wu\u00df\u00b7te", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$.", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Fragen l\u00f6ste das Schweigen.", "tokens": ["Kein", "Fra\u00b7gen", "l\u00f6s\u00b7te", "das", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch schlief die Welle, dann sah ihr Gesicht", "tokens": ["Doch", "schlief", "die", "Wel\u00b7le", ",", "dann", "sah", "ihr", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man \u00fcber den Spiegel sich beugen,", "tokens": ["Man", "\u00fc\u00b7ber", "den", "Spie\u00b7gel", "sich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und zeigte er ihr das eigene Bild,", "tokens": ["Und", "zeig\u00b7te", "er", "ihr", "das", "ei\u00b7ge\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dann fl\u00fcsterte sie beklommen:", "tokens": ["Dann", "fl\u00fcs\u00b7ter\u00b7te", "sie", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u201ewie alt sie sieht, wie irre und wild,", "tokens": ["\u201e", "wie", "alt", "sie", "sieht", ",", "wie", "ir\u00b7re", "und", "wild", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und wie entsetzlich verkommen!\u201c", "tokens": ["Und", "wie", "ent\u00b7setz\u00b7lich", "ver\u00b7kom\u00b7men", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201edoch wenn der Sturm die Woge ger\u00fchrt,", "tokens": ["\u201e", "doch", "wenn", "der", "Sturm", "die", "Wo\u00b7ge", "ge\u00b7r\u00fchrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dann war sie vom B\u00f6sen geschlagen,", "tokens": ["Dann", "war", "sie", "vom", "B\u00f6\u00b7sen", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was sie f\u00fcr bedenkliche Reden gef\u00fchrt,", "tokens": ["Was", "sie", "f\u00fcr", "be\u00b7denk\u00b7li\u00b7che", "Re\u00b7den", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Das m\u00f6ge er lieber nicht sagen.", "tokens": ["Das", "m\u00f6\u00b7ge", "er", "lie\u00b7ber", "nicht", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "So war sie gerannt vor Jahresfrist,", "tokens": ["So", "war", "sie", "ge\u00b7rannt", "vor", "Jah\u00b7res\u00b7frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u2014 Man sah's vom lavirenden Schiffe \u2014", "tokens": ["Man", "sah's", "vom", "la\u00b7vi\u00b7ren\u00b7den", "Schif\u00b7fe"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Zur Brandung, wo sie am hohlsten ist,", "tokens": ["Zur", "Bran\u00b7dung", ",", "wo", "sie", "am", "hohls\u00b7ten", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "APPRART", "ADJA", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und kopf\u00fcber gefahren vom Riffe.", "tokens": ["Und", "kopf\u00b7\u00fc\u00b7ber", "ge\u00b7fah\u00b7ren", "vom", "Rif\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Drum scharrte man sie in's Dickicht dort,", "tokens": ["Drum", "scharr\u00b7te", "man", "sie", "in's", "Di\u00b7ckicht", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie eine verlorene Seele.\u201c", "tokens": ["Wie", "ei\u00b7ne", "ver\u00b7lo\u00b7re\u00b7ne", "See\u00b7le", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich schwieg, und sandte den Burschen fort,", "tokens": ["Ich", "schwieg", ",", "und", "sand\u00b7te", "den", "Bur\u00b7schen", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Brach mir vom Grab' eine Schmehle:", "tokens": ["Brach", "mir", "vom", "Grab'", "ei\u00b7ne", "Schmeh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u201edu armes gehetztes Wild der Pein,", "tokens": ["\u201e", "du", "ar\u00b7mes", "ge\u00b7hetz\u00b7tes", "Wild", "der", "Pein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie m\u00f6gen die Menschen dich richten!\u201c", "tokens": ["Wie", "m\u00f6\u00b7gen", "die", "Men\u00b7schen", "dich", "rich\u00b7ten", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "\u2014 Sacht pochte der K\u00e4fer im morschen Schrein,", "tokens": ["Sacht", "poch\u00b7te", "der", "K\u00e4\u00b7fer", "im", "mor\u00b7schen", "Schrein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Der Mond stand \u00fcber den Fichten. \u2014", "tokens": ["Der", "Mond", "stand", "\u00fc\u00b7ber", "den", "Fich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}