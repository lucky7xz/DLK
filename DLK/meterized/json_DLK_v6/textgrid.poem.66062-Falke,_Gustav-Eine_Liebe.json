: ["Den", "nie", "ein", "Arg\u00b7wohn", "tr\u00fcbt", ",", "mich", "wie\u00b7der\u00b7er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zerbr\u00e4ch ich die beschworenen heiligen Siegel,", "tokens": ["Zer\u00b7br\u00e4ch", "ich", "die", "be\u00b7schwo\u00b7re\u00b7nen", "hei\u00b7li\u00b7gen", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Verriete sie und tr\u00e4te vor sie hin", "tokens": ["Ver\u00b7rie\u00b7te", "sie", "und", "tr\u00e4\u00b7te", "vor", "sie", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mit Schmeichelwort, ein andrer als ich bin,", "tokens": ["Mit", "Schmei\u00b7chel\u00b7wort", ",", "ein", "an\u00b7drer", "als", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "K\u00fc\u00dft sie mit Lippen, drauf dein Ku\u00df noch bl\u00fchte,", "tokens": ["K\u00fc\u00dft", "sie", "mit", "Lip\u00b7pen", ",", "drauf", "dein", "Ku\u00df", "noch", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mit Worten, drin heimliche Glut noch gl\u00