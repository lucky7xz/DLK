{"textgrid.poem.66062": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Eine Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fast noch ein Kind und hast Gewalt schon, bist", "tokens": ["Fast", "noch", "ein", "Kind", "und", "hast", "Ge\u00b7walt", "schon", ",", "bist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "KON", "VAFIN", "NN", "ADV", "$,", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon Herrin \u00fcber mich, der nun sein Gl\u00fcck", "tokens": ["Schon", "Her\u00b7rin", "\u00fc\u00b7ber", "mich", ",", "der", "nun", "sein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einzig an deiner Huld und G\u00fcte mi\u00dft,", "tokens": ["Ein\u00b7zig", "an", "dei\u00b7ner", "Huld", "und", "G\u00fc\u00b7te", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem\u00fctig dein, und kann nicht mehr zur\u00fcck.", "tokens": ["De\u00b7m\u00fc\u00b7tig", "dein", ",", "und", "kann", "nicht", "mehr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "$,", "KON", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O junge Herrin, unter g\u00fctigem Stern", "tokens": ["O", "jun\u00b7ge", "Her\u00b7rin", ",", "un\u00b7ter", "g\u00fc\u00b7ti\u00b7gem", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Sind meine stillen Jahre hingegangen,", "tokens": ["Sind", "mei\u00b7ne", "stil\u00b7len", "Jah\u00b7re", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch tr\u00e4umte mir von einer Insel, fern,", "tokens": ["Doch", "tr\u00e4um\u00b7te", "mir", "von", "ei\u00b7ner", "In\u00b7sel", ",", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ach so traumfern, wo solche Lieder klangen,", "tokens": ["Ach", "so", "traum\u00b7fern", ",", "wo", "sol\u00b7che", "Lie\u00b7der", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVINF", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Wie sie mein waches Ohr niemals vernahm,", "tokens": ["Wie", "sie", "mein", "wa\u00b7ches", "Ohr", "nie\u00b7mals", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "S\u00fc\u00df wie das Singen lockender Sirenen,", "tokens": ["S\u00fc\u00df", "wie", "das", "Sin\u00b7gen", "lo\u00b7cken\u00b7der", "Si\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und wo verschwiegenem und tiefstem Sehnen", "tokens": ["Und", "wo", "ver\u00b7schwie\u00b7ge\u00b7nem", "und", "tiefs\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Selige Erf\u00fcllung hold entgegenkam.", "tokens": ["Se\u00b7li\u00b7ge", "Er\u00b7f\u00fcl\u00b7lung", "hold", "ent\u00b7ge\u00b7gen\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Ein neuer Stern ist leuchtend aufgestiegen,", "tokens": ["Ein", "neu\u00b7er", "Stern", "ist", "leuch\u00b7tend", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In seinem Licht seh ich das Ufer liegen,", "tokens": ["In", "sei\u00b7nem", "Licht", "seh", "ich", "das", "U\u00b7fer", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "An das die Wasser meiner Sehnsucht sch\u00e4umen", "tokens": ["An", "das", "die", "Was\u00b7ser", "mei\u00b7ner", "Sehn\u00b7sucht", "sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In wehem Wachen und in kranken Tr\u00e4umen,", "tokens": ["In", "we\u00b7hem", "Wa\u00b7chen", "und", "in", "kran\u00b7ken", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und all mein Leben zittert ihm entgegen.", "tokens": ["Und", "all", "mein", "Le\u00b7ben", "zit\u00b7tert", "ihm", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "La\u00df mich die Hand in deine H\u00e4nde legen,", "tokens": ["La\u00df", "mich", "die", "Hand", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Auf deinen Scho\u00df die hei\u00dfe Stirne senken,", "tokens": ["Auf", "dei\u00b7nen", "Scho\u00df", "die", "hei\u00b7\u00dfe", "Stir\u00b7ne", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und wenn mich dann dein leiser Atem trifft,", "tokens": ["Und", "wenn", "mich", "dann", "dein", "lei\u00b7ser", "A\u00b7tem", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Glauben, das Meer der Sehnsucht sei durchschifft,", "tokens": ["Glau\u00b7ben", ",", "das", "Meer", "der", "Sehn\u00b7sucht", "sei", "durch\u00b7schifft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.22": {"text": "Und meine Seele sich im Hafen denken.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "sich", "im", "Ha\u00b7fen", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, holde Herrin, fast noch Kind, und schon", "tokens": ["Ja", ",", "hol\u00b7de", "Her\u00b7rin", ",", "fast", "noch", "Kind", ",", "und", "schon"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Schicksal ausersehn f\u00fcr einen Thron,", "tokens": ["Vom", "Schick\u00b7sal", "au\u00b7ser\u00b7sehn", "f\u00fcr", "ei\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So herrlich wie kein K\u00f6nig ihn besteigt,", "tokens": ["So", "herr\u00b7lich", "wie", "kein", "K\u00f6\u00b7nig", "ihn", "be\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nimm hin mein Herz, das sich dir willig neigt,", "tokens": ["Nimm", "hin", "mein", "Herz", ",", "das", "sich", "dir", "wil\u00b7lig", "neigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$,", "PRELS", "PRF", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dies reiche Herz, das eine Welt umschlie\u00dft", "tokens": ["Dies", "rei\u00b7che", "Herz", ",", "das", "ei\u00b7ne", "Welt", "um\u00b7schlie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und hei\u00dfe Lebensstr\u00f6me in sie gie\u00dft,", "tokens": ["Und", "hei\u00b7\u00dfe", "Le\u00b7bens\u00b7str\u00f6\u00b7me", "in", "sie", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Herz, so reich, da\u00df es sich arm nicht gibt,", "tokens": ["Ein", "Herz", ",", "so", "reich", ",", "da\u00df", "es", "sich", "arm", "nicht", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und das sein Alles hingibt, wo es liebt.", "tokens": ["Und", "das", "sein", "Al\u00b7les", "hin\u00b7gibt", ",", "wo", "es", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "PIS", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Lieb, dies sind nicht rasche Schw\u00e4rmerworte,", "tokens": ["O", "Lieb", ",", "dies", "sind", "nicht", "ra\u00b7sche", "Schw\u00e4r\u00b7mer\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PDS", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Schw\u00fcre eines leicht entflammten Knaben.", "tokens": ["Nicht", "Schw\u00fc\u00b7re", "ei\u00b7nes", "leicht", "ent\u00b7flamm\u00b7ten", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Jahr lang hielt verschlossen ich die Pforte,", "tokens": ["Ein", "Jahr", "lang", "hielt", "ver\u00b7schlos\u00b7sen", "ich", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Warf hinter mich den Schl\u00fcssel. M\u00e4hlich haben", "tokens": ["Warf", "hin\u00b7ter", "mich", "den", "Schl\u00fcs\u00b7sel", ".", "M\u00e4h\u00b7lich", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "PPER", "ART", "NN", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Riegel sich gelockert, und nun dr\u00e4ngt", "tokens": ["Die", "Rie\u00b7gel", "sich", "ge\u00b7lo\u00b7ckert", ",", "und", "nun", "dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVPP", "$,", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gefangene Glut, bis sie die Pforten sprengt.", "tokens": ["Ge\u00b7fan\u00b7ge\u00b7ne", "Glut", ",", "bis", "sie", "die", "Pfor\u00b7ten", "sprengt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nie hat es keuschere Leidenschaft gegeben,", "tokens": ["Nie", "hat", "es", "keu\u00b7sche\u00b7re", "Lei\u00b7den\u00b7schaft", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wenn Leidenschaft denn keusch sein kann und ist,", "tokens": ["Wenn", "Lei\u00b7den\u00b7schaft", "denn", "keusch", "sein", "kann", "und", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "VAINF", "VMFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die ja ihr Recht nur an sich selber mi\u00dft.", "tokens": ["Die", "ja", "ihr", "Recht", "nur", "an", "sich", "sel\u00b7ber", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADV", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Liebe sucht Liebe, Leben will zu Leben,", "tokens": ["Lie\u00b7be", "sucht", "Lie\u00b7be", ",", "Le\u00b7ben", "will", "zu", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "NN", "VMFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Und wenn es sucht und sehnt: nenns Leidenschaft,", "tokens": ["Und", "wenn", "es", "sucht", "und", "sehnt", ":", "nenns", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nenns Liebe, M\u00e4dchen, keusch ist jede Kraft,", "tokens": ["Nenns", "Lie\u00b7be", ",", "M\u00e4d\u00b7chen", ",", "keusch", "ist", "je\u00b7de", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Leben wirkt. Und also lieb ich dich,", "tokens": ["Die", "Le\u00b7ben", "wirkt", ".", "Und", "al\u00b7so", "lieb", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ADV", "ADJD", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und so, in Keuschheit, will ich dich f\u00fcr mich.", "tokens": ["Und", "so", ",", "in", "Keuschheit", ",", "will", "ich", "dich", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "$,", "VMFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Es darf nicht sein! Ich hab ein liebes Weib", "tokens": ["Es", "darf", "nicht", "sein", "!", "Ich", "hab", "ein", "lie\u00b7bes", "Weib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und liebe Kinder. Meine Seele ringt.", "tokens": ["Und", "lie\u00b7be", "Kin\u00b7der", ".", "Mei\u00b7ne", "See\u00b7le", "ringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ists auch nicht S\u00fcnde, was sie niederzwingt,", "tokens": ["Ists", "auch", "nicht", "S\u00fcn\u00b7de", ",", "was", "sie", "nie\u00b7der\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df wie im Fieber schauern Herz und Leib,", "tokens": ["Da\u00df", "wie", "im", "Fie\u00b7ber", "schau\u00b7ern", "Herz", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Tage elend, meine N\u00e4chte schwer,", "tokens": ["Die", "Ta\u00b7ge", "e\u00b7lend", ",", "mei\u00b7ne", "N\u00e4ch\u00b7te", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Schlaflos, oder von wilden Tr\u00e4umen krank \u2013", "tokens": ["Schlaf\u00b7los", ",", "o\u00b7der", "von", "wil\u00b7den", "Tr\u00e4u\u00b7men", "krank", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es darf nicht sein! So grundlos w\u00e4lzt kein Meer", "tokens": ["Es", "darf", "nicht", "sein", "!", "So", "grund\u00b7los", "w\u00e4lzt", "kein", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "$.", "ADV", "ADJD", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich zwischen zwei getre