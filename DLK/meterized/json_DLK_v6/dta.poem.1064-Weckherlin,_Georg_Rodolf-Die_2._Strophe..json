{"dta.poem.1064": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 2. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seitenmahl wan die Mayestehe", "tokens": ["Sei\u00b7ten\u00b7mahl", "wan", "die", "Ma\u00b7ye\u00b7ste\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der gravitetischen geberden/", "tokens": ["Der", "gra\u00b7vi\u00b7te\u00b7ti\u00b7schen", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Welche auf deiner stirnen steht/", "tokens": ["Wel\u00b7che", "auf", "dei\u00b7ner", "stir\u00b7nen", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von mir solte gesungen werden;", "tokens": ["Von", "mir", "sol\u00b7te", "ge\u00b7sun\u00b7gen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dein angeborne freundligkeit/", "tokens": ["Dein", "an\u00b7ge\u00b7bor\u00b7ne", "freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Damit du jederman gewogen/", "tokens": ["Da\u00b7mit", "du", "je\u00b7der\u00b7man", "ge\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erzaiget jhre w\u00fcrdigkeit/", "tokens": ["E\u00b7rzai\u00b7get", "jhre", "w\u00fcr\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auf das sie werd herf\u00fcr gezogen.", "tokens": ["Auf", "das", "sie", "werd", "her\u00b7f\u00fcr", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd wan ich zusingen berait", "tokens": ["Vnd", "wan", "ich", "zu\u00b7sin\u00b7gen", "be\u00b7rait"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Dein fliessende wolredenheit/", "tokens": ["Dein", "flies\u00b7sen\u00b7de", "wol\u00b7re\u00b7den\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Will mich al\u00dfbald dein wolstand zwingen/", "tokens": ["Will", "mich", "al\u00df\u00b7bald", "dein", "wol\u00b7stand", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Seine zierlichkeit vorzusingen.", "tokens": ["Sei\u00b7ne", "zier\u00b7lich\u00b7keit", "vor\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}