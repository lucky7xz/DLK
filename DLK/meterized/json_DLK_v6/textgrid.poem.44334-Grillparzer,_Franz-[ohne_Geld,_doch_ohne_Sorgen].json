{"textgrid.poem.44334": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ohne Geld, doch ohne Sorgen]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohne Geld, doch ohne Sorgen,", "tokens": ["Oh\u00b7ne", "Geld", ",", "doch", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was gleicht meiner Seligkeit,", "tokens": ["Was", "gleicht", "mei\u00b7ner", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geld, ei, Geld, das kann ich borgen!", "tokens": ["Geld", ",", "ei", ",", "Geld", ",", "das", "kann", "ich", "bor\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch wer ist, der Frohsinn leiht!", "tokens": ["Doch", "wer", "ist", ",", "der", "Froh\u00b7sinn", "leiht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute sorget ihr f\u00fcr morgen,", "tokens": ["Heu\u00b7te", "sor\u00b7get", "ihr", "f\u00fcr", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgen f\u00fcr die Ewigkeit,", "tokens": ["Mor\u00b7gen", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will heut f\u00fcr heute sorgen,", "tokens": ["Ich", "will", "heut", "f\u00fcr", "heu\u00b7te", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADV", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Morgen ist f\u00fcr morgen Zeit.", "tokens": ["Mor\u00b7gen", "ist", "f\u00fcr", "mor\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Zukunft? \u2013 Wenn auch morgen", "tokens": ["Und", "die", "Zu\u00b7kunft", "?", "\u2013", "Wenn", "auch", "mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$.", "$(", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich der Tod zum Opfer weiht,", "tokens": ["Mich", "der", "Tod", "zum", "Op\u00b7fer", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frei von Schuld sein und von Sorgen,", "tokens": ["Frei", "von", "Schuld", "sein", "und", "von", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAINF", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ja hier schon Seligkeit!", "tokens": ["Ist", "ja", "hier", "schon", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}