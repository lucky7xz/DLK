{"textgrid.poem.51365": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie da\u00df die G\u00f6ttin ihr die Farbe hat erkiest,", "tokens": ["Wie", "da\u00df", "die", "G\u00f6t\u00b7tin", "ihr", "die", "Far\u00b7be", "hat", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In die das sch\u00f6ne Reich der Lufft gekleidet ist.", "tokens": ["In", "die", "das", "sch\u00f6\u00b7ne", "Reich", "der", "Lufft", "ge\u00b7klei\u00b7det", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bedeut es Hoffarth? Nein. Dann sie ist allen gut:", "tokens": ["Be\u00b7deut", "es", "Hof\u00b7farth", "?", "Nein", ".", "Dann", "sie", "ist", "al\u00b7len", "gut", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "PTKANT", "$.", "ADV", "PPER", "VAFIN", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bestand? Nein. Dann sie braucht in etwas Wanckelmuth.", "tokens": ["Be\u00b7stand", "?", "Nein", ".", "Dann", "sie", "braucht", "in", "et\u00b7was", "Wan\u00b7ckel\u00b7muth", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "$.", "ADV", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geht es auf Eyfer? Nein. Dann ihr gilt alles gleich.", "tokens": ["Geht", "es", "auf", "Ey\u00b7fer", "?", "Nein", ".", "Dann", "ihr", "gilt", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$.", "PTKANT", "$.", "ADV", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf heimlich Leiden? Nein. Dann sie ist sonst so bleich.", "tokens": ["Auf", "heim\u00b7lich", "Lei\u00b7den", "?", "Nein", ".", "Dann", "sie", "ist", "sonst", "so", "bleich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "$.", "PTKANT", "$.", "ADV", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich deute diese Farb auf Freud und Hoffnung mir,", "tokens": ["Ich", "deu\u00b7te", "die\u00b7se", "Farb", "auf", "Freud", "und", "Hoff\u00b7nung", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn eines Theil spielt blau, das andre gr\u00fcn herf\u00fcr.", "tokens": ["Denn", "ei\u00b7nes", "Theil", "spielt", "blau", ",", "das", "and\u00b7re", "gr\u00fcn", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,", "PRELS", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}