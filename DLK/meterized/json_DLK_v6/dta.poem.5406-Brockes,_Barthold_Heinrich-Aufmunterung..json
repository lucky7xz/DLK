{"dta.poem.5406": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da GOtt in seiner Creatur", "tokens": ["Da", "Gott", "in", "sei\u00b7ner", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So wunderw\u00fcrdig sich erwiesen;", "tokens": ["So", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", "sich", "er\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja da derselbe sich nicht nur", "tokens": ["Ja", "da", "der\u00b7sel\u00b7be", "sich", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "PDAT", "PRF", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwiesen; sich noch immer weis\u2019t:", "tokens": ["Er\u00b7wie\u00b7sen", ";", "sich", "noch", "im\u00b7mer", "weis't", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie da\u00df man ihn, wenn man ihn nicht gepriesen,", "tokens": ["Wie", "da\u00df", "man", "ihn", ",", "wenn", "man", "ihn", "nicht", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "PPER", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auch noch in unsrer Lust nicht preis\u2019t!", "tokens": ["Auch", "noch", "in", "uns\u00b7rer", "Lust", "nicht", "preis't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist sein Gesch\u00f6pf, wie oder er, nicht wehrt,", "tokens": ["Ist", "sein", "Ge\u00b7sch\u00f6pf", ",", "wie", "o\u00b7der", "er", ",", "nicht", "wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PWAV", "KON", "PPER", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df man in ihnen ihn verehrt?", "tokens": ["Da\u00df", "man", "in", "ih\u00b7nen", "ihn", "ver\u00b7ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ist etwan, lieber Mensch, f\u00fcr deinen hohen Geist", "tokens": ["Ist", "et\u00b7wan", ",", "lie\u00b7ber", "Mensch", ",", "f\u00fcr", "dei\u00b7nen", "ho\u00b7hen", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "ADV", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das, was er schuf, zu niedrig zu geringe?", "tokens": ["Das", ",", "was", "er", "schuf", ",", "zu", "nied\u00b7rig", "zu", "ge\u00b7rin\u00b7ge", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKA", "ADJD", "PTKZU", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Es scheint so gar der Sch\u00f6pffer aller Dinge", "tokens": ["Es", "scheint", "so", "gar", "der", "Sch\u00f6pf\u00b7fer", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nicht deiner Achtung wehrt zu seyn.", "tokens": ["Nicht", "dei\u00b7ner", "Ach\u00b7tung", "wehrt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hiedurch nun raubst du dir nicht deine Lust allein,", "tokens": ["Hie\u00b7durch", "nun", "raubst", "du", "dir", "nicht", "dei\u00b7ne", "Lust", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die er mit seiner Ehr (o Liebe) hier verbindet;", "tokens": ["Die", "er", "mit", "sei\u00b7ner", "Ehr", "(", "o", "Lie\u00b7be", ")", "hier", "ver\u00b7bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "$(", "FM", "NN", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du raubest ihm zugleich die Ehre welche man,", "tokens": ["Du", "rau\u00b7best", "ihm", "zu\u00b7gleich", "die", "Eh\u00b7re", "wel\u00b7che", "man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PRELS", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In seinem Werck allein und seiner F\u00fchrung findet,", "tokens": ["In", "sei\u00b7nem", "Werck", "al\u00b7lein", "und", "sei\u00b7ner", "F\u00fch\u00b7rung", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und ohne sie nicht finden kann.", "tokens": ["Und", "oh\u00b7ne", "sie", "nicht", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sprich selbst, wenns nicht die Menschen wollen,", "tokens": ["Sprich", "selbst", ",", "wenns", "nicht", "die", "Men\u00b7schen", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "PTKNEG", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was doch f\u00fcr Creaturen sollen,", "tokens": ["Was", "doch", "f\u00fcr", "Crea\u00b7tu\u00b7ren", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Aus einem GOtt ergebnen Triebe,", "tokens": ["Aus", "ei\u00b7nem", "Gott", "er\u00b7geb\u00b7nen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Empfindlich und erk\u00e4nntlich seyn,", "tokens": ["Emp\u00b7find\u00b7lich", "und", "er\u00b7k\u00e4nnt\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "F\u00fcr GOttes Weisheit, Macht und Liebe,", "tokens": ["F\u00fcr", "Got\u00b7tes", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "F\u00fcr seiner Gnaden Glantz und Schein?", "tokens": ["F\u00fcr", "sei\u00b7ner", "Gna\u00b7den", "Glantz", "und", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Worin ist doch der Unterscheid,", "tokens": ["Wo\u00b7rin", "ist", "doch", "der", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der zwischen uns und andern Thieren,", "tokens": ["Der", "zwi\u00b7schen", "uns", "und", "an\u00b7dern", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Als blos hierin allein, zu sp\u00fchren,", "tokens": ["Als", "blos", "hie\u00b7rin", "al\u00b7lein", ",", "zu", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Das wir des Gebers Herrlichkeit", "tokens": ["Das", "wir", "des", "Ge\u00b7bers", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und Macht und Lieb\u2019, in seinen Wercken,", "tokens": ["Und", "Macht", "und", "Lieb'", ",", "in", "sei\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Geschickt und f\u00e4hig zu bemercken?", "tokens": ["Ge\u00b7schickt", "und", "f\u00e4\u00b7hig", "zu", "be\u00b7mer\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}