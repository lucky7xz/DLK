{"dta.poem.1112": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Franciscus Maria Marchini.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit giebt des Gl\u00fccks und nicht der Mutter", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "giebt", "des", "Gl\u00fccks", "und", "nicht", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gunst:", "tokens": ["Gunst", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Doch was Natur versagt/ giebt sch\u00f6ner Reimen Kunst.", "tokens": ["Doch", "was", "Na\u00b7tur", "ver\u00b7sagt", "/", "giebt", "sch\u00f6\u00b7ner", "Rei\u00b7men", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVPP", "$(", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch dich/ ", "tokens": ["Durch", "dich", "/"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wenn Adimarens Hand den klugen Pinsel f\u00fchrt.", "tokens": ["Wenn", "A\u00b7di\u00b7ma\u00b7rens", "Hand", "den", "klu\u00b7gen", "Pin\u00b7sel", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Izt wird der Nimphen Ruhm bey dem/ was h\u00e4\u00dflich/ klein:", "tokens": ["Izt", "wird", "der", "Nim\u00b7phen", "Ruhm", "bey", "dem", "/", "was", "h\u00e4\u00df\u00b7lich", "/", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "$(", "PWS", "ADJD", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird selbst greulicher die Venus wollen seyn.", "tokens": ["Es", "wird", "selbst", "greu\u00b7li\u00b7cher", "die", "Ve\u00b7nus", "wol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}