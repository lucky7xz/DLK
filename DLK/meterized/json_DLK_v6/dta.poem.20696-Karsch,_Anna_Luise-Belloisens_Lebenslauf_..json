{"dta.poem.20696": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Belloisens Lebenslauf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ward geboren ohne feierliche Bitte               ", "tokens": ["Ich", "ward", "ge\u00b7bo\u00b7ren", "oh\u00b7ne", "fei\u00b7er\u00b7li\u00b7che", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Kirchspiels ohne Priesterflehn", "tokens": ["Des", "Kirch\u00b7spiels", "oh\u00b7ne", "Pries\u00b7ter\u00b7flehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab ich in strohbedeckter H\u00fctte", "tokens": ["Hab", "ich", "in", "stroh\u00b7be\u00b7deck\u00b7ter", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das erste Tageslicht gesehn,", "tokens": ["Das", "ers\u00b7te", "Ta\u00b7ges\u00b7licht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wuchs unter L\u00e4mmerchen und Tauben", "tokens": ["Wuchs", "un\u00b7ter", "L\u00e4m\u00b7mer\u00b7chen", "und", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und Ziegen bis ins f\u00fcnfte Jahr,", "tokens": ["Und", "Zie\u00b7gen", "bis", "ins", "f\u00fcnf\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und lernt\u2019 an einen Sch\u00f6pfer glauben,", "tokens": ["Und", "lernt'", "an", "ei\u00b7nen", "Sch\u00f6p\u00b7fer", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil\u2019s Morgenroth so lieblich war,", "tokens": ["Weil's", "Mor\u00b7gen\u00b7roth", "so", "lieb\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So gr\u00fcn der Wald, so bunt die Wiesen,", "tokens": ["So", "gr\u00fcn", "der", "Wald", ",", "so", "bunt", "die", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So klar und silbersch\u00f6n der Bach.", "tokens": ["So", "klar", "und", "sil\u00b7ber\u00b7sch\u00f6n", "der", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Lerche sang f\u00fcr Belloisen,", "tokens": ["Die", "Ler\u00b7che", "sang", "f\u00fcr", "Bel\u00b7loi\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Und Belloise sang ihr nach.", "tokens": ["Und", "Bel\u00b7loi\u00b7se", "sang", "ihr", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Die Nachtigall in Elsenstr\u00e4uchen", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "in", "El\u00b7sen\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Erhub ihr s\u00fc\u00dfes Lied, und ich", "tokens": ["Er\u00b7hub", "ihr", "s\u00fc\u00b7\u00dfes", "Lied", ",", "und", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "W\u00fcnscht\u2019 ihr im Tone schon zu gleichen.", "tokens": ["W\u00fcnscht'", "ihr", "im", "To\u00b7ne", "schon", "zu", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Hier fand ein alter Vetter mich", "tokens": ["Hier", "fand", "ein", "al\u00b7ter", "Vet\u00b7ter", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und sagte: du sollst mit mir gehen.", "tokens": ["Und", "sag\u00b7te", ":", "du", "sollst", "mit", "mir", "ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.18": {"text": "Ich ging und lernte bald bei ", "tokens": ["Ich", "ging", "und", "lern\u00b7te", "bald", "bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Die B\u00fccher lesen und verstehen,", "tokens": ["Die", "B\u00fc\u00b7cher", "le\u00b7sen", "und", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die unsern Sinn zum Himmel ziehn.", "tokens": ["Die", "un\u00b7sern", "Sinn", "zum", "Him\u00b7mel", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Vier Sommer und vier Winter flogen", "tokens": ["Vier", "Som\u00b7mer", "und", "vier", "Win\u00b7ter", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zu sehr befl\u00fcgelt uns vorbei;", "tokens": ["Zu", "sehr", "be\u00b7fl\u00fc\u00b7gelt", "uns", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Des Vetters Arm ward ich entzogen", "tokens": ["Des", "Vet\u00b7ters", "Arm", "ward", "ich", "ent\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Zu einer Bruderwiege neu.", "tokens": ["Zu", "ei\u00b7ner", "Bru\u00b7der\u00b7wie\u00b7ge", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Als ich den Bruder gro\u00df getragen,", "tokens": ["Als", "ich", "den", "Bru\u00b7der", "gro\u00df", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Trieb ich drei Rinder auf die Flur,", "tokens": ["Trieb", "ich", "drei", "Rin\u00b7der", "auf", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und pries in meinen Hirtentagen", "tokens": ["Und", "pries", "in", "mei\u00b7nen", "Hir\u00b7ten\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Vergn\u00fcgt die Sch\u00f6nheit der Natur,", "tokens": ["Ver\u00b7gn\u00fcgt", "die", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ward fr\u00fch ins Ehejoch gespannet,", "tokens": ["Ward", "fr\u00fch", "ins", "E\u00b7he\u00b7joch", "ge\u00b7span\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Trugs zweimal nach einander schwer,", "tokens": ["Trugs", "zwei\u00b7mal", "nach", "ein\u00b7an\u00b7der", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und h\u00e4tte mich wol nicht ermannet,", "tokens": ["Und", "h\u00e4t\u00b7te", "mich", "wol", "nicht", "er\u00b7man\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn\u2019s nicht den Musen eigen w\u00e4r,", "tokens": ["Wenn's", "nicht", "den", "Mu\u00b7sen", "ei\u00b7gen", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Im Ungl\u00fcck und in bittern Stunden", "tokens": ["Im", "Un\u00b7gl\u00fcck", "und", "in", "bit\u00b7tern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Dem beizustehn, der ihre Huld", "tokens": ["Dem", "bei\u00b7zu\u00b7stehn", ",", "der", "ih\u00b7re", "Huld"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVINF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Vor der Geburt schon hat empfunden.", "tokens": ["Vor", "der", "Ge\u00b7burt", "schon", "hat", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.36": {"text": "Sie gaben mir Muth und Geduld,", "tokens": ["Sie", "ga\u00b7ben", "mir", "Muth", "und", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.37": {"text": "Und lehreten mich Lieder dichten,", "tokens": ["Und", "leh\u00b7re\u00b7ten", "mich", "Lie\u00b7der", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ADJA", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.38": {"text": "Mit kleinen Kindern auf dem Schoo\u00df.", "tokens": ["Mit", "klei\u00b7nen", "Kin\u00b7dern", "auf", "dem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Bei Weib- und Magd- und Mutterpflichten,", "tokens": ["Bei", "Weib", "und", "Magd", "und", "Mut\u00b7ter\u00b7pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Bei manchem Kummer, schwer und gro\u00df,", "tokens": ["Bei", "man\u00b7chem", "Kum\u00b7mer", ",", "schwer", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sang ich den K\u00f6nig und die Schlachten,", "tokens": ["Sang", "ich", "den", "K\u00f6\u00b7nig", "und", "die", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Die Ihm und seiner Heldenschaar", "tokens": ["Die", "Ihm", "und", "sei\u00b7ner", "Hel\u00b7den\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Unsterblichgr\u00fcne Kr\u00e4nze brachten,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7gr\u00fc\u00b7ne", "Kr\u00e4n\u00b7ze", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.44": {"text": "Und hatte noch manch saures Jahr,", "tokens": ["Und", "hat\u00b7te", "noch", "manch", "sau\u00b7res", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Eh frei von andrer Pflichten Drang", "tokens": ["Eh", "frei", "von", "an\u00b7drer", "Pflich\u00b7ten", "Drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Mir Tage wurden zu Gesang! ", "tokens": ["Mir", "Ta\u00b7ge", "wur\u00b7den", "zu", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}