{"textgrid.poem.33226": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Sanffte Ruhe im Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein m\u00fcder Leichnam ruht nunmehr im Schoo\u00df der Erden,", "tokens": ["Mein", "m\u00fc\u00b7der", "Leich\u00b7nam", "ruht", "nun\u00b7mehr", "im", "Schoo\u00df", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihn als Mutter deckt, da er entseelt und kalt.", "tokens": ["Die", "ihn", "als", "Mut\u00b7ter", "deckt", ",", "da", "er", "ent\u00b7seelt", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier wei\u00df er nichts von Leyd, von Anlauff, von Beschwerden,", "tokens": ["Hier", "wei\u00df", "er", "nichts", "von", "Leyd", ",", "von", "An\u00b7lauff", ",", "von", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier ist sein Ruhe-Bett, sein sichrer Auffenthalt.", "tokens": ["Hier", "ist", "sein", "Ru\u00b7he\u00b7Bett", ",", "sein", "sich\u00b7rer", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar wird sich wohl mein Fleisch nun bald in Staub verkehren,", "tokens": ["Zwar", "wird", "sich", "wohl", "mein", "Fleisch", "nun", "bald", "in", "Staub", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch der, den selbst der Tod und die Verwesung ehren,", "tokens": ["Doch", "der", ",", "den", "selbst", "der", "Tod", "und", "die", "Ver\u00b7we\u00b7sung", "eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADV", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Macht einst gewi\u00df in ihm das Leben wieder neu:", "tokens": ["Macht", "einst", "ge\u00b7wi\u00df", "in", "ihm", "das", "Le\u00b7ben", "wie\u00b7der", "neu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und, da ich in der Grufft soll als ein Saat-Korn k\u00e4umen,", "tokens": ["Und", ",", "da", "ich", "in", "der", "Grufft", "soll", "als", "ein", "Saat\u00b7Korn", "k\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So kan in diesem Schlaff, der aller Sorgen frey,", "tokens": ["So", "kan", "in", "die\u00b7sem", "Schlaff", ",", "der", "al\u00b7ler", "Sor\u00b7gen", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir sonst von nichts, als nur von Aufferstehen, tr\u00e4umen.", "tokens": ["Mir", "sonst", "von", "nichts", ",", "als", "nur", "von", "Auf\u00b7fer\u00b7ste\u00b7hen", ",", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "$,", "KOUS", "ADV", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}