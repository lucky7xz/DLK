{"textgrid.poem.40511": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kreis von gr\u00fcnen B\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kreis von gr\u00fcnen B\u00e4umen,", "tokens": ["Ein", "Kreis", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestr\u00e4uch und Rasengr\u00fcn;", "tokens": ["Ge\u00b7str\u00e4uch", "und", "Ra\u00b7sen\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Pfarrer wandelt betend", "tokens": ["Der", "Pfar\u00b7rer", "wan\u00b7delt", "be\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit dem Brevier dahin.", "tokens": ["Mit", "dem", "Bre\u00b7vier", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die L\u00fcfte bl\u00e4ttern dienend", "tokens": ["Die", "L\u00fcf\u00b7te", "bl\u00e4t\u00b7tern", "die\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sanft Blatt f\u00fcr Blatt herum;", "tokens": ["Sanft", "Blatt", "f\u00fcr", "Blatt", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Strahl der Gnade, leuchtet", "tokens": ["Ein", "Strahl", "der", "Gna\u00b7de", ",", "leuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonn' ins Heiligthum.", "tokens": ["Die", "Sonn'", "ins", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Kreis von gr\u00fcnen B\u00e4umen,", "tokens": ["Ein", "Kreis", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestr\u00e4uch und Rasen dabei,", "tokens": ["Ge\u00b7str\u00e4uch", "und", "Ra\u00b7sen", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und jauchzend tafelt drunter", "tokens": ["Und", "jauch\u00b7zend", "ta\u00b7felt", "drun\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eine lust'ge Kumpanei.", "tokens": ["Ei\u00b7ne", "lust'\u00b7ge", "Kum\u00b7pa\u00b7nei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die B\u00fcsche w\u00f6lben als Keller", "tokens": ["Die", "B\u00fc\u00b7sche", "w\u00f6l\u00b7ben", "als", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich \u00fcber die Flaschen k\u00fchl,", "tokens": ["Sich", "\u00fc\u00b7ber", "die", "Fla\u00b7schen", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als Tafelmusik beginnen", "tokens": ["Als", "Ta\u00b7fel\u00b7mu\u00b7sik", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die V\u00f6gel im Laub ihr Spiel.", "tokens": ["Die", "V\u00f6\u00b7gel", "im", "Laub", "ihr", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Kreis von gr\u00fcnen B\u00e4umen", "tokens": ["Ein", "Kreis", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Rasen und Gestr\u00e4uch,", "tokens": ["Und", "Ra\u00b7sen", "und", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wallt, zermalmt von Elend,", "tokens": ["Da", "wallt", ",", "zer\u00b7malmt", "von", "E\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Mann gar tr\u00fcb' und bleich.", "tokens": ["Ein", "Mann", "gar", "tr\u00fcb'", "und", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er seufzt, \u2013 da seufzt das Echo,", "tokens": ["Er", "seufzt", ",", "\u2013", "da", "seufzt", "das", "E\u00b7cho", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie eine Stimm' aus dem Grab;", "tokens": ["Wie", "ei\u00b7ne", "Stimm'", "aus", "dem", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er weint, \u2013 da weinen die Zweige", "tokens": ["Er", "weint", ",", "\u2013", "da", "wei\u00b7nen", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "$(", "ADV", "VVINF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Abendthau herab.", "tokens": ["Den", "A\u00b7bendt\u00b7hau", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein Kreis von gr\u00fcnen B\u00e4umen,", "tokens": ["Ein", "Kreis", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestr\u00e4uch und Rasenplan;", "tokens": ["Ge\u00b7str\u00e4uch", "und", "Ra\u00b7sen\u00b7plan", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schleicht mit blankem Dolche", "tokens": ["Es", "schleicht", "mit", "blan\u00b7kem", "Dol\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein M\u00f6rder lauernd heran.", "tokens": ["Ein", "M\u00f6r\u00b7der", "lau\u00b7ernd", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Der B\u00fcsche dichtes Dunkel", "tokens": ["Der", "B\u00fc\u00b7sche", "dich\u00b7tes", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versteckt den Finstern gut;", "tokens": ["Ver\u00b7steckt", "den", "Fins\u00b7tern", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da trieft vom Himmel selber", "tokens": ["Da", "trieft", "vom", "Him\u00b7mel", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Abendroth als Blut.", "tokens": ["Das", "A\u00b7ben\u00b7droth", "als", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Kreis von gr\u00fcnen B\u00e4umen,", "tokens": ["Ein", "Kreis", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestr\u00e4uch und Rasen blos;", "tokens": ["Ge\u00b7str\u00e4uch", "und", "Ra\u00b7sen", "blos", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wallt mit Dint' und Feder", "tokens": ["Da", "wallt", "mit", "Dint'", "und", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Amtmann aus den Schlo\u00df.", "tokens": ["Der", "Amt\u00b7mann", "aus", "den", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Als Pult dient ihm ein Baumstamm,", "tokens": ["Als", "Pult", "dient", "ihm", "ein", "Baum\u00b7stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dran lehnt er die Bogen auf,", "tokens": ["Dran", "lehnt", "er", "die", "Bo\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Zweige sch\u00fctteln als Streusand", "tokens": ["Die", "Zwei\u00b7ge", "sch\u00fct\u00b7teln", "als", "Streu\u00b7sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Bl\u00fcthenstaub ihm drauf.", "tokens": ["Den", "Bl\u00fc\u00b7then\u00b7staub", "ihm", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Kreis von gr\u00fcnen B\u00e4umen,", "tokens": ["Ein", "Kreis", "von", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gestr\u00e4uch und Rasengr\u00fcn,", "tokens": ["Ge\u00b7str\u00e4uch", "und", "Ra\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Bursch' und Dirne lagern", "tokens": ["Und", "Bur\u00b7sch'", "und", "Dir\u00b7ne", "la\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich k\u00fcssend und kosend hin.", "tokens": ["Sich", "k\u00fcs\u00b7send", "und", "ko\u00b7send", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Die B\u00e4ume stehen Wache,", "tokens": ["Die", "B\u00e4u\u00b7me", "ste\u00b7hen", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rasen ist breit und weich,", "tokens": ["Der", "Ra\u00b7sen", "ist", "breit", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Nacht senkt still den Vorhang,", "tokens": ["Die", "Nacht", "senkt", "still", "den", "Vor\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschwiegen ist das Gestr\u00e4uch.", "tokens": ["Ver\u00b7schwie\u00b7gen", "ist", "das", "Ge\u00b7str\u00e4uch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}