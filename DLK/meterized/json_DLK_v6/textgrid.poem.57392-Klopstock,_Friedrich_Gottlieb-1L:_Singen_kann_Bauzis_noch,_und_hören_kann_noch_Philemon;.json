{"textgrid.poem.57392": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Singen kann Bauzis noch, und h\u00f6ren kann noch Philemon;", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singen kann Bauzis noch, und h\u00f6ren kann noch Philemon;", "tokens": ["Sin\u00b7gen", "kann", "Bau\u00b7zis", "noch", ",", "und", "h\u00f6\u00b7ren", "kann", "noch", "Phi\u00b7le\u00b7mon", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NE", "ADV", "$,", "KON", "VVINF", "VMFIN", "ADV", "NE", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Und wir beyden wallten doch schon", "tokens": ["Und", "wir", "bey\u00b7den", "wall\u00b7ten", "doch", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PIAT", "NN", "ADV", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Weiter als Ein Jahrhundert. Ich war ganz Ohr, und ich h\u00f6rte", "tokens": ["Wei\u00b7ter", "als", "Ein", "Jahr\u00b7hun\u00b7dert", ".", "Ich", "war", "ganz", "Ohr", ",", "und", "ich", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Alles; denn Herz war ich auch.", "tokens": ["Al\u00b7les", ";", "denn", "Herz", "war", "ich", "auch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "KON", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bauzis war auch Herz! Du s\u00e4ngest nicht, wie du singest;", "tokens": ["Bau\u00b7zis", "war", "auch", "Herz", "!", "Du", "s\u00e4n\u00b7gest", "nicht", ",", "wie", "du", "sin\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "W\u00e4rst du nicht Herz.", "tokens": ["W\u00e4rst", "du", "nicht", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wem sie den Ton trug, f\u00fchlt es. Wie gl\u00fccket ihr: Ombre Compague,", "tokens": ["Wem", "sie", "den", "Ton", "trug", ",", "f\u00fchlt", "es", ".", "Wie", "gl\u00fc\u00b7cket", "ihr", ":", "Om\u00b7bre", "Com\u00b7pa\u00b7gue", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "$.", "NE", "NE", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Oder: Willkommen, o silberner Mond.", "tokens": ["O\u00b7der", ":", "Will\u00b7kom\u00b7men", ",", "o", "sil\u00b7ber\u00b7ner", "Mond", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$.", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Und wie zaubert sie T\u00f6ne, f\u00fcr die's dem Erfinder der Weisen", "tokens": ["Und", "wie", "zau\u00b7bert", "sie", "T\u00f6\u00b7ne", ",", "f\u00fcr", "die's", "dem", "Er\u00b7fin\u00b7der", "der", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "NN", "$,", "APPR", "PDS", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "An der Bezeichnung gebricht.", "tokens": ["An", "der", "Be\u00b7zeich\u00b7nung", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Farb' ist nicht Menschenstimme. Wie Bauzis dem Ohre, gef\u00e4llt dem", "tokens": ["Fa\u00b7rb'", "ist", "nicht", "Men\u00b7schen\u00b7stim\u00b7me", ".", "Wie", "Bau\u00b7zis", "dem", "Oh\u00b7re", ",", "ge\u00b7f\u00e4llt", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG", "NN", "$.", "PWAV", "NE", "ART", "NN", "$,", "VVFIN", "ART"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Aug' Angelika nicht.", "tokens": ["Aug'", "An\u00b7ge\u00b7li\u00b7ka", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}