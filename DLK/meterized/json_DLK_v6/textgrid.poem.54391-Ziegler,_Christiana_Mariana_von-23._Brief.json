{"textgrid.poem.54391": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "23. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erlaube, da\u00df mein Blat sich in dein Zimmer wagt,", "tokens": ["Er\u00b7lau\u00b7be", ",", "da\u00df", "mein", "Blat", "sich", "in", "dein", "Zim\u00b7mer", "wagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dir, o F\u00fcrstinn nur von meiner Ehrfurcht sagt.", "tokens": ["Und", "dir", ",", "o", "F\u00fcrs\u00b7tinn", "nur", "von", "mei\u00b7ner", "Ehr\u00b7furcht", "sagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein himmlicher Verstand, dein gn\u00e4diges Bezeigen", "tokens": ["Dein", "himm\u00b7li\u00b7cher", "Ver\u00b7stand", ",", "dein", "gn\u00e4\u00b7di\u00b7ges", "Be\u00b7zei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will meine Vorstellung bey weitem \u00fcbersteigen.", "tokens": ["Will", "mei\u00b7ne", "Vor\u00b7stel\u00b7lung", "bey", "wei\u00b7tem", "\u00fc\u00b7bers\u00b7tei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Durchlauchtigste, du kennst so Kunst als Wissenschaft,", "tokens": ["Durch\u00b7lauch\u00b7tigs\u00b7te", ",", "du", "kennst", "so", "Kunst", "als", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PPER", "VVFIN", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Dichtkunst reinen Thon, Geist, Feuer, Nachdruck, Kraft.", "tokens": ["Der", "Dicht\u00b7kunst", "rei\u00b7nen", "Thon", ",", "Geist", ",", "Feu\u00b7er", ",", "Nach\u00b7druck", ",", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So la\u00df mich dieses mal ein gn\u00e4dig Auge finden.", "tokens": ["So", "la\u00df", "mich", "die\u00b7ses", "mal", "ein", "gn\u00e4\u00b7dig", "Au\u00b7ge", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PDS", "ADV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich soll itzt nicht der Zwang gesuchter Worte binden.", "tokens": ["Mich", "soll", "itzt", "nicht", "der", "Zwang", "ge\u00b7such\u00b7ter", "Wor\u00b7te", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Blo\u00df F\u00fcrstinn, dein Befehl soll mein Gesetze seyn.", "tokens": ["Blo\u00df", "F\u00fcrs\u00b7tinn", ",", "dein", "Be\u00b7fehl", "soll", "mein", "Ge\u00b7set\u00b7ze", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der spricht mir selbst den Muth dir zu gehorchen ein;", "tokens": ["Der", "spricht", "mir", "selbst", "den", "Muth", "dir", "zu", "ge\u00b7hor\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil sich dein hoher Blick nach meinen Schriften lenket,", "tokens": ["Weil", "sich", "dein", "ho\u00b7her", "Blick", "nach", "mei\u00b7nen", "Schrif\u00b7ten", "len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und ihnen Glanz und Werth durch diese Gnade schenket,", "tokens": ["Und", "ih\u00b7nen", "Glanz", "und", "Werth", "durch", "die\u00b7se", "Gna\u00b7de", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Beyfall, F\u00fcrstinn, ist vor mich der gr\u00f6ste Schatz.", "tokens": ["Dein", "Bey\u00b7fall", ",", "F\u00fcrs\u00b7tinn", ",", "ist", "vor", "mich", "der", "gr\u00f6s\u00b7te", "Schatz", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den B\u00fcchern g\u00f6nne nur auch den geringsten Platz.", "tokens": ["Den", "B\u00fc\u00b7chern", "g\u00f6n\u00b7ne", "nur", "auch", "den", "ge\u00b7rings\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durchlauchtigste, k\u00f6nnt ich das Gl\u00fcck vor sie geniessen,", "tokens": ["Durch\u00b7lauch\u00b7tigs\u00b7te", ",", "k\u00f6nnt", "ich", "das", "Gl\u00fcck", "vor", "sie", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In Unterth\u00e4nigkeit dir Hand und Rock zu k\u00fcssen.", "tokens": ["In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", "dir", "Hand", "und", "Rock", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Itzt nehm ich einen Freund zu meinem Beystand an", "tokens": ["Itzt", "nehm", "ich", "ei\u00b7nen", "Freund", "zu", "mei\u00b7nem", "Beys\u00b7tand", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der mir dein Gn\u00e4dig seyn zum ersten kund gethan;", "tokens": ["Der", "mir", "dein", "Gn\u00e4\u00b7dig", "seyn", "zum", "ers\u00b7ten", "kund", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJD", "VAINF", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wenn es dir gef\u00e4llt, so wirst du von ihm h\u00f6ren,", "tokens": ["Und", "wenn", "es", "dir", "ge\u00b7f\u00e4llt", ",", "so", "wirst", "du", "von", "ihm", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df deine Huld mich zwingt dich ewig zu verehren.", "tokens": ["Da\u00df", "dei\u00b7ne", "Huld", "mich", "zwingt", "dich", "e\u00b7wig", "zu", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}