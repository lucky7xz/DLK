{"dta.poem.8389": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Das liecht wardt schwartze nacht; der himmel lieff zuesammen", "tokens": ["Das", "liecht", "wardt", "schwart\u00b7ze", "nacht", ";", "der", "him\u00b7mel", "lieff", "zu\u00b7es\u00b7am\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zn dickes finsterni\u00df; die wolcken gaben flammen", "tokens": ["Zn", "di\u00b7ckes", "fins\u00b7ter\u00b7ni\u00df", ";", "die", "wol\u00b7cken", "ga\u00b7ben", "flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vndt eilten hefftig fort; man sahe keinen tag", "tokens": ["Vndt", "eil\u00b7ten", "heff\u00b7tig", "fort", ";", "man", "sa\u00b7he", "kei\u00b7nen", "tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$.", "PIS", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wann der grimme plitz durch einen donneischlag", "tokens": ["Als", "wann", "der", "grim\u00b7me", "plitz", "durch", "ei\u00b7nen", "don\u00b7nei\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vorher gesendet kam; der winde starckes prausen", "tokens": ["Vor\u00b7her", "ge\u00b7sen\u00b7det", "kam", ";", "der", "win\u00b7de", "star\u00b7ckes", "prau\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bewegte waldt vndt berg mitt seinem wilden sausen;", "tokens": ["Be\u00b7weg\u00b7te", "waldt", "vndt", "berg", "mitt", "sei\u00b7nem", "wil\u00b7den", "sau\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die lufft wardt lauter see; der h\u00f6llen gantzes reich", "tokens": ["Die", "lufft", "wardt", "lau\u00b7ter", "see", ";", "der", "h\u00f6l\u00b7len", "gant\u00b7zes", "reich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$.", "ART", "ADJA", "ADJA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erregte seine krafft; die b\u00e4wme wurden bleich;", "tokens": ["Er\u00b7reg\u00b7te", "sei\u00b7ne", "krafft", ";", "die", "b\u00e4w\u00b7me", "wur\u00b7den", "bleich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}