{"textgrid.poem.61772": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wittenborg", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war Johannes Wittenborg,", "tokens": ["Das", "war", "Jo\u00b7han\u00b7nes", "Wit\u00b7ten\u00b7borg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Admiral vom Bunde,", "tokens": ["Der", "Ad\u00b7mi\u00b7ral", "vom", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er nahm Bornholm, das feste Schlo\u00df,", "tokens": ["Er", "nahm", "Born\u00b7holm", ",", "das", "fes\u00b7te", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fuhr hinab zum Sunde.", "tokens": ["Und", "fuhr", "hin\u00b7ab", "zum", "Sun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wo er traf ein D\u00e4nenschiff,", "tokens": ["Und", "wo", "er", "traf", "ein", "D\u00e4\u00b7nen\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das stolz die Segel bl\u00e4hte,", "tokens": ["Das", "stolz", "die", "Se\u00b7gel", "bl\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbrannt' er's oder f\u00fchrt' es mit", "tokens": ["Ver\u00b7brannt'", "er's", "o\u00b7der", "f\u00fchrt'", "es", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Beute f\u00fcr die St\u00e4dte.", "tokens": ["Als", "Beu\u00b7te", "f\u00fcr", "die", "St\u00e4d\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als er kam vor Helsing\u00f6r,", "tokens": ["Und", "als", "er", "kam", "vor", "Hel\u00b7sin\u00b7g\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Volk ergriff ein Zagen,", "tokens": ["Das", "Volk", "er\u00b7griff", "ein", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem K\u00f6nig deuchte pl\u00f6tzlich schw\u00fcl", "tokens": ["Dem", "K\u00f6\u00b7nig", "deuch\u00b7te", "pl\u00f6tz\u00b7lich", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft zu Kopenhagen.", "tokens": ["Die", "Luft", "zu", "Ko\u00b7pen\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sandte Brief und Boten aus,", "tokens": ["Er", "sand\u00b7te", "Brief", "und", "Bo\u00b7ten", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Admiral zu gr\u00fc\u00dfen:", "tokens": ["Den", "Ad\u00b7mi\u00b7ral", "zu", "gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbla\u00df ab vom Kampf und komm ans Land,", "tokens": ["\u00bb", "la\u00df", "ab", "vom", "Kampf", "und", "komm", "ans", "Land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen Frieden schlie\u00dfen.", "tokens": ["Wir", "wol\u00b7len", "Frie\u00b7den", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und bis vollf\u00fchrt das S\u00fchnungswerk", "tokens": ["Und", "bis", "voll\u00b7f\u00fchrt", "das", "S\u00fch\u00b7nungs\u00b7werk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Bund und uns zum Frommen,", "tokens": ["Dem", "Bund", "und", "uns", "zum", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im alten Schlo\u00df von Helsing\u00f6r", "tokens": ["Im", "al\u00b7ten", "Schlo\u00df", "von", "Hel\u00b7sin\u00b7g\u00f6r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei mir als Gast willkommen!\u00ab \u2013", "tokens": ["Sei", "mir", "als", "Gast", "will\u00b7kom\u00b7men", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "KOUS", "NN", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im alten Schlo\u00df zu Helsing\u00f6r,", "tokens": ["Im", "al\u00b7ten", "Schlo\u00df", "zu", "Hel\u00b7sin\u00b7g\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schallen Pauken und Zinken,", "tokens": ["Da", "schal\u00b7len", "Pau\u00b7ken", "und", "Zin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Diener rennen aus und ein,", "tokens": ["Die", "Die\u00b7ner", "ren\u00b7nen", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die g\u00fcldnen Becher blinken.", "tokens": ["Die", "g\u00fcld\u00b7nen", "Be\u00b7cher", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bei Tafel sitzt Hans Wittenborg", "tokens": ["Bei", "Ta\u00b7fel", "sitzt", "Hans", "Wit\u00b7ten\u00b7borg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewappnet wie zum Streite,", "tokens": ["Ge\u00b7wapp\u00b7net", "wie", "zum", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die K\u00f6nigstochter aus D\u00e4nemark,", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "aus", "D\u00e4\u00b7ne\u00b7mark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die sitzt an seiner Seite.", "tokens": ["Die", "sitzt", "an", "sei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die K\u00f6nigstochter aus D\u00e4nemark,", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "aus", "D\u00e4\u00b7ne\u00b7mark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die wei\u00df so s\u00fc\u00df zu blicken,", "tokens": ["Die", "wei\u00df", "so", "s\u00fc\u00df", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Goldnetz ist ihr wellig Haar,", "tokens": ["Ein", "Gold\u00b7netz", "ist", "ihr", "wel\u00b7lig", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Herzen zu bestricken.", "tokens": ["Um", "Her\u00b7zen", "zu", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie lacht und schwatzt und l\u00e4\u00dft sich hold", "tokens": ["Sie", "lacht", "und", "schwatzt", "und", "l\u00e4\u00dft", "sich", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein zaudernd Wort gefallen,", "tokens": ["Sein", "zau\u00b7dernd", "Wort", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schenkt ihm ein und trinkt ihm zu,", "tokens": ["Sie", "schenkt", "ihm", "ein", "und", "trinkt", "ihm", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Blut beginnt zu wallen.", "tokens": ["Sein", "Blut", "be\u00b7ginnt", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sch\u00f6n Sigbrit hebt die Tafel auf,", "tokens": ["Sch\u00f6n", "Sig\u00b7brit", "hebt", "die", "Ta\u00b7fel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da rufen lauter die Geigen. \u2013", "tokens": ["Da", "ru\u00b7fen", "lau\u00b7ter", "die", "Gei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bblegt ab den Panzer, Admiral,", "tokens": ["\u00bb", "legt", "ab", "den", "Pan\u00b7zer", ",", "Ad\u00b7mi\u00b7ral", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun geht's zum Fackelreigen.\u00ab", "tokens": ["Nun", "geht's", "zum", "Fa\u00b7ckel\u00b7rei\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und als er tanzt mit ihr im Saal,", "tokens": ["Und", "als", "er", "tanzt", "mit", "ihr", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schwindeln ihm die Sinne,", "tokens": ["Da", "schwin\u00b7deln", "ihm", "die", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm ist's, als ob aus ihrer Hand", "tokens": ["Ihm", "ist's", ",", "als", "ob", "aus", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Strom von Flammen rinne.", "tokens": ["Ein", "Strom", "von", "Flam\u00b7men", "rin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie merkt es wohl und schaut ihn an", "tokens": ["Sie", "merkt", "es", "wohl", "und", "schaut", "ihn", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fl\u00f6tet leis im Tanze:", "tokens": ["Und", "fl\u00f6\u00b7tet", "leis", "im", "Tan\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbgib uns Bornholm, und dir geh\u00f6rt", "tokens": ["\u00bb", "gib", "uns", "Born\u00b7holm", ",", "und", "dir", "ge\u00b7h\u00f6rt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "NE", "$,", "KON", "PPER", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Ros' aus meinem Kranze.\u00ab", "tokens": ["Die", "Ros'", "aus", "mei\u00b7nem", "Kran\u00b7ze", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbdie Ros' aus Eurem Kranz ist sch\u00f6n,", "tokens": ["\u00bb", "die", "Ros'", "aus", "Eu\u00b7rem", "Kranz", "ist", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rubin erbleicht daneben;", "tokens": ["Ru\u00b7bin", "er\u00b7bleicht", "da\u00b7ne\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit Freuden g\u00e4b' ich drum mein Blut,", "tokens": ["Mit", "Freu\u00b7den", "g\u00e4b'", "ich", "drum", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bornholm kann ich nicht geben.\u00ab", "tokens": ["Born\u00b7holm", "kann", "ich", "nicht", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "\u00bbgib uns Bornholm, das feste Schlo\u00df,", "tokens": ["\u00bb", "gib", "uns", "Born\u00b7holm", ",", "das", "fes\u00b7te", "Schlo\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und nimm daf\u00fcr zur Stunde,", "tokens": ["Und", "nimm", "da\u00b7f\u00fcr", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nimm hin daf\u00fcr, du stolzer Mann,", "tokens": ["Nimm", "hin", "da\u00b7f\u00fcr", ",", "du", "stol\u00b7zer", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Ku\u00df von meinem Munde.\u00ab \u2013", "tokens": ["Den", "Ku\u00df", "von", "mei\u00b7nem", "Mun\u00b7de", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$