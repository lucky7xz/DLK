{"textgrid.poem.40574": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus der Gruft heraus im Grabeskleid,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der Gruft heraus im Grabeskleid,", "tokens": ["Aus", "der", "Gruft", "he\u00b7raus", "im", "Gra\u00b7bes\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nach dem Garten wallt die todte Maid,", "tokens": ["Nach", "dem", "Gar\u00b7ten", "wallt", "die", "tod\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Den sie einst so liebevoll gepflegt,", "tokens": ["Den", "sie", "einst", "so", "lie\u00b7be\u00b7voll", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der wohl tief um sie jetzt Trauer tr\u00e4gt.", "tokens": ["Der", "wohl", "tief", "um", "sie", "jetzt", "Trau\u00b7er", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwei\u00dfe Lilien, wie mein Herz so rein,", "tokens": ["\u00bb", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en", ",", "wie", "mein", "Herz", "so", "rein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "KOKOM", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weinen wohl ums todte Schwesterlein?\u00ab", "tokens": ["Wei\u00b7nen", "wohl", "ums", "tod\u00b7te", "Schwes\u00b7ter\u00b7lein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, die Lilien weinen nimmermehr,", "tokens": ["Ach", ",", "die", "Li\u00b7li\u00b7en", "wei\u00b7nen", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVINF", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nein, ihr Kelch ist licht und thr\u00e4nenleer.", "tokens": ["Nein", ",", "ihr", "Kelch", "ist", "licht", "und", "thr\u00e4\u00b7nen\u00b7leer", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbmeine Rosen, die ich so geliebt,", "tokens": ["\u00bb", "mei\u00b7ne", "Ro\u00b7sen", ",", "die", "ich", "so", "ge\u00b7liebt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wohl seid ihr erbla\u00dft und tief betr\u00fcbt?\u00ab", "tokens": ["Wohl", "seid", "ihr", "er\u00b7bla\u00dft", "und", "tief", "be\u00b7tr\u00fcbt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "KON", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach, nicht f\u00e4rbte Gram die Rosen bleich,", "tokens": ["Ach", ",", "nicht", "f\u00e4rb\u00b7te", "Gram", "die", "Ro\u00b7sen", "bleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "VVFIN", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nein, sie gl\u00fchen fort gar wonnereich.", "tokens": ["Nein", ",", "sie", "gl\u00fc\u00b7hen", "fort", "gar", "won\u00b7ne\u00b7reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKVZ", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbnachtigall, du meines Herzens Herz,", "tokens": ["\u00bb", "nach\u00b7ti\u00b7gall", ",", "du", "mei\u00b7nes", "Her\u00b7zens", "Herz", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wohl ist deine Brust jetzt stumm vor Schmerz?\u00ab", "tokens": ["Wohl", "ist", "dei\u00b7ne", "Brust", "jetzt", "stumm", "vor", "Schmerz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, nicht ist verstummt die Nachtigall,", "tokens": ["Ach", ",", "nicht", "ist", "ver\u00b7stummt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "VAFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Wipfel schmettert laut ihr Schall.", "tokens": ["Durch", "die", "Wip\u00b7fel", "schmet\u00b7tert", "laut", "ihr", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbbl\u00fcthenbaum, du neigst dein trauernd Haupt,", "tokens": ["\u00bb", "bl\u00fc\u00b7then\u00b7baum", ",", "du", "neigst", "dein", "trau\u00b7ernd", "Haupt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Weil du nun der Pflegerin beraubt?\u00ab", "tokens": ["Weil", "du", "nun", "der", "Pfle\u00b7ge\u00b7rin", "be\u00b7raubt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, nicht ist des Baumes Haupt geneigt,", "tokens": ["Ach", ",", "nicht", "ist", "des", "Bau\u00b7mes", "Haupt", "ge\u00b7neigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sondern freudig in die Wolken steigt.", "tokens": ["Son\u00b7dern", "freu\u00b7dig", "in", "die", "Wol\u00b7ken", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Einen J\u00fcngling, den sie nie gesehn,", "tokens": ["Ei\u00b7nen", "J\u00fcng\u00b7ling", ",", "den", "sie", "nie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sieht sie jetzt bei ihren Blumen stehn.", "tokens": ["Sieht", "sie", "jetzt", "bei", "ih\u00b7ren", "Blu\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbfremdling, sprich, was f\u00fchrt zu dieser Zeit", "tokens": ["\u00bb", "fremd\u00b7ling", ",", "sprich", ",", "was", "f\u00fchrt", "zu", "die\u00b7ser", "Zeit"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADJD", "$,", "PWS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In den Garten dich der todten Maid?\u00ab", "tokens": ["In", "den", "Gar\u00b7ten", "dich", "der", "tod\u00b7ten", "Maid", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "\u00bbstatt der Rosen bin ich gramesbleich,", "tokens": ["\u00bb", "statt", "der", "Ro\u00b7sen", "bin", "ich", "gra\u00b7mes\u00b7bleich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Statt der Nachtigall so schmerzenreich,", "tokens": ["Statt", "der", "Nach\u00b7ti\u00b7gall", "so", "schmer\u00b7zen\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Statt des Baums neigt meine Stirne sich,", "tokens": ["Statt", "des", "Baums", "neigt", "mei\u00b7ne", "Stir\u00b7ne", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Statt der Lilien wein' ich still um dich.\u00ab", "tokens": ["Statt", "der", "Li\u00b7li\u00b7en", "wein'", "ich", "still", "um", "dich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}