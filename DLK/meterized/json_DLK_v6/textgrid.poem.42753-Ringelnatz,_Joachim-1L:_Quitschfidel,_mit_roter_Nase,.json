{"textgrid.poem.42753": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Quitschfidel, mit roter Nase,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Quitschfidel, mit roter Nase,", "tokens": ["Quitschfi\u00b7del", ",", "mit", "ro\u00b7ter", "Na\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Seh ich Kunze gehn.", "tokens": ["Seh", "ich", "Kun\u00b7ze", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Warum bleibt er pl\u00f6tzlich mitten auf der Stra\u00dfe", "tokens": ["Wa\u00b7rum", "bleibt", "er", "pl\u00f6tz\u00b7lich", "mit\u00b7ten", "auf", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Stehn?", "tokens": ["Stehn", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Warum runzelt er die Brauen?", "tokens": ["Wa\u00b7rum", "run\u00b7zelt", "er", "die", "Brau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Warum mag er so entsetzlich", "tokens": ["Wa\u00b7rum", "mag", "er", "so", "ent\u00b7setz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schmerzlich himmelaufw\u00e4rts schauen?", "tokens": ["Schmerz\u00b7lich", "him\u00b7me\u00b7lauf\u00b7w\u00e4rts", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Warum wird er pl\u00f6tzlich", "tokens": ["Wa\u00b7rum", "wird", "er", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Bla\u00df?", "tokens": ["Bla\u00df", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Oh, nun wei\u00df ich was \u2013 \u2013 \u2013", "tokens": ["Oh", ",", "nun", "wei\u00df", "ich", "was", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PWS", "$(", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Er erblickt mich, winkt. \u2013 Fatal!", "tokens": ["Er", "er\u00b7blickt", "mich", ",", "winkt", ".", "\u2013", "Fa\u00b7tal", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "$.", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "\u00bbservus, armer Kunze! \u2013 eilt! \u2013 ein andermal!\u00ab", "tokens": ["\u00bb", "ser\u00b7vus", ",", "ar\u00b7mer", "Kun\u00b7ze", "!", "\u2013", "eilt", "!", "\u2013", "ein", "an\u00b7der\u00b7mal", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "$,", "ADJA", "NN", "$.", "$(", "VVFIN", "$.", "$(", "ART", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Kalte, falsche, r\u00fccksichtslose", "tokens": ["Kal\u00b7te", ",", "fal\u00b7sche", ",", "r\u00fcck\u00b7sichts\u00b7lo\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Freunde hat die Unterhose.", "tokens": ["Freun\u00b7de", "hat", "die", "Un\u00b7ter\u00b7ho\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}