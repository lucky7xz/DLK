{"dta.poem.10819": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Buch mit welken Blumen  \n Bl\u00e4ttern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Antophilus war stets gewohnt, der Blu-\nmen Sch\u00f6nheit zu betrachten;", "tokens": ["An\u00b7to\u00b7phi\u00b7lus", "war", "stets", "ge\u00b7wohnt", ",", "der", "Blu", "men", "Sch\u00f6n\u00b7heit", "zu", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "$,", "ART", "TRUNC", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Und sie als Kinder der Natur, als", "tokens": ["Und", "sie", "als", "Kin\u00b7der", "der", "Na\u00b7tur", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "KOUS", "NN", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sahe aber auch dabei, da\u00df dieses bunt gemahlte", "tokens": ["Er", "sa\u00b7he", "a\u00b7ber", "auch", "da\u00b7bei", ",", "da\u00df", "die\u00b7ses", "bunt", "ge\u00b7mahl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PAV", "$,", "KOUS", "PDAT", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ein Sinnbild der Verg\u00e4nglichkeit, von einer kur-", "tokens": ["Ein", "Sinn\u00b7bild", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ",", "von", "ei\u00b7ner", "kur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er w\u00fcnschte diese Sinnenlust, sich an den wohl ge-", "tokens": ["Er", "w\u00fcnschte", "die\u00b7se", "Sin\u00b7nen\u00b7lust", ",", "sich", "an", "den", "wohl", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PRF", "APPR", "ART", "ADV", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Auch gerne zu der Winterszeit bey einem strengen", "tokens": ["Auch", "ger\u00b7ne", "zu", "der", "Win\u00b7ter\u00b7szeit", "bey", "ei\u00b7nem", "stren\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Regeln der Natur sind fest, die Zeit bleibt vol-", "tokens": ["Die", "Re\u00b7geln", "der", "Na\u00b7tur", "sind", "fest", ",", "die", "Zeit", "bleibt", "vol"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und nach der Jahres Zeitenlauf, erfolgt auch die", "tokens": ["Und", "nach", "der", "Jah\u00b7res", "Zei\u00b7ten\u00b7lauf", ",", "er\u00b7folgt", "auch", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An Blumen die in G\u00e4rten bl\u00fchn. Der Herbstwind", "tokens": ["An", "Blu\u00b7men", "die", "in", "G\u00e4r\u00b7ten", "bl\u00fchn", ".", "Der", "Herbst\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "APPR", "NN", "VVINF", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verdorrt der Blumen Herrlichkeit, die vor den", "tokens": ["Ver\u00b7dorrt", "der", "Blu\u00b7men", "Herr\u00b7lich\u00b7keit", ",", "die", "vor", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Schnee verdekt der Blumen Gold, begr\u00e4bt", "tokens": ["Der", "Schnee", "ver\u00b7dekt", "der", "Blu\u00b7men", "Gold", ",", "be\u00b7gr\u00e4bt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und h\u00fcllt die Kinder der Natur in lauter Todten", "tokens": ["Und", "h\u00fcllt", "die", "Kin\u00b7der", "der", "Na\u00b7tur", "in", "lau\u00b7ter", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Anthophilus der dies erfahrn, wie bald die Blumen", "tokens": ["An\u00b7tho\u00b7phi\u00b7lus", "der", "dies", "er\u00b7fahrn", ",", "wie", "bald", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "PDS", "VVINF", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der samlete sich in ein Buch, dennoch ein welkendes", "tokens": ["Der", "sam\u00b7le\u00b7te", "sich", "in", "ein", "Buch", ",", "den\u00b7noch", "ein", "wel\u00b7ken\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Er la\u00df die sch\u00f6nen Sorten aus, von Tulpen und", "tokens": ["Er", "la\u00df", "die", "sch\u00f6\u00b7nen", "Sor\u00b7ten", "aus", ",", "von", "Tul\u00b7pen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da er von jedem sich ein Blat, das welk und d\u00fcr-", "tokens": ["Da", "er", "von", "je\u00b7dem", "sich", "ein", "Blat", ",", "das", "welk", "und", "d\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "PRF", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Firnis, damit sie beschmiert, der hatte diesen", "tokens": ["Ein", "Fir\u00b7nis", ",", "da\u00b7mit", "sie", "be\u00b7schmiert", ",", "der", "hat\u00b7te", "die\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$,", "PRELS", "VAFIN", "PDAT"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und eingermassen auch dabei, den sonst gehabten", "tokens": ["Und", "ein\u00b7ger\u00b7mas\u00b7sen", "auch", "da\u00b7bei", ",", "den", "sonst", "ge\u00b7hab\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das Buch war ihm zur Winterszeit gleich einem", "tokens": ["Das", "Buch", "war", "ihm", "zur", "Win\u00b7ter\u00b7szeit", "gleich", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der ihm der Blumen Herrlichkeit und mannigfalt-", "tokens": ["Der", "ihm", "der", "Blu\u00b7men", "Herr\u00b7lich\u00b7keit", "und", "man\u00b7nig\u00b7fal\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es sahe Gotthold einst das Buch und w\u00fcnschte von", "tokens": ["Es", "sa\u00b7he", "Got\u00b7thold", "einst", "das", "Buch", "und", "w\u00fcnschte", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ADV", "ART", "NN", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die er uns sonsten noch geschenkt, ein gleiches Denk-", "tokens": ["Die", "er", "uns", "sons\u00b7ten", "noch", "ge\u00b7schenkt", ",", "ein", "glei\u00b7ches", "Denk"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie manche Wollthat schenkt er uns, die man aus", "tokens": ["Wie", "man\u00b7che", "Woll\u00b7that", "schenkt", "er", "uns", ",", "die", "man", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PIS", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Wenn sie nicht mehr nach dem Genus, bei uns noch", "tokens": ["Wenn", "sie", "nicht", "mehr", "nach", "dem", "Ge\u00b7nus", ",", "bei", "uns", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Wie viele Proben h\u00e4tten wir von unsers Sch\u00f6pfers", "tokens": ["Wie", "vie\u00b7le", "Pro\u00b7ben", "h\u00e4t\u00b7ten", "wir", "von", "un\u00b7sers", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Womit er uns hat Lebenslang, als wie mit einer sch\u00f6-", "tokens": ["Wo\u00b7mit", "er", "uns", "hat", "Le\u00b7bens\u00b7lang", ",", "als", "wie", "mit", "ei\u00b7ner", "sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "NN", "$,", "KOUS", "KOKOM", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "Vergn\u00fcgt gemacht das Herz erquikt; allein wo ist", "tokens": ["Ver\u00b7gn\u00fcgt", "ge\u00b7macht", "das", "Herz", "er\u00b7quikt", ";", "al\u00b7lein", "wo", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ART", "NN", "VVPP", "$.", "ADV", "PWAV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vor so viel Zeichen seiner G\u00fct, damit er unser Herz", "tokens": ["Vor", "so", "viel", "Zei\u00b7chen", "sei\u00b7ner", "G\u00fct", ",", "da\u00b7mit", "er", "un\u00b7ser", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.29": {"text": "Es wird die rege Herzens Pflicht darum nicht immer", "tokens": ["Es", "wird", "die", "re\u00b7ge", "Her\u00b7zens", "Pflicht", "da\u00b7rum", "nicht", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "PAV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil das Ged\u00e4chtnis unsrer Lust, gleich wie ein", "tokens": ["Weil", "das", "Ge\u00b7d\u00e4cht\u00b7nis", "uns\u00b7rer", "Lust", ",", "gleich", "wie", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "ART"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Wie gut w\u00e4r hier ein Denkbuch nicht, darinnen", "tokens": ["Wie", "gut", "w\u00e4r", "hier", "ein", "Denk\u00b7buch", "nicht", ",", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Was GOttes G\u00fct an uns gethan was in dem Lebens-", "tokens": ["Was", "Got\u00b7tes", "G\u00fct", "an", "uns", "ge\u00b7than", "was", "in", "dem", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "NN", "APPR", "PPER", "VVPP", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}