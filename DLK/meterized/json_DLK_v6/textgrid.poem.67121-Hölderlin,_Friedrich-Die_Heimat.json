{"textgrid.poem.67121": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Froh kehrt der Schiffer heim an den stillen Strom", "tokens": ["Froh", "kehrt", "der", "Schif\u00b7fer", "heim", "an", "den", "stil\u00b7len", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von fernen Inseln, wo er geerntet hat;", "tokens": ["Von", "fer\u00b7nen", "In\u00b7seln", ",", "wo", "er", "ge\u00b7ern\u00b7tet", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl m\u00f6cht auch ich zur Heimat wieder;", "tokens": ["Wohl", "m\u00f6cht", "auch", "ich", "zur", "Hei\u00b7mat", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber was hab ich, wie Leid, geerntet? \u2013", "tokens": ["A\u00b7ber", "was", "hab", "ich", ",", "wie", "Leid", ",", "ge\u00b7ern\u00b7tet", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "$,", "PWAV", "NN", "$,", "VVPP", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ihr holden Ufer, die ihr mich auferzogt,", "tokens": ["Ihr", "hol\u00b7den", "U\u00b7fer", ",", "die", "ihr", "mich", "auf\u00b7er\u00b7zogt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stillt ihr der Liebe Leiden? ach! gebt ihr mir,", "tokens": ["Stillt", "ihr", "der", "Lie\u00b7be", "Lei\u00b7den", "?", "ach", "!", "gebt", "ihr", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "XY", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr W\u00e4lder meiner Kindheit, wann ich", "tokens": ["Ihr", "W\u00e4l\u00b7der", "mei\u00b7ner", "Kind\u00b7heit", ",", "wann", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,", "PWAV", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Komme, die Ruhe noch Einmal wieder?", "tokens": ["Kom\u00b7me", ",", "die", "Ru\u00b7he", "noch", "Ein\u00b7mal", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}