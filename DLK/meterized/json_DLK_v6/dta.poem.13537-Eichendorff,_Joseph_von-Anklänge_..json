{"dta.poem.13537": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling will das Blau befreien,", "tokens": ["Fr\u00fch\u00b7ling", "will", "das", "Blau", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Gr\u00fcne, aus dem Schein", "tokens": ["Aus", "der", "Gr\u00fc\u00b7ne", ",", "aus", "dem", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ruft es lockend: Ewig Dein \u2014", "tokens": ["Ruft", "es", "lo\u00b7ckend", ":", "E\u00b7wig", "Dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "ADJD", "PPOSAT", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Minne Zaubereien", "tokens": ["Aus", "der", "Min\u00b7ne", "Zau\u00b7be\u00b7rei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df er sehnen sich nach Fernen,", "tokens": ["Mu\u00df", "er", "seh\u00b7nen", "sich", "nach", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denkend alter Wunderpracht,", "tokens": ["Den\u00b7kend", "al\u00b7ter", "Wun\u00b7der\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter Blumen, Klang und Sternen", "tokens": ["Un\u00b7ter", "Blu\u00b7men", ",", "Klang", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der dunkelgr\u00fcnen Nacht.", "tokens": ["In", "der", "dun\u00b7kel\u00b7gr\u00fc\u00b7nen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}