{"textgrid.poem.24687": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die eine f\u00fcllt die gro\u00dfen Delfter Kr\u00fcge,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die eine f\u00fcllt die gro\u00dfen Delfter Kr\u00fcge,", "tokens": ["Die", "ei\u00b7ne", "f\u00fcllt", "die", "gro\u00b7\u00dfen", "Delf\u00b7ter", "Kr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf denen blaue Drachen sind und V\u00f6gel,", "tokens": ["Auf", "de\u00b7nen", "blau\u00b7e", "Dra\u00b7chen", "sind", "und", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einer lockern Garbe lichter Bl\u00fcten:", "tokens": ["Mit", "ei\u00b7ner", "lo\u00b7ckern", "Gar\u00b7be", "lich\u00b7ter", "Bl\u00fc\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ist Jasmin, da quellen reife Rosen", "tokens": ["Da", "ist", "Jas\u00b7min", ",", "da", "quel\u00b7len", "rei\u00b7fe", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Dahlien und Nelken und Narzissen ...", "tokens": ["Und", "Da\u00b7hli\u00b7en", "und", "Nel\u00b7ken", "und", "Nar\u00b7zis\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dar\u00fcber tanzen hohe Margeriten", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "tan\u00b7zen", "ho\u00b7he", "Mar\u00b7ge\u00b7ri\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Fliederdolden wiegen sich und Schneeball", "tokens": ["Und", "Flie\u00b7der\u00b7dol\u00b7den", "wie\u00b7gen", "sich", "und", "Schnee\u00b7ball"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Halme nicken, Silberflaum und Rispen ...", "tokens": ["Und", "Hal\u00b7me", "ni\u00b7cken", ",", "Sil\u00b7berf\u00b7laum", "und", "Ris\u00b7pen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein duftend Bacchanal ...", "tokens": ["Ein", "duf\u00b7tend", "Bac\u00b7cha\u00b7nal", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die andre bricht mit blassen feinen Fingern", "tokens": ["Die", "and\u00b7re", "bricht", "mit", "blas\u00b7sen", "fei\u00b7nen", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Langstielige und starre Orchideen,", "tokens": ["Langs\u00b7tie\u00b7li\u00b7ge", "und", "star\u00b7re", "Or\u00b7chi\u00b7de\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Zwei oder drei f\u00fcr eine enge Vase ...", "tokens": ["Zwei", "o\u00b7der", "drei", "f\u00fcr", "ei\u00b7ne", "en\u00b7ge", "Va\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Aufragend mit den Farben die verklingen,", "tokens": ["Auf\u00b7ra\u00b7gend", "mit", "den", "Far\u00b7ben", "die", "ver\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit langen Griffeln, seltsam und gewunden,", "tokens": ["Mit", "lan\u00b7gen", "Grif\u00b7feln", ",", "selt\u00b7sam", "und", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Mit Purpurf\u00e4den und mit grellen Tupfen,", "tokens": ["Mit", "Pur\u00b7pur\u00b7f\u00e4\u00b7den", "und", "mit", "grel\u00b7len", "Tup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mit violetten, braunen Pantherflecken", "tokens": ["Mit", "vi\u00b7o\u00b7let\u00b7ten", ",", "brau\u00b7nen", "Pan\u00b7ther\u00b7fle\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und lauernden, verf\u00fchrerischen Kelchen,", "tokens": ["Und", "lau\u00b7ern\u00b7den", ",", "ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7schen", "Kel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die t\u00f6ten wollen ...", "tokens": ["Die", "t\u00f6\u00b7ten", "wol\u00b7len", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}