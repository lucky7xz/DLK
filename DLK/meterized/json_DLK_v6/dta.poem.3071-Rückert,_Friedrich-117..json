{"dta.poem.3071": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "117.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist der N\u00e4chte Licht und bist des Tages Schatten,", "tokens": ["Du", "bist", "der", "N\u00e4ch\u00b7te", "Licht", "und", "bist", "des", "Ta\u00b7ges", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich verzagen nicht, und la\u00df mich nicht ermatten!", "tokens": ["La\u00df", "mich", "ver\u00b7za\u00b7gen", "nicht", ",", "und", "la\u00df", "mich", "nicht", "er\u00b7mat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O der du bist mein Licht und bist mein Schatten du,", "tokens": ["O", "der", "du", "bist", "mein", "Licht", "und", "bist", "mein", "Schat\u00b7ten", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "VAFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich fl\u00fcchte meinem Licht und meinem Schatten zu.", "tokens": ["Ich", "fl\u00fcch\u00b7te", "mei\u00b7nem", "Licht", "und", "mei\u00b7nem", "Schat\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Mitternacht Ruhlicht, des Mittags Schattenruh,", "tokens": ["Der", "Mit\u00b7ter\u00b7nacht", "Ruh\u00b7licht", ",", "des", "Mit\u00b7tags", "Schat\u00b7ten\u00b7ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich fl\u00fcchte dir, du Licht, dir, du mein Schatten, zu.", "tokens": ["Ich", "fl\u00fcch\u00b7te", "dir", ",", "du", "Licht", ",", "dir", ",", "du", "mein", "Schat\u00b7ten", ",", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "$,", "PPER", "$,", "PPER", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}