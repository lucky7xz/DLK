er Knechte,", "tokens": ["Er", "sieht", "den", "Schwarm", "er\u00b7zog\u00b7ner", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihm sein Haus als G\u00fcther hat beschert,", "tokens": ["Die", "ihm", "sein", "Haus", "als", "G\u00fc\u00b7ther", "hat", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "KOUS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich lagern um den netten Heert,", "tokens": ["Sich", "la\u00b7gern", "um", "den", "net\u00b7ten", "Heert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zwar von beyderley Geschlechte,", "tokens": ["Und", "zwar", "von", "bey\u00b7der\u00b7ley", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als deren grosse Menge zeigt;", "tokens": ["Als", "de\u00b7ren", "gros\u00b7se", "Men\u00b7ge", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie hoch wohl sein Verm\u00f6gen steigt.", "tokens": ["Wie", "hoch", "wohl", "sein", "Ver\u00b7m\u00f6\u00b7gen", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "So sprach nun Alphius verwichen", "tokens": ["So", "sprach", "nun", "Al\u00b7phius", "ver\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NE", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Wuchrer, der im Sinn schon Landman\u0303 war,", "tokens": ["Der", "Wuch\u00b7rer", ",", "der", "im", "Sinn", "schon", "Land\u00b7ma\u00f1", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach diesem hat er ganz und gar", "tokens": ["Nach", "die\u00b7sem", "hat", "er", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Gelder wieder eingestrichen,", "tokens": ["Die", "Gel\u00b7der", "wie\u00b7der", "ein\u00b7ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und suchet sie anjetzo nun", "tokens": ["Und", "su\u00b7chet", "sie", "an\u00b7je\u00b7tzo", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Wucher wieder auszuthun.", "tokens": ["Auf", "Wu\u00b7cher", "wie\u00b7der", "aus\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}