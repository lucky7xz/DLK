{"dta.poem.13454": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Horazens Lob des Landlebens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckseelig! welcher mit Vergn\u00fcgen", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "!", "wel\u00b7cher", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "PWAT", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Aemtern frey, wie jene Welt gethan,", "tokens": ["Von", "A\u00b7em\u00b7tern", "frey", ",", "wie", "je\u00b7ne", "Welt", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PWAV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit seinen eignen Ochsen kan", "tokens": ["Mit", "sei\u00b7nen", "eig\u00b7nen", "Och\u00b7sen", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die v\u00e4terlichen Aecker pfl\u00fcgen;", "tokens": ["Die", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "A\u00b7e\u00b7cker", "pfl\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und der sich keine Sorgen macht,", "tokens": ["Und", "der", "sich", "kei\u00b7ne", "Sor\u00b7gen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wieviel sein Wucher eingebracht.", "tokens": ["Wie\u00b7viel", "sein", "Wu\u00b7cher", "ein\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Feldgeschrey darf ihn nicht wecken,", "tokens": ["Kein", "Feld\u00b7ge\u00b7schrey", "darf", "ihn", "nicht", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhn schrecket nicht das ungest\u00fcme Meer,", "tokens": ["Jhn", "schre\u00b7cket", "nicht", "das", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er meidet jene Schranken sehr,", "tokens": ["Er", "mei\u00b7det", "je\u00b7ne", "Schran\u00b7ken", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihm den Richterstuhl entdecken;", "tokens": ["Die", "ihm", "den", "Rich\u00b7ter\u00b7stuhl", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jener Grossen stolzem Haus", "tokens": ["Und", "je\u00b7ner", "Gros\u00b7sen", "stol\u00b7zem", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weicht er, so viel ihm m\u00f6glich, aus.", "tokens": ["Weicht", "er", ",", "so", "viel", "ihm", "m\u00f6g\u00b7lich", ",", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wogegen er in Feld und Garten", "tokens": ["Wo\u00b7ge\u00b7gen", "er", "in", "Feld", "und", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des jungen Weinstocks aufgeschossne Zucht", "tokens": ["Des", "jun\u00b7gen", "Wein\u00b7stocks", "auf\u00b7ge\u00b7schoss\u00b7ne", "Zucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Pappeln zu verm\u00e4hlen sucht,", "tokens": ["Mit", "Pap\u00b7peln", "zu", "ver\u00b7m\u00e4h\u00b7len", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die B\u00e4ume pflegt er abzuwarten,", "tokens": ["Die", "B\u00e4u\u00b7me", "pflegt", "er", "ab\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Messer wird den faulen gramm", "tokens": ["Sein", "Mes\u00b7ser", "wird", "den", "fau\u00b7len", "gramm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und pfropfet bessre auf den Stamm.", "tokens": ["Und", "pfrop\u00b7fet", "bess\u00b7re", "auf", "den", "Stamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald sieht er in verz\u00e4unten Waiden", "tokens": ["Bald", "sieht", "er", "in", "ver\u00b7z\u00e4un\u00b7ten", "Wai\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein bl\u00f6kend Vieh zerstreut in Heerden,", "tokens": ["Sein", "bl\u00f6\u00b7kend", "Vieh", "zer\u00b7streut", "in", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald kann er sich besch\u00e4ftigt sehn", "tokens": ["Bald", "kann", "er", "sich", "be\u00b7sch\u00e4f\u00b7tigt", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit angenehmen Honigschneiden,", "tokens": ["Mit", "an\u00b7ge\u00b7neh\u00b7men", "Ho\u00b7nig\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Womit er leere Tonnen ziert,", "tokens": ["Wo\u00b7mit", "er", "lee\u00b7re", "Ton\u00b7nen", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch wenn er seine Schafe schiert.", "tokens": ["Auch", "wenn", "er", "sei\u00b7ne", "Scha\u00b7fe", "schiert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat erst der Herbst die gr\u00fcnen Zweige,", "tokens": ["Hat", "erst", "der", "Herbst", "die", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit reifem Obst bes\u00e4t, empor gestreckt,", "tokens": ["Mit", "rei\u00b7fem", "Obst", "be\u00b7s\u00e4t", ",", "em\u00b7por", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was wird ihm nicht f\u00fcr Lust erweckt!", "tokens": ["Was", "wird", "ihm", "nicht", "f\u00fcr", "Lust", "er\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er bricht die wundersch\u00f6ne Feige", "tokens": ["Er", "bricht", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Fei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von ihrem, vor dem Jahre kaum", "tokens": ["Von", "ih\u00b7rem", ",", "vor", "dem", "Jah\u00b7re", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "$,", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst fortgesetztem jungen Baum.", "tokens": ["Erst", "fort\u00b7ge\u00b7setz\u00b7tem", "jun\u00b7gen", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Trauben liest er von den Ranken,", "tokens": ["Die", "Trau\u00b7ben", "liest", "er", "von", "den", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die oftermahls so sch\u00f6ne Farben zeigt,", "tokens": ["Die", "of\u00b7ter\u00b7mahls", "so", "sch\u00f6\u00b7ne", "Far\u00b7ben", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie den Purpur \u00fcbersteigt,", "tokens": ["Da\u00df", "sie", "den", "Pur\u00b7pur", "\u00fc\u00b7bers\u00b7teigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat sie euch schon in Gedanken", "tokens": ["Und", "hat", "sie", "euch", "schon", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Feldbesch\u00fctzer zugedacht,", "tokens": ["Ihr", "Feld\u00b7be\u00b7sch\u00fct\u00b7zer", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ihr sein Land so wohl bewacht.", "tokens": ["Weil", "ihr", "sein", "Land", "so", "wohl", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bald denkt er unter einer Eichen,", "tokens": ["Bald", "denkt", "er", "un\u00b7ter", "ei\u00b7ner", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hochbejahrt, den Gliedern wohl zu thun,", "tokens": ["Die", "hoch\u00b7be\u00b7jahrt", ",", "den", "Glie\u00b7dern", "wohl", "zu", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald will er in dem Grase ruhn,", "tokens": ["Bald", "will", "er", "in", "dem", "Gra\u00b7se", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sieht den Bach vor\u00fcber streichen,", "tokens": ["Er", "sieht", "den", "Bach", "vor\u00b7\u00fc\u00b7ber", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00f6rt den angenehmen Schall", "tokens": ["Und", "h\u00f6rt", "den", "an\u00b7ge\u00b7neh\u00b7men", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von seines hohen Ufers Fall.", "tokens": ["Von", "sei\u00b7nes", "ho\u00b7hen", "U\u00b7fers", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die V\u00f6gel singen in dem Walde,", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "in", "dem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es rauscht ein Qvell; das strudelnde Get\u00f6n", "tokens": ["Es", "rauscht", "ein", "Qvell", ";", "das", "stru\u00b7deln\u00b7de", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Klingt seinen Ohren wundersch\u00f6n,", "tokens": ["Klingt", "sei\u00b7nen", "Oh\u00b7ren", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und winkt dem sanften Schlafe balde;", "tokens": ["Und", "winkt", "dem", "sanf\u00b7ten", "Schla\u00b7fe", "bal\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es schlie\u00dft ihm zu beqvemer Ruh", "tokens": ["Es", "schlie\u00dft", "ihm", "zu", "be\u00b7qve\u00b7mer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Augen von sich selber zu.", "tokens": ["Die", "Au\u00b7gen", "von", "sich", "sel\u00b7ber", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn aber nun des Winters Strenge", "tokens": ["Wenn", "a\u00b7ber", "nun", "des", "Win\u00b7ters", "Stren\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Jahr mit Schnee und Regen \u00fcberh\u00e4uft,", "tokens": ["Das", "Jahr", "mit", "Schnee", "und", "Re\u00b7gen", "\u00fc\u00b7berh\u00b7\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Treibt er das Wild; der Eber l\u00e4uft,", "tokens": ["Treibt", "er", "das", "Wild", ";", "der", "E\u00b7ber", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Hund verfolgt, ins Garnes Enge,", "tokens": ["Vom", "Hund", "ver\u00b7folgt", ",", "ins", "Gar\u00b7nes", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Worauf er, wie es ihm gef\u00e4llt,", "tokens": ["Wo\u00b7rauf", "er", ",", "wie", "es", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Netz und Schlingen Vogel stellt.", "tokens": ["Mit", "Netz", "und", "Schlin\u00b7gen", "Vo\u00b7gel", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Erschrockne Haasen kann er hetzen,", "tokens": ["Er\u00b7schrock\u00b7ne", "Haa\u00b7sen", "kann", "er", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kranich, wenn er sich itzt bey dem Zug", "tokens": ["Den", "Kra\u00b7nich", ",", "wenn", "er", "sich", "itzt", "bey", "dem", "Zug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Schleifen selbst verwirrt genug,", "tokens": ["In", "Schlei\u00b7fen", "selbst", "ver\u00b7wirrt", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entlediget er mit Ergetzen", "tokens": ["Ent\u00b7le\u00b7di\u00b7get", "er", "mit", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+--+---", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Die Lust erfreuet ungemein,", "tokens": ["Die", "Lust", "er\u00b7freu\u00b7et", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie bringt auch manches Wildpret ein.", "tokens": ["Sie", "bringt", "auch", "man\u00b7ches", "Wild\u00b7pret", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wer wollte nun wohl nicht vergessen,", "tokens": ["Wer", "woll\u00b7te", "nun", "wohl", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn uns der Geiz viel Ungemach erweckt,", "tokens": ["Wenn", "uns", "der", "Geiz", "viel", "Un\u00b7ge\u00b7mach", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sonsten manche Sorgen heckt!", "tokens": ["Und", "sons\u00b7ten", "man\u00b7che", "Sor\u00b7gen", "heckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die keusche Frau beschickt indessen", "tokens": ["Die", "keu\u00b7sche", "Frau", "be\u00b7schickt", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die lieben Kinder, K\u00fcch und Haus,", "tokens": ["Die", "lie\u00b7ben", "Kin\u00b7der", ",", "K\u00fcch", "und", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn das macht ihr Gesch\u00e4fte aus.", "tokens": ["Denn", "das", "macht", "ihr", "Ge\u00b7sch\u00e4f\u00b7te", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Wie die Sabiner Weiber thaten,", "tokens": ["Wie", "die", "Sa\u00b7bi\u00b7ner", "Wei\u00b7ber", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein nicht verw\u00f6hnt appulisch Angesicht,", "tokens": ["Ein", "nicht", "ver\u00b7w\u00f6hnt", "ap\u00b7pu\u00b7lisch", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das rauhe Luft, der Sonnen Licht", "tokens": ["Das", "rau\u00b7he", "Luft", ",", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz grob gemacht, und halb gebraten,", "tokens": ["Ganz", "grob", "ge\u00b7macht", ",", "und", "halb", "ge\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So unverdrossen bey der M\u00fch,", "tokens": ["So", "un\u00b7ver\u00b7dros\u00b7sen", "bey", "der", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und treuen Sorgfalt sieht man sie.", "tokens": ["Und", "treu\u00b7en", "Sorg\u00b7falt", "sieht", "man", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So bald sie ohngefehr kann schliessen,", "tokens": ["So", "bald", "sie", "ohn\u00b7ge\u00b7fehr", "kann", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wohl nunmehr der m\u00fcde Ehemann", "tokens": ["Da\u00df", "wohl", "nun\u00b7mehr", "der", "m\u00fc\u00b7de", "E\u00b7he\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Feyerabend machen kann,", "tokens": ["Den", "Fe\u00b7ye\u00b7ra\u00b7bend", "ma\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und er vielleicht wird kommen m\u00fcssen,", "tokens": ["Und", "er", "viel\u00b7leicht", "wird", "kom\u00b7men", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So hat sie den geweihten Heert", "tokens": ["So", "hat", "sie", "den", "ge\u00b7weih\u00b7ten", "Heert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bereits mit Holz zur Glut beschwert.", "tokens": ["Be\u00b7reits", "mit", "Holz", "zur", "Glut", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Schleicht denn das liebe Vieh zur H\u00fcrden,", "tokens": ["Schleicht", "denn", "das", "lie\u00b7be", "Vieh", "zur", "H\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So machet sie die starren Euter leer,", "tokens": ["So", "ma\u00b7chet", "sie", "die", "star\u00b7ren", "Eu\u00b7ter", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn holt sie Wein aus F\u00e4ssern her,", "tokens": ["Denn", "holt", "sie", "Wein", "aus", "F\u00e4s\u00b7sern", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bereitet ihren Tisch nach W\u00fcrden,", "tokens": ["Be\u00b7rei\u00b7tet", "ih\u00b7ren", "Tisch", "nach", "W\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den sie mit Speisen ohne Geld,", "tokens": ["Den", "sie", "mit", "Spei\u00b7sen", "oh\u00b7ne", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit lauter Hausmannskost bestellt.", "tokens": ["Mit", "lau\u00b7ter", "Haus\u00b7manns\u00b7kost", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Lucriner-Austern lie\u00df ich liegen,", "tokens": ["Lu\u00b7cri\u00b7ner\u00b7Aus\u00b7tern", "lie\u00df", "ich", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lachs, Laberdan, Forellen noch vielmehr,", "tokens": ["Lachs", ",", "La\u00b7ber\u00b7dan", ",", "Fo\u00b7rel\u00b7len", "noch", "viel\u00b7mehr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und br\u00e4chte sie ein Sturm gleich her,", "tokens": ["Und", "br\u00e4ch\u00b7te", "sie", "ein", "Sturm", "gleich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Schnepfen, die in Zara fliegen,", "tokens": ["Auch", "Schnep\u00b7fen", ",", "die", "in", "Za\u00b7ra", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst H\u00fchner, her aus Calecut,", "tokens": ["Selbst", "H\u00fch\u00b7ner", ",", "her", "aus", "Ca\u00b7le\u00b7cut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die w\u00e4ren mir doch nicht so gut ", "tokens": ["Die", "w\u00e4\u00b7ren", "mir", "doch", "nicht", "so", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Sie w\u00fcrden mir so wohl nicht schmecken", "tokens": ["Sie", "w\u00fcr\u00b7den", "mir", "so", "wohl", "nicht", "schme\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie den, der Oliven fette Frucht,", "tokens": ["Als", "wie", "den", ",", "der", "O\u00b7li\u00b7ven", "fet\u00b7te", "Frucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die man vorher wohl ausgesucht,", "tokens": ["Die", "man", "vor\u00b7her", "wohl", "aus\u00b7ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch so nicht den Geschmack erwecken,", "tokens": ["Auch", "so", "nicht", "den", "Ge\u00b7schmack", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Als wie der Sauerampfer pflegt,", "tokens": ["Als", "wie", "der", "Sau\u00b7e\u00b7ramp\u00b7fer", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der gern auf Wiesen Wurzeln schl\u00e4gt", "tokens": ["Der", "gern", "auf", "Wie\u00b7sen", "Wur\u00b7zeln", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Nicht wie die Datteln, so den Magen,", "tokens": ["Nicht", "wie", "die", "Dat\u00b7teln", ",", "so", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er geschw\u00e4cht, oft wieder gut gemacht,", "tokens": ["Wenn", "er", "ge\u00b7schw\u00e4cht", ",", "oft", "wie\u00b7der", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht als ein Lamm, das man geschlacht", "tokens": ["Nicht", "als", "ein", "Lamm", ",", "das", "man", "ge\u00b7schlacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "ART", "NN", "$,", "PRELS", "PIS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf der Besch\u00fctzer Feyertagen,", "tokens": ["Auf", "der", "Be\u00b7sch\u00fct\u00b7zer", "Fe\u00b7yer\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und gleich dem Bock, den man den Bi\u00df", "tokens": ["Und", "gleich", "dem", "Bock", ",", "den", "man", "den", "Bi\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NE", "$,", "PRELS", "PIS", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Des Wolfes wiederum entri\u00df.", "tokens": ["Des", "Wol\u00b7fes", "wie\u00b7de\u00b7rum", "ent\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Wie sch\u00f6n ists, w\u00