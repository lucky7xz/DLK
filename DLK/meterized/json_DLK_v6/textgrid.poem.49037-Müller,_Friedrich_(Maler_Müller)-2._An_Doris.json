{"textgrid.poem.49037": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "2. An Doris", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein holdes Bildnis schwebt mir immer vor den Augen,", "tokens": ["Dein", "hol\u00b7des", "Bild\u00b7nis", "schwebt", "mir", "im\u00b7mer", "vor", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Thal und W\u00e4lder folgt mir's nach!", "tokens": ["Durch", "Thal", "und", "W\u00e4l\u00b7der", "folgt", "mir's", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald l\u00e4chelnd; schlafend' dann, umscherzt von Zephyrs Hauchen", "tokens": ["Bald", "l\u00e4\u00b7chelnd", ";", "schla\u00b7fend'", "dann", ",", "um\u00b7scherzt", "von", "Ze\u00b7phyrs", "Hau\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "ADV", "$,", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Liebesg\u00f6ttern. Jetzund wach \u2013", "tokens": ["Und", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", ".", "Je\u00b7tzund", "wach", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald wie am Wasserfall auf sanften Blumenb\u00fcschen,", "tokens": ["Bald", "wie", "am", "Was\u00b7ser\u00b7fall", "auf", "sanf\u00b7ten", "Blu\u00b7men\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du deine zarte Glieder streckst;", "tokens": ["Du", "dei\u00b7ne", "zar\u00b7te", "Glie\u00b7der", "streckst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jetzt den jungen West, dich k\u00fchlend zu erfrischen,", "tokens": ["Und", "jetzt", "den", "jun\u00b7gen", "West", ",", "dich", "k\u00fch\u00b7lend", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus Rosenb\u00fcschen zu dir neckst.", "tokens": ["Aus", "Ro\u00b7sen\u00b7b\u00fc\u00b7schen", "zu", "dir", "neckst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vom Myrthenstrauch, um den die Liebesg\u00f6tter gauckeln,", "tokens": ["Vom", "Myr\u00b7then\u00b7strauch", ",", "um", "den", "die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "gau\u00b7ckeln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schwingt Amor sich auf deinen Schoo\u00df,", "tokens": ["Schwingt", "A\u00b7mor", "sich", "auf", "dei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie flattern alle nach, und sto\u00dfen sich, und schauckeln", "tokens": ["Sie", "flat\u00b7tern", "al\u00b7le", "nach", ",", "und", "sto\u00b7\u00dfen", "sich", ",", "und", "schauc\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Einander in das weiche Moo\u00df.", "tokens": ["Ein\u00b7an\u00b7der", "in", "das", "wei\u00b7che", "Moo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bald wie Cytherens Sohn, der z\u00e4rtlich dich betrachtet,", "tokens": ["Bald", "wie", "Cy\u00b7the\u00b7rens", "Sohn", ",", "der", "z\u00e4rt\u00b7lich", "dich", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Drey goldne Pfeile auf dich schie\u00dft.", "tokens": ["Drey", "gold\u00b7ne", "Pfei\u00b7le", "auf", "dich", "schie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df nun dein blaues Aug, in dem ein Amor schmachtet,", "tokens": ["Da\u00df", "nun", "dein", "blau\u00b7es", "Aug", ",", "in", "dem", "ein", "A\u00b7mor", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Empfindungsvoll sich halb verschlie\u00dft.", "tokens": ["Emp\u00b7fin\u00b7dungs\u00b7voll", "sich", "halb", "ver\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Bald seh ich dich am Bach mit deinen Haren spielen;", "tokens": ["Bald", "seh", "ich", "dich", "am", "Bach", "mit", "dei\u00b7nen", "Ha\u00b7ren", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Am Bach, wo ich dich j\u00fcngst beschlich.", "tokens": ["Am", "Bach", ",", "wo", "ich", "dich", "j\u00fcngst", "be\u00b7schlich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich kann, o G\u00f6ttliche! seit ich dich sah', nichts f\u00fchlen,", "tokens": ["Ich", "kann", ",", "o", "G\u00f6tt\u00b7li\u00b7che", "!", "seit", "ich", "dich", "sah'", ",", "nichts", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "FM", "NN", "$.", "KOUS", "PPER", "PRF", "VVFIN", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Nichts sehn, nichts denken, als nur dich.", "tokens": ["Nichts", "sehn", ",", "nichts", "den\u00b7ken", ",", "als", "nur", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PIS", "VVINF", "$,", "KOUS", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Geheiligt sey, o Bach! den sanften s\u00fc\u00dfen Schmerzen,", "tokens": ["Ge\u00b7hei\u00b7ligt", "sey", ",", "o", "Bach", "!", "den", "sanf\u00b7ten", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "FM", "NE", "$.", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Schmerzen, wie mein Herz sie f\u00fchlt.", "tokens": ["Den", "Schmer\u00b7zen", ",", "wie", "mein", "Herz", "sie", "f\u00fchlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOKOM", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Dein sanftes Murmeln weck die Z\u00e4rtlichkeit dem Herzen,", "tokens": ["Dein", "sanf\u00b7tes", "Mur\u00b7meln", "weck", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem Herzen, das die Liebe f\u00fchlt!", "tokens": ["Dem", "Her\u00b7zen", ",", "das", "die", "Lie\u00b7be", "f\u00fchlt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}