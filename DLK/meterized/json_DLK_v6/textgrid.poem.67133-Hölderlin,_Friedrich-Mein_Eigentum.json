{"textgrid.poem.67133": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein Eigentum", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seiner F\u00fclle ruhet der Herbsttag nun,", "tokens": ["In", "sei\u00b7ner", "F\u00fcl\u00b7le", "ru\u00b7het", "der", "Herbst\u00b7tag", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gel\u00e4utert ist die Traub und der Hain ist rot", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "ist", "die", "Traub", "und", "der", "Hain", "ist", "rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Obst, wenn schon der holden Bl\u00fcten", "tokens": ["Vom", "Obst", ",", "wenn", "schon", "der", "hol\u00b7den", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manche der Erde zum Danke fielen.", "tokens": ["Man\u00b7che", "der", "Er\u00b7de", "zum", "Dan\u00b7ke", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und rings im Felde, wo ich den Pfad hinaus,", "tokens": ["Und", "rings", "im", "Fel\u00b7de", ",", "wo", "ich", "den", "Pfad", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "APZR", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den stillen, wandle, ist den Zufriedenen", "tokens": ["Den", "stil\u00b7len", ",", "wand\u00b7le", ",", "ist", "den", "Zu\u00b7frie\u00b7de\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr Gut gereift und viel der frohen", "tokens": ["Ihr", "Gut", "ge\u00b7reift", "und", "viel", "der", "fro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00fche gew\u00e4hret der Reichtum ihnen.", "tokens": ["M\u00fc\u00b7he", "ge\u00b7w\u00e4h\u00b7ret", "der", "Reich\u00b7tum", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Vom Himmel blicket zu den Gesch\u00e4ftigen", "tokens": ["Vom", "Him\u00b7mel", "bli\u00b7cket", "zu", "den", "Ge\u00b7sch\u00e4f\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch ihre B\u00e4ume milde das Licht herab,", "tokens": ["Durch", "ih\u00b7re", "B\u00e4u\u00b7me", "mil\u00b7de", "das", "Licht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Freude teilend, denn es wuchs durch", "tokens": ["Die", "Freu\u00b7de", "tei\u00b7lend", ",", "denn", "es", "wuchs", "durch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4nde der Menschen allein die Frucht nicht.", "tokens": ["H\u00e4n\u00b7de", "der", "Men\u00b7schen", "al\u00b7lein", "die", "Frucht", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Und leuchtest du, o Goldnes, auch mir, und wehst", "tokens": ["Und", "leuch\u00b7test", "du", ",", "o", "Gold\u00b7nes", ",", "auch", "mir", ",", "und", "wehst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "FM", "$,", "ADV", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch du mir wieder, L\u00fcftchen, als segnetest", "tokens": ["Auch", "du", "mir", "wie\u00b7der", ",", "L\u00fcft\u00b7chen", ",", "als", "seg\u00b7ne\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "PPER", "ADV", "$,", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du eine Freude mir, wie einst, und", "tokens": ["Du", "ei\u00b7ne", "Freu\u00b7de", "mir", ",", "wie", "einst", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "PPER", "$,", "PWAV", "ADV", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Irrst, wie um Gl\u00fcckliche, mir am Busen?", "tokens": ["Irrst", ",", "wie", "um", "Gl\u00fcck\u00b7li\u00b7che", ",", "mir", "am", "Bu\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Einst war ichs, doch wie Rosen, verg\u00e4nglich war", "tokens": ["Einst", "war", "ichs", ",", "doch", "wie", "Ro\u00b7sen", ",", "ver\u00b7g\u00e4ng\u00b7lich", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "$,", "ADV", "KOKOM", "NN", "$,", "ADJD", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das fromme Leben, ach! und es mahnen noch,", "tokens": ["Das", "from\u00b7me", "Le\u00b7ben", ",", "ach", "!", "und", "es", "mah\u00b7nen", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ITJ", "$.", "KON", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die bl\u00fchend mir geblieben sind, die", "tokens": ["Die", "bl\u00fc\u00b7hend", "mir", "ge\u00b7blie\u00b7ben", "sind", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "PPER", "VVPP", "VAFIN", "$,", "PRELS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Holden Gestirne zu oft mich dessen.", "tokens": ["Hol\u00b7den", "Ge\u00b7stir\u00b7ne", "zu", "oft", "mich", "des\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADV", "PPER", "PDS", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Begl\u00fcckt, wer, ruhig liebend ein frommes Weib,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wer", ",", "ru\u00b7hig", "lie\u00b7bend", "ein", "from\u00b7mes", "Weib", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "$,", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am eignen Herd in r\u00fchmlicher Heimat lebt,", "tokens": ["Am", "eig\u00b7nen", "Herd", "in", "r\u00fchm\u00b7li\u00b7cher", "Hei\u00b7mat", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es leuchtet \u00fcber festem Boden", "tokens": ["Es", "leuch\u00b7tet", "\u00fc\u00b7ber", "fes\u00b7tem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6ner dem sicheren Mann sein Himmel.", "tokens": ["Sch\u00f6\u00b7ner", "dem", "si\u00b7che\u00b7ren", "Mann", "sein", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Denn, wie die Pflanze, wurzelt auf eignem Grund", "tokens": ["Denn", ",", "wie", "die", "Pflan\u00b7ze", ",", "wur\u00b7zelt", "auf", "eig\u00b7nem", "Grund"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie nicht, vergl\u00fcht die Seele des Sterblichen,", "tokens": ["Sie", "nicht", ",", "ver\u00b7gl\u00fcht", "die", "See\u00b7le", "des", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der mit dem Tageslichte nur, ein", "tokens": ["Der", "mit", "dem", "Ta\u00b7ges\u00b7lich\u00b7te", "nur", ",", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "$,", "ART"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Armer, auf heiliger Erde wandelt.", "tokens": ["Ar\u00b7mer", ",", "auf", "hei\u00b7li\u00b7ger", "Er\u00b7de", "wan\u00b7delt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Zu m\u00e4chtig, ach! ihr himmlischen H\u00f6hen, zieht", "tokens": ["Zu", "m\u00e4ch\u00b7tig", ",", "ach", "!", "ihr", "himm\u00b7li\u00b7schen", "H\u00f6\u00b7hen", ",", "zieht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKA", "ADJD", "$,", "ITJ", "$.", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr mich empor, bei St\u00fcrmen, am heitern Tag", "tokens": ["Ihr", "mich", "em\u00b7por", ",", "bei", "St\u00fcr\u00b7men", ",", "am", "hei\u00b7tern", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PRF", "PTKVZ", "$,", "APPR", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00fchl ich verzehrend euch im Busen", "tokens": ["F\u00fchl", "ich", "ver\u00b7zeh\u00b7rend", "euch", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVPP", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wechseln, ihr wandelnden G\u00f6tterkr\u00e4fte.", "tokens": ["Wech\u00b7seln", ",", "ihr", "wan\u00b7deln\u00b7den", "G\u00f6t\u00b7ter\u00b7kr\u00e4f\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Doch heute la\u00df mich stille den trauten Pfad", "tokens": ["Doch", "heu\u00b7te", "la\u00df", "mich", "stil\u00b7le", "den", "trau\u00b7ten", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Haine gehn, dem golden die Wipfel schm\u00fcckt", "tokens": ["Zum", "Hai\u00b7ne", "gehn", ",", "dem", "gol\u00b7den", "die", "Wip\u00b7fel", "schm\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "ART", "ADJA", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein sterbend Laub, und kr\u00e4nzt auch mir die", "tokens": ["Sein", "ster\u00b7bend", "Laub", ",", "und", "kr\u00e4nzt", "auch", "mir", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "KON", "VVFIN", "ADV", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stirne, ihr holden Erinnerungen!", "tokens": ["Stir\u00b7ne", ",", "ihr", "hol\u00b7den", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+----+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Und da\u00df mir auch, zu retten mein sterblich Herz,", "tokens": ["Und", "da\u00df", "mir", "auch", ",", "zu", "ret\u00b7ten", "mein", "sterb\u00b7lich", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "PTKZU", "VVINF", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie andern eine bleibende St\u00e4tte sei,", "tokens": ["Wie", "an\u00b7dern", "ei\u00b7ne", "blei\u00b7ben\u00b7de", "St\u00e4t\u00b7te", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und heimatlos die Seele mir nicht", "tokens": ["Und", "hei\u00b7mat\u00b7los", "die", "See\u00b7le", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u00dcber das Leben hinweg sich sehne,", "tokens": ["\u00dc\u00b7ber", "das", "Le\u00b7ben", "hin\u00b7weg", "sich", "seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Sei du, Gesang, mein freundlich Asyl! sei du,", "tokens": ["Sei", "du", ",", "Ge\u00b7sang", ",", "mein", "freund\u00b7lich", "A\u00b7syl", "!", "sei", "du", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "ADJD", "NN", "$.", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Begl\u00fcckender! mit sorgender Liebe mir", "tokens": ["Be\u00b7gl\u00fc\u00b7cken\u00b7der", "!", "mit", "sor\u00b7gen\u00b7der", "Lie\u00b7be", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gepflegt, der Garten, wo ich, wandelnd", "tokens": ["Ge\u00b7pflegt", ",", "der", "Gar\u00b7ten", ",", "wo", "ich", ",", "wan\u00b7delnd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "ART", "NN", "$,", "PWAV", "PPER", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den Bl\u00fcten, den immerjungen,", "tokens": ["Un\u00b7ter", "den", "Bl\u00fc\u00b7ten", ",", "den", "im\u00b7mer\u00b7jun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "In sichrer Einfalt wohne, wenn drau\u00dfen mir", "tokens": ["In", "sich\u00b7rer", "Ein\u00b7falt", "woh\u00b7ne", ",", "wenn", "drau\u00b7\u00dfen", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KOUS", "ADV", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit ihren Wellen allen die m\u00e4chtge Zeit,", "tokens": ["Mit", "ih\u00b7ren", "Wel\u00b7len", "al\u00b7len", "die", "m\u00e4cht\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wandelbare, fern rauscht und die", "tokens": ["Die", "Wan\u00b7del\u00b7ba\u00b7re", ",", "fern", "rauscht", "und", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "KON", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stillere Sonne mein Wirken f\u00f6rdert.", "tokens": ["Stil\u00b7le\u00b7re", "Son\u00b7ne", "mein", "Wir\u00b7ken", "f\u00f6r\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Ihr segnet g\u00fctig \u00fcber den Sterblichen,", "tokens": ["Ihr", "seg\u00b7net", "g\u00fc\u00b7tig", "\u00fc\u00b7ber", "den", "Sterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Himmelskr\u00e4fte! jedem sein Eigentum,", "tokens": ["Ihr", "Him\u00b7mels\u00b7kr\u00e4f\u00b7te", "!", "je\u00b7dem", "sein", "Ei\u00b7gen\u00b7tum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O segnet meines auch, und da\u00df zu", "tokens": ["O", "seg\u00b7net", "mei\u00b7nes", "auch", ",", "und", "da\u00df", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADV", "$,", "KON", "KOUS", "APPR"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Fr\u00fche die Parze den Traum nicht ende.", "tokens": ["Fr\u00fc\u00b7he", "die", "Par\u00b7ze", "den", "Traum", "nicht", "en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plu