{"textgrid.poem.29192": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Propheten Gottes Willen seinem Volcke sagten an,", "tokens": ["Wann", "Pro\u00b7phe\u00b7ten", "Got\u00b7tes", "Wil\u00b7len", "sei\u00b7nem", "Vol\u00b7cke", "sag\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hingen sie gemein ein Zeichen und ein sondres Merckmal dran,", "tokens": ["Hin\u00b7gen", "sie", "ge\u00b7mein", "ein", "Zei\u00b7chen", "und", "ein", "sond\u00b7res", "Merck\u00b7mal", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Welches offt f\u00fcr l\u00e4ppisch Ding von den sichren ward gesch\u00e4tzet,", "tokens": ["Wel\u00b7ches", "offt", "f\u00fcr", "l\u00e4p\u00b7pisch", "Ding", "von", "den", "sich\u00b7ren", "ward", "ge\u00b7sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJD", "NN", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Aber Gottes weisen Rath endlich klar an Tag gesetzet.", "tokens": ["A\u00b7ber", "Got\u00b7tes", "wei\u00b7sen", "Rath", "end\u00b7lich", "klar", "an", "Tag", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "ADV", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Werther Freund und Gottes-Diener, da der HERR noch hegte Zorn,", "tokens": ["Wert\u00b7her", "Freund", "und", "Got\u00b7tes\u00b7Die\u00b7ner", ",", "da", "der", "HeRR", "noch", "heg\u00b7te", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "KOUS", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da uns bi\u00df auffs Blut noch riete manch vergiffter Krieges-Sporn,", "tokens": ["Da", "uns", "bi\u00df", "auffs", "Blut", "noch", "rie\u00b7te", "manch", "ver\u00b7giff\u00b7ter", "Krie\u00b7ge\u00b7sSporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Da ihr sagtet, was Gott hie\u00df, da ihr wieset, wie Gott dr\u00e4ute,", "tokens": ["Da", "ihr", "sag\u00b7tet", ",", "was", "Gott", "hie\u00df", ",", "da", "ihr", "wie\u00b7set", ",", "wie", "Gott", "dr\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+-++-", "measure": "anapaest.init"}, "line.8": {"text": "Uns zu werffen gar in Staub, weil niemand die Schl\u00e4ge scheute,", "tokens": ["Uns", "zu", "werf\u00b7fen", "gar", "in", "Staub", ",", "weil", "nie\u00b7mand", "die", "Schl\u00e4\u00b7ge", "scheu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "APPR", "NN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Da war dieses euer Zeichen: Euer eigen Augen-Lust", "tokens": ["Da", "war", "die\u00b7ses", "eu\u00b7er", "Zei\u00b7chen", ":", "Eu\u00b7er", "ei\u00b7gen", "Au\u00b7gen\u00b7Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDAT", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Hat, weil so der HERR befohle, zum Exempel fort gemust.", "tokens": ["Hat", ",", "weil", "so", "der", "HeRR", "be\u00b7foh\u00b7le", ",", "zum", "Ex\u00b7em\u00b7pel", "fort", "ge\u00b7must", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Aber nun da Gottes Hertz durch sich selbsten ist erweichet,", "tokens": ["A\u00b7ber", "nun", "da", "Got\u00b7tes", "Hertz", "durch", "sich", "selbs\u00b7ten", "ist", "er\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "NN", "APPR", "PRF", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Da uns seine Vater-Hand wieder Brot, nicht Steine reichet,", "tokens": ["Da", "uns", "sei\u00b7ne", "Va\u00b7ter\u00b7Hand", "wie\u00b7der", "Brot", ",", "nicht", "Stei\u00b7ne", "rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "NN", "$,", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Da der weisse Friedens-Ritter schlug die rothe Frevler-Schaar,", "tokens": ["Da", "der", "weis\u00b7se", "Frie\u00b7dens\u00b7Rit\u00b7ter", "schlug", "die", "ro\u00b7the", "Frev\u00b7ler\u00b7Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Da nun Leben, Stand und Habe letzlich wieder unser war,", "tokens": ["Da", "nun", "Le\u00b7ben", ",", "Stand", "und", "Ha\u00b7be", "letz\u00b7lich", "wie\u00b7der", "un\u00b7ser", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "So erhebt ihr Gottes G\u00fct und bereitet die Gem\u00fcter,", "tokens": ["So", "er\u00b7hebt", "ihr", "Got\u00b7tes", "G\u00fct", "und", "be\u00b7rei\u00b7tet", "die", "Ge\u00b7m\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Da\u00df mit Bu\u00df und rechtem Sinn sie gebrauchen Friedens-G\u00fcter;", "tokens": ["Da\u00df", "mit", "Bu\u00df", "und", "rech\u00b7tem", "Sinn", "sie", "ge\u00b7brau\u00b7chen", "Frie\u00b7dens\u00b7G\u00fc\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "ADJA", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Diesem nach ist di\u00df das Zeichen: Das nach Gottes Will und Wust", "tokens": ["Die\u00b7sem", "nach", "ist", "di\u00df", "das", "Zei\u00b7chen", ":", "Das", "nach", "Got\u00b7tes", "Will", "und", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "APPR", "VAFIN", "PDS", "ART", "NN", "$.", "PDS", "APPR", "NN", "NE", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Euch in gleichem wird ersetzet, was Gott nam an Augen-Lust.", "tokens": ["Euch", "in", "glei\u00b7chem", "wird", "er\u00b7set\u00b7zet", ",", "was", "Gott", "nam", "an", "Au\u00b7gen\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "VAFIN", "VVPP", "$,", "PRELS", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Gott bleibt Gott! man wird die Welt gar in neuem Baue sehen,", "tokens": ["Gott", "bleibt", "Gott", "!", "man", "wird", "die", "Welt", "gar", "in", "neu\u00b7em", "Bau\u00b7e", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "PIS", "VAFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-----+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Wann man bey der letzten Brunst meinen wird, es sey geschehen.", "tokens": ["Wann", "man", "bey", "der", "letz\u00b7ten", "Brunst", "mei\u00b7nen", "wird", ",", "es", "sey", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "ADJA", "NN", "VVFIN", "VAFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Gott bleibt Gott! wann ihm gef\u00e4llet, ruffet er dem W\u00fcrge-Schwerdt,", "tokens": ["Gott", "bleibt", "Gott", "!", "wann", "ihm", "ge\u00b7f\u00e4l\u00b7let", ",", "ruf\u00b7fet", "er", "dem", "W\u00fcr\u00b7ge\u00b7Schwerdt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "PWAV", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Schafft ihm auch nach seinem Willen, da\u00df es in die Scheide kehrt.", "tokens": ["Schafft", "ihm", "auch", "nach", "sei\u00b7nem", "Wil\u00b7len", ",", "da\u00df", "es", "in", "die", "Schei\u00b7de", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Gott bleibt Gott! er leitet ab und hat Menschen weg genummen;", "tokens": ["Gott", "bleibt", "Gott", "!", "er", "lei\u00b7tet", "ab", "und", "hat", "Men\u00b7schen", "weg", "ge\u00b7num\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "KON", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Gott bleibt Gott! er weiset an und l\u00e4st Menschen wieder kummen;", "tokens": ["Gott", "bleibt", "Gott", "!", "er", "wei\u00b7set", "an", "und", "l\u00e4st", "Men\u00b7schen", "wie\u00b7der", "kum\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Gott bleibt Gott! nimmt weg Rosinen, und Rosinen gibt er her;", "tokens": ["Gott", "bleibt", "Gott", "!", "nimmt", "weg", "Ro\u00b7si\u00b7nen", ",", "und", "Ro\u00b7si\u00b7nen", "gibt", "er", "her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$.", "VVFIN", "ADV", "NN", "$,", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Wittwer wieder zu beweiben, ist ihm desto minder schwer.", "tokens": ["Witt\u00b7wer", "wie\u00b7der", "zu", "be\u00b7wei\u00b7ben", ",", "ist", "ihm", "des\u00b7to", "min\u00b7der", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Gott, bey dem die beste Lust, mit den Menschen-Kindern spielen,", "tokens": ["Gott", ",", "bey", "dem", "die", "bes\u00b7te", "Lust", ",", "mit", "den", "Men\u00b7schen\u00b7Kin\u00b7dern", "spie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Macht es immer so mit uns, da\u00df wir s\u00fc\u00df auff sauer f\u00fchlen.", "tokens": ["Macht", "es", "im\u00b7mer", "so", "mit", "uns", ",", "da\u00df", "wir", "s\u00fc\u00df", "auff", "sau\u00b7er", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "ADJD", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Herr und Freund, ihr must es zeugen, wie sich Gott mit euch ergetzt,", "tokens": ["Herr", "und", "Freund", ",", "ihr", "must", "es", "zeu\u00b7gen", ",", "wie", "sich", "Gott", "mit", "euch", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$,", "PWAV", "PRF", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Euch nach vieler Angst und Trauren nun in Fried und Freude setzt.", "tokens": ["Euch", "nach", "vie\u00b7ler", "Angst", "und", "Trau\u00b7ren", "nun", "in", "Fried", "und", "Freu\u00b7de", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "KON", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.31": {"text": "Nun, Herr Br\u00e4utgam, dessen Heil ist ein Theil von meiner Freude,", "tokens": ["Nun", ",", "Herr", "Br\u00e4ut\u00b7gam", ",", "des\u00b7sen", "Heil", "ist", "ein", "Theil", "von", "mei\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "NE", "$,", "PRELAT", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-++-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Seyd gesichert, da\u00df mein Sinn sich in eurem Gn\u00fcgen weide.", "tokens": ["Seyd", "ge\u00b7si\u00b7chert", ",", "da\u00df", "mein", "Sinn", "sich", "in", "eu\u00b7rem", "Gn\u00fc\u00b7gen", "wei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.33": {"text": "Gebe Gott, der gute Geber, was er euch im Frieden gibt,", "tokens": ["Ge\u00b7be", "Gott", ",", "der", "gu\u00b7te", "Ge\u00b7ber", ",", "was", "er", "euch", "im", "Frie\u00b7den", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ART", "ADJA", "NN", "$,", "PWS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Da\u00df gar nichts sey drum, dran, drinne, das nicht ihr und euch nicht liebt!", "tokens": ["Da\u00df", "gar", "nichts", "sey", "drum", ",", "dran", ",", "drin\u00b7ne", ",", "das", "nicht", "ihr", "und", "euch", "nicht", "liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VAFIN", "PAV", "$,", "PAV", "$,", "VVFIN", "$,", "PRELS", "PTKNEG", "PPER", "KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.35": {"text": "Was er gibt den Seinen sonst, dieses sey euch auch gegeben,", "tokens": ["Was", "er", "gibt", "den", "Sei\u00b7nen", "sonst", ",", "die\u00b7ses", "sey", "euch", "auch", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "PPOSS", "ADV", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Seyd zu friede, wann ihr habt Segen hier und dorte Leben!", "tokens": ["Seyd", "zu", "frie\u00b7de", ",", "wann", "ihr", "habt", "Se\u00b7gen", "hier", "und", "dor\u00b7te", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "NN", "$,", "PWAV", "PPER", "VAFIN", "NN", "ADV", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}