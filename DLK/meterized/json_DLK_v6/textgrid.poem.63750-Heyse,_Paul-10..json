{"textgrid.poem.63750": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Chiaja dr\u00f6hnt von Reitern und Karossen,", "tokens": ["Die", "Chia\u00b7ja", "dr\u00f6hnt", "von", "Rei\u00b7tern", "und", "Ka\u00b7ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Konzert im Gr\u00fcnen, lust'gen Menschenscharen.", "tokens": ["Kon\u00b7zert", "im", "Gr\u00fc\u00b7nen", ",", "lust'\u00b7gen", "Men\u00b7schen\u00b7scha\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Siehst du die sch\u00f6ne Frau mit blonden Haaren,", "tokens": ["Siehst", "du", "die", "sch\u00f6\u00b7ne", "Frau", "mit", "blon\u00b7den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stumm an des Gatten Seite hingegossen?", "tokens": ["Stumm", "an", "des", "Gat\u00b7ten", "Sei\u00b7te", "hin\u00b7ge\u00b7gos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er blickt so kalt, sie traurig und verdrossen.", "tokens": ["Er", "blickt", "so", "kalt", ",", "sie", "trau\u00b7rig", "und", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Dulderin! Kann er ihr's nicht ersparen,", "tokens": ["Die", "Dul\u00b7de\u00b7rin", "!", "Kann", "er", "ih\u00b7r's", "nicht", "er\u00b7spa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dicht an dem Hause dort vorbeizufahren,", "tokens": ["Dicht", "an", "dem", "Hau\u00b7se", "dort", "vor\u00b7bei\u00b7zu\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo er sein freches Liebchen eingeschlossen?", "tokens": ["Wo", "er", "sein", "fre\u00b7ches", "Lieb\u00b7chen", "ein\u00b7ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die zeigt am Fenster sich zur Korsostunde.", "tokens": ["Die", "zeigt", "am", "Fens\u00b7ter", "sich", "zur", "Kor\u00b7sos\u00b7tun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die arme junge Frau sieht stolz vor\u00fcber \u2013", "tokens": ["Die", "ar\u00b7me", "jun\u00b7ge", "Frau", "sieht", "stolz", "vor\u00b7\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohin? Dort nach dem Stutzer hoch zu Pferd?", "tokens": ["Wo\u00b7hin", "?", "Dort", "nach", "dem", "Stut\u00b7zer", "hoch", "zu", "Pferd", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADV", "APPR", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aufblitz ein L\u00e4cheln an dem blassen Munde,", "tokens": ["Auf\u00b7blitz", "ein", "L\u00e4\u00b7cheln", "an", "dem", "blas\u00b7sen", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wink \u2013 ein Blick her\u00fcber und hin\u00fcber \u2013", "tokens": ["Ein", "Wink", "\u2013", "ein", "Blick", "her\u00b7\u00fc\u00b7ber", "und", "hin\u00b7\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Dulderin! \u2013 Ihr seid einander wert!", "tokens": ["O", "Dul\u00b7de\u00b7rin", "!", "\u2013", "Ihr", "seid", "ein\u00b7an\u00b7der", "wert", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "PPER", "VAFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}