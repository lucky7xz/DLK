{"textgrid.poem.36400": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das wunderbare Bild von der heiligen Verk\u00fcndigung in Florenz", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den frommen Sinn zur Jungfrau hingewendet,", "tokens": ["Den", "from\u00b7men", "Sinn", "zur", "Jung\u00b7frau", "hin\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat sich ein Maler das zum Ziel erlesen,", "tokens": ["Hat", "sich", "ein", "Ma\u00b7ler", "das", "zum", "Ziel", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ART", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu zeigen, wie es wirklich ist gewesen;", "tokens": ["Zu", "zei\u00b7gen", ",", "wie", "es", "wirk\u00b7lich", "ist", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wie er malt' und sann, blieb's ungeendet.", "tokens": ["Doch", "wie", "er", "malt'", "und", "sann", ",", "blie\u00b7b's", "un\u00b7ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da ward ein tiefer Schlaf auf ihn gesendet;", "tokens": ["Da", "ward", "ein", "tie\u00b7fer", "Schlaf", "auf", "ihn", "ge\u00b7sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun ist ihm die Verk\u00fcnd'gung hell gewesen,", "tokens": ["Nun", "ist", "ihm", "die", "Ver\u00b7k\u00fcn\u00b7d'\u00b7gung", "hell", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie wir sie in dem heil'gen Buche lesen,", "tokens": ["Wie", "wir", "sie", "in", "dem", "heil'\u00b7gen", "Bu\u00b7che", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Staunend erwacht sieht er sein Bild vollendet.", "tokens": ["Stau\u00b7nend", "er\u00b7wacht", "sieht", "er", "sein", "Bild", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie hier der ew'ge Geist hat eingeschlagen,", "tokens": ["Wie", "hier", "der", "ew'\u00b7ge", "Geist", "hat", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Gott ein Allmachtsstrahl des ew'gen Lichtes", "tokens": ["Von", "Gott", "ein", "All\u00b7machts\u00b7strahl", "des", "ew'\u00b7gen", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In diese zarte Magd; wer mag es sagen?", "tokens": ["In", "die\u00b7se", "zar\u00b7te", "Magd", ";", "wer", "mag", "es", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann hingerissen, aufgel\u00f6st dies Blicken,", "tokens": ["Dann", "hin\u00b7ge\u00b7ris\u00b7sen", ",", "auf\u00b7ge\u00b7l\u00f6st", "dies", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jubelnd wie Feuerzungen des Gerichtes,", "tokens": ["Ju\u00b7belnd", "wie", "Feu\u00b7er\u00b7zun\u00b7gen", "des", "Ge\u00b7rich\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ihr ganzes Sein ein zitterndes Entz\u00fccken! \u2013", "tokens": ["Ihr", "gan\u00b7zes", "Sein", "ein", "zit\u00b7tern\u00b7des", "Ent\u00b7z\u00fc\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}