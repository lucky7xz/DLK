{"dta.poem.12624": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiI .  \n  Die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, angenehme Nacht!", "tokens": ["Will\u00b7kom\u00b7men", ",", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00fcll in deine Schatten", "tokens": ["Ver\u00b7h\u00fcll", "in", "dei\u00b7ne", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Freuden, die sich gatten,", "tokens": ["Die", "Freu\u00b7den", ",", "die", "sich", "gat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und blend, o blende den Verdacht.", "tokens": ["Und", "blend", ",", "o", "blen\u00b7de", "den", "Ver\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "FM", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann treue Liebe k\u00fcssen macht;", "tokens": ["Wann", "treu\u00b7e", "Lie\u00b7be", "k\u00fcs\u00b7sen", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wird der Ku\u00df der Liebe,", "tokens": ["So", "wird", "der", "Ku\u00df", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So werden ihre Triebe", "tokens": ["So", "wer\u00b7den", "ih\u00b7re", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begl\u00fcckter durch die stille Nacht.", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "durch", "die", "stil\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der sch\u00f6ne Mund, den man verehrt,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Mund", ",", "den", "man", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bestrafet, z\u00fcrnt gelinder,", "tokens": ["Be\u00b7stra\u00b7fet", ",", "z\u00fcrnt", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird z\u00e4rtlich, k\u00fc\u00dft geschwinder,", "tokens": ["Wird", "z\u00e4rt\u00b7lich", ",", "k\u00fc\u00dft", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann nichts die sichern K\u00fcsse st\u00f6rt.", "tokens": ["Wann", "nichts", "die", "si\u00b7chern", "K\u00fcs\u00b7se", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, ja! die Nacht ist vorzugswehrt:", "tokens": ["Ja", ",", "ja", "!", "die", "Nacht", "ist", "vor\u00b7zugs\u00b7wehrt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie dient und ist verschwiegen,", "tokens": ["Sie", "dient", "und", "ist", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und liefert dem Vergn\u00fcgen", "tokens": ["Und", "lie\u00b7fert", "dem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den s\u00fcssen Mund, den man verehrt.", "tokens": ["Den", "s\u00fcs\u00b7sen", "Mund", ",", "den", "man", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Tag hat, als ein falscher Freund,", "tokens": ["Der", "Tag", "hat", ",", "als", "ein", "fal\u00b7scher", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu oft der Welt erzehlet,", "tokens": ["Zu", "oft", "der", "Welt", "er\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ihr die Nacht verhehlet,", "tokens": ["Was", "ihr", "die", "Nacht", "ver\u00b7heh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebende nach Wunsch vereint.", "tokens": ["Die", "Lie\u00b7ben\u00b7de", "nach", "Wunsch", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Du bist der Sorg und Unruh feind", "tokens": ["Du", "bist", "der", "Sorg", "und", "Un\u00b7ruh", "feind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und g\u00f6nnest sie dem Tage,", "tokens": ["Und", "g\u00f6n\u00b7nest", "sie", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und widerlegst die Sage:", "tokens": ["Und", "wi\u00b7der\u00b7legst", "die", "Sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du, holde Nacht, seyst niemands Freund.", "tokens": ["Du", ",", "hol\u00b7de", "Nacht", ",", "seyst", "nie\u00b7mands", "Freund", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft schr\u00e4nkt der strenge Tag uns ein;", "tokens": ["Oft", "schr\u00e4nkt", "der", "stren\u00b7ge", "Tag", "uns", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch h\u00e4lt in schweren Stunden", "tokens": ["Doch", "h\u00e4lt", "in", "schwe\u00b7ren", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns mancher Tag gebunden,", "tokens": ["Uns", "man\u00b7cher", "Tag", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mu\u00df die freye Nacht erfreun.", "tokens": ["So", "mu\u00df", "die", "frey\u00b7e", "Nacht", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gl\u00fcck vertraut und froh zu seyn,", "tokens": ["Das", "Gl\u00fcck", "ver\u00b7traut", "und", "froh", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gl\u00fcck zufriedner Herzen,", "tokens": ["Das", "Gl\u00fcck", "zu\u00b7fried\u00b7ner", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die in der Stille scherzen,", "tokens": ["Die", "in", "der", "Stil\u00b7le", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "R\u00e4umt uns der Tag nur selten ein.", "tokens": ["R\u00e4umt", "uns", "der", "Tag", "nur", "sel\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Nacht, da nur der Scherz sich regt,", "tokens": ["O", "Nacht", ",", "da", "nur", "der", "Scherz", "sich", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da keine Neider lauschen,", "tokens": ["Da", "kei\u00b7ne", "Nei\u00b7der", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nur die K\u00fcsse rauschen,", "tokens": ["Und", "nur", "die", "K\u00fcs\u00b7se", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sinnreich wirst du angelegt!", "tokens": ["Wie", "sinn\u00b7reich", "wirst", "du", "an\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wird der Liebes-Gott verpflegt,", "tokens": ["Wie", "wird", "der", "Lie\u00b7bes\u00b7Gott", "ver\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wann selbst die Huld-G\u00f6ttinnen", "tokens": ["Wann", "selbst", "die", "Huld\u00b7G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf sein Vergn\u00fcgen sinnen,", "tokens": ["Auf", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und nichts als Lust und Scherz sich regt.", "tokens": ["Und", "nichts", "als", "Lust", "und", "Scherz", "sich", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PIS", "KOKOM", "NN", "KON", "NN", "PRF", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}