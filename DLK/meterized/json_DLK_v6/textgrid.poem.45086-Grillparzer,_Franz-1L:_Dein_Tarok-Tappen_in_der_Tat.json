{"textgrid.poem.45086": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Tarok-Tappen in der Tat", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Tarok-Tappen in der Tat", "tokens": ["Dein", "Ta\u00b7ro\u00b7kTap\u00b7pen", "in", "der", "Tat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist t\u00e4ppisch und gewagt, als Freund gesprochen,", "tokens": ["Ist", "t\u00e4p\u00b7pisch", "und", "ge\u00b7wagt", ",", "als", "Freund", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bewahrst du gar zu lange den Pagat,", "tokens": ["Be\u00b7wahrst", "du", "gar", "zu", "lan\u00b7ge", "den", "Pa\u00b7gat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird er dir zuletzt noch abgestochen.", "tokens": ["So", "wird", "er", "dir", "zu\u00b7letzt", "noch", "ab\u00b7ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}