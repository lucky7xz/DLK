{"textgrid.poem.62807": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Midas", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Midas", "tokens": ["Ein", "Mi\u00b7das"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Als keiner aus dem Heer der Affen,", "tokens": ["Als", "kei\u00b7ner", "aus", "dem", "Heer", "der", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Jupiter zum Scherz geschaffen,", "tokens": ["Die", "Ju\u00b7pi\u00b7ter", "zum", "Scherz", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch vor Sehnsucht gro\u00df zu seyn", "tokens": ["Und", "doch", "vor", "Sehn\u00b7sucht", "gro\u00df", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon oft bald aus der Haut gesprungen.", "tokens": ["Schon", "oft", "bald", "aus", "der", "Haut", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts lie\u00df er unversucht; allein", "tokens": ["Nichts", "lie\u00df", "er", "un\u00b7ver\u00b7sucht", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch war kein Mittel ihm gelungen;", "tokens": ["Noch", "war", "kein", "Mit\u00b7tel", "ihm", "ge\u00b7lun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das beste fiel zuletzt ihm ein.", "tokens": ["Das", "bes\u00b7te", "fiel", "zu\u00b7letzt", "ihm", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er machte sich von Bambusrohre", "tokens": ["Er", "mach\u00b7te", "sich", "von", "Bam\u00b7bus\u00b7roh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zwo Stelzen und mit keckem Schritt", "tokens": ["Zwo", "Stel\u00b7zen", "und", "mit", "ke\u00b7ckem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Erschien der lose Schelm damit", "tokens": ["Er\u00b7schien", "der", "lo\u00b7se", "Schelm", "da\u00b7mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In der erstaunten Br\u00fcder Chore.", "tokens": ["In", "der", "er\u00b7staun\u00b7ten", "Br\u00fc\u00b7der", "Cho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein jeder ruft so laut er kann:", "tokens": ["Ein", "je\u00b7der", "ruft", "so", "laut", "er", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Betrachtet doch den gro\u00dfen Mann!", "tokens": ["Be\u00b7trach\u00b7tet", "doch", "den", "gro\u00b7\u00dfen", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Geduld! ihr habt noch nichts gesehen;", "tokens": ["Ge\u00b7duld", "!", "ihr", "habt", "noch", "nichts", "ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Bald soll ein Titan vor euch stehen,", "tokens": ["Bald", "soll", "ein", "Ti\u00b7tan", "vor", "euch", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Versetzt er! klimmt auf einen Berg", "tokens": ["Ver\u00b7setzt", "er", "!", "klimmt", "auf", "ei\u00b7nen", "Berg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und scheint nun was er war \u2013 ein Zwerg.", "tokens": ["Und", "scheint", "nun", "was", "er", "war", "\u2013", "ein", "Zwerg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PWS", "PPER", "VAFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So pflegt es manchem Schuft zu gehen.", "tokens": ["So", "pflegt", "es", "man\u00b7chem", "Schuft", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Stelzen scheint er gro\u00df zu seyn.", "tokens": ["Auf", "Stel\u00b7zen", "scheint", "er", "gro\u00df", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wenn er sich, vom Stolz geplaget,", "tokens": ["Doch", "wenn", "er", "sich", ",", "vom", "Stolz", "ge\u00b7pla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einen h\u00f6hern Standpunct waget,", "tokens": ["Auf", "ei\u00b7nen", "h\u00f6\u00b7hern", "Stand\u00b7punct", "wa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wird der Riese wieder klein.", "tokens": ["So", "wird", "der", "Rie\u00b7se", "wie\u00b7der", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}