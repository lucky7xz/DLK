{"dta.poem.7489": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der uns die Freiheit einst so k\u00fchn gelehret,", "tokens": ["Der", "uns", "die", "Frei\u00b7heit", "einst", "so", "k\u00fchn", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt ihr ihn hinter jenem Gitter wohl,", "tokens": ["H\u00f6rt", "ihr", "ihn", "hin\u00b7ter", "je\u00b7nem", "Git\u00b7ter", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dran spottend noch des Glaubens rauh Symbol,", "tokens": ["Dran", "spot\u00b7tend", "noch", "des", "Glau\u00b7bens", "rauh", "Sym\u00b7bol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Manch eisern Kreuz, das ihm die Flucht verwehret?", "tokens": ["Manch", "ei\u00b7sern", "Kreuz", ",", "das", "ihm", "die", "Flucht", "ver\u00b7weh\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das also ist der Lohn, der ihm bescheeret", "tokens": ["Das", "al\u00b7so", "ist", "der", "Lohn", ",", "der", "ihm", "be\u00b7schee\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward von dem angebeteten Idol?", "tokens": ["Ward", "von", "dem", "an\u00b7ge\u00b7be\u00b7te\u00b7ten", "I\u00b7dol", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wangen bla\u00df, die Augen tr\u00fcb und hohl,", "tokens": ["Die", "Wan\u00b7gen", "bla\u00df", ",", "die", "Au\u00b7gen", "tr\u00fcb", "und", "hohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Augen, die er \u2014 ", "tokens": ["Die", "Au\u00b7gen", ",", "die", "er"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Seit Jahren sah er keine Wolke schweben,", "tokens": ["Seit", "Jah\u00b7ren", "sah", "er", "kei\u00b7ne", "Wol\u00b7ke", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit Jahren kein Gestirn in blauer Ferne", "tokens": ["Seit", "Jah\u00b7ren", "kein", "Ge\u00b7stirn", "in", "blau\u00b7er", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die goldne, thaubegl\u00e4nzte Schwinge heben.", "tokens": ["Die", "gold\u00b7ne", ",", "thau\u00b7be\u00b7gl\u00e4nz\u00b7te", "Schwin\u00b7ge", "he\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Erde \u2014 ach! er lie\u00df' sie Euch so gerne;", "tokens": ["Die", "Er\u00b7de", "ach", "!", "er", "lie\u00df'", "sie", "Euch", "so", "ger\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ITJ", "$.", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch sprecht, ihr Herrn, wer hat Euch Macht gegeben,", "tokens": ["Doch", "sprecht", ",", "ihr", "Herrn", ",", "wer", "hat", "Euch", "Macht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Hand zu legen auf des Himmels Sterne?", "tokens": ["Die", "Hand", "zu", "le\u00b7gen", "auf", "des", "Him\u00b7mels", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}