{"dta.poem.13603": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Springer, der in luft'gem Schreiten", "tokens": ["Sprin\u00b7ger", ",", "der", "in", "luft'\u00b7gem", "Schrei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber die gemeine Welt,", "tokens": ["Ue\u00b7ber", "die", "ge\u00b7mei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kokettiret mit den Leuten,", "tokens": ["Ko\u00b7ket\u00b7ti\u00b7ret", "mit", "den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sicherlich vom Seile f\u00e4llt.", "tokens": ["Si\u00b7cher\u00b7lich", "vom", "Sei\u00b7le", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schiffer, der nach jedem Winde,", "tokens": ["Schif\u00b7fer", ",", "der", "nach", "je\u00b7dem", "Win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blas' er witzig oder dumm,", "tokens": ["Blas'", "er", "wit\u00b7zig", "o\u00b7der", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Seegel stellt geschwinde,", "tokens": ["Sei\u00b7ne", "See\u00b7gel", "stellt", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt im Wasser schm\u00e4hlich um.", "tokens": ["Kommt", "im", "Was\u00b7ser", "schm\u00e4h\u00b7lich", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weisen Sterne doch die Richtung,", "tokens": ["Wei\u00b7sen", "Ster\u00b7ne", "doch", "die", "Rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rst du Nachts doch fernen Klang,", "tokens": ["H\u00f6rst", "du", "Nachts", "doch", "fer\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorthin liegt das Land der Dichtung,", "tokens": ["Dor\u00b7thin", "liegt", "das", "Land", "der", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fahre zu und frag' nicht lang.", "tokens": ["Fah\u00b7re", "zu", "und", "frag'", "nicht", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}