{"textgrid.poem.45177": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wir waren nicht sehr p\u00e4pstlich gesinnt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir waren nicht sehr p\u00e4pstlich gesinnt,", "tokens": ["Wir", "wa\u00b7ren", "nicht", "sehr", "p\u00e4pst\u00b7lich", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nun rufen wir uns heiser:", "tokens": ["Nun", "ru\u00b7fen", "wir", "uns", "hei\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gib, Pius, die Tiare auf", "tokens": ["Gib", ",", "Pius", ",", "die", "Ti\u00b7a\u00b7re", "auf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NE", "$,", "ART", "NN", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und werde unser Kaiser.", "tokens": ["Und", "wer\u00b7de", "un\u00b7ser", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}