{"dta.poem.5578": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch ohne fernerhin, mit jenen weisen Alten,", "tokens": ["Doch", "oh\u00b7ne", "fer\u00b7ner\u00b7hin", ",", "mit", "je\u00b7nen", "wei\u00b7sen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PTKVZ", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns aufzuhalten,", "tokens": ["Uns", "auf\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So hat manch edler Geist auch unsre Zeit geziert,", "tokens": ["So", "hat", "manch", "ed\u00b7ler", "Geist", "auch", "uns\u00b7re", "Zeit", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mancher sch\u00f6n-und angenehmen Wahrheit", "tokens": ["Der", "man\u00b7cher", "sch\u00f6n\u00b7\u00b7und", "an\u00b7ge\u00b7neh\u00b7men", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lobw\u00fcrdig nachgesp\u00fcrt.", "tokens": ["Lob\u00b7w\u00fcr\u00b7dig", "nach\u00b7ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vor vielen haben ja die duncklen Finsternissen", "tokens": ["Vor", "vie\u00b7len", "ha\u00b7ben", "ja", "die", "dunck\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der alten Schrifften weichen m\u00fcssen,", "tokens": ["Der", "al\u00b7ten", "Schriff\u00b7ten", "wei\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sie sie, wie vorhin, in die gelehrte Klarheit", "tokens": ["Wenn", "sie", "sie", ",", "wie", "vor\u00b7hin", ",", "in", "die", "ge\u00b7lehr\u00b7te", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "PWAV", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von neuen setzeten. Der untersuchten K\u00fcnste", "tokens": ["Von", "neu\u00b7en", "set\u00b7ze\u00b7ten", ".", "Der", "un\u00b7ter\u00b7such\u00b7ten", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Genaue Wissenschafft, der W\u00fcrckung der Natur", "tokens": ["Ge\u00b7nau\u00b7e", "Wis\u00b7sen\u00b7schafft", ",", "der", "W\u00fcr\u00b7ckung", "der", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch der Erfahrung Licht begl\u00fcckt entdeckte Spur", "tokens": ["Durch", "der", "Er\u00b7fah\u00b7rung", "Licht", "be\u00b7gl\u00fcckt", "ent\u00b7deck\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vertreiben allgemach des groben Jrrthums D\u00fcnste,", "tokens": ["Ver\u00b7trei\u00b7ben", "all\u00b7ge\u00b7mach", "des", "gro\u00b7ben", "Jrr\u00b7thums", "D\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die uns benebelten: und trotz den grossen Nahmen", "tokens": ["Die", "uns", "be\u00b7ne\u00b7bel\u00b7ten", ":", "und", "trotz", "den", "gros\u00b7sen", "Nah\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$.", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Von Nom und von Athen, wodurch so K\u00fcnst\u2019 als Wissen", "tokens": ["Von", "Nom", "und", "von", "A\u00b7then", ",", "wo\u00b7durch", "so", "K\u00fcnst'", "als", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "$,", "PWAV", "ADV", "NN", "KOUS", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Des menschlichen Geschlechts auf solchen Gipffel kamen;", "tokens": ["Des", "menschli\u00b7chen", "Ge\u00b7schlechts", "auf", "sol\u00b7chen", "Gipf\u00b7fel", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "So haben wir jedoch geheime Ding erfahren,", "tokens": ["So", "ha\u00b7ben", "wir", "je\u00b7doch", "ge\u00b7hei\u00b7me", "Ding", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die ihnen jederzeit verborgen waren.", "tokens": ["Die", "ih\u00b7nen", "je\u00b7der\u00b7zeit", "ver\u00b7bor\u00b7gen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}