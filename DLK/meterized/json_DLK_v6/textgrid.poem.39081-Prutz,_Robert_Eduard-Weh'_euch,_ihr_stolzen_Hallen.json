{"textgrid.poem.39081": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Weh' euch, ihr stolzen Hallen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da soll die H\u00fctte niemand bauen,", "tokens": ["Da", "soll", "die", "H\u00fct\u00b7te", "nie\u00b7mand", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da siedle nie ein Mensch sich an,", "tokens": ["da", "sied\u00b7le", "nie", "ein", "Mensch", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wo man den Dichtern nicht mehr trauen,", "tokens": ["wo", "man", "den", "Dich\u00b7tern", "nicht", "mehr", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wo man kein Lied mehr h\u00f6ren kann!", "tokens": ["wo", "man", "kein", "Lied", "mehr", "h\u00f6\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo man der Leier goldne Saite", "tokens": ["Wo", "man", "der", "Lei\u00b7er", "gold\u00b7ne", "Sai\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "weit schlimmer f\u00fcrchtet als das Schwert!", "tokens": ["weit", "schlim\u00b7mer", "f\u00fcrch\u00b7tet", "als", "das", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo zu der Geister frischem Streite", "tokens": ["Wo", "zu", "der", "Geis\u00b7ter", "fri\u00b7schem", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "man weder Raum noch Licht gew\u00e4hrt!", "tokens": ["man", "we\u00b7der", "Raum", "noch", "Licht", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KON", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was n\u00fctzt es Tempel zu errichten,", "tokens": ["Was", "n\u00fctzt", "es", "Tem\u00b7pel", "zu", "er\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "wenn ihr die G\u00f6tter selbst vertreibt?", "tokens": ["wenn", "ihr", "die", "G\u00f6t\u00b7ter", "selbst", "ver\u00b7treibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Wahrheit sucht ihr zu vernichten:", "tokens": ["Die", "Wahr\u00b7heit", "sucht", "ihr", "zu", "ver\u00b7nich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Was n\u00fctzt es, da\u00df die L\u00fcge bleibt?! \u2013", "tokens": ["Was", "n\u00fctzt", "es", ",", "da\u00df", "die", "L\u00fc\u00b7ge", "bleibt", "?!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr habt euch selbst das Los gezogen,", "tokens": ["Ihr", "habt", "euch", "selbst", "das", "Los", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "die Stunde naht, das Ma\u00df ist voll;", "tokens": ["die", "Stun\u00b7de", "naht", ",", "das", "Ma\u00df", "ist", "voll", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "und statt der Leier greift den Bogen", "tokens": ["und", "statt", "der", "Lei\u00b7er", "greift", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "der r\u00e4chende, der Gott Apoll!", "tokens": ["der", "r\u00e4\u00b7chen\u00b7de", ",", "der", "Gott", "A\u00b7poll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}