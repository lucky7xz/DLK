{"dta.poem.10178": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod, Winter des Lebens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wir unsre frische Jugend mit der Fr\u00fchlings-Zeit\nvergleichen,", "tokens": ["Wie", "wir", "uns\u00b7re", "fri\u00b7sche", "Ju\u00b7gend", "mit", "der", "Fr\u00fch\u00b7lings\u00b7Zeit", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die erwachsne Zeit dem Sommer, mit dem Herbst den al-", "tokens": ["Die", "er\u00b7wachs\u00b7ne", "Zeit", "dem", "Som\u00b7mer", ",", "mit", "dem", "Herbst", "den", "al"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Also deucht mich, da\u00df dem Winter unser sterben und er-", "tokens": ["Al\u00b7so", "deucht", "mich", ",", "da\u00df", "dem", "Win\u00b7ter", "un\u00b7ser", "ster\u00b7ben", "und", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPOSAT", "ADJA", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Sich mit Recht vergleichen mag.", "tokens": ["Sich", "mit", "Recht", "ver\u00b7glei\u00b7chen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PRF", "APPR", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie des Winters kalter Hauch unsrer B\u00e4ume Decke", "tokens": ["Wie", "des", "Win\u00b7ters", "kal\u00b7ter", "Hauch", "uns\u00b7rer", "B\u00e4u\u00b7me", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Also wird auch unsrer Seele, durch des Todes kalte", "tokens": ["Al\u00b7so", "wird", "auch", "uns\u00b7rer", "See\u00b7le", ",", "durch", "des", "To\u00b7des", "kal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Jhres C\u00f6rpers Deck\u2019 entnommen, und sie gleichsam auch", "tokens": ["Ih\u00b7res", "C\u00f6r\u00b7pers", "Deck'", "ent\u00b7nom\u00b7men", ",", "und", "sie", "gleich\u00b7sam", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "VVPP", "$,", "KON", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Gleicht die lange Nacht des Grabes nicht des Winters lan-", "tokens": ["Gleicht", "die", "lan\u00b7ge", "Nacht", "des", "Gra\u00b7bes", "nicht", "des", "Win\u00b7ters", "lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Feld und Wald scheint todt im Frost, alle Creatur gestor-", "tokens": ["Feld", "und", "Wald", "scheint", "todt", "im", "Frost", ",", "al\u00b7le", "Crea\u00b7tur", "ge\u00b7stor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,", "PIAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Unser C\u00f6rper scheint im Grabe morsch, verweset und ver-", "tokens": ["Un\u00b7ser", "C\u00f6r\u00b7per", "scheint", "im", "Gra\u00b7be", "morsch", ",", "ver\u00b7we\u00b7set", "und", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$,", "VVFIN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.11": {"text": "Es verfaulen, wie die Bl\u00e4tter, Haut und Sehnen, Fleisch", "tokens": ["Es", "ver\u00b7fau\u00b7len", ",", "wie", "die", "Bl\u00e4t\u00b7ter", ",", "Haut", "und", "Seh\u00b7nen", ",", "Fleisch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVINF", "$,", "PWAV", "ART", "NN", "$,", "NN", "KON", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Da Wust, Moder und Verwesung, die sowol, als jene", "tokens": ["Da", "Wust", ",", "Mo\u00b7der", "und", "Ver\u00b7we\u00b7sung", ",", "die", "so\u00b7wol", ",", "als", "je\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "ADV", "$,", "KOUS", "PDS"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wie wir aber f\u00fcr den Winter uns zwar schen\u2019n, und", "tokens": ["Wie", "wir", "a\u00b7ber", "f\u00fcr", "den", "Win\u00b7ter", "uns", "zwar", "schen'n", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "VVINF", "$,", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Doch, dieweil er unvermeidlich, uns gelassen auf ihn schi-", "tokens": ["Doch", ",", "die\u00b7weil", "er", "un\u00b7ver\u00b7meid\u00b7lich", ",", "uns", "ge\u00b7las\u00b7sen", "auf", "ihn", "schi"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "$,", "PPER", "VVPP", "APPR", "PPER", "TRUNC"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und, mit allem n\u00f6thigen, gegen ihn uns wol versehn;", "tokens": ["Und", ",", "mit", "al\u00b7lem", "n\u00f6\u00b7thi\u00b7gen", ",", "ge\u00b7gen", "ihn", "uns", "wol", "ver\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PIS", "VVINF", "$,", "APPR", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Also sollten wir nicht minder auch dem Tod\u2019 entgegen gehn:", "tokens": ["Al\u00b7so", "soll\u00b7ten", "wir", "nicht", "min\u00b7der", "auch", "dem", "Tod'", "ent\u00b7ge\u00b7gen", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Unsre Seele, f\u00fcr der Neue scharffen Frost, mit guten Wer", "tokens": ["Uns\u00b7re", "See\u00b7le", ",", "f\u00fcr", "der", "Neu\u00b7e", "scharf\u00b7fen", "Frost", ",", "mit", "gu\u00b7ten", "Wer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Glauben, Demuth, Hoffnung, Liebe wol verwahren, sch\u00fc-", "tokens": ["Glau\u00b7ben", ",", "De\u00b7muth", ",", "Hoff\u00b7nung", ",", "Lie\u00b7be", "wol", "ver\u00b7wah\u00b7ren", ",", "sch\u00fc"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "ADV", "VVINF", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Da\u00df wir, sonder Furcht und Schmertzen, jenen seelgen Him", "tokens": ["Da\u00df", "wir", ",", "son\u00b7der", "Furcht", "und", "Schmert\u00b7zen", ",", "je\u00b7nen", "seel\u00b7gen", "Him"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KON", "NN", "KON", "NN", "$,", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Und in ihm den ew\u2019gen Fr\u00fchling, f\u00e4hig w\u00e4ren zu erwarten.", "tokens": ["Und", "in", "ihm", "den", "ew'\u00b7gen", "Fr\u00fch\u00b7ling", ",", "f\u00e4\u00b7hig", "w\u00e4\u00b7ren", "zu", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN", "$,", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}}}}}