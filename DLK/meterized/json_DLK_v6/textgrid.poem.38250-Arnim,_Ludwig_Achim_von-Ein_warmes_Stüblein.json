{"textgrid.poem.38250": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ein warmes St\u00fcblein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann ich des Morgens fr\u00fch aufstehe,", "tokens": ["Wann", "ich", "des", "Mor\u00b7gens", "fr\u00fch", "auf\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist mein St\u00fcblein geheitzet,", "tokens": ["So", "ist", "mein", "St\u00fcb\u00b7lein", "ge\u00b7heit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So kommt mein Lieb, und beut mir einen guten Morgen.", "tokens": ["So", "kommt", "mein", "Lieb", ",", "und", "beut", "mir", "ei\u00b7nen", "gu\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein guter Morgen ist bald dahin,", "tokens": ["Ein", "gu\u00b7ter", "Mor\u00b7gen", "ist", "bald", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gott geb meiner Lieb ein steten Sinn,", "tokens": ["Gott", "geb", "mei\u00b7ner", "Lieb", "ein", "ste\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Dazu ein fr\u00f6hlich Gem\u00fcthe.", "tokens": ["Da\u00b7zu", "ein", "fr\u00f6h\u00b7lich", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}