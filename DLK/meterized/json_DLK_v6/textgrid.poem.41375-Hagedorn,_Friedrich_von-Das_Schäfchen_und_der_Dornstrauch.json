{"textgrid.poem.41375": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Sch\u00e4fchen und der Dornstrauch", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sch\u00e4fchen kroch in dicke Hecken,", "tokens": ["Ein", "Sch\u00e4f\u00b7chen", "kroch", "in", "di\u00b7cke", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem rauhen Regen zu entgehn.", "tokens": ["Dem", "rau\u00b7hen", "Re\u00b7gen", "zu", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier konnt' es freilich trocken stehn;", "tokens": ["Hier", "konnt'", "es", "frei\u00b7lich", "tro\u00b7cken", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein die Wolle blieb ihm stecken.", "tokens": ["Al\u00b7lein", "die", "Wol\u00b7le", "blieb", "ihm", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Begl\u00fcckt ist, den dies Schaf belehrt.", "tokens": ["Be\u00b7gl\u00fcckt", "ist", ",", "den", "dies", "Schaf", "be\u00b7lehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beth\u00f6rte Had'rer, la\u00dft euch rathen.", "tokens": ["Be\u00b7th\u00f6r\u00b7te", "Ha\u00b7d'\u00b7rer", ",", "la\u00dft", "euch", "ra\u00b7then", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vertraut die Wolle nicht den scharfen Advocaten.", "tokens": ["Ver\u00b7traut", "die", "Wol\u00b7le", "nicht", "den", "schar\u00b7fen", "Ad\u00b7vo\u00b7ca\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Oft ist, was ihr gewinnt, nicht halb der Kosten werth.", "tokens": ["Oft", "ist", ",", "was", "ihr", "ge\u00b7winnt", ",", "nicht", "halb", "der", "Kos\u00b7ten", "werth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}