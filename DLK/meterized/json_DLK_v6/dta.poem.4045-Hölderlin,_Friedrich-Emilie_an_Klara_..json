{"dta.poem.4045": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Emilie an Klara .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.85", "nl:0.14"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Er woll' ihn morgen sprechen, sagte mir", "tokens": ["Er", "woll'", "ihn", "mor\u00b7gen", "spre\u00b7chen", ",", "sag\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Vater, morgen! und er schien nicht freundlich.", "tokens": ["Mein", "Va\u00b7ter", ",", "mor\u00b7gen", "!", "und", "er", "schien", "nicht", "freund\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun sitz' ich hier und meine Augen ruhn", "tokens": ["Nun", "sitz'", "ich", "hier", "und", "mei\u00b7ne", "Au\u00b7gen", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schlummern nicht; \u2014 ach! sch\u00e4men mu\u00df ich mich,", "tokens": ["Und", "schlum\u00b7mern", "nicht", ";", "ach", "!", "sch\u00e4\u00b7men", "mu\u00df", "ich", "mich", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "$(", "ITJ", "$.", "VVINF", "VMFIN", "PPER", "PRF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es dir zu klagen, \u2014 will ich stille werden,", "tokens": ["Es", "dir", "zu", "kla\u00b7gen", ",", "will", "ich", "stil\u00b7le", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKZU", "VVINF", "$,", "$(", "VMFIN", "PPER", "VVFIN", "VAINF", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So regt ein Laut mich auf; ich sinn' und bitte,", "tokens": ["So", "regt", "ein", "Laut", "mich", "auf", ";", "ich", "sinn'", "und", "bit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "KON", "PTKANT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wei\u00df nicht, was? und sagen m\u00f6cht' ich viel,", "tokens": ["Und", "wei\u00df", "nicht", ",", "was", "?", "und", "sa\u00b7gen", "m\u00f6cht'", "ich", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "$.", "KON", "VVINF", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch ist die Seele stumm; \u2014 o fragen m\u00f6cht' ich", "tokens": ["Doch", "ist", "die", "See\u00b7le", "stumm", ";", "o", "fra\u00b7gen", "m\u00f6cht'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "$.", "$(", "FM", "VVINF", "VMFIN", "PPER"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Die sorgenfreien B\u00e4ume hier, die Stralen", "tokens": ["Die", "sor\u00b7gen\u00b7frei\u00b7en", "B\u00e4u\u00b7me", "hier", ",", "die", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Nacht und ihre Schatten, wie es nun", "tokens": ["Der", "Nacht", "und", "ih\u00b7re", "Schat\u00b7ten", ",", "wie", "es", "nun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mir endlich werden wird.", "tokens": ["Mir", "end\u00b7lich", "wer\u00b7den", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu still ist's mir", "tokens": ["Zu", "still", "ist's", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In dieser sch\u00f6nen Nacht, und ihre L\u00fcfte", "tokens": ["In", "die\u00b7ser", "sch\u00f6\u00b7nen", "Nacht", ",", "und", "ih\u00b7re", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind mir nicht hold, wie sonst. Die Th\u00f6rin!", "tokens": ["Sind", "mir", "nicht", "hold", ",", "wie", "sonst", ".", "Die", "Th\u00f6\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "PWAV", "ADV", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lang er ferne war, so liebt' ich ihn;", "tokens": ["So", "lang", "er", "fer\u00b7ne", "war", ",", "so", "liebt'", "ich", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun bin ich kalt, und zag' und z\u00fcrne mir", "tokens": ["Nun", "bin", "ich", "kalt", ",", "und", "zag'", "und", "z\u00fcr\u00b7ne", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und andern. \u2014 Auch die Worte, so ich dir", "tokens": ["Und", "an\u00b7dern", ".", "Auch", "die", "Wor\u00b7te", ",", "so", "ich", "dir"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$.", "$(", "ADV", "ART", "NN", "$,", "ADV", "PPER", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In dieser b\u00f6sen Stunde schreibe, lieb'", "tokens": ["In", "die\u00b7ser", "b\u00f6\u00b7sen", "Stun\u00b7de", "schrei\u00b7be", ",", "lieb'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich nicht, und was ich sonst von ihm geschrieben,", "tokens": ["Ich", "nicht", ",", "und", "was", "ich", "sonst", "von", "ihm", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "KON", "PWS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Unleidlich ist es mir. Was ist es denn?", "tokens": ["Un\u00b7leid\u00b7lich", "ist", "es", "mir", ".", "Was", "ist", "es", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "$.", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich w\u00fcnsche fast, ich h\u00e4tt' ihn nie gesehn.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "fast", ",", "ich", "h\u00e4tt'", "ihn", "nie", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mein Friede war doch sch\u00f6ner. Theures Herz!", "tokens": ["Mein", "Frie\u00b7de", "war", "doch", "sch\u00f6\u00b7ner", ".", "Theu\u00b7res", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich bin betr\u00fcbt, und anders, denn ichs war,", "tokens": ["Ich", "bin", "be\u00b7tr\u00fcbt", ",", "und", "an\u00b7ders", ",", "denn", "ichs", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "ADV", "$,", "KON", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da ich um den Verlornen trauerte.", "tokens": ["Da", "ich", "um", "den", "Ver\u00b7lor\u00b7nen", "trau\u00b7er\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich bin es nimmer, nein! ich bin es nicht,", "tokens": ["Ich", "bin", "es", "nim\u00b7mer", ",", "nein", "!", "ich", "bin", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PTKANT", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich bin nicht gut, und seellos bin ich auch.", "tokens": ["Ich", "bin", "nicht", "gut", ",", "und", "seel\u00b7los", "bin", "ich", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KON", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mich l\u00e4\u00dft die Furcht, die h\u00e4\u00dfliche, nicht ruhn.", "tokens": ["Mich", "l\u00e4\u00dft", "die", "Furcht", ",", "die", "h\u00e4\u00df\u00b7li\u00b7che", ",", "nicht", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O da\u00df der goldne Tag die Ruhe mir,", "tokens": ["O", "da\u00df", "der", "gold\u00b7ne", "Tag", "die", "Ru\u00b7he", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein eigen Leben wiederbr\u00e4cht'! \u2014", "tokens": ["Mein", "ei\u00b7gen", "Le\u00b7ben", "wie\u00b7der\u00b7br\u00e4cht'", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich will", "tokens": ["Ich", "will"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Geduldig seyn, und wenn der Vater ihn", "tokens": ["Ge\u00b7dul\u00b7dig", "seyn", ",", "und", "wenn", "der", "Va\u00b7ter", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAINF", "$,", "KON", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht ehrt, mir ihn versagt, den Theuren,", "tokens": ["Nicht", "ehrt", ",", "mir", "ihn", "ver\u00b7sagt", ",", "den", "Theu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PPER", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schweig' ich lieber, und es soll mir nicht", "tokens": ["So", "schweig'", "ich", "lie\u00b7ber", ",", "und", "es", "soll", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu sehr die Seele kr\u00e4nken; kann ich still", "tokens": ["Zu", "sehr", "die", "See\u00b7le", "kr\u00e4n\u00b7ken", ";", "kann", "ich", "still"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "VVINF", "$.", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihn ehren doch, und bleiben, wie ich bin.", "tokens": ["Ihn", "eh\u00b7ren", "doch", ",", "und", "blei\u00b7ben", ",", "wie", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}