{"textgrid.poem.31376": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr Theodor K\u00f6rner]", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck auf! Was in der Erde schie\u00dfet,", "tokens": ["Gl\u00fcck", "auf", "!", "Was", "in", "der", "Er\u00b7de", "schie\u00b7\u00dfet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gold, das suchst du auf.", "tokens": ["Das", "Gold", ",", "das", "suchst", "du", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das, was dein Herz, o Freund, verschlie\u00dfet,", "tokens": ["Das", ",", "was", "dein", "Herz", ",", "o", "Freund", ",", "ver\u00b7schlie\u00b7\u00dfet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00dft du nicht. Gl\u00fcck auf!", "tokens": ["Ver\u00b7gi\u00dft", "du", "nicht", ".", "Gl\u00fcck", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}