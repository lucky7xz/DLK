{"textgrid.poem.55406": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beuge, Liakos, dem Pascha,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beuge, Liakos, dem Pascha,", "tokens": ["Beu\u00b7ge", ",", "Lia\u00b7kos", ",", "dem", "Pa\u00b7scha", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Beuge dem Wesire dich.", "tokens": ["Beu\u00b7ge", "dem", "We\u00b7si\u00b7re", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Warst du vormals Armatole,", "tokens": ["Warst", "du", "vor\u00b7mals", "Ar\u00b7ma\u00b7to\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Landgebieter wirst du nun.", "tokens": ["Land\u00b7ge\u00b7bie\u00b7ter", "wirst", "du", "nun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbbleibt nur Liakos am Leben,", "tokens": ["\u00bb", "bleibt", "nur", "Lia\u00b7kos", "am", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wird er nie ein Beugender.", "tokens": ["Wird", "er", "nie", "ein", "Beu\u00b7gen\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur sein Schwert ist ihm der Pascha,", "tokens": ["Nur", "sein", "Schwert", "ist", "ihm", "der", "Pa\u00b7scha", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Ist Wesir das Schie\u00dfgewehr.\u00ab", "tokens": ["Ist", "We\u00b7sir", "das", "Schie\u00df\u00b7ge\u00b7wehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ali Pascha, das vernehmend,", "tokens": ["A\u00b7li", "Pa\u00b7scha", ",", "das", "ver\u00b7neh\u00b7mend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Z\u00fcrnt dem Unwillkommenen,", "tokens": ["Z\u00fcrnt", "dem", "Un\u00b7will\u00b7kom\u00b7me\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Schreibt die Briefe, die Befehle,", "tokens": ["Schreibt", "die", "Brie\u00b7fe", ",", "die", "Be\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "So bestimmt er, was zu tun:", "tokens": ["So", "be\u00b7stimmt", "er", ",", "was", "zu", "tun", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bbveli Guekas, eile kr\u00e4ftig", "tokens": ["\u00bb", "ve\u00b7li", "Gu\u00b7e\u00b7kas", ",", "ei\u00b7le", "kr\u00e4f\u00b7tig"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM.la", "FM.la", "$,", "VVFIN", "ADJD"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.14": {"text": "Durch die St\u00e4dte, durch das Land,", "tokens": ["Durch", "die", "St\u00e4d\u00b7te", ",", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Bring mir Liakos zur Stelle,", "tokens": ["Bring", "mir", "Lia\u00b7kos", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Lebend sei er oder tot!\u00ab", "tokens": ["Le\u00b7bend", "sei", "er", "o\u00b7der", "tot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Guekas streift nun durch die Gegend,", "tokens": ["Gu\u00b7e\u00b7kas", "streift", "nun", "durch", "die", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auf die K\u00e4mpfer macht er Jagd,", "tokens": ["Auf", "die", "K\u00e4mp\u00b7fer", "macht", "er", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Forscht sie aus und \u00fcberrascht sie,", "tokens": ["Forscht", "sie", "aus", "und", "\u00fc\u00b7berr\u00b7ascht", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "An der Vorhut ist er schon.", "tokens": ["An", "der", "Vor\u00b7hut", "ist", "er", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Kontogiakupis, der schreit nun", "tokens": ["Kon\u00b7to\u00b7gi\u00b7a\u00b7ku\u00b7pis", ",", "der", "schreit", "nun"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Von des Bollwerks hohem Stand:", "tokens": ["Von", "des", "Boll\u00b7werks", "ho\u00b7hem", "Stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "\u00bbherzhaft, Kinder mein! zur Arbeit,", "tokens": ["\u00bb", "herz\u00b7haft", ",", "Kin\u00b7der", "mein", "!", "zur", "Ar\u00b7beit", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "NN", "PPOSAT", "$.", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Kinder mein, zum Streit hervor!\u00ab", "tokens": ["Kin\u00b7der", "mein", ",", "zum", "Streit", "her\u00b7vor", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "$,", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Liakos erscheint behende,", "tokens": ["Lia\u00b7kos", "er\u00b7scheint", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "H\u00e4lt in Z\u00e4hnen fest das Schwert.", "tokens": ["H\u00e4lt", "in", "Z\u00e4h\u00b7nen", "fest", "das", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Tag und Nacht ward nun geschlagen,", "tokens": ["Tag", "und", "Nacht", "ward", "nun", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Tage drei, der N\u00e4chte drei,", "tokens": ["Ta\u00b7ge", "drei", ",", "der", "N\u00e4ch\u00b7te", "drei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "ART", "NN", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Albaneserinnen weinen,", "tokens": ["Al\u00b7ba\u00b7ne\u00b7se\u00b7rin\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Schwarz in Trauerkleid geh\u00fcllt;", "tokens": ["Schwarz", "in", "Trau\u00b7er\u00b7kleid", "ge\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Veli Guekas kehrt nur wieder", "tokens": ["Ve\u00b7li", "Gu\u00b7e\u00b7kas", "kehrt", "nur", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Hingew\u00fcrgt im eignen Blut.", "tokens": ["Hin\u00b7ge\u00b7w\u00fcrgt", "im", "eig\u00b7nen", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}