{"textgrid.poem.33112": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die niedren H\u00fctten, wie die stolzen Hallen", "tokens": ["Die", "nied\u00b7ren", "H\u00fct\u00b7ten", ",", "wie", "die", "stol\u00b7zen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Firenze's sind erf\u00fcllt von Gram und Schweigen,", "tokens": ["Fi\u00b7ren\u00b7ze's", "sind", "er\u00b7f\u00fcllt", "von", "Gram", "und", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "APPR", "NE", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die sch\u00f6nste Jungfrau, edel sonder Gleichen,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Jung\u00b7frau", ",", "e\u00b7del", "son\u00b7der", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die dort gewohnet, ist dem Tod gefallen,", "tokens": ["Die", "dort", "ge\u00b7woh\u00b7net", ",", "ist", "dem", "Tod", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu ihrer Bahre Str\u00f6me Volkes wallen;", "tokens": ["Zu", "ih\u00b7rer", "Bah\u00b7re", "Str\u00f6\u00b7me", "Vol\u00b7kes", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Jeder will vor ihr die Kniee neigen,", "tokens": ["Ein", "Je\u00b7der", "will", "vor", "ihr", "die", "Kni\u00b7ee", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm d\u00e4ucht an dieser lieblichsten der Leichen,", "tokens": ["Ihm", "d\u00e4ucht", "an", "die\u00b7ser", "lieb\u00b7lichs\u00b7ten", "der", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Es sei der Sch\u00f6nheit Reich mit ihr zerfallen,", "tokens": ["Es", "sei", "der", "Sch\u00f6n\u00b7heit", "Reich", "mit", "ihr", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, wie die Sonne, eh' sie niedergeht,", "tokens": ["Denn", ",", "wie", "die", "Son\u00b7ne", ",", "eh'", "sie", "nie\u00b7der\u00b7geht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Noch ihre vollste Gluth der Erde sendet,", "tokens": ["Noch", "ih\u00b7re", "volls\u00b7te", "Gluth", "der", "Er\u00b7de", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im letzten Blick den h\u00f6chsten Reiz verschwendet \u2013", "tokens": ["Im", "letz\u00b7ten", "Blick", "den", "h\u00f6chs\u00b7ten", "Reiz", "ver\u00b7schwen\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So hat im Leben selbst sie nie umweht", "tokens": ["So", "hat", "im", "Le\u00b7ben", "selbst", "sie", "nie", "um\u00b7weht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein solcher Zauber, solcher Sch\u00f6nheit F\u00fclle,", "tokens": ["Ein", "sol\u00b7cher", "Zau\u00b7ber", ",", "sol\u00b7cher", "Sch\u00f6n\u00b7heit", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als schadenfroh der Tod leiht ihrer H\u00fclle!", "tokens": ["Als", "scha\u00b7den\u00b7froh", "der", "Tod", "leiht", "ih\u00b7rer", "H\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}