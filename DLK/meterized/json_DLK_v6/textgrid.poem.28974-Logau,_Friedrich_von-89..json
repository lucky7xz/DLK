{"textgrid.poem.28974": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist die Welt der grosse Mensch? Ist der Mensch die kleine Welt?", "tokens": ["Ist", "die", "Welt", "der", "gros\u00b7se", "Mensch", "?", "Ist", "der", "Mensch", "die", "klei\u00b7ne", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie da\u00df dieser dann kein Lentz sich auff ihren Winter stellt?", "tokens": ["Wie", "da\u00df", "die\u00b7ser", "dann", "kein", "Lentz", "sich", "auff", "ih\u00b7ren", "Win\u00b7ter", "stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PDS", "ADV", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Welt-Verliebte klagen so; Himmel-Buhlen k\u00fcmmet ein,", "tokens": ["Welt\u00b7Ver\u00b7lieb\u00b7te", "kla\u00b7gen", "so", ";", "Him\u00b7mel\u00b7Buh\u00b7len", "k\u00fcm\u00b7met", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Jene Zeit, da immer Lentz, nimmer nie wird Winter seyn.", "tokens": ["Je\u00b7ne", "Zeit", ",", "da", "im\u00b7mer", "Lentz", ",", "nim\u00b7mer", "nie", "wird", "Win\u00b7ter", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "ADV", "NN", "$,", "ADV", "ADV", "VAFIN", "NN", "VAINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}