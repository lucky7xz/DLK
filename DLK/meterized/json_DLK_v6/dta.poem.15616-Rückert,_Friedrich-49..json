{"dta.poem.15616": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie du die Erde siehst von Sch\u00f6pferkraft durchwaltet,", "tokens": ["Wie", "du", "die", "Er\u00b7de", "siehst", "von", "Sch\u00f6p\u00b7fer\u00b7kraft", "durch\u00b7wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Naturabstufungen der Menschheit zugestaltet;", "tokens": ["Na\u00b7tur\u00b7ab\u00b7stu\u00b7fun\u00b7gen", "der", "Menschheit", "zu\u00b7ge\u00b7stal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "So hindert nichts, da\u00df nicht auf andern Himmelsf\u00e4ren", "tokens": ["So", "hin\u00b7dert", "nichts", ",", "da\u00df", "nicht", "auf", "an\u00b7dern", "Him\u00b7mels\u00b7f\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch andre Ordnungen und Gipfelpunkte w\u00e4ren,", "tokens": ["Auch", "and\u00b7re", "Ord\u00b7nun\u00b7gen", "und", "Gip\u00b7fel\u00b7punk\u00b7te", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf andrer Grundlag' aufgef\u00fchrt ein andrer Bau", "tokens": ["Auf", "an\u00b7drer", "Grund\u00b7lag'", "auf\u00b7ge\u00b7f\u00fchrt", "ein", "an\u00b7drer", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebens, eingeweiht zu andrer Geister Schau;", "tokens": ["Des", "Le\u00b7bens", ",", "ein\u00b7ge\u00b7weiht", "zu", "an\u00b7drer", "Geis\u00b7ter", "Schau", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die etwas geist'ges thun, das unserm Denken gleicht,", "tokens": ["Die", "et\u00b7was", "geist'\u00b7ges", "thun", ",", "das", "un\u00b7serm", "Den\u00b7ken", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielleicht es \u00fcbertrifft, vielleicht es nicht erreicht.", "tokens": ["Viel\u00b7leicht", "es", "\u00fc\u00b7bert\u00b7rifft", ",", "viel\u00b7leicht", "es", "nicht", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "ADV", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er denkt in seiner Sf\u00e4r' alswie in deiner du;", "tokens": ["Er", "denkt", "in", "sei\u00b7ner", "Sf\u00e4r'", "als\u00b7wie", "in", "dei\u00b7ner", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "APPR", "PPOSAT", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne da\u00df ihrs denkt, denkt ihr einander zu.", "tokens": ["Und", "oh\u00b7ne", "da\u00df", "ihrs", "denkt", ",", "denkt", "ihr", "ein\u00b7an\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wenn mit Geisteskraft er seinen Kreis durchdrungen,", "tokens": ["Und", "wenn", "mit", "Geis\u00b7tes\u00b7kraft", "er", "sei\u00b7nen", "Kreis", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du an deinem Theil den deinigen bezwungen;", "tokens": ["Und", "du", "an", "dei\u00b7nem", "Theil", "den", "dei\u00b7ni\u00b7gen", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dann werdet an der Grenz' ihr aneinander reichen,", "tokens": ["Dann", "wer\u00b7det", "an", "der", "Grenz'", "ihr", "an\u00b7ein\u00b7an\u00b7der", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um mit Gedanken euch erg\u00e4nzend auszugleichen:", "tokens": ["Um", "mit", "Ge\u00b7dan\u00b7ken", "euch", "er\u00b7g\u00e4n\u00b7zend", "aus\u00b7zu\u00b7glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PPER", "ADJD", "VVIZU", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Alswie zwei V\u00f6lker lang' in sich gesondert leben,", "tokens": ["Als\u00b7wie", "zwei", "V\u00f6l\u00b7ker", "lang'", "in", "sich", "ge\u00b7son\u00b7dert", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADV", "APPR", "PRF", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuletzt gemeinschaftlich in Eins zusammenstreben.", "tokens": ["Zu\u00b7letzt", "ge\u00b7mein\u00b7schaft\u00b7lich", "in", "Eins", "zu\u00b7sam\u00b7men\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Denn wol auch V\u00f6lker sind von eignen Grundanlagen,", "tokens": ["Denn", "wol", "auch", "V\u00f6l\u00b7ker", "sind", "von", "eig\u00b7nen", "Grun\u00b7dan\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergleichbar eigenem Planetenbau, getragen,", "tokens": ["Ver\u00b7gleich\u00b7bar", "ei\u00b7ge\u00b7nem", "Pla\u00b7ne\u00b7ten\u00b7bau", ",", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Aus eignem Wurzelstock, mit eignen Stammgeberden", "tokens": ["Aus", "eig\u00b7nem", "Wur\u00b7zel\u00b7stock", ",", "mit", "eig\u00b7nen", "Stamm\u00b7ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwachsend, f\u00e4hig doch als Menschen gleich zu werden.", "tokens": ["Er\u00b7wach\u00b7send", ",", "f\u00e4\u00b7hig", "doch", "als", "Men\u00b7schen", "gleich", "zu", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ADV", "KOUS", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "So hoff' ich, da\u00df wenn Zeit genug der ew'gen Urne", "tokens": ["So", "hoff'", "ich", ",", "da\u00df", "wenn", "Zeit", "ge\u00b7nug", "der", "ew'\u00b7gen", "Ur\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "KOUS", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entflo\u00df, die Erde tritt in Tausch mit dem Saturne.", "tokens": ["Ent\u00b7flo\u00df", ",", "die", "Er\u00b7de", "tritt", "in", "Tausch", "mit", "dem", "Sa\u00b7tur\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Worin dann sollen sich die beiden Eins erkennen?", "tokens": ["Wo\u00b7rin", "dann", "sol\u00b7len", "sich", "die", "bei\u00b7den", "Eins", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PRF", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weltb\u00fcrger sollen sie in h\u00f6herm Sinn sich nennen.", "tokens": ["Welt\u00b7b\u00fcr\u00b7ger", "sol\u00b7len", "sie", "in", "h\u00f6\u00b7herm", "Sinn", "sich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Inde\u00df, Astronomie, magst du der Himmelstaaten", "tokens": ["In\u00b7de\u00df", ",", "Ast\u00b7ro\u00b7no\u00b7mie", ",", "magst", "du", "der", "Him\u00b7mel\u00b7staa\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VMFIN", "PPER", "ART", "NN"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Entfernt-ausw\u00e4rtige Verh\u00e4ltnisse berathen.", "tokens": ["Ent\u00b7fernt\u00b7aus\u00b7w\u00e4r\u00b7ti\u00b7ge", "Ver\u00b7h\u00e4lt\u00b7nis\u00b7se", "be\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}