{"textgrid.poem.35115": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Worte", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu fr\u00fch, zu sp\u00e4t \u2013 \u2013 zwei Worte, welche eigen", "tokens": ["Zu", "fr\u00fch", ",", "zu", "sp\u00e4t", "\u2013", "\u2013", "zwei", "Wor\u00b7te", ",", "wel\u00b7che", "ei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "$(", "$(", "CARD", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Menschenleben, auch dem deinen, sind.", "tokens": ["Dem", "Men\u00b7schen\u00b7le\u00b7ben", ",", "auch", "dem", "dei\u00b7nen", ",", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "PPOSAT", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du siehst, da\u00df dir die Stunden schnell verstreichen", "tokens": ["Du", "siehst", ",", "da\u00df", "dir", "die", "Stun\u00b7den", "schnell", "ver\u00b7strei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df mit ihnen deine Zeit verrinnt.", "tokens": ["Und", "da\u00df", "mit", "ih\u00b7nen", "dei\u00b7ne", "Zeit", "ver\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du ahnst den Irrthum nicht, an dem du leidest;", "tokens": ["Du", "ahnst", "den", "Irr\u00b7thum", "nicht", ",", "an", "dem", "du", "lei\u00b7dest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du hast ja Zeit, du hast unendlich Zeit,", "tokens": ["Du", "hast", "ja", "Zeit", ",", "du", "hast", "un\u00b7end\u00b7lich", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn du dich in ihr zu fr\u00fch entscheidest,", "tokens": ["Und", "wenn", "du", "dich", "in", "ihr", "zu", "fr\u00fch", "ent\u00b7schei\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entscheidest du f\u00fcr deine Ewigkeit.", "tokens": ["Ent\u00b7schei\u00b7dest", "du", "f\u00fcr", "dei\u00b7ne", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es war zu fr\u00fch, als du die Rechnung schlossest", "tokens": ["Es", "war", "zu", "fr\u00fch", ",", "als", "du", "die", "Rech\u00b7nung", "schlos\u00b7sest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in das Deficit den Himmel warfst,", "tokens": ["Und", "in", "das", "De\u00b7fi\u00b7cit", "den", "Him\u00b7mel", "warfst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu fr\u00fch, als du begeistert \u00fcberflossest", "tokens": ["Zu", "fr\u00fch", ",", "als", "du", "be\u00b7geis\u00b7tert", "\u00fc\u00b7berf\u00b7los\u00b7sest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Zwecke, denen du nicht dienen darfst.", "tokens": ["F\u00fcr", "Zwe\u00b7cke", ",", "de\u00b7nen", "du", "nicht", "die\u00b7nen", "darfst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es war zu fr\u00fch; du warst nicht reif zum Denken,", "tokens": ["Es", "war", "zu", "fr\u00fch", ";", "du", "warst", "nicht", "reif", "zum", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als du dein Ziel nur an das Grab gestellt,", "tokens": ["Als", "du", "dein", "Ziel", "nur", "an", "das", "Grab", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn du verstandst noch nicht, dich in die Gruft zu senken,", "tokens": ["Denn", "du", "ver\u00b7standst", "noch", "nicht", ",", "dich", "in", "die", "Gruft", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Um aufzustehn schon hier in dieser Welt.", "tokens": ["Um", "auf\u00b7zu\u00b7stehn", "schon", "hier", "in", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es war zu sp\u00e4t, als pl\u00f6tzlich du erkanntest,", "tokens": ["Es", "war", "zu", "sp\u00e4t", ",", "als", "pl\u00f6tz\u00b7lich", "du", "er\u00b7kann\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du vielleicht, vielleicht nicht recht gethan,", "tokens": ["Da\u00df", "du", "viel\u00b7leicht", ",", "viel\u00b7leicht", "nicht", "recht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu sp\u00e4t, als du dich halb, nur halb ermanntest,", "tokens": ["Zu", "sp\u00e4t", ",", "als", "du", "dich", "halb", ",", "nur", "halb", "er\u00b7mann\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn das \u00bbVielleicht\u00ab hielt dich auf falscher Bahn.", "tokens": ["Denn", "das", "\u00bb", "Viel\u00b7leicht", "\u00ab", "hielt", "dich", "auf", "fal\u00b7scher", "Bahn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ADV", "$(", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es war zu sp\u00e4t; du hattest dich entschieden", "tokens": ["Es", "war", "zu", "sp\u00e4t", ";", "du", "hat\u00b7test", "dich", "ent\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$.", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und lebtest also nicht mehr in der Zeit.", "tokens": ["Und", "leb\u00b7test", "al\u00b7so", "nicht", "mehr", "in", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zwar warst und bist du immer noch hienieden,", "tokens": ["Zwar", "warst", "und", "bist", "du", "im\u00b7mer", "noch", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch wars schon Tod und ist schon Ewigkeit.", "tokens": ["Doch", "wars", "schon", "Tod", "und", "ist", "schon", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}