{"dta.poem.1098": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.71", "cy:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Obwol auch", "tokens": ["Ob\u00b7wol", "auch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Durch poe", "tokens": ["Durch", "po\u00b7e"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mein lob/ wie du/ kan beleben;", "tokens": ["Mein", "lob", "/", "wie", "du", "/", "kan", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PWAV", "PPER", "$(", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sitzt Er doch so wol zu pferd/", "tokens": ["Sitzt", "Er", "doch", "so", "wol", "zu", "pferd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df f\u00fcr jhn/ so lieb vnd wehrt/", "tokens": ["Da\u00df", "f\u00fcr", "jhn", "/", "so", "lieb", "vnd", "wehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$(", "ADV", "ADJD", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich mich in den tod wolt geben.", "tokens": ["Ich", "mich", "in", "den", "tod", "wolt", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}}}}