{"textgrid.poem.32602": {"metadata": {"author": {"name": "Shelley, Percy Bysshe", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die feurigen Berge donnern sich zu,", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die feurigen Berge donnern sich zu,", "tokens": ["Die", "feu\u00b7ri\u00b7gen", "Ber\u00b7ge", "don\u00b7nern", "sich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es hallt ihr Krachen von Zone zu Zone;", "tokens": ["Es", "hallt", "ihr", "Kra\u00b7chen", "von", "Zo\u00b7ne", "zu", "Zo\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "APPR", "NE", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Meere st\u00fcrmen sich auf aus der Ruh',", "tokens": ["Die", "Mee\u00b7re", "st\u00fcr\u00b7men", "sich", "auf", "aus", "der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und es bebt des Nordpols eisige Krone,", "tokens": ["Und", "es", "bebt", "des", "Nord\u00b7pols", "ei\u00b7si\u00b7ge", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn erschallt des Typhons Trombone.", "tokens": ["Wenn", "er\u00b7schallt", "des", "Ty\u00b7phons", "Trom\u00b7bo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ART", "NN", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Einer einzigen Wolke der Blitz entwettert,", "tokens": ["Ei\u00b7ner", "ein\u00b7zi\u00b7gen", "Wol\u00b7ke", "der", "Blitz", "ent\u00b7wet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der tausend Inseln in Gluth entfacht;", "tokens": ["Der", "tau\u00b7send", "In\u00b7seln", "in", "Gluth", "ent\u00b7facht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Erde bebt \u2013 eine Stadt ist zerschmettert,", "tokens": ["Die", "Er\u00b7de", "bebt", "\u2013", "ei\u00b7ne", "Stadt", "ist", "zer\u00b7schmet\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hundert beben und wanken; es kracht", "tokens": ["Und", "hun\u00b7dert", "be\u00b7ben", "und", "wan\u00b7ken", ";", "es", "kracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "CARD", "VVINF", "KON", "VVINF", "$.", "PPER", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Erde tiefunterster Schacht.", "tokens": ["Der", "Er\u00b7de", "tie\u00b7fun\u00b7ters\u00b7ter", "Schacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Doch heller ", "tokens": ["Doch", "hel\u00b7ler"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und wie du, so dr\u00f6hnet die Erde nimmer;", "tokens": ["Und", "wie", "du", ",", "so", "dr\u00f6h\u00b7net", "die", "Er\u00b7de", "nim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Des Meeres Getos, der Vulkane Spein", "tokens": ["Des", "Mee\u00b7res", "Ge\u00b7tos", ",", "der", "Vul\u00b7ka\u00b7ne", "Spein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Uebert\u00f6nst, \u00fcberstrahlst du; der Sonne Schimmer", "tokens": ["Ue\u00b7ber\u00b7t\u00f6nst", ",", "\u00fc\u00b7bers\u00b7trahlst", "du", ";", "der", "Son\u00b7ne", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$.", "ART", "NN", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Ist vor ", "tokens": ["Ist", "vor"], "token_info": ["word", "word"], "pos": ["VAFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Von Berg und Woge und jagender Wolke", "tokens": ["Von", "Berg", "und", "Wo\u00b7ge", "und", "ja\u00b7gen\u00b7der", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gl\u00e4nzt die Sonne durch Nebel und dunstigen Flor;", "tokens": ["Gl\u00e4nzt", "die", "Son\u00b7ne", "durch", "Ne\u00b7bel", "und", "duns\u00b7ti\u00b7gen", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Von Seele zu Seele, von Volke zu Volke,", "tokens": ["Von", "See\u00b7le", "zu", "See\u00b7le", ",", "von", "Vol\u00b7ke", "zu", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von Stadt zu Dorf schwingt dein Tag sich empor \u2013", "tokens": ["Von", "Stadt", "zu", "Dorf", "schwingt", "dein", "Tag", "sich", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie Schatten der Nacht fliehn Sklav und Tyrann,", "tokens": ["Wie", "Schat\u00b7ten", "der", "Nacht", "fliehn", "Sklav", "und", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn dein Licht zu leuchten begann.", "tokens": ["Wenn", "dein", "Licht", "zu", "leuch\u00b7ten", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}