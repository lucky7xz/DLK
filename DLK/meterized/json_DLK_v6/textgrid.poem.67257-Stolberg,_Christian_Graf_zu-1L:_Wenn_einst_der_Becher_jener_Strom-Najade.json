{"textgrid.poem.67257": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn einst der Becher jener Strom-Najade", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einst der Becher jener Strom-Najade", "tokens": ["Wenn", "einst", "der", "Be\u00b7cher", "je\u00b7ner", "Strom\u00b7Na\u00b7ja\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des R\u00fcckblicks eitlen Sehnsuchtsdurst mir stillt,", "tokens": ["Des", "R\u00fcck\u00b7blicks", "eit\u00b7len", "Sehn\u00b7suchts\u00b7durst", "mir", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn mir Vergessenheit die bunten Pfade", "tokens": ["Wenn", "mir", "Ver\u00b7ges\u00b7sen\u00b7heit", "die", "bun\u00b7ten", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Lebenswandrung nun in Nebel h\u00fcllt;", "tokens": ["Der", "Le\u00b7bens\u00b7wand\u00b7rung", "nun", "in", "Ne\u00b7bel", "h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verschwinde dann, am seligen Gestade,", "tokens": ["Ver\u00b7schwin\u00b7de", "dann", ",", "am", "se\u00b7li\u00b7gen", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort wo der ernsten Wage Loth nur gilt,", "tokens": ["Dort", "wo", "der", "erns\u00b7ten", "Wa\u00b7ge", "Loth", "nur", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du tanzender Phantomen Myriade,", "tokens": ["Du", "tan\u00b7zen\u00b7der", "Phan\u00b7to\u00b7men", "My\u00b7ria\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verflogner, leerer Stunden Dunstgebild.", "tokens": ["Ver\u00b7flog\u00b7ner", ",", "lee\u00b7rer", "Stun\u00b7den", "Dunst\u00b7ge\u00b7bild", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch eh' die Lipp' ich netze: Gnade, Gnade!", "tokens": ["Doch", "eh'", "die", "Lipp'", "ich", "net\u00b7ze", ":", "Gna\u00b7de", ",", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fleh' ich, o Nymph'. Erbarmend blick' und mild'", "tokens": ["Fleh'", "ich", ",", "o", "Nym\u00b7ph'", ".", "Er\u00b7bar\u00b7mend", "blick'", "und", "mild'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$.", "VVPP", "VVFIN", "KON", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf diese Z\u00e4hre, die mir w\u00fcnschend quillt:", "tokens": ["Auf", "die\u00b7se", "Z\u00e4h\u00b7re", ",", "die", "mir", "w\u00fcn\u00b7schend", "quillt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verschone du mit deinem strengen Bade", "tokens": ["Ver\u00b7scho\u00b7ne", "du", "mit", "dei\u00b7nem", "stren\u00b7gen", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Heiligthum in meiner Kleinods-Lade,", "tokens": ["Ein", "Hei\u00b7lig\u00b7thum", "in", "mei\u00b7ner", "Klein\u00b7ods\u00b7La\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein T\u00e4flein, das Amalia's Nam' erf\u00fcllt.", "tokens": ["Ein", "T\u00e4\u00b7flein", ",", "das", "A\u00b7ma\u00b7lia's", "Nam'", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}