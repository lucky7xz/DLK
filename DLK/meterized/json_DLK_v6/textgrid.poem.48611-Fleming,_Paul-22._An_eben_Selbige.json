{"textgrid.poem.48611": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "22. An eben Selbige", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieses, was ich euch itzt singe,", "tokens": ["Die\u00b7ses", ",", "was", "ich", "euch", "itzt", "sin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "denkt nicht, da\u00df es meine sei!", "tokens": ["denkt", "nicht", ",", "da\u00df", "es", "mei\u00b7ne", "sei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen ists, der solche Dinge", "tokens": ["Des\u00b7sen", "ists", ",", "der", "sol\u00b7che", "Din\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "durch euch, ihr gelobten Zwei,", "tokens": ["durch", "euch", ",", "ihr", "ge\u00b7lob\u00b7ten", "Zwei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ihm gedenket zu verbringen,", "tokens": ["ihm", "ge\u00b7den\u00b7ket", "zu", "ver\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die man weit und breit soll singen.", "tokens": ["die", "man", "weit", "und", "breit", "soll", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das erl\u00f6ste Volk der Erden,", "tokens": ["Das", "er\u00b7l\u00f6s\u00b7te", "Volk", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so nach Gottes Sohne hei\u00dft", "tokens": ["so", "nach", "Got\u00b7tes", "Soh\u00b7ne", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und durch euch soll freier werden,", "tokens": ["und", "durch", "euch", "soll", "frei\u00b7er", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das erhebet Stimm' und Geist", "tokens": ["das", "er\u00b7he\u00b7bet", "Stimm'", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "und ruft wie aus einem Munde:", "tokens": ["und", "ruft", "wie", "aus", "ei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zieht zu einer guten Stunde!", "tokens": ["Zieht", "zu", "ei\u00b7ner", "gu\u00b7ten", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zieht zu einer guten Stunde!", "tokens": ["Zieht", "zu", "ei\u00b7ner", "gu\u00b7ten", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "rufen wir und wer euch liebt.", "tokens": ["ru\u00b7fen", "wir", "und", "wer", "euch", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem es geht von Herzengrunde,", "tokens": ["Wem", "es", "geht", "von", "Her\u00b7zen\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der ist mehr froh als betr\u00fcbt;", "tokens": ["der", "ist", "mehr", "froh", "als", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "ADJD", "KOKOM", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "di\u00df, weil ihr ihm seid benommen;", "tokens": ["di\u00df", ",", "weil", "ihr", "ihm", "seid", "be\u00b7nom\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jenes, was darauf soll kommen,", "tokens": ["je\u00b7nes", ",", "was", "da\u00b7rauf", "soll", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PAV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "was ihr halb froh itzund schauen,", "tokens": ["was", "ihr", "halb", "froh", "it\u00b7zund", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ja kaum noch besprechen k\u00f6nnt.", "tokens": ["ja", "kaum", "noch", "be\u00b7spre\u00b7chen", "k\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure selbst Ihr, eure Frauen,", "tokens": ["Eu\u00b7re", "selbst", "Ihr", ",", "eu\u00b7re", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "w\u00fcndschen euch, was ihr euch g\u00f6nnt,", "tokens": ["w\u00fcnd\u00b7schen", "euch", ",", "was", "ihr", "euch", "g\u00f6nnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "hoffend, dieses saure Scheiden", "tokens": ["hof\u00b7fend", ",", "die\u00b7ses", "sau\u00b7re", "Schei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "bring' einst desto s\u00fc\u00dfre Freuden.", "tokens": ["bring'", "einst", "des\u00b7to", "s\u00fc\u00df\u00b7re", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Geschrei so gro\u00dfer Sachen", "tokens": ["Das", "Ge\u00b7schrei", "so", "gro\u00b7\u00dfer", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dringet durch die breite Welt.", "tokens": ["drin\u00b7get", "durch", "die", "brei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die erfreuten ", "tokens": ["Die", "er\u00b7freu\u00b7ten"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "bis ihr euch vor ihnen stellt.", "tokens": ["bis", "ihr", "euch", "vor", "ih\u00b7nen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wie es euch bald soll empfangen.", "tokens": ["wie", "es", "euch", "bald", "soll", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Euch soll treffen ganz kein Schade!", "tokens": ["Euch", "soll", "tref\u00b7fen", "ganz", "kein", "Scha\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "soll das ", "tokens": ["soll", "das"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "euch sehn steigen auf sein Land!", "tokens": ["euch", "sehn", "stei\u00b7gen", "auf", "sein", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Ihr sollt, wie ihr werdet sp\u00fcren,", "tokens": ["Ihr", "sollt", ",", "wie", "ihr", "wer\u00b7det", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "auch den Schw\u00e4chsten nicht verlieren!", "tokens": ["auch", "den", "Schw\u00e4chs\u00b7ten", "nicht", "ver\u00b7lie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gott, der Leitstern, ist nicht tr\u00fcbe,", "tokens": ["Gott", ",", "der", "Leits\u00b7tern", ",", "ist", "nicht", "tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zeigt den Weg auf fremder See.", "tokens": ["zeigt", "den", "Weg", "auf", "frem\u00b7der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eurer hohen F\u00fcrsten Liebe", "tokens": ["Eu\u00b7rer", "ho\u00b7hen", "F\u00fcrs\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sein die Br\u00fcder Helene.", "tokens": ["sein", "die", "Br\u00fc\u00b7der", "He\u00b7le\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wind und Flut fugt nach Begehren", "tokens": ["Wind", "und", "Flut", "fugt", "nach", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "durch so manche W\u00fcndsch' und Z\u00e4hren.", "tokens": ["durch", "so", "man\u00b7che", "W\u00fcnd\u00b7sch'", "und", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Nord und Osten, S\u00fcd und Westen,", "tokens": ["Nord", "und", "Os\u00b7ten", ",", "S\u00fcd", "und", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die verschw\u00f6ren sich zu euch.", "tokens": ["die", "ver\u00b7schw\u00f6\u00b7ren", "sich", "zu", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch ist g\u00f6nstig nach dem Besten", "tokens": ["Euch", "ist", "g\u00f6ns\u00b7tig", "nach", "dem", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das gemeine Sternen-Reich.", "tokens": ["das", "ge\u00b7mei\u00b7ne", "Ster\u00b7nen\u00b7Reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer mit Gott und Menschen reiset,", "tokens": ["Wer", "mit", "Gott", "und", "Men\u00b7schen", "rei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der ist billich hochgepreiset.", "tokens": ["der", "ist", "bil\u00b7lich", "hoch\u00b7ge\u00b7prei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich bin froh, da\u00df mir der Himmel", "tokens": ["Ich", "bin", "froh", ",", "da\u00df", "mir", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "solche Gunst hat angetan,", "tokens": ["sol\u00b7che", "Gunst", "hat", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich unser Kriegsget\u00fcmmel", "tokens": ["da\u00df", "ich", "un\u00b7ser", "Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kan von fernen sehen an", "tokens": ["kan", "von", "fer\u00b7nen", "se\u00b7hen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und den Weg so hoher Sachen", "tokens": ["und", "den", "Weg", "so", "ho\u00b7her", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mit euch gro\u00dfen Leuten machen.", "tokens": ["mit", "euch", "gro\u00b7\u00dfen", "Leu\u00b7ten", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Was wol soll mich das bewegen,", "tokens": ["Was", "wol", "soll", "mich", "das", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "was hiervon der P\u00f6vel spricht,", "tokens": ["was", "hier\u00b7von", "der", "P\u00f6\u00b7vel", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der sich Allem setzt entgegen,", "tokens": ["der", "sich", "Al\u00b7lem", "setzt", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was er selbst kan haben nicht?", "tokens": ["was", "er", "selbst", "kan", "ha\u00b7ben", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "VAFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcnftig will ich Einen fragen,", "tokens": ["K\u00fcnf\u00b7tig", "will", "ich", "Ei\u00b7nen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was er denn darzu wird sagen.", "tokens": ["was", "er", "denn", "dar\u00b7zu", "wird", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PAV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Mutter ", "tokens": ["Mut\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "deine langen Kriege hin,", "tokens": ["dei\u00b7ne", "lan\u00b7gen", "Krie\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "weil du nun kanst satt ermessen,", "tokens": ["weil", "du", "nun", "kanst", "satt", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was bei Zank ist f\u00fcr Gewin!", "tokens": ["was", "bei", "Zank", "ist", "f\u00fcr", "Ge\u00b7win", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So du bist des Schlagens m\u00fcde,", "tokens": ["So", "du", "bist", "des", "Schla\u00b7gens", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "so ergreife doch den Friede!", "tokens": ["so", "er\u00b7grei\u00b7fe", "doch", "den", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Des Verh\u00e4ngn\u00fc\u00df Schreiberinnen,", "tokens": ["Des", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "Schrei\u00b7be\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die drei Parzen, gehn herein:", "tokens": ["die", "drei", "Par\u00b7zen", ",", "gehn", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "was sie k\u00fcnftig werden spinnen,", "tokens": ["was", "sie", "k\u00fcnf\u00b7tig", "wer\u00b7den", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "soll von Nichts als Gutem sein.", "tokens": ["soll", "von", "Nichts", "als", "Gu\u00b7tem", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIS", "KOKOM", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die verlebte Zeit der Erden", "tokens": ["Die", "ver\u00b7leb\u00b7te", "Zeit", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "soll der jungen \u00e4hnlich werden.", "tokens": ["soll", "der", "jun\u00b7gen", "\u00e4hn\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hier hat Gottes Grimm ein Ende:", "tokens": ["Hier", "hat", "Got\u00b7tes", "Grimm", "ein", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00f6rderhin gilt Nichts als Gunst.", "tokens": ["f\u00f6r\u00b7der\u00b7hin", "gilt", "Nichts", "als", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Samlet euch, ihr hohen St\u00e4nde,", "tokens": ["Sam\u00b7let", "euch", ",", "ihr", "ho\u00b7hen", "St\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "heget eine neue Brunst,", "tokens": ["he\u00b7get", "ei\u00b7ne", "neu\u00b7e", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "eine Brunst, so die noch Schwachen", "tokens": ["ei\u00b7ne", "Brunst", ",", "so", "die", "noch", "Schwa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "froh und warm und stark kan machen!", "tokens": ["froh", "und", "warm", "und", "stark", "kan", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Di\u00df Land, das von neuem Zagen", "tokens": ["Di\u00df", "Land", ",", "das", "von", "neu\u00b7em", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "itzt erzittert weit und breit,", "tokens": ["itzt", "er\u00b7zit\u00b7tert", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wird alsdenn erfreuet sagen:", "tokens": ["wird", "als\u00b7denn", "er\u00b7freu\u00b7et", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist \u00fcberhin mein Leid.", "tokens": ["Es", "ist", "\u00fc\u00b7berh\u00b7in", "mein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Ach, da\u00df dieser Trost der Frommen", "tokens": ["Ach", ",", "da\u00df", "die\u00b7ser", "Trost", "der", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "doch noch heute solle kommen!", "tokens": ["doch", "noch", "heu\u00b7te", "sol\u00b7le", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Gute Nacht, ihr ", "tokens": ["Gu\u00b7te", "Nacht", ",", "ihr"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "du ber\u00fchmtes ebnes Land!", "tokens": ["du", "be\u00b7r\u00fchm\u00b7tes", "eb\u00b7nes", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasser, Berge, Wildni\u00df, W\u00e4lder", "tokens": ["Was\u00b7ser", ",", "Ber\u00b7ge", ",", "Wild\u00b7ni\u00df", ",", "W\u00e4l\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sto\u00dfen uns forthin zu Hand.", "tokens": ["sto\u00b7\u00dfen", "uns", "for\u00b7thin", "zu", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Denkt, da\u00df eurer Ruhe wegen", "tokens": ["Denkt", ",", "da\u00df", "eu\u00b7rer", "Ru\u00b7he", "we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wir der M\u00fche ziehn entgegen!", "tokens": ["wir", "der", "M\u00fc\u00b7he", "ziehn", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Gott der geb' euch besser Gl\u00fccke", "tokens": ["Gott", "der", "geb'", "euch", "bes\u00b7ser", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VVFIN", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und gew\u00fcndschtern Sonnenschein!", "tokens": ["und", "ge\u00b7w\u00fcnd\u00b7schtern", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommen wir, will er, zur\u00fccke,", "tokens": ["Kom\u00b7men", "wir", ",", "will", "er", ",", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "VMFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "so soll alles Friede sein", "tokens": ["so", "soll", "al\u00b7les", "Frie\u00b7de", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "oder, will es nicht gelingen,", "tokens": ["o\u00b7der", ",", "will", "es", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "so wolln wir euch einen bringen!", "tokens": ["so", "wolln", "wir", "euch", "ei\u00b7nen", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}