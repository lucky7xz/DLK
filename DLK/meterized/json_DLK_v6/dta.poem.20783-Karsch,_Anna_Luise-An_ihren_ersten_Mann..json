{"dta.poem.20783": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  ihren ersten Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erlaube, werther Schatz, da\u00df ich f\u00fcr allen Dingen             ", "tokens": ["Er\u00b7lau\u00b7be", ",", "wert\u00b7her", "Schatz", ",", "da\u00df", "ich", "f\u00fcr", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus ganz besonderm Trieb und dir ergebnen Pflicht,", "tokens": ["Aus", "ganz", "be\u00b7son\u00b7derm", "Trieb", "und", "dir", "er\u00b7geb\u00b7nen", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darf meine Schuldigkeit durch diese Zeilen bringen,", "tokens": ["Darf", "mei\u00b7ne", "Schul\u00b7dig\u00b7keit", "durch", "die\u00b7se", "Zei\u00b7len", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ein erfreuter Tag erscheint mit seinem Licht,", "tokens": ["Weil", "ein", "er\u00b7freu\u00b7ter", "Tag", "er\u00b7scheint", "mit", "sei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Tag, an welchem du zuerst die Welt erblicket,", "tokens": ["Ein", "Tag", ",", "an", "wel\u00b7chem", "du", "zu\u00b7erst", "die", "Welt", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Tag, der uns zugleich auch deinen Namen zeigt,", "tokens": ["Ein", "Tag", ",", "der", "uns", "zu\u00b7gleich", "auch", "dei\u00b7nen", "Na\u00b7men", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den hast du abermals erlebet h\u00f6chst begl\u00fccket,", "tokens": ["Den", "hast", "du", "a\u00b7ber\u00b7mals", "er\u00b7le\u00b7bet", "h\u00f6chst", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darum mein Herze sich zugleich mit dir erfreut.", "tokens": ["Da\u00b7rum", "mein", "Her\u00b7ze", "sich", "zu\u00b7gleich", "mit", "dir", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "VVFIN", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein Wohlergehen kann auch meinen Geist erg\u00f6tzen,", "tokens": ["Dein", "Woh\u00b7ler\u00b7ge\u00b7hen", "kann", "auch", "mei\u00b7nen", "Geist", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dein Vergn\u00fcgen macht da\u00df sich der schwache Kiel", "tokens": ["Und", "dein", "Ver\u00b7gn\u00fc\u00b7gen", "macht", "da\u00df", "sich", "der", "schwa\u00b7che", "Kiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit tausend Freuden thut in schwarzer Dinte netzen,", "tokens": ["Mit", "tau\u00b7send", "Freu\u00b7den", "thut", "in", "schwar\u00b7zer", "Din\u00b7te", "net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn ich zu diesen Tag dir gratuliren will.", "tokens": ["Wenn", "ich", "zu", "die\u00b7sen", "Tag", "dir", "gra\u00b7tu\u00b7li\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Drum leg ich diesen Wunsch zu deiner Freude bei:", "tokens": ["Drum", "leg", "ich", "die\u00b7sen", "Wunsch", "zu", "dei\u00b7ner", "Freu\u00b7de", "bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Himmel kr\u00f6ne dich mit stetem Wohlergehen,", "tokens": ["Der", "Him\u00b7mel", "kr\u00f6\u00b7ne", "dich", "mit", "ste\u00b7tem", "Woh\u00b7ler\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er mache deinen Geist von aller Unlust frei,", "tokens": ["Er", "ma\u00b7che", "dei\u00b7nen", "Geist", "von", "al\u00b7ler", "Un\u00b7lust", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und lasse lauter Gl\u00fcck an deiner Seite stehen.", "tokens": ["Und", "las\u00b7se", "lau\u00b7ter", "Gl\u00fcck", "an", "dei\u00b7ner", "Sei\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er lasse diesen Tag dich oftmals \u00fcberleben", "tokens": ["Er", "las\u00b7se", "die\u00b7sen", "Tag", "dich", "oft\u00b7mals", "\u00fc\u00b7berl\u00b7e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und \u00fcbersch\u00fctte dich mit tausend G\u00fctigkeit;", "tokens": ["Und", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7te", "dich", "mit", "tau\u00b7send", "G\u00fc\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und endlich wolle dir der H\u00f6chste alles geben,", "tokens": ["Und", "end\u00b7lich", "wol\u00b7le", "dir", "der", "H\u00f6chs\u00b7te", "al\u00b7les", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "ADJA", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was dir mein Herze w\u00fcnscht, und dich nur selbsterfreut.", "tokens": ["Was", "dir", "mein", "Her\u00b7ze", "w\u00fcnscht", ",", "und", "dich", "nur", "selbs\u00b7ter\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indessen bitt ich nimm doch dieses g\u00fctig an,", "tokens": ["In\u00b7des\u00b7sen", "bitt", "ich", "nimm", "doch", "die\u00b7ses", "g\u00fc\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVIMP", "ADV", "PDAT", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was hier die treue Hand aus Liebe \u00fcberreicht,", "tokens": ["Was", "hier", "die", "treu\u00b7e", "Hand", "aus", "Lie\u00b7be", "\u00fc\u00b7berr\u00b7eicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Weil ich f\u00fcr dieses mal nichts Be\u00dfres geben kann,", "tokens": ["Weil", "ich", "f\u00fcr", "die\u00b7ses", "mal", "nichts", "Be\u00df\u00b7res", "ge\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADV", "PIS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So nimm es g\u00fctig hin und bleibe mir geneigt.", "tokens": ["So", "nimm", "es", "g\u00fc\u00b7tig", "hin", "und", "blei\u00b7be", "mir", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}