{"textgrid.poem.24648": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[doch still!]", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch still! der Himmel stellt selbst euer Volck zu frieden:", "tokens": ["Doch", "still", "!", "der", "Him\u00b7mel", "stellt", "selbst", "eu\u00b7er", "Volck", "zu", "frie\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat auf diesen Sturm euch wieder wohl gethan.", "tokens": ["Er", "hat", "auf", "die\u00b7sen", "Sturm", "euch", "wie\u00b7der", "wohl", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist Jena schon von euch durch diesen Tod geschieden;", "tokens": ["Ist", "Je\u00b7na", "schon", "von", "euch", "durch", "die\u00b7sen", "Tod", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "APPR", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fchrt das Ruder itzt ein weiser Danckelmann.", "tokens": ["So", "f\u00fchrt", "das", "Ru\u00b7der", "itzt", "ein", "wei\u00b7ser", "Dan\u00b7ckel\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat jener euch geliebt; Der hat euch gar gebauet", "tokens": ["Hat", "je\u00b7ner", "euch", "ge\u00b7liebt", ";", "Der", "hat", "euch", "gar", "ge\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "PPER", "VVPP", "$.", "PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "War jener nur allein: Hier ist sein gantzes Haus.", "tokens": ["War", "je\u00b7ner", "nur", "al\u00b7lein", ":", "Hier", "ist", "sein", "gant\u00b7zes", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADV", "$.", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Bruder", "tokens": ["Dem", "Bru\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Und bey Ihm geht ihr wie bekannte ein und aus.", "tokens": ["Und", "bey", "Ihm", "geht", "ihr", "wie", "be\u00b7kann\u00b7te", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "KOKOM", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem neuen Cantzler rufft mit fr\u00f6lichem Gem\u00fcthe;", "tokens": ["Dem", "neu\u00b7en", "Cantz\u00b7ler", "rufft", "mit", "fr\u00f6\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mi\u00dfg\u00f6nnt dem alten Haupt nicht seine sanffte Ruh;", "tokens": ["Mi\u00df\u00b7g\u00f6nnt", "dem", "al\u00b7ten", "Haupt", "nicht", "sei\u00b7ne", "sanff\u00b7te", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kommt/ bittet di\u00df zuletzt/ da\u00df nun des Himmels G\u00fcte;", "tokens": ["Kommt", "/", "bit\u00b7tet", "di\u00df", "zu\u00b7letzt", "/", "da\u00df", "nun", "des", "Him\u00b7mels", "G\u00fc\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PDS", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Alten Jahre auch dem Neuen lege zu.", "tokens": ["Des", "Al\u00b7ten", "Jah\u00b7re", "auch", "dem", "Neu\u00b7en", "le\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}