{"textgrid.poem.62705": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Sendschreiben an eine Schlesische Dichterin", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sprich nicht, da\u00df mein Haupt", "tokens": ["O", "sprich", "nicht", ",", "da\u00df", "mein", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem Lorber-Kranz bezieret und belaubt,", "tokens": ["Mit", "ei\u00b7nem", "Lor\u00b7ber\u00b7Kranz", "be\u00b7zie\u00b7ret", "und", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ausgeschm\u00fccket sey? Ich kenne mein Verm\u00f6gen,", "tokens": ["Und", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", "sey", "?", "Ich", "ken\u00b7ne", "mein", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich mu\u00df noch st\u00e4rkern Grund zu meiner Dichtkunst legen.", "tokens": ["Ich", "mu\u00df", "noch", "st\u00e4r\u00b7kern", "Grund", "zu", "mei\u00b7ner", "Dicht\u00b7kunst", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eh ich ihn w\u00fcrdiglich und r\u00fchmlich tragen kan.", "tokens": ["Eh", "ich", "ihn", "w\u00fcr\u00b7dig\u00b7lich", "und", "r\u00fchm\u00b7lich", "tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man steigt mit Flei\u00df und Schwei\u00df gemach den Berg hinan,", "tokens": ["Man", "steigt", "mit", "Flei\u00df", "und", "Schwei\u00df", "ge\u00b7mach", "den", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Worauf der Musen-F\u00fcrst den Altar hat gegr\u00fcndet,", "tokens": ["Wo\u00b7rauf", "der", "Mu\u00b7sen\u00b7F\u00fcrst", "den", "Al\u00b7tar", "hat", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Auf welchem man sein Feur und Opfer-Gaben findet.", "tokens": ["Auf", "wel\u00b7chem", "man", "sein", "Feur", "und", "Op\u00b7fer\u00b7Ga\u00b7ben", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich strebe zwar darnach, da\u00df ich mit Ehr und Ruhm,", "tokens": ["Ich", "stre\u00b7be", "zwar", "dar\u00b7nach", ",", "da\u00df", "ich", "mit", "Ehr", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und zu Minervens Lust der Musen Heiligthum", "tokens": ["Und", "zu", "Mi\u00b7ner\u00b7vens", "Lust", "der", "Mu\u00b7sen", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "ART", "NN", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Dereinst betreten mag; Ob aber meine Kr\u00e4fte,", "tokens": ["De\u00b7reinst", "be\u00b7tre\u00b7ten", "mag", ";", "Ob", "a\u00b7ber", "mei\u00b7ne", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "KOUS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Wille, M\u00fch und Flei\u00df die\u00df wichtige Gesch\u00e4fte", "tokens": ["Mein", "Wil\u00b7le", ",", "M\u00fch", "und", "Flei\u00df", "die\u00df", "wich\u00b7ti\u00b7ge", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu fassen f\u00e4hig sind, da\u00df mu\u00df ich erst noch sehn.", "tokens": ["Zu", "fas\u00b7sen", "f\u00e4\u00b7hig", "sind", ",", "da\u00df", "mu\u00df", "ich", "erst", "noch", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich zweifle, da\u00df ich werd so weit zum Pindus gehn.", "tokens": ["Ich", "zweif\u00b7le", ",", "da\u00df", "ich", "werd", "so", "weit", "zum", "Pin\u00b7dus", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie? solte ich der Stof von deinen Lidern heisen?", "tokens": ["Wie", "?", "sol\u00b7te", "ich", "der", "Stof", "von", "dei\u00b7nen", "Li\u00b7dern", "hei\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kan dir nichts edleres den Weg zum Dichten weisen?", "tokens": ["Kan", "dir", "nichts", "ed\u00b7le\u00b7res", "den", "Weg", "zum", "Dich\u00b7ten", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADJA", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nimm einen Held vor dich, und singe ihm zum Prei\u00df,", "tokens": ["Nimm", "ei\u00b7nen", "Held", "vor", "dich", ",", "und", "sin\u00b7ge", "ihm", "zum", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die\u00df gibt dir bessern Stof; die\u00df wird dir deinen Flei\u00df", "tokens": ["Die\u00df", "gibt", "dir", "bes\u00b7sern", "Stof", ";", "die\u00df", "wird", "dir", "dei\u00b7nen", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$.", "PDS", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und angewandte Kunst mit Ehr und Lob begleiten,", "tokens": ["Und", "an\u00b7ge\u00b7wand\u00b7te", "Kunst", "mit", "Ehr", "und", "Lob", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die\u00df wird dir Hand und Kiel yu sch\u00f6nen Liedern leiten:", "tokens": ["Die\u00df", "wird", "dir", "Hand", "und", "Kiel", "yu", "sch\u00f6\u00b7nen", "Lie\u00b7dern", "lei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "KON", "NE", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df mancher muntre Geist zu meiner Ehre singt;", "tokens": ["Da\u00df", "man\u00b7cher", "mun\u00b7tre", "Geist", "zu", "mei\u00b7ner", "Eh\u00b7re", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mancher netter Kiel ein sch\u00f6n Geschenke bringt,", "tokens": ["Und", "man\u00b7cher", "net\u00b7ter", "Kiel", "ein", "sch\u00f6n", "Ge\u00b7schen\u00b7ke", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das w\u00fcrkt die H\u00f6flichkeit; die Gro\u00dfmuth will darneben/", "tokens": ["Das", "w\u00fcrkt", "die", "H\u00f6f\u00b7lich\u00b7keit", ";", "die", "Gro\u00df\u00b7muth", "will", "dar\u00b7ne\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VMFIN", "PAV", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ist mein Verdienst gleich schlecht, mein wenig Lob erheben.", "tokens": ["Ist", "mein", "Ver\u00b7dienst", "gleich", "schlecht", ",", "mein", "we\u00b7nig", "Lob", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "PPOSAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein, Wehrte Guttmanin! Mein Thon und mein Gesang,", "tokens": ["Nein", ",", "Wehr\u00b7te", "Gutt\u00b7ma\u00b7nin", "!", "Mein", "Thon", "und", "mein", "Ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein ganzes Sayten-Spiel und meiner Harfen Klang", "tokens": ["Mein", "gan\u00b7zes", "Say\u00b7ten\u00b7Spiel", "und", "mei\u00b7ner", "Har\u00b7fen", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gleicht gar auf keine Art des Orpheus s\u00fcssen Th\u00f6nen.", "tokens": ["Gleicht", "gar", "auf", "kei\u00b7ne", "Art", "des", "Or\u00b7pheus", "s\u00fcs\u00b7sen", "Th\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich glaube, du wilst mich mit diesem Gleichni\u00df h\u00f6hnen;", "tokens": ["Ich", "glau\u00b7be", ",", "du", "wilst", "mich", "mit", "die\u00b7sem", "Gleich\u00b7ni\u00df", "h\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist ja wohl werth genug, da\u00df man ihn scharf beweint.", "tokens": ["Ist", "ja", "wohl", "werth", "ge\u00b7nug", ",", "da\u00df", "man", "ihn", "scharf", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ADV", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich nur seinen Tod und sein Parade-Bette,", "tokens": ["Wenn", "ich", "nur", "sei\u00b7nen", "Tod", "und", "sein", "Pa\u00b7ra\u00b7de\u00b7Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach Wunsch und W\u00fcrdigkeit recht sch\u00f6n besungen h\u00e4tte.", "tokens": ["Nach", "Wunsch", "und", "W\u00fcr\u00b7dig\u00b7keit", "recht", "sch\u00f6n", "be\u00b7sun\u00b7gen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00e4r mein Geist vergn\u00fcgt. Wie? Ist die\u00df nicht zu viel,", "tokens": ["So", "w\u00e4r", "mein", "Geist", "ver\u00b7gn\u00fcgt", ".", "Wie", "?", "Ist", "die\u00df", "nicht", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$.", "PWAV", "$.", "VAFIN", "PDS", "PTKNEG", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df du, ", "tokens": ["Da\u00df", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Mehr als das Schlesische in deinem Brief wilst loben?", "tokens": ["Mehr", "als", "das", "Schle\u00b7si\u00b7sche", "in", "dei\u00b7nem", "Brief", "wilst", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du schmeichlest mir zu stark. Nein, meiner Lieder Proben", "tokens": ["Du", "schmeich\u00b7lest", "mir", "zu", "stark", ".", "Nein", ",", "mei\u00b7ner", "Lie\u00b7der", "Pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$.", "PTKANT", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bezeugen, da\u00df ich nicht die zehnde Muse bin.", "tokens": ["Be\u00b7zeu\u00b7gen", ",", "da\u00df", "ich", "nicht", "die", "zehn\u00b7de", "Mu\u00b7se", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darzu gelang ich nicht. Da denke ich nicht hin.", "tokens": ["Dar\u00b7zu", "ge\u00b7lang", "ich", "nicht", ".", "Da", "den\u00b7ke", "ich", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indessen dank ich dir vor dein geehrtes Schreiben,", "tokens": ["In\u00b7des\u00b7sen", "dank", "ich", "dir", "vor", "dein", "ge\u00b7ehr\u00b7tes", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es soll mir gleich wie du im Andenken bleiben.", "tokens": ["Es", "soll", "mir", "gleich", "wie", "du", "im", "An\u00b7den\u00b7ken", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KOKOM", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich schliesse, lebe wohl! begehe dieses Fest,", "tokens": ["Ich", "schlies\u00b7se", ",", "le\u00b7be", "wohl", "!", "be\u00b7ge\u00b7he", "die\u00b7ses", "Fest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$.", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das uns des Menschen Sohn mit n\u00e4chsten feyren l\u00e4\u00dft", "token