{"dta.poem.20091": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Wenersee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem Meergott k\u00e4mpften hei\u00dfer die Giganten", "tokens": ["Mit", "dem", "Meer\u00b7gott", "k\u00e4mpf\u00b7ten", "hei\u00b7\u00dfer", "die", "Gi\u00b7gan\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "einst, denn je:", "tokens": ["einst", ",", "denn", "je", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Siegreich, aus des Nordmeers Armen, rissen sie", "tokens": ["Sieg\u00b7reich", ",", "aus", "des", "Nord\u00b7meers", "Ar\u00b7men", ",", "ris\u00b7sen", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "den Wenersee,", "tokens": ["den", "We\u00b7ner\u00b7see", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Bauten, zwischen Sohn und Vater, einen l\u00e4nder-", "tokens": ["Bau\u00b7ten", ",", "zwi\u00b7schen", "Sohn", "und", "Va\u00b7ter", ",", "ei\u00b7nen", "l\u00e4n\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "KON", "NN", "$,", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "breiten Damm,", "tokens": ["brei\u00b7ten", "Damm", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Stellten vor das Thor, als W\u00e4chter, einen ganzen", "tokens": ["Stell\u00b7ten", "vor", "das", "Thor", ",", "als", "W\u00e4ch\u00b7ter", ",", "ei\u00b7nen", "gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "NE", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Felsenkamm.", "tokens": ["Fel\u00b7sen\u00b7kamm", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Oft erfasst den See ein Zittern tiefer Sehnsucht,", "tokens": ["Oft", "er\u00b7fasst", "den", "See", "ein", "Zit\u00b7tern", "tie\u00b7fer", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "und er lauscht,", "tokens": ["und", "er", "lauscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn\u2019s, gleich fernem Meeresbrausen, in den", "tokens": ["Wenn's", ",", "gleich", "fer\u00b7nem", "Mee\u00b7res\u00b7brau\u00b7sen", ",", "in", "den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "ADJA", "NN", "$,", "APPR", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tannengipfeln rauscht,", "tokens": ["Tan\u00b7nen\u00b7gip\u00b7feln", "rauscht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Beim Geheul der W\u00f6lfe w\u00e4hnt er, da\u00df die Winds-", "tokens": ["Beim", "Ge\u00b7heul", "der", "W\u00f6l\u00b7fe", "w\u00e4hnt", "er", ",", "da\u00df", "die", "Winds"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "braut nahe sei,", "tokens": ["braut", "na\u00b7he", "sei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und im heisren Lied des H\u00e4hers h\u00f6rt er nur der", "tokens": ["Und", "im", "heis\u00b7ren", "Lied", "des", "H\u00e4\u00b7hers", "h\u00f6rt", "er", "nur", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "M\u00f6ve Schrei.", "tokens": ["M\u00f6\u00b7ve", "Schrei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchling wird\u2019s, und drei\u00dfig Str\u00f6me zahlen", "tokens": ["Fr\u00fch\u00b7ling", "wird's", ",", "und", "drei\u00b7\u00dfig", "Str\u00f6\u00b7me", "zah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "KON", "CARD", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "pl\u00f6tzlich ihm Tribut,", "tokens": ["pl\u00f6tz\u00b7lich", "ihm", "Tri\u00b7but", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Drei\u00dfig Str\u00f6me, die sonst meerw\u00e4rts nieder-", "tokens": ["Drei\u00b7\u00dfig", "Str\u00f6\u00b7me", ",", "die", "sonst", "meer\u00b7w\u00e4rts", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "PRELS", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "st\u00fcrzten ihre Fluth,", "tokens": ["st\u00fcrz\u00b7ten", "ih\u00b7re", "Fluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Mit der Wasser Steigen steigt auch das Gef\u00fchl", "tokens": ["Mit", "der", "Was\u00b7ser", "Stei\u00b7gen", "steigt", "auch", "das", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "ihm seiner Kraft,", "tokens": ["ihm", "sei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und dem Freiheitsdrang gesellt sich jetzt der Zorn", "tokens": ["Und", "dem", "Frei\u00b7heits\u00b7drang", "ge\u00b7sellt", "sich", "jetzt", "der", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "ob seiner Haft.", "tokens": ["ob", "sei\u00b7ner", "Haft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Hoch schon \u00fcberragt der Spiegel seiner Fluth", "tokens": ["Hoch", "schon", "\u00fc\u00b7berr\u00b7agt", "der", "Spie\u00b7gel", "sei\u00b7ner", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "den Riesendamm,", "tokens": ["den", "Rie\u00b7sen\u00b7damm", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zwischen ihm und seiner Heimath hebt sich nur", "tokens": ["Zwi\u00b7schen", "ihm", "und", "sei\u00b7ner", "Hei\u00b7math", "hebt", "sich", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "der Felsenkamm,", "tokens": ["der", "Fel\u00b7sen\u00b7kamm", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da in siegessichrem Muthe, ruft er: \u201eVater,", "tokens": ["Da", "in", "sie\u00b7ges\u00b7sich\u00b7rem", "Mu\u00b7the", ",", "ruft", "er", ":", "\u201e", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "$.", "$(", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "meine Hand", "tokens": ["mei\u00b7ne", "Hand"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Streck\u2019 ich Dir noch heut entgegen durch das", "tokens": ["Streck'", "ich", "Dir", "noch", "heut", "ent\u00b7ge\u00b7gen", "durch", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "APPR", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "felsbewachte Land.\u201c", "tokens": ["fels\u00b7be\u00b7wach\u00b7te", "Land", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und der drei\u00dfig Str\u00f6me jeden schleudert er als", "tokens": ["Und", "der", "drei\u00b7\u00dfig", "Str\u00f6\u00b7me", "je\u00b7den", "schleu\u00b7dert", "er", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "CARD", "NN", "PIAT", "VVFIN", "PPER", "KOUS"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wurfgescho\u00df", "tokens": ["Wurf\u00b7ge\u00b7scho\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Auf den W\u00e4chter, und zertr\u00fcmmert Haupt und", "tokens": ["Auf", "den", "W\u00e4ch\u00b7ter", ",", "und", "zer\u00b7tr\u00fcm\u00b7mert", "Haupt", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "KON", "VVFIN", "NN", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Glieder dem Kolo\u00df,", "tokens": ["Glie\u00b7der", "dem", "Ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den gewalt\u2019gen Rumpf des Felsens \u00fcbersch\u00e4umt", "tokens": ["Den", "ge\u00b7walt'\u00b7gen", "Rumpf", "des", "Fel\u00b7sens", "\u00fc\u00b7bersc\u00b7h\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "sein Wasserschwall,", "tokens": ["sein", "Was\u00b7ser\u00b7schwall", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und zum ersten Mal, zur Tiefe donnert der", "tokens": ["Und", "zum", "ers\u00b7ten", "Mal", ",", "zur", "Tie\u00b7fe", "don\u00b7nert", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "ART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Trolh\u00e4tta-Fall.", "tokens": ["Trol\u00b7h\u00e4t\u00b7ta\u00b7Fall", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "In dem Riesendamme w\u00fchlt er sich mit leichter", "tokens": ["In", "dem", "Rie\u00b7sen\u00b7dam\u00b7me", "w\u00fchlt", "er", "sich", "mit", "leich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "M\u00fch ein Bett,", "tokens": ["M\u00fch", "ein", "Bett", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und das Meer kommt ihm entgegen, und sie", "tokens": ["Und", "das", "Meer", "kommt", "ihm", "ent\u00b7ge\u00b7gen", ",", "und", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "graben um die Wett\u2019,", "tokens": ["gra\u00b7ben", "um", "die", "Wett'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Jauchzend reichen Sohn und Vater zum Will-", "tokens": ["Jauch\u00b7zend", "rei\u00b7chen", "Sohn", "und", "Va\u00b7ter", "zum", "Will"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "APPRART", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "kommen sich die Hand,", "tokens": ["kom\u00b7men", "sich", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Felsenglieder, wie Troph\u00e4en, decken rings um-", "tokens": ["Fel\u00b7sen\u00b7glie\u00b7der", ",", "wie", "Trop\u00b7h\u00e4\u00b7en", ",", "de\u00b7cken", "rings", "um"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "VVFIN", "ADV", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "her das Land.", "tokens": ["her", "das", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}