{"textgrid.poem.26043": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Keine Dunkelheit tr\u00e4gt dich aus meinem Aug'", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abend tr\u00e4gt die Dinge fort,", "tokens": ["Der", "A\u00b7bend", "tr\u00e4gt", "die", "Din\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit die Herzen n\u00e4her zueinander r\u00fccken.", "tokens": ["Da\u00b7mit", "die", "Her\u00b7zen", "n\u00e4\u00b7her", "zu\u00b7ei\u00b7nan\u00b7der", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch die Nacht hat einen gl\u00fchenden Faden, der uns leuchtet,", "tokens": ["Noch", "die", "Nacht", "hat", "ei\u00b7nen", "gl\u00fc\u00b7hen\u00b7den", "Fa\u00b7den", ",", "der", "uns", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Keine Dunkelheit tr\u00e4gt dich aus meinem Aug'.", "tokens": ["Kei\u00b7ne", "Dun\u00b7kel\u00b7heit", "tr\u00e4gt", "dich", "aus", "mei\u00b7nem", "Aug'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn die Sonne unter den B\u00e4umen hingeht,", "tokens": ["Wenn", "die", "Son\u00b7ne", "un\u00b7ter", "den", "B\u00e4u\u00b7men", "hin\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die Nacht aus dem Gras aufsteht,", "tokens": ["Und", "die", "Nacht", "aus", "dem", "Gras", "auf\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Folge ich deines Herzens brennender Spur.", "tokens": ["Fol\u00b7ge", "ich", "dei\u00b7nes", "Her\u00b7zens", "bren\u00b7nen\u00b7der", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "O, nimm von meinen Lippen den lautlosen Schwur.", "tokens": ["O", ",", "nimm", "von", "mei\u00b7nen", "Lip\u00b7pen", "den", "laut\u00b7lo\u00b7sen", "Schwur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}