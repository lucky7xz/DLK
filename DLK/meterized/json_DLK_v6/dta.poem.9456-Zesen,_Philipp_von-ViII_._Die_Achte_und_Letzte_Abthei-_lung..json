{"dta.poem.9456": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n Die Achte und Letzte Abthei-  \n lung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleicht Sie den Mauren/ so wollen wir bauen", "tokens": ["Gleicht", "Sie", "den", "Mau\u00b7ren", "/", "so", "wol\u00b7len", "wir", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Silberne Bollwerck und Th\u00fcrne zur pracht;", "tokens": ["Sil\u00b7ber\u00b7ne", "Boll\u00b7werck", "und", "Th\u00fcr\u00b7ne", "zur", "pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ist Sie wie Th\u00fcren und Thore zu schauen/", "tokens": ["Ist", "Sie", "wie", "Th\u00fc\u00b7ren", "und", "Tho\u00b7re", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seyn wir auf Cederne bohlen bedacht/", "tokens": ["Seyn", "wir", "auf", "Ce\u00b7der\u00b7ne", "boh\u00b7len", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "APPR", "NN", "VVFIN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Die Libanon schickt:", "tokens": ["Die", "Li\u00b7ba\u00b7non", "schickt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wir wollen die Th\u00fcren", "tokens": ["Wir", "wol\u00b7len", "die", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit Tafelwerck zieren", "tokens": ["Mit", "Ta\u00b7fel\u00b7werck", "zie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Von Golde geschm\u00fcckt.", "tokens": ["Von", "Gol\u00b7de", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}