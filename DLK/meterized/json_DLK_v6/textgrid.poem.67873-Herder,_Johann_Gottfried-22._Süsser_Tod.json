{"textgrid.poem.67873": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "22. S\u00fcsser Tod", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's wahr, da\u00df Liebe sich an T\u00f6nen labet,", "tokens": ["Ist's", "wahr", ",", "da\u00df", "Lie\u00b7be", "sich", "an", "T\u00f6\u00b7nen", "la\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spiel auf! gib ihrer mir genug! zu gnug!", "tokens": ["Spiel", "auf", "!", "gib", "ih\u00b7rer", "mir", "ge\u00b7nug", "!", "zu", "gnug", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVIMP", "PPER", "PPER", "ADV", "$.", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df \u00fcbers\u00e4ttigt meine Liebe schwinde", "tokens": ["Da\u00df", "\u00fc\u00b7bers\u00b7\u00e4t\u00b7tigt", "mei\u00b7ne", "Lie\u00b7be", "schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sterbe. Noch einmal den Gang! \u2013 Er f\u00e4llt", "tokens": ["Und", "ster\u00b7be", ".", "Noch", "ein\u00b7mal", "den", "Gang", "!", "\u2013", "Er", "f\u00e4llt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "ADV", "ART", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So sterbend! O, er \u00fcberschlich mein Ohr,", "tokens": ["So", "ster\u00b7bend", "!", "O", ",", "er", "\u00fc\u00b7berschlich", "mein", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NE", "$,", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So wie das s\u00fcsse L\u00fcftchen \u00fcbers Beet", "tokens": ["So", "wie", "das", "s\u00fcs\u00b7se", "L\u00fcft\u00b7chen", "\u00fc\u00b7bers", "Beet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von Veilchen haucht und stielt und gibt Ger\u00fcche \u2013", "tokens": ["Von", "Veil\u00b7chen", "haucht", "und", "stielt", "und", "gibt", "Ge\u00b7r\u00fc\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Genug \u2013 nicht mehr! Dies klingt nicht mehr so s\u00fc\u00df. \u2013", "tokens": ["Ge\u00b7nug", "\u2013", "nicht", "mehr", "!", "Dies", "klingt", "nicht", "mehr", "so", "s\u00fc\u00df", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "PTKNEG", "ADV", "$.", "PDS", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u2013 Nur, lieber Freund, das St\u00fcckchen! \u2013 jenen alten", "tokens": ["\u2013", "Nur", ",", "lie\u00b7ber", "Freund", ",", "das", "St\u00fcck\u00b7chen", "!", "\u2013", "je\u00b7nen", "al\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADV", "$,", "ADV", "NN", "$,", "ART", "NN", "$.", "$(", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Altvatersang, wir h\u00f6rten's gestern Nacht \u2013", "tokens": ["Alt\u00b7va\u00b7ter\u00b7sang", ",", "wir", "h\u00f6r\u00b7ten's", "ge\u00b7stern", "Nacht", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und mich d\u00fcnkt, all mein Herz hob sich empor,", "tokens": ["Und", "mich", "d\u00fcnkt", ",", "all", "mein", "Herz", "hob", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PIAT", "PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "O, mehr als bei den luft'gen Arien,", "tokens": ["O", ",", "mehr", "als", "bei", "den", "luft'\u00b7gen", "A\u00b7rien", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Dem Wortgelese unsrer h\u00fcpfenden", "tokens": ["Dem", "Wort\u00b7ge\u00b7le\u00b7se", "uns\u00b7rer", "h\u00fcp\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Taumelnden Zeiten \u2013 komm \u2013 Ein Verschen nur!", "tokens": ["Tau\u00b7meln\u00b7den", "Zei\u00b7ten", "\u2013", "komm", "\u2013", "Ein", "Ver\u00b7schen", "nur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VVFIN", "$(", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Komm, lieber Junge, was wir gestern Nacht \u2013", "tokens": ["Komm", ",", "lie\u00b7ber", "Jun\u00b7ge", ",", "was", "wir", "ge\u00b7stern", "Nacht", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "NN", "$,", "PRELS", "PPER", "ADV", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Merk es, Cesario, 's ist alt und plan,", "tokens": ["Merk", "es", ",", "Ce\u00b7sa\u00b7rio", ",", "'s", "ist", "alt", "und", "plan", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NE", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Spinn- und Knittem\u00e4dchen an der Luft,", "tokens": ["Die", "Spinn", "und", "Knit\u00b7te\u00b7m\u00e4d\u00b7chen", "an", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Stubenm\u00e4dchen, wenn ihr Garn sie weben,", "tokens": ["Die", "Stu\u00b7ben\u00b7m\u00e4d\u00b7chen", ",", "wenn", "ihr", "Garn", "sie", "we\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So singen sies; 's ist Honigs\u00fc\u00df, es dahlt", "tokens": ["So", "sin\u00b7gen", "sies", ";", "'s", "ist", "Ho\u00b7ni\u00b7gs\u00fc\u00df", ",", "es", "dahlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So mit der Unschuldliebe, wie man vormals", "tokens": ["So", "mit", "der", "Un\u00b7schuld\u00b7lie\u00b7be", ",", "wie", "man", "vor\u00b7mals"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "PIS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Noch liebte \u2013 Bitt dich, sing'!", "tokens": ["Noch", "lieb\u00b7te", "\u2013", "Bitt", "dich", ",", "sing'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "NN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "S\u00fcsser Tod, s\u00fcsser Tod, komm,", "tokens": ["S\u00fcs\u00b7ser", "Tod", ",", "s\u00fcs\u00b7ser", "Tod", ",", "komm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Komm, senk mich nieder ins k\u00fchle Grab!", "tokens": ["Komm", ",", "senk", "mich", "nie\u00b7der", "ins", "k\u00fch\u00b7le", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Brich, o Herz, brich, o Herz fromm,", "tokens": ["Brich", ",", "o", "Herz", ",", "brich", ",", "o", "Herz", "fromm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ADJD", "$,", "FM", "NN", "ADJD", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Stirb fromm der s\u00fcssen Tyrannin ab!", "tokens": ["Stirb", "fromm", "der", "s\u00fcs\u00b7sen", "Ty\u00b7ran\u00b7nin", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Mein Gruftgewand schneewei\u00df und rein,", "tokens": ["Mein", "Gruft\u00b7ge\u00b7wand", "schnee\u00b7wei\u00df", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt es fertig!", "tokens": ["Legt", "es", "fer\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Kein Br\u00e4ut'gam h\u00fcllte je sich drein", "tokens": ["Kein", "Br\u00e4ut'\u00b7gam", "h\u00fcll\u00b7te", "je", "sich", "drein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fr\u00f6hlich.", "tokens": ["So", "fr\u00f6h\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Keine Blum', keine Blum' s\u00fc\u00df", "tokens": ["Kei\u00b7ne", "Blum'", ",", "kei\u00b7ne", "Blum'", "s\u00fc\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sollt ihr auf'n schwarzen Sarg mir streun!", "tokens": ["Sollt", "ihr", "auf'n", "schwar\u00b7zen", "Sarg", "mir", "streun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Keine Thr\u00e4n', keine Thr\u00e4n' flie\u00df,", "tokens": ["Kei\u00b7ne", "Thr\u00e4n'", ",", "kei\u00b7ne", "Thr\u00e4n'", "flie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wo sanft wird ruhn mein Todtenbein!", "tokens": ["Wo", "sanft", "wird", "ruhn", "mein", "Tod\u00b7ten\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach tausend, tausend Seufzer schwer \u2013", "tokens": ["Ach", "tau\u00b7send", ",", "tau\u00b7send", "Seuf\u00b7zer", "schwer", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "CARD", "$,", "CARD", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nein \u2013 ihr Meinen,", "tokens": ["Nein", "\u2013", "ihr", "Mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Legt hin mich, wo kein Liebender", "tokens": ["Legt", "hin", "mich", ",", "wo", "kein", "Lie\u00b7ben\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt weinen.", "tokens": ["Kommt", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}