{"textgrid.poem.64179": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Jung Anne", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, klinge nur lustig, du H\u00f6rnerklang, ich folge dir gern zum Streit;", "tokens": ["Ja", ",", "klin\u00b7ge", "nur", "lus\u00b7tig", ",", "du", "H\u00f6r\u00b7ner\u00b7klang", ",", "ich", "fol\u00b7ge", "dir", "gern", "zum", "Streit", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ADJD", "$,", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Heut k\u00fc\u00dft' ich, die ich freite so lang, jung Anne, die s\u00fc\u00dfe Maid.", "tokens": ["Heut", "k\u00fc\u00dft'", "ich", ",", "die", "ich", "frei\u00b7te", "so", "lang", ",", "jung", "An\u00b7ne", ",", "die", "s\u00fc\u00b7\u00dfe", "Maid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ich zog vor\u00fcber im Morgenstrahl: da stand sie im gr\u00fcnen Hag:", "tokens": ["Ich", "zog", "vor\u00b7\u00fc\u00b7ber", "im", "Mor\u00b7gen\u00b7strahl", ":", "da", "stand", "sie", "im", "gr\u00fc\u00b7nen", "Hag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "\u2013 \u00bbEi wohin, Childe Arthur, im blauen Stahl, wohin so fr\u00fch' am Tag?\u00ab", "tokens": ["\u2013", "\u00bb", "Ei", "wo\u00b7hin", ",", "Chil\u00b7de", "Ar\u00b7thur", ",", "im", "blau\u00b7en", "Stahl", ",", "wo\u00b7hin", "so", "fr\u00fch'", "am", "Tag", "?", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "NN", "PWAV", "$,", "NN", "NE", "$,", "APPRART", "ADJA", "NN", "$,", "PWAV", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "+--+-+--+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "\u00bbdie Schotten sind \u00fcber den blauen Tweed: Lord Percy will sie bestehn.", "tokens": ["\u00bb", "die", "Schot\u00b7ten", "sind", "\u00fc\u00b7ber", "den", "blau\u00b7en", "Tweed", ":", "Lord", "Per\u00b7cy", "will", "sie", "be\u00b7stehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NE", "$.", "NN", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+-+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Manch' Auge, das jetzt sie aufgehn sieht, sieht die Sonne nicht untergehn!", "tokens": ["Man\u00b7ch'", "Au\u00b7ge", ",", "das", "jetzt", "sie", "auf\u00b7gehn", "sieht", ",", "sieht", "die", "Son\u00b7ne", "nicht", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "PPER", "VVIZU", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "--+-+--+--+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Und es hat gereut schon manche Maid, die nie ihren Liebsten gek\u00fc\u00dft:", "tokens": ["Und", "es", "hat", "ge\u00b7reut", "schon", "man\u00b7che", "Maid", ",", "die", "nie", "ih\u00b7ren", "Liebs\u00b7ten", "ge\u00b7k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Dann ward er erschlagen im blutigen Streit, hat ku\u00dflos sterben gem\u00fc\u00dft.\u00ab", "tokens": ["Dann", "ward", "er", "er\u00b7schla\u00b7gen", "im", "blu\u00b7ti\u00b7gen", "Streit", ",", "hat", "ku\u00df\u00b7los", "ster\u00b7ben", "ge\u00b7m\u00fc\u00dft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "VAFIN", "ADJD", "VVINF", "VVPP", "$.", "$("], "meter": "-+--+--+--+-+-+--+", "measure": "amphibrach.tetra.plus"}, "line.9": {"text": "Da brach sie die Rose vom Gartenzaun und gab sie mir abgewandt:", "tokens": ["Da", "brach", "sie", "die", "Ro\u00b7se", "vom", "Gar\u00b7ten\u00b7zaun", "und", "gab", "sie", "mir", "ab\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+--+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Ich wei\u00df nicht, war es das Morgentaun: \u2013 ein Tropfe lag auf der Hand.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "war", "es", "das", "Mor\u00b7gen\u00b7taun", ":", "\u2013", "ein", "Trop\u00b7fe", "lag", "auf", "der", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VAFIN", "PPER", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Und ich zog an mich die zitternde Hand, ihr ins blaue Auge zu sehn,", "tokens": ["Und", "ich", "zog", "an", "mich", "die", "zit\u00b7tern\u00b7de", "Hand", ",", "ihr", "ins", "blau\u00b7e", "Au\u00b7ge", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+--+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Wegk\u00fc\u00dft' ich die Tr\u00e4ne, die drinnen stand und sie lie\u00df es gerne geschehn! \u2013", "tokens": ["Weg\u00b7k\u00fc\u00dft'", "ich", "die", "Tr\u00e4\u00b7ne", ",", "die", "drin\u00b7nen", "stand", "und", "sie", "lie\u00df", "es", "ger\u00b7ne", "ge\u00b7schehn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "KON", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+--+--+-+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Jetzt klinge nur lustig, du H\u00f6rnerklang, ich folge dir gern zum Streit:", "tokens": ["Jetzt", "klin\u00b7ge", "nur", "lus\u00b7tig", ",", "du", "H\u00f6r\u00b7ner\u00b7klang", ",", "ich", "fol\u00b7ge", "dir", "gern", "zum", "Streit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Heut k\u00fc\u00dft' ich, die ich freite so lang, jung Anne, die s\u00fc\u00dfe Maid! \u2013", "tokens": ["Heut", "k\u00fc\u00dft'", "ich", ",", "die", "ich", "frei\u00b7te", "so", "lang", ",", "jung", "An\u00b7ne", ",", "die", "s\u00fc\u00b7\u00dfe", "Maid", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "NE", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+--+-+", "measure": "iambic.septa.relaxed"}}}}}