{"textgrid.poem.50004": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "36. Danck vor Nichts zu Hofe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir ein Staatsmann viel verspricht,", "tokens": ["Wenn", "dir", "ein", "Staats\u00b7mann", "viel", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So zeige keinen Zweiffel nicht,", "tokens": ["So", "zei\u00b7ge", "kei\u00b7nen", "Zweif\u00b7fel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Nimm an die ", "tokens": ["Nimm", "an", "die"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Und das er nicht viel M\u00fche hab',", "tokens": ["Und", "das", "er", "nicht", "viel", "M\u00fc\u00b7he", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PTKNEG", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "So find'st du endlich in der That,", "tokens": ["So", "fin\u00b7d'st", "du", "end\u00b7lich", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dass der ", "tokens": ["Dass", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}