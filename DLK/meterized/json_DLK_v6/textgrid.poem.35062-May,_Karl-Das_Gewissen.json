{"textgrid.poem.35062": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das Gewissen", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was thatest du, als ich dich einstens bat,", "tokens": ["Was", "tha\u00b7test", "du", ",", "als", "ich", "dich", "eins\u00b7tens", "bat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach Gottes Wohlgefallen nur zu streben?", "tokens": ["Nach", "Got\u00b7tes", "Wohl\u00b7ge\u00b7fal\u00b7len", "nur", "zu", "stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte dir das Gl\u00fcck des Lebens geben;", "tokens": ["Ich", "woll\u00b7te", "dir", "das", "Gl\u00fcck", "des", "Le\u00b7bens", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun aber sag, was galt dir da mein Rath?", "tokens": ["Nun", "a\u00b7ber", "sag", ",", "was", "galt", "dir", "da", "mein", "Rath", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was thatest du, als ich dich einst belehrt,", "tokens": ["Was", "tha\u00b7test", "du", ",", "als", "ich", "dich", "einst", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df deine Wege falsche Wege seien?", "tokens": ["Da\u00df", "dei\u00b7ne", "We\u00b7ge", "fal\u00b7sche", "We\u00b7ge", "sei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte dich vom B\u00f6sen gern befreien;", "tokens": ["Ich", "woll\u00b7te", "dich", "vom", "B\u00f6\u00b7sen", "gern", "be\u00b7frei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun mu\u00df ich fragen: Hast du dich bekehrt?", "tokens": ["Nun", "mu\u00df", "ich", "fra\u00b7gen", ":", "Hast", "du", "dich", "be\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was thatest du, als ich dich dann verlie\u00df?", "tokens": ["Was", "tha\u00b7test", "du", ",", "als", "ich", "dich", "dann", "ver\u00b7lie\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich glaubte wohl, du werdest mich vermissen", "tokens": ["Ich", "glaub\u00b7te", "wohl", ",", "du", "wer\u00b7dest", "mich", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reuevoll um mich zu bitten wissen;", "tokens": ["Und", "reu\u00b7e\u00b7voll", "um", "mich", "zu", "bit\u00b7ten", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun frag ich dich: Was hat geholfen dies?", "tokens": ["Nun", "frag", "ich", "dich", ":", "Was", "hat", "ge\u00b7hol\u00b7fen", "dies", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "PWS", "VAFIN", "VVPP", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt komme ich ein letztes Mal zu dir", "tokens": ["Jetzt", "kom\u00b7me", "ich", "ein", "letz\u00b7tes", "Mal", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und frage dich: Wozu bist du geboren?", "tokens": ["Und", "fra\u00b7ge", "dich", ":", "Wo\u00b7zu", "bist", "du", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rst du auch diesmal nicht, bist du verloren;", "tokens": ["H\u00f6rst", "du", "auch", "dies\u00b7mal", "nicht", ",", "bist", "du", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin es, dein Gewissen. Folge mir!", "tokens": ["Ich", "bin", "es", ",", "dein", "Ge\u00b7wis\u00b7sen", ".", "Fol\u00b7ge", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}