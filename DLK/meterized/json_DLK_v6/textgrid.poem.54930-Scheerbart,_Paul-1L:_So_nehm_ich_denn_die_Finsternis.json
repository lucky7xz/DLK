{"textgrid.poem.54930": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: So nehm ich denn die Finsternis", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So nehm ich denn die Finsternis", "tokens": ["So", "nehm", "ich", "denn", "die", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und balle sie zusammen", "tokens": ["Und", "bal\u00b7le", "sie", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und werfe sie, so weit ich kann,", "tokens": ["Und", "wer\u00b7fe", "sie", ",", "so", "weit", "ich", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis in die gro\u00dfen Flammen,", "tokens": ["Bis", "in", "die", "gro\u00b7\u00dfen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die ich noch nicht gesehen habe", "tokens": ["Die", "ich", "noch", "nicht", "ge\u00b7se\u00b7hen", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die doch da sind \u2013 irgendwo", "tokens": ["Und", "die", "doch", "da", "sind", "\u2013", "ir\u00b7gend\u00b7wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADV", "ADV", "VAFIN", "$(", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lichterloh ...", "tokens": ["Lich\u00b7ter\u00b7loh", "..."], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}