{"textgrid.poem.33849": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zu Pferd! Zu Pferd!", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Pferd! Zu Pferd! Es saus't der Wind!", "tokens": ["Zu", "Pferd", "!", "Zu", "Pferd", "!", "Es", "saus't", "der", "Wind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schneewolken, d\u00fcstre, jagen!", "tokens": ["Schnee\u00b7wol\u00b7ken", ",", "d\u00fcst\u00b7re", ",", "ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die sch\u00fctten nun den Winter aus!", "tokens": ["Die", "sch\u00fct\u00b7ten", "nun", "den", "Win\u00b7ter", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Pferd! Zu Pferd! Durch Saus und Braus", "tokens": ["Zu", "Pferd", "!", "Zu", "Pferd", "!", "Durch", "Saus", "und", "Braus"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die hei\u00dfe Brust zu tragen!", "tokens": ["Die", "hei\u00b7\u00dfe", "Brust", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit krausen N\u00fcstern pr\u00fcft das Ro\u00df", "tokens": ["Mit", "krau\u00b7sen", "N\u00fcs\u00b7tern", "pr\u00fcft", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Luft, dann wiehert's muthig;", "tokens": ["Die", "Luft", ",", "dann", "wie\u00b7hert's", "mut\u00b7hig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur wie ich herrsche, dient das Thier,", "tokens": ["Nur", "wie", "ich", "herr\u00b7sche", ",", "dient", "das", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Druck: von dannen fliegt's mit mir,", "tokens": ["Ein", "Druck", ":", "von", "dan\u00b7nen", "fliegt's", "mit", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als w\u00e4r' mein Sporn schon blutig.", "tokens": ["Als", "w\u00e4r'", "mein", "Sporn", "schon", "blu\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In meinem Mantel w\u00fchlt der Wind,", "tokens": ["In", "mei\u00b7nem", "Man\u00b7tel", "w\u00fchlt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er raubt mir fast die M\u00fctze;", "tokens": ["Er", "raubt", "mir", "fast", "die", "M\u00fct\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' ihn gern auf meiner Spur,", "tokens": ["Ich", "hab'", "ihn", "gern", "auf", "mei\u00b7ner", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seiner Wuth erprob' ich's nur,", "tokens": ["An", "sei\u00b7ner", "Wuth", "er\u00b7prob'", "ich's", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie fest ich oben sitze.", "tokens": ["Wie", "fest", "ich", "o\u00b7ben", "sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}