{"textgrid.poem.48436": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Als H.M. Elias Major Po\u00ebta Laureatus worden", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die himmelische Kunst, durch die wir unsre Sinnen,", "tokens": ["Die", "him\u00b7me\u00b7li\u00b7sche", "Kunst", ",", "durch", "die", "wir", "uns\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die auch vom Himmel sind, zu Himmel schwingen k\u00fcnnen", "tokens": ["die", "auch", "vom", "Him\u00b7mel", "sind", ",", "zu", "Him\u00b7mel", "schwin\u00b7gen", "k\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VAFIN", "$,", "APPR", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hoch \u00fcber unsre Gruft; der wahre Widertod,", "tokens": ["hoch", "\u00fc\u00b7ber", "uns\u00b7re", "Gruft", ";", "der", "wah\u00b7re", "Wi\u00b7der\u00b7tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der uns nach uns belebt und machet frisch und rot,", "tokens": ["der", "uns", "nach", "uns", "be\u00b7lebt", "und", "ma\u00b7chet", "frisch", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wenn wir vorl\u00e4ngst erbla\u00dft: das sind die hohen Gaben,", "tokens": ["wenn", "wir", "vor\u00b7l\u00e4ngst", "er\u00b7bla\u00dft", ":", "das", "sind", "die", "ho\u00b7hen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die von der Po\u00ebsie ein reger Mut kan haben,", "tokens": ["die", "von", "der", "Po\u00eb\u00b7sie", "ein", "re\u00b7ger", "Mut", "kan", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "der mehr als Erde liebt, der durch die Zeit sich rei\u00dft.", "tokens": ["der", "mehr", "als", "Er\u00b7de", "liebt", ",", "der", "durch", "die", "Zeit", "sich", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch sie er schon ein Gott, weil er noch Mensch ist, hei\u00dft.", "tokens": ["Durch", "sie", "er", "schon", "ein", "Gott", ",", "weil", "er", "noch", "Mensch", "ist", ",", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der klagen Geister Witz, der ist hierzu vonn\u00f6ten;", "tokens": ["Der", "kla\u00b7gen", "Geis\u00b7ter", "Witz", ",", "der", "ist", "hier\u00b7zu", "von\u00b7n\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "VAFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der Flei\u00df kan nicht allein' uns machen zu Po\u00ebten.", "tokens": ["der", "Flei\u00df", "kan", "nicht", "al\u00b7lein'", "uns", "ma\u00b7chen", "zu", "Po\u00ebten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Hier mu\u00df das Beste tun die g\u00fcnstige Natur,", "tokens": ["Hier", "mu\u00df", "das", "Bes\u00b7te", "tun", "die", "g\u00fcns\u00b7ti\u00b7ge", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die unsre Seelen weist auf eine bessre Spur,", "tokens": ["die", "uns\u00b7re", "See\u00b7len", "weist", "auf", "ei\u00b7ne", "bess\u00b7re", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "als wo der P\u00f6bel kreucht. Und ihr seid auch von diesen,", "tokens": ["als", "wo", "der", "P\u00f6\u00b7bel", "kreucht", ".", "Und", "ihr", "seid", "auch", "von", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "VVFIN", "$.", "KON", "PPER", "VAFIN", "ADV", "APPR", "PDAT", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "so sich die Himmelsgunst sehr freundlich hat erwiesen,", "tokens": ["so", "sich", "die", "Him\u00b7mels\u00b7gunst", "sehr", "freund\u00b7lich", "hat", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und sie mit Kunst erf\u00fcllt! Auch ihr gl\u00fcht von der Glut,", "tokens": ["und", "sie", "mit", "Kunst", "er\u00b7f\u00fcllt", "!", "Auch", "ihr", "gl\u00fcht", "von", "der", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$.", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "die der Po\u00ebten Sinn' allzeit beflammen tut!", "tokens": ["die", "der", "Po\u00ebten", "Sinn'", "all\u00b7zeit", "be\u00b7flam\u00b7men", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wolan, so nehmt nun hin die immergr\u00fcne Krone,", "tokens": ["Wo\u00b7lan", ",", "so", "nehmt", "nun", "hin", "die", "im\u00b7mer\u00b7gr\u00fc\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "die euch, Herr ", "tokens": ["die", "euch", ",", "Herr"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "PPER", "$,", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "der klugen Schwestern Zunft! Nehmt hin und setzt sie auf", "tokens": ["der", "klu\u00b7gen", "Schwes\u00b7tern", "Zunft", "!", "Nehmt", "hin", "und", "setzt", "sie", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und schwingt euch freudig hin, wo der Po\u00ebten Hauf'", "tokens": ["und", "schwingt", "euch", "freu\u00b7dig", "hin", ",", "wo", "der", "Po\u00ebten", "Hauf'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "umb ihren Pindus tanzt. Die frische Hippocrene", "tokens": ["umb", "ih\u00b7ren", "Pin\u00b7dus", "tanzt", ".", "Die", "fri\u00b7sche", "Hip\u00b7po\u00b7cre\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "fleust reicher als zuvor, der breite Platz ist sch\u00f6ne", "tokens": ["fleust", "rei\u00b7cher", "als", "zu\u00b7vor", ",", "der", "brei\u00b7te", "Platz", "ist", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "von Neuem ganz begr\u00fcnt, auf dem ihr tichten solt.", "tokens": ["von", "Neu\u00b7em", "ganz", "be\u00b7gr\u00fcnt", ",", "auf", "dem", "ihr", "tich\u00b7ten", "solt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das ganze Himmelsfeld ist blau und pures Gold.", "tokens": ["Das", "gan\u00b7ze", "Him\u00b7mels\u00b7feld", "ist", "blau", "und", "pu\u00b7res", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Apollo gibt euch selbst von Hand zu Hand die Saiten,", "tokens": ["A\u00b7pol\u00b7lo", "gibt", "euch", "selbst", "von", "Hand", "zu", "Hand", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "die er auf seiner Harf' auch f\u00fchrete vor Zeiten.", "tokens": ["die", "er", "auf", "sei\u00b7ner", "Ha\u00b7rf'", "auch", "f\u00fch\u00b7re\u00b7te", "vor", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.27": {"text": "Der ganze Helicon ist still' und h\u00f6ret an,", "tokens": ["Der", "gan\u00b7ze", "He\u00b7li\u00b7con", "ist", "still'", "und", "h\u00f6\u00b7ret", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "was dieser neue Gast f\u00fcr sch\u00f6ne Lieder kan,", "tokens": ["was", "die\u00b7ser", "neu\u00b7e", "Gast", "f\u00fcr", "sch\u00f6\u00b7ne", "Lie\u00b7der", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "wie er so wol besteht. Ich, der ich weit zur\u00fccke", "tokens": ["wie", "er", "so", "wol", "be\u00b7steht", ".", "Ich", ",", "der", "ich", "weit", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$.", "PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "von eurer Hochheit bin, w\u00fcndsch' euch zu diesem Gl\u00fccke,", "tokens": ["von", "eu\u00b7rer", "Hoch\u00b7heit", "bin", ",", "w\u00fcnd\u00b7sch'", "euch", "zu", "die\u00b7sem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "was mancher w\u00fcndschet ihm. Tut, was ihr vor getan,", "tokens": ["was", "man\u00b7cher", "w\u00fcnd\u00b7schet", "ihm", ".", "Tut", ",", "was", "ihr", "vor", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "PPER", "$.", "NN", "$,", "PWS", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "da\u00df man auch seh' an euch, was ein Po\u00ebte kan!", "tokens": ["da\u00df", "man", "auch", "seh'", "an", "euch", ",", "was", "ein", "Po\u00ebte", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "PPER", "$,", "PRELS", "ART", "NN", "VMFIN", "$."], "meter": "+-+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Von Leipzig \u00fcbersendets", "tokens": ["Von", "Leip\u00b7zig", "\u00fc\u00b7ber\u00b7sen\u00b7dets"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}