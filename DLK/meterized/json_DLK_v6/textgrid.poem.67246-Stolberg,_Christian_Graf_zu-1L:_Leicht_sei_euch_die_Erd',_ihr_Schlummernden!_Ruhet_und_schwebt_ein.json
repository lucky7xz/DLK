{"textgrid.poem.67246": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leicht sei euch die Erd', ihr Schlummernden! Ruhet und schwebt ein", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leicht sei euch die Erd', ihr Schlummernden! Ruhet und schwebt ein", "tokens": ["Leicht", "sei", "euch", "die", "Erd'", ",", "ihr", "Schlum\u00b7mern\u00b7den", "!", "Ru\u00b7het", "und", "schwebt", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN", "ART"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Traum um das schattige Grab, sei es ein freundlicher Traum,", "tokens": ["Traum", "um", "das", "schat\u00b7ti\u00b7ge", "Grab", ",", "sei", "es", "ein", "freund\u00b7li\u00b7cher", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Der euch K\u00fcsse der Lieb' im Morgenstrahle des Lenzes", "tokens": ["Der", "euch", "K\u00fcs\u00b7se", "der", "Lieb'", "im", "Mor\u00b7gen\u00b7strah\u00b7le", "des", "Len\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bild', und die Kinder im Nest und das erwachende Ey,", "tokens": ["Bild'", ",", "und", "die", "Kin\u00b7der", "im", "Nest", "und", "das", "er\u00b7wa\u00b7chen\u00b7de", "Ey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "APPRART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ach, und meine Hand, aus der ihr w\u00e4hlend die Gerste", "tokens": ["Ach", ",", "und", "mei\u00b7ne", "Hand", ",", "aus", "der", "ihr", "w\u00e4h\u00b7lend", "die", "Gers\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Laset, inde\u00df ich den Hals kos'te, den schimmernden Hals.", "tokens": ["La\u00b7set", ",", "in\u00b7de\u00df", "ich", "den", "Hals", "kos'\u00b7te", ",", "den", "schim\u00b7mern\u00b7den", "Hals", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Blutig war der Mord, doch schreckenlos; denn des Todes", "tokens": ["Blu\u00b7tig", "war", "der", "Mord", ",", "doch", "schre\u00b7cken\u00b7los", ";", "denn", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Bruder zuckte den Pfeil, schnell und erbarmend, der Schlaf.", "tokens": ["Bru\u00b7der", "zuck\u00b7te", "den", "Pfeil", ",", "schnell", "und", "er\u00b7bar\u00b7mend", ",", "der", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Gl\u00fccklich seyd ihr V\u00f6gel der Unschuld! \u2013 Thr\u00e4nen der Sehnsucht", "tokens": ["Gl\u00fcck\u00b7lich", "seyd", "ihr", "V\u00f6\u00b7gel", "der", "Un\u00b7schuld", "!", "\u2013", "Thr\u00e4\u00b7nen", "der", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$.", "$(", "NN", "ART", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Weint mein Auge! \u2013 der Tod trennte die Liebenden nicht.", "tokens": ["Weint", "mein", "Au\u00b7ge", "!", "\u2013", "der", "Tod", "trenn\u00b7te", "die", "Lie\u00b7ben\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Einst umschwebt ihr vielleicht die himmlischen Lauben der Meinen,", "tokens": ["Einst", "um\u00b7schwebt", "ihr", "viel\u00b7leicht", "die", "himm\u00b7li\u00b7schen", "Lau\u00b7ben", "der", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Leset des strahlenden Halms Saat mir auch dann aus der Hand.", "tokens": ["Le\u00b7set", "des", "strah\u00b7len\u00b7den", "Halms", "Saat", "mir", "auch", "dann", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}