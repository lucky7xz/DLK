{"textgrid.poem.33996": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6nheitsprobe", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie l\u00e4\u00dft die echte Sch\u00f6nheit sich erproben?", "tokens": ["Wie", "l\u00e4\u00dft", "die", "ech\u00b7te", "Sch\u00f6n\u00b7heit", "sich", "er\u00b7pro\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl einzig an dem selbstbewu\u00dften Frieden,", "tokens": ["Wohl", "ein\u00b7zig", "an", "dem", "selbst\u00b7be\u00b7wu\u00df\u00b7ten", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sie umflie\u00dft, weil sie sich wie geschieden", "tokens": ["Der", "sie", "um\u00b7flie\u00dft", ",", "weil", "sie", "sich", "wie", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "KOKOM", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von allen K\u00e4mpfen f\u00fchlt, die sie umtoben.", "tokens": ["Von", "al\u00b7len", "K\u00e4mp\u00b7fen", "f\u00fchlt", ",", "die", "sie", "um\u00b7to\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr steht er, wie ein Sternenkranz von oben,", "tokens": ["Ihr", "steht", "er", ",", "wie", "ein", "Ster\u00b7nen\u00b7kranz", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den, da sie ganz den innern Zwist gemieden,", "tokens": ["Den", ",", "da", "sie", "ganz", "den", "in\u00b7nern", "Zwist", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alles Uebrige verwirrt, hienieden,", "tokens": ["Der", "al\u00b7les", "Ueb\u00b7ri\u00b7ge", "ver\u00b7wirrt", ",", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ew'ge Mutter selbst f\u00fcr sie gewoben.", "tokens": ["Die", "ew'\u00b7ge", "Mut\u00b7ter", "selbst", "f\u00fcr", "sie", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wehe ihren Afterschwestern allen,", "tokens": ["Doch", "we\u00b7he", "ih\u00b7ren", "Af\u00b7ter\u00b7schwes\u00b7tern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr nicht gleichen und sich selber kr\u00f6nen,", "tokens": ["Die", "ihr", "nicht", "glei\u00b7chen", "und", "sich", "sel\u00b7ber", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "KON", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil Faun und Satyr ihnen Beifall zollen!", "tokens": ["Weil", "Faun", "und", "Sa\u00b7tyr", "ih\u00b7nen", "Bei\u00b7fall", "zol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie k\u00f6nnen nur, wenn sie sich nicht gefallen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nur", ",", "wenn", "sie", "sich", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihrem halben Dasein uns vers\u00f6hnen,", "tokens": ["Mit", "ih\u00b7rem", "hal\u00b7ben", "Da\u00b7sein", "uns", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur, wenn sie zeigen, da\u00df sie weiter wollen.", "tokens": ["Nur", ",", "wenn", "sie", "zei\u00b7gen", ",", "da\u00df", "sie", "wei\u00b7ter", "wol\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}