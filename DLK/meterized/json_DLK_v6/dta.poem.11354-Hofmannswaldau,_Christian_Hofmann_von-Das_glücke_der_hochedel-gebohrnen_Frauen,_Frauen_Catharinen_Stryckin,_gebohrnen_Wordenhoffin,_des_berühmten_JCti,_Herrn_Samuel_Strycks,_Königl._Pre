{"dta.poem.11354": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das gl\u00fccke der hochedel-gebohrnen  \n Frauen, Frauen Catharinen Stryckin,  \n gebohrnen Wordenhoffin, des ber\u00fchmten  \n  JCti,  Herrn Samuel Strycks, K\u00f6nigl.  \n Preu\u00dfischen geheimen raths, Frauen  \n gemahlin, bey ihrem  anno  1707 erfolgten  \n absterben, vorgestellt im nahmen des  \n Herrn  Profess. Ludovici  s\u00e4m\u0303tl.  \n tisch-compagnie.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder mensch strebt nach gel\u00fcckt,", "tokens": ["Ein", "je\u00b7der", "mensch", "strebt", "nach", "ge\u00b7l\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist in die natur gesetzt.", "tokens": ["Es", "ist", "in", "die", "na\u00b7tur", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl diesem, den des H\u00f6chsten blicke,", "tokens": ["Wohl", "die\u00b7sem", ",", "den", "des", "H\u00f6chs\u00b7ten", "bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der holden gnade werth gesch\u00e4tzt!", "tokens": ["Der", "hol\u00b7den", "gna\u00b7de", "werth", "ge\u00b7sch\u00e4tzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie vieler w\u00fcnsche sind hier nichtig!", "tokens": ["Wie", "vie\u00b7ler", "w\u00fcn\u00b7sche", "sind", "hier", "nich\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das gl\u00fccke will den r\u00fccken drehn.", "tokens": ["Das", "gl\u00fc\u00b7cke", "will", "den", "r\u00fc\u00b7cken", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es sind nicht aller augen t\u00fcchtig,", "tokens": ["Es", "sind", "nicht", "al\u00b7ler", "au\u00b7gen", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sonnen-strahlen anzusehn.", "tokens": ["Die", "son\u00b7nen\u00b7strah\u00b7len", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott hat auch viel verborgne wege:", "tokens": ["Gott", "hat", "auch", "viel", "ver\u00b7borg\u00b7ne", "we\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den f\u00fchrt er auf der rosen-bahn,", "tokens": ["Den", "f\u00fchrt", "er", "auf", "der", "ro\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den andern durch viel w\u00fcste stege,", "tokens": ["Den", "an\u00b7dern", "durch", "viel", "w\u00fcs\u00b7te", "ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In das verlangte Canaan.", "tokens": ["In", "das", "ver\u00b7lang\u00b7te", "Ca\u00b7naan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Drum mu\u00df hier die vernunfft nicht kl\u00fcgeln,", "tokens": ["Drum", "mu\u00df", "hier", "die", "ver\u00b7nunfft", "nicht", "kl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der vorwitz hemme seinen lauff!", "tokens": ["Der", "vor\u00b7witz", "hem\u00b7me", "sei\u00b7nen", "lauff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das, was der himmel will verriegeln,", "tokens": ["Das", ",", "was", "der", "him\u00b7mel", "will", "ver\u00b7rie\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schliest keines menschen klugheit auf.", "tokens": ["Schliest", "kei\u00b7nes", "men\u00b7schen", "klug\u00b7heit", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dich, nunmehr seelige matrone!", "tokens": ["Dich", ",", "nun\u00b7mehr", "see\u00b7li\u00b7ge", "mat\u00b7ro\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er die sanffte bahn gef\u00fchrt.", "tokens": ["Hat", "er", "die", "sanff\u00b7te", "bahn", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat des gl\u00fcckes sch\u00f6ne crone", "tokens": ["Es", "hat", "des", "gl\u00fc\u00b7ckes", "sch\u00f6\u00b7ne", "cro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein gantzes leben ausgeziert.", "tokens": ["Dein", "gant\u00b7zes", "le\u00b7ben", "aus\u00b7ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du warst ein kind von edlen eltern.", "tokens": ["Du", "warst", "ein", "kind", "von", "ed\u00b7len", "el\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das gl\u00fccke wolte dich erh\u00f6hn:", "tokens": ["Das", "gl\u00fc\u00b7cke", "wol\u00b7te", "dich", "er\u00b7h\u00f6hn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "De\u00dfwegen musten deine keltern", "tokens": ["De\u00df\u00b7we\u00b7gen", "mus\u00b7ten", "dei\u00b7ne", "kel\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll most, und sonder e\u00dfig stehn.", "tokens": ["Voll", "most", ",", "und", "son\u00b7der", "e\u00b7\u00dfig", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du gleichtest den gepflantzten schnaten,", "tokens": ["Du", "gleich\u00b7test", "den", "ge\u00b7pflantz\u00b7ten", "schna\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wegen sonne, flei\u00df und wind,", "tokens": ["Die", "we\u00b7gen", "son\u00b7ne", ",", "flei\u00df", "und", "wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch begiessen wohl gerathen,", "tokens": ["Und", "durch", "be\u00b7gies\u00b7sen", "wohl", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie in gutem boden sind.", "tokens": ["Wenn", "sie", "in", "gu\u00b7tem", "bo\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du liessest dich vern\u00fcnfftig leiten,", "tokens": ["Du", "lies\u00b7sest", "dich", "ver\u00b7n\u00fcnff\u00b7tig", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die auferziehung schlug wohl an,", "tokens": ["Die", "auf\u00b7er\u00b7zie\u00b7hung", "schlug", "wohl", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So da\u00df man schon bey diesen zeiten,", "tokens": ["So", "da\u00df", "man", "schon", "bey", "die\u00b7sen", "zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von deinem gl\u00fccke sagen kan.", "tokens": ["Von", "dei\u00b7nem", "gl\u00fc\u00b7cke", "sa\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann aber wurd es mehr vollkommen,", "tokens": ["Dann", "a\u00b7ber", "wurd", "es", "mehr", "voll\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als dich der grosse Stryck umfieng,", "tokens": ["Als", "dich", "der", "gros\u00b7se", "Stryck", "um\u00b7fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Den du zum eh-gemahl genommen,", "tokens": ["Den", "du", "zum", "eh\u00b7ge\u00b7mahl", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir mit lieb entgegen gieng.", "tokens": ["Der", "dir", "mit", "lieb", "ent\u00b7ge\u00b7gen", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier hat die edelste vergn\u00fcgung", "tokens": ["Hier", "hat", "die", "e\u00b7dels\u00b7te", "ver\u00b7gn\u00fc\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir ihre sch\u00e4tze kund gemacht,", "tokens": ["Dir", "ih\u00b7re", "sch\u00e4t\u00b7ze", "kund", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch des H\u00f6chsten weise f\u00fcgung", "tokens": ["Und", "durch", "des", "H\u00f6chs\u00b7ten", "wei\u00b7se", "f\u00fc\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dich zu der wahren ruh gebracht.", "tokens": ["Dich", "zu", "der", "wah\u00b7ren", "ruh", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es herrschte zwar von zarter jugend", "tokens": ["Es", "herrschte", "zwar", "von", "zar\u00b7ter", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die fr\u00f6mmigkeit in deiner brust;", "tokens": ["Die", "fr\u00f6m\u00b7mig\u00b7keit", "in", "dei\u00b7ner", "brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wem aber ist die macht der tugend", "tokens": ["Wem", "a\u00b7ber", "ist", "die", "macht", "der", "tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VAFIN", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem wachsthum unbewust?", "tokens": ["In", "ih\u00b7rem", "wach\u00b7sthum", "un\u00b7be\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Gottesfurcht hat ihre grade;", "tokens": ["Die", "Got\u00b7tes\u00b7furcht", "hat", "ih\u00b7re", "gra\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein ceder-reis wird gleich ein baum:", "tokens": ["Kein", "ce\u00b7der\u00b7reis", "wird", "gleich", "ein", "baum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der mensch nimmt zu durch flei\u00df und gnade,", "tokens": ["Der", "mensch", "nimmt", "zu", "durch", "flei\u00df", "und", "gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gott selber g\u00f6nnt ihm zeit und raum.", "tokens": ["Gott", "sel\u00b7ber", "g\u00f6nnt", "ihm", "zeit", "und", "raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Di\u00df wachsthum war dein liebstes gl\u00fccke,", "tokens": ["Di\u00df", "wach\u00b7sthum", "war", "dein", "liebs\u00b7tes", "gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das himmelreich dein eintzig ziel:", "tokens": ["Das", "him\u00b7mel\u00b7reich", "dein", "eint\u00b7zig", "ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wiewohl inde\u00df durch sein geschicke", "tokens": ["Wie\u00b7wohl", "in\u00b7de\u00df", "durch", "sein", "ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir auch der erden gut zufiel.", "tokens": ["Dir", "auch", "der", "er\u00b7den", "gut", "zu\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier war kein mangel zu erstatten,", "tokens": ["Hier", "war", "kein", "man\u00b7gel", "zu", "er\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergn\u00fcgung f\u00fcllte dein gemach:", "tokens": ["Ver\u00b7gn\u