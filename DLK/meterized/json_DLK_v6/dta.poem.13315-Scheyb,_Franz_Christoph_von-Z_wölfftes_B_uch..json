{"dta.poem.13315": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egesezt: ich liesse mich als euer Haupt begr\u00fcssen,", "tokens": ["\u201e", "ge\u00b7sezt", ":", "ich", "lies\u00b7se", "mich", "als", "eu\u00b7er", "Haupt", "be\u00b7gr\u00fcs\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PPER", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas w\u00fcrdet, Tugenden! ihr auf den Spruch beschliessen?", "tokens": ["\u201e", "was", "w\u00fcr\u00b7det", ",", "Tu\u00b7gen\u00b7den", "!", "ihr", "auf", "den", "Spruch", "be\u00b7schlies\u00b7sen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$,", "NN", "$.", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie Folge weist sich selbst; ich blieb am End allein,", "tokens": ["\u201e", "die", "Fol\u00b7ge", "weist", "sich", "selbst", ";", "ich", "blieb", "am", "End", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund nichts von meinem Amt w\u00e4r euerm Rath gemein.", "tokens": ["\u201e", "und", "nichts", "von", "mei\u00b7nem", "Amt", "w\u00e4r", "eu\u00b7erm", "Rath", "ge\u00b7mein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIS", "APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "395\u201dWas n\u00fczte meine Pflicht? die Wahrheit w\u00e4r verlassen;", "tokens": ["\"", "Was", "n\u00fcz\u00b7te", "mei\u00b7ne", "Pflicht", "?", "die", "Wahr\u00b7heit", "w\u00e4r", "ver\u00b7las\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewo keine Tugend ist, da pflegt man sie zu hassen.", "tokens": ["\u201e", "wo", "kei\u00b7ne", "Tu\u00b7gend", "ist", ",", "da", "pflegt", "man", "sie", "zu", "has\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso wend\u2019 ich mich zum Schlu\u00df und Ausspruch dieses Streits,", "tokens": ["\u201e", "so", "wend'", "ich", "mich", "zum", "Schlu\u00df", "und", "Aus\u00b7spruch", "die\u00b7ses", "Streits", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "PRF", "APPRART", "NN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ejhr kennt desselben Sinn und Innbegriff bereits.", "tokens": ["\u201e", "jhr", "kennt", "des\u00b7sel\u00b7ben", "Sinn", "und", "Inn\u00b7be\u00b7griff", "be\u00b7reits", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PDAT", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}