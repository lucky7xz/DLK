{"dta.poem.19246": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus einem Kreise kanst du nie ein Viereck machen,", "tokens": ["Aus", "ei\u00b7nem", "Krei\u00b7se", "kanst", "du", "nie", "ein", "Vier\u00b7eck", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht aus Unendlichem die Endlichkeit der Sachen.", "tokens": ["Nicht", "aus", "Un\u00b7end\u00b7li\u00b7chem", "die", "End\u00b7lich\u00b7keit", "der", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wol im Kreise kanst du dir ein Viereck denken,", "tokens": ["Doch", "wol", "im", "Krei\u00b7se", "kanst", "du", "dir", "ein", "Vier\u00b7eck", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Viereck einen Kreis, und eins ins andre schrenken.", "tokens": ["Im", "Vier\u00b7eck", "ei\u00b7nen", "Kreis", ",", "und", "eins", "ins", "and\u00b7re", "schren\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KON", "PIS", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So von Unendlichem ist Endliches umfangen,", "tokens": ["So", "von", "Un\u00b7end\u00b7li\u00b7chem", "ist", "End\u00b7li\u00b7ches", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst im Endlichen Unendlichs aufgegangen.", "tokens": ["Und", "selbst", "im", "End\u00b7li\u00b7chen", "Un\u00b7end\u00b7lichs", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zum Viereck ist der Kreis erstarrt, wenn seiner Speichen", "tokens": ["Zum", "Vier\u00b7eck", "ist", "der", "Kreis", "er\u00b7starrt", ",", "wenn", "sei\u00b7ner", "Spei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier stille stehn und sich die Hand durch Sennen reichen.", "tokens": ["Vier", "stil\u00b7le", "stehn", "und", "sich", "die", "Hand", "durch", "Sen\u00b7nen", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "VVINF", "KON", "PRF", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Viereck wird zum Kreis sich runden, wenn sich drehn", "tokens": ["Das", "Vier\u00b7eck", "wird", "zum", "Kreis", "sich", "run\u00b7den", ",", "wenn", "sich", "drehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PRF", "ADJA", "$,", "KOUS", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Speichen, und im Schwung die Sennen rasch vergehn.", "tokens": ["Die", "Spei\u00b7chen", ",", "und", "im", "Schwung", "die", "Sen\u00b7nen", "rasch", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das starrste wandelt sich, in Schwung gesetzt, zum Rade;", "tokens": ["Das", "starrs\u00b7te", "wan\u00b7delt", "sich", ",", "in", "Schwung", "ge\u00b7setzt", ",", "zum", "Ra\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "$,", "APPR", "NN", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebens Kreis ist rund, und Tod ist alles Grade.", "tokens": ["Des", "Le\u00b7bens", "Kreis", "ist", "rund", ",", "und", "Tod", "ist", "al\u00b7les", "Gra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,", "KON", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}