{"textgrid.poem.47414": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ Der Ost gerieth in Streit mit der Natur]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ost gerieth in Streit mit der Natur;", "tokens": ["Der", "Ost", "ge\u00b7rieth", "in", "Streit", "mit", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er wollte nicht mehr auf der Rosenflur,", "tokens": ["Er", "woll\u00b7te", "nicht", "mehr", "auf", "der", "Ro\u00b7sen\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wollte weh'n auf einer sch\u00f6neren,", "tokens": ["Er", "woll\u00b7te", "weh'n", "auf", "ei\u00b7ner", "sch\u00f6\u00b7ne\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er wollte weh'n auf deiner Wange nur.", "tokens": ["Er", "woll\u00b7te", "weh'n", "auf", "dei\u00b7ner", "Wan\u00b7ge", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}