{"textgrid.poem.52075": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil niemand vor gewi\u00df kan seinen Vater kennen:", "tokens": ["Weil", "nie\u00b7mand", "vor", "ge\u00b7wi\u00df", "kan", "sei\u00b7nen", "Va\u00b7ter", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was plaget ihr das Mensch: Die Kinder, die sind dar", "tokens": ["Was", "pla\u00b7get", "ihr", "das", "Mensch", ":", "Die", "Kin\u00b7der", ",", "die", "sind", "dar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ohn Vaters Nein, doch nehmt hier des Entdeckten wahr:", "tokens": ["Ohn", "Va\u00b7ters", "Nein", ",", "doch", "nehmt", "hier", "des", "Ent\u00b7deck\u00b7ten", "wahr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKANT", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann den gewi\u00dfen wird sie euch gar schwerlich nennen.", "tokens": ["Dann", "den", "ge\u00b7wi\u00b7\u00dfen", "wird", "sie", "euch", "gar", "schwer\u00b7lich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}