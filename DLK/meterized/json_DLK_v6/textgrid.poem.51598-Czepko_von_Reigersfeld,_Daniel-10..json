{"textgrid.poem.51598": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gr\u00e4slein ist ein Buch, suchst du es aufzuschliessen,", "tokens": ["Das", "Gr\u00e4s\u00b7lein", "ist", "ein", "Buch", ",", "suchst", "du", "es", "auf\u00b7zu\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kanst die Sch\u00f6pffung draus und alle Weisheit wissen.", "tokens": ["Du", "kanst", "die", "Sch\u00f6pf\u00b7fung", "draus", "und", "al\u00b7le", "Weis\u00b7heit", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PAV", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}