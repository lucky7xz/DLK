{"textgrid.poem.42386": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herbst verflog. Der erste Nachtfrost kerbt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbst verflog. Der erste Nachtfrost kerbt", "tokens": ["Der", "Herbst", "ver\u00b7flog", ".", "Der", "ers\u00b7te", "Nacht\u00b7frost", "kerbt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Felder, drauf die Sp\u00e4tfrucht abger\u00e4umt,", "tokens": ["Die", "Fel\u00b7der", ",", "drauf", "die", "Sp\u00e4t\u00b7frucht", "ab\u00b7ge\u00b7r\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Waldesboden ist kahl und entf\u00e4rbt,", "tokens": ["Der", "Wal\u00b7des\u00b7bo\u00b7den", "ist", "kahl", "und", "ent\u00b7f\u00e4rbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Wegrand rissig und mit Reif bes\u00e4umt.", "tokens": ["Der", "Weg\u00b7rand", "ris\u00b7sig", "und", "mit", "Reif", "be\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In klarer Luft fliegt hoch am Horizont", "tokens": ["In", "kla\u00b7rer", "Luft", "fliegt", "hoch", "am", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kr\u00e4he und entspannt die Fl\u00fcgel weit,", "tokens": ["Die", "Kr\u00e4\u00b7he", "und", "ent\u00b7spannt", "die", "Fl\u00fc\u00b7gel", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Welt ist nur von ferne noch besonnt,", "tokens": ["Die", "Welt", "ist", "nur", "von", "fer\u00b7ne", "noch", "be\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man sp\u00fcrt: es w\u00e4hrt nicht lange, bis es schneit.", "tokens": ["Man", "sp\u00fcrt", ":", "es", "w\u00e4hrt", "nicht", "lan\u00b7ge", ",", "bis", "es", "schneit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In einem Meer von grauen Nebeln rinnt", "tokens": ["In", "ei\u00b7nem", "Meer", "von", "grau\u00b7en", "Ne\u00b7beln", "rinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag ins Dunkel und lang wird die Nacht;", "tokens": ["Der", "Tag", "ins", "Dun\u00b7kel", "und", "lang", "wird", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es scheint, das Leben feiert oder sinnt,", "tokens": ["Es", "scheint", ",", "das", "Le\u00b7ben", "fei\u00b7ert", "o\u00b7der", "sinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine alte Mutter, die vollbracht", "tokens": ["Wie", "ei\u00b7ne", "al\u00b7te", "Mut\u00b7ter", ",", "die", "voll\u00b7bracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Tagwerk und nur noch der Kinder denkt,", "tokens": ["Ihr", "Tag\u00b7werk", "und", "nur", "noch", "der", "Kin\u00b7der", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und einzig nur von ihrem Gl\u00fccke zehrt,", "tokens": ["Und", "ein\u00b7zig", "nur", "von", "ih\u00b7rem", "Gl\u00fc\u00b7cke", "zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In ihre fernste Zukunft sich versenkt,", "tokens": ["In", "ih\u00b7re", "ferns\u00b7te", "Zu\u00b7kunft", "sich", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und die doch einsam ist und abgekehrt.", "tokens": ["Und", "die", "doch", "ein\u00b7sam", "ist", "und", "ab\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}