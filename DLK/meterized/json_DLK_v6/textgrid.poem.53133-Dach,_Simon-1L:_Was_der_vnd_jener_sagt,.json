{"textgrid.poem.53133": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was der vnd jener sagt,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was der vnd jener sagt,", "tokens": ["Was", "der", "vnd", "je\u00b7ner", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es ist auff Gott gewagt,", "tokens": ["Es", "ist", "auff", "Gott", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben vns genommen,", "tokens": ["Wir", "ha\u00b7ben", "vns", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versichre dich, mein Kind,", "tokens": ["Ver\u00b7sich\u00b7re", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ohn Himmels-Stifftung sind", "tokens": ["Ohn", "Him\u00b7mels\u00b7Stiff\u00b7tung", "sind"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wir nicht zusammen kommen.", "tokens": ["Wir", "nicht", "zu\u00b7sam\u00b7men", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein' Eh hat doch gekunt", "tokens": ["Kein'", "Eh", "hat", "doch", "ge\u00b7kunt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der b\u00f6sten Leute Mund", "tokens": ["Der", "b\u00f6s\u00b7ten", "Leu\u00b7te", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd jhren F\u00fcrwitz meiden,", "tokens": ["Vnd", "jhren", "F\u00fcr\u00b7witz", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nur fest auff Gott getrawt!", "tokens": ["Nur", "fest", "auff", "Gott", "ge\u00b7trawt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer an dem Wege bawt,", "tokens": ["Wer", "an", "dem", "We\u00b7ge", "bawt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df aller Vrtheil leiden.", "tokens": ["Mu\u00df", "al\u00b7ler", "Vrtheil", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Indem die arge Welt", "tokens": ["In\u00b7dem", "die", "ar\u00b7ge", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur viel auff Falschheit h\u00e4lt,", "tokens": ["Nur", "viel", "auff", "Falschheit", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was bessers ist zu finden,", "tokens": ["Was", "bes\u00b7sers", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als da\u00df zwey Hertzen sich", "tokens": ["Als", "da\u00df", "zwey", "Hert\u00b7zen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "CARD", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gew\u00fcnscht vnd jnniglich", "tokens": ["Ge\u00b7w\u00fcnscht", "vnd", "jn\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auff Gl\u00fcck vnd Fall verbinden?", "tokens": ["Auff", "Gl\u00fcck", "vnd", "Fall", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Giebt Gott den dritten Mann,", "tokens": ["Giebt", "Gott", "den", "drit\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie wol sind sie daran!", "tokens": ["Wie", "wol", "sind", "sie", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Thun mu\u00df nur gelingen:", "tokens": ["Ihr", "Thun", "mu\u00df", "nur", "ge\u00b7lin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Abgunst vnd kein Leid,", "tokens": ["Kein", "Ab\u00b7gunst", "vnd", "kein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja keiner H\u00f6llen Neid", "tokens": ["Ja", "kei\u00b7ner", "H\u00f6l\u00b7len", "Neid"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird solches Band durchdringen.", "tokens": ["Wird", "sol\u00b7ches", "Band", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn Tr\u00fcbsal mich verletzt", "tokens": ["Wenn", "Tr\u00fcb\u00b7sal", "mich", "ver\u00b7letzt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd Kranckheit an mich setzt,", "tokens": ["Vnd", "Kran\u00b7ck\u00b7heit", "an", "mich", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ohn Creutz wird niemand leben,", "tokens": ["Ohn", "Creutz", "wird", "nie\u00b7mand", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo kehr' ich dann mich hin?", "tokens": ["Wo", "kehr'", "ich", "dann", "mich", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wer wird meinen Sinn", "tokens": ["Vnd", "wer", "wird", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Trawrigheit entheben?", "tokens": ["Der", "Traw\u00b7rig\u00b7heit", "ent\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ob jemand mich ersucht,", "tokens": ["Ob", "je\u00b7mand", "mich", "er\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es ist wie auff der Flucht,", "tokens": ["Es", "ist", "wie", "auff", "der", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man gehet seiner Wege;", "tokens": ["Man", "ge\u00b7het", "sei\u00b7ner", "We\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein jeder liebt sein Hau\u00df,", "tokens": ["Ein", "je\u00b7der", "liebt", "sein", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Ehgeno\u00df h\u00e4lt aus", "tokens": ["Der", "Eh\u00b7ge\u00b7no\u00df", "h\u00e4lt", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit wahrer Pflicht vnd Pflege.", "tokens": ["Mit", "wah\u00b7rer", "Pflicht", "vnd", "Pfle\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir wollen vns in Trew", "tokens": ["Wir", "wol\u00b7len", "vns", "in", "Trew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd Gottesfurcht dabey,", "tokens": ["Vnd", "Got\u00b7tes\u00b7furcht", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Kind, zusammen halten,", "tokens": ["Mein", "Kind", ",", "zu\u00b7sam\u00b7men", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was gilt es, Gn\u00fcg' vnd Rhue", "tokens": ["Was", "gilt", "es", ",", "Gn\u00fcg'", "vnd", "Rhue"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "KON", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd Fruchtbarkeit dazu", "tokens": ["Vnd", "Frucht\u00b7bar\u00b7keit", "da\u00b7zu"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird \u00fcber vns noch walten!", "tokens": ["Wird", "\u00fc\u00b7ber", "vns", "noch", "wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "K\u00f6mpt aber M\u00fch vnd Pein,", "tokens": ["K\u00f6mpt", "a\u00b7ber", "M\u00fch", "vnd", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird vnser Sonnen Schein", "tokens": ["Wird", "vn\u00b7ser", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wetter vieler Plagen;", "tokens": ["Ein", "Wet\u00b7ter", "vie\u00b7ler", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Gott es schickt, wol her!", "tokens": ["Wie", "Gott", "es", "schickt", ",", "wol", "her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Creutz ist nicht so schwer,", "tokens": ["Das", "Creutz", "ist", "nicht", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn jhrer zween es tragen.", "tokens": ["Wenn", "jhrer", "zween", "es", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Mit diesem Vorsatz auff!", "tokens": ["Mit", "die\u00b7sem", "Vor\u00b7satz", "auff", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weil noch der Jugend Lauff", "tokens": ["Weil", "noch", "der", "Ju\u00b7gend", "Lauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vns Frewd' vnd Freyheit g\u00f6nnet,", "tokens": ["Vns", "Fre\u00b7wd'", "vnd", "Frey\u00b7heit", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weg Vnmuth, Gram vnd Wust!", "tokens": ["Weg", "Vn\u00b7muth", ",", "Gram", "vnd", "Wust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr G\u00e4st', empfindet Lust,", "tokens": ["Ihr", "G\u00e4st'", ",", "emp\u00b7fin\u00b7det", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vbt Kurtzweil, wie jhr k\u00f6nnet!", "tokens": ["Vbt", "Kurt\u00b7zweil", ",", "wie", "jhr", "k\u00f6n\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wir wollen dieses mahl", "tokens": ["Wir", "wol\u00b7len", "die\u00b7ses", "mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den, der von Angst vnd Qual", "tokens": ["Den", ",", "der", "von", "Angst", "vnd", "Qual"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verdrie\u00dflich schw\u00e4tzt, nicht h\u00f6ren.", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", "schw\u00e4tzt", ",", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tantz, Lieb' vnd Spiel heran,", "tokens": ["Tantz", ",", "Lieb'", "vnd", "Spiel", "he\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd was ergetzen kan!", "tokens": ["Vnd", "was", "er\u00b7get\u00b7zen", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geschieht es doch in Ehren.", "tokens": ["Ge\u00b7schieht", "es", "doch", "in", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}