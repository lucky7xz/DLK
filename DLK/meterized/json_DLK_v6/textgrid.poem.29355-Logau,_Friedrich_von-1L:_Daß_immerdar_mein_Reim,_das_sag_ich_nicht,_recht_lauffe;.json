{"textgrid.poem.29355": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df immerdar mein Reim, das sag ich nicht, recht lauffe;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df immerdar mein Reim, das sag ich nicht, recht lauffe;", "tokens": ["Da\u00df", "im\u00b7mer\u00b7dar", "mein", "Reim", ",", "das", "sag", "ich", "nicht", ",", "recht", "lauf\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schliesse mich nicht gantz in Schrancken, die der Hauffe", "tokens": ["Ich", "schlies\u00b7se", "mich", "nicht", "gantz", "in", "Schran\u00b7cken", ",", "die", "der", "Hauf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Reimen-K\u00fcnstler baut. Das lang f\u00fcr kurtz, f\u00fcr lang", "tokens": ["Der", "Rei\u00b7men\u00b7K\u00fcnst\u00b7ler", "baut", ".", "Das", "lang", "f\u00fcr", "kurtz", ",", "f\u00fcr", "lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJD", "APPR", "ADJD", "$,", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das kurtz, das glaub ich wol, zu Zeiten schlich und sprang;", "tokens": ["Das", "kurtz", ",", "das", "glaub", "ich", "wol", ",", "zu", "Zei\u00b7ten", "schlich", "und", "sprang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu Zeiten satzt ich was im Kummer, was in Eile;", "tokens": ["Zu", "Zei\u00b7ten", "satzt", "ich", "was", "im", "Kum\u00b7mer", ",", "was", "in", "Ei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRELS", "APPRART", "NN", "$,", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu Zeiten hatt ich kurtz-, zu Zeiten lange-weile.", "tokens": ["Zu", "Zei\u00b7ten", "hatt", "ich", "kurtz", ",", "zu", "Zei\u00b7ten", "lan\u00b7ge\u00b7wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "TRUNC", "$,", "APPR", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann nur der Sinn recht f\u00e4llt, wo nur die Meinung recht,", "tokens": ["Wann", "nur", "der", "Sinn", "recht", "f\u00e4llt", ",", "wo", "nur", "die", "Mei\u00b7nung", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So sey der Sinn der Herr, so sey der Reim der Knecht.", "tokens": ["So", "sey", "der", "Sinn", "der", "Herr", ",", "so", "sey", "der", "Reim", "der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}