{"textgrid.poem.35631": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun schreite herab, titanischer Bursche,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun schreite herab, titanischer Bursche,", "tokens": ["Nun", "schrei\u00b7te", "her\u00b7ab", ",", "ti\u00b7ta\u00b7ni\u00b7scher", "Bur\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wecke die vielgeliebte Schlummernde dir!", "tokens": ["Und", "we\u00b7cke", "die", "viel\u00b7ge\u00b7lieb\u00b7te", "Schlum\u00b7mern\u00b7de", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schreite herab, und umg\u00fcrte", "tokens": ["Schrei\u00b7te", "her\u00b7ab", ",", "und", "um\u00b7g\u00fcr\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit zartlichten Bl\u00fcten das tr\u00e4umende Haupt.", "tokens": ["Mit", "zart\u00b7lich\u00b7ten", "Bl\u00fc\u00b7ten", "das", "tr\u00e4u\u00b7men\u00b7de", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Entz\u00fcnde den bangenden Himmel mit lodernder Fackel,", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "den", "ban\u00b7gen\u00b7den", "Him\u00b7mel", "mit", "lo\u00b7dern\u00b7der", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Da\u00df die erblassenden Sterne tanzend ert\u00f6nen", "tokens": ["Da\u00df", "die", "er\u00b7blas\u00b7sen\u00b7den", "Ster\u00b7ne", "tan\u00b7zend", "er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und die fliegenden Schleier der Nacht", "tokens": ["Und", "die", "flie\u00b7gen\u00b7den", "Schlei\u00b7er", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Aufflammend vergehen,", "tokens": ["Auf\u00b7flam\u00b7mend", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Da\u00df die zyklopischen Wolken zerstieben,", "tokens": ["Da\u00df", "die", "zyk\u00b7lo\u00b7pi\u00b7schen", "Wol\u00b7ken", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "In denen der Winter, der Erde entfliehend,", "tokens": ["In", "de\u00b7nen", "der", "Win\u00b7ter", ",", "der", "Er\u00b7de", "ent\u00b7flie\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Noch heulend droht mit eisigen Schauern,", "tokens": ["Noch", "heu\u00b7lend", "droht", "mit", "ei\u00b7si\u00b7gen", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und die himmlischen Fernen sich auftun in leuchtender Reinheit.", "tokens": ["Und", "die", "himm\u00b7li\u00b7schen", "Fer\u00b7nen", "sich", "auf\u00b7tun", "in", "leuch\u00b7ten\u00b7der", "Rein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVIZU", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Und steigst dann, Herrlicher du, mit fliegenden Locken", "tokens": ["Und", "steigst", "dann", ",", "Herr\u00b7li\u00b7cher", "du", ",", "mit", "flie\u00b7gen\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "NN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zur Erde herab, empf\u00e4ngt sie mit seligem Schweigen", "tokens": ["Zur", "Er\u00b7de", "her\u00b7ab", ",", "emp\u00b7f\u00e4ngt", "sie", "mit", "se\u00b7li\u00b7gem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Den br\u00fcnstigen Freier, und in tiefen Schauern erbebend", "tokens": ["Den", "br\u00fcns\u00b7ti\u00b7gen", "Frei\u00b7er", ",", "und", "in", "tie\u00b7fen", "Schau\u00b7ern", "er\u00b7be\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Von deiner so wilden, sturmrasenden Umarmung,", "tokens": ["Von", "dei\u00b7ner", "so", "wil\u00b7den", ",", "sturm\u00b7ra\u00b7sen\u00b7den", "Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "\u00d6ffnet sie dir ihren heiligen Scho\u00df.", "tokens": ["\u00d6ff\u00b7net", "sie", "dir", "ih\u00b7ren", "hei\u00b7li\u00b7gen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Und es erfa\u00dft die Trunkene s\u00fc\u00dfeste Ahnung,", "tokens": ["Und", "es", "er\u00b7fa\u00dft", "die", "Trun\u00b7ke\u00b7ne", "s\u00fc\u00b7\u00dfes\u00b7te", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Wenn Bl\u00fctengl\u00fchender du das keimende Leben", "tokens": ["Wenn", "Bl\u00fc\u00b7ten\u00b7gl\u00fc\u00b7hen\u00b7der", "du", "das", "kei\u00b7men\u00b7de", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ART", "PIAT", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ihr weckest, des hohe Vergangenheit", "tokens": ["Ihr", "we\u00b7ckest", ",", "des", "ho\u00b7he", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "H\u00f6herer Zukunft sich zudr\u00e4ngt,", "tokens": ["H\u00f6\u00b7he\u00b7rer", "Zu\u00b7kunft", "sich", "zu\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Das dir gleich ist, wie du dir selber gleichst,", "tokens": ["Das", "dir", "gleich", "ist", ",", "wie", "du", "dir", "sel\u00b7ber", "gleichst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VAFIN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und deinem Willen ergeben, stets Bewegter,", "tokens": ["Und", "dei\u00b7nem", "Wil\u00b7len", "er\u00b7ge\u00b7ben", ",", "stets", "Be\u00b7weg\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,", "ADV", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Da\u00df an ihr ein ewig R\u00e4tselvolles", "tokens": ["Da\u00df", "an", "ihr", "ein", "e\u00b7wig", "R\u00e4t\u00b7sel\u00b7vol\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJD", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "In hoher Sch\u00f6nheit sich wieder k\u00fcnftig erneuert.", "tokens": ["In", "ho\u00b7her", "Sch\u00f6n\u00b7heit", "sich", "wie\u00b7der", "k\u00fcnf\u00b7tig", "er\u00b7neu\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}}}}