{"textgrid.poem.55474": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "55", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6chterchen! nach tr\u00fcben Stunden", "tokens": ["T\u00f6ch\u00b7ter\u00b7chen", "!", "nach", "tr\u00fc\u00b7ben", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Eltern Lust erschienen,", "tokens": ["Zu", "der", "El\u00b7tern", "Lust", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast so jung das Gl\u00fcck gefunden,", "tokens": ["Hast", "so", "jung", "das", "Gl\u00fcck", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Geliebtesten zu dienen.", "tokens": ["Den", "Ge\u00b7lieb\u00b7tes\u00b7ten", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6gest du den frohsten Stunden", "tokens": ["M\u00f6\u00b7gest", "du", "den", "frohs\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihres Lebens bl\u00fchend gr\u00fcnen.", "tokens": ["Ih\u00b7res", "Le\u00b7bens", "bl\u00fc\u00b7hend", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}