{"textgrid.poem.57176": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaum da\u00df sich, was sich einst von Dir getrennt,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum da\u00df sich, was sich einst von Dir getrennt,", "tokens": ["Kaum", "da\u00df", "sich", ",", "was", "sich", "einst", "von", "Dir", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "$,", "PRELS", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in seiner Sonderwesensart erkannt,", "tokens": ["in", "sei\u00b7ner", "Son\u00b7der\u00b7we\u00b7sen\u00b7sart", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "begehrt zur\u00fcck es in sein Element.", "tokens": ["be\u00b7gehrt", "zu\u00b7r\u00fcck", "es", "in", "sein", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKVZ", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es f\u00fchlt sich selbst und doch zugleich verbannt", "tokens": ["Es", "f\u00fchlt", "sich", "selbst", "und", "doch", "zu\u00b7gleich", "ver\u00b7bannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und sehnt sich heim in seines Ursprungs Scho\u00df ...", "tokens": ["und", "sehnt", "sich", "heim", "in", "sei\u00b7nes", "Ur\u00b7sprungs", "Scho\u00df", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch vor ihm steht noch ehern unverwandt", "tokens": ["Doch", "vor", "ihm", "steht", "noch", "e\u00b7hern", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00e4onengro\u00df sein menschheitliches Los!", "tokens": ["\u00e4\u00b7o\u00b7nen\u00b7gro\u00df", "sein", "menschheit\u00b7li\u00b7ches", "Los", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}}}}