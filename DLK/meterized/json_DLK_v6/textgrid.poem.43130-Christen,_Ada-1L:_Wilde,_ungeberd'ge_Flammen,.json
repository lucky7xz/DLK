{"textgrid.poem.43130": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wilde, ungeberd'ge Flammen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wilde, ungeberd'ge Flammen,", "tokens": ["Wil\u00b7de", ",", "un\u00b7ge\u00b7berd'\u00b7ge", "Flam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich suchen und verstecken,", "tokens": ["Die", "sich", "su\u00b7chen", "und", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie zischeln, wie sie schmeicheln", "tokens": ["Wie", "sie", "zi\u00b7scheln", ",", "wie", "sie", "schmei\u00b7cheln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sich schl\u00e4ngeln und sich necken;", "tokens": ["Und", "sich", "schl\u00e4n\u00b7geln", "und", "sich", "ne\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie sie prasseln, knistern, jubeln,", "tokens": ["Wie", "sie", "pras\u00b7seln", ",", "knis\u00b7tern", ",", "ju\u00b7beln", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich verfolgen und umschlingen,", "tokens": ["Sich", "ver\u00b7fol\u00b7gen", "und", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie zu dem hei\u00dfen Reigen", "tokens": ["Wie", "sie", "zu", "dem", "hei\u00b7\u00dfen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre lockern Lieder singen!", "tokens": ["Ih\u00b7re", "lo\u00b7ckern", "Lie\u00b7der", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie sie endlich gl\u00fchend z\u00fcngeln,", "tokens": ["Wie", "sie", "end\u00b7lich", "gl\u00fc\u00b7hend", "z\u00fcn\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jauchzend hoch und h\u00f6her schlagen,", "tokens": ["Jauch\u00b7zend", "hoch", "und", "h\u00f6\u00b7her", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mit den schlanken rothen Armen", "tokens": ["Mit", "den", "schlan\u00b7ken", "ro\u00b7then", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gierig in einander ragen!", "tokens": ["Gie\u00b7rig", "in", "ein\u00b7an\u00b7der", "ra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Welches gl\u00fchend frische Leben", "tokens": ["Wel\u00b7ches", "gl\u00fc\u00b7hend", "fri\u00b7sche", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Seh' ich in den Flammen treiben \u2013", "tokens": ["Seh'", "ich", "in", "den", "Flam\u00b7men", "trei\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und nichts als ein H\u00e4uflein Asche", "tokens": ["Und", "nichts", "als", "ein", "H\u00e4uf\u00b7lein", "A\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Soll von all' den Gluthen bleiben? ....", "tokens": ["Soll", "von", "all'", "den", "Glu\u00b7then", "blei\u00b7ben", "?", "...."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "APPR", "PIS", "ART", "NN", "VVINF", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}