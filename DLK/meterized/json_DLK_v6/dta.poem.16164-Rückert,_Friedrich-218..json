{"dta.poem.16164": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "218.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Gleiche k\u00f6nnen nicht im gleichen Felde gelten;", "tokens": ["Zwei", "Glei\u00b7che", "k\u00f6n\u00b7nen", "nicht", "im", "glei\u00b7chen", "Fel\u00b7de", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Anspruch machen zwei aufs v\u00f6llig gleiche selten.", "tokens": ["Doch", "An\u00b7spruch", "ma\u00b7chen", "zwei", "aufs", "v\u00f6l\u00b7lig", "glei\u00b7che", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "CARD", "APPRART", "ADJD", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Meist hat doch jeder Mann sein eignes Feld, und kann", "tokens": ["Meist", "hat", "doch", "je\u00b7der", "Mann", "sein", "eig\u00b7nes", "Feld", ",", "und", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "$,", "KON", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Nebenmanne wohl das g\u00f6nnen nebenan.", "tokens": ["Dem", "Ne\u00b7ben\u00b7man\u00b7ne", "wohl", "das", "g\u00f6n\u00b7nen", "ne\u00b7be\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+---+", "measure": "unknown