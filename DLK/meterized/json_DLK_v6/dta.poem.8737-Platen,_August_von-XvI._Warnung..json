{"dta.poem.8737": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  \n  Warnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und willst du noch einmal, o Jugendlicher,", "tokens": ["Und", "willst", "du", "noch", "ein\u00b7mal", ",", "o", "Ju\u00b7gend\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uneingedenk verschuldeter Gefahren,", "tokens": ["Un\u00b7ein\u00b7ge\u00b7denk", "ver\u00b7schul\u00b7de\u00b7ter", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Z\u00fcge sehn, die dir so t\u00f6dtlich waren?", "tokens": ["Die", "Z\u00fc\u00b7ge", "sehn", ",", "die", "dir", "so", "t\u00f6dt\u00b7lich", "wa\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Darfst du so fest auf deine Seele bauen,", "tokens": ["Darfst", "du", "so", "fest", "auf", "dei\u00b7ne", "See\u00b7le", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00e4hnst du mit Besonnenheit zu schauen", "tokens": ["Und", "w\u00e4hnst", "du", "mit", "Be\u00b7son\u00b7nen\u00b7heit", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der schwarzen Augen, die dir Sterne deuchten,", "tokens": ["Der", "schwar\u00b7zen", "Au\u00b7gen", ",", "die", "dir", "Ster\u00b7ne", "deuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bedeutungsvolles, dunkeltiefes Leuchten?", "tokens": ["Be\u00b7deu\u00b7tungs\u00b7vol\u00b7les", ",", "dun\u00b7kel\u00b7tie\u00b7fes", "Leuch\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nein! La\u00df die Wunde lieber sich vernarben,", "tokens": ["Nein", "!", "La\u00df", "die", "Wun\u00b7de", "lie\u00b7ber", "sich", "ver\u00b7nar\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVIMP", "ART", "NN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entschlie\u00dfe dich zu meiden und zu darben,", "tokens": ["Ent\u00b7schlie\u00b7\u00dfe", "dich", "zu", "mei\u00b7den", "und", "zu", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und vor dir selbst sogar, o Herz, verh\u00fclle", "tokens": ["Und", "vor", "dir", "selbst", "so\u00b7gar", ",", "o", "Herz", ",", "ver\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "ADV", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ganzen Reichthum deiner Liebesf\u00fclle!", "tokens": ["Den", "gan\u00b7zen", "Reicht\u00b7hum", "dei\u00b7ner", "Lie\u00b7bes\u00b7f\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}