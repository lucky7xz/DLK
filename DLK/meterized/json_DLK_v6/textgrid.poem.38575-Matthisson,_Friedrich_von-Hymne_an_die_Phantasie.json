{"textgrid.poem.38575": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie von Bl\u00fcthe zu Bl\u00fcthe die Biene fleugt,", "tokens": ["Wie", "von", "Bl\u00fc\u00b7the", "zu", "Bl\u00fc\u00b7the", "die", "Bie\u00b7ne", "fleugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Also schwebst du, o Phantasie,", "tokens": ["Al\u00b7so", "schwebst", "du", ",", "o", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Umflossen von des Aetherlichts goldenem Strom,", "tokens": ["Um\u00b7flos\u00b7sen", "von", "des", "A\u00b7e\u00b7ther\u00b7lichts", "gol\u00b7de\u00b7nem", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Durch des Himmels heilige Gefilde,", "tokens": ["Durch", "des", "Him\u00b7mels", "hei\u00b7li\u00b7ge", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wonnestralend von Welt zu Welt!", "tokens": ["Won\u00b7nest\u00b7ra\u00b7lend", "von", "Welt", "zu", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Gleich des Nordscheins str\u00f6mendem Purpur gl\u00e4nzt", "tokens": ["Gleich", "des", "Nord\u00b7scheins", "str\u00f6\u00b7men\u00b7dem", "Pur\u00b7pur", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Deines Fluges blendende Bahn!", "tokens": ["Dei\u00b7nes", "Flu\u00b7ges", "blen\u00b7den\u00b7de", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ahndung und Sehnen und Wehmuth,", "tokens": ["Ahn\u00b7dung", "und", "Seh\u00b7nen", "und", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Und Ruh' und Entz\u00fccken und Wonne", "tokens": ["Und", "Ruh'", "und", "Ent\u00b7z\u00fc\u00b7cken", "und", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Umtanzen in holder", "tokens": ["Um\u00b7tan\u00b7zen", "in", "hol\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Geniusbildung, o G\u00f6ttin, dich!", "tokens": ["Ge\u00b7nius\u00b7bil\u00b7dung", ",", "o", "G\u00f6t\u00b7tin", ",", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Heil! dir, Unsterbliche, Heil!", "tokens": ["Heil", "!", "dir", ",", "U\u00b7nsterb\u00b7li\u00b7che", ",", "Heil", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.13": {"text": "Du entschleierst der Erinnerung freundliches Gestirn,", "tokens": ["Du", "ent\u00b7schlei\u00b7erst", "der", "E\u00b7rin\u00b7ne\u00b7rung", "freund\u00b7li\u00b7ches", "Ge\u00b7stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Welchem Allvater \u00fcber der Lebenszeit", "tokens": ["Wel\u00b7chem", "All\u00b7va\u00b7ter", "\u00fc\u00b7ber", "der", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "ART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "D\u00e4mmerndem Grabe zu leuchten gebot!", "tokens": ["D\u00e4m\u00b7mern\u00b7dem", "Gra\u00b7be", "zu", "leuch\u00b7ten", "ge\u00b7bot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.16": {"text": "Heil! dir, Unsterbliche, Heil!", "tokens": ["Heil", "!", "dir", ",", "U\u00b7nsterb\u00b7li\u00b7che", ",", "Heil", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.17": {"text": "Du bestralst mit Hofnungsmorgenr\u00f6the", "tokens": ["Du", "be\u00b7stralst", "mit", "Hof\u00b7nungs\u00b7mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Der Zukunft umnachteten Hain!", "tokens": ["Der", "Zu\u00b7kunft", "um\u00b7nach\u00b7te\u00b7ten", "Hain", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.19": {"text": "Heil! dir, Unsterbliche, Heil!", "tokens": ["Heil", "!", "dir", ",", "U\u00b7nsterb\u00b7li\u00b7che", ",", "Heil", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.20": {"text": "Auf des Mondes lieblichen Fluren", "tokens": ["Auf", "des", "Mon\u00b7des", "lieb\u00b7li\u00b7chen", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Weilst du im Schimmer des Erdenlichts,", "tokens": ["Weilst", "du", "im", "Schim\u00b7mer", "des", "Er\u00b7den\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Auf der Sonne flammenden Wogen", "tokens": ["Auf", "der", "Son\u00b7ne", "flam\u00b7men\u00b7den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Wiegst du, Himmlische, jauchzend dich,", "tokens": ["Wiegst", "du", ",", "Himm\u00b7li\u00b7sche", ",", "jauch\u00b7zend", "dich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "Wie auf der Waizensaat gr\u00fcnlichen Wallungen", "tokens": ["Wie", "auf", "der", "Wai\u00b7zen\u00b7saat", "gr\u00fcn\u00b7li\u00b7chen", "Wal\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-++--++-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Sanft sich wieget der Abendwind!", "tokens": ["Sanft", "sich", "wie\u00b7get", "der", "Ab\u00b7end\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "Schwingst dich h\u00f6her hinan, wo der ", "tokens": ["Schwingst", "dich", "h\u00f6\u00b7her", "hi\u00b7nan", ",", "wo", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PWAV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Dem, der aus Welten ihn baute, flammt;", "tokens": ["Dem", ",", "der", "aus", "Wel\u00b7ten", "ihn", "bau\u00b7te", ",", "flammt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Wo im ", "tokens": ["Wo", "im"], "token_info": ["word", "word"], "pos": ["PWAV", "APPRART"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Opferger\u00fcche zu ihm sendet empor", "tokens": ["Op\u00b7fer\u00b7ge\u00b7r\u00fc\u00b7che", "zu", "ihm", "sen\u00b7det", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVFIN", "PTKVZ"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.30": {"text": "Der aus Lichtglanz webte ihrer Bl\u00e4tter", "tokens": ["Der", "aus", "Licht\u00b7glanz", "web\u00b7te", "ih\u00b7rer", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Stralende Herrlichkeit;", "tokens": ["Stra\u00b7len\u00b7de", "Herr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.32": {"text": "Wo sein Haupt der ", "tokens": ["Wo", "sein", "Haupt", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "Und der melodische ", "tokens": ["Und", "der", "me\u00b7lo\u00b7di\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Horchet der ", "tokens": ["Hor\u00b7chet", "der"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.35": {"text": "Breitest die Fittige st\u00fcrmender dann,", "tokens": ["Brei\u00b7test", "die", "Fit\u00b7ti\u00b7ge", "st\u00fcr\u00b7men\u00b7der", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.36": {"text": "Und fleugst empor, empor, wo der Sterne Lied", "tokens": ["Und", "fleugst", "em\u00b7por", ",", "em\u00b7por", ",", "wo", "der", "Ster\u00b7ne", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PTKVZ", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Triumph und Jubel und Vollendung t\u00f6nt;", "tokens": ["Tri\u00b7umph", "und", "Ju\u00b7bel", "und", "Vol\u00b7len\u00b7dung", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.38": {"text": "Wo des unverg\u00e4nglichen Seyns", "tokens": ["Wo", "des", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "Seyns"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Lebendige Vorempfindung, (ach! im Thal des Staubs", "tokens": ["Le\u00b7ben\u00b7di\u00b7ge", "Vor\u00b7emp\u00b7fin\u00b7dung", ",", "(", "ach", "!", "im", "Thal", "des", "Staubs"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "$(", "ITJ", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.40": {"text": "Nur leiser, kaumgeh\u00f6rter Laut!)", "tokens": ["Nur", "lei\u00b7ser", ",", "kaum\u00b7ge\u00b7h\u00f6r\u00b7ter", "Laut", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Im reinsten Vollklang dich umstr\u00f6mt;", "tokens": ["Im", "reins\u00b7ten", "Voll\u00b7klang", "dich", "um\u00b7str\u00f6mt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Wo der Wesen unendliche Leiter,", "tokens": ["Wo", "der", "We\u00b7sen", "un\u00b7end\u00b7li\u00b7che", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.43": {"text": "Umschlungen von den Banden der ewigen Harmonie,", "tokens": ["Um\u00b7schlun\u00b7gen", "von", "den", "Ban\u00b7den", "der", "e\u00b7wi\u00b7gen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Sich dir in unbew\u00f6lktem Himmelsschein enth\u00fcllt,", "tokens": ["Sich", "dir", "in", "un\u00b7be\u00b7w\u00f6lk\u00b7tem", "Him\u00b7mels\u00b7schein", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Bis dahin, wo sie an des Urlichts Quell,", "tokens": ["Bis", "da\u00b7hin", ",", "wo", "sie", "an", "des", "Ur\u00b7lichts", "Quell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "In eignem Glanze sich verliert,", "tokens": ["In", "eig\u00b7nem", "Glan\u00b7ze", "sich", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und wo der k\u00fchnste deiner Schw\u00fcnge", "tokens": ["Und", "wo", "der", "k\u00fchns\u00b7te", "dei\u00b7ner", "Schw\u00fcn\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Sie ewig und ewig nicht ermi\u00dft!", "tokens": ["Sie", "e\u00b7wig", "und", "e\u00b7wig", "nicht", "er\u00b7mi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}