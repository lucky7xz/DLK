{"textgrid.poem.48869": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "94. Auf ihrer Beider Tr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! ist es noch nicht Zeit, o du gesalzne Flut,", "tokens": ["Ach", "!", "ist", "es", "noch", "nicht", "Zeit", ",", "o", "du", "ge\u00b7salz\u00b7ne", "Flut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$,", "FM", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die aus vier Augen hier in einem Rinnen rinnet,", "tokens": ["die", "aus", "vier", "Au\u00b7gen", "hier", "in", "ei\u00b7nem", "Rin\u00b7nen", "rin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ach! ist es noch nicht Zeit, da\u00df ihr einmal beginnet,", "tokens": ["ach", "!", "ist", "es", "noch", "nicht", "Zeit", ",", "da\u00df", "ihr", "ein\u00b7mal", "be\u00b7gin\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr Tr\u00e4nen, aus zu sein? Es mu\u00df das rote Blut", "tokens": ["ihr", "Tr\u00e4\u00b7nen", ",", "aus", "zu", "sein", "?", "Es", "mu\u00df", "das", "ro\u00b7te", "Blut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PTKZU", "VAINF", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "auch sein heraus geweint? Ach! tut nicht, wie ihr tut,", "tokens": ["auch", "sein", "he\u00b7raus", "ge\u00b7weint", "?", "Ach", "!", "tut", "nicht", ",", "wie", "ihr", "tut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APZR", "VVPP", "$.", "ITJ", "$.", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "seid gn\u00e4dig unsrer Angst, als die ihr mindern k\u00f6nnet,", "tokens": ["seid", "gn\u00e4\u00b7dig", "uns\u00b7rer", "Angst", ",", "als", "die", "ihr", "min\u00b7dern", "k\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "ART", "PPOSAT", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn ihr zu trucknen aus nur selbsten seid gesinnet.", "tokens": ["wenn", "ihr", "zu", "truck\u00b7nen", "aus", "nur", "selbs\u00b7ten", "seid", "ge\u00b7sin\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVFIN", "APPR", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr l\u00f6scht nicht, wie ihr meint, die hei\u00dfe Liebesglut.", "tokens": ["Ihr", "l\u00f6scht", "nicht", ",", "wie", "ihr", "meint", ",", "die", "hei\u00b7\u00dfe", "Lie\u00b7bes\u00b7glut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O da\u00df Cupido doch Register halten solte", "tokens": ["O", "da\u00df", "Cu\u00b7pi\u00b7do", "doch", "Re\u00b7gis\u00b7ter", "hal\u00b7ten", "sol\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "NE", "KON", "NN", "VVINF", "VMFIN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "und nur das zehnte Teil des Wassers messen wolte,", "tokens": ["und", "nur", "das", "zehn\u00b7te", "Teil", "des", "Was\u00b7sers", "mes\u00b7sen", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o m\u00f6chte Venus nur sein Richtrin dieser Pein,", "tokens": ["o", "m\u00f6ch\u00b7te", "Ve\u00b7nus", "nur", "sein", "Richt\u00b7rin", "die\u00b7ser", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "ich wei\u00df, es w\u00fcrde nicht der Himmel so viel haben,", "tokens": ["ich", "wei\u00df", ",", "es", "w\u00fcr\u00b7de", "nicht", "der", "Him\u00b7mel", "so", "viel", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so viel an alter Lust der ganze Himmel haben,", "tokens": ["so", "viel", "an", "al\u00b7ter", "Lust", "der", "gan\u00b7ze", "Him\u00b7mel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als viel der Tropfen nun von uns vergossen seyn.", "tokens": ["als", "viel", "der", "Trop\u00b7fen", "nun", "von", "uns", "ver\u00b7gos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}