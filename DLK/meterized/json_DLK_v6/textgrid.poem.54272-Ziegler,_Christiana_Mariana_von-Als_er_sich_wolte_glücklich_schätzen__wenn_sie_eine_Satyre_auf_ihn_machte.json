{"textgrid.poem.54272": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Als er sich wolte gl\u00fccklich sch\u00e4tzen / wenn sie eine Satyre auf ihn machte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ruffte nechst den Pan mit seinem Wald-Gesinde;", "tokens": ["Ich", "ruff\u00b7te", "nechst", "den", "Pan", "mit", "sei\u00b7nem", "Wald\u00b7Ge\u00b7sin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt, schrie ich, Faunen! kommt und zimmert mir geschwinde", "tokens": ["Kommt", ",", "schrie", "ich", ",", "Fau\u00b7nen", "!", "kommt", "und", "zim\u00b7mert", "mir", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "KON", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Lied, das stachlicht klingt, vor unsern Selidor,", "tokens": ["Ein", "Lied", ",", "das", "stach\u00b7licht", "klingt", ",", "vor", "un\u00b7sern", "Se\u00b7li\u00b7dor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wenn ihr fertig seyd, so le\u00dft ihm solches vor.", "tokens": ["Und", "wenn", "ihr", "fer\u00b7tig", "seyd", ",", "so", "le\u00dft", "ihm", "sol\u00b7ches", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch an statt, da\u00df sich ein Satyr liesse blicken,", "tokens": ["Je\u00b7doch", "an", "statt", ",", "da\u00df", "sich", "ein", "Sa\u00b7tyr", "lies\u00b7se", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sah man die Gratien aus ihren Lager r\u00fccken,", "tokens": ["Sah", "man", "die", "Gra\u00b7ti\u00b7en", "aus", "ih\u00b7ren", "La\u00b7ger", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die schrien auf mich zu: Schreib, freche! nur soviel:", "tokens": ["Die", "schri\u00b7en", "auf", "mich", "zu", ":", "Schreib", ",", "fre\u00b7che", "!", "nur", "so\u00b7viel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "NN", "$,", "ADJA", "$.", "ADV", "PIS", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Wer Selidorens Geist und Wesen tadeln will,", "tokens": ["Wer", "Se\u00b7li\u00b7do\u00b7rens", "Geist", "und", "We\u00b7sen", "ta\u00b7deln", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der mu\u00df, wofern er nicht will selbst beschimpffet heissen,", "tokens": ["Der", "mu\u00df", ",", "wo\u00b7fern", "er", "nicht", "will", "selbst", "be\u00b7schimpf\u00b7fet", "heis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mehr Gifft als Zoilus, mehr Gall als Momus, weisen.", "tokens": ["Mehr", "Gifft", "als", "Zoi\u00b7lus", ",", "mehr", "Gall", "als", "Mo\u00b7mus", ",", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "KOKOM", "NE", "$,", "PIAT", "NN", "KOKOM", "NE", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Schaut durchs Vergr\u00f6\u00dferungs-Gla\u00df den gantzen C\u00f6rper an,", "tokens": ["Schaut", "durchs", "Ver\u00b7gr\u00f6\u00b7\u00dfe\u00b7rungs\u00b7Gla\u00df", "den", "gant\u00b7zen", "C\u00f6r\u00b7per", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ob man ein P\u00fcncktgen wohl, das unrecht, finden kan.", "tokens": ["Ob", "man", "ein", "P\u00fcn\u00b7ckt\u00b7gen", "wohl", ",", "das", "un\u00b7recht", ",", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "$,", "ART", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}