{"textgrid.poem.43577": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling sprach zu der Nachtigall:", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling sprach zu der Nachtigall:", "tokens": ["Fr\u00fch\u00b7ling", "sprach", "zu", "der", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ich will euch geben einen Ball.", "tokens": ["Ich", "will", "euch", "ge\u00b7ben", "ei\u00b7nen", "Ball", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lade, Nachtigall, alle ein,", "tokens": ["La\u00b7de", ",", "Nach\u00b7ti\u00b7gall", ",", "al\u00b7le", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PIS", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Alle V\u00f6gel gro\u00df und klein,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle V\u00f6gel, alle!", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", ",", "al\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und da kamen die V\u00f6gel all", "tokens": ["Und", "da", "ka\u00b7men", "die", "V\u00f6\u00b7gel", "all"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PIAT"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zum Fr\u00fchlingsball mit Sang und Schall:", "tokens": ["Zum", "Fr\u00fch\u00b7lings\u00b7ball", "mit", "Sang", "und", "Schall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kuckuck, Wiedehopf, Elster, Staar,", "tokens": ["Ku\u00b7ckuck", ",", "Wie\u00b7de\u00b7hopf", ",", "Els\u00b7ter", ",", "Staar", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Reiher, Rabe, Strau\u00df und Aar,", "tokens": ["Rei\u00b7her", ",", "Ra\u00b7be", ",", "Strau\u00df", "und", "Aar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drossel, Fink' und Zeisig.", "tokens": ["Dros\u00b7sel", ",", "Fink'", "und", "Zei\u00b7sig", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und sie tanzten im Blumenduft", "tokens": ["Und", "sie", "tanz\u00b7ten", "im", "Blu\u00b7men\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bei Sonnenschein und linder Luft,", "tokens": ["Bei", "Son\u00b7nen\u00b7schein", "und", "lin\u00b7der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tranken w\u00fcrzigen Bl\u00fcthenmost,", "tokens": ["Tran\u00b7ken", "w\u00fcr\u00b7zi\u00b7gen", "Bl\u00fc\u00b7then\u00b7most", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Schmausten lauter feine Kost,", "tokens": ["Schmaus\u00b7ten", "lau\u00b7ter", "fei\u00b7ne", "Kost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Theure, selt'ne Sachen.", "tokens": ["Theu\u00b7re", ",", "selt'\u00b7ne", "Sa\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Als der Abend begann zu nah'n,", "tokens": ["Als", "der", "A\u00b7bend", "be\u00b7gann", "zu", "nah'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da sprach zur Nachtigall der Hahn:", "tokens": ["Da", "sprach", "zur", "Nach\u00b7ti\u00b7gall", "der", "Hahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbjetzo wird wohl das Beste sein,", "tokens": ["\u00bb", "jet\u00b7zo", "wird", "wohl", "das", "Bes\u00b7te", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir V\u00f6gel gro\u00df und klein", "tokens": ["Wenn", "wir", "V\u00f6\u00b7gel", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gehen heim zu Neste.", "tokens": ["Ge\u00b7hen", "heim", "zu", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aber billig vor Allem ist,", "tokens": ["A\u00b7ber", "bil\u00b7lig", "vor", "Al\u00b7lem", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIS", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Da\u00df man des Wirthes nicht vergi\u00dft.", "tokens": ["Da\u00df", "man", "des", "Wirt\u00b7hes", "nicht", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft uns, V\u00f6glein gro\u00df und klein,", "tokens": ["La\u00dft", "uns", ",", "V\u00f6\u00b7glein", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kikriki! recht dankbar sein:", "tokens": ["Kik\u00b7ri\u00b7ki", "!", "recht", "dank\u00b7bar", "sein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "VAINF", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Vivat hoch, Herr Fr\u00fchling!\u00ab", "tokens": ["Vi\u00b7vat", "hoch", ",", "Herr", "Fr\u00fch\u00b7ling", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "NE", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}