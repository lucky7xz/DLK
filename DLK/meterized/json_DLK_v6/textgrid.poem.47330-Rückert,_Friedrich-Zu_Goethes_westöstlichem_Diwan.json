{"textgrid.poem.47330": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zu Goethes west\u00f6stlichem Diwan", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr kosten", "tokens": ["Wollt", "ihr", "kos\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Reinen Osten,", "tokens": ["Rei\u00b7nen", "Os\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "M\u00fc\u00dft ihr gehn von hier zum selben Manne,", "tokens": ["M\u00fc\u00dft", "ihr", "gehn", "von", "hier", "zum", "sel\u00b7ben", "Man\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der vom Westen", "tokens": ["Der", "vom", "Wes\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Auch den besten", "tokens": ["Auch", "den", "bes\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wein von jeher schenkt' aus voller Kanne.", "tokens": ["Wein", "von", "je\u00b7her", "schenkt'", "aus", "vol\u00b7ler", "Kan\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Als der West war durchgekostet,", "tokens": ["Als", "der", "West", "war", "durch\u00b7ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat er nun den Ost entmostet;", "tokens": ["Hat", "er", "nun", "den", "Ost", "ent\u00b7mos\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seht, dort schwelgt er auf der Ottomane.", "tokens": ["Seht", ",", "dort", "schwelgt", "er", "auf", "der", "Ot\u00b7to\u00b7ma\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Abendr\u00f6ten", "tokens": ["A\u00b7bend\u00b7r\u00f6\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dienten Goethen", "tokens": ["Dien\u00b7ten", "Goe\u00b7then"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Freudig als dem Stern des Abendlandes;", "tokens": ["Freu\u00b7dig", "als", "dem", "Stern", "des", "A\u00b7bend\u00b7lan\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nun erh\u00f6hten", "tokens": ["Nun", "er\u00b7h\u00f6h\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Morgenr\u00f6ten", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Herrlich ihn zum Herrn des Morgenlandes.", "tokens": ["Herr\u00b7lich", "ihn", "zum", "Herrn", "des", "Mor\u00b7gen\u00b7lan\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wo die beiden gl\u00fchn zusammen,", "tokens": ["Wo", "die", "bei\u00b7den", "gl\u00fchn", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df der Himmel bl\u00fchn in Flammen,", "tokens": ["Mu\u00df", "der", "Him\u00b7mel", "bl\u00fchn", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ein Diwan voll lichten Rosenbrandes.", "tokens": ["Ein", "Di\u00b7wan", "voll", "lich\u00b7ten", "Ro\u00b7sen\u00b7bran\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnt ihr merken", "tokens": ["K\u00f6nnt", "ihr", "mer\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "An den St\u00e4rken", "tokens": ["An", "den", "St\u00e4r\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dieses Arms, wie lang' er hat gefochten?", "tokens": ["Die\u00b7ses", "Arms", ",", "wie", "lang'", "er", "hat", "ge\u00b7foch\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PWAV", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dem das Alter", "tokens": ["Dem", "das", "Al\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nicht den Psalter", "tokens": ["Nicht", "den", "Psal\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Hat entwunden, sondern neu umflochten.", "tokens": ["Hat", "ent\u00b7wun\u00b7den", ",", "son\u00b7dern", "neu", "um\u00b7floch\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Aus iran'schen Naphthabronnen", "tokens": ["Aus", "i\u00b7ran'\u00b7schen", "Naph\u00b7tha\u00b7bron\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6pft der Greis itzt, was die Sonnen", "tokens": ["Sch\u00f6pft", "der", "Greis", "itzt", ",", "was", "die", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Einst Italiens ihm, dem J\u00fcngling, kochten.", "tokens": ["Einst", "I\u00b7ta\u00b7li\u00b7ens", "ihm", ",", "dem", "J\u00fcng\u00b7ling", ",", "koch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Jugendhadern", "tokens": ["Ju\u00b7gend\u00b7ha\u00b7dern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In den Adern,", "tokens": ["In", "den", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Zorn und Glut und Mild' und s\u00fc\u00dfes Kosen;", "tokens": ["Zorn", "und", "Glut", "und", "Mild'", "und", "s\u00fc\u00b7\u00dfes", "Ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles Lieben", "tokens": ["Al\u00b7les", "Lie\u00b7ben"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Jung geblieben,", "tokens": ["Jung", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Seiner Stirne stehen sch\u00f6n die Rosen.", "tokens": ["Sei\u00b7ner", "Stir\u00b7ne", "ste\u00b7hen", "sch\u00f6n", "die", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn nicht etwa ew'ges Leben", "tokens": ["Wenn", "nicht", "et\u00b7wa", "ew'\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihm verliehn ist, sei gegeben", "tokens": ["Ihm", "ver\u00b7liehn", "ist", ",", "sei", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "VAFIN", "$,", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Langes ihm von uns gewognen Losen.", "tokens": ["Lan\u00b7ges", "ihm", "von", "uns", "ge\u00b7wog\u00b7nen", "Lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ja von jenen", "tokens": ["Ja", "von", "je\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "APPR", "PDAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Selbst, mit denen", "tokens": ["Selbst", ",", "mit", "de\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "APPR", "PRELS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Du den neuen Jugendbund errichtet,", "tokens": ["Du", "den", "neu\u00b7en", "Ju\u00b7gend\u00b7bund", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sei mit Br\u00fcnsten", "tokens": ["Sei", "mit", "Br\u00fcns\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Unter K\u00fcnsten", "tokens": ["Un\u00b7ter", "K\u00fcns\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Aller Art, in der auch unterrichtet,", "tokens": ["Al\u00b7ler", "Art", ",", "in", "der", "auch", "un\u00b7ter\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wie Saadi in jenem Orden", "tokens": ["Wie", "Saa\u00b7di", "in", "je\u00b7nem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "\u00dcber hundert Jahr' alt worden,", "tokens": ["\u00dc\u00b7ber", "hun\u00b7dert", "Jahr'", "alt", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und Dschami hat nah' daran gedichtet.", "tokens": ["Und", "Dscha\u00b7mi", "hat", "nah'", "da\u00b7ran", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}