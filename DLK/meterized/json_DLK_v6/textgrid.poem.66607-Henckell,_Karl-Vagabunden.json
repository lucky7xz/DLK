{"textgrid.poem.66607": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Vagabunden", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In rauchiger Kneipe \u2013 hollaho! \u2013", "tokens": ["In", "rau\u00b7chi\u00b7ger", "Knei\u00b7pe", "\u2013", "hol\u00b7la\u00b7ho", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "ITJ", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kauern vier Vagabunden.", "tokens": ["Kau\u00b7ern", "vier", "Va\u00b7ga\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihr Maul ist frech, ihr Witz ist roh,", "tokens": ["Ihr", "Maul", "ist", "frech", ",", "ihr", "Witz", "ist", "roh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie versaufen die d\u00e4mmernden Stunden.", "tokens": ["Sie", "ver\u00b7sau\u00b7fen", "die", "d\u00e4m\u00b7mern\u00b7den", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die Lampe schmaucht und brennt so tr\u00fcb,", "tokens": ["Die", "Lam\u00b7pe", "schmaucht", "und", "brennt", "so", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als gl\u00f6mme sie \u00fcber Leichen.", "tokens": ["Als", "gl\u00f6m\u00b7me", "sie", "\u00fc\u00b7ber", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Kerlen ist das Zwielicht lieb,", "tokens": ["Den", "Ker\u00b7len", "ist", "das", "Zwie\u00b7licht", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wirt mu\u00df Branntwein reichen.", "tokens": ["Der", "Wirt", "mu\u00df", "Brannt\u00b7wein", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie leeren die Gl\u00e4ser, der Schnaps heizt gut,", "tokens": ["Sie", "lee\u00b7ren", "die", "Gl\u00e4\u00b7ser", ",", "der", "Schnaps", "heizt", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das brennt wie fl\u00fcssige Flammen \u2013", "tokens": ["Das", "brennt", "wie", "fl\u00fcs\u00b7si\u00b7ge", "Flam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hei, wie siedet im Leibe das Blut!", "tokens": ["Hei", ",", "wie", "sie\u00b7det", "im", "Lei\u00b7be", "das", "Blut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sie r\u00fccken dichter zusammen.", "tokens": ["Sie", "r\u00fc\u00b7cken", "dich\u00b7ter", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbwas n\u00fctzt der Ollen das viele Geld,", "tokens": ["\u00bb", "was", "n\u00fctzt", "der", "Ol\u00b7len", "das", "vie\u00b7le", "Geld", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gehamstert im rostigen Kasten?", "tokens": ["Ge\u00b7hams\u00b7tert", "im", "ros\u00b7ti\u00b7gen", "Kas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Man lebt nur einmal auf der Welt,", "tokens": ["Man", "lebt", "nur", "ein\u00b7mal", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sauer schmeckt dies Fasten.", "tokens": ["Zu", "sau\u00b7er", "schmeckt", "dies", "Fas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht l\u00e4nger lungern wir so umher,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "lun\u00b7gern", "wir", "so", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geknufft wie r\u00e4udige Hunde,", "tokens": ["Ge\u00b7knufft", "wie", "r\u00e4u\u00b7di\u00b7ge", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Magen leer, die Taschen leer,", "tokens": ["Den", "Ma\u00b7gen", "leer", ",", "die", "Ta\u00b7schen", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Elende Vagabunden.", "tokens": ["E\u00b7len\u00b7de", "Va\u00b7ga\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Wir wollen Braten, Weiber und Wein ...", "tokens": ["Wir", "wol\u00b7len", "Bra\u00b7ten", ",", "Wei\u00b7ber", "und", "Wein", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was soll der lausige Bettel?", "tokens": ["Was", "soll", "der", "lau\u00b7si\u00b7ge", "Bet\u00b7tel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir schlagen dir sch\u00f6n den Sch\u00e4del ein,", "tokens": ["Wir", "schla\u00b7gen", "dir", "sch\u00f6n", "den", "Sch\u00e4\u00b7del", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du giftige, geizige Vettel!\u00ab", "tokens": ["Du", "gif\u00b7ti\u00b7ge", ",", "gei\u00b7zi\u00b7ge", "Vet\u00b7tel", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Der eine flucht, der andere grinst,", "tokens": ["Der", "ei\u00b7ne", "flucht", ",", "der", "an\u00b7de\u00b7re", "grinst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwei nicken schwer mit der Stirne,", "tokens": ["Zwei", "ni\u00b7cken", "schwer", "mit", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVINF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Blutlache vor ihren Blicken glinst", "tokens": ["Blut\u00b7la\u00b7che", "vor", "ih\u00b7ren", "Bli\u00b7cken", "glinst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit ausgespritztem Gehirne.", "tokens": ["Mit", "aus\u00b7ge\u00b7spritz\u00b7tem", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Hu, wie sie schwanken und torkeln hinein", "tokens": ["Hu", ",", "wie", "sie", "schwan\u00b7ken", "und", "tor\u00b7keln", "hin\u00b7ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPER", "VVINF", "KON", "NN", "APZR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und schl\u00fcrfen Blut ohn' Ende!", "tokens": ["Und", "schl\u00fcr\u00b7fen", "Blut", "ohn'", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Schnaps wird Blut, das Blut wird Wein \u2013", "tokens": ["Der", "Schnaps", "wird", "Blut", ",", "das", "Blut", "wird", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sch\u00fctteln die zitternden H\u00e4nde.", "tokens": ["Sie", "sch\u00fct\u00b7teln", "die", "zit\u00b7tern\u00b7den", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbauf morgen nacht! Verrat hei\u00dft Tod!", "tokens": ["\u00bb", "auf", "mor\u00b7gen", "nacht", "!", "Ver\u00b7rat", "hei\u00dft", "Tod", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$.", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirt, auf! Wen's trifft, steht Schmiere.\u00ab", "tokens": ["Wirt", ",", "auf", "!", "Wen's", "trifft", ",", "steht", "Schmie\u00b7re", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PTKVZ", "$.", "NE", "VVFIN", "$,", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Himmel r\u00e4ndert das Morgenrot,", "tokens": ["Den", "Him\u00b7mel", "r\u00e4n\u00b7dert", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lichtscheu hintaumeln die Viere.", "tokens": ["Licht\u00b7scheu", "hin\u00b7tau\u00b7meln", "die", "Vie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}