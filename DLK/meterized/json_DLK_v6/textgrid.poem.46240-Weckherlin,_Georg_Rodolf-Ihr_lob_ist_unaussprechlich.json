{"textgrid.poem.46240": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ihr lob ist unaussprechlich", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welchen der g\u00f6tter schatz, der natur beste kunst", "tokens": ["Wel\u00b7chen", "der", "g\u00f6t\u00b7ter", "schatz", ",", "der", "na\u00b7tur", "bes\u00b7te", "kunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "des himmels k\u00f6stlichkeit alhie zu sehen sehnet,", "tokens": ["des", "him\u00b7mels", "k\u00f6st\u00b7lich\u00b7keit", "al\u00b7hie", "zu", "se\u00b7hen", "seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der segne sein gesicht durch die sch\u00f6n, deren gunst", "tokens": ["der", "seg\u00b7ne", "sein", "ge\u00b7sicht", "durch", "die", "sch\u00f6n", ",", "de\u00b7ren", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "VVPP", "APPR", "ART", "ADJD", "$,", "PRELAT", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "erquicket meinen geist und dise welt besch\u00f6net;", "tokens": ["er\u00b7quic\u00b7ket", "mei\u00b7nen", "geist", "und", "di\u00b7se", "welt", "be\u00b7sch\u00f6\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch kommet er nicht bald, so kommet er umsunst,", "tokens": ["Doch", "kom\u00b7met", "er", "nicht", "bald", ",", "so", "kom\u00b7met", "er", "um\u00b7sunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dan schon ein jeder got begirig nach ihr g\u00f6net", "tokens": ["dan", "schon", "ein", "je\u00b7der", "got", "be\u00b7gi\u00b7rig", "nach", "ihr", "g\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "damit in purer lieb und heilig s\u00fc\u00dfer brunst", "tokens": ["da\u00b7mit", "in", "pu\u00b7rer", "lieb", "und", "hei\u00b7lig", "s\u00fc\u00b7\u00dfer", "brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "ADJD", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "er werd mit ehr von ihr und sie von ihm gekr\u00f6net.", "tokens": ["er", "werd", "mit", "ehr", "von", "ihr", "und", "sie", "von", "ihm", "ge\u00b7kr\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der tugend s\u00fc\u00dfe kraft, der lieb und sch\u00f6nheit prob,", "tokens": ["Der", "tu\u00b7gend", "s\u00fc\u00b7\u00dfe", "kraft", ",", "der", "lieb", "und", "sch\u00f6n\u00b7heit", "prob", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "mit der holdseligkeit und g\u00f6tlichen geberden", "tokens": ["mit", "der", "hold\u00b7se\u00b7lig\u00b7keit", "und", "g\u00f6t\u00b7li\u00b7chen", "ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "in eines engels leib er sehen wird auf erden,", "tokens": ["in", "ei\u00b7nes", "en\u00b7gels", "leib", "er", "se\u00b7hen", "wird", "auf", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bekennend, da\u00df allein des s\u00fc\u00dfen engels lob", "tokens": ["Be\u00b7ken\u00b7nend", ",", "da\u00df", "al\u00b7lein", "des", "s\u00fc\u00b7\u00dfen", "en\u00b7gels", "lob"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "von eines engels mund kan recht gesungen werden,", "tokens": ["von", "ei\u00b7nes", "en\u00b7gels", "mund", "kan", "recht", "ge\u00b7sun\u00b7gen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und da\u00df ohn sie die welt ganz elend, arm und grob.", "tokens": ["und", "da\u00df", "ohn", "sie", "die", "welt", "ganz", "e\u00b7lend", ",", "arm", "und", "grob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}