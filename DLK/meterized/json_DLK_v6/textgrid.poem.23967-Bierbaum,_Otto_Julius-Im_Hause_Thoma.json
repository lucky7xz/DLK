{"textgrid.poem.23967": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Im Hause Thoma", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stiller Heiterkeit ein Glanz,", "tokens": ["Stil\u00b7ler", "Hei\u00b7ter\u00b7keit", "ein", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leisen Gl\u00fcckes leiser Tanz,", "tokens": ["Lei\u00b7sen", "Gl\u00fc\u00b7ckes", "lei\u00b7ser", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaffens frohe Kraft,", "tokens": ["Schaf\u00b7fens", "fro\u00b7he", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Heitrer Liebe stille Hut,", "tokens": ["Hei\u00b7trer", "Lie\u00b7be", "stil\u00b7le", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schalkheit auch, das Kleinod gut,", "tokens": ["Schalk\u00b7heit", "auch", ",", "das", "Klei\u00b7nod", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Meisterschaft.", "tokens": ["Und", "die", "Meis\u00b7ter\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alles dies in ", "tokens": ["Al\u00b7les", "dies", "in"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PDS", "APPR"], "meter": "+---", "measure": "dactylic.init"}, "line.2": {"text": "Keiner ging noch aus ihm aus,", "tokens": ["Kei\u00b7ner", "ging", "noch", "aus", "ihm", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der das Gl\u00fcck nicht pries,", "tokens": ["Der", "das", "Gl\u00fcck", "nicht", "pries", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Das ihn hier in engem Raum", "tokens": ["Das", "ihn", "hier", "in", "en\u00b7gem", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Einen guten klaren Traum", "tokens": ["Ei\u00b7nen", "gu\u00b7ten", "kla\u00b7ren", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leibhaft sehen lie\u00df.", "tokens": ["Leib\u00b7haft", "se\u00b7hen", "lie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}