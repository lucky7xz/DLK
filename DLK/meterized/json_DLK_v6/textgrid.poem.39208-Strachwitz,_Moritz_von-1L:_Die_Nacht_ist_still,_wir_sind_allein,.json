{"textgrid.poem.39208": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist still, wir sind allein,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist still, wir sind allein,", "tokens": ["Die", "Nacht", "ist", "still", ",", "wir", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Du bist sch\u00f6n, wie je!", "tokens": ["Und", "Du", "bist", "sch\u00f6n", ",", "wie", "je", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum denn k\u00fc\u00dft die Lippe mein", "tokens": ["Wa\u00b7rum", "denn", "k\u00fc\u00dft", "die", "Lip\u00b7pe", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht halb so gut, wie eh'?", "tokens": ["Nicht", "halb", "so", "gut", ",", "wie", "eh'", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Warum ist denn mein Mund so karg,", "tokens": ["Wa\u00b7rum", "ist", "denn", "mein", "Mund", "so", "karg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Aug' so flammenarm,", "tokens": ["Mein", "Aug'", "so", "flam\u00b7me\u00b7narm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als l\u00e4g' ich in dem schw\u00e4rz'sten Sarg,", "tokens": ["Als", "l\u00e4g'", "ich", "in", "dem", "schw\u00e4rz'\u00b7sten", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Statt in dem wei\u00df'sten Arm?", "tokens": ["Statt", "in", "dem", "wei\u00df'\u00b7sten", "Arm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Warum ist denn meine Liebe so lau,", "tokens": ["Wa\u00b7rum", "ist", "denn", "mei\u00b7ne", "Lie\u00b7be", "so", "lau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mein Herz so leer, so leer?", "tokens": ["Mein", "Herz", "so", "leer", ",", "so", "leer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Gott, ich f\u00fcrchte, gn\u00e4d'ge Frau,", "tokens": ["Bei", "Gott", ",", "ich", "f\u00fcrch\u00b7te", ",", "gn\u00e4d'\u00b7ge", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich \u2013 liebe Sie nicht mehr! \u2013", "tokens": ["Ich", "\u2013", "lie\u00b7be", "Sie", "nicht", "mehr", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$(", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}