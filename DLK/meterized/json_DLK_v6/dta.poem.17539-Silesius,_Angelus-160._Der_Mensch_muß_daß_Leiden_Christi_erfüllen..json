{"dta.poem.17539": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "160.  Der Mensch mu\u00df da\u00df Leiden  \n Christi erf\u00fcllen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch du solst Paulus seyn/ und in dir selbst erf\u00fcllen/", "tokens": ["Mensch", "du", "solst", "Pau\u00b7lus", "seyn", "/", "und", "in", "dir", "selbst", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "NE", "VAINF", "$(", "KON", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Christus nicht gethan/ wo sich der zorn sol stillen.", "tokens": ["Was", "Chris\u00b7tus", "nicht", "ge\u00b7than", "/", "wo", "sich", "der", "zorn", "sol", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PTKNEG", "VVPP", "$(", "PWAV", "PRF", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}