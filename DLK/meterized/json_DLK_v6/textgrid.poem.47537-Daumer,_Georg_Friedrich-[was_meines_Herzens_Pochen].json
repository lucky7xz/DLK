{"textgrid.poem.47537": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[was meines Herzens Pochen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was meines Herzens Pochen,", "tokens": ["Was", "mei\u00b7nes", "Her\u00b7zens", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das m\u00e4chtige, bedeute?", "tokens": ["Das", "m\u00e4ch\u00b7ti\u00b7ge", ",", "be\u00b7deu\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es liebt Hafis, der alte,", "tokens": ["Es", "liebt", "Ha\u00b7fis", ",", "der", "al\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ehedem, noch heute.", "tokens": ["Wie", "e\u00b7he\u00b7dem", ",", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Selma's s\u00fc\u00dfes \u00c4ugeln.", "tokens": ["Und", "Sel\u00b7ma's", "s\u00fc\u00b7\u00dfes", "\u00c4u\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Inmitten aller Leute?", "tokens": ["In\u00b7mit\u00b7ten", "al\u00b7ler", "Leu\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wird geliebt, der gute,", "tokens": ["Er", "wird", "ge\u00b7liebt", ",", "der", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ehedem, noch heute.", "tokens": ["Wie", "e\u00b7he\u00b7dem", ",", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbdu bist ja bald, du bist ja", "tokens": ["\u00bb", "du", "bist", "ja", "bald", ",", "du", "bist", "ja"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fast schon des Grabes Beute!\u00ab", "tokens": ["Fast", "schon", "des", "Gra\u00b7bes", "Beu\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jung ist das Herz, das hei\u00dfe,", "tokens": ["Jung", "ist", "das", "Herz", ",", "das", "hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ehedem, noch heute.", "tokens": ["Wie", "e\u00b7he\u00b7dem", ",", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ob die Trauerglocke", "tokens": ["Und", "ob", "die", "Trau\u00b7er\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir schon zu Grabe l\u00e4ute \u2013", "tokens": ["Mir", "schon", "zu", "Gra\u00b7be", "l\u00e4u\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich singe, trinke, k\u00fcsse,", "tokens": ["Ich", "sin\u00b7ge", ",", "trin\u00b7ke", ",", "k\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ehedem, noch heute.", "tokens": ["Wie", "e\u00b7he\u00b7dem", ",", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}