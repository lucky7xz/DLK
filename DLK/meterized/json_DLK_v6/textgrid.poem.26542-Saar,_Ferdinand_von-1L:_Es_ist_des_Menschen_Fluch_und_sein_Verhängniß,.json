{"textgrid.poem.26542": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist des Menschen Fluch und sein Verh\u00e4ngni\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist des Menschen Fluch und sein Verh\u00e4ngni\u00df,", "tokens": ["Es", "ist", "des", "Men\u00b7schen", "Fluch", "und", "sein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df seine Fehler sicher wirkend schreiten", "tokens": ["Da\u00df", "sei\u00b7ne", "Feh\u00b7ler", "si\u00b7cher", "wir\u00b7kend", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, offenkundig rings, ihm gleich bereiten", "tokens": ["Und", ",", "of\u00b7fen\u00b7kun\u00b7dig", "rings", ",", "ihm", "gleich", "be\u00b7rei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "ADV", "$,", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jedweden Schmerz und jegliche Bedr\u00e4ngni\u00df.", "tokens": ["Jed\u00b7we\u00b7den", "Schmerz", "und", "jeg\u00b7li\u00b7che", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sein Bestes aber lebt wie im Gef\u00e4ngni\u00df", "tokens": ["Sein", "Bes\u00b7tes", "a\u00b7ber", "lebt", "wie", "im", "Ge\u00b7f\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Tugenden sind Heimlichkeiten;", "tokens": ["Und", "sei\u00b7ne", "Tu\u00b7gen\u00b7den", "sind", "Heim\u00b7lich\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er selber mu\u00df sie zweifelnd oft bestreiten,", "tokens": ["Er", "sel\u00b7ber", "mu\u00df", "sie", "zwei\u00b7felnd", "oft", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rauh \u00fcberlassen seiner Herzensb\u00e4ngni\u00df.", "tokens": ["Rauh", "\u00fc\u00b7ber\u00b7las\u00b7sen", "sei\u00b7ner", "Her\u00b7zens\u00b7b\u00e4ng\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn diese Welt, so rasch im Schulderkennen,", "tokens": ["Denn", "die\u00b7se", "Welt", ",", "so", "rasch", "im", "Schul\u00b7der\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So gern bereit, werkth\u00e4tig sich zu zeigen,", "tokens": ["So", "gern", "be\u00b7reit", ",", "werk\u00b7th\u00e4\u00b7tig", "sich", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sobald es gilt, ein Schandmal aufzubrennen:", "tokens": ["So\u00b7bald", "es", "gilt", ",", "ein", "Schand\u00b7mal", "auf\u00b7zu\u00b7bren\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie h\u00fcllt sich allsogleich in starres Schweigen,", "tokens": ["Sie", "h\u00fcllt", "sich", "all\u00b7so\u00b7gleich", "in", "star\u00b7res", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll sie ein \u00e4cht Verdienst beim Namen nennen", "tokens": ["Soll", "sie", "ein", "\u00e4cht", "Ver\u00b7dienst", "beim", "Na\u00b7men", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "NN", "APPRART", "NN", "VVINF"], "meter": "---+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und einem hohen Wollen sich verneigen.", "tokens": ["Und", "ei\u00b7nem", "ho\u00b7hen", "Wol\u00b7len", "sich", "ver\u00b7nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}