{"dta.poem.2244": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchle, da\u00df von vielen Zweifeln die gro\u00dfe Wahr-", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "da\u00df", "von", "vie\u00b7len", "Zwei\u00b7feln", "die", "gro\u00b7\u00dfe", "Wahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "PIAT", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "heit mich entlade:", "tokens": ["heit", "mich", "ent\u00b7la\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es stammt die Ordnung der Natur sowohl aus Gott,", "tokens": ["Es", "stammt", "die", "Ord\u00b7nung", "der", "Na\u00b7tur", "so\u00b7wohl", "aus", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als wie die Gnade.", "tokens": ["als", "wie", "die", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}