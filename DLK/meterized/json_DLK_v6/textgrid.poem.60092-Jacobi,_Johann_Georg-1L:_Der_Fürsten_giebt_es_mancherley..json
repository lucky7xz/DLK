{"textgrid.poem.60092": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der F\u00fcrsten giebt es mancherley.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der F\u00fcrsten giebt es mancherley.", "tokens": ["Der", "F\u00fcrs\u00b7ten", "giebt", "es", "man\u00b7cher\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf stolzen Alpen, k\u00fchn und frey,", "tokens": ["Auf", "stol\u00b7zen", "Al\u00b7pen", ",", "k\u00fchn", "und", "frey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erbaut der Adler seinen Sitz,", "tokens": ["Er\u00b7baut", "der", "Ad\u00b7ler", "sei\u00b7nen", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sendet weit der Augen Blitz", "tokens": ["Und", "sen\u00b7det", "weit", "der", "Au\u00b7gen", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Umher in hoher, hoher Luft,", "tokens": ["Um\u00b7her", "in", "ho\u00b7her", ",", "ho\u00b7her", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wittert in die ferne Kluft.", "tokens": ["Und", "wit\u00b7tert", "in", "die", "fer\u00b7ne", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er fliegt, und schreckt, und k\u00e4mpft, und schafft,", "tokens": ["Er", "fliegt", ",", "und", "schreckt", ",", "und", "k\u00e4mpft", ",", "und", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was ihn gel\u00fcstet, heldenhaft,", "tokens": ["Was", "ihn", "ge\u00b7l\u00fcs\u00b7tet", ",", "hel\u00b7den\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit eignem Willen, eigner Kraft.", "tokens": ["Mit", "eig\u00b7nem", "Wil\u00b7len", ",", "eig\u00b7ner", "Kraft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Karten-K\u00f6nig, so genannt", "tokens": ["Der", "Kar\u00b7ten\u00b7K\u00f6\u00b7nig", ",", "so", "ge\u00b7nannt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob seinem Zepter in der Hand,", "tokens": ["Ob", "sei\u00b7nem", "Zep\u00b7ter", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob seinem farbigen Talar,", "tokens": ["Ob", "sei\u00b7nem", "far\u00b7bi\u00b7gen", "Ta\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ob der Hofbedienten Schaar,", "tokens": ["Und", "ob", "der", "Hof\u00b7be\u00b7dien\u00b7ten", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die alle zu Geboth ihm stehn,", "tokens": ["Die", "al\u00b7le", "zu", "Ge\u00b7both", "ihm", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und, wenn er's fordert, mit ihm gehn \u2013", "tokens": ["Und", ",", "wenn", "er's", "for\u00b7dert", ",", "mit", "ihm", "gehn", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "VVFIN", "$,", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Karten-K\u00f6nig, taub und blind,", "tokens": ["Der", "Kar\u00b7ten\u00b7K\u00f6\u00b7nig", ",", "taub", "und", "blind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist Karte, wie die andern sind,", "tokens": ["Ist", "Kar\u00b7te", ",", "wie", "die", "an\u00b7dern", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PWAV", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Stolzieret nur in solcher Pracht,", "tokens": ["Stol\u00b7zie\u00b7ret", "nur", "in", "sol\u00b7cher", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Weil ihn der Mahler bunt gemacht;", "tokens": ["Weil", "ihn", "der", "Mah\u00b7ler", "bunt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Thut selber nichts, ist immer Knecht,", "tokens": ["Thut", "sel\u00b7ber", "nichts", ",", "ist", "im\u00b7mer", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIS", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bey Guten gut, bey Schlechten schlecht,", "tokens": ["Bey", "Gu\u00b7ten", "gut", ",", "bey", "Schlech\u00b7ten", "schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und, je nachdem sein Loos ihm fiel,", "tokens": ["Und", ",", "je", "nach\u00b7dem", "sein", "Loos", "ihm", "fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Klugen und der Narren Spiel.", "tokens": ["Der", "Klu\u00b7gen", "und", "der", "Nar\u00b7ren", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}