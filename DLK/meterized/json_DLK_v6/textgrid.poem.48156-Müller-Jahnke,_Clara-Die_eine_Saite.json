{"textgrid.poem.48156": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die eine Saite", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder spielt der Abend auf den Wogen", "tokens": ["Und", "wie\u00b7der", "spielt", "der", "A\u00b7bend", "auf", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in seiner herbstlich sonnenroten Pracht.", "tokens": ["in", "sei\u00b7ner", "herbst\u00b7lich", "son\u00b7nen\u00b7ro\u00b7ten", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf goldner Stra\u00dfe kommt dahergezogen", "tokens": ["Auf", "gold\u00b7ner", "Stra\u00b7\u00dfe", "kommt", "da\u00b7her\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die stille Sehnsucht, die so selig macht.", "tokens": ["die", "stil\u00b7le", "Sehn\u00b7sucht", ",", "die", "so", "se\u00b7lig", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In lila Purpurd\u00e4mmrung tr\u00e4umt der Strand.", "tokens": ["In", "li\u00b7la", "Pur\u00b7pur\u00b7d\u00e4mm\u00b7rung", "tr\u00e4umt", "der", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein lauer Wind aus rosenroter Weite:", "tokens": ["Ein", "lau\u00b7er", "Wind", "aus", "ro\u00b7sen\u00b7ro\u00b7ter", "Wei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und mir im Herzen t\u00f6nt die eine Saite, \u2013", "tokens": ["und", "mir", "im", "Her\u00b7zen", "t\u00f6nt", "die", "ei\u00b7ne", "Sai\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "ART", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 die du gespannt.", "tokens": ["\u2013", "die", "du", "ge\u00b7spannt", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}