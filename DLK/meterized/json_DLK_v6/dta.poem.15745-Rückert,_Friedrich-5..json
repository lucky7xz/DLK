{"dta.poem.15745": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wohlgestalt ist sch\u00f6n in jeglichem Gewande,", "tokens": ["Die", "Wohl\u00b7ge\u00b7stalt", "ist", "sch\u00f6n", "in", "jeg\u00b7li\u00b7chem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am sch\u00f6nsten ist sie nackt, doch nur im Unschuldstande.", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "ist", "sie", "nackt", ",", "doch", "nur", "im", "Un\u00b7schuld\u00b7stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Alter kann zur\u00fcck zur Kindesunschuld kehren,", "tokens": ["Das", "Al\u00b7ter", "kann", "zu\u00b7r\u00fcck", "zur", "Kin\u00b7des\u00b7un\u00b7schuld", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur soweit nicht um auch des Kleides zu entbehren.", "tokens": ["Nur", "so\u00b7weit", "nicht", "um", "auch", "des", "Klei\u00b7des", "zu", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "APPR", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch Kindeseinfalt des Gedankens liebt Bekleidung,", "tokens": ["Auch", "Kin\u00b7de\u00b7sein\u00b7falt", "des", "Ge\u00b7dan\u00b7kens", "liebt", "Be\u00b7klei\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Denn erst das Kleid gibt ihm anmuth'ge Unterscheidung.", "tokens": ["Denn", "erst", "das", "Kleid", "gibt", "ihm", "an\u00b7muth'\u00b7ge", "Un\u00b7ter\u00b7schei\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Man h\u00e4lt zum Werktagkleid sich an die Landesart,", "tokens": ["Man", "h\u00e4lt", "zum", "Werk\u00b7tag\u00b7kleid", "sich", "an", "die", "Lan\u00b7des\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lustverkleidung bleibt dem Festtag aufgespart.", "tokens": ["Die", "Lust\u00b7ver\u00b7klei\u00b7dung", "bleibt", "dem", "Fest\u00b7tag", "auf\u00b7ge\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Man mag Bekanntes gern in fremder H\u00fclle sehn,", "tokens": ["Man", "mag", "Be\u00b7kann\u00b7tes", "gern", "in", "frem\u00b7der", "H\u00fcl\u00b7le", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es zugleich so fern und nahe scheint zu stehn.", "tokens": ["Weil", "es", "zu\u00b7gleich", "so", "fern", "und", "na\u00b7he", "scheint", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum liebt der Sch\u00f6nheit Glanz viel wechselnde Gewande,", "tokens": ["Drum", "liebt", "der", "Sch\u00f6n\u00b7heit", "Glanz", "viel", "wech\u00b7seln\u00b7de", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Weil keins allein ihn ganz zu fassen ist im Stande.", "tokens": ["Weil", "keins", "al\u00b7lein", "ihn", "ganz", "zu", "fas\u00b7sen", "ist", "im", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Durch andres Kleid erh\u00e4lt der Leib auch andre Haltung,", "tokens": ["Durch", "and\u00b7res", "Kleid", "er\u00b7h\u00e4lt", "der", "Leib", "auch", "and\u00b7re", "Hal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede neue Falt' ist neuer Reiz' Entfaltung.", "tokens": ["Und", "je\u00b7de", "neu\u00b7e", "Falt'", "ist", "neu\u00b7er", "Rei\u00b7z'", "Ent\u00b7fal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Das Fremde nur ist sch\u00f6n, das Fremde nur gef\u00e4llt,", "tokens": ["Das", "Frem\u00b7de", "nur", "ist", "sch\u00f6n", ",", "das", "Frem\u00b7de", "nur", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das eigenth\u00fcmlich dar ein Allgemeines stellt.", "tokens": ["Das", "ei\u00b7gent\u00b7h\u00fcm\u00b7lich", "dar", "ein", "All\u00b7ge\u00b7mei\u00b7nes", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PTKVZ", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wo dem Besondern fehlt und Fremden diese Spur,", "tokens": ["Wo", "dem", "Be\u00b7son\u00b7dern", "fehlt", "und", "Frem\u00b7den", "die\u00b7se", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das meid' als sonderbar und als befremdlich nur.", "tokens": ["Das", "meid'", "als", "son\u00b7der\u00b7bar", "und", "als", "be\u00b7fremd\u00b7lich", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "KOKOM", "ADJD", "KON", "KOUS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}