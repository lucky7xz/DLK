{"textgrid.poem.38733": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willt du dich zur Reis' bequemen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willt du dich zur Reis' bequemen", "tokens": ["Willt", "du", "dich", "zur", "Reis'", "be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Feld", "tokens": ["Ue\u00b7ber", "Feld"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Berg und Thal", "tokens": ["Berg", "und", "Thal"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Durch die Welt,", "tokens": ["Durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Fremde St\u00e4dte allzumal,", "tokens": ["Frem\u00b7de", "St\u00e4d\u00b7te", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dft Gesundheit mit dir nehmen.", "tokens": ["Mu\u00dft", "Ge\u00b7sund\u00b7heit", "mit", "dir", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Neue Freunde aufzufinden", "tokens": ["Neu\u00b7e", "Freun\u00b7de", "auf\u00b7zu\u00b7fin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft die alten du dahinten,", "tokens": ["L\u00e4\u00dft", "die", "al\u00b7ten", "du", "da\u00b7hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch am Morgen bist du wach,", "tokens": ["Fr\u00fch", "am", "Mor\u00b7gen", "bist", "du", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher sieht dem Wandrer nach", "tokens": ["Man\u00b7cher", "sieht", "dem", "Wand\u00b7rer", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weint dahinten,", "tokens": ["Weint", "da\u00b7hin\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Kann die Freud' nicht wiederfinden.", "tokens": ["Kann", "die", "Freud'", "nicht", "wie\u00b7der\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eltern, Schwester, Bruder, Freund,", "tokens": ["El\u00b7tern", ",", "Schwes\u00b7ter", ",", "Bru\u00b7der", ",", "Freund", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch vielleicht das Liebchen weint;", "tokens": ["Auch", "viel\u00b7leicht", "das", "Lieb\u00b7chen", "weint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie weinen, traurig und froh", "tokens": ["La\u00df", "sie", "wei\u00b7nen", ",", "trau\u00b7rig", "und", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wechselt das Leben bald so bald so", "tokens": ["Wech\u00b7selt", "das", "Le\u00b7ben", "bald", "so", "bald", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "ADV"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nimmer ohne Ach! und O!", "tokens": ["Nim\u00b7mer", "oh\u00b7ne", "Ach", "!", "und", "O", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heimath bleibt dir treu und bieder,", "tokens": ["Hei\u00b7math", "bleibt", "dir", "treu", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kehrst du nur als Treuer wieder,", "tokens": ["Kehrst", "du", "nur", "als", "Treu\u00b7er", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reisen und Scheiden", "tokens": ["Rei\u00b7sen", "und", "Schei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Bringt des Wiedersehens Freuden.", "tokens": ["Bringt", "des", "Wie\u00b7der\u00b7se\u00b7hens", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}