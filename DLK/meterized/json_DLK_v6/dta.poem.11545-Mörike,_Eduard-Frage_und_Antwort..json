{"dta.poem.11545": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Frage und Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe mir zum Herzen kam,", "tokens": ["Lie\u00b7be", "mir", "zum", "Her\u00b7zen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und warum ich ihr nicht lange", "tokens": ["Und", "wa\u00b7rum", "ich", "ihr", "nicht", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "PTKNEG", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Schon den bittern Stachel nahm?", "tokens": ["Schon", "den", "bit\u00b7tern", "Sta\u00b7chel", "nahm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprich, warum mit Geisterschnelle", "tokens": ["Sprich", ",", "wa\u00b7rum", "mit", "Geis\u00b7ter\u00b7schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl der Wind die Fl\u00fcgel r\u00fchrt,", "tokens": ["Wohl", "der", "Wind", "die", "Fl\u00fc\u00b7gel", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und woher die s\u00fc\u00dfe Quelle", "tokens": ["Und", "wo\u00b7her", "die", "s\u00fc\u00b7\u00dfe", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verborgnen Wasser f\u00fchrt?", "tokens": ["Die", "ver\u00b7borg\u00b7nen", "Was\u00b7ser", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Banne du auf seiner F\u00e4hrte", "tokens": ["Ban\u00b7ne", "du", "auf", "sei\u00b7ner", "F\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir den Wind in vollem Lauf!", "tokens": ["Mir", "den", "Wind", "in", "vol\u00b7lem", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halte mit der Zaubergerte", "tokens": ["Hal\u00b7te", "mit", "der", "Zau\u00b7ber\u00b7ger\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du die s\u00fc\u00dfen Quellen auf!", "tokens": ["Du", "die", "s\u00fc\u00b7\u00dfen", "Quel\u00b7len", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}