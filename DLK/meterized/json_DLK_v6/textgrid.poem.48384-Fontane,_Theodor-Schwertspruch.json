{"textgrid.poem.48384": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schwertspruch", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter allen Lords in meinem Reich", "tokens": ["Un\u00b7ter", "al\u00b7len", "Lords", "in", "mei\u00b7nem", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "War keiner doch dem Douglas gleich.", "tokens": ["War", "kei\u00b7ner", "doch", "dem", "Doug\u00b7las", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum trag du, wenn ich gestorben bin,", "tokens": ["Drum", "trag", "du", ",", "wenn", "ich", "ge\u00b7stor\u00b7ben", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Herz zum heiligen Grabe hin.", "tokens": ["Mein", "Herz", "zum", "hei\u00b7li\u00b7gen", "Gra\u00b7be", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort mag es liegen tief und still,", "tokens": ["Dort", "mag", "es", "lie\u00b7gen", "tief", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis mein Erl\u00f6ser es wecken will.", "tokens": ["Bis", "mein", "Er\u00b7l\u00f6\u00b7ser", "es", "we\u00b7cken", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein besserer Ritter bis diese Stund'", "tokens": ["Ein", "bes\u00b7se\u00b7rer", "Rit\u00b7ter", "bis", "die\u00b7se", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An keines K\u00f6nigs Seite stund.", "tokens": ["An", "kei\u00b7nes", "K\u00f6\u00b7nigs", "Sei\u00b7te", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}