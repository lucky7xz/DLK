{"textgrid.poem.60069": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stets in Lieb einander nah,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets in Lieb einander nah,", "tokens": ["Stets", "in", "Lieb", "ein\u00b7an\u00b7der", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schn\u00e4beln meine T\u00e4ubchen da", "tokens": ["Schn\u00e4\u00b7beln", "mei\u00b7ne", "T\u00e4ub\u00b7chen", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mit innigem Vertrauen;", "tokens": ["Sich", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "F\u00fcrchten keine M\u00f6rder-Klauen;", "tokens": ["F\u00fcrch\u00b7ten", "kei\u00b7ne", "M\u00f6r\u00b7der\u00b7Klau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Finden reichlich allezeit", "tokens": ["Fin\u00b7den", "reich\u00b7lich", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre Nahrung hingestreut;", "tokens": ["Ih\u00b7re", "Nah\u00b7rung", "hin\u00b7ge\u00b7streut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6chten solche Seligkeit", "tokens": ["M\u00f6ch\u00b7ten", "sol\u00b7che", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht dem K\u00f6nig Adler geben", "tokens": ["Nicht", "dem", "K\u00f6\u00b7nig", "Ad\u00b7ler", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "F\u00fcr sein ganzes F\u00fcrstenleben.", "tokens": ["F\u00fcr", "sein", "gan\u00b7zes", "F\u00fcrs\u00b7ten\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wonne-K\u00fcsse dieser Art", "tokens": ["Won\u00b7ne\u00b7K\u00fcs\u00b7se", "die\u00b7ser", "Art"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenke, so wie meinem T\u00e4ubchen,", "tokens": ["Schen\u00b7ke", ",", "so", "wie", "mei\u00b7nem", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er, der alles wohl gepaart,", "tokens": ["Er", ",", "der", "al\u00b7les", "wohl", "ge\u00b7paart", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcnftig uns und unsern Weibchen!", "tokens": ["K\u00fcnf\u00b7tig", "uns", "und", "un\u00b7sern", "Weib\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}