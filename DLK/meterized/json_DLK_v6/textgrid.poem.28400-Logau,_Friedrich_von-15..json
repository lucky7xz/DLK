{"textgrid.poem.28400": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilff Gott, da\u00df mir geschieht, als wie ich glaub und traue;", "tokens": ["Hilff", "Gott", ",", "da\u00df", "mir", "ge\u00b7schieht", ",", "als", "wie", "ich", "glaub", "und", "trau\u00b7e", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df noch mein Auge Lust an deinen Siegen schaue!", "tokens": ["Da\u00df", "noch", "mein", "Au\u00b7ge", "Lust", "an", "dei\u00b7nen", "Sie\u00b7gen", "schau\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Fall du wilst, geh\u00f6rt ein eintzig Wort dazu,", "tokens": ["Im", "Fall", "du", "wilst", ",", "ge\u00b7h\u00f6rt", "ein", "eint\u00b7zig", "Wort", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VMFIN", "$,", "VVFIN", "ART", "ADJD", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat der Frevler Pein, so hat der Frome Ruh.", "tokens": ["So", "hat", "der", "Frev\u00b7ler", "Pein", ",", "so", "hat", "der", "Fro\u00b7me", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}