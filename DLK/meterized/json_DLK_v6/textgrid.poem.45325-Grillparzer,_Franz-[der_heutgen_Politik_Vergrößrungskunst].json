{"textgrid.poem.45325": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der heutgen Politik Vergr\u00f6\u00dfrungskunst]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der heutgen Politik Vergr\u00f6\u00dfrungskunst", "tokens": ["Der", "heut\u00b7gen", "Po\u00b7li\u00b7tik", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7kunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist Diebstahl w\u00e4hrend einer Feuersbrunst.", "tokens": ["Ist", "Dieb\u00b7stahl", "w\u00e4h\u00b7rend", "ei\u00b7ner", "Feu\u00b7ers\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So haben sie einst sich Schlesien gestohlen", "tokens": ["So", "ha\u00b7ben", "sie", "einst", "sich", "Schle\u00b7si\u00b7en", "ge\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PRF", "NE", "VVPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und m\u00f6chten nun noch das \u00dcbrige holen.", "tokens": ["Und", "m\u00f6ch\u00b7ten", "nun", "noch", "das", "\u00dcb\u00b7ri\u00b7ge", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch bringst du den Raub unter sicheres Dach,", "tokens": ["Doch", "bringst", "du", "den", "Raub", "un\u00b7ter", "si\u00b7che\u00b7res", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Flamme hat Fl\u00fcgel und kommt dir nach.", "tokens": ["Die", "Flam\u00b7me", "hat", "Fl\u00fc\u00b7gel", "und", "kommt", "dir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Die Ehre verbeuts, hei\u00dft im Munde der Narren", "tokens": ["Die", "Eh\u00b7re", "ver\u00b7beuts", ",", "hei\u00dft", "im", "Mun\u00b7de", "der", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In seiner eigenen Schande beharren.", "tokens": ["In", "sei\u00b7ner", "ei\u00b7ge\u00b7nen", "Schan\u00b7de", "be\u00b7har\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}