{"textgrid.poem.66292": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Am heiligen Pfingstfeste", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, blaset die Posaune an!", "tokens": ["Auf", ",", "bla\u00b7set", "die", "Po\u00b7sau\u00b7ne", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon r\u00fcckt der Tag des Herrn heran;", "tokens": ["Schon", "r\u00fcckt", "der", "Tag", "des", "Herrn", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wolkiger, ein finstrer Tag,", "tokens": ["Ein", "wol\u00b7ki\u00b7ger", ",", "ein", "finst\u00b7rer", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo kein Gesch\u00f6pf bestehen mag,", "tokens": ["Wo", "kein", "Ge\u00b7sch\u00f6pf", "be\u00b7ste\u00b7hen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es f\u00e4hrt der gro\u00dfe Donnerer", "tokens": ["Es", "f\u00e4hrt", "der", "gro\u00b7\u00dfe", "Don\u00b7ne\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Auf seinem Wolkensitz daher.", "tokens": ["Auf", "sei\u00b7nem", "Wol\u00b7ken\u00b7sitz", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verzehrend Feuer geht vor ihm,", "tokens": ["Ver\u00b7zeh\u00b7rend", "Feu\u00b7er", "geht", "vor", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Flammen z\u00fccken hinter ihm.", "tokens": ["Und", "Flam\u00b7men", "z\u00fc\u00b7cken", "hin\u00b7ter", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was vor ihm sch\u00f6n, wie Eden steht,", "tokens": ["Was", "vor", "ihm", "sch\u00f6n", ",", "wie", "E\u00b7den", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADJD", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nach ihm, wie die W\u00fcste, \u00f6d!", "tokens": ["Ist", "nach", "ihm", ",", "wie", "die", "W\u00fcs\u00b7te", ",", "\u00f6d", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hoch rasseln seine Wagen her,", "tokens": ["Hoch", "ras\u00b7seln", "sei\u00b7ne", "Wa\u00b7gen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und aus Gewittern redet er:", "tokens": ["Und", "aus", "Ge\u00b7wit\u00b7tern", "re\u00b7det", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bekehret euch, ihr S\u00fcnder, eilt!", "tokens": ["Be\u00b7keh\u00b7ret", "euch", ",", "ihr", "S\u00fcn\u00b7der", ",", "eilt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fastet! klaget! weinet! heult!", "tokens": ["Und", "fas\u00b7tet", "!", "kla\u00b7get", "!", "wei\u00b7net", "!", "heult", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bekehret euch noch in der Zeit", "tokens": ["Be\u00b7keh\u00b7ret", "euch", "noch", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Vater der Barmherzigkeit.", "tokens": ["Zum", "Va\u00b7ter", "der", "Barm\u00b7her\u00b7zig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voll Gnade, voll Geduld bin ich,", "tokens": ["Voll", "Gna\u00b7de", ",", "voll", "Ge\u00b7duld", "bin", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Zorn und Strafe reuet mich.", "tokens": ["Und", "Zorn", "und", "Stra\u00b7fe", "reu\u00b7et", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich gie\u00dfe \u00fcber euch den Geist,", "tokens": ["Ich", "gie\u00b7\u00dfe", "\u00fc\u00b7ber", "euch", "den", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der euch der Wahrheit Pfade weist;", "tokens": ["Der", "euch", "der", "Wahr\u00b7heit", "Pfa\u00b7de", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch den das Kind prophetisch gl\u00fcht,", "tokens": ["Durch", "den", "das", "Kind", "pro\u00b7phe\u00b7tisch", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Alte gro\u00dfe Tr\u00e4ume sieht;", "tokens": ["Der", "Al\u00b7te", "gro\u00b7\u00dfe", "Tr\u00e4u\u00b7me", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch den vom himmlischen Gesicht", "tokens": ["Durch", "den", "vom", "himm\u00b7li\u00b7schen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der J\u00fcngling mit Entz\u00fccken spricht.", "tokens": ["Der", "J\u00fcng\u00b7ling", "mit", "Ent\u00b7z\u00fc\u00b7cken", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Donner schwieg. Der neue Bund", "tokens": ["Sein", "Don\u00b7ner", "schwieg", ".", "Der", "neu\u00b7e", "Bund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "That die verhei\u00df'ne Gnade kund.", "tokens": ["That", "die", "ver\u00b7hei\u00df'\u00b7ne", "Gna\u00b7de", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auf Fromme go\u00df sein Geist sich aus,", "tokens": ["Auf", "From\u00b7me", "go\u00df", "sein", "Geist", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Rauch und Dampf erf\u00fcllt das Haus,", "tokens": ["Und", "Rauch", "und", "Dampf", "er\u00b7f\u00fcllt", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worinnen die verscheuchte Schaar", "tokens": ["Wo\u00b7rin\u00b7nen", "die", "ver\u00b7scheuch\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der treuen J\u00fcnger Christi war.", "tokens": ["Der", "treu\u00b7en", "J\u00fcn\u00b7ger", "Chris\u00b7ti", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du Hauch vom Vater, komm herab", "tokens": ["Du", "Hauch", "vom", "Va\u00b7ter", ",", "komm", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "APPRART", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem, der dich der Erde gab;", "tokens": ["Von", "dem", ",", "der", "dich", "der", "Er\u00b7de", "gab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Flamme Gottes, ruh' auf mir,", "tokens": ["Du", "Flam\u00b7me", "Got\u00b7tes", ",", "ruh'", "auf", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im tiefsten Staube wart' ich hier,", "tokens": ["Im", "tiefs\u00b7ten", "Stau\u00b7be", "wart'", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis Feuer auf der Zunge glimmt", "tokens": ["Bis", "Feu\u00b7er", "auf", "der", "Zun\u00b7ge", "glimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sie zu deinem Lobe stimmt.", "tokens": ["Und", "sie", "zu", "dei\u00b7nem", "Lo\u00b7be", "stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des Frommen Trost, entzieh' mir nicht", "tokens": ["Des", "From\u00b7men", "Trost", ",", "ent\u00b7zieh'", "mir", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein gnadestrahlendes Gesicht.", "tokens": ["Dein", "gna\u00b7des\u00b7trah\u00b7len\u00b7des", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch tobt in mir der S\u00fcndenschmerz:", "tokens": ["Noch", "tobt", "in", "mir", "der", "S\u00fcn\u00b7den\u00b7schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum schaff' in mir ein reines Herz,", "tokens": ["Drum", "schaff'", "in", "mir", "ein", "rei\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sei mir ein gewisser Geist,", "tokens": ["Und", "sei", "mir", "ein", "ge\u00b7wis\u00b7ser", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mir die Bahn der Tugend weist.", "tokens": ["Der", "mir", "die", "Bahn", "der", "Tu\u00b7gend", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du Licht aus Gott, erleuchte mich,", "tokens": ["Du", "Licht", "aus", "Gott", ",", "er\u00b7leuch\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mein Verstand erkenne dich;", "tokens": ["Da\u00df", "mein", "Ver\u00b7stand", "er\u00b7ken\u00b7ne", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wann ich sinke, reiche du", "tokens": ["Und", "wann", "ich", "sin\u00b7ke", ",", "rei\u00b7che", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wahrheit mir zur St\u00fctze zu;", "tokens": ["Die", "Wahr\u00b7heit", "mir", "zur", "St\u00fct\u00b7ze", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sei die Welt von Zweifeln voll,", "tokens": ["Dann", "sei", "die", "Welt", "von", "Zwei\u00b7feln", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df, an wen ich glauben soll.", "tokens": ["Ich", "wei\u00df", ",", "an", "wen", "ich", "glau\u00b7ben", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn mein Gewissen mich verklagt,", "tokens": ["Wenn", "mein", "Ge\u00b7wis\u00b7sen", "mich", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz belastet in mir zagt;", "tokens": ["Mein", "Herz", "be\u00b7las\u00b7tet", "in", "mir", "zagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fl\u00f6\u00dfe mir die Engellust", "tokens": ["So", "fl\u00f6\u00b7\u00dfe", "mir", "die", "En\u00b7gel\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Seelenfriedens in die Brust,", "tokens": ["Des", "See\u00b7len\u00b7frie\u00b7dens", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und s\u00e4usle wieder nach dem Schmerz", "tokens": ["Und", "s\u00e4us\u00b7le", "wie\u00b7der", "nach", "dem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir Ruh' und Heiterkeit ins Herz.", "tokens": ["Mir", "Ruh'", "und", "Hei\u00b7ter\u00b7keit", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mach du mich deiner Gnade voll,", "tokens": ["Mach", "du", "mich", "dei\u00b7ner", "Gna\u00b7de", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "ADJD", 