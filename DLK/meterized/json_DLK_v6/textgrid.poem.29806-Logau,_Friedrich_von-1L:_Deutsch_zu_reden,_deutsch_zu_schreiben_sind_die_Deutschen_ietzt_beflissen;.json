{"textgrid.poem.29806": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deutsch zu reden, deutsch zu schreiben sind die Deutschen ietzt beflissen;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsch zu reden, deutsch zu schreiben sind die Deutschen ietzt beflissen;", "tokens": ["Deutsch", "zu", "re\u00b7den", ",", "deutsch", "zu", "schrei\u00b7ben", "sind", "die", "Deut\u00b7schen", "ietzt", "be\u00b7flis\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie sie sich recht deutsch bekleiden, k\u00fcnnen sie zur Zeit nicht wissen,", "tokens": ["Wie", "sie", "sich", "recht", "deutsch", "be\u00b7klei\u00b7den", ",", "k\u00fcn\u00b7nen", "sie", "zur", "Zeit", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Bi\u00df zum kleiden, wie zum reden, eine Gno\u00dfschafft sie beschlissen.", "tokens": ["Bi\u00df", "zum", "klei\u00b7den", ",", "wie", "zum", "re\u00b7den", ",", "ei\u00b7ne", "Gno\u00df\u00b7schafft", "sie", "be\u00b7schlis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "$,", "PWAV", "APPRART", "VVINF", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+