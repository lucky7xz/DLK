{"textgrid.poem.36980": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Der Sultan und sein Hof", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lotte klopfte; klopfte n\u00e4mlich, weil man drau\u00dfen rief: Herein!", "tokens": ["Lot\u00b7te", "klopf\u00b7te", ";", "klopf\u00b7te", "n\u00e4m\u00b7lich", ",", "weil", "man", "drau\u00b7\u00dfen", "rief", ":", "Her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$.", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und es traten das Kinderm\u00e4dchen, das bei Sultans diente, ein,", "tokens": ["Und", "es", "tra\u00b7ten", "das", "Kin\u00b7der\u00b7m\u00e4d\u00b7chen", ",", "das", "bei", "Sul\u00b7tans", "dien\u00b7te", ",", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "--+--+-+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf dem Arm den j\u00fcngsten Prinzen, General der Cavall'rie,", "tokens": ["Auf", "dem", "Arm", "den", "j\u00fcng\u00b7sten", "Prin\u00b7zen", ",", "Ge\u00b7ne\u00b7ral", "der", "Ca\u00b7vall'\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Eine Hoheit von Acht Wochen, welche ganz abscheulich schrie.", "tokens": ["Ei\u00b7ne", "Ho\u00b7heit", "von", "Acht", "Wo\u00b7chen", ",", "wel\u00b7che", "ganz", "ab\u00b7scheu\u00b7lich", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Erst nachdem das hohe W\u00fcrmchen lutschbeutlig beruhigt war,", "tokens": ["Erst", "nach\u00b7dem", "das", "ho\u00b7he", "W\u00fcrm\u00b7chen", "lutschbeut\u00b7lig", "be\u00b7ru\u00b7higt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Reichte Pumpel-Pmapels Magd mir die Visitenkarte dar.", "tokens": ["Reich\u00b7te", "Pum\u00b7pel\u00b7\u00b7P\u00b7ma\u00b7pels", "Magd", "mir", "die", "Vi\u00b7si\u00b7ten\u00b7kar\u00b7te", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Zehn Minuten lie\u00df ich h\u00f6flichst unten im Kartoffelgarten", "tokens": ["Zehn", "Mi\u00b7nu\u00b7ten", "lie\u00df", "ich", "h\u00f6f\u00b7lichst", "un\u00b7ten", "im", "Kar\u00b7tof\u00b7fel\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Den Beherrscher von Dummdummdumm mit dem Hofgefolge warten.", "tokens": ["Den", "Be\u00b7herr\u00b7scher", "von", "Dumm\u00b7dumm\u00b7dumm", "mit", "dem", "Hof\u00b7ge\u00b7fol\u00b7ge", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-++-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann schrie Lotte aus dem Fenster. \u00bbMein Gebieter \u2013 heda, Sultan!", "tokens": ["Dann", "schrie", "Lot\u00b7te", "aus", "dem", "Fens\u00b7ter", ".", "\u00bb", "Mein", "Ge\u00b7bie\u00b7ter", "\u2013", "he\u00b7da", ",", "Sul\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "$.", "$(", "PPOSAT", "NN", "$(", "NE", "$,", "NN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Nimmt nunmehro die Visite in Herablassung und Huld an!\u00ab", "tokens": ["Nimmt", "nun\u00b7meh\u00b7ro", "die", "Vi\u00b7si\u00b7te", "in", "Her\u00b7ab\u00b7las\u00b7sung", "und", "Huld", "an", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$.", "$("], "meter": "+-+--+-+-++-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbsch\u00f6n!\u00ab erwiederte der Herrscher und stieg auf zu mir die Treppe.", "tokens": ["\u00bb", "sch\u00f6n", "!", "\u00ab", "er\u00b7wie\u00b7der\u00b7te", "der", "Herr\u00b7scher", "und", "stieg", "auf", "zu", "mir", "die", "Trep\u00b7pe", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zwanzig Professoren trugen seine lange Purpurschleppe.", "tokens": ["Zwan\u00b7zig", "Pro\u00b7fes\u00b7so\u00b7ren", "tru\u00b7gen", "sei\u00b7ne", "lan\u00b7ge", "Pur\u00b7pur\u00b7schlep\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Neben ihm der Ober-Henker (er hie\u00df hier \u00bbGedankenr\u00e4cher\u00ab)", "tokens": ["Ne\u00b7ben", "ihm", "der", "O\u00b7ber\u00b7Hen\u00b7ker", "(", "er", "hie\u00df", "hier", "\u00bb", "Ge\u00b7dan\u00b7ken\u00b7r\u00e4\u00b7cher", "\u00ab", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$(", "PPER", "VVFIN", "ADV", "$(", "NN", "$(", "$("], "meter": "+---+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Seiner dicken Hoheit wedelnd K\u00fchlung mit dem Kuhschwanzf\u00e4cher.", "tokens": ["Sei\u00b7ner", "di\u00b7cken", "Ho\u00b7heit", "we\u00b7delnd", "K\u00fch\u00b7lung", "mit", "dem", "Kuh\u00b7schwanz\u00b7f\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Sechszehn T\u00e4nzerinnen hielten \u00fcber ihn den Baldachin,", "tokens": ["Sechs\u00b7zehn", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "hiel\u00b7ten", "\u00fc\u00b7ber", "ihn", "den", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "All' von reizender Figur und \u2013 angekleidet, wie mir schien.", "tokens": ["All'", "von", "rei\u00b7zen\u00b7der", "Fi\u00b7gur", "und", "\u2013", "an\u00b7ge\u00b7klei\u00b7det", ",", "wie", "mir", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "KON", "$(", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Fahsionable Bettelm\u00f6nche gingen Arm in Arm zusammen", "tokens": ["Fah\u00b7si\u00b7o\u00b7nab\u00b7le", "Bet\u00b7tel\u00b7m\u00f6n\u00b7che", "gin\u00b7gen", "Arm", "in", "Arm", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "NN", "APPR", "NN", "PTKVZ"], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.8": {"text": "Mit Zehn unentweihten Jungfrau'n: approbirten Hebeammen.", "tokens": ["Mit", "Zehn", "un\u00b7ent\u00b7weih\u00b7ten", "Jung\u00b7frau'n", ":", "ap\u00b7pro\u00b7bir\u00b7ten", "He\u00b7beam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ferner folgten Dreizehn Ritter, edel und grundsteuerfrei,", "tokens": ["Fer\u00b7ner", "folg\u00b7ten", "Drei\u00b7zehn", "Rit\u00b7ter", ",", "e\u00b7del", "und", "grund\u00b7steu\u00b7er\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Sechs die gro\u00dfe Pauke blasend, Sieben trommelnd die Schalmei.", "tokens": ["Sechs", "die", "gro\u00b7\u00dfe", "Pau\u00b7ke", "bla\u00b7send", ",", "Sie\u00b7ben", "trom\u00b7melnd", "die", "Schal\u00b7mei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "ADJA", "NE", "ADJD", "$,", "NN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Dann der Leibpfaff der zugleich des Harems oberster Eunuche;", "tokens": ["Dann", "der", "Leib\u00b7pfaff", "der", "zu\u00b7gleich", "des", "Ha\u00b7rems", "o\u00b7bers\u00b7ter", "Eu\u00b7nu\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "--++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Dann der Zwerg, der Lustigmacher, mit dem Reichsgesetzesbuche;", "tokens": ["Dann", "der", "Zwerg", ",", "der", "Lus\u00b7tig\u00b7ma\u00b7cher", ",", "mit", "dem", "Reichs\u00b7ge\u00b7set\u00b7zes\u00b7bu\u00b7che", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Ferner noch der Koch, der Hofarzt und ein gro\u00dfer Stabstrompeter,", "tokens": ["Fer\u00b7ner", "noch", "der", "Koch", ",", "der", "Hof\u00b7arzt", "und", "ein", "gro\u00b7\u00dfer", "Stabs\u00b7trom\u00b7pe\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NE", "$,", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Kammerherrn und Kammerdiener und ein kleiner Volksvertreter,", "tokens": ["Kam\u00b7mer\u00b7herrn", "und", "Kam\u00b7mer\u00b7die\u00b7ner", "und", "ein", "klei\u00b7ner", "Volks\u00b7ver\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Der, getragen auf den Armen des Ministerpr\u00e4sidenten,", "tokens": ["Der", ",", "ge\u00b7tra\u00b7gen", "auf", "den", "Ar\u00b7men", "des", "Mi\u00b7nis\u00b7ter\u00b7pr\u00e4\u00b7si\u00b7den\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Einschlief beim Gesang der andern hohen Diener des Regenten.", "tokens": ["Ein\u00b7schlief", "beim", "Ge\u00b7sang", "der", "an\u00b7dern", "ho\u00b7hen", "Die\u00b7ner", "des", "Re\u00b7gen\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+--+--", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Endlich kamen noch zwei Dutzend G\u00f6tzen-Pfaffen, Einer feister", "tokens": ["End\u00b7lich", "ka\u00b7men", "noch", "zwei", "Dut\u00b7zend", "G\u00f6t\u00b7zen\u00b7Pfaf\u00b7fen", ",", "Ei\u00b7ner", "feis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "NN", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Als der Andre, und zum Schlu\u00df ein kugelrunder Kellermeister.", "tokens": ["Als", "der", "And\u00b7re", ",", "und", "zum", "Schlu\u00df", "ein", "ku\u00b7gel\u00b7run\u00b7der", "Kel\u00b7ler\u00b7meis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "\u00bbfremdling!\u00ab sprach zu mir der Sultan, tief und ehrfurchtsvoll sich b\u00fcckend:", "tokens": ["\u00bb", "fremd\u00b7ling", "!", "\u00ab", "sprach", "zu", "mir", "der", "Sul\u00b7tan", ",", "tief", "und", "ehr\u00b7furchts\u00b7voll", "sich", "b\u00fc\u00b7ckend", ":"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00bbdeine Ankunft, Mich, den Herrscher, und ganz Dummdummdumm begl\u00fcckend,", "tokens": ["\u00bb", "dei\u00b7ne", "An\u00b7kunft", ",", "Mich", ",", "den", "Herr\u00b7scher", ",", "und", "ganz", "Dumm\u00b7dumm\u00b7dumm", "be\u00b7gl\u00fc\u00b7ckend", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "$,", "ART", "NN", "$,", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sei mit aller G\u00f6tter Segen hoch von heute an gesegnet,", "tokens": ["Sei", "mit", "al\u00b7ler", "G\u00f6t\u00b7ter", "Se\u00b7gen", "hoch", "von", "heu\u00b7te", "an", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "NN", "ADJD", "APPR", "ADV", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis zur Zeit, von der verk\u00fcndet, da\u00df es goldne Scudi's regnet!\u00ab", "tokens": ["Bis", "zur", "Zeit", ",", "von", "der", "ver\u00b7k\u00fcn\u00b7det", ",", "da\u00df", "es", "gold\u00b7ne", "Scu\u00b7di's", "reg\u00b7net", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "APPR", "ART", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbm\u00f6chten sie,\u00ab war meine Antwort, \u00bbdann so dicht herunterfallen,", "tokens": ["\u00bb", "m\u00f6ch\u00b7ten", "sie", ",", "\u00ab", "war", "mei\u00b7ne", "Ant\u00b7wort", ",", "\u00bb", "dann", "so", "dicht", "her\u00b7un\u00b7ter\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "$,", "$(", "VAFIN", "PPOSAT", "NN", "$,", "$(", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df sie, trotz der harten Schale, Deinen Sch\u00e4del Dir zerknallen!", "tokens": ["Da\u00df", "sie", ",", "trotz", "der", "har\u00b7ten", "Scha\u00b7le", ",", "Dei\u00b7nen", "Sch\u00e4\u00b7del", "Dir", "zer\u00b7knal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn es d\u00fcrften sich so leicht nicht andre R\u00e4ume finden lassen,", "tokens": ["Denn", "es", "d\u00fcrf\u00b7ten", "sich", "so", "leicht", "nicht", "and\u00b7re", "R\u00e4u\u00b7me", "fin\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADV", "ADJD", "PTKNEG", "ADJA", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "l