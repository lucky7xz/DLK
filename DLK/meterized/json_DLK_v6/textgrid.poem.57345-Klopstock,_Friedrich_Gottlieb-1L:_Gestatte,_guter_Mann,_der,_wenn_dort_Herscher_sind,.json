{"textgrid.poem.57345": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gestatte, guter Mann, der, wenn dort Herscher sind,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestatte, guter Mann, der, wenn dort Herscher sind,", "tokens": ["Ge\u00b7stat\u00b7te", ",", "gu\u00b7ter", "Mann", ",", "der", ",", "wenn", "dort", "Her\u00b7scher", "sind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS", "$,", "KOUS", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzo herscht in der Strasse des Lichts,", "tokens": ["Jet\u00b7zo", "herscht", "in", "der", "Stras\u00b7se", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dass einen Lorber auch ich", "tokens": ["Dass", "ei\u00b7nen", "Lor\u00b7ber", "auch", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Pflanz' in den Hain an dein Grat.", "tokens": ["Pflanz'", "in", "den", "Hain", "an", "dein", "Grat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Sie gehn, und entweihen", "tokens": ["Sie", "gehn", ",", "und", "ent\u00b7wei\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Deinen heiligen Staub;", "tokens": ["Dei\u00b7nen", "hei\u00b7li\u00b7gen", "Staub", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Denn Dich, zu dein sich keiner erhob,", "tokens": ["Denn", "Dich", ",", "zu", "dein", "sich", "kei\u00b7ner", "er\u00b7hob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Feinden die Ehreverschwender durch Vergleichungen an.", "tokens": ["Fein\u00b7den", "die", "E\u00b7hre\u00b7ver\u00b7schwen\u00b7der", "durch", "Ver\u00b7glei\u00b7chun\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.3": {"line.1": {"text": "Wie der Ruf in dem Felsen verhallt,", "tokens": ["Wie", "der", "Ruf", "in", "dem", "Fel\u00b7sen", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "So vergehe sein Lied, der dich erreichende schuf:", "tokens": ["So", "ver\u00b7ge\u00b7he", "sein", "Lied", ",", "der", "dich", "er\u00b7rei\u00b7chen\u00b7de", "schuf", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nein, es bleib', und es h\u00f6re nicht auf", "tokens": ["Nein", ",", "es", "bleib'", ",", "und", "es", "h\u00f6\u00b7re", "nicht", "auf"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "APPR"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Des entweihenden Schmach zu seyn.", "tokens": ["Des", "ent\u00b7wei\u00b7hen\u00b7den", "Schmach", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Guter, edler, nicht scheinender,", "tokens": ["Gu\u00b7ter", ",", "ed\u00b7ler", ",", "nicht", "schei\u00b7nen\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "PTKNEG", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wirklich grosser Mann, wahrhaftester!", "tokens": ["Wirk\u00b7lich", "gros\u00b7ser", "Mann", ",", "wahr\u00b7haf\u00b7tes\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dich vergleichen sie, und gl\u00fchn nicht vor Scham,", "tokens": ["Dich", "ver\u00b7glei\u00b7chen", "sie", ",", "und", "gl\u00fchn", "nicht", "vor", "Scham", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Vergleichen dich, Markus Aurelius!", "tokens": ["Ver\u00b7glei\u00b7chen", "dich", ",", "Mar\u00b7kus", "Au\u00b7re\u00b7li\u00b7us", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}