{"textgrid.poem.26656": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Kindsm\u00f6rderin", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch \u2013 die Glocken weinen dumpf zusammen,", "tokens": ["Horch", "\u2013", "die", "Glo\u00b7cken", "wei\u00b7nen", "dumpf", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "VVINF", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Zeiger hat vollbracht den Lauf.", "tokens": ["Und", "der", "Zei\u00b7ger", "hat", "voll\u00b7bracht", "den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nun, so seis denn! \u2013 Nun, in Gottes Namen!", "tokens": ["Nun", ",", "so", "seis", "denn", "!", "\u2013", "Nun", ",", "in", "Got\u00b7tes", "Na\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "ADV", "$.", "$(", "ADV", "$,", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Grabgef\u00e4hrten, brecht zum Richtplatz auf!", "tokens": ["Grab\u00b7ge\u00b7f\u00e4hr\u00b7ten", ",", "brecht", "zum", "Richt\u00b7platz", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nimm, o Welt, die letzten Abschiedsk\u00fcsse,", "tokens": ["Nimm", ",", "o", "Welt", ",", "die", "letz\u00b7ten", "Ab\u00b7schieds\u00b7k\u00fcs\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Diese Tr\u00e4nen nimm, o Welt, noch hin!", "tokens": ["Die\u00b7se", "Tr\u00e4\u00b7nen", "nimm", ",", "o", "Welt", ",", "noch", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,", "FM", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Deine Gifte \u2013 o sie schmeckten s\u00fc\u00dfe!", "tokens": ["Dei\u00b7ne", "Gif\u00b7te", "\u2013", "o", "sie", "schmeck\u00b7ten", "s\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "FM", "PPER", "VVFIN", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wir sind quitt, du Herzvergifterin.", "tokens": ["Wir", "sind", "quitt", ",", "du", "Herz\u00b7ver\u00b7gif\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Fahret wohl, ihr Freuden dieser Sonne,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "Freu\u00b7den", "die\u00b7ser", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gegen schwarzen Moder umgetauscht!", "tokens": ["Ge\u00b7gen", "schwar\u00b7zen", "Mo\u00b7der", "um\u00b7ge\u00b7tauscht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fahre wohl, du Rosenzeit voll Wonne,", "tokens": ["Fah\u00b7re", "wohl", ",", "du", "Ro\u00b7sen\u00b7zeit", "voll", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die so oft das M\u00e4dchen lustberauscht!", "tokens": ["Die", "so", "oft", "das", "M\u00e4d\u00b7chen", "lust\u00b7be\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Fahret wohl, ihr goldgewebten Tr\u00e4ume,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "gold\u00b7ge\u00b7web\u00b7ten", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Paradieseskinder-Phantasien!", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7kin\u00b7der\u00b7Phan\u00b7tasi\u00b7en", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Weh! sie starben schon im Morgenkeime,", "tokens": ["Weh", "!", "sie", "star\u00b7ben", "schon", "im", "Mor\u00b7gen\u00b7kei\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig nimmer an das Licht zu bl\u00fchn.", "tokens": ["E\u00b7wig", "nim\u00b7mer", "an", "das", "Licht", "zu", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n geschm\u00fcckt mit rosenroten Schleifen", "tokens": ["Sch\u00f6n", "ge\u00b7schm\u00fcckt", "mit", "ro\u00b7sen\u00b7ro\u00b7ten", "Schlei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deckte mich der Unschuld Schwanenkleid,", "tokens": ["Deck\u00b7te", "mich", "der", "Un\u00b7schuld", "Schwa\u00b7nen\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der blonden Locken loses Schweifen", "tokens": ["In", "der", "blon\u00b7den", "Lo\u00b7cken", "lo\u00b7ses", "Schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Waren junge Rosen eingestreut: \u2013", "tokens": ["Wa\u00b7ren", "jun\u00b7ge", "Ro\u00b7sen", "ein\u00b7ge\u00b7streut", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wehe! \u2013 die Geopferte der H\u00f6lle", "tokens": ["We\u00b7he", "!", "\u2013", "die", "Ge\u00b7o\u00b7pfer\u00b7te", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Schm\u00fcckt noch itzt das wei\u00dflichte Gewand,", "tokens": ["Schm\u00fcckt", "noch", "itzt", "das", "wei\u00df\u00b7lich\u00b7te", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber ach! \u2013 der Rosenschleifen Stelle", "tokens": ["A\u00b7ber", "ach", "!", "\u2013", "der", "Ro\u00b7sen\u00b7schlei\u00b7fen", "Stel\u00b7le"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "$(", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nahm ein schwarzes Totenband.", "tokens": ["Nahm", "ein", "schwar\u00b7zes", "To\u00b7ten\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weinet um mich, die ihr nie gefallen,", "tokens": ["Wei\u00b7net", "um", "mich", ",", "die", "ihr", "nie", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Denen noch der Unschuld Lilien bl\u00fchn,", "tokens": ["De\u00b7nen", "noch", "der", "Un\u00b7schuld", "Li\u00b7li\u00b7en", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Denen zu dem weichen Busenwallen", "tokens": ["De\u00b7nen", "zu", "dem", "wei\u00b7chen", "Bu\u00b7sen\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heldenst\u00e4rke die Natur verliehn!", "tokens": ["Hel\u00b7den\u00b7st\u00e4r\u00b7ke", "die", "Na\u00b7tur", "ver\u00b7liehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wehe! \u2013 menschlich hat dies Herz empfunden! \u2013", "tokens": ["We\u00b7he", "!", "\u2013", "menschlich", "hat", "dies", "Herz", "emp\u00b7fun\u00b7den", "!", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "ADJD", "VAFIN", "PDS", "NN", "VVPP", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und Empfindung soll mein Richtschwert sein! \u2013", "tokens": ["Und", "Emp\u00b7fin\u00b7dung", "soll", "mein", "Richt\u00b7schwert", "sein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Weh! vom Arm des falschen Manns umwunden,", "tokens": ["Weh", "!", "vom", "Arm", "des", "fal\u00b7schen", "Manns", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schlief Louisens Tugend ein.", "tokens": ["Schlief", "Lou\u00b7i\u00b7sens", "Tu\u00b7gend", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach vielleicht umflattert eine andre,", "tokens": ["Ach", "viel\u00b7leicht", "um\u00b7flat\u00b7tert", "ei\u00b7ne", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein vergessen, dieses Schlangenherz,", "tokens": ["Mein", "ver\u00b7ges\u00b7sen", ",", "die\u00b7ses", "Schlan\u00b7gen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberflie\u00dft, wenn ich zum Grabe wandre,", "tokens": ["\u00dc\u00b7berf\u00b7lie\u00dft", ",", "wenn", "ich", "zum", "Gra\u00b7be", "wand\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "An dem Putztisch in verliebten Scherz?", "tokens": ["An", "dem", "Putz\u00b7tisch", "in", "ver\u00b7lieb\u00b7ten", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Spielt vielleicht mit seines M\u00e4dchens Locke?", "tokens": ["Spielt", "viel\u00b7leicht", "mit", "sei\u00b7nes", "M\u00e4d\u00b7chens", "Lo\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schlingt den Ku\u00df, den sie entgegenbringt?", "tokens": ["Schlingt", "den", "Ku\u00df", ",", "den", "sie", "ent\u00b7ge\u00b7gen\u00b7bringt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn, verspritzt auf diesem Todesblocke,", "tokens": ["Wenn", ",", "ver\u00b7spritzt", "auf", "die\u00b7sem", "To\u00b7des\u00b7blo\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Hoch mein Blut vom Rumpfe springt.", "tokens": ["Hoch", "mein", "Blut", "vom", "Rump\u00b7fe", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Joseph! Joseph! auf entfernte Meilen", "tokens": ["Jo\u00b7se\u00b7ph", "!", "Jo\u00b7se\u00b7ph", "!", "auf", "ent\u00b7fern\u00b7te", "Mei\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Folge dir Louisens Totenchor,", "tokens": ["Fol\u00b7ge", "dir", "Lou\u00b7i\u00b7sens", "To\u00b7ten\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und des Glockenturmes dumpfes Heulen", "tokens": ["Und", "des", "Glo\u00b7cken\u00b7tur\u00b7mes", "dum\u00b7pfes", "Heu\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schlage schr\u00f6cklichmahnend an dein Ohr \u2013", "tokens": ["Schla\u00b7ge", "schr\u00f6ck\u00b7lich\u00b7mah\u00b7nend", "an", "dein", "Ohr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn von eines M\u00e4dchens weichem Munde", "tokens": ["Wenn", "von", "ei\u00b7nes", "M\u00e4d\u00b7chens", "wei\u00b7chem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dir der Liebe sanft Gelispel quillt,", "tokens": ["Dir", "der", "Lie\u00b7be", "sanft", "Ge\u00b7lis\u00b7pel", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bohr es pl\u00f6tzlich eine H\u00f6llenwunde", "tokens": ["Bohr", "es", "pl\u00f6tz\u00b7lich", "ei\u00b7ne", "H\u00f6l\u00b7len\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In der Wollust Rosenbild!", "tokens": ["In", "der", "Wol\u00b7lust", "Ro\u00b7sen\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha Verr\u00e4ter! nicht Louisens Schmerzen?", "tokens": ["Ha", "Ver\u00b7r\u00e4\u00b7ter", "!", "nicht", "Lou\u00b7i\u00b7sens", "Schmer\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PTKNEG", "NE", "NN", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Nicht des Weibes Schande, harter Mann?", "tokens": ["Nicht", "des", "Wei\u00b7bes", "Schan\u00b7de", ",", "har\u00b7ter", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht das Kn\u00e4blein unter meinem Herzen?", "tokens": ["Nicht", "das", "Kn\u00e4\u00b7blein", "un\u00b7ter", "mei\u00b7nem", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "m