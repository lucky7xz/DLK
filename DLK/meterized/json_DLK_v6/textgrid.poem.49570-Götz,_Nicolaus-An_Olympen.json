{"textgrid.poem.49570": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Olympen", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Leib ist sch\u00f6n; noch sch\u00f6ner deine Seele.", "tokens": ["Dein", "Leib", "ist", "sch\u00f6n", ";", "noch", "sch\u00f6\u00b7ner", "dei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man sieht auf dich vor tausend Frauen nur.", "tokens": ["Man", "sieht", "auf", "dich", "vor", "tau\u00b7send", "Frau\u00b7en", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PRF", "APPR", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Mund entz\u00fcckt und singt, wie Philomele;", "tokens": ["Dein", "Mund", "ent\u00b7z\u00fcckt", "und", "singt", ",", "wie", "Phi\u00b7lo\u00b7me\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was du singst ist Regung und Natur.", "tokens": ["Und", "was", "du", "singst", "ist", "Re\u00b7gung", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4ttest du am Simois", "tokens": ["H\u00e4t\u00b7test", "du", "am", "Si\u00b7mois"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NE"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Zu der alten Zeit gelebet,", "tokens": ["Zu", "der", "al\u00b7ten", "Zeit", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "O so h\u00e4tte Tyndaris", "tokens": ["O", "so", "h\u00e4t\u00b7te", "Tyn\u00b7da\u00b7ris"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Neben dir gebetet!", "tokens": ["Ne\u00b7ben", "dir", "ge\u00b7be\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ein Blick von dir", "tokens": ["Ein", "Blick", "von", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "H\u00e4tt' ihr", "tokens": ["H\u00e4tt'", "ihr"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Den Liebling abgezwungen;", "tokens": ["Den", "Lieb\u00b7ling", "ab\u00b7ge\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und Homer, gewi\u00df!", "tokens": ["Und", "Ho\u00b7mer", ",", "ge\u00b7wi\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "H\u00e4tte nur von dir gesungen.", "tokens": ["H\u00e4t\u00b7te", "nur", "von", "dir", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}