{"textgrid.poem.61285": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lied am Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch diesen Herbstestag voll Sturm", "tokens": ["Durch", "die\u00b7sen", "Herbs\u00b7tes\u00b7tag", "voll", "Sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Drachenfels empor die Steige!", "tokens": ["Zum", "Dra\u00b7chen\u00b7fels", "em\u00b7por", "die", "Stei\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon winkt zu H\u00e4upten mir der Turm,", "tokens": ["Schon", "winkt", "zu", "H\u00e4up\u00b7ten", "mir", "der", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der breite, durch die falben Zweige.", "tokens": ["Der", "brei\u00b7te", ",", "durch", "die", "fal\u00b7ben", "Zwei\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da steh' ich - roter Sonnenschein", "tokens": ["Da", "steh'", "ich", "ro\u00b7ter", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umlodert k\u00f6niglich die Klippe;", "tokens": ["Um\u00b7lo\u00b7dert", "k\u00f6\u00b7nig\u00b7lich", "die", "Klip\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu meinen F\u00fc\u00dfen braust der Rhein -", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "braust", "der", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir schl\u00e4gt das Herz. O reichet Wein,", "tokens": ["Mir", "schl\u00e4gt", "das", "Herz", ".", "O", "rei\u00b7chet", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das volle Glas reicht meiner Lippe!", "tokens": ["Das", "vol\u00b7le", "Glas", "reicht", "mei\u00b7ner", "Lip\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir sei's, o deutsches Volk, gebracht,", "tokens": ["Dir", "sei's", ",", "o", "deut\u00b7sches", "Volk", ",", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem einen, gro\u00dfen, wundervollen,", "tokens": ["Dem", "ei\u00b7nen", ",", "gro\u00b7\u00dfen", ",", "wun\u00b7der\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So weit der Himmel um dich lacht", "tokens": ["So", "weit", "der", "Him\u00b7mel", "um", "dich", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcber dir die Donner rollen!", "tokens": ["Und", "\u00fc\u00b7ber", "dir", "die", "Don\u00b7ner", "rol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was k\u00fcmmert's mich, auf Stein und Holz", "tokens": ["Was", "k\u00fcm\u00b7mert's", "mich", ",", "auf", "Stein", "und", "Holz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie deiner Wappen Farben streiten!", "tokens": ["Wie", "dei\u00b7ner", "Wap\u00b7pen", "Far\u00b7ben", "strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich meine dich, das j\u00fcngst noch stolz", "tokens": ["Ich", "mei\u00b7ne", "dich", ",", "das", "j\u00fcngst", "noch", "stolz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Hamburgs Brand zusammenschmolz,", "tokens": ["In", "Ham\u00b7burgs", "Brand", "zu\u00b7sam\u00b7men\u00b7schmolz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Korinthisch Erz f\u00fcr alle Zeiten.", "tokens": ["Ko\u00b7rin\u00b7thisch", "Erz", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wieder f\u00fcllt den R\u00f6mer mir!", "tokens": ["Und", "wie\u00b7der", "f\u00fcllt", "den", "R\u00f6\u00b7mer", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft spr\u00fchn, la\u00dft spr\u00fchn die goldnen Funken!", "tokens": ["La\u00dft", "spr\u00fchn", ",", "la\u00dft", "spr\u00fchn", "die", "gold\u00b7nen", "Fun\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sei aus vollem Herzen dir", "tokens": ["Er", "sei", "aus", "vol\u00b7lem", "Her\u00b7zen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Preis, o deutscher Geist, getrunken;", "tokens": ["Zum", "Preis", ",", "o", "deut\u00b7scher", "Geist", ",", "ge\u00b7trun\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "FM", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dir, der sich aus den Tiefen n\u00e4hrt,", "tokens": ["Dir", ",", "der", "sich", "aus", "den", "Tie\u00b7fen", "n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der gleich dem wilden Sohn der Trauben,", "tokens": ["Der", "gleich", "dem", "wil\u00b7den", "Sohn", "der", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn er im Lenze braust und g\u00e4rt,", "tokens": ["Wenn", "er", "im", "Len\u00b7ze", "braust", "und", "g\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu s\u00fc\u00dferm Feuer nur sich kl\u00e4rt,", "tokens": ["Zu", "s\u00fc\u00b7\u00dferm", "Feu\u00b7er", "nur", "sich", "kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dir Geist voll Liebe, Kraft und Glauben!", "tokens": ["Dir", "Geist", "voll", "Lie\u00b7be", ",", "Kraft", "und", "Glau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nochmals f\u00fcllt! Und wenn darein", "tokens": ["Und", "noch\u00b7mals", "f\u00fcllt", "!", "Und", "wenn", "da\u00b7rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$.", "KON", "KOUS", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Neigen aus der Flasche troffen:", "tokens": ["Die", "Nei\u00b7gen", "aus", "der", "Fla\u00b7sche", "trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es soll darum nicht schlechter sein;", "tokens": ["Es", "soll", "da\u00b7rum", "nicht", "schlech\u00b7ter", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten Becher unserm Hoffen!", "tokens": ["Den", "letz\u00b7ten", "Be\u00b7cher", "un\u00b7serm", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Wort ein fr\u00f6hlich Auferstehn,", "tokens": ["Dem", "Wort", "ein", "fr\u00f6h\u00b7lich", "Auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem freien Kampfe der Gedanken!", "tokens": ["Dem", "frei\u00b7en", "Kamp\u00b7fe", "der", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00dft k\u00fchn des Geistes St\u00fcrme gehn!", "tokens": ["La\u00dft", "k\u00fchn", "des", "Geis\u00b7tes", "St\u00fcr\u00b7me", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was Spreu ist, mag wie Spreu verwehn,", "tokens": ["Was", "Spreu", "ist", ",", "mag", "wie", "Spreu", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VMFIN", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was Felsen ist, wird doch nicht wanken!", "tokens": ["Was", "Fel\u00b7sen", "ist", ",", "wird", "doch", "nicht", "wan\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VAFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vorw\u00e4rts! hei\u00dft unser Losungswort,", "tokens": ["Vor\u00b7w\u00e4rts", "!", "hei\u00dft", "un\u00b7ser", "Lo\u00b7sungs\u00b7wort", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und durch die Reihen rauscht's im Volke -", "tokens": ["Und", "durch", "die", "Rei\u00b7hen", "rauscht's", "im", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schneegest\u00f6ber dr\u00e4ut vom Nord,", "tokens": ["Ein", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "dr\u00e4ut", "vom", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dort im Westen murrt die Wolke.", "tokens": ["Und", "dort", "im", "Wes\u00b7ten", "murrt", "die", "Wol\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vorw\u00e4rts darum am eignen Herd,", "tokens": ["Vor\u00b7w\u00e4rts", "da\u00b7rum", "am", "eig\u00b7nen", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Da\u00df Jenas Schmach sich nicht erneue!", "tokens": ["Da\u00df", "Je\u00b7nas", "Schmach", "sich", "nicht", "er\u00b7neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vorw\u00e4rts! Und wenn's der Tag begehrt,", "tokens": ["Vor\u00b7w\u00e4rts", "!", "Und", "wenn's", "der", "Tag", "be\u00b7gehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Dann blitz' in jeder Faust ein Schwert,", "tokens": ["Dann", "blitz'", "in", "je\u00b7der", "Faust", "ein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und Gott mit uns und deutsche Treue!", "tokens": ["Und", "Gott", "mit", "uns", "und", "deut\u00b7sche", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}