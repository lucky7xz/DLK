{"dta.poem.11453": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund! gehe nicht zu weit, halt dich in deinen schrancken!", "tokens": ["Freund", "!", "ge\u00b7he", "nicht", "zu", "weit", ",", "halt", "dich", "in", "dei\u00b7nen", "schran\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du weist wohl, wie es stets den staats-propheten geht:", "tokens": ["Du", "weist", "wohl", ",", "wie", "es", "stets", "den", "staats\u00b7pro\u00b7phe\u00b7ten", "geht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ich versichre dich: Der k\u00f6nige gedancken", "tokens": ["Und", "ich", "ver\u00b7sich\u00b7re", "dich", ":", "Der", "k\u00f6\u00b7ni\u00b7ge", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind b\u00fccher, die gewi\u00df kein unterthan versteht.", "tokens": ["Sind", "b\u00fc\u00b7cher", ",", "die", "ge\u00b7wi\u00df", "kein", "un\u00b7ter\u00b7than", "ver\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "ADV", "PIAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}