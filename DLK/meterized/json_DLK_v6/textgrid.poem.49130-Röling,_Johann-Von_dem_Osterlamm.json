{"textgrid.poem.49130": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Osterlamm", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schickest, Jesu, aus nach einem raumen Saal,", "tokens": ["Du", "schi\u00b7ckest", ",", "Je\u00b7su", ",", "aus", "nach", "ei\u00b7nem", "rau\u00b7men", "Saal", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "$,", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woselbst das Osterlamm und letzte Wunder-Mahl", "tokens": ["Wo\u00b7selbst", "das", "Os\u00b7ter\u00b7lamm", "und", "letz\u00b7te", "Wun\u00b7der\u00b7Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir zugerichtet w\u00fcrde,", "tokens": ["Dir", "zu\u00b7ge\u00b7rich\u00b7tet", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie das Gesetz gebeut,", "tokens": ["Wie", "das", "Ge\u00b7setz", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seit von Aegyptens B\u00fcrde", "tokens": ["Seit", "von", "A\u00b7e\u00b7gyp\u00b7tens", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du hast dein Volk befreit.", "tokens": ["Du", "hast", "dein", "Volk", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, nimm mich mit zu Tisch, ich habe gro\u00dfes Recht,", "tokens": ["Ach", ",", "nimm", "mich", "mit", "zu", "Tisch", ",", "ich", "ha\u00b7be", "gro\u00b7\u00dfes", "Recht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "APPR", "APPR", "NN", "$,", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich auch Ostern halt. Ich war der H\u00f6llen Knecht,", "tokens": ["Da\u00df", "ich", "auch", "Os\u00b7tern", "halt", ".", "Ich", "war", "der", "H\u00f6l\u00b7len", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "All mein Beginn und Wesen", "tokens": ["All", "mein", "Be\u00b7ginn", "und", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stand in betr\u00fcbtem Stand,", "tokens": ["Stand", "in", "be\u00b7tr\u00fcb\u00b7tem", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hievon bin ich genesen", "tokens": ["Hie\u00b7von", "bin", "ich", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Durch deine liebe Hand.", "tokens": ["Durch", "dei\u00b7ne", "lie\u00b7be", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du hast mich au\u00dfgef\u00fchrt, du warst des Feindes Pest,", "tokens": ["Du", "hast", "mich", "au\u00df\u00b7ge\u00b7f\u00fchrt", ",", "du", "warst", "des", "Fein\u00b7des", "Pest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies stifft mein Passah an zu einem Andenks-Fest,", "tokens": ["Dies", "stifft", "mein", "Pas\u00b7sah", "an", "zu", "ei\u00b7nem", "An\u00b7denks\u00b7Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich bin durchgekommen", "tokens": ["Da\u00df", "ich", "bin", "durch\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo sonst nicht Steg noch Bahn,", "tokens": ["Wo", "sonst", "nicht", "Steg", "noch", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und du mich aufgenommen", "tokens": ["Und", "du", "mich", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu einem freyen Mann.", "tokens": ["Zu", "ei\u00b7nem", "frey\u00b7en", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nimm mich denn mit zu Tisch, woselbst das Lamm du bist,", "tokens": ["Nimm", "mich", "denn", "mit", "zu", "Tisch", ",", "wo\u00b7selbst", "das", "Lamm", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "APPR", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das her von Ewigkeit f\u00fcr uns geschlachtet ist", "tokens": ["Das", "her", "von", "E\u00b7wig\u00b7keit", "f\u00fcr", "uns", "ge\u00b7schlach\u00b7tet", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "APPR", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und itzt wird auffgegeben", "tokens": ["Und", "itzt", "wird", "auff\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereitt durch Lieb' und Treu,", "tokens": ["Be\u00b7reitt", "durch", "Lieb'", "und", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auff da\u00df, o unser Leben,", "tokens": ["Auff", "da\u00df", ",", "o", "un\u00b7ser", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es unsre Speise sey.", "tokens": ["Es", "uns\u00b7re", "Spei\u00b7se", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O nie erh\u00f6rtes Mahl, bei dem der Wirth die Kost,", "tokens": ["O", "nie", "er\u00b7h\u00f6r\u00b7tes", "Mahl", ",", "bei", "dem", "der", "Wirth", "die", "Kost", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Fleisch selbst ist das Brodt, sein Blut selbst ist der Most,", "tokens": ["Sein", "Fleisch", "selbst", "ist", "das", "Brodt", ",", "sein", "Blut", "selbst", "ist", "der", "Most", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Mahl, dem keines gleichet,", "tokens": ["O", "Mahl", ",", "dem", "kei\u00b7nes", "glei\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mahl, mehr als wunderns werth,", "tokens": ["Mahl", ",", "mehr", "als", "wun\u00b7derns", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOUS", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da man die Speise reichet,", "tokens": ["Da", "man", "die", "Spei\u00b7se", "rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die nimmer wird verzehrt.", "tokens": ["Die", "nim\u00b7mer", "wird", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach, labe mich mit dir, hochheiligs Gottes-Lamm,", "tokens": ["Ach", ",", "la\u00b7be", "mich", "mit", "dir", ",", "hoch\u00b7hei\u00b7ligs", "Got\u00b7tes\u00b7Lamm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PRF", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es breitt dich meine S\u00fcnd' an deines Kreutzes Stamm", "tokens": ["Es", "breitt", "dich", "mei\u00b7ne", "S\u00fcnd'", "an", "dei\u00b7nes", "Kreut\u00b7zes", "Stamm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Am Zorn-Feur, an den Flammen,", "tokens": ["Am", "Zorn\u00b7Feur", ",", "an", "den", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die uns dein Vater dreut'", "tokens": ["Die", "uns", "dein", "Va\u00b7ter", "dreut'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcber uns zusammen", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gleich itzund hielt bereit.", "tokens": ["Gleich", "it\u00b7zund", "hielt", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du zogst sie all' auff dich, so ward ich ihrer frey,", "tokens": ["Du", "zogst", "sie", "all'", "auff", "dich", ",", "so", "ward", "ich", "ih\u00b7rer", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies setzet, o mein Heyl, mich deinen G\u00e4sten bey,", "tokens": ["Dies", "set\u00b7zet", ",", "o", "mein", "Heyl", ",", "mich", "dei\u00b7nen", "G\u00e4s\u00b7ten", "bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies heist mich Ostern feyren.", "tokens": ["Dies", "heist", "mich", "Os\u00b7tern", "fey\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hilff, Jesu, aber mir,", "tokens": ["Hilff", ",", "Je\u00b7su", ",", "a\u00b7ber", "mir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "KON", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df mich nichts mag vers\u00e4uren,", "tokens": ["Da\u00df", "mich", "nichts", "mag", "ver\u00b7s\u00e4u\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da ich gespeist mit dir.", "tokens": ["Da", "ich", "ge\u00b7speist", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Im Stehen wurde vor das Osterlamm verzehrt;", "tokens": ["Im", "Ste\u00b7hen", "wur\u00b7de", "vor", "das", "Os\u00b7ter\u00b7lamm", "ver\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lehr mich, da\u00df dieses Hau\u00df mir eigen nicht geh\u00f6rt,", "tokens": ["Lehr", "mich", ",", "da\u00df", "die\u00b7ses", "Hau\u00df", "mir", "ei\u00b7gen", "nicht", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PDAT", "NN", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df mich im Glauben stehen,", "tokens": ["La\u00df", "mich", "im", "Glau\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Der Welt und Erden frey,", "tokens": ["Der", "Welt", "und", "Er\u00b7den", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df, wenn du mich heist gehen,", "tokens": ["Da\u00df", ",", "wenn", "du", "mich", "heist", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich Reyse-fertig sey.", "tokens": ["Ich", "Rey\u00b7se\u00b7fer\u00b7tig", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Legst du indessen hie auch zu dem s\u00fc\u00dfen Brodt", "tokens": ["Legst", "du", "in\u00b7des\u00b7sen", "hie", "auch", "zu", "dem", "s\u00fc\u00b7\u00dfen", "Brodt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die bittren Salsen mit des Kreutzes und der Noth,", "tokens": ["Die", "bit\u00b7tren", "Sal\u00b7sen", "mit", "des", "Kreut\u00b7zes", "und", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Woll, Herr; nur streich dein Zeichen", "tokens": ["Woll", ",", "Herr", ";", "nur", "streich", "dein", "Zei\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "$,", "NN", "$.", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir an mit deinem Blut", "tokens": ["Mir", "an", "mit", "dei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und la\u00df den W\u00fcrger weichen,", "tokens": ["Und", "la\u00df", "den", "W\u00fcr\u00b7ger", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So ist mir Alles gut.", "tokens": ["So", "ist", "mir", "Al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}