{"textgrid.poem.27599": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: All ihr K\u00fcnstler in der Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "All ihr K\u00fcnstler in der Welt,", "tokens": ["All", "ihr", "K\u00fcnst\u00b7ler", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Derer k\u00fchnes Auge-schauen", "tokens": ["De\u00b7rer", "k\u00fch\u00b7nes", "Au\u00b7ge\u00b7schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch so viel kan H\u00e4user bauen", "tokens": ["Euch", "so", "viel", "kan", "H\u00e4u\u00b7ser", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VMFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das blaue G\u00f6tter-Feld:", "tokens": ["In", "das", "blau\u00b7e", "G\u00f6t\u00b7ter\u00b7Feld", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt ihr nicht voran mir sagen,", "tokens": ["K\u00f6nnt", "ihr", "nicht", "vo\u00b7ran", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was sich gutes zu wird tragen,", "tokens": ["Was", "sich", "gu\u00b7tes", "zu", "wird", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJA", "APPR", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn sich Mars zu Venus stellt", "tokens": ["Wenn", "sich", "Mars", "zu", "Ve\u00b7nus", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem sch\u00f6nen Jungfern-Zeichen?", "tokens": ["In", "dem", "sch\u00f6\u00b7nen", "Jung\u00b7fern\u00b7Zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tycho sage, was er wil;", "tokens": ["Ty\u00b7cho", "sa\u00b7ge", ",", "was", "er", "wil", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Fehl ich, fehl ich doch nicht viel:", "tokens": ["Fehl", "ich", ",", "fehl", "ich", "doch", "nicht", "viel", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Kinder werden dannen reichen,", "tokens": ["Kin\u00b7der", "wer\u00b7den", "dan\u00b7nen", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die de\u00df Vaters tapffren Sinn", "tokens": ["Die", "de\u00df", "Va\u00b7ters", "tapf\u00b7fren", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und der Mutter sch\u00f6nes Kinn", "tokens": ["Und", "der", "Mut\u00b7ter", "sch\u00f6\u00b7nes", "Kinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Lieblich werden abegleichen.", "tokens": ["Lieb\u00b7lich", "wer\u00b7den", "a\u00b7be\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}