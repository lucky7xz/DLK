{"dta.poem.14708": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00e4mpe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas vertraust Du, warum baust Du", "tokens": ["\u201e", "was", "ver\u00b7traust", "Du", ",", "wa\u00b7rum", "baust", "Du"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Auf der M\u00e4nner wilde Brust,", "tokens": ["Auf", "der", "M\u00e4n\u00b7ner", "wil\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das Blut ziert und der Streit r\u00fchrt", "tokens": ["Die", "das", "Blut", "ziert", "und", "der", "Streit", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und die sch\u00f6ne Todeslust!\u201c", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "To\u00b7des\u00b7lust", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}