{"textgrid.poem.49537": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Fabel", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit stolz erhabner Stirn', und nicht durch Last gedr\u00fcckt,", "tokens": ["Mit", "stolz", "er\u00b7hab\u00b7ner", "Stirn'", ",", "und", "nicht", "durch", "Last", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "KON", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach einst ein leerer Halm zu einer vollen \u00c4hre:", "tokens": ["Sprach", "einst", "ein", "lee\u00b7rer", "Halm", "zu", "ei\u00b7ner", "vol\u00b7len", "\u00c4h\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbwie kommt es, da\u00df dein Haupt so nach dem Boden nickt?\u00ab", "tokens": ["\u00bb", "wie", "kommt", "es", ",", "da\u00df", "dein", "Haupt", "so", "nach", "dem", "Bo\u00b7den", "nickt", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sogleich versetzte die dem Br\u00fcdergen zur Lehre:", "tokens": ["Sog\u00b7leich", "ver\u00b7setz\u00b7te", "die", "dem", "Br\u00fc\u00b7der\u00b7gen", "zur", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00bbich st\u00fcnde freylich nicht so tief hinab geb\u00fcckt,", "tokens": ["\u00bb", "ich", "st\u00fcn\u00b7de", "frey\u00b7lich", "nicht", "so", "tief", "hin\u00b7ab", "ge\u00b7b\u00fcckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann ich so leer, wie du, in meiner Stirne w\u00e4re.\u00ab", "tokens": ["Wann", "ich", "so", "leer", ",", "wie", "du", ",", "in", "mei\u00b7ner", "Stir\u00b7ne", "w\u00e4\u00b7re", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}