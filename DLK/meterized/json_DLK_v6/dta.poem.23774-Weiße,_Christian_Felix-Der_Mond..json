{"dta.poem.23774": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Mond.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00df und freundlich lacht", "tokens": ["Wie", "s\u00fc\u00df", "und", "freund\u00b7lich", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Monden stille Pracht,", "tokens": ["Des", "Mon\u00b7den", "stil\u00b7le", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den ich von jener H\u00f6h", "tokens": ["Den", "ich", "von", "je\u00b7ner", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Her\u00fcber steigen seh.", "tokens": ["Her\u00b7\u00fc\u00b7ber", "stei\u00b7gen", "seh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sehe gl\u00e4nzend ihn", "tokens": ["Ich", "se\u00b7he", "gl\u00e4n\u00b7zend", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf jenen B\u00e4umen gl\u00fchn,", "tokens": ["Auf", "je\u00b7nen", "B\u00e4u\u00b7men", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wie der Ph\u00f6nix ruht", "tokens": ["So", "wie", "der", "Ph\u00f6\u00b7nix", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Rest voll Gluth.", "tokens": ["In", "sei\u00b7nem", "Rest", "voll", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein sein silbern Bild", "tokens": ["Al\u00b7lein", "sein", "sil\u00b7bern", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Ist ruhig, lieblich, mild,", "tokens": ["Ist", "ru\u00b7hig", ",", "lieb\u00b7lich", ",", "mild", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er l\u00e4chelt jedem Ruh", "tokens": ["Er", "l\u00e4\u00b7chelt", "je\u00b7dem", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und s\u00fc\u00dfe Stille zu.", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Stil\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Weisheit gleichet ihm,", "tokens": ["Die", "Weis\u00b7heit", "glei\u00b7chet", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nie wild und ungest\u00fcm,", "tokens": ["Nie", "wild", "und", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die jedem, der sie liebt,", "tokens": ["Die", "je\u00b7dem", ",", "der", "sie", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auch gleiche Sanftmuth giebt.", "tokens": ["Auch", "glei\u00b7che", "Sanft\u00b7muth", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sein liebreich Angesicht", "tokens": ["Sein", "lieb\u00b7reich", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4rbt sich vom Sonnenlicht,", "tokens": ["F\u00e4rbt", "sich", "vom", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum denn? ohne die\u00df", "tokens": ["Wa\u00b7rum", "denn", "?", "oh\u00b7ne", "die\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$.", "APPR", "PDS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bedeckt es Finsterni\u00df.", "tokens": ["Be\u00b7deckt", "es", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So mu\u00df der Tugend Schein", "tokens": ["So", "mu\u00df", "der", "Tu\u00b7gend", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Weisheit Glanz verleihn \u2014", "tokens": ["Der", "Weis\u00b7heit", "Glanz", "ver\u00b7leihn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich, Weisheit, such auch ich,", "tokens": ["Dich", ",", "Weis\u00b7heit", ",", "such", "auch", "ich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch, Tugend, blo\u00df durch dich!", "tokens": ["Doch", ",", "Tu\u00b7gend", ",", "blo\u00df", "durch", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}