{"textgrid.poem.48379": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Harry war ein tapfres Blut,", "tokens": ["Mein", "Har\u00b7ry", "war", "ein", "tapf\u00b7res", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah ihn neben der Fahne gehn,", "tokens": ["Ich", "sah", "ihn", "ne\u00b7ben", "der", "Fah\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun ist er \u00fcber die gro\u00dfe Flut", "tokens": ["Nun", "ist", "er", "\u00fc\u00b7ber", "die", "gro\u00b7\u00dfe", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf Nimmer-, Nimmer-Wiedersehn;", "tokens": ["Auf", "Nim\u00b7mer", ",", "Nim\u00b7mer\u00b7Wie\u00b7der\u00b7sehn", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "TRUNC", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und doch nur einmal herzen ihn,", "tokens": ["Und", "doch", "nur", "ein\u00b7mal", "her\u00b7zen", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was g\u00e4b' ich alles nicht drum hin!", "tokens": ["Was", "g\u00e4b'", "ich", "al\u00b7les", "nicht", "drum", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "PTKNEG", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich g\u00e4b' unser Hafer- und Gerstenland", "tokens": ["Ich", "g\u00e4b'", "un\u00b7ser", "Ha\u00b7fer", "und", "Gers\u00b7ten\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "F\u00fcr den kleinen Finger von seiner Hand.", "tokens": ["F\u00fcr", "den", "klei\u00b7nen", "Fin\u00b7ger", "von", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "0ft, wenn es still geworden im Haus", "tokens": ["ft", ",", "wenn", "es", "still", "ge\u00b7wor\u00b7den", "im", "Haus"], "token_info": ["measurement", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAPP", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und von Abend her die L\u00fcfte wehn,", "tokens": ["Und", "von", "A\u00b7bend", "her", "die", "L\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APZR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann frag' ich in den Wind hinaus:", "tokens": ["Dann", "frag'", "ich", "in", "den", "Wind", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werd' ich ihn nimmer wiedersehn?", "tokens": ["Werd'", "ich", "ihn", "nim\u00b7mer", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn sehn, nur einmal herzen ihn,", "tokens": ["Ihn", "sehn", ",", "nur", "ein\u00b7mal", "her\u00b7zen", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was g\u00e4b' ich alles nicht drum hin!", "tokens": ["Was", "g\u00e4b'", "ich", "al\u00b7les", "nicht", "drum", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "PTKNEG", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich g\u00e4b' unser Hafer- und Gerstenland", "tokens": ["Ich", "g\u00e4b'", "un\u00b7ser", "Ha\u00b7fer", "und", "Gers\u00b7ten\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "TRUNC", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "F\u00fcr den kleinen Finger von seiner Hand.", "tokens": ["F\u00fcr", "den", "klei\u00b7nen", "Fin\u00b7ger", "von", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O hingen einige Schurken hoch", "tokens": ["O", "hin\u00b7gen", "ei\u00b7ni\u00b7ge", "Schur\u00b7ken", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lie\u00df' uns Gott einen R\u00e4cher erstehn,", "tokens": ["Und", "lie\u00df'", "uns", "Gott", "ei\u00b7nen", "R\u00e4\u00b7cher", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da k\u00e4men frohe Tage noch", "tokens": ["Da", "k\u00e4\u00b7men", "fro\u00b7he", "Ta\u00b7ge", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und den Liebsten w\u00fcrd' ich wiedersehn;", "tokens": ["Und", "den", "Liebs\u00b7ten", "w\u00fcrd'", "ich", "wie\u00b7der\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihn sehn, nur einmal herzen ihn,", "tokens": ["Ihn", "sehn", ",", "nur", "ein\u00b7mal", "her\u00b7zen", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was g\u00e4b' ich alles nicht drum hin!", "tokens": ["Was", "g\u00e4b'", "ich", "al\u00b7les", "nicht", "drum", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "PTKNEG", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich g\u00e4b' unser Hafer- und Gerstenland", "tokens": ["Ich", "g\u00e4b'", "un\u00b7ser", "Ha\u00b7fer", "und", "Gers\u00b7ten\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "TRUNC", "KON",