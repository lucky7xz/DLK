{"textgrid.poem.46270": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist kein m\u00e4dlin in der stat,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist kein m\u00e4dlin in der stat,", "tokens": ["Es", "ist", "kein", "m\u00e4d\u00b7lin", "in", "der", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "das Witzleer nicht wert und lieb hat,", "tokens": ["das", "Witz\u00b7leer", "nicht", "wert", "und", "lieb", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "spricht er, ob wahr, kan ich nicht wissen;", "tokens": ["spricht", "er", ",", "ob", "wahr", ",", "kan", "ich", "nicht", "wis\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADJD", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "das aber kan ich wol vergwissen,", "tokens": ["das", "a\u00b7ber", "kan", "ich", "wol", "ver\u00b7gwis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df keine jungfrau in der stat", "tokens": ["da\u00df", "kei\u00b7ne", "jung\u00b7frau", "in", "der", "stat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "lieb und wert den Witzleeren hat.", "tokens": ["lieb", "und", "wert", "den", "Witz\u00b7lee\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}