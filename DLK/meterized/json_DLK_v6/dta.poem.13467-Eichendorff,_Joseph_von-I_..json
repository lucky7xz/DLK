{"dta.poem.13467": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wandern lieb' ich f\u00fcr mein Leben,", "tokens": ["Wan\u00b7dern", "lieb'", "ich", "f\u00fcr", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebe eben wie ich kann,", "tokens": ["Le\u00b7be", "e\u00b7ben", "wie", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt' ich mir auch M\u00fche geben,", "tokens": ["Wollt'", "ich", "mir", "auch", "M\u00fc\u00b7he", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pa\u00dft' es mir doch gar nicht an.", "tokens": ["Pa\u00dft'", "es", "mir", "doch", "gar", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne alte Lieder wei\u00df ich,", "tokens": ["Sch\u00f6\u00b7ne", "al\u00b7te", "Lie\u00b7der", "wei\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der K\u00e4lte, ohne Schuh'", "tokens": ["In", "der", "K\u00e4l\u00b7te", ",", "oh\u00b7ne", "Schuh'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen in die Saiten rei\u00df' ich,", "tokens": ["Drau\u00b7\u00dfen", "in", "die", "Sai\u00b7ten", "rei\u00df'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df nicht, wo ich Abend's ruh'.", "tokens": ["Wei\u00df", "nicht", ",", "wo", "ich", "A\u00b7ben\u00b7d's", "ruh'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Manche Sch\u00f6ne macht wohl Augen,", "tokens": ["Man\u00b7che", "Sch\u00f6\u00b7ne", "macht", "wohl", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinet, ich gefiel' ihr sehr,", "tokens": ["Mei\u00b7net", ",", "ich", "ge\u00b7fiel'", "ihr", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich nur was wollte taugen,", "tokens": ["Wenn", "ich", "nur", "was", "woll\u00b7te", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PWS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So ein armer Lump nicht w\u00e4r'. \u2014", "tokens": ["So", "ein", "ar\u00b7mer", "Lump", "nicht", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKNEG", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag dir Gott ein'n Mann bescheeren,", "tokens": ["Mag", "dir", "Gott", "ein'n", "Mann", "be\u00b7schee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl mit Haus und Hof versehn!", "tokens": ["Wohl", "mit", "Haus", "und", "Hof", "ver\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir zwei zusammen w\u00e4ren,", "tokens": ["Wenn", "wir", "zwei", "zu\u00b7sam\u00b7men", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht' mein Singen mir vergehn.", "tokens": ["M\u00f6cht'", "mein", "Sin\u00b7gen", "mir", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}