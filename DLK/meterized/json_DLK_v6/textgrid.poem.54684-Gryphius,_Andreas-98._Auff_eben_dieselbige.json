{"textgrid.poem.54684": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "98. Auff eben dieselbige", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann in sein K\u00f6nigreich ein F\u00fcrst den Einzug h\u00e4lt/", "tokens": ["Wann", "in", "sein", "K\u00f6\u00b7nig\u00b7reich", "ein", "F\u00fcrst", "den", "Ein\u00b7zug", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Folgt/ wer vorhin verbannt/ ihm itzt zu Fusse f\u00e4lt.", "tokens": ["Folgt", "/", "wer", "vor\u00b7hin", "ver\u00b7bannt", "/", "ihm", "itzt", "zu", "Fus\u00b7se", "f\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "ADV", "VVPP", "$(", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drumb nun der Herr den Thron wil durch das Creutz einnehmen:", "tokens": ["Drumb", "nun", "der", "Herr", "den", "Thron", "wil", "durch", "das", "Creutz", "ein\u00b7neh\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will er auch M\u00f6rder nicht sich mit zu f\u00fchren sch\u00e4men.", "tokens": ["Will", "er", "auch", "M\u00f6r\u00b7der", "nicht", "sich", "mit", "zu", "f\u00fch\u00b7ren", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "PTKNEG", "PRF", "APPR", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}