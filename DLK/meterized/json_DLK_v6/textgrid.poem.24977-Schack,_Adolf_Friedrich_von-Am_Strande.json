{"textgrid.poem.24977": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Strande", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Strand, von Flocken Schaumes \u00fcbertaut,", "tokens": ["Am", "Strand", ",", "von", "Flo\u00b7cken", "Schau\u00b7mes", "\u00fc\u00b7bert\u00b7aut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lieg' ich gestreckt in duft'ges Heidekraut.", "tokens": ["Lieg'", "ich", "ge\u00b7streckt", "in", "duft'\u00b7ges", "Hei\u00b7de\u00b7kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schaue, wie die Flut in Gr\u00fcn und Gold", "tokens": ["Ich", "schau\u00b7e", ",", "wie", "die", "Flut", "in", "Gr\u00fcn", "und", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Purpur wechselnd mir zu F\u00fc\u00dfen rollt,", "tokens": ["Und", "Pur\u00b7pur", "wech\u00b7selnd", "mir", "zu", "F\u00fc\u00b7\u00dfen", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mir ans Ohr t\u00f6nt in der Wogen Schwall", "tokens": ["Und", "mir", "ans", "Ohr", "t\u00f6nt", "in", "der", "Wo\u00b7gen", "Schwall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Geliebter Stimmen Widerhall.", "tokens": ["Ge\u00b7lieb\u00b7ter", "Stim\u00b7men", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fern durch der schaumbekr\u00f6nten Wellen Tanz,", "tokens": ["Fern", "durch", "der", "schaum\u00b7be\u00b7kr\u00f6n\u00b7ten", "Wel\u00b7len", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was schimmert wei\u00df im Mittagssonnenglanz?", "tokens": ["Was", "schim\u00b7mert", "wei\u00df", "im", "Mit\u00b7tags\u00b7son\u00b7nen\u00b7glanz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Segel ist's; und noch ein andres blinkt,", "tokens": ["Ein", "Se\u00b7gel", "ist's", ";", "und", "noch", "ein", "and\u00b7res", "blinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "KON", "ADV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indes die Flut sich hebt und wieder sinkt.", "tokens": ["In\u00b7des", "die", "Flut", "sich", "hebt", "und", "wie\u00b7der", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie nahn! sie nahn! Die Fahrt geht k\u00fcstenw\u00e4rts!", "tokens": ["Sie", "nahn", "!", "sie", "nahn", "!", "Die", "Fahrt", "geht", "k\u00fcs\u00b7ten\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was klopfst du, ungest\u00fcmes Herz?", "tokens": ["Was", "klopfst", "du", ",", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Herz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoffnungen werden, die ich fast verga\u00df,", "tokens": ["Hoff\u00b7nun\u00b7gen", "wer\u00b7den", ",", "die", "ich", "fast", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von neuem wach; was ich vordem besa\u00df,", "tokens": ["Von", "neu\u00b7em", "wach", ";", "was", "ich", "vor\u00b7dem", "be\u00b7sa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PTKVZ", "$.", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Teuern all, die ich verlor, das Gl\u00fcck,", "tokens": ["Die", "Teu\u00b7ern", "all", ",", "die", "ich", "ver\u00b7lor", ",", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die erste Liebe, kehren sie zur\u00fcck? \u2013 \u2013", "tokens": ["Die", "ers\u00b7te", "Lie\u00b7be", ",", "keh\u00b7ren", "sie", "zu\u00b7r\u00fcck", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach! in die Ferne schwinden, sichtbar kaum,", "tokens": ["Ach", "!", "in", "die", "Fer\u00b7ne", "schwin\u00b7den", ",", "sicht\u00b7bar", "kaum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Segel hin am Himmelssaum.", "tokens": ["Die", "Se\u00b7gel", "hin", "am", "Him\u00b7mels\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}