{"textgrid.poem.58658": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mancher hat sich nicht bem\u00fcht, zu heilen Babel wieder,", "tokens": ["Wie", "man\u00b7cher", "hat", "sich", "nicht", "be\u00b7m\u00fcht", ",", "zu", "hei\u00b7len", "Ba\u00b7bel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PRF", "PTKNEG", "VVPP", "$,", "PTKZU", "VVINF", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wie mancher hat im Eifergeist sie wollen rei\u00dfen nieder!", "tokens": ["Wie", "man\u00b7cher", "hat", "im", "Ei\u00b7fer\u00b7geist", "sie", "wol\u00b7len", "rei\u00b7\u00dfen", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "PPER", "VMFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Doch bleibet Babel Babel noch, sie wird geheilet nie;", "tokens": ["Doch", "blei\u00b7bet", "Ba\u00b7bel", "Ba\u00b7bel", "noch", ",", "sie", "wird", "ge\u00b7hei\u00b7let", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "ADV", "$,", "PPER", "VAFIN", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Gott kennt ihr gro\u00df Verderben wohl, Gott selbst will strafen sie.", "tokens": ["Gott", "kennt", "ihr", "gro\u00df", "Ver\u00b7der\u00b7ben", "wohl", ",", "Gott", "selbst", "will", "stra\u00b7fen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "NN", "ADV", "$,", "NN", "ADV", "VMFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Erweckte Seelen, merket doch, ihr k\u00f6nnt die M\u00fche sparen,", "tokens": ["Er\u00b7weck\u00b7te", "See\u00b7len", ",", "mer\u00b7ket", "doch", ",", "ihr", "k\u00f6nnt", "die", "M\u00fc\u00b7he", "spa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Nicht heilen und nicht eifern mehr, la\u00dft Babel stille fahren;", "tokens": ["Nicht", "hei\u00b7len", "und", "nicht", "ei\u00b7fern", "mehr", ",", "la\u00dft", "Ba\u00b7bel", "stil\u00b7le", "fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "ADV", "ADV", "$,", "VVIMP", "NE", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Ein jeder kehr' nur in sein Land und in sein Herz hinein,", "tokens": ["Ein", "je\u00b7der", "kehr'", "nur", "in", "sein", "Land", "und", "in", "sein", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Da merkt und preiset Gottes Werk, wenn ihr gerecht wollt sein!", "tokens": ["Da", "merkt", "und", "prei\u00b7set", "Got\u00b7tes", "Werk", ",", "wenn", "ihr", "ge\u00b7recht", "wollt", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "NN", "NN", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}