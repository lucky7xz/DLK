{"textgrid.poem.50666": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fccks- und Ungl\u00fccksstern", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat ein Ungl\u00fcckskind, wie du, gewisse Stunden,", "tokens": ["Es", "hat", "ein", "Un\u00b7gl\u00fccks\u00b7kind", ",", "wie", "du", ",", "ge\u00b7wis\u00b7se", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit welchen sich sein Gl\u00fcck verbunden,", "tokens": ["Mit", "wel\u00b7chen", "sich", "sein", "Gl\u00fcck", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonnenweiser zeigt sie an;", "tokens": ["Der", "Son\u00b7nen\u00b7wei\u00b7ser", "zeigt", "sie", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur da\u00df er sie nicht so, wie du, erkennen kann:", "tokens": ["Nur", "da\u00df", "er", "sie", "nicht", "so", ",", "wie", "du", ",", "er\u00b7ken\u00b7nen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wenn aber ", "tokens": ["Wenn", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}}}}