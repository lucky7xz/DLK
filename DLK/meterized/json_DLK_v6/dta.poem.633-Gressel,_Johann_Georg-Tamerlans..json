{"dta.poem.633": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Tamerlans.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Den T\u00fcrcken/ Russen und Sines hab ich besieget/", "tokens": ["Den", "T\u00fcr\u00b7cken", "/", "Rus\u00b7sen", "und", "Si\u00b7nes", "hab", "ich", "be\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "KON", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+---+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Tartar stellte mir sein ", "tokens": ["Der", "Tar\u00b7tar", "stell\u00b7te", "mir", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Persen Tauris ist durch meine Macht bekrieget/", "tokens": ["Der", "Per\u00b7sen", "Tau\u00b7ris", "ist", "durch", "mei\u00b7ne", "Macht", "be\u00b7krie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier liegt ein k\u00fchner Held/ der grosse ", "tokens": ["Hier", "liegt", "ein", "k\u00fch\u00b7ner", "Held", "/", "der", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}