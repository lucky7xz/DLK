{"textgrid.poem.55753": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Traum der keuschen Liebe,", "tokens": ["Der", "Traum", "der", "keu\u00b7schen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4ngst ist er ausgetr\u00e4umt,", "tokens": ["L\u00e4ngst", "ist", "er", "aus\u00b7ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es tanzen und toben die Nerven,", "tokens": ["Es", "tan\u00b7zen", "und", "to\u00b7ben", "die", "Ner\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Blut zum Hirne sch\u00e4umt;", "tokens": ["Das", "Blut", "zum", "Hir\u00b7ne", "sch\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es bricht sich in wilden Kaskaden", "tokens": ["Es", "bricht", "sich", "in", "wil\u00b7den", "Kas\u00b7ka\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Am Herzen, verdorrt und versteint,", "tokens": ["Am", "Her\u00b7zen", ",", "ver\u00b7dorrt", "und", "ver\u00b7steint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Das seine verbissenen Qualen", "tokens": ["Das", "sei\u00b7ne", "ver\u00b7bis\u00b7se\u00b7nen", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Versch\u00fcttet und ausgeweint.", "tokens": ["Ver\u00b7sch\u00fct\u00b7tet", "und", "aus\u00b7ge\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ich will meine Z\u00e4hne vergraben", "tokens": ["Ich", "will", "mei\u00b7ne", "Z\u00e4h\u00b7ne", "ver\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "In Deinem knirschenden Haar,", "tokens": ["In", "Dei\u00b7nem", "knir\u00b7schen\u00b7den", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Im Blutrausch will ich vergessen,", "tokens": ["Im", "Blu\u00b7trausch", "will", "ich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Da\u00df ich ein Anderer war.", "tokens": ["Da\u00df", "ich", "ein", "An\u00b7de\u00b7rer", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Ich wei\u00df, Du kannst genie\u00dfen,", "tokens": ["Ich", "wei\u00df", ",", "Du", "kannst", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Unfa\u00dfbar, riesenhaft stark,", "tokens": ["Un\u00b7fa\u00df\u00b7bar", ",", "rie\u00b7sen\u00b7haft", "stark", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Wohlan, so genie\u00df' mich, Lucia \u2013", "tokens": ["Wo\u00b7hlan", ",", "so", "ge\u00b7ni\u00b7e\u00df'", "mich", ",", "Lu\u00b7cia", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVIMP", "PPER", "$,", "NE", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Es schreit nach F\u00e4ulnis mein Mark.", "tokens": ["Es", "schreit", "nach", "F\u00e4ul\u00b7nis", "mein", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}