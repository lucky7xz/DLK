{"textgrid.poem.53376": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "An denselben seelig verstorbenen lieben Mann", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man wolte dich begraben,", "tokens": ["Man", "wol\u00b7te", "dich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Ohm, und soltest nicht", "tokens": ["Mein", "Ohm", ",", "und", "sol\u00b7test", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VMFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von mir ein Denckmal haben", "tokens": ["Von", "mir", "ein", "Denck\u00b7mal", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Br\u00fcderlichen Pflicht;", "tokens": ["Der", "Br\u00fc\u00b7der\u00b7li\u00b7chen", "Pflicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00e4llt unsre Lieb' auch nieder", "tokens": ["F\u00e4llt", "uns\u00b7re", "Lieb'", "auch", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gleich vnter einen Stein,", "tokens": ["Gleich", "vn\u00b7ter", "ei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd sol, wie deine Glieder,", "tokens": ["Vnd", "sol", ",", "wie", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit eingescharret seyn?", "tokens": ["Mit", "ein\u00b7ge\u00b7schar\u00b7ret", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Natura w\u00fcrd' es hassen", "tokens": ["Na\u00b7tu\u00b7ra", "w\u00fcrd'", "es", "has\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch die wir uns geliebt,", "tokens": ["Durch", "die", "wir", "uns", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd Phoebus mich verlassen", "tokens": ["Vnd", "Phoe\u00b7bus", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der mir die Seiten giebt,", "tokens": ["Der", "mir", "die", "Sei\u00b7ten", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die mich erhalten werden", "tokens": ["Die", "mich", "er\u00b7hal\u00b7ten", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ich schon l\u00e4ngst verreckt,", "tokens": ["Wenn", "ich", "schon", "l\u00e4ngst", "ver\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd mit der schwartzen Erden", "tokens": ["Vnd", "mit", "der", "schwart\u00b7zen", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bin ewig zugedeckt.", "tokens": ["Bin", "e\u00b7wig", "zu\u00b7ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sey sicher, wenn die alle", "tokens": ["Sey", "si\u00b7cher", ",", "wenn", "die", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ART", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So mir zu wieder sind", "tokens": ["So", "mir", "zu", "wie\u00b7der", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PTKA", "ADV", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Eiter-voller Galle,", "tokens": ["Aus", "Ei\u00b7ter\u00b7vol\u00b7ler", "Gal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind zehnmal Rauch vnd Wind,", "tokens": ["Sind", "zehn\u00b7mal", "Rauch", "vnd", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wird man uns noch zeigen", "tokens": ["So", "wird", "man", "uns", "noch", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Belebt vnd jung vnd frey", "tokens": ["Be\u00b7lebt", "vnd", "jung", "vnd", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich wegen meiner Geigen,", "tokens": ["Mich", "we\u00b7gen", "mei\u00b7ner", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich wegen deiner Trew.", "tokens": ["Dich", "we\u00b7gen", "dei\u00b7ner", "Trew", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich weis vmb dein Gem\u00fcte", "tokens": ["Ich", "weis", "vmb", "dein", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drumb liebet' ich dich sehr,", "tokens": ["Drumb", "lie\u00b7bet'", "ich", "dich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es f\u00fcgt vns das Gebl\u00fcte,", "tokens": ["Es", "f\u00fcgt", "vns", "das", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sinnen Eintracht mehr:", "tokens": ["Der", "Sin\u00b7nen", "Ein\u00b7tracht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo sind die s\u00fcssen Stunden,", "tokens": ["Wo", "sind", "die", "s\u00fcs\u00b7sen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo mancher Wege Fahrt", "tokens": ["Wo", "man\u00b7cher", "We\u00b7ge", "Fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die vns genaw verbunden", "tokens": ["Die", "vns", "ge\u00b7naw", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach Br\u00fcderlicher Art?", "tokens": ["Nach", "Br\u00fc\u00b7der\u00b7li\u00b7cher", "Art", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie wir verkn\u00fcpffet wahren", "tokens": ["Wie", "wir", "ver\u00b7kn\u00fcpf\u00b7fet", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So war auch vnser Stand,", "tokens": ["So", "war", "auch", "vn\u00b7ser", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat in gleichen Jahren", "tokens": ["Es", "hat", "in", "glei\u00b7chen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schul uns beyd' erkant,", "tokens": ["Die", "Schul", "uns", "beyd'", "er\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir freyten auch zusammen", "tokens": ["Wir", "frey\u00b7ten", "auch", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Pohliynchen die ward mein,", "tokens": ["Poh\u00b7liyn\u00b7chen", "die", "ward", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stracks nam in keuschen Flammen", "tokens": ["Stracks", "nam", "in", "keu\u00b7schen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich auch die Mackinn ein.", "tokens": ["Dich", "auch", "die", "Ma\u00b7ckinn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "F\u00fcnff Kinder hab ich leben", "tokens": ["F\u00fcnff", "Kin\u00b7der", "hab", "ich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du sahst derselben vier,", "tokens": ["Du", "sahst", "der\u00b7sel\u00b7ben", "vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott wird dem F\u00fcnfften geben", "tokens": ["Gott", "wird", "dem", "F\u00fcnff\u00b7ten", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch bald der Sonnen Zier,", "tokens": ["Auch", "bald", "der", "Son\u00b7nen", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wann es wird geschehen", "tokens": ["Doch", "wann", "es", "wird", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df es das sch\u00f6ne Liecht", "tokens": ["Da\u00df", "es", "das", "sch\u00f6\u00b7ne", "Liecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Tages gleich kan sehen,", "tokens": ["Des", "Ta\u00b7ges", "gleich", "kan", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es sieht den Vater nicht.", "tokens": ["Es", "sieht", "den", "Va\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch pflagst du stets zu klagen,", "tokens": ["Auch", "pflagst", "du", "stets", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gesund seyn wolle sich", "tokens": ["Ge\u00b7sund", "seyn", "wol\u00b7le", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "VMFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie recht mit dir vertragen", "tokens": ["Nie", "recht", "mit", "dir", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df eben klag' auch ich,", "tokens": ["Die\u00df", "e\u00b7ben", "klag'", "auch", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hieraus pflag ich zu schliessen", "tokens": ["Hier\u00b7aus", "pflag", "ich", "zu", "schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich w\u00fcrd' erst sterben, nein,", "tokens": ["Ich", "w\u00fcrd'", "erst", "ster\u00b7ben", ",", "nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "PTKANT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Ordnung wird zerrissen,", "tokens": ["Die", "Ord\u00b7nung", "wird", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich lebe, du gehst ein.", "tokens": ["Ich", "le\u00b7be", ",", "du", "gehst", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was sol ich nun beginnen?", "tokens": ["Was", "sol", "ich", "nun", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wen geh ich ferner an", "tokens": ["Wen", "geh", "ich", "fer\u00b7ner", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit so vertrauten Sinnen", "tokens": ["Mit", "so", "ver\u00b7trau\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Als wie ich dir gethan", "tokens": ["Als", "wie", "ich", "dir", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PPER", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bey also b\u00f6sen Zeiten", "tokens": ["Bey", "al\u00b7so", "b\u00f6\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als man sie je erkant,", "tokens": ["Als", "man", "sie", "je", "er\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da mir von allen Seiten", "tokens": ["Da", "mir", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Freunde sind entwandt.", "tokens": ["Die", "Freun\u00b7de", "sind", "ent\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zwar dir ist wol gerahten", "tokens": ["Zwar", "dir", "ist", "wol", "ge\u00b7rah\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jetzt siehest du den Gott", "tokens": ["Jetzt", "sie\u00b7hest", "du", "den", "Gott"], "