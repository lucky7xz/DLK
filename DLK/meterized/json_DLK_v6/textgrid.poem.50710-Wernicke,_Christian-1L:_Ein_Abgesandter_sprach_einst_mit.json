{"textgrid.poem.50710": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Abgesandter sprach einst mit ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Abgesandter sprach einst mit ", "tokens": ["Ein", "Ab\u00b7ge\u00b7sand\u00b7ter", "sprach", "einst", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im k\u00f6niglichen Vorgemache,", "tokens": ["Im", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Vor\u00b7ge\u00b7ma\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und als er ihr verwies, da\u00df, was in einer Sache", "tokens": ["Und", "als", "er", "ihr", "ver\u00b7wies", ",", "da\u00df", ",", "was", "in", "ei\u00b7ner", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie kurz zuvor gesprochen h\u00e4tt',", "tokens": ["Sie", "kurz", "zu\u00b7vor", "ge\u00b7spro\u00b7chen", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht Gottes Wort gem\u00e4\u00df gewesen,", "tokens": ["Nicht", "Got\u00b7tes", "Wort", "ge\u00b7m\u00e4\u00df", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "NN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch manchen Bibelspruch ihr zum Beweise brachte:", "tokens": ["Auch", "man\u00b7chen", "Bi\u00b7bel\u00b7spruch", "ihr", "zum", "Be\u00b7wei\u00b7se", "brach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u00bbihr habt die ", "tokens": ["\u00bb", "ihr", "habt", "die"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}