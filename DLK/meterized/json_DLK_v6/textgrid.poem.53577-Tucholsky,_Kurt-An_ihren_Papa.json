{"textgrid.poem.53577": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An ihren Papa", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amici! Plaudite! \u2013 Die bunten B\u00e4nder", "tokens": ["A\u00b7mi\u00b7ci", "!", "Plau\u00b7di\u00b7te", "!", "\u2013", "Die", "bun\u00b7ten", "B\u00e4n\u00b7der"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "und Wimpel flattern froh im Wind!", "tokens": ["und", "Wim\u00b7pel", "flat\u00b7tern", "froh", "im", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie danke ich dir g\u00fctigem Spender", "tokens": ["Wie", "dan\u00b7ke", "ich", "dir", "g\u00fc\u00b7ti\u00b7gem", "Spen\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "f\u00fcr dieses Kind! \u2013", "tokens": ["f\u00fcr", "die\u00b7ses", "Kind", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du w\u00fcrdiger Greis \u2013 vor so und so viel Jahren", "tokens": ["Du", "w\u00fcr\u00b7di\u00b7ger", "Greis", "\u2013", "vor", "so", "und", "so", "viel", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "APPR", "ADV", "KON", "ADV", "PIAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "erzeugtest dus in einer Nacht . . .", "tokens": ["er\u00b7zeug\u00b7test", "dus", "in", "ei\u00b7ner", "Nacht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, da\u00df dies bei ungebleichten Haaren", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "dies", "bei", "un\u00b7ge\u00b7bleich\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schon M\u00fche macht.", "tokens": ["schon", "M\u00fc\u00b7he", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und du, im r\u00fcstigen Mannesalter,", "tokens": ["Und", "du", ",", "im", "r\u00fcs\u00b7ti\u00b7gen", "Man\u00b7ne\u00b7sal\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "du produziertest dies ", "tokens": ["du", "pro\u00b7du\u00b7zier\u00b7test", "dies"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ein Frauenseufzer . . . leis verhallt er . . . \u2013", "tokens": ["ein", "Frau\u00b7en\u00b7seuf\u00b7zer", ".", ".", ".", "leis", "ver\u00b7hallt", "er", ".", ".", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$.", "$.", "ADJD", "VVFIN", "PPER", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir Evo\u00eb! \u2013", "tokens": ["Dir", "E\u00b7vo\u00eb", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "NE", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Dir Evo\u00eb! \u2013 Ich gratuliere!", "tokens": ["Dir", "E\u00b7vo\u00eb", "!", "\u2013", "Ich", "gra\u00b7tu\u00b7lie\u00b7re", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein denk ich, Autor, ist sie da \u2013", "tokens": ["Dein", "denk", "ich", ",", "Au\u00b7tor", ",", "ist", "sie", "da", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein denk ich, wenn ich kokettiere \u2013", "tokens": ["Dein", "denk", "ich", ",", "wenn", "ich", "ko\u00b7ket\u00b7tie\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df Gott, Papa!", "tokens": ["Gr\u00fc\u00df", "Gott", ",", "Pa\u00b7pa", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}