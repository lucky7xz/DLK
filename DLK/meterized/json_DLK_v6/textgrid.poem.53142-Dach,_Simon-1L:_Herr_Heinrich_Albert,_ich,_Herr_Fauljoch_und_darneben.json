{"textgrid.poem.53142": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Heinrich Albert, ich, Herr Fauljoch und darneben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Heinrich Albert, ich, Herr Fauljoch und darneben", "tokens": ["Herr", "Hein\u00b7rich", "Al\u00b7bert", ",", "ich", ",", "Herr", "Faul\u00b7joch", "und", "dar\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "$,", "PPER", "$,", "NN", "NN", "KON", "PAV"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Herr Robertin, sind heut entschlossen recht zu leben,", "tokens": ["Herr", "Ro\u00b7ber\u00b7tin", ",", "sind", "heut", "ent\u00b7schlos\u00b7sen", "recht", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VAFIN", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zwar bey euch, Herr Bluhm, als dem nichts liebers ist,", "tokens": ["Und", "zwar", "bey", "euch", ",", "Herr", "Bluhm", ",", "als", "dem", "nichts", "lie\u00b7bers", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$,", "NN", "NN", "$,", "KOUS", "ART", "PIS", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn da\u00df man ohne Scheu, ohn Heucheley und List", "tokens": ["Denn", "da\u00df", "man", "oh\u00b7ne", "Scheu", ",", "ohn", "Heu\u00b7che\u00b7ley", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "NN", "$,", "KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf diesen Tag zu euch mag vor die Pfanne kommen", "tokens": ["Auf", "die\u00b7sen", "Tag", "zu", "euch", "mag", "vor", "die", "Pfan\u00b7ne", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPR", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und thun, was Redlichkeit und wahre Treu der Frommen", "tokens": ["Und", "thun", ",", "was", "Red\u00b7lich\u00b7keit", "und", "wah\u00b7re", "Treu", "der", "From\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "PWS", "NN", "KON", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu thun erlaubet hat. Herr Robert, unser Hertz,", "tokens": ["Zu", "thun", "er\u00b7lau\u00b7bet", "hat", ".", "Herr", "Ro\u00b7bert", ",", "un\u00b7ser", "Hertz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "VAFIN", "$.", "NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bringt weise Tischgespr\u00e4ch, Herr Fauljoch treugen Schertz,", "tokens": ["Bringt", "wei\u00b7se", "Tischge\u00b7spr\u00e4ch", ",", "Herr", "Faul\u00b7joch", "treu\u00b7gen", "Schertz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Herr Heinrich l\u00e4sset wo ein Instrument herholen", "tokens": ["Herr", "Hein\u00b7rich", "l\u00e4s\u00b7set", "wo", "ein", "Inst\u00b7ru\u00b7ment", "her\u00b7ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Und spielet uns was auf. Ich streich auf der Violen,", "tokens": ["Und", "spie\u00b7let", "uns", "was", "auf", ".", "Ich", "streich", "auf", "der", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Endschaft meiner Pein und Mutter meiner Ruh,", "tokens": ["Der", "End\u00b7schaft", "mei\u00b7ner", "Pein", "und", "Mut\u00b7ter", "mei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und h\u00e4tt ich eine Stimm, ich sing auch wohl dazu.", "tokens": ["Und", "h\u00e4tt", "ich", "ei\u00b7ne", "Stimm", ",", "ich", "sing", "auch", "wohl", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir Armen, die wir hier das Elend m\u00fcssen bauen,", "tokens": ["Wir", "Ar\u00b7men", ",", "die", "wir", "hier", "das", "E\u00b7lend", "m\u00fcs\u00b7sen", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was sind wir, wenn wir nicht einander wolten trauen", "tokens": ["Was", "sind", "wir", ",", "wenn", "wir", "nicht", "ein\u00b7an\u00b7der", "wol\u00b7ten", "trau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und gute Freunde seyn. Die stets nur vor sich hin", "tokens": ["Und", "gu\u00b7te", "Freun\u00b7de", "seyn", ".", "Die", "stets", "nur", "vor", "sich", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAINF", "$.", "ART", "ADV", "ADV", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Menschen-scheue sind, was thun die ihrem Sinn", "tokens": ["Und", "Men\u00b7schen\u00b7scheue", "sind", ",", "was", "thun", "die", "ih\u00b7rem", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$,", "PWS", "VVFIN", "ART", "PPOSAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Vor Leid und Pein nicht an! Sie haben sich verziehen", "tokens": ["Vor", "Leid", "und", "Pein", "nicht", "an", "!", "Sie", "ha\u00b7ben", "sich", "ver\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "PTKVZ", "$.", "PPER", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Rechtes der Natur, die anfangs uns verliehen", "tokens": ["Des", "Rech\u00b7tes", "der", "Na\u00b7tur", ",", "die", "an\u00b7fangs", "uns", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vnd noch will, da\u00df ein Mensch den andern suchen soll,", "tokens": ["Vnd", "noch", "will", ",", "da\u00df", "ein", "Mensch", "den", "an\u00b7dern", "su\u00b7chen", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit welchem er, es geh ihm \u00fcbel oder wohl,", "tokens": ["Mit", "wel\u00b7chem", "er", ",", "es", "geh", "ihm", "\u00fc\u00b7bel", "o\u00b7der", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zusammenhalten mag. Wir sehen an den Sternen,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", "mag.", "Wir", "se\u00b7hen", "an", "den", "Ster\u00b7nen", ","], "token_info": ["word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wie da\u00df sie Freunde sind. Die Wolcken ziehn von Fernen", "tokens": ["Wie", "da\u00df", "sie", "Freun\u00b7de", "sind", ".", "Die", "Wol\u00b7cken", "ziehn", "von", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie in Gesellschaft fort, die schnellen Fisch im Meer,", "tokens": ["Wie", "in", "Ge\u00b7sell\u00b7schaft", "fort", ",", "die", "schnel\u00b7len", "Fisch", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die V\u00f6gel in der Luft beschlie\u00dfen auch ein Heer", "tokens": ["Die", "V\u00f6\u00b7gel", "in", "der", "Luft", "be\u00b7schlie\u00b7\u00dfen", "auch", "ein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und streichen also hin, das Wild geht in den W\u00e4ldern", "tokens": ["Und", "strei\u00b7chen", "al\u00b7so", "hin", ",", "das", "Wild", "geht", "in", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,", "PRELS", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Gepart in gro\u00dfer Zahl, das Horn-Vieh auf den Feldern", "tokens": ["Ge\u00b7part", "in", "gro\u00b7\u00dfer", "Zahl", ",", "das", "Horn\u00b7Vieh", "auf", "den", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Kan niehmals eintzeln seyn; der Mensch entschl\u00e4get sich", "tokens": ["Kan", "nieh\u00b7mals", "eint\u00b7zeln", "seyn", ";", "der", "Mensch", "ent\u00b7schl\u00e4\u00b7get", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$.", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der G\u00fcter oft, wozu Gott ihn und mich und dich", "tokens": ["Der", "G\u00fc\u00b7ter", "oft", ",", "wo\u00b7zu", "Gott", "ihn", "und", "mich", "und", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "NN", "PPER", "KON", "PPER", "KON", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Gar wei\u00dflich hat gemacht. La\u00dft uns zusammenhalten,", "tokens": ["Gar", "wei\u00df\u00b7lich", "hat", "ge\u00b7macht", ".", "La\u00dft", "uns", "zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$.", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So wird die Freude mehr, das Ungl\u00fcck minder walten,", "tokens": ["So", "wird", "die", "Freu\u00b7de", "mehr", ",", "das", "Un\u00b7gl\u00fcck", "min\u00b7der", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Als wo man einsam ist. Was aber f\u00e4llt mir ein?", "tokens": ["Als", "wo", "man", "ein\u00b7sam", "ist", ".", "Was", "a\u00b7ber", "f\u00e4llt", "mir", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PIS", "ADJD", "VAFIN", "$.", "PWS", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn kommt Sybillen Tag? Ihr m\u00fcsst gebunden seyn.", "tokens": ["Wenn", "kommt", "Sy\u00b7bil\u00b7len", "Tag", "?", "Ihr", "m\u00fcsst", "ge\u00b7bun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "NN", "NN", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Frau Bluhmin, morgen will sich euer Fest ansagen,", "tokens": ["Frau", "Bluh\u00b7min", ",", "mor\u00b7gen", "will", "sich", "eu\u00b7er", "Fest", "an\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Tag, an welchem ihr sollt dieses B\u00e4ndlein tragen,", "tokens": ["Der", "Tag", ",", "an", "wel\u00b7chem", "ihr", "sollt", "die\u00b7ses", "B\u00e4n\u00b7dlein", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "An Zier und Ansehn schlecht, doch wenn ihr wissen wollt,", "tokens": ["An", "Zier", "und", "An\u00b7sehn", "schlecht", ",", "doch", "wenn", "ihr", "wis\u00b7sen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das Band ist unser Hertz, der Will in uns das Gold,", "tokens": ["Das", "Band", "ist", "un\u00b7ser", "Hertz", ",", "der", "Will", "in", "uns", "das", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das reinlich gl\u00e4ntzet trotz den k\u00f6stlichsten Metallen", "tokens": ["Das", "rein\u00b7lich", "gl\u00e4nt\u00b7zet", "trotz", "den", "k\u00f6st\u00b7lichs\u00b7ten", "Me\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.38": {"text": "Und was dem Auge sch\u00f6n auch sonsten mag gefallen.", "tokens": ["Und", "was", "dem", "Au\u00b7ge", "sch\u00f6n", "auch", "sons\u00b7ten", "mag", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "ADV", "ADV", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nehmt dieses so vorlieb und tragt es unbeschwehrt:", "tokens": ["Nehmt", "die\u00b7ses", "so", "vor\u00b7lieb", "und", "tragt", "es", "un\u00b7be\u00b7schwehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADV", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wer Tugend liebt, hat nie ein besser Band begehrt.", "tokens": ["Wer", "Tu\u00b7gend", "liebt", ",", "hat", "nie", "ein", "bes\u00b7ser", "Band", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}