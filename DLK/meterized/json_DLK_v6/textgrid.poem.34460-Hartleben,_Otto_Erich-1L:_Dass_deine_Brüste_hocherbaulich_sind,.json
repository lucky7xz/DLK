{"textgrid.poem.34460": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dass deine Br\u00fcste hocherbaulich sind,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass deine Br\u00fcste hocherbaulich sind,", "tokens": ["Dass", "dei\u00b7ne", "Br\u00fcs\u00b7te", "ho\u00b7cher\u00b7bau\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hat auch der Theologe tief empfunden", "tokens": ["hat", "auch", "der", "Theo\u00b7lo\u00b7ge", "tief", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VVPP"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "und will dich nun in keuscher Liebe retten.", "tokens": ["und", "will", "dich", "nun", "in", "keu\u00b7scher", "Lie\u00b7be", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du gutes Kind! Welch Seelenzwist f\u00fcr dich!", "tokens": ["Du", "gu\u00b7tes", "Kind", "!", "Welch", "See\u00b7lenz\u00b7wist", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich kenne das, auch ich war einmal fromm", "tokens": ["Ich", "ken\u00b7ne", "das", ",", "auch", "ich", "war", "ein\u00b7mal", "fromm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "$,", "ADV", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und hab ein sch\u00f6nes M\u00e4dchen retten wollen.", "tokens": ["und", "hab", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "ret\u00b7ten", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du armes Kind! Heut bin ich lasterhaft,", "tokens": ["Du", "ar\u00b7mes", "Kind", "!", "Heut", "bin", "ich", "las\u00b7ter\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und mich entz\u00fcckt dein junger, weisser Leib", "tokens": ["und", "mich", "ent\u00b7z\u00fcckt", "dein", "jun\u00b7ger", ",", "weis\u00b7ser", "Leib"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "weit mehr, als deine Tugend je verm\u00f6chte.", "tokens": ["weit", "mehr", ",", "als", "dei\u00b7ne", "Tu\u00b7gend", "je", "ver\u00b7m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So geh zu ihm und lass dich retten. \u2013 Nein?", "tokens": ["So", "geh", "zu", "ihm", "und", "lass", "dich", "ret\u00b7ten", ".", "\u2013", "Nein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "VVINF", "$.", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mich hast du lieb, der dich nicht anders will,", "tokens": ["Mich", "hast", "du", "lieb", ",", "der", "dich", "nicht", "an\u00b7ders", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "als dich die g\u00fctige Natur geschaffen? Wie?", "tokens": ["als", "dich", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", "ge\u00b7schaf\u00b7fen", "?", "Wie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$.", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u2013 O Kind: du bist so lasterhaft, wie ich!", "tokens": ["\u2013", "O", "Kind", ":", "du", "bist", "so", "las\u00b7ter\u00b7haft", ",", "wie", "ich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In s\u00fcndigen Gluthen schlingst du deine Arme", "tokens": ["In", "s\u00fcn\u00b7di\u00b7gen", "Glu\u00b7then", "schlingst", "du", "dei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "um mich, dein M\u00fcndchen spottet z\u00fcgellos", "tokens": ["um", "mich", ",", "dein", "M\u00fcnd\u00b7chen", "spot\u00b7tet", "z\u00fc\u00b7gel\u00b7los"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "des reinen J\u00fcnglings, der dich retten m\u00f6chte \u2013", "tokens": ["des", "rei\u00b7nen", "J\u00fcng\u00b7lings", ",", "der", "dich", "ret\u00b7ten", "m\u00f6ch\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "dem deine Br\u00fcste hocherbaulich sind.", "tokens": ["dem", "dei\u00b7ne", "Br\u00fcs\u00b7te", "ho\u00b7cher\u00b7bau\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}