{"textgrid.poem.42163": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der Pr\u00fcgelheini", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pr\u00fcgelheini, der ist mein Mann,", "tokens": ["Der", "Pr\u00fc\u00b7gel\u00b7hei\u00b7ni", ",", "der", "ist", "mein", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der ist eine Menschenplage;", "tokens": ["Der", "ist", "ei\u00b7ne", "Men\u00b7schen\u00b7pla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der pr\u00fcgelt, was er mich pr\u00fcgeln kann,", "tokens": ["Der", "pr\u00fc\u00b7gelt", ",", "was", "er", "mich", "pr\u00fc\u00b7geln", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die N\u00e4chte sowie die Tage.", "tokens": ["Die", "N\u00e4ch\u00b7te", "so\u00b7wie", "die", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Heut mittag st\u00fcrzt er noch auf mich los:", "tokens": ["Heut", "mit\u00b7tag", "st\u00fcrzt", "er", "noch", "auf", "mich", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdu bist mir untreu gewesen!", "tokens": ["\u00bb", "du", "bist", "mir", "un\u00b7treu", "ge\u00b7we\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das steht in Buchstaben riesengro\u00df", "tokens": ["Das", "steht", "in", "Buch\u00b7sta\u00b7ben", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VVFIN"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Auf deiner Stirne zu lesen!\u00ab \u2013", "tokens": ["Auf", "dei\u00b7ner", "Stir\u00b7ne", "zu", "le\u00b7sen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbbei Gott, mein Heini, dir blieb ich treu!", "tokens": ["\u00bb", "bei", "Gott", ",", "mein", "Hei\u00b7ni", ",", "dir", "blieb", "ich", "treu", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sonst steht mir nichts auf der Stirne.\u00ab \u2013", "tokens": ["Sonst", "steht", "mir", "nichts", "auf", "der", "Stir\u00b7ne", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da schwang er seinen Pr\u00fcgel aufs neu:", "tokens": ["Da", "schwang", "er", "sei\u00b7nen", "Pr\u00fc\u00b7gel", "aufs", "neu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u00bbdich schlag ich nieder, du Dirne!\u00ab \u2013", "tokens": ["\u00bb", "dich", "schlag", "ich", "nie\u00b7der", ",", "du", "Dir\u00b7ne", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "NN", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und als ich ihm zitternd zu F\u00fc\u00dfen sank,", "tokens": ["Und", "als", "ich", "ihm", "zit\u00b7ternd", "zu", "F\u00fc\u00b7\u00dfen", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich \u00e4rmste von allen Frauen,", "tokens": ["Ich", "\u00e4rms\u00b7te", "von", "al\u00b7len", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da warf er mich hin auf die Gartenbank", "tokens": ["Da", "warf", "er", "mich", "hin", "auf", "die", "Gar\u00b7ten\u00b7bank"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hat mich zusammengehauen.", "tokens": ["Und", "hat", "mich", "zu\u00b7sam\u00b7men\u00b7ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}