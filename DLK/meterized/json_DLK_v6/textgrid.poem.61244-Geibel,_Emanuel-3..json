{"textgrid.poem.61244": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du mein Horn erklingen,", "tokens": ["H\u00f6rst", "du", "mein", "Horn", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wundersch\u00f6ne Maid?", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Maid", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fleht zu dir: O flieh mit mir!", "tokens": ["Es", "fleht", "zu", "dir", ":", "O", "flieh", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "NE", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Rappe steht bereit.", "tokens": ["Mein", "Rap\u00b7pe", "steht", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gott gr\u00fc\u00df' in meinen Armen,", "tokens": ["Gott", "gr\u00fc\u00df'", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Grafenkind, Gott gr\u00fc\u00df'!", "tokens": ["Du", "Gra\u00b7fen\u00b7kind", ",", "Gott", "gr\u00fc\u00df'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist so sch\u00f6n, ich bin so jung,", "tokens": ["Du", "bist", "so", "sch\u00f6n", ",", "ich", "bin", "so", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und K\u00fcssen und Kosen so s\u00fc\u00df.", "tokens": ["Und", "K\u00fcs\u00b7sen", "und", "Ko\u00b7sen", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Nacht ist still und dunkel,", "tokens": ["Die", "Nacht", "ist", "still", "und", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein R\u00f6\u00dflein treibt der Sporn,", "tokens": ["Mein", "R\u00f6\u00df\u00b7lein", "treibt", "der", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns treibt die Lieb', uns treibt zur Hast", "tokens": ["Uns", "treibt", "die", "Lieb'", ",", "uns", "treibt", "zur", "Hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deines Vaters scharfer Zorn.", "tokens": ["Dei\u00b7nes", "Va\u00b7ters", "schar\u00b7fer", "Zorn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, schlie\u00dft kein Riegel so feste,", "tokens": ["Ach", ",", "schlie\u00dft", "kein", "Rie\u00b7gel", "so", "fes\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PIAT", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Liebe sprengt ihn bald;", "tokens": ["Die", "Lie\u00b7be", "sprengt", "ihn", "bald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun reit' ich seliger J\u00e4gersmann", "tokens": ["Nun", "reit'", "ich", "se\u00b7li\u00b7ger", "J\u00e4\u00b7gers\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit der k\u00f6stlichen Beute zu Wald.", "tokens": ["Mit", "der", "k\u00f6st\u00b7li\u00b7chen", "Beu\u00b7te", "zu", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}