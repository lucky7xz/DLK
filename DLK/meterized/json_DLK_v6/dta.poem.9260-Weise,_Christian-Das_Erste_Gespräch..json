{"dta.poem.9260": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Erste Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein bruder/ ist dieses die r\u00fchmliche tugend/", "tokens": ["Mein", "bru\u00b7der", "/", "ist", "die\u00b7ses", "die", "r\u00fchm\u00b7li\u00b7che", "tu\u00b7gend", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PDAT", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ist di\u00df die angenehme braut/", "tokens": ["Ist", "di\u00df", "die", "an\u00b7ge\u00b7neh\u00b7me", "braut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist dieses die liebe der ehrlichen jugend/", "tokens": ["Ist", "die\u00b7ses", "die", "lie\u00b7be", "der", "ehr\u00b7li\u00b7chen", "ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ART", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die sich und dich verliebet schaut?", "tokens": ["Die", "sich", "und", "dich", "ver\u00b7lie\u00b7bet", "schaut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So fassen die stricke", "tokens": ["So", "fas\u00b7sen", "die", "stri\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Der lieblichen blicke/", "tokens": ["Der", "lieb\u00b7li\u00b7chen", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durch freundliches schertzen/", "tokens": ["Durch", "freund\u00b7li\u00b7ches", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die munteren hertzen/", "tokens": ["Die", "mun\u00b7te\u00b7ren", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit solcher krafft und wirckung an/", "tokens": ["Mit", "sol\u00b7cher", "krafft", "und", "wir\u00b7ckung", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die schmertzen und wieder befriedigen kan.", "tokens": ["Die", "schmert\u00b7zen", "und", "wie\u00b7der", "be\u00b7frie\u00b7di\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "2. Ich f\u00fchre noch ferner mein einsames leben", "tokens": ["Ich", "f\u00fch\u00b7re", "noch", "fer\u00b7ner", "mein", "ein\u00b7sa\u00b7mes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wei\u00df von keiner liebes-glut/", "tokens": ["Und", "wei\u00df", "von", "kei\u00b7ner", "lie\u00b7bes\u00b7glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir aber wird itzo zu kosten gegeben/", "tokens": ["Dir", "a\u00b7ber", "wird", "it\u00b7zo", "zu", "kos\u00b7ten", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie sanfft das keusche feuer thut.", "tokens": ["Wie", "sanfft", "das", "keu\u00b7sche", "feu\u00b7er", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein s\u00fcsses verlangen", "tokens": ["Dein", "s\u00fcs\u00b7ses", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das kanstu umbfangen", "tokens": ["Das", "kans\u00b7tu", "umb\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und freundlich umbschrencken:", "tokens": ["Und", "freund\u00b7lich", "umbschren\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ich aber mu\u00df dencken/", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wer wei\u00df wo die verborgen liegt/", "tokens": ["Wer", "wei\u00df", "wo", "die", "ver\u00b7bor\u00b7gen", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PWAV", "ART", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die k\u00fcnfftig auch meine gedancken vergn\u00fcgt.", "tokens": ["Die", "k\u00fcnff\u00b7tig", "auch", "mei\u00b7ne", "ge\u00b7dan\u00b7cken", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "3. Ich dachte ja neulich ein R\u00f6\u00dfgen zu brechen/", "tokens": ["Ich", "dach\u00b7te", "ja", "neu\u00b7lich", "ein", "R\u00f6\u00df\u00b7gen", "zu", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein guter freund der kam mir vor:", "tokens": ["Ein", "gu\u00b7ter", "freund", "der", "kam", "mir", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jedennoch begehrt ich kein w\u00f6rtgen zu sprechen/", "tokens": ["Je\u00b7den\u00b7noch", "be\u00b7gehrt", "ich", "kein", "w\u00f6rt\u00b7gen", "zu", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Als ich die hoffnung gantz verlohr.", "tokens": ["Als", "ich", "die", "hoff\u00b7nung", "gantz", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein hertze gedachte/", "tokens": ["Mein", "hert\u00b7ze", "ge\u00b7dach\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der himmel der machte", "tokens": ["Der", "him\u00b7mel", "der", "mach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den rathschlu\u00df dort oben/", "tokens": ["Den", "rath\u00b7schlu\u00df", "dort", "o\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den m\u00fcsse man loben:", "tokens": ["Den", "m\u00fcs\u00b7se", "man", "lo\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "VVINF", "$."], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.9": {"text": "Darzu es ist ein guter freund/", "tokens": ["Dar\u00b7zu", "es", "ist", "ein", "gu\u00b7ter", "freund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den diese gew\u00fcnschete Sonne bescheint.", "tokens": ["Den", "die\u00b7se", "ge\u00b7w\u00fcn\u00b7sche\u00b7te", "Son\u00b7ne", "be\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "4. Ich g\u00f6nne den freunden das gl\u00fccke viel lieber", "tokens": ["Ich", "g\u00f6n\u00b7ne", "den", "freun\u00b7den", "das", "gl\u00fc\u00b7cke", "viel", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PDS", "VVFIN", "ADV", "ADV"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zum minsten doch so lieb als mir:", "tokens": ["Zum", "mins\u00b7ten", "doch", "so", "lieb", "als", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum freuet sich eben mein hertze dar\u00fcber", "tokens": ["Drum", "freu\u00b7et", "sich", "e\u00b7ben", "mein", "hert\u00b7ze", "da\u00b7r\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "PAV"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und w\u00fcnschet gl\u00fccke zu der zier/", "tokens": ["Und", "w\u00fcn\u00b7schet", "gl\u00fc\u00b7cke", "zu", "der", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die deine gedancken", "tokens": ["Die", "dei\u00b7ne", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ohn eintziges wancken", "tokens": ["Ohn", "eint\u00b7zi\u00b7ges", "wan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Zur liebe bewogen", "tokens": ["Zur", "lie\u00b7be", "be\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und an sich gezogen/", "tokens": ["Und", "an", "sich", "ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVPP", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Dein gl\u00fccke sey so gut es sey/", "tokens": ["Dein", "gl\u00fc\u00b7cke", "sey", "so", "gut", "es", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich bleibe dir ferner wie sonsten getreu.", "tokens": ["Ich", "blei\u00b7be", "dir", "fer\u00b7ner", "wie", "sons\u00b7ten", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "5. Mein bruder/ was hastu nun weiter zu sorgen/", "tokens": ["Mein", "bru\u00b7der", "/", "was", "has\u00b7tu", "nun", "wei\u00b7ter", "zu", "sor\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PWS", "VAFIN", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der anfang ist nunmehr gemacht/", "tokens": ["Der", "an\u00b7fang", "ist", "nun\u00b7mehr", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indessen erwarte gedultig den morgen/", "tokens": ["In\u00b7des\u00b7sen", "er\u00b7war\u00b7te", "ge\u00b7dul\u00b7tig", "den", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da deine lust vollkommen lacht.", "tokens": ["Da", "dei\u00b7ne", "lust", "voll\u00b7kom\u00b7men", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jtzt bleibet beysammen", "tokens": ["Jtzt", "blei\u00b7bet", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und nehret die flammen", "tokens": ["Und", "neh\u00b7ret", "die", "flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Der g\u00fcnstigen hertzen/", "tokens": ["Der", "g\u00fcns\u00b7ti\u00b7gen", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Durch k\u00fcssen und schertzen.", "tokens": ["Durch", "k\u00fcs\u00b7sen", "und", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Hernachmahls thut was euch beliebt/", "tokens": ["Her\u00b7nach\u00b7mahls", "thut", "was", "euch", "be\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn euere stunde gelegenheit giebt.", "tokens": ["Wenn", "eu\u00b7e\u00b7re", "stun\u00b7de", "ge\u00b7le\u00b7gen\u00b7heit", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}