{"dta.poem.5472": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Grosse Buchstaben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frage doch die Thiere nur, ob sie dich nicht lehren", "tokens": ["Fra\u00b7ge", "doch", "die", "Thie\u00b7re", "nur", ",", "ob", "sie", "dich", "nicht", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "werden,", "tokens": ["wer\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df ein GOtt, ein Sch\u00f6pfer sey? Oder rede mit der Erden,", "tokens": ["Da\u00df", "ein", "Gott", ",", "ein", "Sch\u00f6p\u00b7fer", "sey", "?", "O\u00b7der", "re\u00b7de", "mit", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$.", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wenn sie gleich mit keinen Lippen und mit keiner Zunge", "tokens": ["Wenn", "sie", "gleich", "mit", "kei\u00b7nen", "Lip\u00b7pen", "und", "mit", "kei\u00b7ner", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "spricht;", "tokens": ["spricht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Giebt sie dir von dieser Warheit dennoch deutlichen Bericht.", "tokens": ["Giebt", "sie", "dir", "von", "die\u00b7ser", "War\u00b7heit", "den\u00b7noch", "deut\u00b7li\u00b7chen", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PDAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Das Gefl\u00fcgel in der Luft darfst du ebenfals nur fragen;", "tokens": ["Das", "Ge\u00b7fl\u00fc\u00b7gel", "in", "der", "Luft", "darfst", "du", "e\u00b7ben\u00b7fals", "nur", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Selbst mit ihrem stummen Munde werden dir\u2019s die Fische", "tokens": ["Selbst", "mit", "ih\u00b7rem", "stum\u00b7men", "Mun\u00b7de", "wer\u00b7den", "dir's", "die", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PIS", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "sagen;", "tokens": ["sa\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Frage Blumen, B\u00e4um\u2019 und Kr\u00e4uter! Es erzehlen Thal", "tokens": ["Fra\u00b7ge", "Blu\u00b7men", ",", "B\u00e4um'", "und", "Kr\u00e4u\u00b7ter", "!", "Es", "er\u00b7zeh\u00b7len", "Thal"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "und H\u00f6hen", "tokens": ["und", "H\u00f6\u00b7hen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Von des grossen Sch\u00f6pfers Liebe, Weisheit, Herrlichkeit", "tokens": ["Von", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Lie\u00b7be", ",", "Weis\u00b7heit", ",", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "und Macht.", "tokens": ["und", "Macht", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Deine Seele wird die Sprache (siehet sie nur mit Bedacht", "tokens": ["Dei\u00b7ne", "See\u00b7le", "wird", "die", "Spra\u00b7che", "(", "sie\u00b7het", "sie", "nur", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Die geschaffnen Wunder an) durch dein Auge, bald verstehen.", "tokens": ["Die", "ge\u00b7schaff\u00b7nen", "Wun\u00b7der", "an", ")", "durch", "dein", "Au\u00b7ge", ",", "bald", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$(", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Menschen Rede pr\u00fcft das Ohr; diese Sprache kanst du", "tokens": ["Men\u00b7schen", "Re\u00b7de", "pr\u00fcft", "das", "Ohr", ";", "die\u00b7se", "Spra\u00b7che", "kanst", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$.", "PDAT", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "sehen,", "tokens": ["se\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Und durch dein Gesicht vernehmen und begreiffen. Schaue", "tokens": ["Und", "durch", "dein", "Ge\u00b7sicht", "ver\u00b7neh\u00b7men", "und", "be\u00b7greif\u00b7fen", ".", "Schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$.", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "dann", "tokens": ["dann"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "In des Welt-Buchs sch\u00f6nen Lettern unsers Sch\u00f6pfers", "tokens": ["In", "des", "Welt\u00b7Buchs", "sch\u00f6\u00b7nen", "Let\u00b7tern", "un\u00b7sers", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Schriften an!", "tokens": ["Schrif\u00b7ten", "an", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}