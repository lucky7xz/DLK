{"textgrid.poem.40044": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachdem ich j\u00fcngst, in dicht-verwachsnen W\u00e4ldern,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich j\u00fcngst, in dicht-verwachsnen W\u00e4ldern,", "tokens": ["Nach\u00b7dem", "ich", "j\u00fcngst", ",", "in", "dicht\u00b7ver\u00b7wachs\u00b7nen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In bunten G\u00e4rten, Wiesen, Feldern", "tokens": ["In", "bun\u00b7ten", "G\u00e4r\u00b7ten", ",", "Wie\u00b7sen", ",", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erden mannigfalt'ge Pracht,", "tokens": ["Der", "Er\u00b7den", "man\u00b7nig\u00b7falt'\u00b7ge", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihres Sch\u00f6pfers Ruhm, bewundernd \u00fcberdacht',", "tokens": ["Zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ruhm", ",", "be\u00b7wun\u00b7dernd", "\u00fc\u00b7ber\u00b7dacht'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und in der That erfuhr, da\u00df durch die grosse Zahl", "tokens": ["Und", "in", "der", "That", "er\u00b7fuhr", ",", "da\u00df", "durch", "die", "gros\u00b7se", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sinnen (Aug' und Ohr zumahl)", "tokens": ["Die", "Sin\u00b7nen", "(", "Aug'", "und", "Ohr", "zu\u00b7mahl", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "KOUS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich in sich selbst verwirrten,", "tokens": ["Sich", "in", "sich", "selbst", "ver\u00b7wirr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und, sonder Ordnung, sich verirrten,", "tokens": ["Und", ",", "son\u00b7der", "Ord\u00b7nung", ",", "sich", "ver\u00b7irr\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "NN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Daher nichts f\u00f6rmliches zusammen brachten,", "tokens": ["Da\u00b7her", "nichts", "f\u00f6rm\u00b7li\u00b7ches", "zu\u00b7sam\u00b7men", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und, um zu viel zu dencken, nichts gedachten;", "tokens": ["Und", ",", "um", "zu", "viel", "zu", "den\u00b7cken", ",", "nichts", "ge\u00b7dach\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "APPR", "PIS", "PTKZU", "VVINF", "$,", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So theilt' ich, samt der Zeit, der Sinnen Vorw\u00fcrf' ein.", "tokens": ["So", "theilt'", "ich", ",", "samt", "der", "Zeit", ",", "der", "Sin\u00b7nen", "Vor\u00b7w\u00fcr\u00b7f'", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Ich that gar nichts, die erste Viertel-Stunde,", "tokens": ["Ich", "that", "gar", "nichts", ",", "die", "ers\u00b7te", "Vier\u00b7tel\u00b7Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sehn der Creatur Pracht, Sch\u00f6nheit, Farb' und Schein.", "tokens": ["Als", "sehn", "der", "Crea\u00b7tur", "Pracht", ",", "Sch\u00f6n\u00b7heit", ",", "Fa\u00b7rb'", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die and're, h\u00f6rt' ich blo\u00df allein,", "tokens": ["Die", "an\u00b7d'\u00b7re", ",", "h\u00f6rt'", "ich", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wie mannigfach, wie s\u00fc\u00df der V\u00f6gel Stimmen seyn,", "tokens": ["Wie", "man\u00b7nig\u00b7fach", ",", "wie", "s\u00fc\u00df", "der", "V\u00f6\u00b7gel", "Stim\u00b7men", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie lieblich eines Bachs Ger\u00e4usch und Fall,", "tokens": ["Wie", "lieb\u00b7lich", "ei\u00b7nes", "Bachs", "Ge\u00b7r\u00e4usch", "und", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie angenehm der Wiederhall.", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "der", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die dritte, lobt' mein Hertz, mit vollem Munde,", "tokens": ["Die", "drit\u00b7te", ",", "lobt'", "mein", "Hertz", ",", "mit", "vol\u00b7lem", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den wunderbaren Gott, der alle Welt", "tokens": ["Den", "wun\u00b7der\u00b7ba\u00b7ren", "Gott", ",", "der", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So sch\u00f6n gemacht, so fest erh\u00e4lt.", "tokens": ["So", "sch\u00f6n", "ge\u00b7macht", ",", "so", "fest", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und in der vierten schrieb ichs auf:", "tokens": ["Und", "in", "der", "vier\u00b7ten", "schrieb", "ichs", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "O Gott! ach w\u00e4r' doch so mein gantzer Lebens-Lauf!", "tokens": ["O", "Gott", "!", "ach", "w\u00e4r'", "doch", "so", "mein", "gant\u00b7zer", "Le\u00b7bens\u00b7Lauf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "XY", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}