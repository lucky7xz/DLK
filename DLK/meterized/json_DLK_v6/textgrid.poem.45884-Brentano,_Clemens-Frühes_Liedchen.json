{"textgrid.poem.45884": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fches Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb' und Leid im leichten Leben", "tokens": ["Lieb'", "und", "Leid", "im", "leich\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich erheben, abw\u00e4rts schweben", "tokens": ["Sich", "er\u00b7he\u00b7ben", ",", "ab\u00b7w\u00e4rts", "schwe\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "VVINF", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Spiegel schauen Bilder,", "tokens": ["Aus", "dem", "Spie\u00b7gel", "schau\u00b7en", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blicken milder, blicken wilder.", "tokens": ["Bli\u00b7cken", "mil\u00b7der", ",", "bli\u00b7cken", "wil\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem Strome Well' auf Welle", "tokens": ["In", "dem", "Stro\u00b7me", "Well'", "auf", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich geselle, tr\u00fcb und helle,", "tokens": ["Sich", "ge\u00b7sel\u00b7le", ",", "tr\u00fcb", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schauet nieder arme Triebe", "tokens": ["Schau\u00b7et", "nie\u00b7der", "ar\u00b7me", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell und tr\u00fcbe ist die Liebe.", "tokens": ["Hell", "und", "tr\u00fc\u00b7be", "ist", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fr\u00fchling mu\u00df mit s\u00fc\u00dfen Blicken", "tokens": ["Fr\u00fch\u00b7ling", "mu\u00df", "mit", "s\u00fc\u00b7\u00dfen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich entr\u00fccken den ber\u00fccken", "tokens": ["Mich", "ent\u00b7r\u00fc\u00b7cken", "den", "be\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sommer mu\u00df mit Frucht und Mirten", "tokens": ["Som\u00b7mer", "mu\u00df", "mit", "Frucht", "und", "Mir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich bewirten und umg\u00fcrten.", "tokens": ["Mich", "be\u00b7wir\u00b7ten", "und", "um\u00b7g\u00fcr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herbst du sollst mich Haushalt lehren", "tokens": ["Herbst", "du", "sollst", "mich", "Haus\u00b7halt", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu begehren zu entbehren", "tokens": ["Zu", "be\u00b7geh\u00b7ren", "zu", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Winter lehre mich erwerben,", "tokens": ["Win\u00b7ter", "leh\u00b7re", "mich", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gerne sterben, Fr\u00fchling erben.", "tokens": ["Ger\u00b7ne", "ster\u00b7ben", ",", "Fr\u00fch\u00b7ling", "er\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wasser fallen um zu springen,", "tokens": ["Was\u00b7ser", "fal\u00b7len", "um", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um zu klingen um zu singen", "tokens": ["Um", "zu", "klin\u00b7gen", "um", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF", "KOUI", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweig' ich stille, denn zu sagen", "tokens": ["Schweig'", "ich", "stil\u00b7le", ",", "denn", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "KON", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4re wagen und entsagen.", "tokens": ["W\u00e4\u00b7re", "wa\u00b7gen", "und", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}