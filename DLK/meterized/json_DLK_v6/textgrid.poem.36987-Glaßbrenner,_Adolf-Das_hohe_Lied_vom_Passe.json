{"textgrid.poem.36987": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Das hohe Lied vom Passe", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinaus in's Freie wollte nun ich wandeln,", "tokens": ["Hin\u00b7aus", "in's", "Frei\u00b7e", "woll\u00b7te", "nun", "ich", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Das geht nicht so! Da gibt's noch viel zu handeln!", "tokens": ["Das", "geht", "nicht", "so", "!", "Da", "gibt's", "noch", "viel", "zu", "han\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "$.", "ADV", "VVFIN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hast ja keinen Pa\u00df!", "tokens": ["Du", "hast", "ja", "kei\u00b7nen", "Pa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hast ja keinen Pa\u00df!", "tokens": ["Du", "hast", "ja", "kei\u00b7nen", "Pa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jedwedes Wesen, das hier promeniret", "tokens": ["Jed\u00b7we\u00b7des", "We\u00b7sen", ",", "das", "hier", "pro\u00b7me\u00b7ni\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Im Feld und in der Stadt,", "tokens": ["Im", "Feld", "und", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird sofort gesetzlich arretiret,", "tokens": ["Es", "wird", "so\u00b7fort", "ge\u00b7setz\u00b7lich", "ar\u00b7re\u00b7ti\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn keinen Pa\u00df es hat!", "tokens": ["Wenn", "kei\u00b7nen", "Pa\u00df", "es", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn keinen Pa\u00df es hat!", "tokens": ["Wenn", "kei\u00b7nen", "Pa\u00df", "es", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der B\u00fcrgersmann, so still des Weges ziehet", "tokens": ["Der", "B\u00fcr\u00b7gers\u00b7mann", ",", "so", "still", "des", "We\u00b7ges", "zie\u00b7het"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Weib und Kind am Arm,", "tokens": ["Mit", "Weib", "und", "Kind", "am", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Braucht einen Pa\u00df, denn eh' er's sich versiehet,", "tokens": ["Braucht", "ei\u00b7nen", "Pa\u00df", ",", "denn", "eh'", "er's", "sich", "ver\u00b7sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "KOUS", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umarmt ihn ein Gensd'arm!", "tokens": ["Um\u00b7armt", "ihn", "ein", "Gens\u00b7d'\u00b7arm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Umarmt ihn ein Gensd'arm!", "tokens": ["Um\u00b7armt", "ihn", "ein", "Gens\u00b7d'\u00b7arm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Umarmet ihn und k\u00fc\u00dft ihn auf die Backe;", "tokens": ["Um\u00b7ar\u00b7met", "ihn", "und", "k\u00fc\u00dft", "ihn", "auf", "die", "Ba\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bringt aber gleich nachher", "tokens": ["Bringt", "a\u00b7ber", "gleich", "nach\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn in Prison, und haut ihm durch die Jacke,", "tokens": ["Ihn", "in", "Pri\u00b7son", ",", "und", "haut", "ihm", "durch", "die", "Ja\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entbehrt des Passes er!", "tokens": ["Ent\u00b7behrt", "des", "Pas\u00b7ses", "er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entbehrt des Passes er!", "tokens": ["Ent\u00b7behrt", "des", "Pas\u00b7ses", "er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Dienstmagd, welche geht nach Brod und K\u00e4se,", "tokens": ["Die", "Dienst\u00b7magd", ",", "wel\u00b7che", "geht", "nach", "Brod", "und", "K\u00e4\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wurst oder sonst so was,", "tokens": ["Wurst", "o\u00b7der", "sonst", "so", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00e4m sofort in strenge Haft, verg\u00e4\u00dfe", "tokens": ["Sie", "k\u00e4m", "so\u00b7fort", "in", "stren\u00b7ge", "Haft", ",", "ver\u00b7g\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie einmal ihren Pa\u00df!", "tokens": ["Sie", "ein\u00b7mal", "ih\u00b7ren", "Pa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie einmal ihren Pa\u00df!", "tokens": ["Sie", "ein\u00b7mal", "ih\u00b7ren", "Pa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn der Schneider auf dem wilden Stiere", "tokens": ["Und", "wenn", "der", "Schnei\u00b7der", "auf", "dem", "wil\u00b7den", "Stie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zuf\u00e4llig unbepa\u00dft", "tokens": ["Zu\u00b7f\u00e4l\u00b7lig", "un\u00b7be\u00b7pa\u00dft"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Kundschaft reitet, wird mitsammt dem Thiere", "tokens": ["Zur", "Kund\u00b7schaft", "rei\u00b7tet", ",", "wird", "mit\u00b7sammt", "dem", "Thie\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das fromme Weib, das seine Blumenspenden", "tokens": ["Das", "from\u00b7me", "Weib", ",", "das", "sei\u00b7ne", "Blu\u00b7men\u00b7spen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum G\u00f6ttertempel tr\u00e4gt,", "tokens": ["Zum", "G\u00f6t\u00b7ter\u00b7tem\u00b7pel", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht, wenn sie pa\u00dflos, von Gensd'armenh\u00e4nden", "tokens": ["Sieht", ",", "wenn", "sie", "pa\u00df\u00b7los", ",", "von", "Gens\u00b7d'\u00b7ar\u00b7men\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sich mit Beschlag belegt!", "tokens": ["Sich", "mit", "Be\u00b7schlag", "be\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich mit Beschlag belegt!", "tokens": ["Sich", "mit", "Be\u00b7schlag", "be\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Philosoph, der gr\u00f6\u00dfte Mann der Lettern,", "tokens": ["Der", "Phi\u00b7lo\u00b7soph", ",", "der", "gr\u00f6\u00df\u00b7te", "Mann", "der", "Let\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kommt, w\u00e4r' er noch so dumm,", "tokens": ["Kommt", ",", "w\u00e4r'", "er", "noch", "so", "dumm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4re er gespickt mit Lorbeerbl\u00e4ttern,", "tokens": ["Und", "w\u00e4\u00b7re", "er", "ge\u00b7spickt", "mit", "Lor\u00b7beer\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht um den Pa\u00df herum!", "tokens": ["Nicht", "um", "den", "Pa\u00df", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht um den Pa\u00df herum!", "tokens": ["Nicht", "um", "den", "Pa\u00df", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Arzt mu\u00df nach dem hohen und hochdummen", "tokens": ["Der", "Arzt", "mu\u00df", "nach", "dem", "ho\u00b7hen", "und", "hoch\u00b7dum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "ADJA", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ministerial-Erla\u00df", "tokens": ["Mi\u00b7nis\u00b7te\u00b7ri\u00b7a\u00b7lEr\u00b7la\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vom 3. Mai 217 brummen,", "tokens": ["Vom", "3.", "Mai", "217", "brum\u00b7men", ","], "token_info": ["word", "ordinal", "word", "number", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "CARD", "NN", "$,"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.4": {"text": "Besitzt er keinen Pa\u00df!", "tokens": ["Be\u00b7sitzt", "er", "kei\u00b7nen", "Pa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Besitzt er keinen Pa\u00df!", "tokens": ["Be\u00b7sitzt", "er", "kei\u00b7nen", "Pa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Den Volksvertreter selbst thut in den Bann man,", "tokens": ["Den", "Volks\u00b7ver\u00b7tre\u00b7ter", "selbst", "thut", "in", "den", "Bann", "man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "PIS", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihn, der zur Kammerfrist", "tokens": ["Ihn", ",", "der", "zur", "Kam\u00b7mer\u00b7frist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch unantastbar, ihn selbst tastet man an,", "tokens": ["Doch", "un\u00b7an\u00b7tast\u00b7bar", ",", "ihn", "selbst", "tas\u00b7tet", "man", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wenn er passiv nicht ist!", "tokens": ["Wenn", "er", "pas\u00b7siv", "nicht", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn er passiv nicht ist!", "tokens": ["Wenn", "er", "pas\u00b7siv", "nicht", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und wer verreis't, zwei Meilen oder weiter,", "tokens": ["Und", "wer", "ver\u00b7reis't", ",", "zwei", "Mei\u00b7len", "o\u00b7der", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "CARD", "NN", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist zwar des Passes quitt,", "tokens": ["Ist", "zwar", "des", "Pas\u00b7ses", "quitt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kriegt aber zwei Gensd'armen als Begleiter", "tokens": ["Kriegt", "a\u00b7ber", "zwei", "Gens\u00b7d'\u00b7ar\u00b7men", "als", "Be\u00b7glei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "CARD", "NN", "KOUS", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf seine Kosten mit!", "tokens": ["Auf", "sei\u00b7ne", "Kos\u00b7ten", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf seine Kosten mit!", "tokens": ["Auf", "sei\u00b7ne", "Kos\u00b7ten", "mit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ganz ungest\u00f6rt, noch von des Volkes Liebe", "tokens": ["Ganz", "un\u00b7ge\u00b7st\u00f6rt", ",", "noch", "von", "des", "Vol\u00b7kes", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bejubelt, zieh'n durch's Land", "tokens": ["Be\u00b7ju\u00b7belt", ",", "zieh'n", "durch's", "Land"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einzig nur die Betr\u00fcger und die Diebe", "tokens": ["Ein\u00b7zig", "nur", "die", "Be\u00b7tr\u00fc\u00b7ger", "und", "die", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+--