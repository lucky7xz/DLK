{"dta.poem.23380": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Engel schuf die Pre\u00dffreiheit,", "tokens": ["Ein", "En\u00b7gel", "schuf", "die", "Pre\u00df\u00b7frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ein Teufel die Censur:", "tokens": ["Ein", "Teu\u00b7fel", "die", "Cen\u00b7sur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und leider ist drum auch zur Zeit", "tokens": ["Und", "lei\u00b7der", "ist", "drum", "auch", "zur", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PAV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von jener wenig Spur.", "tokens": ["Von", "je\u00b7ner", "we\u00b7nig", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn wer ein b\u00f6s Gewissen hat,", "tokens": ["Denn", "wer", "ein", "b\u00f6s", "Ge\u00b7wis\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem steht der Teufel bei;", "tokens": ["Dem", "steht", "der", "Teu\u00b7fel", "bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Teufel will in Kirch' und Staat,", "tokens": ["Der", "Teu\u00b7fel", "will", "in", "Kirch'", "und", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man des Teufels sei.", "tokens": ["Da\u00df", "man", "des", "Teu\u00b7fels", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}