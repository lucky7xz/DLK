{"textgrid.poem.49942": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An J.G. Fischer", "genre": "verse", "period": "N.A.", "pub_year": 1864, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcnftig, sooft man dem \u00bbMeister\u00ab den wohlerworbenen Lorbeer", "tokens": ["K\u00fcnf\u00b7tig", ",", "sooft", "man", "dem", "\u00bb", "Meis\u00b7ter", "\u00ab", "den", "woh\u00b7ler\u00b7wor\u00b7be\u00b7nen", "Lor\u00b7beer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "PIS", "ART", "$(", "NN", "$(", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Neu um die Schl\u00e4fe, den zwief\u00e4ltig gewundenen legt,", "tokens": ["Neu", "um", "die", "Schl\u00e4\u00b7fe", ",", "den", "zwie\u00b7f\u00e4l\u00b7tig", "ge\u00b7wun\u00b7de\u00b7nen", "legt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Oder im Lenz auch, wenn er die fr\u00fchesten Rosen zum Opfer", "tokens": ["O\u00b7der", "im", "Lenz", "auch", ",", "wenn", "er", "die", "fr\u00fc\u00b7hes\u00b7ten", "Ro\u00b7sen", "zum", "Op\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Seinen Chariten weiht, denk er des Freundes dabei.", "tokens": ["Sei\u00b7nen", "Cha\u00b7ri\u00b7ten", "weiht", ",", "denk", "er", "des", "Freun\u00b7des", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}}}}}