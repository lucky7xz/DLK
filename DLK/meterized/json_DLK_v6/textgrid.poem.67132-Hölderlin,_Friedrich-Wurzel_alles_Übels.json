{"textgrid.poem.67132": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Wurzel alles \u00dcbels", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einig zu sein, ist g\u00f6ttlich und gut; woher ist die Sucht denn", "tokens": ["Ei\u00b7nig", "zu", "sein", ",", "ist", "g\u00f6tt\u00b7lich", "und", "gut", ";", "wo\u00b7her", "ist", "die", "Sucht", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$.", "PWAV", "VAFIN", "ART", "NN", "KON"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Unter den Menschen, da\u00df nur Einer und Eines nur sei?", "tokens": ["Un\u00b7ter", "den", "Men\u00b7schen", ",", "da\u00df", "nur", "Ei\u00b7ner", "und", "Ei\u00b7nes", "nur", "sei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ADV", "PIS", "KON", "PIS", "ADV", "VAFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}