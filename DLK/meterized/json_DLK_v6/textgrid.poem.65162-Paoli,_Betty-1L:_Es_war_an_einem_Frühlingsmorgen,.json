{"textgrid.poem.65162": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war an einem Fr\u00fchlingsmorgen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war an einem Fr\u00fchlingsmorgen,", "tokens": ["Es", "war", "an", "ei\u00b7nem", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rosen bl\u00fchten, der Jasmin,", "tokens": ["Die", "Ro\u00b7sen", "bl\u00fch\u00b7ten", ",", "der", "Jas\u00b7min", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem Gestr\u00e4uche halb verborgen", "tokens": ["Von", "dem", "Ge\u00b7str\u00e4u\u00b7che", "halb", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lugt' ich verstohlen nach dir hin.", "tokens": ["Lugt'", "ich", "ver\u00b7stoh\u00b7len", "nach", "dir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du knietest an des Weihers Rand,", "tokens": ["Du", "knie\u00b7test", "an", "des", "Wei\u00b7hers", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umspielt vom hellen Sonnenscheine,", "tokens": ["Um\u00b7spielt", "vom", "hel\u00b7len", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und suchtest emsig bunte Steine", "tokens": ["Und", "such\u00b7test", "em\u00b7sig", "bun\u00b7te", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Wege, mit gesch\u00e4ft'ger Hand.", "tokens": ["Am", "We\u00b7ge", ",", "mit", "ge\u00b7sch\u00e4ft'\u00b7ger", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus Kieseln, gelben, wei\u00dfen, blauen,", "tokens": ["Aus", "Kie\u00b7seln", ",", "gel\u00b7ben", ",", "wei\u00b7\u00dfen", ",", "blau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich's gerade f\u00fcgt' und fand,", "tokens": ["Wie", "sich's", "ge\u00b7ra\u00b7de", "f\u00fcgt'", "und", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begannst du dir ein Haus zu bauen", "tokens": ["Be\u00b7gannst", "du", "dir", "ein", "Haus", "zu", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf des Ger\u00f6lles feuchtem Sand.", "tokens": ["Auf", "des", "Ge\u00b7r\u00f6l\u00b7les", "feuch\u00b7tem", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vertieft in deines Werks Beschau", "tokens": ["Ver\u00b7tieft", "in", "dei\u00b7nes", "Werks", "Be\u00b7schau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sah ich dich wohlgef\u00e4llig nicken.", "tokens": ["Sah", "ich", "dich", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "ni\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da, \u2013 nur ein Hauch, ein leises R\u00fccken, \u2013", "tokens": ["Da", ",", "\u2013", "nur", "ein", "Hauch", ",", "ein", "lei\u00b7ses", "R\u00fc\u00b7cken", ",", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "$(", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! und versch\u00fcttet lag der Bau.", "tokens": ["Ach", "!", "und", "ver\u00b7sch\u00fct\u00b7tet", "lag", "der", "Bau", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geduldig, ohne Zorn und Klage,", "tokens": ["Ge\u00b7dul\u00b7dig", ",", "oh\u00b7ne", "Zorn", "und", "Kla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein liebes Herz sich drein ergab.", "tokens": ["Dein", "lie\u00b7bes", "Herz", "sich", "drein", "er\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt tratest du zum Rosenhage", "tokens": ["Jetzt", "tra\u00b7test", "du", "zum", "Ro\u00b7sen\u00b7ha\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und pfl\u00fccktest ein paar Rosen ab.", "tokens": ["Und", "pfl\u00fcck\u00b7test", "ein", "paar", "Ro\u00b7sen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, kaum von ihrem Duft umwallt,", "tokens": ["Doch", ",", "kaum", "von", "ih\u00b7rem", "Duft", "um\u00b7wallt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gewahrtest du im Kelch der einen,", "tokens": ["Ge\u00b7wahr\u00b7test", "du", "im", "Kelch", "der", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit gift'gem Bauch und Zappelbeinen,", "tokens": ["Mit", "gift'\u00b7gem", "Bauch", "und", "Zap\u00b7pel\u00b7bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Spinne schn\u00f6de Mi\u00dfgestalt.", "tokens": ["Der", "Spin\u00b7ne", "schn\u00f6\u00b7de", "Mi\u00df\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich sah, wie vor dem w\u00fcsten Scheuel", "tokens": ["Ich", "sah", ",", "wie", "vor", "dem", "w\u00fcs\u00b7ten", "Scheu\u00b7el"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein banger Ekel dich erfa\u00dft!", "tokens": ["Ein", "ban\u00b7ger", "E\u00b7kel", "dich", "er\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen d\u00fcnkten dich ein Greuel,", "tokens": ["Die", "Ro\u00b7sen", "d\u00fcnk\u00b7ten", "dich", "ein", "Greu\u00b7el", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herberg' boten solchem Gast.", "tokens": ["Die", "Her\u00b7ber\u00b7g'", "bo\u00b7ten", "sol\u00b7chem", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du warfst sie hin in's gr\u00fcne Moos,", "tokens": ["Du", "warfst", "sie", "hin", "in's", "gr\u00fc\u00b7ne", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und setztest dich am Ufer nieder,", "tokens": ["Und", "setz\u00b7test", "dich", "am", "U\u00b7fer", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gesenkt die zarten Augenlider,", "tokens": ["Ge\u00b7senkt", "die", "zar\u00b7ten", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die H\u00e4nde achtlos in dem Scho\u00df.", "tokens": ["Die", "H\u00e4n\u00b7de", "acht\u00b7los", "in", "dem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Umsonst strich dir um Stirn und Wangen", "tokens": ["Um\u00b7sonst", "strich", "dir", "um", "Stirn", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Morgenluft, von D\u00fcften schwer.", "tokens": ["Die", "Mor\u00b7gen\u00b7luft", ",", "von", "D\u00fcf\u00b7ten", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lust am Spiel war dir vergangen,", "tokens": ["Die", "Lust", "am", "Spiel", "war", "dir", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich lockte keine Rose mehr!", "tokens": ["Dich", "lock\u00b7te", "kei\u00b7ne", "Ro\u00b7se", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sa\u00dfest du in l\u00e4ss'ger Ruh'", "tokens": ["So", "sa\u00b7\u00dfest", "du", "in", "l\u00e4ss'\u00b7ger", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schautest nur dem Zug der Wellen,", "tokens": ["Und", "schau\u00b7test", "nur", "dem", "Zug", "der", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Tanz der gaukelnden Libellen,", "tokens": ["Dem", "Tanz", "der", "gau\u00b7keln\u00b7den", "Li\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit tr\u00e4umerischem Blicke zu.", "tokens": ["Mit", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Bli\u00b7cke", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Durch's Herz flog mir ein leises Beben,", "tokens": ["Durch's", "Herz", "flog", "mir", "ein", "lei\u00b7ses", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum wei\u00df ich selbst, wie mir geschah,", "tokens": ["Kaum", "wei\u00df", "ich", "selbst", ",", "wie", "mir", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich dich, ohne Wunsch und Streben,", "tokens": ["Als", "ich", "dich", ",", "oh\u00b7ne", "Wunsch", "und", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So still in dich versunken sah.", "tokens": ["So", "still", "in", "dich", "ver\u00b7sun\u00b7ken", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! und noch heute steigt und schwillt,", "tokens": ["Ach", "!", "und", "noch", "heu\u00b7te", "steigt", "und", "schwillt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In mir ein Strom von dunkeln Sorgen. \u2013", "tokens": ["In", "mir", "ein", "Strom", "von", "dun\u00b7keln", "Sor\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erschien an jenem Fr\u00fchlingsmorgen", "tokens": ["Er\u00b7schien", "an", "je\u00b7nem", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vielleicht mir deiner Zukunft Bild?", "tokens": ["Viel\u00b7leicht", "mir", "dei\u00b7ner", "Zu\u00b7kunft", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}