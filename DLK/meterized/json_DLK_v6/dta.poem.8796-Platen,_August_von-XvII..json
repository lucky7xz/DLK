{"dta.poem.8796": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die lange M\u00fch' vergilt der Schacht endlich;", "tokens": ["Die", "lan\u00b7ge", "M\u00fch'", "ver\u00b7gilt", "der", "Schacht", "end\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolken bargen stets den Mond wieder,", "tokens": ["Die", "Wol\u00b7ken", "bar\u00b7gen", "stets", "den", "Mond", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Doch er gewann die sch\u00f6ne Schlacht endlich;", "tokens": ["Doch", "er", "ge\u00b7wann", "die", "sch\u00f6\u00b7ne", "Schlacht", "end\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es hat die Sonne gr\u00fcne Brautperlen", "tokens": ["Es", "hat", "die", "Son\u00b7ne", "gr\u00fc\u00b7ne", "Braut\u00b7per\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Aus Wittwenthr\u00e4nenthau gemacht endlich;", "tokens": ["Aus", "Witt\u00b7went\u00b7hr\u00e4\u00b7nent\u00b7hau", "ge\u00b7macht", "end\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ADV", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Der Samenfunke glimmt im Erdreiche,", "tokens": ["Der", "Sa\u00b7men\u00b7fun\u00b7ke", "glimmt", "im", "Er\u00b7drei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bis man die Tulpenflamme facht endlich!", "tokens": ["Bis", "man", "die", "Tul\u00b7pen\u00b7flam\u00b7me", "facht", "end\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}