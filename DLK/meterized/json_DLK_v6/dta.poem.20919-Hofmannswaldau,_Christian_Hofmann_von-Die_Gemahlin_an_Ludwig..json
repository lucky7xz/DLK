{"dta.poem.20919": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gemahlin an Ludwig.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Es bringt der kleine Brief dir mehr getreuer", "tokens": ["Es", "bringt", "der", "klei\u00b7ne", "Brief", "dir", "mehr", "ge\u00b7treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gr\u00fcsse/", "tokens": ["Gr\u00fcs\u00b7se", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Als Freude sich itzund in meinem Hertzen regt/", "tokens": ["Als", "Freu\u00b7de", "sich", "it\u00b7zund", "in", "mei\u00b7nem", "Hert\u00b7zen", "regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich schwere/ da\u00df ich dich recht in Gedancken", "tokens": ["Ich", "schwe\u00b7re", "/", "da\u00df", "ich", "dich", "recht", "in", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "k\u00fcsse/", "tokens": ["k\u00fcs\u00b7se", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und meine Seite sich an deine Seite legt.", "tokens": ["Und", "mei\u00b7ne", "Sei\u00b7te", "sich", "an", "dei\u00b7ne", "Sei\u00b7te", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verzeihe/ Liebster Schatz/ doch meinen schlechten", "tokens": ["Ver\u00b7zei\u00b7he", "/", "Liebs\u00b7ter", "Schatz", "/", "doch", "mei\u00b7nen", "schlech\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schreiben/", "tokens": ["Schrei\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Da\u00df Wort und Zeilen nicht in rechter Ordnung stehn;", "tokens": ["Da\u00df", "Wort", "und", "Zei\u00b7len", "nicht", "in", "rech\u00b7ter", "Ord\u00b7nung", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wem Freud und Zuversicht die schwachen Finger", "tokens": ["Wem", "Freud", "und", "Zu\u00b7ver\u00b7sicht", "die", "schwa\u00b7chen", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "treiben/", "tokens": ["trei\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Dem wil die Feder nicht in gleicher Wage gehn.", "tokens": ["Dem", "wil", "die", "Fe\u00b7der", "nicht", "in", "glei\u00b7cher", "Wa\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald lesch\u2019 ich etwas aus/ bald mach\u2019 ich neue Zeilen/", "tokens": ["Bald", "le\u00b7sch'", "ich", "et\u00b7was", "aus", "/", "bald", "mach'", "ich", "neu\u00b7e", "Zei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Bald werd\u2019 ich halb entzuckt/ bald schlaf ich dr\u00fcber ein/", "tokens": ["Bald", "werd'", "ich", "halb", "ent\u00b7zuckt", "/", "bald", "schlaf", "ich", "dr\u00fc\u00b7ber", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$(", "ADV", "VVFIN", "PPER", "PAV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald wird die Feder faul/ bald wil sie fertig eilen/", "tokens": ["Bald", "wird", "die", "Fe\u00b7der", "faul", "/", "bald", "wil", "sie", "fer\u00b7tig", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$(", "ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und heist offt einen Kleck an statt der W\u00f6rter seyn.", "tokens": ["Und", "heist", "offt", "ei\u00b7nen", "Kleck", "an", "statt", "der", "W\u00f6r\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wei\u00df nicht wie mir ist/ und kan mir selbst nicht", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "mir", "ist", "/", "und", "kan", "mir", "selbst", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$(", "KON", "VMFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "trauen/", "tokens": ["trau\u00b7en", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Ob mein Gesichte hier den wahren Zweck erkiest?", "tokens": ["Ob", "mein", "Ge\u00b7sich\u00b7te", "hier", "den", "wah\u00b7ren", "Zweck", "er\u00b7kiest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ob meine Hoffnung auch recht feste wei\u00df zubauen?", "tokens": ["Ob", "mei\u00b7ne", "Hoff\u00b7nung", "auch", "recht", "fes\u00b7te", "wei\u00df", "zu\u00b7bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ob nicht ein schlechter Dunst itzund mein Grundstein", "tokens": ["Ob", "nicht", "ein", "schlech\u00b7ter", "Dunst", "it\u00b7zund", "mein", "Grund\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "ist?", "tokens": ["ist", "?"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Bald rei\u00df\u2019 ich wiederum aus diesen falschen Schran-", "tokens": ["Bald", "rei\u00df'", "ich", "wie\u00b7de\u00b7rum", "aus", "die\u00b7sen", "fal\u00b7schen", "Schran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "cken/", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Und schaue deinen Brieff mit scharffen Augen an/", "tokens": ["Und", "schau\u00b7e", "dei\u00b7nen", "Brieff", "mit", "scharf\u00b7fen", "Au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Umbschl\u00fcsse mit Vernunfft die fl\u00fcchtigen Gedancken/", "tokens": ["U\u00b7mbschl\u00fcs\u00b7se", "mit", "Ver\u00b7nunfft", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Weil solche Klarheit ja mich nicht verblenden kan.", "tokens": ["Weil", "sol\u00b7che", "Klar\u00b7heit", "ja", "mich", "nicht", "ver\u00b7blen\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich schaue klar genug und k\u00fcsse mein Gel\u00fccke/", "tokens": ["Ich", "schau\u00b7e", "klar", "ge\u00b7nug", "und", "k\u00fcs\u00b7se", "mein", "Ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So itzt mit seiner Hand die \u00f6den N\u00e4chte st\u00f6hrt/", "tokens": ["So", "itzt", "mit", "sei\u00b7ner", "Hand", "die", "\u00f6\u00b7den", "N\u00e4ch\u00b7te", "st\u00f6hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich sp\u00fchre wiederum des Himmels warme Blicke/", "tokens": ["Ich", "sp\u00fch\u00b7re", "wie\u00b7de\u00b7rum", "des", "Him\u00b7mels", "war\u00b7me", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der dich mir auf das neu aus seiner Scho\u00df verehrt.", "tokens": ["Der", "dich", "mir", "auf", "das", "neu", "aus", "sei\u00b7ner", "Scho\u00df", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "ART", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was hab ich nicht bi\u00dfher in Einsamkeit erlitten?", "tokens": ["Was", "hab", "ich", "nicht", "bi\u00df\u00b7her", "in", "Ein\u00b7sam\u00b7keit", "er\u00b7lit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.33": {"text": "Was hat mir nicht vor Angst gesesselt Geist und Sinn?", "tokens": ["Was", "hat", "mir", "nicht", "vor", "Angst", "ge\u00b7ses\u00b7selt", "Geist", "und", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was hat mich nicht vor Furcht zu mancher Zeit be-", "tokens": ["Was", "hat", "mich", "nicht", "vor", "Furcht", "zu", "man\u00b7cher", "Zeit", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "stritten?", "tokens": ["strit\u00b7ten", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Da\u00df ich/ wie mich bedeucht/ mir fast nicht ehnlich bin.", "tokens": ["Da\u00df", "ich", "/", "wie", "mich", "be\u00b7deucht", "/", "mir", "fast", "nicht", "ehn\u00b7lich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$(", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie hab ich manchesmahl nach deinem Abereisen/", "tokens": ["Wie", "hab", "ich", "man\u00b7ches\u00b7mahl", "nach", "dei\u00b7nem", "A\u00b7be\u00b7rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn ich erwachet bin/ die Hand nach dir gestreckt?", "tokens": ["Wenn", "ich", "er\u00b7wa\u00b7chet", "bin", "/", "die", "Hand", "nach", "dir", "ge\u00b7streckt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$(", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie offtmahls hat ein Traum dich mir in Band und", "tokens": ["Wie", "offt\u00b7mahls", "hat", "ein", "Traum", "dich", "mir", "in", "Band", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "PPER", "PPER", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Eisen", "tokens": ["Ei\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Erschrecklich f\u00fcrgestellt/ und denn mich aufgeweckt?", "tokens": ["Er\u00b7schreck\u00b7lich", "f\u00fcr\u00b7ge\u00b7stellt", "/", "und", "denn", "mich", "auf\u00b7ge\u00b7weckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "KON", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Bald hab\u2019 ich schlaffende gemeinet dich zuk\u00fcssen/", "tokens": ["Bald", "hab'", "ich", "schlaf\u00b7fen\u00b7de", "ge\u00b7mei\u00b7net", "dich", "zu\u00b7k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "VVPP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und meinen Jrrthum denn aus leerer Lufft vermerckt/", "tokens": ["Und", "mei\u00b7nen", "Jrr\u00b7thum", "denn", "aus", "lee\u00b7rer", "Lufft", "ver\u00b7merckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Man schaut die Menschen ja am allermeisten b\u00fcssen/", "tokens": ["Man", "schaut", "die", "Men\u00b7schen", "ja", "am", "al\u00b7ler\u00b7meis\u00b7ten", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "In dem der Mangel uns die alte Lust versterckt.", "tokens": ["In", "dem", "der", "Man\u00b7gel", "uns", "die", "al\u00b7te", "Lust", "ver\u00b7sterckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Bald hat dein Hochzeit Kleid/ bald haben deine Ringe", "tokens": ["Bald", "hat", "dein", "Hoch\u00b7zeit", "Kleid", "/", "bald", "ha\u00b7ben", "dei\u00b7ne", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Pf\u00e4nder erster Gunst/ mir Z\u00e4hren ausgeprest/", "tokens": ["Die", "Pf\u00e4n\u00b7der", "ers\u00b7ter", "Gunst", "/", "mir", "Z\u00e4h\u00b7ren", "aus\u00b7ge\u00b7prest", "/"], "token_info": ["word", "word", "word", "word", 