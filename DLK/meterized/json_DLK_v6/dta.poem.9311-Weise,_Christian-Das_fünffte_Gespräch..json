{"dta.poem.9311": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "5. Wer aber schon das vierdte kriegt/", "tokens": ["Wer", "a\u00b7ber", "schon", "das", "vierd\u00b7te", "kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das droben auf dem simse liegt/", "tokens": ["Das", "dro\u00b7ben", "auf", "dem", "sim\u00b7se", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schneidet gr\u00f6\u00dfre bissen/", "tokens": ["Der", "schnei\u00b7det", "gr\u00f6\u00df\u00b7re", "bis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die weiber-taffel wird damit", "tokens": ["Die", "wei\u00b7ber\u00b7taf\u00b7fel", "wird", "da\u00b7mit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am fische-fleisch und kuchen-schnitt", "tokens": ["Am", "fi\u00b7sche\u00b7fleisch", "und", "ku\u00b7chen\u00b7schnitt"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich wohl behelffen m\u00fcssen.", "tokens": ["Sich", "wohl", "be\u00b7hel\u00b7ffen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Das auf die m\u00e4nner-tafel sol.", "tokens": ["Das", "auf", "die", "m\u00e4n\u00b7ner\u00b7ta\u00b7fel", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das schneidet etwas k\u00fchner:", "tokens": ["Das", "schnei\u00b7det", "et\u00b7was", "k\u00fch\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn sind die lieben herren nicht", "tokens": ["Denn", "sind", "die", "lie\u00b7ben", "her\u00b7ren", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf \u00fcbrig fressen abgericht", "tokens": ["Auf", "\u00fcb\u00b7rig", "fres\u00b7sen", "ab\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So fressens ihre diener.", "tokens": ["So", "fres\u00b7sens", "ih\u00b7re", "die\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "7. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Dasselb ist aus der weise gro\u00df/", "tokens": ["Das\u00b7selb", "ist", "aus", "der", "wei\u00b7se", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "ART", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hilfft die t\u00fctsche-m\u00fctter/", "tokens": ["Und", "hilfft", "die", "t\u00fct\u00b7sche\u00b7m\u00fct\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch im trenchiren solls allein", "tokens": ["Doch", "im", "tren\u00b7chi\u00b7ren", "solls", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "VVINF", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort vor die musicanten seyn", "tokens": ["Dort", "vor", "die", "mu\u00b7si\u00b7can\u00b7ten", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und vor die hochzeit-bitter.", "tokens": ["Und", "vor", "die", "hoch\u00b7zeit\u00b7bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "8. Jtzt wird das siebend\u2019 angericht/", "tokens": ["Jtzt", "wird", "das", "sie\u00b7bend'", "an\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wird man bey dem schniedte nicht", "tokens": ["Das", "wird", "man", "bey", "dem", "schnied\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIS", "APPR", "ART", "ADJA", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So liederlich vergessen/", "tokens": ["So", "lie\u00b7der\u00b7lich", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und haben wir es nicht beschaut/", "tokens": ["Und", "ha\u00b7ben", "wir", "es", "nicht", "be\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird doch wol die jungfer braut", "tokens": ["So", "wird", "doch", "wol", "die", "jung\u00b7fer", "braut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit auf der hochzeit essen.", "tokens": ["Mit", "auf", "der", "hoch\u00b7zeit", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "9. Die messer sind sehr gut bestalt/", "tokens": ["Die", "mes\u00b7ser", "sind", "sehr", "gut", "be\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum wird die speise warm und kalt", "tokens": ["Drum", "wird", "die", "spei\u00b7se", "warm", "und", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den g\u00e4sten niedlich schmecken:", "tokens": ["Den", "g\u00e4s\u00b7ten", "nied\u00b7lich", "schme\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sonderlich die liebe braut/", "tokens": ["Und", "son\u00b7der\u00b7lich", "die", "lie\u00b7be", "braut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird auff die henn\u2019 und sauerkraut", "tokens": ["Wird", "auff", "die", "henn'", "und", "sau\u00b7er\u00b7kraut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die finger fleissig lecken.", "tokens": ["Die", "fin\u00b7ger", "fleis\u00b7sig", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}