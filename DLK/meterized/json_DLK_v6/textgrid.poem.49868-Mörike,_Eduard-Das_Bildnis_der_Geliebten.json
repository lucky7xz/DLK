{"textgrid.poem.49868": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das Bildnis der Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maler, du zweifelst mit Recht, indem du den seltenen Umri\u00df", "tokens": ["Ma\u00b7ler", ",", "du", "zwei\u00b7felst", "mit", "Recht", ",", "in\u00b7dem", "du", "den", "sel\u00b7te\u00b7nen", "Um\u00b7ri\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Meiner Geliebten bedenkst, wie du beginnest dein Werk.", "tokens": ["Mei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "be\u00b7denkst", ",", "wie", "du", "be\u00b7gin\u00b7nest", "dein", "Werk", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ob von vorn das Gesichtchen, ob du's von der Seite mir zeigest?", "tokens": ["Ob", "von", "vorn", "das", "Ge\u00b7sicht\u00b7chen", ",", "ob", "du's", "von", "der", "Sei\u00b7te", "mir", "zei\u00b7gest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ART", "NN", "$,", "KOUS", "PIS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Viel hat beides f\u00fcr sich und mich beklemmet die Wahl.", "tokens": ["Viel", "hat", "bei\u00b7des", "f\u00fcr", "sich", "und", "mich", "be\u00b7klem\u00b7met", "die", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PRF", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "\u00bbnun, dreiviertel?\u00ab Ich m\u00f6chte das reine Profil nicht entbehren,", "tokens": ["\u00bb", "nun", ",", "drei\u00b7vier\u00b7tel", "?", "\u00ab", "Ich", "m\u00f6ch\u00b7te", "das", "rei\u00b7ne", "Pro\u00b7fil", "nicht", "ent\u00b7beh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADJD", "$.", "$(", "PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Wo sie, so eigen, so neu, kaum nur sich wiedererkennt.", "tokens": ["Wo", "sie", ",", "so", "ei\u00b7gen", ",", "so", "neu", ",", "kaum", "nur", "sich", "wie\u00b7der\u00b7er\u00b7kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Sinnen wir lang? Schon wei\u00df ich, vernimm, die nat\u00fcrlichste Auskunft:", "tokens": ["Sin\u00b7nen", "wir", "lang", "?", "Schon", "wei\u00df", "ich", ",", "ver\u00b7nimm", ",", "die", "na\u00b7t\u00fcr\u00b7lichs\u00b7te", "Aus\u00b7kunft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Male die doppelte mir k\u00fchn auf dasselbige Tuch.", "tokens": ["Ma\u00b7le", "die", "dop\u00b7pel\u00b7te", "mir", "k\u00fchn", "auf", "das\u00b7sel\u00b7bi\u00b7ge", "Tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PPER", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+--++-+--+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Denn was wagst du dabei? Man wird zwei Schwestern erblicken,", "tokens": ["Denn", "was", "wagst", "du", "da\u00b7bei", "?", "Man", "wird", "zwei", "Schwes\u00b7tern", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PAV", "$.", "PIS", "VAFIN", "CARD", "NN", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "\u00c4hnlich einander, doch hat jede das Ihre voraus.", "tokens": ["\u00c4hn\u00b7lich", "ein\u00b7an\u00b7der", ",", "doch", "hat", "je\u00b7de", "das", "Ih\u00b7re", "vo\u00b7raus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "VAFIN", "PIAT", "PDS", "PPOSAT", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Und mich stell in die Mitte! Den Arm auf die Achsel der einen", "tokens": ["Und", "mich", "stell", "in", "die", "Mit\u00b7te", "!", "Den", "Arm", "auf", "die", "Ach\u00b7sel", "der", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "APPR", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "NN", "ART", "ART"], "meter": "--+--+--+--++-+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Leg ich, aber den Blick fe\u00dfle die andere mir,", "tokens": ["Leg", "ich", ",", "a\u00b7ber", "den", "Blick", "fe\u00df\u00b7le", "die", "an\u00b7de\u00b7re", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "PPER", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Die mit h\u00e4ngenden Flechten im h\u00e4uslichen Kleide dabeisteht,", "tokens": ["Die", "mit", "h\u00e4n\u00b7gen\u00b7den", "Flech\u00b7ten", "im", "h\u00e4us\u00b7li\u00b7chen", "Klei\u00b7de", "da\u00b7bei\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Nieder zum Boden die lang schattende Wimper gesenkt,", "tokens": ["Nie\u00b7der", "zum", "Bo\u00b7den", "die", "lang", "schat\u00b7ten\u00b7de", "Wim\u00b7per", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Indes jene, geschm\u00fcckt, und die flei\u00dfig geordneten Z\u00f6pfe", "tokens": ["In\u00b7des", "je\u00b7ne", ",", "ge\u00b7schm\u00fcckt", ",", "und", "die", "flei\u00b7\u00dfig", "ge\u00b7ord\u00b7ne\u00b7ten", "Z\u00f6p\u00b7fe"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "VVPP", "$,", "KON", "ART", "ADJD", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Unter dem griechischen Netz, offenen Auges mir lacht.", "tokens": ["Un\u00b7ter", "dem", "grie\u00b7chi\u00b7schen", "Netz", ",", "of\u00b7fe\u00b7nen", "Au\u00b7ges", "mir", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "\u2013 Eifersucht qu\u00e4lte dich \u00f6fter umsonst: wie gef\u00e4llt dir, Helene,", "tokens": ["\u2013", "Ei\u00b7fer\u00b7sucht", "qu\u00e4l\u00b7te", "dich", "\u00f6f\u00b7ter", "um\u00b7sonst", ":", "wie", "ge\u00b7f\u00e4llt", "dir", ",", "He\u00b7le\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "PPER", "ADV", "ADV", "$.", "PWAV", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Dein zweideutiger Freund zwischen dies P\u00e4rchen gestellt?", "tokens": ["Dein", "zwei\u00b7deu\u00b7ti\u00b7ger", "Freund", "zwi\u00b7schen", "dies", "P\u00e4r\u00b7chen", "ge\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDS", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}