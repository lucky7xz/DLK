{"textgrid.poem.54411": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Musen weckten mich durch ihren schnellen Lauf", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Musen weckten mich durch ihren schnellen Lauf", "tokens": ["Die", "Mu\u00b7sen", "weck\u00b7ten", "mich", "durch", "ih\u00b7ren", "schnel\u00b7len", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch vor Aurorens Glanz von meinem Lager auf.", "tokens": ["Noch", "vor", "Au\u00b7ro\u00b7rens", "Glanz", "von", "mei\u00b7nem", "La\u00b7ger", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie? willst du, sprachen sie, dich nicht mit uns bequemen,", "tokens": ["Wie", "?", "willst", "du", ",", "spra\u00b7chen", "sie", ",", "dich", "nicht", "mit", "uns", "be\u00b7que\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An diesem sch\u00f6nen Fest ein frohes Theil zu nehmen?", "tokens": ["An", "die\u00b7sem", "sch\u00f6\u00b7nen", "Fest", "ein", "fro\u00b7hes", "Theil", "zu", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Tag, an dem sich Mars und selbst Minerva freut,", "tokens": ["Der", "Tag", ",", "an", "dem", "sich", "Mars", "und", "selbst", "Mi\u00b7ner\u00b7va", "freut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF", "NN", "KON", "ADV", "NE", "VVFIN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ermuntert dich gewi\u00df, wie uns, zur Fr\u00f6lichkeit.", "tokens": ["Er\u00b7mun\u00b7tert", "dich", "ge\u00b7wi\u00df", ",", "wie", "uns", ",", "zur", "Fr\u00f6\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Komm, unser Singen hat nur \u2013 \u2013 Ruhm zum Ziele.", "tokens": ["Komm", ",", "un\u00b7ser", "Sin\u00b7gen", "hat", "nur", "\u2013", "\u2013", "Ruhm", "zum", "Zie\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$(", "$(", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O nein, versetzt ich drauf, verzeiht, da\u00df ich nicht spiele;", "tokens": ["O", "nein", ",", "ver\u00b7setzt", "ich", "drauf", ",", "ver\u00b7zeiht", ",", "da\u00df", "ich", "nicht", "spie\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer diesem Held ein Lied zur Ehre bringen will", "tokens": ["Wer", "die\u00b7sem", "Held", "ein", "Lied", "zur", "Eh\u00b7re", "brin\u00b7gen", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das seiner W\u00fcrde gleicht, mu\u00df keinen schwachen Kiel,", "tokens": ["Das", "sei\u00b7ner", "W\u00fcr\u00b7de", "gleicht", ",", "mu\u00df", "kei\u00b7nen", "schwa\u00b7chen", "Kiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von Bl\u00f6digkeit gef\u00fchrt, nein, jener Sappho Gaben,", "tokens": ["Von", "Bl\u00f6\u00b7dig\u00b7keit", "ge\u00b7f\u00fchrt", ",", "nein", ",", "je\u00b7ner", "Sap\u00b7pho", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PTKANT", "$,", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Geist der Scuderey, ja noch was mehres haben.", "tokens": ["Den", "Geist", "der", "Scu\u00b7de\u00b7rey", ",", "ja", "noch", "was", "meh\u00b7res", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "ADV", "PWS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}