{"textgrid.poem.41343": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Bei einem Carneval", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Spiel der Welt besteht aus Mummereien:", "tokens": ["Das", "Spiel", "der", "Welt", "be\u00b7steht", "aus", "Mum\u00b7me\u00b7rei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hofmann schleicht in priesterlicher Tracht;", "tokens": ["Ein", "Hof\u00b7mann", "schleicht", "in", "pries\u00b7ter\u00b7li\u00b7cher", "Tracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Nonne winkt die Nymphe Schmeicheleien;", "tokens": ["Als", "Non\u00b7ne", "winkt", "die", "Nym\u00b7phe", "Schmei\u00b7che\u00b7lei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wuchrer stutzt in eines Sultans Pracht;", "tokens": ["Ein", "Wuch\u00b7rer", "stutzt", "in", "ei\u00b7nes", "Sul\u00b7tans", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der falsche Phrax erscheint im Sch\u00e4ferkleide;", "tokens": ["Der", "fal\u00b7sche", "Phrax", "er\u00b7scheint", "im", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als B\u00e4urin stampft die zarte Flavia;", "tokens": ["Als", "B\u00e4u\u00b7rin", "stampft", "die", "zar\u00b7te", "Fla\u00b7via", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verblendend gl\u00e4nzt im stolzen Erbgeschmeide", "tokens": ["Ver\u00b7blen\u00b7dend", "gl\u00e4nzt", "im", "stol\u00b7zen", "Erb\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Atossa selbst, der L\u00e4ufer Zulica;", "tokens": ["A\u00b7tos\u00b7sa", "selbst", ",", "der", "L\u00e4u\u00b7fer", "Zu\u00b7li\u00b7ca", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als Fledermaus l\u00e4\u00dft Phryne sich nicht nennen,", "tokens": ["Als", "Fle\u00b7der\u00b7maus", "l\u00e4\u00dft", "Phry\u00b7ne", "sich", "nicht", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "NE", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Auch Myrtis nicht, der bunte Papagei.", "tokens": ["Auch", "Myr\u00b7tis", "nicht", ",", "der", "bun\u00b7te", "Pa\u00b7pa\u00b7gei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PTKNEG", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "O m\u00f6chte man stets jedem sagen k\u00f6nnen:", "tokens": ["O", "m\u00f6ch\u00b7te", "man", "stets", "je\u00b7dem", "sa\u00b7gen", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ADV", "PIS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dich, Maske, kenn' ich; ... nur vorbei!", "tokens": ["Dich", ",", "Mas\u00b7ke", ",", "kenn'", "ich", ";", "...", "nur", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$.", "$(", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}