{"dta.poem.5453": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOtt, wie sch\u00f6n ist doch der Himmel? wie wun-", "tokens": ["Ach", "Gott", ",", "wie", "sch\u00f6n", "ist", "doch", "der", "Him\u00b7mel", "?", "wie", "wun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$.", "PWAV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "dersch\u00f6n die gantze Welt?", "tokens": ["der\u00b7sch\u00f6n", "die", "gant\u00b7ze", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die dein Verstand und Will\u2019 erschaffen, die dein Verstand", "tokens": ["Die", "dein", "Ver\u00b7stand", "und", "Will'", "er\u00b7schaf\u00b7fen", ",", "die", "dein", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und Will' erh\u00e4lt!", "tokens": ["und", "Will'", "er\u00b7h\u00e4lt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ach la\u00df mich doch an diesen Wundern, zu deiner Ehr\u2019, in", "tokens": ["Ach", "la\u00df", "mich", "doch", "an", "die\u00b7sen", "Wun\u00b7dern", ",", "zu", "dei\u00b7ner", "Ehr'", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "tausend Freuden,", "tokens": ["tau\u00b7send", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Durch alle Sinnen meinen Geist in fr\u00f6licher Betrachtung", "tokens": ["Durch", "al\u00b7le", "Sin\u00b7nen", "mei\u00b7nen", "Geist", "in", "fr\u00f6\u00b7li\u00b7cher", "Be\u00b7trach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "weiden,", "tokens": ["wei\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und die dadurch in mir gewirckte Vergn\u00fcglichkeit der re-", "tokens": ["Und", "die", "da\u00b7durch", "in", "mir", "ge\u00b7wirck\u00b7te", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", "der", "re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PAV", "APPR", "PPER", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "gen Seelen,", "tokens": ["gen", "See\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Die durch die Wirckung deiner Liebe aus aller Wunder", "tokens": ["Die", "durch", "die", "Wir\u00b7ckung", "dei\u00b7ner", "Lie\u00b7be", "aus", "al\u00b7ler", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Sch\u00f6nheit qvillt,", "tokens": ["Sch\u00f6n\u00b7heit", "qvillt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Die, wenn ich sie mit Lust betrachte, mit Lust mein gan-", "tokens": ["Die", ",", "wenn", "ich", "sie", "mit", "Lust", "be\u00b7trach\u00b7te", ",", "mit", "Lust", "mein", "gan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "tzes Wesen f\u00fcllt,", "tokens": ["tzes", "We\u00b7sen", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Zu deines grossen Nahmens Ruhme, auch andern oft zur", "tokens": ["Zu", "dei\u00b7nes", "gros\u00b7sen", "Nah\u00b7mens", "Ruh\u00b7me", ",", "auch", "an\u00b7dern", "oft", "zur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,", "ADV", "PIS", "ADV", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Folg', erzehlen!", "tokens": ["Fol\u00b7g'", ",", "er\u00b7zeh\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}