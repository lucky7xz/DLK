{"textgrid.poem.26701": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbglaub ich\u00ab, sprichst du, \u00bbdem Wort, das der Weisheit Meister mich lehren,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbglaub ich\u00ab, sprichst du, \u00bbdem Wort, das der Weisheit Meister mich lehren,", "tokens": ["\u00bb", "glaub", "ich", "\u00ab", ",", "sprichst", "du", ",", "\u00bb", "dem", "Wort", ",", "das", "der", "Weis\u00b7heit", "Meis\u00b7ter", "mich", "leh\u00b7ren", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Das der Lehrlinge Schar sicher und fertig beschw\u00f6rt?", "tokens": ["Das", "der", "Lehr\u00b7lin\u00b7ge", "Schar", "si\u00b7cher", "und", "fer\u00b7tig", "be\u00b7schw\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "---+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Kann die Wissenschaft nur zum wahren Frieden mich f\u00fchren,", "tokens": ["Kann", "die", "Wis\u00b7sen\u00b7schaft", "nur", "zum", "wah\u00b7ren", "Frie\u00b7den", "mich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nur des Systemes Geb\u00e4lk st\u00fctzen das Gl\u00fcck und das Recht?", "tokens": ["Nur", "des", "Sys\u00b7te\u00b7mes", "Ge\u00b7b\u00e4lk", "st\u00fct\u00b7zen", "das", "Gl\u00fcck", "und", "das", "Recht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "--+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Mu\u00df ich dem Trieb mi\u00dftraun, der leise mich warnt, dem Gesetze,", "tokens": ["Mu\u00df", "ich", "dem", "Trieb", "mi\u00df\u00b7traun", ",", "der", "lei\u00b7se", "mich", "warnt", ",", "dem", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Das du selber, Natur, mir in den Busen gepr\u00e4gt,", "tokens": ["Das", "du", "sel\u00b7ber", ",", "Na\u00b7tur", ",", "mir", "in", "den", "Bu\u00b7sen", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Bis auf die ewige Schrift die ", "tokens": ["Bis", "auf", "die", "e\u00b7wi\u00b7ge", "Schrift", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und der Formel Gef\u00e4\u00df bindet den fl\u00fcchtigen Geist? Sage du mirs, du bist in diese Tiefen gestiegen,", "tokens": ["Und", "der", "For\u00b7mel", "Ge\u00b7f\u00e4\u00df", "bin\u00b7det", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Geist", "?", "Sa\u00b7ge", "du", "mirs", ",", "du", "bist", "in", "die\u00b7se", "Tie\u00b7fen", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "NN", "NE", "NE", "$,", "PPER", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "--+--+---+--+---+-+-+-+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Aus dem modrigten Grab kamst du erhalten zur\u00fcck,", "tokens": ["Aus", "dem", "mod\u00b7rig\u00b7ten", "Grab", "kamst", "du", "er\u00b7hal\u00b7ten", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Dir ist bekannt, was die Gruft der dunklen W\u00f6rter bewahret,", "tokens": ["Dir", "ist", "be\u00b7kannt", ",", "was", "die", "Gruft", "der", "dunk\u00b7len", "W\u00f6r\u00b7ter", "be\u00b7wah\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Ob der Lebenden Trost dort bei den Mumien wohnt.", "tokens": ["Ob", "der", "Le\u00b7ben\u00b7den", "Trost", "dort", "bei", "den", "Mu\u00b7mi\u00b7en", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Mu\u00df ich ihn wandeln, den n\u00e4chtlichen Weg? Mir graut, ich bekenn es!", "tokens": ["Mu\u00df", "ich", "ihn", "wan\u00b7deln", ",", "den", "n\u00e4cht\u00b7li\u00b7chen", "Weg", "?", "Mir", "graut", ",", "ich", "be\u00b7kenn", "es", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Wandeln will ich ihn doch, f\u00fchrt er zu Wahrheit und Recht.\u00ab", "tokens": ["Wan\u00b7deln", "will", "ich", "ihn", "doch", ",", "f\u00fchrt", "er", "zu", "Wahr\u00b7heit", "und", "Recht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "$,", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Freund, du kennst doch die Goldene Zeit, es haben die Dichter", "tokens": ["Freund", ",", "du", "kennst", "doch", "die", "Gol\u00b7de\u00b7ne", "Zeit", ",", "es", "ha\u00b7ben", "die", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Manche Sage von ihr r\u00fchrend und kindlich erz\u00e4hlt,", "tokens": ["Man\u00b7che", "Sa\u00b7ge", "von", "ihr", "r\u00fch\u00b7rend", "und", "kind\u00b7lich", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Jene Zeit, da das Heilige noch im Leben gewandelt,", "tokens": ["Je\u00b7ne", "Zeit", ",", "da", "das", "Hei\u00b7li\u00b7ge", "noch", "im", "Le\u00b7ben", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "ART", "ADJA", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Da jungfr\u00e4ulich und keusch noch das Gef\u00fchl sich bewahrt,", "tokens": ["Da", "jung\u00b7fr\u00e4u\u00b7lich", "und", "keusch", "noch", "das", "Ge\u00b7f\u00fchl", "sich", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Da noch das gro\u00dfe Gesetz, das oben im Sonnenlauf waltet", "tokens": ["Da", "noch", "das", "gro\u00b7\u00dfe", "Ge\u00b7setz", ",", "das", "o\u00b7ben", "im", "Son\u00b7nen\u00b7lauf", "wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und verborgen im Ei reget den h\u00fcpfenden Punkt,", "tokens": ["Und", "ver\u00b7bor\u00b7gen", "im", "Ei", "re\u00b7get", "den", "h\u00fcp\u00b7fen\u00b7den", "Punkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Noch der Notwendigkeit stilles Gesetz, das stetige, gleiche,", "tokens": ["Noch", "der", "Not\u00b7wen\u00b7dig\u00b7keit", "stil\u00b7les", "Ge\u00b7setz", ",", "das", "ste\u00b7ti\u00b7ge", ",", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+---+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Auch der menschlichen Brust freiere Wellen bewegt,", "tokens": ["Auch", "der", "menschli\u00b7chen", "Brust", "frei\u00b7e\u00b7re", "Wel\u00b7len", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.22": {"text": "Da nicht irrend der Sinn und treu, wie der Zeiger am Uhrwerk,", "tokens": ["Da", "nicht", "ir\u00b7rend", "der", "Sinn", "und", "treu", ",", "wie", "der", "Zei\u00b7ger", "am", "Uhr\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "ART", "NN", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Auf das Wahrhaftige nur, nur auf das Ewige wies?", "tokens": ["Auf", "das", "Wahr\u00b7haf\u00b7ti\u00b7ge", "nur", ",", "nur", "auf", "das", "E\u00b7wi\u00b7ge", "wies", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.24": {"text": "Da war kein Profaner, kein Eingeweihter zu sehen,", "tokens": ["Da", "war", "kein", "Pro\u00b7fa\u00b7ner", ",", "kein", "Ein\u00b7ge\u00b7weih\u00b7ter", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Was man lebendig empfand, ward nicht bei Toten gesucht,", "tokens": ["Was", "man", "le\u00b7ben\u00b7dig", "emp\u00b7fand", ",", "ward", "nicht", "bei", "To\u00b7ten", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$,", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Gleich verst\u00e4ndlich f\u00fcr jegliches Herz war die ewige Regel,", "tokens": ["Gleich", "ver\u00b7st\u00e4nd\u00b7lich", "f\u00fcr", "jeg\u00b7li\u00b7ches", "Herz", "war", "die", "e\u00b7wi\u00b7ge", "Re\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Hat der getreuen Natur g\u00f6ttlichen Frieden gest\u00f6rt.", "tokens": ["Hat", "der", "ge\u00b7treu\u00b7en", "Na\u00b7tur", "g\u00f6tt\u00b7li\u00b7chen", "Frie\u00b7den", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das entweihte Gef\u00fchl ist nicht mehr Stimme der G\u00f6tter,", "tokens": ["Das", "ent\u00b7weih\u00b7te", "Ge\u00b7f\u00fchl", "ist", "nicht", "mehr", "Stim\u00b7me", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "PIAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und das Orakel verstummt in der entadelten Brust.", "tokens": ["Und", "das", "O\u00b7ra\u00b7kel", "ver\u00b7stummt", "in", "der", "en\u00b7ta\u00b7del\u00b7ten", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nur in dem stilleren Selbst vernimmt es der horchende Geist noch,", "tokens": ["Nur", "in", "dem", "stil\u00b7le\u00b7ren", "Selbst", "ver\u00b7nimmt", "es", "der", "hor\u00b7chen\u00b7de", "Geist", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Und den heiligen Sinn h\u00fctet das mystische Wort.", "tokens": ["Und", "den", "hei\u00b7li\u00b7gen", "Sinn", "h\u00fc\u00b7tet", "das", "mys\u00b7ti\u00b7sche", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Hier beschw\u00f6rt es der Forscher, der reines Herzens hinabsteigt,", "tokens": ["Hier", "be\u00b7schw\u00f6rt", "es", "der", "For\u00b7scher", ",", "der", "rei\u00b7nes", "Her\u00b7zens", "hin\u00b7ab\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und die verlorne Natur gibt ihm die Weisheit zur\u00fcck.", "tokens": ["Und", "die", "ver\u00b7lor\u00b7ne", "Na\u00b7tur", "gibt", "ihm", "die", "Weis\u00b7heit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Hast du, Gl\u00fccklicher, nie den sch\u00fctzenden Engel verloren,", "tokens": ["Hast", "du", ",", "Gl\u00fcck\u00b7li\u00b7cher", ",", "nie", "den", "sch\u00fct\u00b7zen\u00b7den", "En\u00b7gel", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Nie des frommen Instinkts liebende Warnung verwirkt,", "tokens": ["Nie", "des", "from\u00b7men", "Ins\u00b7tinkts", "lie\u00b7ben\u00b7de", "War\u00b7nung", "ver\u00b7wirkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Malt in dem keuschen Auge noch treu und rein sich die Wahrheit,", "tokens": ["Malt", "in", "dem", "keu\u00b7schen", "Au\u00b7ge", "noch", "treu", "und", "rein", "sich", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.11": {"text": "T\u00f6nt ihr Rufen dir noch hell in der kindlichen Brust,", "tokens": ["T\u00f6nt", "ihr", "Ru\u00b7fen", "dir", "noch", "hell", "in", "der", "kind\u00b7li\u00b7chen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "APPR", 