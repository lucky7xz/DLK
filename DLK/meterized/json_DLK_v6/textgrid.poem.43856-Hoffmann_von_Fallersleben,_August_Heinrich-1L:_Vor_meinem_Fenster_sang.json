{"textgrid.poem.43856": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor meinem Fenster sang", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Fenster sang", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein V\u00f6gelein,", "tokens": ["Ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und bittend sprach's: \u00bbmach' auf!\u00ab", "tokens": ["Und", "bit\u00b7tend", "sprach's", ":", "\u00bb", "mach'", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "$(", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da lie\u00df ich's ein.", "tokens": ["Da", "lie\u00df", "ich's", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sag', liebes V\u00f6glein, sag',", "tokens": ["Sag'", ",", "lie\u00b7bes", "V\u00f6\u00b7glein", ",", "sag'", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was willst du hier?", "tokens": ["Was", "willst", "du", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was k\u00f6nnt'st du bringen jetzt", "tokens": ["Was", "k\u00f6nnt'st", "du", "brin\u00b7gen", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Botschaft mir?", "tokens": ["F\u00fcr", "Bot\u00b7schaft", "mir", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Es liegt in Trauer still", "tokens": ["Es", "liegt", "in", "Trau\u00b7er", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Noch Wald und Feld,", "tokens": ["Noch", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es ist, als w\u00e4re todt", "tokens": ["Es", "ist", ",", "als", "w\u00e4\u00b7re", "todt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Welt. \u2013", "tokens": ["Die", "gan\u00b7ze", "Welt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "\u00bbder Fr\u00fchling gr\u00fc\u00dfet dich,", "tokens": ["\u00bb", "der", "Fr\u00fch\u00b7ling", "gr\u00fc\u00b7\u00dfet", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er denket dein,", "tokens": ["Er", "den\u00b7ket", "dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Er wollte bald bei dir", "tokens": ["Er", "woll\u00b7te", "bald", "bei", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich finden ein.\u00ab \u2013", "tokens": ["Sich", "fin\u00b7den", "ein", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PRF", "VVFIN", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Hab' Dank, lieb V\u00f6gelein,", "tokens": ["Hab'", "Dank", ",", "lieb", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hab' Dank daf\u00fcr!", "tokens": ["Hab'", "Dank", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sag' ihm: ge\u00f6ffnet sei", "tokens": ["Sag'", "ihm", ":", "ge\u00b7\u00f6ff\u00b7net", "sei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$.", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihm Herz und Th\u00fcr!", "tokens": ["Ihm", "Herz", "und", "Th\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}