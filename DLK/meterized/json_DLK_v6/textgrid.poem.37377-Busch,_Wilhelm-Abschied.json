{"textgrid.poem.37377": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wie eilte so geschwinde", "tokens": ["Ach", ",", "wie", "eil\u00b7te", "so", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Sommer durch die Welt:", "tokens": ["Die\u00b7ser", "Som\u00b7mer", "durch", "die", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herbstlich rauscht es in der Linde,", "tokens": ["Herbst\u00b7lich", "rauscht", "es", "in", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Bl\u00e4tter mit dem Winde", "tokens": ["Ih\u00b7re", "Bl\u00e4t\u00b7ter", "mit", "dem", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wehen \u00fcbers Stoppelfeld.", "tokens": ["We\u00b7hen", "\u00fc\u00b7bers", "Stop\u00b7pel\u00b7feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rst du in den L\u00fcften klingend", "tokens": ["H\u00f6rst", "du", "in", "den", "L\u00fcf\u00b7ten", "klin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehnlich klagend das Kuru?", "tokens": ["Sehn\u00b7lich", "kla\u00b7gend", "das", "Ku\u00b7ru", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wanderv\u00f6gel, fl\u00fcgelschwingend,", "tokens": ["Wan\u00b7der\u00b7v\u00f6\u00b7gel", ",", "fl\u00fc\u00b7gel\u00b7schwin\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebewohl der Heimat singend,", "tokens": ["Le\u00b7be\u00b7wohl", "der", "Hei\u00b7mat", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ziehn dem fremden Lande zu.", "tokens": ["Ziehn", "dem", "frem\u00b7den", "Lan\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Morgen mu\u00df ich in die Ferne.", "tokens": ["Mor\u00b7gen", "mu\u00df", "ich", "in", "die", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebes M\u00e4dchen, bleib mir gut.", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", ",", "bleib", "mir", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen lebt in der Kaserne,", "tokens": ["Mor\u00b7gen", "lebt", "in", "der", "Ka\u00b7ser\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er exerzieren lerne,", "tokens": ["Da\u00df", "er", "ex\u00b7er\u00b7zie\u00b7ren", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein dich liebender Rekrut.", "tokens": ["Dein", "dich", "lie\u00b7ben\u00b7der", "Re\u00b7krut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}