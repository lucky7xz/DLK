{"textgrid.poem.31519": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du reiner Gottheit-Glantz / den gar die Seraphinnen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du reiner Gottheit-Glantz / den gar die Seraphinnen", "tokens": ["Du", "rei\u00b7ner", "Gott\u00b7heit\u00b7Glantz", "/", "den", "gar", "die", "Se\u00b7ra\u00b7phin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sich unbedecket selbst / zu loben / wagen nicht!", "tokens": ["sich", "un\u00b7be\u00b7de\u00b7cket", "selbst", "/", "zu", "lo\u00b7ben", "/", "wa\u00b7gen", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "$(", "PTKZU", "VVINF", "$(", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wird mit dem S\u00fcndenschlamm / dem Speichel / hie verpicht", "tokens": ["wird", "mit", "dem", "S\u00fcn\u00b7den\u00b7schlamm", "/", "dem", "Spei\u00b7chel", "/", "hie", "ver\u00b7picht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "$(", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Quell der Reinigkeit / l\u00e4sst sich mit Koht umrinnen /", "tokens": ["die", "Quell", "der", "Rei\u00b7nig\u00b7keit", "/", "l\u00e4sst", "sich", "mit", "Koht", "um\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "in dem am Thabor vor die G\u00f6ttlich Sch\u00f6n' erschienen.", "tokens": ["in", "dem", "am", "Tha\u00b7bor", "vor", "die", "G\u00f6tt\u00b7lich", "Sch\u00f6n'", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPRART", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der H\u00f6llen Vnflat jetzt das Drachen-Gifft anricht /", "tokens": ["Der", "H\u00f6l\u00b7len", "Vn\u00b7flat", "jetzt", "das", "Dra\u00b7chen\u00b7Gifft", "an\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "er schiest jetzt seinen Strahl in unser Lebens-Liecht.", "tokens": ["er", "schiest", "jetzt", "sei\u00b7nen", "Strahl", "in", "un\u00b7ser", "Le\u00b7bens\u00b7Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch pfleget Gottes Lieb' im Speyungs-Meer zu brinnen.", "tokens": ["Noch", "pfle\u00b7get", "Got\u00b7tes", "Lieb'", "im", "Speyungs\u00b7Meer", "zu", "brin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ach wesentliche Ehr der selbsten G\u00f6ttlichkeit!", "tokens": ["Ach", "we\u00b7sent\u00b7li\u00b7che", "Ehr", "der", "selbs\u00b7ten", "G\u00f6tt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie kanst des Spottes du doch f\u00e4hig seyn / und dulten?", "tokens": ["wie", "kanst", "des", "Spot\u00b7tes", "du", "doch", "f\u00e4\u00b7hig", "seyn", "/", "und", "dul\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PPER", "ADV", "ADJD", "VAINF", "$(", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "es ist kein' Eusserung der Liebes-Macht zu weit /", "tokens": ["es", "ist", "kein'", "Eus\u00b7se\u00b7rung", "der", "Lie\u00b7bes\u00b7Macht", "zu", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und nichts unleidenlich so Gott-gleich-grossen Hulden.", "tokens": ["und", "nichts", "un\u00b7lei\u00b7den\u00b7lich", "so", "Got\u00b7tgleich\u00b7gros\u00b7sen", "Hul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Je mehr die G\u00f6ttlichkeit die Strahlen in sich r\u00fcckt /", "tokens": ["Je", "mehr", "die", "G\u00f6tt\u00b7lich\u00b7keit", "die", "Strah\u00b7len", "in", "sich", "r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "je mehrer deren Krafft aus deinem Leiden blickt.", "tokens": ["je", "meh\u00b7rer", "de\u00b7ren", "Krafft", "aus", "dei\u00b7nem", "Lei\u00b7den", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}