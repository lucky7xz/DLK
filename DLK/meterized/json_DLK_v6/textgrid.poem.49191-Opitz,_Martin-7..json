{"textgrid.poem.49191": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Liebste, la\u00df uns eilen,", "tokens": ["Ach", "Liebs\u00b7te", ",", "la\u00df", "uns", "ei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir haben Zeit,", "tokens": ["Wir", "ha\u00b7ben", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es schadet uns verweilen", "tokens": ["Es", "scha\u00b7det", "uns", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns beyderseit.", "tokens": ["Uns", "bey\u00b7der\u00b7seit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der edlen Sch\u00f6nheit Gaben", "tokens": ["Der", "ed\u00b7len", "Sch\u00f6n\u00b7heit", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fliehen Fu\u00df f\u00fcr Fu\u00df,", "tokens": ["Flie\u00b7hen", "Fu\u00df", "f\u00fcr", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df alles, was wir haben,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verschwinden mu\u00df.", "tokens": ["Ver\u00b7schwin\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Wangen Ziehr verbleichet,", "tokens": ["Der", "Wan\u00b7gen", "Ziehr", "ver\u00b7blei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Haar wird grei\u00df,", "tokens": ["Das", "Haar", "wird", "grei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Augen Feuer weichet,", "tokens": ["Der", "Au\u00b7gen", "Feu\u00b7er", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Brunst wird Ei\u00df.", "tokens": ["Die", "Brunst", "wird", "Ei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das M\u00fcndlein von Corallen", "tokens": ["Das", "M\u00fcnd\u00b7lein", "von", "Co\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird ungestalt,", "tokens": ["Wird", "un\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die H\u00e4nd' als Schnee verfallen,", "tokens": ["Die", "H\u00e4nd'", "als", "Schnee", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und du wirst alt.", "tokens": ["Und", "du", "wirst", "alt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Drumb la\u00df uns jetzt geniessen", "tokens": ["Drumb", "la\u00df", "uns", "jetzt", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Jugend Frucht,", "tokens": ["Der", "Ju\u00b7gend", "Frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eh' als wir folgen m\u00fcssen", "tokens": ["Eh'", "als", "wir", "fol\u00b7gen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Jahre Flucht.", "tokens": ["Der", "Jah\u00b7re", "Flucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wo du dich selber liebest,", "tokens": ["Wo", "du", "dich", "sel\u00b7ber", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So liebe mich,", "tokens": ["So", "lie\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gieb mir das, wann du giebest,", "tokens": ["Gieb", "mir", "das", ",", "wann", "du", "gie\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verlier auch ich.", "tokens": ["Ver\u00b7lier", "auch", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}