{"textgrid.poem.65718": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier ligt ein Mann von klugem Raht.", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ligt ein Mann von klugem Raht.", "tokens": ["Hier", "ligt", "ein", "Mann", "von", "klu\u00b7gem", "Raht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zweyerley begangen hat,", "tokens": ["Der", "zwey\u00b7er\u00b7ley", "be\u00b7gan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit er Ruhm erwarbe.", "tokens": ["Da\u00b7mit", "er", "Ruhm", "er\u00b7war\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn erstlich lebt er manche Stund;", "tokens": ["Denn", "erst\u00b7lich", "lebt", "er", "man\u00b7che", "Stund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da er nicht mehr leben kunnt,", "tokens": ["Und", "da", "er", "nicht", "mehr", "le\u00b7ben", "kunnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bedacht er sich, und starbe.", "tokens": ["Be\u00b7dacht", "er", "sich", ",", "und", "star\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}