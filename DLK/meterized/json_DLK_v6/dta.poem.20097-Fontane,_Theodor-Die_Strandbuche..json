{"dta.poem.20097": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Strandbuche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf meerumbrauster D\u00fcne ragt in voller", "tokens": ["Hoch", "auf", "meer\u00b7um\u00b7braus\u00b7ter", "D\u00fc\u00b7ne", "ragt", "in", "vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eine Buche; \u201eMutter \u2014 ruft sie \u2014 wieder kam", "tokens": ["Ei\u00b7ne", "Bu\u00b7che", ";", "\u201e", "Mut\u00b7ter", "ruft", "sie", "wie\u00b7der", "kam"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "NN", "$(", "VVFIN", "PPER", "$(", "ADV", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wieder hat\u2019s aus gr\u00fcnem Seetang viel der Kr\u00e4nze", "tokens": ["Wie\u00b7der", "hat's", "aus", "gr\u00fc\u00b7nem", "See\u00b7tang", "viel", "der", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Hat mir Bernsteinschmuck gespendet, und von", "tokens": ["Hat", "mir", "Bern\u00b7stein\u00b7schmuck", "ge\u00b7spen\u00b7det", ",", "und", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$,", "KON", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u201emutter, schilt es nicht Verf\u00fchrer, sag nicht,", "tokens": ["\u201e", "mut\u00b7ter", ",", "schilt", "es", "nicht", "Ver\u00b7f\u00fch\u00b7rer", ",", "sag", "nicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Treulos ist allein die Schw\u00e4che und gewaltig", "tokens": ["Treu\u00b7los", "ist", "al\u00b7lein", "die", "Schw\u00e4\u00b7che", "und", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Hieltest Du mich nicht umklammert, Mutter", "tokens": ["Hiel\u00b7test", "Du", "mich", "nicht", "um\u00b7klam\u00b7mert", ",", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4r ich Nachts, als es mich lockte, hin an seine", "tokens": ["W\u00e4r", "ich", "Nachts", ",", "als", "es", "mich", "lock\u00b7te", ",", "hin", "an", "sei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "\u201e\u201eSturm herbei!\u201c\u201c rief wild-aufjauchzend jetzt", "tokens": ["\u201e", "\u201e", "Sturm", "her\u00b7bei", "!", "\u201c", "\u201c", "rief", "wild\u00b7auf\u00b7jauch\u00b7zend", "jetzt"], "token_info": ["punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "$(", "NN", "PTKVZ", "$.", "$(", "$(", "VVFIN", "ADJD", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und auf hundert Wolkenrossen jagte schnaubend", "tokens": ["Und", "auf", "hun\u00b7dert", "Wol\u00b7ken\u00b7ros\u00b7sen", "jag\u00b7te", "schnau\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201e\u201eAuf! entwurzle mir die Buche, \u2019s gilt der Sehn-", "tokens": ["\u201e", "\u201e", "Auf", "!", "ent\u00b7wurz\u00b7le", "mir", "die", "Bu\u00b7che", ",", "'s", "gilt", "der", "Sehn"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "$(", "APPR", "$.", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "W\u00e4r sie frei, sie w\u00fcrde selber sich in meine", "tokens": ["W\u00e4r", "sie", "frei", ",", "sie", "w\u00fcr\u00b7de", "sel\u00b7ber", "sich", "in", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "ADV", "PRF", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "\u201e\u201eArme Th\u00f6rin, die des Meeres eitlen Liebes-", "tokens": ["\u201e", "\u201e", "Ar\u00b7me", "Th\u00f6\u00b7rin", ",", "die", "des", "Mee\u00b7res", "eit\u00b7len", "Lie\u00b7bes"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Jeder Tanne spend ich Bernstein, jede Buche", "tokens": ["Je\u00b7der", "Tan\u00b7ne", "spend", "ich", "Bern\u00b7stein", ",", "je\u00b7de", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "PPER", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Nicht um ", "tokens": ["Nicht", "um"], "token_info": ["word", "word"], "pos": ["PTKNEG", "KOUI"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und der Liebe Wonne wird Dich bald wie Todes-", "tokens": ["Und", "der", "Lie\u00b7be", "Won\u00b7ne", "wird", "Dich", "bald", "wie", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "PPER", "ADV", "KOKOM", "TRUNC"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Tiefes Schweigen; \u2014 aber pl\u00f6tzlich kracht die", "tokens": ["Tie\u00b7fes", "Schwei\u00b7gen", ";", "a\u00b7ber", "pl\u00f6tz\u00b7lich", "kracht", "die"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "$(", "ADV", "ADJD", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4tterstiebend st\u00fcrzt sie nieder wie ein gr\u00fcner", "tokens": ["Bl\u00e4t\u00b7ter\u00b7stie\u00b7bend", "st\u00fcrzt", "sie", "nie\u00b7der", "wie", "ein", "gr\u00fc\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Laut erbrausend hei\u00dft sein neues Opfer jetzt das", "tokens": ["Laut", "er\u00b7brau\u00b7send", "hei\u00dft", "sein", "neu\u00b7es", "Op\u00b7fer", "jetzt", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVPP", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Hochaufsch\u00e4umend hat\u2019s der Riese an die Wellen-", "tokens": ["Hoch\u00b7auf\u00b7sch\u00e4u\u00b7mend", "hat's", "der", "Rie\u00b7se", "an", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "\u201eweh, halt ein in Deinem Rasen, das mich zu", "tokens": ["\u201e", "weh", ",", "halt", "ein", "in", "Dei\u00b7nem", "Ra\u00b7sen", ",", "das", "mich", "zu"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PTKZU"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "So entbl\u00e4ttert nicht die Liebe, so entbl\u00e4ttert", "tokens": ["So", "ent\u00b7bl\u00e4t\u00b7tert", "nicht", "die", "Lie\u00b7be", ",", "so", "ent\u00b7bl\u00e4t\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch die Leidenschaft des Riesen kennet nicht der", "tokens": ["Doch", "die", "Lei\u00b7den\u00b7schaft", "des", "Rie\u00b7sen", "ken\u00b7net", "nicht", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und er spielt mit seinem Opfer, bis es todt in", "tokens": ["Und", "er", "spielt", "mit", "sei\u00b7nem", "Op\u00b7fer", ",", "bis", "es", "todt", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Aber dann, als ob er Abscheu gegen eine Leiche", "tokens": ["A\u00b7ber", "dann", ",", "als", "ob", "er", "Ab\u00b7scheu", "ge\u00b7gen", "ei\u00b7ne", "Lei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOKOM", "KOUS", "PPER", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Hat er seiner L\u00fcste Spielzeug wieder an den", "tokens": ["Hat", "er", "sei\u00b7ner", "L\u00fcs\u00b7te", "Spiel\u00b7zeug", "wie\u00b7der", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "An dem Fu\u00df der D\u00fcne, deren Gipfel einst der", "tokens": ["An", "dem", "Fu\u00df", "der", "D\u00fc\u00b7ne", ",", "de\u00b7ren", "Gip\u00b7fel", "einst", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELAT", "NN", "ADV", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Hat die alte Mutter Erde ihr entf\u00fchrtes Kind", "tokens": ["Hat", "die", "al\u00b7te", "Mut\u00b7ter", "Er\u00b7de", "ihr", "ent\u00b7f\u00fchr\u00b7tes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}