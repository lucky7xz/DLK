{"textgrid.poem.46149": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Abwesenheit", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf, fleug bald mein junges herz", "tokens": ["Auf", ",", "auf", ",", "fleug", "bald", "mein", "jun\u00b7ges", "herz"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$,", "PTKVZ", "$,", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zu deren, die dich allein n\u00e4hret;", "tokens": ["zu", "de\u00b7ren", ",", "die", "dich", "al\u00b7lein", "n\u00e4h\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "sag ihr, wie \u00fcbergro\u00dfer schmerz", "tokens": ["sag", "ihr", ",", "wie", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfer", "schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "von ihretwegen mich beth\u00f6ret.", "tokens": ["von", "ih\u00b7ret\u00b7we\u00b7gen", "mich", "be\u00b7th\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sag ihr, wie mein geist tag und nacht", "tokens": ["Sag", "ihr", ",", "wie", "mein", "geist", "tag", "und", "nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "KOKOM", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "nichts dan klagwort von ihr erdichtet,", "tokens": ["nichts", "dan", "klag\u00b7wort", "von", "ihr", "er\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und wie der lieb zu gro\u00dfe macht", "tokens": ["und", "wie", "der", "lieb", "zu", "gro\u00b7\u00dfe", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJD", "PTKZU", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in mir schier die vernunft vernichtet.", "tokens": ["in", "mir", "schier", "die", "ver\u00b7nunft", "ver\u00b7nich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ART", "ADJD", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sag ihr, wie die abwesenheit", "tokens": ["Sag", "ihr", ",", "wie", "die", "ab\u00b7we\u00b7sen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein angesicht untr\u00f6stlich netzet,", "tokens": ["mein", "an\u00b7ge\u00b7sicht", "un\u00b7tr\u00f6st\u00b7lich", "net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und wie ihr s\u00fc\u00dfe freindlichkeit", "tokens": ["und", "wie", "ihr", "s\u00fc\u00b7\u00dfe", "freind\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mich, leider! t\u00f6dlich jetz verletzet.", "tokens": ["mich", ",", "lei\u00b7der", "!", "t\u00f6d\u00b7lich", "jetz", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "$.", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sag auch, da\u00df, wan in der pein", "tokens": ["Doch", "sag", "auch", ",", "da\u00df", ",", "wan", "in", "der", "pein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "not, tr\u00fcbsal, elend, angst und klagen", "tokens": ["not", ",", "tr\u00fcb\u00b7sal", ",", "e\u00b7lend", ",", "angst", "und", "kla\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sie meiner ingedenk wird sein,", "tokens": ["sie", "mei\u00b7ner", "in\u00b7ge\u00b7denk", "wird", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich selig, solches zu ertragen.", "tokens": ["ich", "se\u00b7lig", ",", "sol\u00b7ches", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}