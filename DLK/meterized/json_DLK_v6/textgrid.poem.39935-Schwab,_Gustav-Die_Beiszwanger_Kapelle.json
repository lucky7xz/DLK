{"textgrid.poem.39935": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Beiszwanger Kapelle", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ritter von dem Rosenstein,", "tokens": ["Die", "Rit\u00b7ter", "von", "dem", "Ro\u00b7sen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ritten aus beim Sonnenschein,", "tokens": ["Sie", "rit\u00b7ten", "aus", "beim", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ritten aus mit ihren Knappen,", "tokens": ["Sie", "rit\u00b7ten", "aus", "mit", "ih\u00b7ren", "Knap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mit den d\u00fcstern Nebelkappen", "tokens": ["Wenn", "mit", "den", "d\u00fcs\u00b7tern", "Ne\u00b7bel\u00b7kap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Berge regendurstig nickten,", "tokens": ["Die", "Ber\u00b7ge", "re\u00b7gen\u00b7durs\u00b7tig", "nick\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und in die Ebne finster blickten,", "tokens": ["Und", "in", "die", "Eb\u00b7ne", "fins\u00b7ter", "blick\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, wenn das Wetter blitzt' und kracht',", "tokens": ["Ja", ",", "wenn", "das", "Wet\u00b7ter", "blitzt'", "und", "kracht'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie ritten aus in schwarzer Nacht;", "tokens": ["Sie", "rit\u00b7ten", "aus", "in", "schwar\u00b7zer", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn immer war der Fang gelungen,", "tokens": ["Denn", "im\u00b7mer", "war", "der", "Fang", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn durch die stillen Niederungen", "tokens": ["Wenn", "durch", "die", "stil\u00b7len", "Nie\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Wandersmann, ein Kaufherr zog,", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", ",", "ein", "Kauf\u00b7herr", "zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und sichre Fahrt die Stra\u00dfe log.", "tokens": ["Und", "sich\u00b7re", "Fahrt", "die", "Stra\u00b7\u00dfe", "log", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Jetzt zogen sie an einem Morgen,", "tokens": ["Jetzt", "zo\u00b7gen", "sie", "an", "ei\u00b7nem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Noch war die Welt in Schlaf geborgen,", "tokens": ["Noch", "war", "die", "Welt", "in", "Schlaf", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Von ihrem hohen Felsen aus,", "tokens": ["Von", "ih\u00b7rem", "ho\u00b7hen", "Fel\u00b7sen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zur Ebne nach dem Gotteshaus.", "tokens": ["Zur", "Eb\u00b7ne", "nach", "dem", "Got\u00b7tes\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das hob sich aus den gr\u00fcnen Matten", "tokens": ["Das", "hob", "sich", "aus", "den", "gr\u00fc\u00b7nen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "In seiner Linden k\u00fchlem Schatten,", "tokens": ["In", "sei\u00b7ner", "Lin\u00b7den", "k\u00fch\u00b7lem", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Als f\u00fcrchtet' es, umrankt mit Laub,", "tokens": ["Als", "f\u00fcrch\u00b7tet'", "es", ",", "um\u00b7rankt", "mit", "Laub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.20": {"text": "Von keiner Seite Hohn und Raub.", "tokens": ["Von", "kei\u00b7ner", "Sei\u00b7te", "Hohn", "und", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Es hingen an den schmucken W\u00e4nden,", "tokens": ["Es", "hin\u00b7gen", "an", "den", "schmu\u00b7cken", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Gestiftet rings von frommen H\u00e4nden,", "tokens": ["Ge\u00b7stif\u00b7tet", "rings", "von", "from\u00b7men", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die Weihgeschenke silbern, golden,", "tokens": ["Die", "Weih\u00b7ge\u00b7schen\u00b7ke", "sil\u00b7bern", ",", "gol\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Marien dargebracht, der Holden;", "tokens": ["Ma\u00b7ri\u00b7en", "dar\u00b7ge\u00b7bracht", ",", "der", "Hol\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dem Gl\u00e4ubigen zur Augenweide,", "tokens": ["Dem", "Gl\u00e4u\u00b7bi\u00b7gen", "zur", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dem R\u00e4uber zur geheimen Freude.", "tokens": ["Dem", "R\u00e4u\u00b7ber", "zur", "ge\u00b7hei\u00b7men", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Dahin lenkt sich der Ritterzug,", "tokens": ["Da\u00b7hin", "lenkt", "sich", "der", "Rit\u00b7ter\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "So rasch ging nie der Pferde Flug;", "tokens": ["So", "rasch", "ging", "nie", "der", "Pfer\u00b7de", "Flug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Boden ist so fest und trocken,", "tokens": ["Der", "Bo\u00b7den", "ist", "so", "fest", "und", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die goldnen Sonnenstralen locken,", "tokens": ["Die", "gold\u00b7nen", "Son\u00b7nen\u00b7stra\u00b7len", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und h\u00f6hnend spricht die freche Schar:", "tokens": ["Und", "h\u00f6h\u00b7nend", "spricht", "die", "fre\u00b7che", "Schar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "\u00bbwie ist der Himmel hell und klar,", "tokens": ["\u00bb", "wie", "ist", "der", "Him\u00b7mel", "hell", "und", "klar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Unendlich wolkenlos und blau!", "tokens": ["Un\u00b7end\u00b7lich", "wol\u00b7ken\u00b7los", "und", "blau", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Maria winkt, die sch\u00f6ne Frau,", "tokens": ["Ma\u00b7ria", "winkt", ",", "die", "sch\u00f6\u00b7ne", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Dort aus der Fenster Glanz verstohlen,", "tokens": ["Dort", "aus", "der", "Fens\u00b7ter", "Glanz", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die Gaben ihrer Gunst zu holen.\u00ab", "tokens": ["Die", "Ga\u00b7ben", "ih\u00b7rer", "Gunst", "zu", "ho\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ein einzig Silberw\u00f6lklein helle", "tokens": ["Ein", "ein\u00b7zig", "Sil\u00b7ber\u00b7w\u00f6lk\u00b7lein", "hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Schwebt l\u00e4chelnd \u00fcber der Kapelle;", "tokens": ["Schwebt", "l\u00e4\u00b7chelnd", "\u00fc\u00b7ber", "der", "Ka\u00b7pel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Die Reiter fl\u00fcgeln ihren Lauf;", "tokens": ["Die", "Rei\u00b7ter", "fl\u00fc\u00b7geln", "ih\u00b7ren", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Die kleine Wolke steiget auf \u2013", "tokens": ["Die", "klei\u00b7ne", "Wol\u00b7ke", "stei\u00b7get", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Erst duftig in dem Sonnenlichte,", "tokens": ["Erst", "duf\u00b7tig", "in", "dem", "Son\u00b7nen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Drauf, sich entfaltend, Schicht' auf Schichte:", "tokens": ["Drauf", ",", "sich", "ent\u00b7fal\u00b7tend", ",", "Schicht'", "auf", "Schich\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRF", "VVPP", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Sie traben an, von Glut ermattet,", "tokens": ["Sie", "tra\u00b7ben", "an", ",", "von", "Glut", "er\u00b7mat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Da f\u00fchlen sie sich j\u00e4h beschattet;", "tokens": ["Da", "f\u00fch\u00b7len", "sie", "sich", "j\u00e4h", "be\u00b7schat\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Die Sonn' ist hin, die Wolke grau", "tokens": ["Die", "Sonn'", "ist", "hin", ",", "die", "Wol\u00b7ke", "grau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Lauft \u00fcber in des Himmels Blau. \u2013", "tokens": ["Lauft", "\u00fc\u00b7ber", "in", "des", "Him\u00b7mels", "Blau", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "\u00bbwas ist's? noch schreckt' uns nie ein Regen!", "tokens": ["\u00bb", "was", "ist's", "?", "noch", "schreckt'", "uns", "nie", "ein", "Re\u00b7gen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Dort winkt uns die Kapell' entgegen!\u00ab", "tokens": ["Dort", "winkt", "uns", "die", "Ka\u00b7pell'", "ent\u00b7ge\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Sie sprengen r\u00fcstig an zum Ziele;", "tokens": ["Sie", "spren\u00b7gen", "r\u00fcs\u00b7tig", "an", "zum", "Zie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Herab vom Ro\u00df, hin geht's zur Diele,", "tokens": ["Her\u00b7ab", "vom", "Ro\u00df", ",", "hin", "geht's", "zur", "Die\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Sie treten zu den Hallen ein,", "tokens": ["Sie", "tre\u00b7ten", "zu", "den", "Hal\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Des Silbers winkt, des Goldes Schein,", "tokens": ["Des", "Sil\u00b7bers", "winkt", ",", "des", "Gol\u00b7des", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Die Jungfrau sehn sie in der feuchten", "tokens": ["Die", "Jung\u00b7frau", "sehn", "sie", "in", "der", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Gesteine Glanz entgegenleuchten:", "tokens": ["Ge\u00b7stei\u00b7ne", "Glanz", "ent\u00b7ge\u00b7gen\u00b7leuch\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Ausstrecken sie die freche Hand \u2013", "tokens": ["Aus\u00b7stre\u00b7cken", "sie", "die", "fre\u00b7che", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Da z\u00fcckt es durch die Deckenwand,", "tokens": ["Da", "z\u00fcckt", "es", "durch", "die", "De\u00b7cken\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Ein einz'ger Blitzstral f\u00e4hrt hernieder;", "tokens": ["Ein", "einz'\u00b7ger", "Blitz\u00b7stral", "f\u00e4hrt", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Die gold'gen W\u00e4nde leuchten wieder,", "tokens": ["Die", "gold'\u00b7gen", "W\u00e4n\u00b7de", "leuch\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.59": {"text": "Die ganze Wolke rauscht herein,", "tokens": ["Die", "gan\u00b7ze", "Wol\u00b7ke", "rauscht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Ein Regengu\u00df, durch Wand und Stein;", "tokens": ["Ein", "Re\u00b7gen\u00b7gu\u00df", ",", "durch", "Wand", "und", "Stein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Er schwemmt der wilden R\u00e4uber Leichen,", "tokens": ["Er", "schwemmt", "der", "wil\u00b7den", "R\u00e4u\u00b7ber", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "Begleitet von des Donners Streichen,", "tokens": ["Be\u00b7glei\u00b7tet", "von", "des", "Don\u00b7ners", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.63": {"text": "Fort aus dem Heiligtum mit Macht. \u2013", "tokens": ["Fort", "aus", "dem", "Hei\u00b7lig\u00b7tum", "mit", "Macht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Da leuchtet neu der Sonne Pracht,", "tokens": ["Da", "leuch\u00b7tet", "neu", "der", "Son\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Da lacht das Feld verkl\u00e4rt, erneuet,", "tokens": ["Da", "lacht", "das", "Feld", "ver\u00b7kl\u00e4rt", ",", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Die ganze Sch\u00f6pfung steht erfreuet,", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "steht", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.67": {"te