{"textgrid.poem.60031": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frommes M\u00e4dchen! nimm den Schleyer,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frommes M\u00e4dchen! nimm den Schleyer,", "tokens": ["From\u00b7mes", "M\u00e4d\u00b7chen", "!", "nimm", "den", "Schle\u00b7yer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ein Heiliger dir giebt;", "tokens": ["Den", "ein", "Hei\u00b7li\u00b7ger", "dir", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann h\u00f6re meine Leyer:", "tokens": ["Und", "dann", "h\u00f6\u00b7re", "mei\u00b7ne", "Le\u00b7yer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "M\u00e4dchen hat sie nie getr\u00fcbt.", "tokens": ["M\u00e4d\u00b7chen", "hat", "sie", "nie", "ge\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fromm, wie du, sind ihre Saiten:", "tokens": ["Fromm", ",", "wie", "du", ",", "sind", "ih\u00b7re", "Sai\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ach! was sing' ich dir?", "tokens": ["A\u00b7ber", "ach", "!", "was", "sing'", "ich", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernste Todtenglocken l\u00e4uten;", "tokens": ["Erns\u00b7te", "Tod\u00b7ten\u00b7glo\u00b7cken", "l\u00e4u\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Opferkerzen schimmern hier.", "tokens": ["Op\u00b7fer\u00b7ker\u00b7zen", "schim\u00b7mern", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sing' ich, wie der Engel be\u00dfter", "tokens": ["Sing'", "ich", ",", "wie", "der", "En\u00b7gel", "be\u00df\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Sch\u00f6nheit sich bekennt,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "sich", "be\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dich seufzend seine Schwester,", "tokens": ["Und", "dich", "seuf\u00b7zend", "sei\u00b7ne", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Chor der Engel, nennt?", "tokens": ["In", "dem", "Chor", "der", "En\u00b7gel", ",", "nennt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie am jungfr\u00e4ulichen Bette,", "tokens": ["Wie", "am", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Wo er sorgsam dich bewacht,", "tokens": ["Wo", "er", "sorg\u00b7sam", "dich", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Holder ihm, als Antoinette,", "tokens": ["Hol\u00b7der", "ihm", ",", "als", "An\u00b7to\u00b7i\u00b7net\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Kein geweihtes M\u00e4dchen lacht?", "tokens": ["Kein", "ge\u00b7weih\u00b7tes", "M\u00e4d\u00b7chen", "lacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie, bevor die Morgensonne", "tokens": ["Wie", ",", "be\u00b7vor", "die", "Mor\u00b7gen\u00b7son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter Bergen sich entdeckt,", "tokens": ["Hin\u00b7ter", "Ber\u00b7gen", "sich", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er vertraulich seine Nonne", "tokens": ["Er", "ver\u00b7trau\u00b7lich", "sei\u00b7ne", "Non\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem goldnen Fl\u00fcgel weckt;", "tokens": ["Mit", "dem", "gold\u00b7nen", "Fl\u00fc\u00b7gel", "weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jedes Aemtchen treu verwaltet,", "tokens": ["Je\u00b7des", "A\u00b7emt\u00b7chen", "treu", "ver\u00b7wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Emsig um den Nachttisch irrt,", "tokens": ["Em\u00b7sig", "um", "den", "Nacht\u00b7tisch", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Schleyer selbst entfaltet,", "tokens": ["Und", "den", "Schle\u00b7yer", "selbst", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher dich versch\u00f6nern wird?", "tokens": ["Wel\u00b7cher", "dich", "ver\u00b7sch\u00f6\u00b7nern", "wird", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie die kleinsten Seraphinen,", "tokens": ["Wie", "die", "kleins\u00b7ten", "Se\u00b7ra\u00b7phi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du dich zum Feste schm\u00fcckst,", "tokens": ["Wenn", "du", "dich", "zum", "Fes\u00b7te", "schm\u00fcckst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Wette dich bedienen,", "tokens": ["Um", "die", "Wet\u00b7te", "dich", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du alle sie entz\u00fcckst?", "tokens": ["Und", "du", "al\u00b7le", "sie", "ent\u00b7z\u00fcckst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Oder, wenn in deine Zelle,", "tokens": ["O\u00b7der", ",", "wenn", "in", "dei\u00b7ne", "Zel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey gestorbner Lampe, still,", "tokens": ["Bey", "ge\u00b7storb\u00b7ner", "Lam\u00b7pe", ",", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Geisterchen der H\u00f6lle", "tokens": ["Von", "den", "Geis\u00b7ter\u00b7chen", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Sich der K\u00fchnste wagen will;", "tokens": ["Sich", "der", "K\u00fchns\u00b7te", "wa\u00b7gen", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie er nach dem rothen Kreuze", "tokens": ["Wie", "er", "nach", "dem", "ro\u00b7then", "Kreu\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Schleyers \u00e4ngstlich sieht;", "tokens": ["Dei\u00b7nes", "Schley\u00b7ers", "\u00e4ngst\u00b7lich", "sieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schneller vor dem Reize", "tokens": ["A\u00b7ber", "schnel\u00b7ler", "vor", "dem", "Rei\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines sanften Auges flieht?", "tokens": ["Dei\u00b7nes", "sanf\u00b7ten", "Au\u00b7ges", "flieht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Soll ich singen, wie die Seelen", "tokens": ["Soll", "ich", "sin\u00b7gen", ",", "wie", "die", "See\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Verkl\u00e4rten, unsichtbar,", "tokens": ["Der", "Ver\u00b7kl\u00e4r\u00b7ten", ",", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Himmel dich verm\u00e4hlen,", "tokens": ["Mit", "dem", "Him\u00b7mel", "dich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Am erleuchteten Altar?", "tokens": ["Am", "er\u00b7leuch\u00b7te\u00b7ten", "Al\u00b7tar", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie die jauchzenden Gerechten", "tokens": ["Wie", "die", "jauch\u00b7zen\u00b7den", "Ge\u00b7rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort, im Paradiese, schon", "tokens": ["Dort", ",", "im", "Pa\u00b7ra\u00b7die\u00b7se", ",", "schon"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "APPRART", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mirthen dir zum Kranze flechten,", "tokens": ["Mir\u00b7then", "dir", "zum", "Kran\u00b7ze", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey der Hymne lautem Ton;", "tokens": ["Bey", "der", "Hym\u00b7ne", "lau\u00b7tem", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und, f\u00fcr diese dunkle Zelle,", "tokens": ["Und", ",", "f\u00fcr", "die\u00b7se", "dunk\u00b7le", "Zel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon die j\u00fcngste Himmelsbraut", "tokens": ["Schon", "die", "j\u00fcngs\u00b7te", "Him\u00b7mels\u00b7braut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir, an ewig gr\u00fcner Quelle,", "tokens": ["Dir", ",", "an", "e\u00b7wig", "gr\u00fc\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frische Rosenh\u00fctten baut?", "tokens": ["Fri\u00b7sche", "Ro\u00b7sen\u00b7h\u00fct\u00b7ten", "baut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie du, gl\u00e4nzender und freyer,", "tokens": ["Wie", "du", ",", "gl\u00e4n\u00b7zen\u00b7der", "und", "frey\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJA", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst in Sonnentempeln stehst,", "tokens": ["Einst", "in", "Son\u00b7nen\u00b7tem\u00b7peln", "stehst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Engeln, ohne Schleyer,", "tokens": ["Und", "den", "En\u00b7geln", ",", "oh\u00b7ne", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Freudiger entgegen gehst?", "tokens": ["Freu\u00b7di\u00b7ger", "ent\u00b7ge\u00b7gen", "gehst", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie \u2013 doch nein, geliebtes M\u00e4dchen!", "tokens": ["Wie", "\u2013", "doch", "nein", ",", "ge\u00b7lieb\u00b7tes", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$(", "ADV", "PTKANT", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Lieder bringen nur", "tokens": ["Mei\u00b7ne", "Lie\u00b7der", "brin\u00b7gen", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesem oder jenem St\u00e4dtchen", "tokens": ["Die\u00b7sem", "o\u00b7der", "je\u00b7nem", "St\u00e4dt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "KON", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Befehle der Natur.", "tokens": ["Die", "Be\u00b7feh\u00b7le", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Lerne denn von meiner Leyer,", "tokens": ["Ler\u00b7ne", "denn", "von", "mei\u00b7ner", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df der Liebe L\u00e4cheln nicht", "tokens": ["Da\u00df", "der", "Lie\u00b7be", "L\u00e4\u00b7cheln", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinem Kreuze, deinem Schleyer,", "tokens": ["Dei\u00b7nem", "Kreu\u00b7ze", ",", "dei\u00b7nem", "Schle\u00b7yer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos