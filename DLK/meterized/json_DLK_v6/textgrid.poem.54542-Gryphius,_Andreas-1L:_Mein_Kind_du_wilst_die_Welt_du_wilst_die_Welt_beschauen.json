{"textgrid.poem.54542": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kind du wilst die Welt/ du wilst die Welt beschauen/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind du wilst die Welt/ du wilst die Welt beschauen/", "tokens": ["Mein", "Kind", "du", "wilst", "die", "Welt", "/", "du", "wilst", "die", "Welt", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "ART", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jtzt nun die Wanckelmuth uns und die Welt verkehrt", "tokens": ["Jtzt", "nun", "die", "Wan\u00b7ckel\u00b7muth", "uns", "und", "die", "Welt", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "KON", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sch\u00e4rffer denn vorhin der rauhe Krieg verheert/", "tokens": ["Und", "sch\u00e4rf\u00b7fer", "denn", "vor\u00b7hin", "der", "rau\u00b7he", "Krieg", "ver\u00b7heert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Komm denn und lern allhier nur deinem Christo trauen/", "tokens": ["Komm", "denn", "und", "lern", "all\u00b7hier", "nur", "dei\u00b7nem", "Chris\u00b7to", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der H\u00f6chste gab dich mir/ komm la\u00df dir ja nicht grauen/", "tokens": ["Der", "H\u00f6chs\u00b7te", "gab", "dich", "mir", "/", "komm", "la\u00df", "dir", "ja", "nicht", "grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$(", "VVFIN", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem H\u00f6chsten geb ich dich/ was dich und mich beschwert", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "geb", "ich", "dich", "/", "was", "dich", "und", "mich", "be\u00b7schwert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "$(", "PWS", "PPER", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nimmt seine Treu hinweg/ er pr\u00fcfet auf der Erd", "tokens": ["Nimmt", "sei\u00b7ne", "Treu", "hin\u00b7weg", "/", "er", "pr\u00fc\u00b7fet", "auf", "der", "Erd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APZR", "$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem er zum Vaterland verspricht des Himmels Auen.", "tokens": ["Dem", "er", "zum", "Va\u00b7ter\u00b7land", "ver\u00b7spricht", "des", "Him\u00b7mels", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Geh Kind! bleib ewig Treu den/ dem ich ietzt dich schicke/", "tokens": ["Geh", "Kind", "!", "bleib", "e\u00b7wig", "Treu", "den", "/", "dem", "ich", "ietzt", "dich", "schi\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADJD", "NN", "ART", "$(", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Best\u00e4ndigkeit/ mein Kind/ trotzt Himmel/ Todt/ und Welt/", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/", "mein", "Kind", "/", "trotzt", "Him\u00b7mel", "/", "Todt", "/", "und", "Welt", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "NN", "$(", "NN", "$(", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und steht wenn Tag/ wenn Nacht/ wenn Mond/ und Sonne f\u00e4llt/", "tokens": ["Und", "steht", "wenn", "Tag", "/", "wenn", "Nacht", "/", "wenn", "Mond", "/", "und", "Son\u00b7ne", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "$(", "KOUS", "NN", "$(", "KOUS", "NN", "$(", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Geh Kind! bleib ewig treu/ schau nimmermehr zur\u00fccke/", "tokens": ["Geh", "Kind", "!", "bleib", "e\u00b7wig", "treu", "/", "schau", "nim\u00b7mer\u00b7mehr", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADJD", "ADJD", "$(", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Geh hin! und schwere dem best\u00e4ndig stets zu seyn", "tokens": ["Geh", "hin", "!", "und", "schwe\u00b7re", "dem", "be\u00b7st\u00e4n\u00b7dig", "stets", "zu", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "KON", "VVFIN", "ART", "ADJD", "ADV", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der vor dein Leben litt best\u00e4ndig Creutz und Pein.", "tokens": ["Der", "vor", "dein", "Le\u00b7ben", "litt", "be\u00b7st\u00e4n\u00b7dig", "Creutz", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}