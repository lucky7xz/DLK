{"textgrid.poem.61258": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An den Genius", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Genius, der von ew'gem Herd", "tokens": ["Du", "Ge\u00b7nius", ",", "der", "von", "ew'\u00b7gem", "Herd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mein Wesen all gesetzt in Flammen,", "tokens": ["Mein", "We\u00b7sen", "all", "ge\u00b7setzt", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O halte diesen Leib zusammen,", "tokens": ["O", "hal\u00b7te", "die\u00b7sen", "Leib", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich ein Werk schuf deiner wert;", "tokens": ["Bis", "ich", "ein", "Werk", "schuf", "dei\u00b7ner", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann mag in Erde, Luft und Wellen", "tokens": ["Dann", "mag", "in", "Er\u00b7de", ",", "Luft", "und", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Staub dem Staube sich gesellen,", "tokens": ["Der", "Staub", "dem", "Stau\u00b7be", "sich", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Tropfen, der zum Meere kehrt.", "tokens": ["Ein", "Trop\u00b7fen", ",", "der", "zum", "Mee\u00b7re", "kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du legtest tief in diese Brust", "tokens": ["Du", "leg\u00b7test", "tief", "in", "die\u00b7se", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sehnsucht, Gott und Welt zu schauen,", "tokens": ["Die", "Sehn\u00b7sucht", ",", "Gott", "und", "Welt", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Lied es selig zu vertrauen", "tokens": ["Dem", "Lied", "es", "se\u00b7lig", "zu", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wort und Klang, was mir bewu\u00dft;", "tokens": ["Mit", "Wort", "und", "Klang", ",", "was", "mir", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O la\u00df mich fahren nicht von hinnen,", "tokens": ["O", "la\u00df", "mich", "fah\u00b7ren", "nicht", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis einmal ich mit reinen Sinnen", "tokens": ["Bis", "ein\u00b7mal", "ich", "mit", "rei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gekostet der Erf\u00fcllung Lust.", "tokens": ["Ge\u00b7kos\u00b7tet", "der", "Er\u00b7f\u00fcl\u00b7lung", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir schl\u00e4ft im Herzen noch so viel;", "tokens": ["Mir", "schl\u00e4ft", "im", "Her\u00b7zen", "noch", "so", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Erbarme dich des Ungebornen,", "tokens": ["Er\u00b7bar\u00b7me", "dich", "des", "Un\u00b7ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib Leben, Leben, bis ans Ziel!", "tokens": ["Gib", "Le\u00b7ben", ",", "Le\u00b7ben", ",", "bis", "ans", "Ziel", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich dort unten Ruhe finde,", "tokens": ["Da\u00df", "ich", "dort", "un\u00b7ten", "Ru\u00b7he", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Trostes voll der Kranz sich winde", "tokens": ["Und", "Tros\u00b7tes", "voll", "der", "Kranz", "sich", "win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um mein verstummend Saitenspiel.", "tokens": ["Um", "mein", "ver\u00b7stum\u00b7mend", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}