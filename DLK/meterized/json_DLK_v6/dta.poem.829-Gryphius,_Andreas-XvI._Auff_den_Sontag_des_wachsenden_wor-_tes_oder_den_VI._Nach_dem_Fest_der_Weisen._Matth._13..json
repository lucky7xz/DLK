{"dta.poem.829": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvI.  Auff den Sontag des wachsenden wor-  \n tes/ oder den  VI.  Nach dem Fest der Weisen.  \n Matth. 13.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Kein K\u00f6rnlein ist so klein/ al\u00df Senf vor vn\u00df zu sch\u00e4tzen/ ", "tokens": ["Kein", "K\u00f6rn\u00b7lein", "ist", "so", "klein", "/", "al\u00df", "Senf", "vor", "vn\u00df", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "KOUS", "NE", "APPR", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wenn es in die Scho\u00df der feuchten Erden f\u00e4llt", "tokens": ["Doch", "wenn", "es", "in", "die", "Scho\u00df", "der", "feuch\u00b7ten", "Er\u00b7den", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So wurtzelts eilend eyn/ vnd keimet in die Welt", "tokens": ["So", "wurt\u00b7zelts", "ei\u00b7lend", "eyn", "/", "vnd", "kei\u00b7met", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "VAINF", "$(", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd wird ein hoher Baum/ der rund vmb allen pl\u00e4tzen", "tokens": ["Vnd", "wird", "ein", "ho\u00b7her", "Baum", "/", "der", "rund", "vmb", "al\u00b7len", "pl\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "De\u00df Schattens lust austheilt. Denn eylet sich zu setzen", "tokens": ["De\u00df", "Schat\u00b7tens", "lust", "aust\u00b7heilt", ".", "Denn", "ey\u00b7let", "sich", "zu", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "KON", "VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch Vogel vmb den Ast/ der sich da sicher h\u00e4lt", "tokens": ["Manch", "Vo\u00b7gel", "vmb", "den", "Ast", "/", "der", "sich", "da", "si\u00b7cher", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alsbald der Himmel plitzt al\u00dfbald man nach jhm stelt.", "tokens": ["Als\u00b7bald", "der", "Him\u00b7mel", "plitzt", "al\u00df\u00b7bald", "man", "nach", "jhm", "stelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhn kan kein Wind/ kein Sturm/ kein J\u00e4gergarn verletzen.", "tokens": ["Jhn", "kan", "kein", "Wind", "/", "kein", "Sturm", "/", "kein", "J\u00e4\u00b7ger\u00b7garn", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So scheint de\u00df H\u00f6chsten Wortt in Menschen Augen klein", "tokens": ["So", "scheint", "de\u00df", "H\u00f6chs\u00b7ten", "Wortt", "in", "Men\u00b7schen", "Au\u00b7gen", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch komt\u2019s einmal ins Hertz/ so nimt\u2019s die Sinnen eyn", "tokens": ["Doch", "komt's", "ein\u00b7mal", "ins", "Hertz", "/", "so", "nimt's", "die", "Sin\u00b7nen", "eyn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd l\u00e4\u00dft bald Stock/ vnd Zweig/ vnd Bl\u00fctt\u2019 vnd Fr\u00fcchte", "tokens": ["Vnd", "l\u00e4\u00dft", "bald", "Stock", "/", "vnd", "Zweig", "/", "vnd", "Bl\u00fctt'", "vnd", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "$(", "KON", "NN", "$(", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schawen. ", "tokens": ["scha\u00b7wen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Wer vnter diesem Baum bey tr\u00fcber wetters Zeit", "tokens": ["Wer", "vn\u00b7ter", "die\u00b7sem", "Baum", "bey", "tr\u00fc\u00b7ber", "wet\u00b7ters", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PDAT", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhm zuflucht au\u00dferkiest dem wird vors Windes streitt/", "tokens": ["Jhm", "zu\u00b7flucht", "au\u00b7\u00dfer\u00b7kiest", "dem", "wird", "vors", "Win\u00b7des", "streitt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "VAFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vors Teuffels Vogel Netz/ vor\u2019s Todes Pfeyl nicht grawen.", "tokens": ["Vors", "Teuf\u00b7fels", "Vo\u00b7gel", "Netz", "/", "vor's", "To\u00b7des", "Pfeyl", "nicht", "gra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "NE", "$(", "APPRART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}