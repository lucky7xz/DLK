{"textgrid.poem.59962": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An meinem Gott ich hange,", "genre": "verse", "period": "N.A.", "pub_year": 1653, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An meinem Gott ich hange,", "tokens": ["An", "mei\u00b7nem", "Gott", "ich", "han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn halt' ich, der mich h\u00e4lt.", "tokens": ["Ihn", "halt'", "ich", ",", "der", "mich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach nichts mich sonst verlange!", "tokens": ["Nach", "nichts", "mich", "sonst", "ver\u00b7lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist Alles schn\u00f6de Welt.", "tokens": ["Ist", "Al\u00b7les", "schn\u00f6\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer will, an ihr sich labe,", "tokens": ["Wer", "will", ",", "an", "ihr", "sich", "la\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Such' Lust und finde Leid.", "tokens": ["Such'", "Lust", "und", "fin\u00b7de", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr, wenn ich dich nur habe,", "tokens": ["Herr", ",", "wenn", "ich", "dich", "nur", "ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ach, das ist meine Freud'!", "tokens": ["Ach", ",", "das", "ist", "mei\u00b7ne", "Freud'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Gott lieben ist mein Leben;", "tokens": ["Gott", "lie\u00b7ben", "ist", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihm ich bleibe gr\u00fcn", "tokens": ["An", "ihm", "ich", "blei\u00b7be", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und saftig, gleich der Reben,", "tokens": ["Und", "saf\u00b7tig", ",", "gleich", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin d\u00fcrr und todt ohn' ihn.", "tokens": ["Bin", "d\u00fcrr", "und", "todt", "ohn'", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von ihm kommt alle Gabe,", "tokens": ["Von", "ihm", "kommt", "al\u00b7le", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "So zieret mein Gem\u00fcth.", "tokens": ["So", "zie\u00b7ret", "mein", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr, wenn ich dich nur habe,", "tokens": ["Herr", ",", "wenn", "ich", "dich", "nur", "ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Mein Herz von Tugend bl\u00fcht.", "tokens": ["Mein", "Herz", "von", "Tu\u00b7gend", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es brennet, Gott zu loben,", "tokens": ["Es", "bren\u00b7net", ",", "Gott", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herz in Andachtsglut.", "tokens": ["Mein", "Herz", "in", "An\u00b7dachts\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich suche das, was oben,", "tokens": ["Ich", "su\u00b7che", "das", ",", "was", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das macht mich wohlgemuth.", "tokens": ["Das", "macht", "mich", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dies sei es, was mich labe,", "tokens": ["Dies", "sei", "es", ",", "was", "mich", "la\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn ich dich jede Stund',", "tokens": ["Wenn", "ich", "dich", "je\u00b7de", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr, wenn ich dich nur habe", "tokens": ["Herr", ",", "wenn", "ich", "dich", "nur", "ha\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Im Herzen und im Mund!", "tokens": ["Im", "Her\u00b7zen", "und", "im", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ob mich die Dornen stechen,", "tokens": ["Ob", "mich", "die", "Dor\u00b7nen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Glaub' vor Anker liegt;", "tokens": ["Mein", "Glaub'", "vor", "An\u00b7ker", "liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werd' noch Rosen brechen,", "tokens": ["Ich", "werd'", "noch", "Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Hoffnung gr\u00fcnt und siegt.", "tokens": ["Die", "Hoff\u00b7nung", "gr\u00fcnt", "und", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Elend ich begrabe", "tokens": ["Mein", "E\u00b7lend", "ich", "be\u00b7gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In seinen Vaterscho\u00df.", "tokens": ["In", "sei\u00b7nen", "Va\u00b7ter\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr, wenn ich dich nur habe,", "tokens": ["Herr", ",", "wenn", "ich", "dich", "nur", "ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "So leb' ich sorgenlos.", "tokens": ["So", "leb'", "ich", "sor\u00b7gen\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sollt' auch der Leib veschmachten,", "tokens": ["Sollt'", "auch", "der", "Leib", "vesc\u00b7hmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So will ich himmelw\u00e4rts", "tokens": ["So", "will", "ich", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Aus diesem Kerker trachten.", "tokens": ["Aus", "die\u00b7sem", "Ker\u00b7ker", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es wird ein kurzer Schmerz", "tokens": ["Es", "wird", "ein", "kur\u00b7zer", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich f\u00f6rdern hin zum Grabe", "tokens": ["Mich", "f\u00f6r\u00b7dern", "hin", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und enden alle Noth.", "tokens": ["Und", "en\u00b7den", "al\u00b7le", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr, wenn ich dich nur habe,", "tokens": ["Herr", ",", "wenn", "ich", "dich", "nur", "ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Was frag' ich nach dem Tod?", "tokens": ["Was", "frag'", "ich", "nach", "dem", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So leb' ich, Gott ergeben,", "tokens": ["So", "leb'", "ich", ",", "Gott", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sterb' auf seine Gnad'.", "tokens": ["Und", "sterb'", "auf", "sei\u00b7ne", "Gnad'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Himmel und das Leben", "tokens": ["Den", "Him\u00b7mel", "und", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat ewig, der Gott hat.", "tokens": ["Hat", "e\u00b7wig", ",", "der", "Gott", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den Leib send' ich zu Grabe,", "tokens": ["Den", "Leib", "send'", "ich", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Seel' in seine Hut.", "tokens": ["Die", "Seel'", "in", "sei\u00b7ne", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr, wenn ich dich nur habe,", "tokens": ["Herr", ",", "wenn", "ich", "dich", "nur", "ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "So hab' ich alles Gut.", "tokens": ["So", "hab'", "ich", "al\u00b7les", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}