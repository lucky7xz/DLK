{"textgrid.poem.67049": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00e4rmerei", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freunde! Freunde! wenn er heute k\u00e4me,", "tokens": ["Freun\u00b7de", "!", "Freun\u00b7de", "!", "wenn", "er", "heu\u00b7te", "k\u00e4\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heute mich aus unserm Bunde n\u00e4hme,", "tokens": ["Heu\u00b7te", "mich", "aus", "un\u00b7serm", "Bun\u00b7de", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jener letzte gro\u00dfe Augenblick \u2013", "tokens": ["Je\u00b7ner", "letz\u00b7te", "gro\u00b7\u00dfe", "Au\u00b7gen\u00b7blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wann der frohe Puls so pl\u00f6tzlich st\u00fcnde", "tokens": ["Wann", "der", "fro\u00b7he", "Puls", "so", "pl\u00f6tz\u00b7lich", "st\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "ADJD", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und verworren Freundesstimme t\u00f6nte", "tokens": ["Und", "ver\u00b7wor\u00b7ren", "Freun\u00b7dess\u00b7tim\u00b7me", "t\u00f6n\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und, ein Nebel, mich umschwebte Erdengl\u00fcck.", "tokens": ["Und", ",", "ein", "Ne\u00b7bel", ",", "mich", "um\u00b7schweb\u00b7te", "Er\u00b7den\u00b7gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Ha! so pl\u00f6tzlich Lebewohl zu sagen", "tokens": ["Ha", "!", "so", "pl\u00f6tz\u00b7lich", "Le\u00b7be\u00b7wohl", "zu", "sa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ADJD", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All den lieben sch\u00f6ndurchlebten Tagen \u2013", "tokens": ["All", "den", "lie\u00b7ben", "sch\u00f6n\u00b7durch\u00b7leb\u00b7ten", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch \u2013 ich glaube \u2013 nein! ich bebte nicht!", "tokens": ["Doch", "\u2013", "ich", "glau\u00b7be", "\u2013", "nein", "!", "ich", "beb\u00b7te", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VVFIN", "$(", "PTKANT", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbfreunde! spr\u00e4ch ich, dort auf jenen H\u00f6hen", "tokens": ["\u00bb", "freun\u00b7de", "!", "spr\u00e4ch", "ich", ",", "dort", "auf", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "$.", "VVFIN", "PPER", "$,", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Werden wir uns alle wiedersehen,", "tokens": ["Wer\u00b7den", "wir", "uns", "al\u00b7le", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Freunde! wo ein sch\u00f6nrer Tag die Wolken bricht.", "tokens": ["Freun\u00b7de", "!", "wo", "ein", "sch\u00f6n\u00b7rer", "Tag", "die", "Wol\u00b7ken", "bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Aber Stella! fern ist deine H\u00fctte,", "tokens": ["A\u00b7ber", "Stel\u00b7la", "!", "fern", "ist", "dei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nahe rauschen schon des W\u00fcrgers Tritte \u2013", "tokens": ["Na\u00b7he", "rau\u00b7schen", "schon", "des", "W\u00fcr\u00b7gers", "Trit\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stella! meine Stella! weine nicht!", "tokens": ["Stel\u00b7la", "!", "mei\u00b7ne", "Stel\u00b7la", "!", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nur noch einmal m\u00f6cht ich sie umarmen,", "tokens": ["Nur", "noch", "ein\u00b7mal", "m\u00f6cht", "ich", "sie", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sterben dann in meiner Stella Armen,", "tokens": ["Ster\u00b7ben", "dann", "in", "mei\u00b7ner", "Stel\u00b7la", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eile, Stella! eile, eh das Auge bricht.", "tokens": ["Ei\u00b7le", ",", "Stel\u00b7la", "!", "ei\u00b7le", ",", "eh", "das", "Au\u00b7ge", "bricht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$.", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Aber ferne, ferne deine H\u00fctte,", "tokens": ["A\u00b7ber", "fer\u00b7ne", ",", "fer\u00b7ne", "dei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nahe rauschen schon des W\u00fcrgers Tritte \u2013", "tokens": ["Na\u00b7he", "rau\u00b7schen", "schon", "des", "W\u00fcr\u00b7gers", "Trit\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Freunde! bringet meine Lieder ihr.", "tokens": ["Freun\u00b7de", "!", "brin\u00b7get", "mei\u00b7ne", "Lie\u00b7der", "ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lieber Gott! ein gro\u00dfer Mann zu werden,", "tokens": ["Lie\u00b7ber", "Gott", "!", "ein", "gro\u00b7\u00dfer", "Mann", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "War so oft mein Wunsch, mein Traum auf Erden,", "tokens": ["War", "so", "oft", "mein", "Wunsch", ",", "mein", "Traum", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aber \u2013 Br\u00fcder \u2013 gr\u00f6\u00dfre Rollen winken mir.", "tokens": ["A\u00b7ber", "\u2013", "Br\u00fc\u00b7der", "\u2013", "gr\u00f6\u00df\u00b7re", "Rol\u00b7len", "win\u00b7ken", "mir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$(", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Traurt ihr, Br\u00fcder! da\u00df so weggeschwunden", "tokens": ["Traurt", "ihr", ",", "Br\u00fc\u00b7der", "!", "da\u00df", "so", "weg\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "KOUS", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All der Zukunft sch\u00f6ngetr\u00e4umte Stunden,", "tokens": ["All", "der", "Zu\u00b7kunft", "sch\u00f6n\u00b7ge\u00b7tr\u00e4um\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Alle, alle meine Hoffnungen!", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "mei\u00b7ne", "Hoff\u00b7nun\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da\u00df die Erde meinen Leichnam decket,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "mei\u00b7nen", "Leich\u00b7nam", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Eh ich mir ein Denkmal aufgestecket,", "tokens": ["Eh", "ich", "mir", "ein", "Denk\u00b7mal", "auf\u00b7ge\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und der Enkel nimmer denkt des Schlummernden.", "tokens": ["Und", "der", "En\u00b7kel", "nim\u00b7mer", "denkt", "des", "Schlum\u00b7mern\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Da\u00df er kalt an meinem Leichensteine", "tokens": ["Da\u00df", "er", "kalt", "an", "mei\u00b7nem", "Lei\u00b7chen\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stehet, und des Modernden Gebeine", "tokens": ["Ste\u00b7het", ",", "und", "des", "Mo\u00b7dern\u00b7den", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Keines J\u00fcnglings stiller Segen gr\u00fc\u00dft,", "tokens": ["Kei\u00b7nes", "J\u00fcng\u00b7lings", "stil\u00b7ler", "Se\u00b7gen", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df auf meines Grabes Rosenhecken", "tokens": ["Da\u00df", "auf", "mei\u00b7nes", "Gra\u00b7bes", "Ro\u00b7sen\u00b7he\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf den Lilien, die den Moder decken,", "tokens": ["Auf", "den", "Li\u00b7li\u00b7en", ",", "die", "den", "Mo\u00b7der", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Keines M\u00e4dchens herzergo\u00dfne Tr\u00e4ne flie\u00dft.", "tokens": ["Kei\u00b7nes", "M\u00e4d\u00b7chens", "her\u00b7zer\u00b7go\u00df\u00b7ne", "Tr\u00e4\u00b7ne", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Da\u00df von M\u00e4nnern, die vor\u00fcberwallen,", "tokens": ["Da\u00df", "von", "M\u00e4n\u00b7nern", ",", "die", "vor\u00b7\u00fc\u00b7berw\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht die Worte in die Gruft erschallen:", "tokens": ["Nicht", "die", "Wor\u00b7te", "in", "die", "Gruft", "er\u00b7schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "J\u00fcngling! du entschlummertest zu fr\u00fch!", "tokens": ["J\u00fcng\u00b7ling", "!", "du", "ent\u00b7schlum\u00b7mer\u00b7test", "zu", "fr\u00fch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df den Kleinen keine Silbergreise", "tokens": ["Da\u00df", "den", "Klei\u00b7nen", "kei\u00b7ne", "Sil\u00b7berg\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sagen an dem Ziel der Lebensreise:", "tokens": ["Sa\u00b7gen", "an", "dem", "Ziel", "der", "Le\u00b7bens\u00b7rei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kinder! mein und jenes Grab vergesset nie!", "tokens": ["Kin\u00b7der", "!", "mein", "und", "je\u00b7nes", "Grab", "ver\u00b7ges\u00b7set", "nie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "KON", "PDAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Da\u00df sie mir so grausam weggeschwunden,", "tokens": ["Da\u00df", "sie", "mir", "so", "grau\u00b7sam", "weg\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "All der Zukunft langersehnte Stunden,", "tokens": ["All", "der", "Zu\u00b7kunft", "lan\u00b7ger\u00b7sehn\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "All der frohen Hoffnung Seligkeit,", "tokens": ["All", "der", "fro\u00b7hen", "Hoff\u00b7nung", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df die sch\u00f6nste Tr\u00e4ume dieser Erden", "tokens": ["Da\u00df", "die", "sch\u00f6ns\u00b7te", "Tr\u00e4u\u00b7me", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hin sind, ewig niemals wahr zu werden,", "tokens": ["Hin", "sind", ",", "e\u00b7wig", "nie\u00b7mals", "wahr", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJD", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hin die Tr\u00e4ume von Unsterblichkeit.", "tokens": ["Hin", "die", "Tr\u00e4u\u00b7me", "von", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Aber weg! in diesem toten Herzen", "tokens": ["A\u00b7ber", "weg", "!", "in", "die\u00b7sem", "to\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KO