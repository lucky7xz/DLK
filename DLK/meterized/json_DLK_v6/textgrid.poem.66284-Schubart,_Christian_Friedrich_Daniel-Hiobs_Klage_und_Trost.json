{"textgrid.poem.66284": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Hiobs Klage und Trost", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer legt den Jammer meiner Tage,", "tokens": ["Wer", "legt", "den", "Jam\u00b7mer", "mei\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer meine Leiden, meine Qual,", "tokens": ["Wer", "mei\u00b7ne", "Lei\u00b7den", ",", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer leget sie auf eine Wage", "tokens": ["Wer", "le\u00b7get", "sie", "auf", "ei\u00b7ne", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wiegt die Felsenlast einmal?", "tokens": ["Und", "wiegt", "die", "Fel\u00b7sen\u00b7last", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwer ist sie, wie Gebirge schwer,", "tokens": ["Schwer", "ist", "sie", ",", "wie", "Ge\u00b7bir\u00b7ge", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zahlreich, wie der Sand am Meer.", "tokens": ["Und", "zahl\u00b7reich", ",", "wie", "der", "Sand", "am", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn Gottes Pfeile in mir stecken,", "tokens": ["Wenn", "Got\u00b7tes", "Pfei\u00b7le", "in", "mir", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn ihr Z\u00fcrnen aus mir s\u00e4uft;", "tokens": ["Und", "wenn", "ihr", "Z\u00fcr\u00b7nen", "aus", "mir", "s\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn, wie ein Krieger, mir ein Schrecken", "tokens": ["Wenn", ",", "wie", "ein", "Krie\u00b7ger", ",", "mir", "ein", "Schre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "$,", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Allmacht nach der Seele greift:", "tokens": ["Der", "All\u00b7macht", "nach", "der", "See\u00b7le", "greift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann lieg' ich, unter meiner Noth,", "tokens": ["Dann", "lieg'", "ich", ",", "un\u00b7ter", "mei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gekr\u00fcmmt, und rufe laut dem Tod.", "tokens": ["Ge\u00b7kr\u00fcmmt", ",", "und", "ru\u00b7fe", "laut", "dem", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was bist du Leben auf der Erden?", "tokens": ["Was", "bist", "du", "Le\u00b7ben", "auf", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Streit, ein Leben voll Verdru\u00df,", "tokens": ["Ein", "Streit", ",", "ein", "Le\u00b7ben", "voll", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man den t\u00e4glichen Beschwerden,", "tokens": ["Wo", "man", "den", "t\u00e4g\u00b7li\u00b7chen", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gleich einem Sklaven, fr\u00f6hnen mu\u00df;", "tokens": ["Gleich", "ei\u00b7nem", "Skla\u00b7ven", ",", "fr\u00f6h\u00b7nen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein langer Seufzer nach der Ruh',", "tokens": ["Ein", "lan\u00b7ger", "Seuf\u00b7zer", "nach", "der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach k\u00fchlen Schatten, das bist du!", "tokens": ["Nach", "k\u00fch\u00b7len", "Schat\u00b7ten", ",", "das", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel N\u00e4chte hab' ich durchgeweinet,", "tokens": ["Viel", "N\u00e4ch\u00b7te", "hab'", "ich", "durch\u00b7ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie auf Dornen zugebracht;", "tokens": ["Und", "wie", "auf", "Dor\u00b7nen", "zu\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schrei' ich, wenn der Tag erscheinet:", "tokens": ["Doch", "schrei'", "ich", ",", "wenn", "der", "Tag", "er\u00b7schei\u00b7net", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann kommt die Nacht, wann kommt die Nacht?", "tokens": ["Wann", "kommt", "die", "Nacht", ",", "wann", "kommt", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Finsterni\u00df, wann deckest du", "tokens": ["O", "Fins\u00b7ter\u00b7ni\u00df", ",", "wann", "de\u00b7ckest", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mich Scheusal vor der Sonne zu?", "tokens": ["Mich", "Scheu\u00b7sal", "vor", "der", "Son\u00b7ne", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo ist ein Balsam, mich zu heilen?", "tokens": ["Wo", "ist", "ein", "Bal\u00b7sam", ",", "mich", "zu", "hei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist der Arzt, dem es nicht graut?", "tokens": ["Wo", "ist", "der", "Arzt", ",", "dem", "es", "nicht", "graut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein K\u00f6rper starrt von Eiterbeulen,", "tokens": ["Mein", "K\u00f6r\u00b7per", "starrt", "von", "Ei\u00b7ter\u00b7beu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und W\u00fcrmer nisten in der Haut.", "tokens": ["Und", "W\u00fcr\u00b7mer", "nis\u00b7ten", "in", "der", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott, was umd\u00e4mmst du mich so sehr,", "tokens": ["Gott", ",", "was", "um\u00b7d\u00e4mmst", "du", "mich", "so", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie einen Walfisch, wie ein Meer?", "tokens": ["Wie", "ei\u00b7nen", "Wal\u00b7fisch", ",", "wie", "ein", "Meer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Komm, Tod, du Ende meiner Qualen,", "tokens": ["Komm", ",", "Tod", ",", "du", "En\u00b7de", "mei\u00b7ner", "Qua\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sanfter Schlummer nach der Noth;", "tokens": ["Du", "sanf\u00b7ter", "Schlum\u00b7mer", "nach", "der", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seufze schon zu tausendmalen", "tokens": ["Ich", "seuf\u00b7ze", "schon", "zu", "tau\u00b7send\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dir! Wann kommst du doch, o Tod?", "tokens": ["Nach", "dir", "!", "Wann", "kommst", "du", "doch", ",", "o", "Tod", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PWAV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann k\u00fchlest du im stillen Grab", "tokens": ["Wann", "k\u00fch\u00b7lest", "du", "im", "stil\u00b7len", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den hei\u00dfen Schmerz mit Erde ab?", "tokens": ["Den", "hei\u00b7\u00dfen", "Schmerz", "mit", "Er\u00b7de", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch, welch ein Trost, der, wie die Sonne", "tokens": ["Doch", ",", "welch", "ein", "Trost", ",", "der", ",", "wie", "die", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAT", "ART", "NN", "$,", "PRELS", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Donnerwolken, sich erhebt!", "tokens": ["Aus", "Don\u00b7ner\u00b7wol\u00b7ken", ",", "sich", "er\u00b7hebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df \u2013 Gedanke voller Wonne!", "tokens": ["Ich", "wei\u00df", "\u2013", "Ge\u00b7dan\u00b7ke", "vol\u00b7ler", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df, da\u00df mein Erl\u00f6ser lebt.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df \u2013 mein starker Glaube spricht:", "tokens": ["Ich", "wei\u00df", "\u2013", "mein", "star\u00b7ker", "Glau\u00b7be", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er lebt! Er lebt! ich zittre nicht.", "tokens": ["Er", "lebt", "!", "Er", "lebt", "!", "ich", "zitt\u00b7re", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Entflieht, der H\u00f6ll' entflohne Schrecken,", "tokens": ["Ent\u00b7flieht", ",", "der", "H\u00f6ll'", "ent\u00b7floh\u00b7ne", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil Gottes Trost in mir erwacht;", "tokens": ["Weil", "Got\u00b7tes", "Trost", "in", "mir", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird mich Armen auferwecken", "tokens": ["Er", "wird", "mich", "Ar\u00b7men", "au\u00b7fer\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meines Grabes Mitternacht.", "tokens": ["Aus", "mei\u00b7nes", "Gra\u00b7bes", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wird kein Eiter, keine Pein", "tokens": ["Dann", "wird", "kein", "Ei\u00b7ter", ",", "kei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Henker meines K\u00f6rpers sein.", "tokens": ["Ein", "Hen\u00b7ker", "mei\u00b7nes", "K\u00f6r\u00b7pers", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit diesen meinen Augen sehen,", "tokens": ["Mit", "die\u00b7sen", "mei\u00b7nen", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hier mein Elend roth geweint.", "tokens": ["Die", "hier", "mein", "E\u00b7lend", "roth", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O welch ein hoher Trost f\u00fcr mich!", "tokens": ["O", "welch", "ein", "ho\u00b7her", "Trost", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich soll den Mittler schauen, ich!", "tokens": ["Ich", "soll", "den", "Mitt\u00b7ler", "schau\u00b7en", ",", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "La\u00dft den Gedanken voll Vertrauen,", "tokens": ["La\u00dft", "den", "Ge\u00b7dan\u00b7ken", "voll", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lindrung in der Todespein,", "tokens": ["Die", "Lind\u00b7rung", "in", "der", "To\u00b7de\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft ihn in einen Felsen hauen,", "tokens": ["La\u00dft", "ihn", "in", "ei\u00b7nen", "Fel\u00b7sen", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Grabt ihn in Erz und Marmor ein,", "tokens": ["Grabt", "ihn", "in", "Erz", "und", "Mar\u00b7mor", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Trost, der meinen Geist erhebt:", "tokens": ["Den", "Trost", ",", "der", "mei\u00b7nen", "Geist", "er\u00b7hebt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df, da\u00df mein Erl\u00f6ser lebt!", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}