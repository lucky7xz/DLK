{"textgrid.poem.45319": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[du ahmst ein hohes Muster nach]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ahmst ein hohes Muster nach,", "tokens": ["Du", "ahmst", "ein", "ho\u00b7hes", "Mus\u00b7ter", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist die \u00c4hnlichkeit nur schwach,", "tokens": ["Doch", "ist", "die", "\u00c4hn\u00b7lich\u00b7keit", "nur", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Bombe fehlt das rechte Brandrohr,", "tokens": ["Der", "Bom\u00b7be", "fehlt", "das", "rech\u00b7te", "Brand\u00b7rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und du bist h\u00f6chstens ein Graf Schand-ohr.", "tokens": ["Und", "du", "bist", "h\u00f6chs\u00b7tens", "ein", "Graf", "Schan\u00b7dohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}