{"dta.poem.14795": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er gr\u00fc\u00dft' sie so f\u00fcrchterlich heiter,", "tokens": ["Er", "gr\u00fc\u00dft'", "sie", "so", "f\u00fcrch\u00b7ter\u00b7lich", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Er hei\u00dft sie sein' liebliche Braut,", "tokens": ["Er", "hei\u00dft", "sie", "sein'", "lieb\u00b7li\u00b7che", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da kannt' sie mit Schaudern den Reiter,", "tokens": ["Da", "kannt'", "sie", "mit", "Schau\u00b7dern", "den", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "F\u00e4llt nieder auf ihre Knie.", "tokens": ["F\u00e4llt", "nie\u00b7der", "auf", "ih\u00b7re", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}