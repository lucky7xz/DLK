{"textgrid.poem.37211": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Des Seemanns Zechlied", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00e4hre mir Bacchus", "tokens": ["Ge\u00b7w\u00e4h\u00b7re", "mir", "Bac\u00b7chus"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Preisw\u00fcrdig und hehr", "tokens": ["Preis\u00b7w\u00fcr\u00b7dig", "und", "hehr"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Des Weines nicht Tonnen,", "tokens": ["Des", "Wei\u00b7nes", "nicht", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein v\u00f6lliges Meer!", "tokens": ["Ein", "v\u00f6l\u00b7li\u00b7ges", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Im Weinozeane", "tokens": ["Im", "Wei\u00b7no\u00b7zea\u00b7ne"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bemannen ein Schiff", "tokens": ["Be\u00b7man\u00b7nen", "ein", "Schiff"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Mir Br\u00fcder, wie Lust hier", "tokens": ["Mir", "Br\u00fc\u00b7der", ",", "wie", "Lust", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "NN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zusammen sie rief.", "tokens": ["Zu\u00b7sam\u00b7men", "sie", "rief", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Gewinnet an Klippen", "tokens": ["Ge\u00b7win\u00b7net", "an", "Klip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das Schiff auch den Leck,", "tokens": ["Das", "Schiff", "auch", "den", "Leck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nie ruft uns zur Arbeit", "tokens": ["Nie", "ruft", "uns", "zur", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Pump aufs Verdeck.", "tokens": ["Die", "Pump", "aufs", "Ver\u00b7deck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wir schwimmen, wir sinken,", "tokens": ["Wir", "schwim\u00b7men", ",", "wir", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Es bleibet dabei", "tokens": ["Es", "blei\u00b7bet", "da\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df nicht Element nur", "tokens": ["Da\u00df", "nicht", "E\u00b7le\u00b7ment", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das Wa\u00dfer uns sei.", "tokens": ["Das", "Wa\u00b7\u00dfer", "uns", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und droht auch Verderben,", "tokens": ["Und", "droht", "auch", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So hat es nicht Noth:", "tokens": ["So", "hat", "es", "nicht", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir finden im Weine", "tokens": ["Wir", "fin\u00b7den", "im", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den trunkenen Tod.", "tokens": ["Den", "trun\u00b7ke\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es treibe die Woge,", "tokens": ["Es", "trei\u00b7be", "die", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wohin ihrs gef\u00e4llt,", "tokens": ["Wo\u00b7hin", "ihrs", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Lebendig den Leichnam", "tokens": ["Le\u00b7ben\u00b7dig", "den", "Leich\u00b7nam"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und todt um die Welt!", "tokens": ["Und", "todt", "um", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}