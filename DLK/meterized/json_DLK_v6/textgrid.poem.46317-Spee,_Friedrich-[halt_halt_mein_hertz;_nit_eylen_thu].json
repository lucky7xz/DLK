{"textgrid.poem.46317": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[halt halt mein hertz; nit eylen thu]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt halt mein hertz; nit eylen thu", "tokens": ["Halt", "halt", "mein", "hertz", ";", "nit", "ey\u00b7len", "thu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "PTKNEG", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach lust, vnd frewd auff Erden;", "tokens": ["Nach", "lust", ",", "vnd", "frewd", "auff", "Er\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirst nie darin doch finden ruh;", "tokens": ["Wirst", "nie", "da\u00b7rin", "doch", "fin\u00b7den", "ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan dir nicht alles werden.", "tokens": ["Kan", "dir", "nicht", "al\u00b7les", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIS", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df ab, la\u00df ab, ist nur vmbsonst,", "tokens": ["La\u00df", "ab", ",", "la\u00df", "ab", ",", "ist", "nur", "vmbsonst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "La\u00df ab darnach zu trachten:", "tokens": ["La\u00df", "ab", "dar\u00b7nach", "zu", "trach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich fande nie kein be\u00dfer kunst,", "tokens": ["Ich", "fan\u00b7de", "nie", "kein", "be\u00b7\u00dfer", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dan alles schlecht verachten.", "tokens": ["Dan", "al\u00b7les", "schlecht", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drumb du den pracht: so gar nit acht,", "tokens": ["Drumb", "du", "den", "pracht", ":", "so", "gar", "nit", "acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "$.", "ADV", "ADV", "PTKNEG", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dich zur welt nit binden:", "tokens": ["La\u00df", "dich", "zur", "welt", "nit", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach kurtzer weill: sie wird in eil", "tokens": ["Nach", "kurt\u00b7zer", "weill", ":", "sie", "wird", "in", "eil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie rauch im lufft verschwinden.", "tokens": ["Wie", "rauch", "im", "lufft", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gleich wie der lufft: vnd \u00f6der tufft", "tokens": ["Gleich", "wie", "der", "lufft", ":", "vnd", "\u00f6\u00b7der", "tufft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$.", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird alles bald zerfahren.", "tokens": ["Wird", "al\u00b7les", "bald", "zer\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Glaub mir zu mahl: die pfeil ohn zahl", "tokens": ["Glaub", "mir", "zu", "mahl", ":", "die", "pfeil", "ohn", "zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADV", "$.", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tod nit pflegt zu sparen.", "tokens": ["Der", "Tod", "nit", "pflegt", "zu", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als wie die sch\u00f6n gez\u00fcndte kertz", "tokens": ["Als", "wie", "die", "sch\u00f6n", "ge\u00b7z\u00fcnd\u00b7te", "kertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich selbest mu\u00df verzehren,", "tokens": ["Sich", "sel\u00b7best", "mu\u00df", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil au\u00df ihr selbst das brinnend hertz", "tokens": ["Weil", "au\u00df", "ihr", "selbst", "das", "brin\u00b7nend", "hertz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbest mu\u00df ernehren:", "tokens": ["Sich", "sel\u00b7best", "mu\u00df", "er\u00b7neh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Also verzehrt sich alles gleich,", "tokens": ["Al\u00b7so", "ver\u00b7zehrt", "sich", "al\u00b7les", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Auff diser welt so schwinde;", "tokens": ["Auff", "di\u00b7ser", "welt", "so", "schwin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da fleu\u00dft es her in einem streich,", "tokens": ["Da", "fleu\u00dft", "es", "her", "in", "ei\u00b7nem", "streich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es steht die kertz im winde.", "tokens": ["Es", "steht", "die", "kertz", "im", "win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sag dir mein kind: wer wollust find,", "tokens": ["Sag", "dir", "mein", "kind", ":", "wer", "wol\u00b7lust", "find", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$.", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Find zil, vnd ma\u00df darneben,", "tokens": ["Find", "zil", ",", "vnd", "ma\u00df", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im himmel dort: geht alles fort,", "tokens": ["Im", "him\u00b7mel", "dort", ":", "geht", "al\u00b7les", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df alles allweg leben:", "tokens": ["Mu\u00df", "al\u00b7les", "all\u00b7weg", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bey solchem spiel: ist nie kein zil,", "tokens": ["Bey", "sol\u00b7chem", "spiel", ":", "ist", "nie", "kein", "zil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird ewig, ewig wehren:", "tokens": ["Wird", "e\u00b7wig", ",", "e\u00b7wig", "weh\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wan ichs gedenck: f\u00fcr lieb erkrenck,", "tokens": ["Wan", "ichs", "ge\u00b7denck", ":", "f\u00fcr", "lieb", "er\u00b7krenck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$.", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Werd na\u00df von haissen z\u00e4hren.", "tokens": ["Werd", "na\u00df", "von", "hais\u00b7sen", "z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jerusalem du sch\u00f6ne statt,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "du", "sch\u00f6\u00b7ne", "statt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wan ich nur dein gedencke,", "tokens": ["Wan", "ich", "nur", "dein", "ge\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin diser welt so sauber satt,", "tokens": ["Bin", "di\u00b7ser", "welt", "so", "sau\u00b7ber", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein zu dir mich lencke:", "tokens": ["Al\u00b7lein", "zu", "dir", "mich", "len\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach dir allein wird mir so wee,", "tokens": ["Nach", "dir", "al\u00b7lein", "wird", "mir", "so", "wee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Frag nichts nach eitel sachen;", "tokens": ["Frag", "nichts", "nach", "ei\u00b7tel", "sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wei\u00df warlich nit, wan ichs versteh,", "tokens": ["Wei\u00df", "war\u00b7lich", "nit", ",", "wan", "ichs", "ver\u00b7steh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was hie soll weiter machen", "tokens": ["Was", "hie", "soll", "wei\u00b7ter", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sein eigne frewd: mir Gott anbeut,", "tokens": ["Sein", "eig\u00b7ne", "frewd", ":", "mir", "Gott", "an\u00b7beut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ewigkeit sols dauren;", "tokens": ["In", "e\u00b7wig\u00b7keit", "sols", "dau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All was vns helt: bey diser welt", "tokens": ["All", "was", "vns", "helt", ":", "bey", "di\u00b7ser", "welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PWS", "PPER", "VVFIN", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt endlich ewig trawren:", "tokens": ["Bringt", "end\u00b7lich", "e\u00b7wig", "traw\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ey was mag dan: vns kleben an,", "tokens": ["Ey", "was", "mag", "dan", ":", "vns", "kle\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "VMFIN", "ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "O welt, bey deiner th\u00fcren?", "tokens": ["O", "welt", ",", "bey", "dei\u00b7ner", "th\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein bin ich m\u00fcd: dich Gott beh\u00fct,", "tokens": ["Dein", "bin", "ich", "m\u00fcd", ":", "dich", "Gott", "be\u00b7h\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADJD", "$.", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Solt mich nit mehr verf\u00fchren.", "tokens": ["Solt", "mich", "nit", "mehr", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}