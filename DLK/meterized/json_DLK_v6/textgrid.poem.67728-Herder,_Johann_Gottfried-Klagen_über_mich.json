{"textgrid.poem.67728": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Klagen \u00fcber mich", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh mir! Ich suche Menschenfreunde! weh!", "tokens": ["Weh", "mir", "!", "Ich", "su\u00b7che", "Men\u00b7schen\u00b7freun\u00b7de", "!", "weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "ADJA", "NN", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo lauter Larven stehn.", "tokens": ["Wo", "lau\u00b7ter", "Lar\u00b7ven", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier, wo ich um mich seh',", "tokens": ["Hier", ",", "wo", "ich", "um", "mich", "seh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Seh' ich wirksame Schatten gehn.", "tokens": ["Seh'", "ich", "wirk\u00b7sa\u00b7me", "Schat\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "O Zaubernacht, wo Alles scheint, nichts ist,", "tokens": ["O", "Zau\u00b7ber\u00b7nacht", ",", "wo", "Al\u00b7les", "scheint", ",", "nichts", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel leuchtet und nichts brennt! \u2013", "tokens": ["Viel", "leuch\u00b7tet", "und", "nichts", "brennt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "KON", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Hand, von Schattenlist", "tokens": ["Du", "Hand", ",", "von", "Schat\u00b7ten\u00b7list"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Get\u00e4uscht, wenn findst Du? \u2013 Ach, wenn kennt", "tokens": ["Ge\u00b7t\u00e4uscht", ",", "wenn", "findst", "Du", "?", "\u2013", "Ach", ",", "wenn", "kennt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "VVFIN", "PPER", "$.", "$(", "ITJ", "$,", "KOUS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Aug', das B\u00e4ume Menschen gleich anstarrt,", "tokens": ["Mein", "Aug'", ",", "das", "B\u00e4u\u00b7me", "Men\u00b7schen", "gleich", "an\u00b7starrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn lernt's denn Fernen, Schein", "tokens": ["Wenn", "lernt's", "denn", "Fer\u00b7nen", ",", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "NE", "KON", "ADJA", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Knaben, blendend zart! \u2013", "tokens": ["Und", "Kna\u00b7ben", ",", "blen\u00b7dend", "zart", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O, werd' ich bald Maschine sein,", "tokens": ["O", ",", "werd'", "ich", "bald", "Ma\u00b7schi\u00b7ne", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir selbst nichts, Andern nur gesellig Thier?", "tokens": ["Mir", "selbst", "nichts", ",", "An\u00b7dern", "nur", "ge\u00b7sel\u00b7lig", "Thier", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "$,", "ADJA", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn wei\u00df ich's, da\u00df das Herz", "tokens": ["Wenn", "wei\u00df", "ich's", ",", "da\u00df", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des J\u00fcnglings dort und hier", "tokens": ["Des", "J\u00fcng\u00b7lings", "dort", "und", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als besten Freund umarmt den \u2013 Scherz!", "tokens": ["Als", "bes\u00b7ten", "Freund", "um\u00b7armt", "den", "\u2013", "Scherz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "ART", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}