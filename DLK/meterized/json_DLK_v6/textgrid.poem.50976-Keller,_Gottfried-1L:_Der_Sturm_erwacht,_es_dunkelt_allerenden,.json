{"textgrid.poem.50976": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sturm erwacht, es dunkelt allerenden,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sturm erwacht, es dunkelt allerenden,", "tokens": ["Der", "Sturm", "er\u00b7wacht", ",", "es", "dun\u00b7kelt", "al\u00b7le\u00b7ren\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt eben, hinter jenen Wolkenw\u00e4nden,", "tokens": ["Jetzt", "e\u00b7ben", ",", "hin\u00b7ter", "je\u00b7nen", "Wol\u00b7ken\u00b7w\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort mu\u00df die Sonne untergehn;", "tokens": ["Dort", "mu\u00df", "die", "Son\u00b7ne", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort ist es abendklar und goldenhelle", "tokens": ["Dort", "ist", "es", "a\u00b7bend\u00b7klar", "und", "gol\u00b7den\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sind nun Lilie, Rosenhag und Quelle", "tokens": ["Und", "sind", "nun", "Li\u00b7lie", ",", "Ro\u00b7sen\u00b7hag", "und", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Hier aber ist ein kaltes Wehn und Brausen,", "tokens": ["Hier", "a\u00b7ber", "ist", "ein", "kal\u00b7tes", "Wehn", "und", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dunkler Luft die hohen W\u00e4lder sausen,", "tokens": ["In", "dunk\u00b7ler", "Luft", "die", "ho\u00b7hen", "W\u00e4l\u00b7der", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die B\u00e4che toben durchs Gestein;", "tokens": ["Die", "B\u00e4\u00b7che", "to\u00b7ben", "durchs", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Windes Peitsche f\u00fchlt die Heide streichen,", "tokens": ["Des", "Win\u00b7des", "Peit\u00b7sche", "f\u00fchlt", "die", "Hei\u00b7de", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Asketisch beugen sich die ernsten Eichen,", "tokens": ["As\u00b7ke\u00b7tisch", "beu\u00b7gen", "sich", "die", "erns\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Nacht wankt finster in das Land herein.", "tokens": ["Die", "Nacht", "wankt", "fins\u00b7ter", "in", "das", "Land", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sp\u00e4he kaum den Grund zu meinen F\u00fc\u00dfen,", "tokens": ["Ich", "sp\u00e4\u00b7he", "kaum", "den", "Grund", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch h\u00f6r ich rings die Regenstr\u00f6me gie\u00dfen,", "tokens": ["Doch", "h\u00f6r", "ich", "rings", "die", "Re\u00b7gen\u00b7str\u00f6\u00b7me", "gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es weint das schwarz verh\u00fcllte Land;", "tokens": ["Es", "weint", "das", "schwarz", "ver\u00b7h\u00fcll\u00b7te", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Herzen hallt die Klage wider,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "hallt", "die", "Kla\u00b7ge", "wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und es ergreift mich, wirft mich jach darnieder,", "tokens": ["Und", "es", "er\u00b7greift", "mich", ",", "wirft", "mich", "jach", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und meine Stirne pre\u00dft sich in den Sand.", "tokens": ["Und", "mei\u00b7ne", "Stir\u00b7ne", "pre\u00dft", "sich", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O reiner Schmerz, der von den H\u00f6hn gewittert,", "tokens": ["O", "rei\u00b7ner", "Schmerz", ",", "der", "von", "den", "H\u00f6hn", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du heil'ges Weh, das durch die Tiefen zittert,", "tokens": ["Du", "heil'\u00b7ges", "Weh", ",", "das", "durch", "die", "Tie\u00b7fen", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr schlie\u00dft auch mir die Augen auf!", "tokens": ["Ihr", "schlie\u00dft", "auch", "mir", "die", "Au\u00b7gen", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr habt zu mir das Zauberwort gesprochen", "tokens": ["Ihr", "habt", "zu", "mir", "das", "Zau\u00b7ber\u00b7wort", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und meinen Hochmut wie ein Rohr gebrochen,", "tokens": ["Und", "mei\u00b7nen", "Hoch\u00b7mut", "wie", "ein", "Rohr", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ungehemmt flie\u00dft meiner Tr\u00e4nen Lauf!", "tokens": ["Und", "un\u00b7ge\u00b7hemmt", "flie\u00dft", "mei\u00b7ner", "Tr\u00e4\u00b7nen", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du s\u00fc\u00dfes Leid, hast ganz mich \u00fcberwunden!", "tokens": ["Du", "s\u00fc\u00b7\u00dfes", "Leid", ",", "hast", "ganz", "mich", "\u00fc\u00b7berw\u00b7un\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch dunkle Lust, die ich noch nie empfunden,", "tokens": ["Welch", "dunk\u00b7le", "Lust", ",", "die", "ich", "noch", "nie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist mit der Demut angefacht!", "tokens": ["Ist", "mit", "der", "De\u00b7mut", "an\u00b7ge\u00b7facht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie reich bist, Mutter Erde! du zu nennen:", "tokens": ["Wie", "reich", "bist", ",", "Mut\u00b7ter", "Er\u00b7de", "!", "du", "zu", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "NN", "NN", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich glaubte deine Herrlichkeit zu kennen,", "tokens": ["Ich", "glaub\u00b7te", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nun schau ich erst in deiner Tiefe Schacht!", "tokens": ["Nun", "schau", "ich", "erst", "in", "dei\u00b7ner", "Tie\u00b7fe", "Schacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und leise schallen h\u00f6r ich ferne Tritte,", "tokens": ["Und", "lei\u00b7se", "schal\u00b7len", "h\u00f6r", "ich", "fer\u00b7ne", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es naht sich mir mit leicht beschwingtem Schritte", "tokens": ["Es", "naht", "sich", "mir", "mit", "leicht", "be\u00b7schwing\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch die geheim erhellte Nacht;", "tokens": ["Durch", "die", "ge\u00b7heim", "er\u00b7hell\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df, wie entstiegen einem Marmorgrabe,", "tokens": ["Wei\u00df", ",", "wie", "ent\u00b7stie\u00b7gen", "ei\u00b7nem", "Mar\u00b7mor\u00b7gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wandelt her ein sch\u00f6ner schlanker Knabe,", "tokens": ["So", "wan\u00b7delt", "her", "ein", "sch\u00f6\u00b7ner", "schlan\u00b7ker", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Einsamer Bergmann in dem lichten Schacht.", "tokens": ["Ein\u00b7sa\u00b7mer", "Berg\u00b7mann", "in", "dem", "lich\u00b7ten", "Schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Willkommen, Tod! dir will ich mich vertrauen,", "tokens": ["Will\u00b7kom\u00b7men", ",", "Tod", "!", "dir", "will", "ich", "mich", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df mich in deine treuen Augen schauen", "tokens": ["La\u00df", "mich", "in", "dei\u00b7ne", "treu\u00b7en", "Au\u00b7gen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum ersten Male fest und klar!", "tokens": ["Zum", "ers\u00b7ten", "Ma\u00b7le", "fest", "und", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wenn man einen neuen Freund gefunden,", "tokens": ["Wie", "wenn", "man", "ei\u00b7nen", "neu\u00b7en", "Freund", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kaum noch von der Verlassenheit umwunden,", "tokens": ["Kaum", "noch", "von", "der", "Ver\u00b7las\u00b7sen\u00b7heit", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wird mein Herz der Qual und Sorge bar.", "tokens": ["So", "wird", "mein", "Herz", "der", "Qual", "und", "Sor\u00b7ge", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Tief schau ich dir ins Aug, das sternenklare.", "tokens": ["Tief", "schau", "ich", "dir", "ins", "Aug", ",", "das", "ster\u00b7nen\u00b7kla\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie stehn dir gut die schwarzgelockten Haare,", "tokens": ["Wie", "stehn", "dir", "gut", "die", "schwarz\u00b7ge\u00b7lock\u00b7ten", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sanft ist deine k\u00fchle Hand!", "tokens": ["Wie", "sanft", "ist", "dei\u00b7ne", "k\u00fch\u00b7le", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O lege sie in meine warmen H\u00e4nde,", "tokens": ["O", "le\u00b7ge", "sie", "in", "mei\u00b7ne", "war\u00b7men", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein heil'ges Antlitz zu mir nieder wende!", "tokens": ["Dein", "heil'\u00b7ges", "Ant\u00b7litz", "zu", "mir", "nie\u00b7der", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wohl mir, da\u00df ich dies traute Wissen fand!", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "dies", "trau\u00b7te", "Wis\u00b7sen", "fand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ob mir auch noch begl\u00fcckte Stunden schlagen,", "tokens": ["Ob", "mir", "auch", "noch", "be\u00b7gl\u00fcck\u00b7te", "Stun\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will dich heimlich tief im Herzen tragen,", "tokens": ["Ich", "will", "dich", "heim\u00b7lich", "tief", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo mich einst dein Ruf ereilt:", "tokens": ["Und", "wo", "mich", "einst", "dein", "Ruf", "er\u00b7eilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Bl\u00fctenfeld, im festlich bunten Saale,", "tokens": ["Im", "Bl\u00fc\u00b7ten\u00b7feld", ",", "im", "fest\u00b7lich", "bun\u00b7ten", "Saa\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf d\u00fcrft'gem Bett, im schlachterf\u00fcllten Tale,", "tokens": ["Auf", "d\u00fcrft'\u00b7gem", "Bett", ",", "im", "schlach\u00b7ter\u00b7f\u00fcll\u00b7ten", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich folge dir getrost und unverweilt. \u2013", "tokens": ["Ich", "fol\u00b7ge", "dir", "ge\u00b7trost", "und", "un\u00b7ver\u00b7weilt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Nacht vergeht, die grauen Wolken fliegen,", "tokens": ["Die", "Nacht", "ver\u00b7geht", ",", "die", "grau\u00b7en", "Wol\u00b7ken", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag erwacht und seine Strahlen siegen,", "tokens": ["Der", "Tag", "er\u00b7wacht", "und", "sei\u00b7ne", "Strah\u00b7len", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Osten steigt der Sonnenschild empor,", "tokens": ["Im", "Os\u00b7ten", "steigt", "der", "Son\u00b7nen\u00b7schild", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es blitzt sein Schein auf meinen alten Wegen;", "tokens": ["Es", "blitzt", "sein", "Schein", "auf", "mei\u00b7nen", "al\u00b7ten", "We\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein andrer aber tret ich ihm entgegen,", "tokens": ["Ein", "an\u00b7drer", "a\u00b7ber", "tret", "ich", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der ich die Furcht des Todes still verlor.", "tokens": ["Der", "ich", "die", "Furcht", "des", "To\u00b7des", "still", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}