{"dta.poem.20423": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der reinen lieblichkeit/ so unser blut durchstreichet/", "tokens": ["Der", "rei\u00b7nen", "lieb\u00b7lich\u00b7keit", "/", "so", "un\u00b7ser", "blut", "durch\u00b7strei\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergleichet sich der tranck der g\u00f6tter selber nicht;", "tokens": ["Ver\u00b7glei\u00b7chet", "sich", "der", "tranck", "der", "g\u00f6t\u00b7ter", "sel\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die rosen/ derer glantz kein purpur hat erreichet/", "tokens": ["Die", "ro\u00b7sen", "/", "de\u00b7rer", "glantz", "kein", "pur\u00b7pur", "hat", "er\u00b7rei\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "NN", "PIAT", "ADJD", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind als ein meisterst\u00fcck im himmel zugericht;", "tokens": ["Sind", "als", "ein", "meis\u00b7ter\u00b7st\u00fcck", "im", "him\u00b7mel", "zu\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der wunder starcke safft/ der s\u00fcsse thau der seelen/", "tokens": ["Der", "wun\u00b7der", "star\u00b7cke", "safft", "/", "der", "s\u00fcs\u00b7se", "thau", "der", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$(", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So um rubinen fleu\u00dft/ und hier auff perlen steht/", "tokens": ["So", "um", "ru\u00b7bi\u00b7nen", "fleu\u00dft", "/", "und", "hier", "auff", "per\u00b7len", "steht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$(", "KON", "ADV", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gibt deutlich zu verstehn/ da\u00df in der augen h\u00f6len/", "tokens": ["Gibt", "deut\u00b7lich", "zu", "ver\u00b7stehn", "/", "da\u00df", "in", "der", "au\u00b7gen", "h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKZU", "VVINF", "$(", "KOUS", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die reitzung \u00f6ffters schl\u00e4fft/ hier niemahls untergeht.", "tokens": ["Die", "reit\u00b7zung", "\u00f6ff\u00b7ters", "schl\u00e4fft", "/", "hier", "nie\u00b7mahls", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$(", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}