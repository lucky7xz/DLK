{"textgrid.poem.39249": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Lieb, die Welt ist kalt und kahl,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieb, die Welt ist kalt und kahl,", "tokens": ["Mein", "Lieb", ",", "die", "Welt", "ist", "kalt", "und", "kahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leute tr\u00e4g' und tr\u00fcbe,", "tokens": ["Die", "Leu\u00b7te", "tr\u00e4g'", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist, beim Himmel, nicht einmal", "tokens": ["Es", "ist", ",", "beim", "Him\u00b7mel", ",", "nicht", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel Spa\u00df mehr bei der Liebe.", "tokens": ["Viel", "Spa\u00df", "mehr", "bei", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir schmachten uns von weitem an", "tokens": ["Wir", "schmach\u00b7ten", "uns", "von", "wei\u00b7tem", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fcssen uns in der N\u00e4he", "tokens": ["Und", "k\u00fcs\u00b7sen", "uns", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und fahren auseinander dann,", "tokens": ["Und", "fah\u00b7ren", "aus\u00b7ein\u00b7an\u00b7der", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df keiner es ersp\u00e4he.", "tokens": ["Da\u00df", "kei\u00b7ner", "es", "er\u00b7sp\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn Wolken zwischen uns sich ziehn,", "tokens": ["Wenn", "Wol\u00b7ken", "zwi\u00b7schen", "uns", "sich", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00f6rst Du auf, mich zu gr\u00fc\u00dfen,", "tokens": ["So", "h\u00f6rst", "Du", "auf", ",", "mich", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ist Dein Zorn recht weit gediehn,", "tokens": ["Und", "ist", "Dein", "Zorn", "recht", "weit", "ge\u00b7diehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fall' ich Dir zu F\u00fc\u00dfen.", "tokens": ["So", "fall'", "ich", "Dir", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir lieben uns, wie es nutzt und frommt,", "tokens": ["Wir", "lie\u00b7ben", "uns", ",", "wie", "es", "nutzt", "und", "frommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Damit die Welt nicht richte,", "tokens": ["Da\u00b7mit", "die", "Welt", "nicht", "rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn die Sache aufs H\u00f6chste kommt,", "tokens": ["Und", "wenn", "die", "Sa\u00b7che", "aufs", "H\u00f6chs\u00b7te", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So mach' ich schlechte Gedichte.", "tokens": ["So", "mach'", "ich", "schlech\u00b7te", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir f\u00fchren uns beim Spazierengehn", "tokens": ["Wir", "f\u00fch\u00b7ren", "uns", "beim", "Spa\u00b7zie\u00b7ren\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lieben uns unendlich", "tokens": ["Und", "lie\u00b7ben", "uns", "un\u00b7end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manchmal, nun, Du mu\u00dft's gestehn,", "tokens": ["Und", "manch\u00b7mal", ",", "nun", ",", "Du", "mu\u00dft's", "ge\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Langweilen wir uns ganz sch\u00e4ndlich! \u2013", "tokens": ["Lang\u00b7wei\u00b7len", "wir", "uns", "ganz", "sch\u00e4nd\u00b7lich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u2013 Wie war die Zeit doch bl\u00fchender,", "tokens": ["\u2013", "Wie", "war", "die", "Zeit", "doch", "bl\u00fc\u00b7hen\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit der Helme und Koller!", "tokens": ["Die", "Zeit", "der", "Hel\u00b7me", "und", "Kol\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da waren die Weiber noch gl\u00fchender,", "tokens": ["Da", "wa\u00b7ren", "die", "Wei\u00b7ber", "noch", "gl\u00fc\u00b7hen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die M\u00e4nner frischer und toller.", "tokens": ["Die", "M\u00e4n\u00b7ner", "fri\u00b7scher", "und", "tol\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "KON", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Da war die Liebe noch Heiligtum,", "tokens": ["Da", "war", "die", "Lie\u00b7be", "noch", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Schwert noch sch\u00e4rfer und spitzer,", "tokens": ["Das", "Schwert", "noch", "sch\u00e4r\u00b7fer", "und", "spit\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da waren die Frauen der Helden Ruhm,", "tokens": ["Da", "wa\u00b7ren", "die", "Frau\u00b7en", "der", "Hel\u00b7den", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Helden Frauenbesch\u00fctzer. \u2013", "tokens": ["Die", "Hel\u00b7den", "Frau\u00b7en\u00b7be\u00b7sch\u00fct\u00b7zer", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u2013 Es streckt in die Nacht, in die Mondscheinnacht", "tokens": ["\u2013", "Es", "streckt", "in", "die", "Nacht", ",", "in", "die", "Mond\u00b7schein\u00b7nacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Turm sein Haupt, das starre,", "tokens": ["Der", "Turm", "sein", "Haupt", ",", "das", "star\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch die Mondscheinnacht t\u00f6nt liebentfacht", "tokens": ["Durch", "die", "Mond\u00b7schein\u00b7nacht", "t\u00f6nt", "lie\u00b7bent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das klagende Lied der Guitarre.", "tokens": ["Das", "kla\u00b7gen\u00b7de", "Lied", "der", "Gui\u00b7tar\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Die Laute schmachtet, die Laute fleht,", "tokens": ["Die", "Lau\u00b7te", "schmach\u00b7tet", ",", "die", "Lau\u00b7te", "fleht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mond wird heller und heller,", "tokens": ["Der", "Mond", "wird", "hel\u00b7ler", "und", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Fr\u00e4ulein auf dem S\u00f6ller steht,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "auf", "dem", "S\u00f6l\u00b7ler", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Junker unter dem S\u00f6ller.", "tokens": ["Der", "Jun\u00b7ker", "un\u00b7ter", "dem", "S\u00f6l\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Den Turm umklettert ein Rosenstrauch", "tokens": ["Den", "Turm", "um\u00b7klet\u00b7tert", "ein", "Ro\u00b7sen\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Ranken schweifend und lose.", "tokens": ["Mit", "Ran\u00b7ken", "schwei\u00b7fend", "und", "lo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wei\u00df nicht, fiel sie vom Windeshauch,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "fiel", "sie", "vom", "Win\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch nieder fiel eine Rose. \u2013", "tokens": ["Doch", "nie\u00b7der", "fiel", "ei\u00b7ne", "Ro\u00b7se", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "\u2013 Auf tausend Helmen die Sonne blitzt,", "tokens": ["\u2013", "Auf", "tau\u00b7send", "Hel\u00b7men", "die", "Son\u00b7ne", "blitzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "CARD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es flattert die Scharlachfahne,", "tokens": ["Es", "flat\u00b7tert", "die", "Schar\u00b7lach\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er auf dem b\u00e4umenden Schimmel sitzt,", "tokens": ["Er", "auf", "dem", "b\u00e4u\u00b7men\u00b7den", "Schim\u00b7mel", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sitzt auf hohem Altane.", "tokens": ["Sie", "sitzt", "auf", "ho\u00b7hem", "Al\u00b7ta\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Den Speer gesenkt, die Z\u00fcgel verh\u00e4ngt,", "tokens": ["Den", "Speer", "ge\u00b7senkt", ",", "die", "Z\u00fc\u00b7gel", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Haupt auf die Faust gebogen,", "tokens": ["Das", "Haupt", "auf", "die", "Faust", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So kommt er durch die Schranken gesprengt,", "tokens": ["So", "kommt", "er", "durch", "die", "Schran\u00b7ken", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Federn nicken und wogen.", "tokens": ["Die", "Fe\u00b7dern", "ni\u00b7cken", "und", "wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Sie faltet die H\u00e4nde im Todesschreck:", "tokens": ["Sie", "fal\u00b7tet", "die", "H\u00e4n\u00b7de", "im", "To\u00b7des\u00b7schreck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbgott sei dem Liebsten gn\u00e4dig!\u00ab", "tokens": ["\u00bb", "gott", "sei", "dem", "Liebs\u00b7ten", "gn\u00e4\u00b7dig", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Liebster wiegt sich im Sattel keck,", "tokens": ["Ihr", "Liebs\u00b7ter", "wiegt", "sich", "im", "Sat\u00b7tel", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Gegners Hengst ist ledig.", "tokens": ["Des", "Geg\u00b7ners", "Hengst", "ist", "le\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Durch die Nacht, durch die mondlos finst're Nacht", "tokens": ["Durch", "die", "Nacht", ",", "durch", "die", "mond\u00b7los", "finst'\u00b7re", "Nacht"], 