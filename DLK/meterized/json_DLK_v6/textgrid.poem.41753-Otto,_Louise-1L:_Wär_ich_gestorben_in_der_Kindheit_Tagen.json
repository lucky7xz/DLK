{"textgrid.poem.41753": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r ich gestorben in der Kindheit Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich gestorben in der Kindheit Tagen", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "in", "der", "Kind\u00b7heit", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ahnungsvoll mein erstes Lied ich sang,", "tokens": ["Als", "ah\u00b7nungs\u00b7voll", "mein", "ers\u00b7tes", "Lied", "ich", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df im Marsellaisenwirbel-Schlagen", "tokens": ["In\u00b7de\u00df", "im", "Mar\u00b7sel\u00b7laisen\u00b7wir\u00b7bel\u00b7Schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Freiheitsjauchzen meines Volkes klang,", "tokens": ["Das", "Frei\u00b7hei\u00b7ts\u00b7jauch\u00b7zen", "mei\u00b7nes", "Vol\u00b7kes", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Wo ich versteckt in meiner stillen Zelle", "tokens": ["Wo", "ich", "ver\u00b7steckt", "in", "mei\u00b7ner", "stil\u00b7len", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Begeistrungsvoll den Sieg des Fortschritts pries,", "tokens": ["Be\u00b7geis\u00b7trungs\u00b7voll", "den", "Sieg", "des", "Fort\u00b7schritts", "pries", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wo der Neuzeit morgenrote Helle", "tokens": ["Und", "wo", "der", "Neu\u00b7zeit", "mor\u00b7gen\u00b7ro\u00b7te", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein tr\u00e4umrisch Kind zur S\u00e4ngrin werden lie\u00df.", "tokens": ["Ein", "tr\u00e4um\u00b7risch", "Kind", "zur", "S\u00e4n\u00b7grin", "wer\u00b7den", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00e4r ich gestorben, da mich der umfangen", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", ",", "da", "mich", "der", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Der mir der Liebe G\u00f6tterkraft gelehrt,", "tokens": ["Der", "mir", "der", "Lie\u00b7be", "G\u00f6t\u00b7ter\u00b7kraft", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beim ersten Ku\u00df auf meine bleichen Wangen", "tokens": ["Beim", "ers\u00b7ten", "Ku\u00df", "auf", "mei\u00b7ne", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beim ersten Liebeswort, das ich geh\u00f6rt \u2013", "tokens": ["Beim", "ers\u00b7ten", "Lie\u00b7bes\u00b7wort", ",", "das", "ich", "ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da schwebten alle Himmel zu mir nieder,", "tokens": ["Da", "schweb\u00b7ten", "al\u00b7le", "Him\u00b7mel", "zu", "mir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da l\u00e4chelten mir alle Engel zu!", "tokens": ["Da", "l\u00e4\u00b7chel\u00b7ten", "mir", "al\u00b7le", "En\u00b7gel", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In seinem Herzen fand ich meines wieder", "tokens": ["In", "sei\u00b7nem", "Her\u00b7zen", "fand", "ich", "mei\u00b7nes", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In seinem Arm allein der Sel'gen Ruh.", "tokens": ["In", "sei\u00b7nem", "Arm", "al\u00b7lein", "der", "Sel'\u00b7gen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "W\u00e4r ich gestorben als mit freien Liedern", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "als", "mit", "frei\u00b7en", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich einst begr\u00fc\u00dft ein deutscher S\u00e4ngerchor,", "tokens": ["Mich", "einst", "be\u00b7gr\u00fc\u00dft", "ein", "deut\u00b7scher", "S\u00e4n\u00b7ge\u00b7rchor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ihre Stimmen mir sich zu verbr\u00fcdern", "tokens": ["Wo", "ih\u00b7re", "Stim\u00b7men", "mir", "sich", "zu", "ver\u00b7br\u00fc\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch n\u00e4cht'ge Stille schallten laut empor;", "tokens": ["Durch", "n\u00e4cht'\u00b7ge", "Stil\u00b7le", "schall\u00b7ten", "laut", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich es fr\u00f6hlich durfte nun erkennen:", "tokens": ["Da\u00df", "ich", "es", "fr\u00f6h\u00b7lich", "durf\u00b7te", "nun", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ich gestrebt mit redlich frommen Sinn,", "tokens": ["Was", "ich", "ge\u00b7strebt", "mit", "red\u00b7lich", "from\u00b7men", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was ich gethan mich Deutschlands wert zu nennen", "tokens": ["Was", "ich", "ge\u00b7than", "mich", "Deutschlands", "wert", "zu", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "PPER", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die deutsche Jugend nahm es fr\u00f6hlich hin!", "tokens": ["Die", "deut\u00b7sche", "Ju\u00b7gend", "nahm", "es", "fr\u00f6h\u00b7lich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "W\u00e4r ich gestorben in der T\u00f6ne Wettern", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "in", "der", "T\u00f6\u00b7ne", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beim Freudenchor der neunten Symphonie,", "tokens": ["Beim", "Freu\u00b7denc\u00b7hor", "der", "neun\u00b7ten", "Sym\u00b7pho\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Menschen werden zu lebend'gen G\u00f6ttern", "tokens": ["Wo", "Men\u00b7schen", "wer\u00b7den", "zu", "le\u00b7ben\u00b7d'\u00b7gen", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In dem Titannensturm der Poesie;", "tokens": ["In", "dem", "Ti\u00b7tan\u00b7nen\u00b7sturm", "der", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wo Flammenblicke in das Herz mir gl\u00fchten", "tokens": ["Wo", "Flam\u00b7men\u00b7bli\u00b7cke", "in", "das", "Herz", "mir", "gl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu gleicher jubelnder Begeisterung!", "tokens": ["Zu", "glei\u00b7cher", "ju\u00b7beln\u00b7der", "Be\u00b7geis\u00b7te\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo neue Paradiese mich umbl\u00fchten", "tokens": ["Wo", "neu\u00b7e", "Pa\u00b7ra\u00b7die\u00b7se", "mich", "um\u00b7bl\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und in den offnen Himmel war ein Sprung \u2013", "tokens": ["Und", "in", "den", "off\u00b7nen", "Him\u00b7mel", "war", "ein", "Sprung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "W\u00e4r ich gestorben als Du mich, Poete", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "als", "Du", "mich", ",", "Poe\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVPP", "KOKOM", "PPER", "PRF", "$,", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von Gottes Gnaden, Schwester hast genannt,", "tokens": ["Von", "Got\u00b7tes", "Gna\u00b7den", ",", "Schwes\u00b7ter", "hast", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des klagend Lied und dessen freie Rede", "tokens": ["Des", "kla\u00b7gend", "Lied", "und", "des\u00b7sen", "frei\u00b7e", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meinem Herzen lautes Echo fand,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "lau\u00b7tes", "E\u00b7cho", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und als Du selber lauschtest meinem Sange", "tokens": ["Und", "als", "Du", "sel\u00b7ber", "lauschtest", "mei\u00b7nem", "San\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie einer liebgewordnen Molodie,", "tokens": ["Wie", "ei\u00b7ner", "lieb\u00b7ge\u00b7word\u00b7nen", "Mo\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So lauscht der Strom auf seinem weiten Gange", "tokens": ["So", "lauscht", "der", "Strom", "auf", "sei\u00b7nem", "wei\u00b7ten", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der nahen Quelle und dem Strom lauscht sie.", "tokens": ["Der", "na\u00b7hen", "Quel\u00b7le", "und", "dem", "Strom", "lauscht", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "W\u00e4r ich gestorben \u2013 doch es ist vergebens \u2013", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "\u2013", "doch", "es", "ist", "ver\u00b7ge\u00b7bens", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$(", "KON", "PPER", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht in den Stunden reiner Seligkeit,", "tokens": ["Nicht", "in", "den", "Stun\u00b7den", "rei\u00b7ner", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht in der F\u00fclle eines k\u00fchnen Strebens", "tokens": ["Nicht", "in", "der", "F\u00fcl\u00b7le", "ei\u00b7nes", "k\u00fch\u00b7nen", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Naht uns der Tod und findet uns bereit!", "tokens": ["Naht", "uns", "der", "Tod", "und", "fin\u00b7det", "uns", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erst mu\u00df vorbei die stolze Stunde rennen", "tokens": ["Erst", "mu\u00df", "vor\u00b7bei", "die", "stol\u00b7ze", "Stun\u00b7de", "ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In der wir zweifellos uns selbst geglaubt,", "tokens": ["In", "der", "wir", "zwei\u00b7fel\u00b7los", "uns", "selbst", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Erst mu\u00df die heil'ge Flamme niederbrennen,", "tokens": ["Erst", "mu\u00df", "die", "heil'\u00b7ge", "Flam\u00b7me", "nie\u00b7der\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Kranz verdorren der uns frisch umlaubt!", "tokens": ["Der", "Kranz", "ver\u00b7dor\u00b7ren", "der", "uns", "frisch", "um\u00b7laubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Erst m\u00fcssen wir auf Gr\u00e4bern wandeln lernen", "tokens": ["Erst", "m\u00fcs\u00b7sen", "wir", "auf", "Gr\u00e4\u00b7bern", "wan\u00b7deln", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unser Herz mu\u00df werden s