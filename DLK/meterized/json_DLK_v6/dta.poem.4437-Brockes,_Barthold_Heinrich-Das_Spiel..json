{"dta.poem.4437": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Spiel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mi\u00dfbrauchet man der Lust; so wird sie gleich zu Gallen.", "tokens": ["Mi\u00df\u00b7brau\u00b7chet", "man", "der", "Lust", ";", "so", "wird", "sie", "gleich", "zu", "Gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein wenig spielen schadet nicht;", "tokens": ["Ein", "we\u00b7nig", "spie\u00b7len", "scha\u00b7det", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mu\u00df das Spiel nur blo\u00df zum Zeit-Vertreib gefallen.", "tokens": ["Doch", "mu\u00df", "das", "Spiel", "nur", "blo\u00df", "zum", "Zeit\u00b7Ver\u00b7treib", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Spieler hat, nach aller Welt Bericht,", "tokens": ["Ein", "Spie\u00b7ler", "hat", ",", "nach", "al\u00b7ler", "Welt", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gar nichts, was menschlich ist, als nur den blossen Schein.", "tokens": ["Gar", "nichts", ",", "was", "menschlich", "ist", ",", "als", "nur", "den", "blos\u00b7sen", "Schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Recht ehrlich, und ein Spieler seyn,", "tokens": ["Recht", "ehr\u00b7lich", ",", "und", "ein", "Spie\u00b7ler", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KON", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist schwerer, als man glaubet.", "tokens": ["Ist", "schwe\u00b7rer", ",", "als", "man", "glau\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die heisse Sucht, nur immer zu gewinnen,", "tokens": ["Die", "heis\u00b7se", "Sucht", ",", "nur", "im\u00b7mer", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die ihn des Tages plag\u2019t, des Nachts der Ruh beraubet,", "tokens": ["Die", "ihn", "des", "Ta\u00b7ges", "plag't", ",", "des", "Nachts", "der", "Ruh", "be\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist ihm ein steter Sporn und Stachel seiner Sinnen.", "tokens": ["Ist", "ihm", "ein", "ste\u00b7ter", "Sporn", "und", "Sta\u00b7chel", "sei\u00b7ner", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und w\u00e4r\u2019 er von Natur der Redlichkeit gewogen;", "tokens": ["Und", "w\u00e4r'", "er", "von", "Na\u00b7tur", "der", "Red\u00b7lich\u00b7keit", "ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So ist doch die\u00df der allgemeine Lauf:", "tokens": ["So", "ist", "doch", "die\u00df", "der", "all\u00b7ge\u00b7mei\u00b7ne", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Man f\u00e4nget an, und wird betrogen,", "tokens": ["Man", "f\u00e4n\u00b7get", "an", ",", "und", "wird", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und h\u00f6r\u2019t als ein Betrieger auf.", "tokens": ["Und", "h\u00f6r't", "als", "ein", "Be\u00b7trie\u00b7ger", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}