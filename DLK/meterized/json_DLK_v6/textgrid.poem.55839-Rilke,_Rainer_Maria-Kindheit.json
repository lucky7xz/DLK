{"textgrid.poem.55839": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da rinnt der Schule lange Angst und Zeit", "tokens": ["Da", "rinnt", "der", "Schu\u00b7le", "lan\u00b7ge", "Angst", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit Warten hin, mit lauter dumpfen Dingen.", "tokens": ["mit", "War\u00b7ten", "hin", ",", "mit", "lau\u00b7ter", "dum\u00b7pfen", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Einsamkeit, o schweres Zeitverbringen...", "tokens": ["O", "Ein\u00b7sam\u00b7keit", ",", "o", "schwe\u00b7res", "Zeit\u00b7ver\u00b7brin\u00b7gen", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dann hinaus: die Stra\u00dfen spr\u00fchn und klingen", "tokens": ["Und", "dann", "hin\u00b7aus", ":", "die", "Stra\u00b7\u00dfen", "spr\u00fchn", "und", "klin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "$.", "ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und auf den Pl\u00e4tzen die Font\u00e4nen springen", "tokens": ["und", "auf", "den", "Pl\u00e4t\u00b7zen", "die", "Fon\u00b7t\u00e4\u00b7nen", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und in den G\u00e4rten wird die Welt so weit \u2013.", "tokens": ["und", "in", "den", "G\u00e4r\u00b7ten", "wird", "die", "Welt", "so", "weit", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und durch das alles gehn im kleinen Kleid,", "tokens": ["Und", "durch", "das", "al\u00b7les", "gehn", "im", "klei\u00b7nen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ganz anders als die andern gehn und gingen \u2013:", "tokens": ["ganz", "an\u00b7ders", "als", "die", "an\u00b7dern", "gehn", "und", "gin\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "ADJA", "VVINF", "KON", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O wunderliche Zeit, o Zeitverbringen,", "tokens": ["O", "wun\u00b7der\u00b7li\u00b7che", "Zeit", ",", "o", "Zeit\u00b7ver\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "o Einsamkeit.", "tokens": ["o", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und in das alles fern hinauszuschauen:", "tokens": ["Und", "in", "das", "al\u00b7les", "fern", "hin\u00b7aus\u00b7zu\u00b7schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00e4nner und Frauen; M\u00e4nner, M\u00e4nner, Frauen", "tokens": ["M\u00e4n\u00b7ner", "und", "Frau\u00b7en", ";", "M\u00e4n\u00b7ner", ",", "M\u00e4n\u00b7ner", ",", "Frau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$.", "NN", "$,", "NN", "$,", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "und Kinder, welche anders sind und bunt;", "tokens": ["und", "Kin\u00b7der", ",", "wel\u00b7che", "an\u00b7ders", "sind", "und", "bunt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und da ein Haus und dann und wann ein Hund", "tokens": ["und", "da", "ein", "Haus", "und", "dann", "und", "wann", "ein", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "KON", "ADV", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und Schrecken lautlos wechselnd mit Vertrauen \u2013:", "tokens": ["und", "Schre\u00b7cken", "laut\u00b7los", "wech\u00b7selnd", "mit", "Ver\u00b7trau\u00b7en", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "ADJD", "ADJD", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O Trauer ohne Sinn, o Traum, o Grauen,", "tokens": ["O", "Trau\u00b7er", "oh\u00b7ne", "Sinn", ",", "o", "Traum", ",", "o", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,", "FM", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "o Tiefe ohne Grund.", "tokens": ["o", "Tie\u00b7fe", "oh\u00b7ne", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und so zu spielen: Ball und Ring und Reifen", "tokens": ["Und", "so", "zu", "spie\u00b7len", ":", "Ball", "und", "Ring", "und", "Rei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$.", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in einem Garten, welcher sanft verbla\u00dft,", "tokens": ["in", "ei\u00b7nem", "Gar\u00b7ten", ",", "wel\u00b7cher", "sanft", "ver\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und manchmal die Erwachsenen zu streifen,", "tokens": ["und", "manch\u00b7mal", "die", "Er\u00b7wach\u00b7se\u00b7nen", "zu", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "blind und verwildert in des Haschens Hast,", "tokens": ["blind", "und", "ver\u00b7wil\u00b7dert", "in", "des", "Ha\u00b7schens", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "aber am Abend still, mit kleinen steifen", "tokens": ["a\u00b7ber", "am", "A\u00b7bend", "still", ",", "mit", "klei\u00b7nen", "stei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Schritten nachhaus zu gehn, fest angefa\u00dft \u2013:", "tokens": ["Schrit\u00b7ten", "nach\u00b7haus", "zu", "gehn", ",", "fest", "an\u00b7ge\u00b7fa\u00dft", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APZR", "PTKZU", "VVINF", "$,", "ADJD", "VVPP", "$(", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "O immer mehr entweichendes Begreifen,", "tokens": ["O", "im\u00b7mer", "mehr", "ent\u00b7wei\u00b7chen\u00b7des", "Be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "o Angst, o Last.", "tokens": ["o", "Angst", ",", "o", "Last", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und stundenlang am gro\u00dfen grauen Teiche", "tokens": ["Und", "stun\u00b7den\u00b7lang", "am", "gro\u00b7\u00dfen", "grau\u00b7en", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit einem kleinen Segelschiff zu knien;", "tokens": ["mit", "ei\u00b7nem", "klei\u00b7nen", "Se\u00b7gel\u00b7schiff", "zu", "kni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "es zu vergessen, weil noch andre, gleiche", "tokens": ["es", "zu", "ver\u00b7ges\u00b7sen", ",", "weil", "noch", "and\u00b7re", ",", "glei\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "KOUS", "ADV", "PIS", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und sch\u00f6nere Segel durch die Ringe ziehn,", "tokens": ["und", "sch\u00f6\u00b7ne\u00b7re", "Se\u00b7gel", "durch", "die", "Rin\u00b7ge", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und denken m\u00fcssen an das kleine bleiche", "tokens": ["und", "den\u00b7ken", "m\u00fcs\u00b7sen", "an", "das", "klei\u00b7ne", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gesicht, das sinkend aus dem Teiche schien \u2013:", "tokens": ["Ge\u00b7sicht", ",", "das", "sin\u00b7kend", "aus", "dem", "Tei\u00b7che", "schien", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Kindheit, o entgleitende Vergleiche.", "tokens": ["O", "Kind\u00b7heit", ",", "o", "ent\u00b7glei\u00b7ten\u00b7de", "Ver\u00b7glei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wohin? Wohin?", "tokens": ["Wo\u00b7hin", "?", "Wo\u00b7hin", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}