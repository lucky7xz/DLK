{"dta.poem.4411": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Menschliche Unempfindlichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00fcl't die Luft zur holden Fr\u00fchlings-Zeit", "tokens": ["Es", "f\u00fcl't", "die", "Luft", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von der sich n\u00e4hernden beflammten Sonnen-", "tokens": ["Von", "der", "sich", "n\u00e4\u00b7hern\u00b7den", "be\u00b7flamm\u00b7ten", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ADJD", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gluht", "tokens": ["Gluht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das rege Licht voll neuer Fruchtbarkeit.", "tokens": ["Das", "re\u00b7ge", "Licht", "voll", "neu\u00b7er", "Frucht\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es f\u00fclet ihre Kraft die feucht\u2019 und kalte Flut:", "tokens": ["Es", "f\u00fc\u00b7let", "ih\u00b7re", "Kraft", "die", "feucht'", "und", "kal\u00b7te", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es f\u00fclet sie die Erd\u2019, es f\u00fclet\u2019s alles das,", "tokens": ["Es", "f\u00fc\u00b7let", "sie", "die", "Erd'", ",", "es", "f\u00fc\u00b7let's", "al\u00b7les", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PIS", "PDS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was sie hervor bringt, Kraut und Gras.", "tokens": ["Was", "sie", "her\u00b7vor", "bringt", ",", "Kraut", "und", "Gras", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKVZ", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es f\u00fclen V\u00f6gel, Fisch\u2019 und Thiere,", "tokens": ["Es", "f\u00fc\u00b7len", "V\u00f6\u00b7gel", ",", "Fisch'", "und", "Thie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df sich ein neuer Trieb in ihren Adern r\u00fchre.", "tokens": ["Da\u00df", "sich", "ein", "neu\u00b7er", "Trieb", "in", "ih\u00b7ren", "A\u00b7dern", "r\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kurz, alles f\u00fclet itzt, wie alles angef\u00fcllt", "tokens": ["Kurz", ",", "al\u00b7les", "f\u00fc\u00b7let", "itzt", ",", "wie", "al\u00b7les", "an\u00b7ge\u00b7f\u00fcllt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PIS", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von einer Himmels-Kraft, vom Geist des Son-", "tokens": ["Von", "ei\u00b7ner", "Him\u00b7mels\u00b7Kraft", ",", "vom", "Geist", "des", "Son"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "nen-Lichts:", "tokens": ["nen\u00b7Lichts", ":"], "token_info": ["word", "punct"], "pos": ["FM.la", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Nur blo\u00df des Sch\u00f6pfers Ebenbild,", "tokens": ["Nur", "blo\u00df", "des", "Sch\u00f6p\u00b7fers", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Mensch, als Mensch, versp\u00fcr\u2019t von allem, leider!", "tokens": ["Der", "Mensch", ",", "als", "Mensch", ",", "ver\u00b7sp\u00fcr't", "von", "al\u00b7lem", ",", "lei\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "APPR", "PIS", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "nichts.", "tokens": ["nichts", "."], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "-", "measure": "single.down"}}}}}