{"textgrid.poem.48121": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Rangierertod", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Goldstrom flie\u00dft in Wogen.", "tokens": ["Der", "Gold\u00b7strom", "flie\u00dft", "in", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch seine flimmernde Fl\u00e4che ist", "tokens": ["Durch", "sei\u00b7ne", "flim\u00b7mern\u00b7de", "Fl\u00e4\u00b7che", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "eine blutige Spur gezogen.", "tokens": ["ei\u00b7ne", "blu\u00b7ti\u00b7ge", "Spur", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Tiefschwarz die Nacht. Ein Pfeifen schrillt.", "tokens": ["Tief\u00b7schwarz", "die", "Nacht", ".", "Ein", "Pfei\u00b7fen", "schrillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00fchw\u00fcrmchen h\u00fcpfen auf dunkler Bahn.", "tokens": ["Gl\u00fch\u00b7w\u00fcrm\u00b7chen", "h\u00fcp\u00b7fen", "auf", "dunk\u00b7ler", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus g\u00e4hnendem D\u00fcster taucht und schwillt", "tokens": ["Aus", "g\u00e4h\u00b7nen\u00b7dem", "D\u00fcs\u00b7ter", "taucht", "und", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "es empor wie ein formloses G\u00f6tzenbild \u2013", "tokens": ["es", "em\u00b7por", "wie", "ein", "form\u00b7lo\u00b7ses", "G\u00f6t\u00b7zen\u00b7bild", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "und es faucht und zischt und es braust heran,", "tokens": ["und", "es", "faucht", "und", "zischt", "und", "es", "braust", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "KON", "VVFIN", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "die Luft ersch\u00fcttert ein Beben . . .", "tokens": ["die", "Luft", "er\u00b7sch\u00fct\u00b7tert", "ein", "Be\u00b7ben", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "und horch: auch von dr\u00fcben Gest\u00f6hn und Geschnauf,", "tokens": ["und", "horch", ":", "auch", "von", "dr\u00fc\u00b7ben", "Ge\u00b7st\u00f6hn", "und", "Ge\u00b7schnauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "ein h\u00f6llisches Auge funkelt auf.", "tokens": ["ein", "h\u00f6l\u00b7li\u00b7sches", "Au\u00b7ge", "fun\u00b7kelt", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Nur ein Ruck \u2013 und die R\u00e4der zerschmettern dich", "tokens": ["Nur", "ein", "Ruck", "\u2013", "und", "die", "R\u00e4\u00b7der", "zer\u00b7schmet\u00b7tern", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$(", "KON", "ART", "NN", "VVFIN", "PPER"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "\u2013 unseliger Mann, nun wahre dich!", "tokens": ["\u2013", "un\u00b7se\u00b7li\u00b7ger", "Mann", ",", "nun", "wah\u00b7re", "dich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Duck dich und lauf \u2013", "tokens": ["Duck", "dich", "und", "lauf", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "lauf zu: es gilt das Leben!", "tokens": ["lauf", "zu", ":", "es", "gilt", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Tiefschwarz die Nacht. Und ein Angstschrei gellt", "tokens": ["Tief\u00b7schwarz", "die", "Nacht", ".", "Und", "ein", "Angst\u00b7schrei", "gellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "$.", "KON", "ART", "NN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und hallt in die d\u00fcstersten Ecken.", "tokens": ["und", "hallt", "in", "die", "d\u00fcs\u00b7ters\u00b7ten", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Schein der Blendlaterne f\u00e4llt", "tokens": ["Der", "Schein", "der", "Blend\u00b7la\u00b7ter\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auf das grausige Bild der Schrecken", "tokens": ["auf", "das", "grau\u00b7si\u00b7ge", "Bild", "der", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "und spiegelt sich rot in dem rinnenden Blut,", "tokens": ["und", "spie\u00b7gelt", "sich", "rot", "in", "dem", "rin\u00b7nen\u00b7den", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "in des brechenden Auges Fieberglut.", "tokens": ["in", "des", "bre\u00b7chen\u00b7den", "Au\u00b7ges", "Fie\u00b7berg\u00b7lut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Und die andern alle, sie sind's ", "tokens": ["Und", "die", "an\u00b7dern", "al\u00b7le", ",", "sie", "sin\u00b7d's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "PIS", "$,", "PPER", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "sie haben sich stumm in die Nacht gewandt,", "tokens": ["sie", "ha\u00b7ben", "sich", "stumm", "in", "die", "Nacht", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "nur einer, ein junger, blondb\u00e4rtiger Fant", "tokens": ["nur", "ei\u00b7ner", ",", "ein", "jun\u00b7ger", ",", "blond\u00b7b\u00e4r\u00b7ti\u00b7ger", "Fant"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIS", "$,", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "starrt flackernden Blicks in das flackernde Licht", "tokens": ["starrt", "fla\u00b7ckern\u00b7den", "Blicks", "in", "das", "fla\u00b7ckern\u00b7de", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "und seine zuckende Lippe spricht:", "tokens": ["und", "sei\u00b7ne", "zu\u00b7cken\u00b7de", "Lip\u00b7pe", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00bbder F\u00fcnfte, der F\u00fcnfte in diesem Mond!\u00ab", "tokens": ["\u00bb", "der", "F\u00fcnf\u00b7te", ",", "der", "F\u00fcnf\u00b7te", "in", "die\u00b7sem", "Mond", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Die Z\u00e4hne gepre\u00dft und die Faust geballt!", "tokens": ["Die", "Z\u00e4h\u00b7ne", "ge\u00b7pre\u00dft", "und", "die", "Faust", "ge\u00b7ballt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein neues Pfeifen die Nacht durchhallt \u2013", "tokens": ["Ein", "neu\u00b7es", "Pfei\u00b7fen", "die", "Nacht", "durch\u00b7hallt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und keine Ehre und kein Gewinnst,", "tokens": ["Und", "kei\u00b7ne", "Eh\u00b7re", "und", "kein", "Ge\u00b7winnst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "keine Stunde Ruhe im Todesdienst,", "tokens": ["kei\u00b7ne", "Stun\u00b7de", "Ru\u00b7he", "im", "To\u00b7des\u00b7dienst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "kein freundliches Licht, das die Nacht erhellt,", "tokens": ["kein", "freund\u00b7li\u00b7ches", "Licht", ",", "das", "die", "Nacht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "keine rettende Hand \u2013 das kostet Geld! \u2013", "tokens": ["kei\u00b7ne", "ret\u00b7ten\u00b7de", "Hand", "\u2013", "das", "kos\u00b7tet", "Geld", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "PDS", "VVFIN", "NN", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "\u2013 Und der Goldstrom geht in Wogen.", "tokens": ["\u2013", "Und", "der", "Gold\u00b7strom", "geht", "in", "Wo\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Durch seine flimmernde Fl\u00e4che ist", "tokens": ["Durch", "sei\u00b7ne", "flim\u00b7mern\u00b7de", "Fl\u00e4\u00b7che", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "eine blutige Spur gezogen.", "tokens": ["ei\u00b7ne", "blu\u00b7ti\u00b7ge", "Spur", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}