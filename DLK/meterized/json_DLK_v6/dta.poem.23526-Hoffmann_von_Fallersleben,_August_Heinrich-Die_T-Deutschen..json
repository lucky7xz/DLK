{"dta.poem.23526": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die T-Deutschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr k\u00f6nnt nicht unterscheiden d und t,", "tokens": ["Ihr", "k\u00f6nnt", "nicht", "un\u00b7ter\u00b7schei\u00b7den", "d", "und", "t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVINF", "XY", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wollt uns lehren wie man schreibt und spricht?", "tokens": ["Und", "wollt", "uns", "leh\u00b7ren", "wie", "man", "schreibt", "und", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "KOKOM", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr macht doch sonst ein b und sprechet p,", "tokens": ["Ihr", "macht", "doch", "sonst", "ein", "b", "und", "spre\u00b7chet", "p", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "XY", "KON", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Warum doch macht ihr's d in deutsch denn nicht?", "tokens": ["Wa\u00b7rum", "doch", "macht", "ih\u00b7r's", "d", "in", "deutsch", "denn", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ADJD", "ADV", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es nimmt's euch \u00fcbel noch der deutsche Bund;", "tokens": ["Es", "nimmt's", "euch", "\u00fc\u00b7bel", "noch", "der", "deut\u00b7sche", "Bund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr wisst, er will einmal kein teutscher sein.", "tokens": ["Ihr", "wisst", ",", "er", "will", "ein\u00b7mal", "kein", "teut\u00b7scher", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "PIAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr protestiert ja doch nur ohne Grund,", "tokens": ["Ihr", "pro\u00b7tes\u00b7tiert", "ja", "doch", "nur", "oh\u00b7ne", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lasst einmal das viele ", "tokens": ["So", "lasst", "ein\u00b7mal", "das", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}