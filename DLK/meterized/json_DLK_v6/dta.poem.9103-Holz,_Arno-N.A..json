{"dta.poem.9103": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meinem schwarzen Taxuswald", "tokens": ["In", "mei\u00b7nem", "schwar\u00b7zen", "Ta\u00b7xus\u00b7wald"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "singt ein M\u00e4rchenvogel \u2014", "tokens": ["singt", "ein", "M\u00e4r\u00b7chen\u00b7vo\u00b7gel"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "die ganze Nacht.", "tokens": ["die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Blumen blinken.", "tokens": ["Blu\u00b7men", "blin\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Unter Sternen, die sich spiegeln,", "tokens": ["Un\u00b7ter", "Ster\u00b7nen", ",", "die", "sich", "spie\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "treibt mein Boot.", "tokens": ["treibt", "mein", "Boot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Meine tr\u00e4umenden H\u00e4nde", "tokens": ["Mei\u00b7ne", "tr\u00e4u\u00b7men\u00b7den", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "tauchen in schwimmende Wasserrosen.", "tokens": ["tau\u00b7chen", "in", "schwim\u00b7men\u00b7de", "Was\u00b7ser\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Unten,", "tokens": ["Un\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "lautlos, die Tiefe.", "tokens": ["laut\u00b7los", ",", "die", "Tie\u00b7fe", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Fern die Ufer! Das Lied. . .", "tokens": ["Fern", "die", "U\u00b7fer", "!", "Das", "Lied", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN", "$.", "$.", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}