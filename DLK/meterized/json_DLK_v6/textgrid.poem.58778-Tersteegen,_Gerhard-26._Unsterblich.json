{"textgrid.poem.58778": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "26. Unsterblich", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist, du warst, wirst immer sein;", "tokens": ["Du", "bist", ",", "du", "warst", ",", "wirst", "im\u00b7mer", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblichkeit hast du allein,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "hast", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Geist, dein Hauch, hat s' durch dein Geben.", "tokens": ["Mein", "Geist", ",", "dein", "Hauch", ",", "hat", "s'", "durch", "dein", "Ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es mag vergehn die ganze Welt,", "tokens": ["Es", "mag", "ver\u00b7gehn", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob auch mein's Leibes Bau zerf\u00e4llt,", "tokens": ["Ob", "auch", "mein's", "Lei\u00b7bes", "Bau", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du sagst mir zu unsterblich's Leben:", "tokens": ["Du", "sagst", "mir", "zu", "un\u00b7sterb\u00b7lich's", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der schlecht ges\u00e4te Leib soll sch\u00f6n,", "tokens": ["Der", "schlecht", "ge\u00b7s\u00e4\u00b7te", "Leib", "soll", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VMFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Geist unendlich fr\u00f6hlich stehn.", "tokens": ["Der", "Geist", "un\u00b7end\u00b7lich", "fr\u00f6h\u00b7lich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}