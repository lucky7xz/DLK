{"textgrid.poem.40758": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Spinnenfl\u00f6re, Epheuhecken,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Spinnenfl\u00f6re, Epheuhecken,", "tokens": ["Ihr", "Spin\u00b7nen\u00b7fl\u00f6\u00b7re", ",", "E\u00b7pheu\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr um Schutt Gew\u00e4nder sch\u00fcrzt;", "tokens": ["Die", "ihr", "um", "Schutt", "Ge\u00b7w\u00e4n\u00b7der", "sch\u00fcrzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gips du Engelschaar der Decken,", "tokens": ["Von", "Gips", "du", "En\u00b7gel\u00b7schaar", "der", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die bald aufs neu vom Himmel st\u00fcrzt;", "tokens": ["Die", "bald", "aufs", "neu", "vom", "Him\u00b7mel", "st\u00fcrzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun jubelt oder bangt mit Schweigen,", "tokens": ["Nun", "ju\u00b7belt", "o\u00b7der", "bangt", "mit", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Euch bringt's Verderben, bringt's Gedeihn:", "tokens": ["Euch", "bringt's", "Ver\u00b7der\u00b7ben", ",", "bringt's", "Ge\u00b7deihn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heut nimmt Besitz von seinem Eigen", "tokens": ["Heut", "nimmt", "Be\u00b7sitz", "von", "sei\u00b7nem", "Ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der neue Herr auf Rabenstein.", "tokens": ["Der", "neu\u00b7e", "Herr", "auf", "Ra\u00b7ben\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem, der nach Gransons Schlacht gefunden", "tokens": ["Dem", ",", "der", "nach", "Gran\u00b7sons", "Schlacht", "ge\u00b7fun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "NE", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Karls Demant, schien's ein Glas gering;", "tokens": ["Karls", "De\u00b7mant", ",", "schien's", "ein", "Glas", "ge\u00b7ring", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Herzogshut einst der Burgunden,", "tokens": ["Im", "Her\u00b7zogs\u00b7hut", "einst", "der", "Bur\u00b7gun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziert jetzt er Habsburgs Kronenring.", "tokens": ["Ziert", "jetzt", "er", "Habs\u00b7burgs", "Kro\u00b7nen\u00b7ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sch\u00f6n und echt, bleibt auch das Alte,", "tokens": ["Wenn", "sch\u00f6n", "und", "echt", ",", "bleibt", "auch", "das", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mag wechselnd gleich das Beiwerk sein;", "tokens": ["Mag", "wech\u00b7selnd", "gleich", "das", "Bei\u00b7werk", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum neuer Fassung Schmuck erhalte", "tokens": ["Drum", "neu\u00b7er", "Fas\u00b7sung", "Schmuck", "er\u00b7hal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das alte Kleinod Rabenstein.", "tokens": ["Das", "al\u00b7te", "Klei\u00b7nod", "Ra\u00b7ben\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es baut ein K\u00f6nig sich am Rheine", "tokens": ["Es", "baut", "ein", "K\u00f6\u00b7nig", "sich", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus altem Stein ein neues Haus;", "tokens": ["Aus", "al\u00b7tem", "Stein", "ein", "neu\u00b7es", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Lied, das moos'ge Runensteine", "tokens": ["Das", "Lied", ",", "das", "moos'\u00b7ge", "Ru\u00b7nen\u00b7stei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gern kr\u00e4nzt mit heut' erbl\u00fchtem Strau\u00df,", "tokens": ["Gern", "kr\u00e4nzt", "mit", "heut'", "er\u00b7bl\u00fch\u00b7tem", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es m\u00f6cht' ihn mahnen, zu umwinden", "tokens": ["Es", "m\u00f6cht'", "ihn", "mah\u00b7nen", ",", "zu", "um\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit frischem Kranz den alten Stein;", "tokens": ["Mit", "fri\u00b7schem", "Kranz", "den", "al\u00b7ten", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch leichter wird Geh\u00f6r es finden", "tokens": ["Doch", "leich\u00b7ter", "wird", "Ge\u00b7h\u00f6r", "es", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beim schlichten Herrn von Rabenstein.", "tokens": ["Beim", "schlich\u00b7ten", "Herrn", "von", "Ra\u00b7ben\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Am Thor des alten Bauherrn Wappen,", "tokens": ["Am", "Thor", "des", "al\u00b7ten", "Bau\u00b7herrn", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O la\u00df es stehn, wie sonst es stand:", "tokens": ["O", "la\u00df", "es", "stehn", ",", "wie", "sonst", "es", "stand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es adelt auch den Leinwandlappen", "tokens": ["Es", "a\u00b7delt", "auch", "den", "Lein\u00b7wand\u00b7lap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Monogramm der K\u00fcnstlerhand.", "tokens": ["Das", "Mo\u00b7no\u00b7gramm", "der", "K\u00fcnst\u00b7ler\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ringmauern morsch mit schart'gen Th\u00fcrmen,", "tokens": ["Ring\u00b7mau\u00b7ern", "morsch", "mit", "schart'\u00b7gen", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df sie in Schutt zerfallen sein;", "tokens": ["La\u00df", "sie", "in", "Schutt", "zer\u00b7fal\u00b7len", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur Freundeschaaren werden st\u00fcrmen,", "tokens": ["Nur", "Freun\u00b7de\u00b7schaa\u00b7ren", "wer\u00b7den", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was soll ein Wall auf Rabenstein?", "tokens": ["Was", "soll", "ein", "Wall", "auf", "Ra\u00b7ben\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlingpflanzen lasse Ranken schlagen,", "tokens": ["Schling\u00b7pflan\u00b7zen", "las\u00b7se", "Ran\u00b7ken", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An morscher Wand aufklettern weit,", "tokens": ["An", "mor\u00b7scher", "Wand", "auf\u00b7klet\u00b7tern", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bl\u00fcht die Gegenwart, getragen", "tokens": ["So", "bl\u00fcht", "die", "Ge\u00b7gen\u00b7wart", ",", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Schultern der Vergangenheit.", "tokens": ["Auf", "Schul\u00b7tern", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Hofraum la\u00df vielfarbig prangen", "tokens": ["Im", "Hof\u00b7raum", "la\u00df", "viel\u00b7far\u00b7big", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sch\u00f6nsten Dahlien bunte Reihn,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Da\u00b7hli\u00b7en", "bun\u00b7te", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wie Pagen, die geschm\u00fcckt empfangen", "tokens": ["Wie", "Pa\u00b7gen", ",", "die", "ge\u00b7schm\u00fcckt", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den edeln Herrn auf Rabenstein.", "tokens": ["Den", "e\u00b7deln", "Herrn", "auf", "Ra\u00b7ben\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Steil klimmt der Pfad zu Himmelshallen,", "tokens": ["Steil", "klimmt", "der", "Pfad", "zu", "Him\u00b7mels\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dahin f\u00fchrt diese Treppe wohl;", "tokens": ["Da\u00b7hin", "f\u00fchrt", "die\u00b7se", "Trep\u00b7pe", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht gut ist's, nur in Stapfen wallen,", "tokens": ["Nicht", "gut", "ist's", ",", "nur", "in", "Stap\u00b7fen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die noch vom Tritt der Ahnherrn hohl;", "tokens": ["Die", "noch", "vom", "Tritt", "der", "Ahn\u00b7herrn", "hohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Saal und Himmel l\u00e4\u00dft sich kommen", "tokens": ["In", "Saal", "und", "Him\u00b7mel", "l\u00e4\u00dft", "sich", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl auch mit ungebrochnem Bein,", "tokens": ["Wohl", "auch", "mit", "un\u00b7ge\u00b7broch\u00b7nem", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum wird die neue Treppe frommen", "tokens": ["Drum", "wird", "die", "neu\u00b7e", "Trep\u00b7pe", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem alten Haus auf Rabenstein.", "tokens": ["Dem", "al\u00b7ten", "Haus", "auf", "Ra\u00b7ben\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Neuzeit lehrt den Lenz bestehlen,", "tokens": ["Die", "Neu\u00b7zeit", "lehrt", "den", "Lenz", "be\u00b7steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Zimmer bl\u00fcht als Gartenbeet;", "tokens": ["Ihr", "Zim\u00b7mer", "bl\u00fcht", "als", "Gar\u00b7ten\u00b7beet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu treu ist's, wenn in deinen S\u00e4len", "tokens": ["Zu", "treu", "ist's", ",", "wenn", "in", "dei\u00b7nen", "S\u00e4\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Regen tropft, der Sturmwind weht;", "tokens": ["Der", "Re\u00b7gen", "tropft", ",", "der", "Sturm\u00b7wind", "weht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein altes Recht ist's span'scher Granden,", "tokens": ["Ein", "al\u00b7tes", "Recht", "ist's", "span'\u00b7scher", "Gran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vorm Thron bedeckten Haupts zu sein;", "tokens": ["Vorm", "Thron", "be\u00b7deck\u00b7ten", "Haupts", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Baarh\u00e4uptig ist er lang gestanden,", "tokens": ["B