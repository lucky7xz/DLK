{"textgrid.poem.53725": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Warum ich nicht mehr . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das L\u00f6wenreh durcheilt den Wald", "tokens": ["Das", "L\u00f6\u00b7wen\u00b7reh", "durch\u00b7eilt", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sucht den F\u00f6rster Theobald.", "tokens": ["und", "sucht", "den", "F\u00f6rs\u00b7ter", "Theo\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der F\u00f6rster Theobald desgleichen", "tokens": ["Der", "F\u00f6rs\u00b7ter", "Theo\u00b7bald", "des\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "sucht es durch P\u00fcrschen zu erreichen,", "tokens": ["sucht", "es", "durch", "P\u00fcr\u00b7schen", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "und zwar mit Kugeln, deren Gift", "tokens": ["und", "zwar", "mit", "Ku\u00b7geln", ",", "de\u00b7ren", "Gift"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zu Rauch verwandelt, wen es trifft.", "tokens": ["zu", "Rauch", "ver\u00b7wan\u00b7delt", ",", "wen", "es", "trifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als sie sich endlich haben, schie\u00dft", "tokens": ["Als", "sie", "sich", "end\u00b7lich", "ha\u00b7ben", ",", "schie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er es, worauf es ihn genie\u00dft.", "tokens": ["er", "es", ",", "wo\u00b7rauf", "es", "ihn", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Allein die Kugel wirkt alsbald:", "tokens": ["Al\u00b7lein", "die", "Ku\u00b7gel", "wirkt", "als\u00b7bald", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Rauch wird Reh nebst Theobald.", "tokens": ["Zu", "Rauch", "wird", "Reh", "nebst", "Theo\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Seitdem sind beide ohne Frage", "tokens": ["Seit\u00b7dem", "sind", "bei\u00b7de", "oh\u00b7ne", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein dankbares Objekt der Sage.", "tokens": ["ein", "dank\u00b7ba\u00b7res", "Ob\u00b7jekt", "der", "Sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Ich sicherlich. Und die Republik \u2013 aber das ist Sache Ihrer politischen Mitarbeiter.", "tokens": ["Ich", "si\u00b7cher\u00b7lich", ".", "Und", "die", "Re\u00b7pub\u00b7lik", "\u2013", "a\u00b7ber", "das", "ist", "Sa\u00b7che", "Ih\u00b7rer", "po\u00b7li\u00b7ti\u00b7schen", "Mit\u00b7ar\u00b7bei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "$.", "KON", "ART", "NN", "$(", "KON", "PDS", "VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}