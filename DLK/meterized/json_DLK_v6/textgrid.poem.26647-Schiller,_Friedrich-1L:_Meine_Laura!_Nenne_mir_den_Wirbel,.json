{"textgrid.poem.26647": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Laura! Nenne mir den Wirbel,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Laura! Nenne mir den Wirbel,", "tokens": ["Mei\u00b7ne", "Lau\u00b7ra", "!", "Nen\u00b7ne", "mir", "den", "Wir\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der an K\u00f6rper K\u00f6rper m\u00e4chtig rei\u00dft,", "tokens": ["Der", "an", "K\u00f6r\u00b7per", "K\u00f6r\u00b7per", "m\u00e4ch\u00b7tig", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nenne, meine Laura, mir den Zauber,", "tokens": ["Nen\u00b7ne", ",", "mei\u00b7ne", "Lau\u00b7ra", ",", "mir", "den", "Zau\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der zum Geist monarchisch zwingt den Geist.", "tokens": ["Der", "zum", "Geist", "mon\u00b7ar\u00b7chisch", "zwingt", "den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh! er lehrt die schwebenden Planeten", "tokens": ["Sieh", "!", "er", "lehrt", "die", "schwe\u00b7ben\u00b7den", "Pla\u00b7ne\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ewgen Ringgangs um die Sonne fliehn", "tokens": ["Ew\u00b7gen", "Ring\u00b7gangs", "um", "die", "Son\u00b7ne", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und, gleich Kindern um die Mutter h\u00fcpfend,", "tokens": ["Und", ",", "gleich", "Kin\u00b7dern", "um", "die", "Mut\u00b7ter", "h\u00fcp\u00b7fend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bunte Zirkel um die F\u00fcrstin ziehn;", "tokens": ["Bun\u00b7te", "Zir\u00b7kel", "um", "die", "F\u00fcrs\u00b7tin", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Durstig trinkt den goldnen Strahlenregen", "tokens": ["Durs\u00b7tig", "trinkt", "den", "gold\u00b7nen", "Strah\u00b7len\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jedes rollende Gestirn,", "tokens": ["Je\u00b7des", "rol\u00b7len\u00b7de", "Ge\u00b7stirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinkt aus ihrem Feuerkelch Erquickung,", "tokens": ["Trinkt", "aus", "ih\u00b7rem", "Feu\u00b7er\u00b7kelch", "Er\u00b7quic\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie die Glieder Geister vom Gehirn.", "tokens": ["Wie", "die", "Glie\u00b7der", "Geis\u00b7ter", "vom", "Ge\u00b7hirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sonnenst\u00e4ubchen paart mit Sonnenst\u00e4ubchen", "tokens": ["Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "paart", "mit", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich in trauter Harmonie,", "tokens": ["Sich", "in", "trau\u00b7ter", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sph\u00e4ren ineinander lenkt die Liebe,", "tokens": ["Sph\u00e4\u00b7ren", "in\u00b7ein\u00b7an\u00b7der", "lenkt", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weltsysteme dauren nur durch sie.", "tokens": ["Welt\u00b7sys\u00b7te\u00b7me", "dau\u00b7ren", "nur", "durch", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Tilge sie vom Uhrwerk der Naturen \u2013", "tokens": ["Til\u00b7ge", "sie", "vom", "Uhr\u00b7werk", "der", "Na\u00b7tu\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00fcmmernd auseinander springt das All,", "tokens": ["Tr\u00fcm\u00b7mernd", "aus\u00b7ein\u00b7an\u00b7der", "springt", "das", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In das Chaos donnern eure Welten,", "tokens": ["In", "das", "Chaos", "don\u00b7nern", "eu\u00b7re", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Weint, Newtone, ihren Riesenfall!", "tokens": ["Weint", ",", "New\u00b7to\u00b7ne", ",", "ih\u00b7ren", "Rie\u00b7sen\u00b7fall", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Tilg die G\u00f6ttin aus der Geister Orden,", "tokens": ["Tilg", "die", "G\u00f6t\u00b7tin", "aus", "der", "Geis\u00b7ter", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sie erstarren in der K\u00f6rper Tod,", "tokens": ["Sie", "er\u00b7star\u00b7ren", "in", "der", "K\u00f6r\u00b7per", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ohne Liebe kehrt kein Fr\u00fchling wieder,", "tokens": ["Oh\u00b7ne", "Lie\u00b7be", "kehrt", "kein", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ohne Liebe preist kein Wesen Gott!", "tokens": ["Oh\u00b7ne", "Lie\u00b7be", "preist", "kein", "We\u00b7sen", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und was ists, das, wenn mich Laura k\u00fcsset,", "tokens": ["Und", "was", "ists", ",", "das", ",", "wenn", "mich", "Lau\u00b7ra", "k\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "$,", "PDS", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Purpurflammen auf die Wangen geu\u00dft,", "tokens": ["Pur\u00b7pur\u00b7flam\u00b7men", "auf", "die", "Wan\u00b7gen", "geu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meinem Herzen raschern Schwung gebietet,", "tokens": ["Mei\u00b7nem", "Her\u00b7zen", "ra\u00b7schern", "Schwung", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fiebrisch wild mein Blut von hinnen rei\u00dft?", "tokens": ["Fie\u00b7brisch", "wild", "mein", "Blut", "von", "hin\u00b7nen", "rei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Aus den Schranken schwellen alle Sennen,", "tokens": ["Aus", "den", "Schran\u00b7ken", "schwel\u00b7len", "al\u00b7le", "Sen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seine Ufer \u00fcberwallt das Blut,", "tokens": ["Sei\u00b7ne", "U\u00b7fer", "\u00fc\u00b7berw\u00b7allt", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00f6rper will in K\u00f6rper \u00fcberst\u00fcrzen,", "tokens": ["K\u00f6r\u00b7per", "will", "in", "K\u00f6r\u00b7per", "\u00fc\u00b7bers\u00b7t\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lodern Seelen in vereinter Glut;", "tokens": ["Lo\u00b7dern", "See\u00b7len", "in", "ver\u00b7ein\u00b7ter", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Gleich allm\u00e4chtig wie dort in der toten", "tokens": ["Gleich", "all\u00b7m\u00e4ch\u00b7tig", "wie", "dort", "in", "der", "to\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "APPR", "ART", "NN"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6pfung ewgen Federtrieb", "tokens": ["Sch\u00f6p\u00b7fung", "ew\u00b7gen", "Fe\u00b7der\u00b7trieb"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrscht im arachneischen Gewebe", "tokens": ["Herrscht", "im", "a\u00b7rach\u00b7nei\u00b7schen", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der empfindenden Natur die Lieb.", "tokens": ["Der", "emp\u00b7fin\u00b7den\u00b7den", "Na\u00b7tur", "die", "Lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Siehe, Laura, Fr\u00f6hlichkeit umarmet", "tokens": ["Sie\u00b7he", ",", "Lau\u00b7ra", ",", "Fr\u00f6h\u00b7lich\u00b7keit", "um\u00b7ar\u00b7met"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "NE", "$,", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wilder Schmerzen \u00dcberschwung,", "tokens": ["Wil\u00b7der", "Schmer\u00b7zen", "\u00dc\u00b7bersc\u00b7hwung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Hoffnung Liebesbrust erwarmet", "tokens": ["An", "der", "Hoff\u00b7nung", "Lie\u00b7bes\u00b7brust", "er\u00b7war\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Starrende Verzweifelung.", "tokens": ["Star\u00b7ren\u00b7de", "Ver\u00b7zwei\u00b7fe\u00b7lung", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+--", "measure": "dactylic.init"}}, "stanza.11": {"line.1": {"text": "Schwesterliche Wollust mildert", "tokens": ["Schwes\u00b7ter\u00b7li\u00b7che", "Wol\u00b7lust", "mil\u00b7dert"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcstrer Schwermut Schauernacht,", "tokens": ["D\u00fcst\u00b7rer", "Schwer\u00b7mut", "Schau\u00b7er\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und entbunden von den goldnen Kindern", "tokens": ["Und", "ent\u00b7bun\u00b7den", "von", "den", "gold\u00b7nen", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Strahlt das Auge Sonnenpracht.", "tokens": ["Strahlt", "das", "Au\u00b7ge", "Son\u00b7nen\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Waltet nicht auch durch des \u00dcbels Reiche", "tokens": ["Wal\u00b7tet", "nicht", "auch", "durch", "des", "\u00dc\u00b7bels", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrchterliche Sympathie?", "tokens": ["F\u00fcrch\u00b7ter\u00b7li\u00b7che", "Sym\u00b7pa\u00b7thie", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der H\u00f6lle buhlen unsre Laster,", "tokens": ["Mit", "der", "H\u00f6l\u00b7le", "buh\u00b7len", "uns\u00b7re", "Las\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit dem Himmel grollen sie.", "tokens": ["Mit", "dem", "Him\u00b7mel", "grol\u00b7len", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Um die S\u00fcnde flechten Schlangenwirbel", "tokens": ["Um", "die", "S\u00fcn\u00b7de", "flech\u00b7ten", "Schlan\u00b7gen\u00b7wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Scham und Reu, das Eumenidenpaar,", "tokens": ["Scham", "und", "Reu", ",", "das", "Eu\u00b7me\u00b7ni\u00b7den\u00b7paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um der Gr\u00f6\u00dfe Adlerfl\u00fcgel windet", "tokens": ["Um", "der", "Gr\u00f6\u00b7\u00dfe", "Ad\u00b7ler\u00b7fl\u00fc\u00b7gel", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich verr\u00e4trisch die Gefahr.", "tokens": ["Sich", "ver\u00b7r\u00e4t\u00b7risch", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Mit dem Stolze pflegt der Sturz zu t\u00e4ndeln,", "tokens": ["Mit", "dem", "Stol\u00b7ze", "pflegt", "der", "Sturz", "zu", "t\u00e4n\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um das Gl\u00fcck zu klammern sich der Neid,", "tokens": ["Um", "das", "Gl\u00fcck", "zu", "klam\u00b7mern", "sich", "der", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "wor