{"dta.poem.18030": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Poesie des sechszehnten  \n Jahrhunderts.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ehr niemand f&#252;r Gut begert.", "tokens": ["Der", "Ehr", "nie\u00b7mand", "f", "&#252;", "r", "Gut", "be\u00b7gert", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$(", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Des Mannes Ehr recht also staht,", "tokens": ["Des", "Man\u00b7nes", "Ehr", "recht", "al\u00b7so", "staht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darnach als er sich selber hat.", "tokens": ["Dar\u00b7nach", "als", "er", "sich", "sel\u00b7ber", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie soll des Lasters werden Rath", "tokens": ["Wie", "soll", "des", "Las\u00b7ters", "wer\u00b7den", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem all sein Ehr zu Laster gat.", "tokens": ["Dem", "all", "sein", "Ehr", "zu", "Las\u00b7ter", "gat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vil mancher hat der Ehre Namen,", "tokens": ["Vil", "man\u00b7cher", "hat", "der", "Eh\u00b7re", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und will sich doch der Ehre schamen.", "tokens": ["Und", "will", "sich", "doch", "der", "Eh\u00b7re", "scha\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}