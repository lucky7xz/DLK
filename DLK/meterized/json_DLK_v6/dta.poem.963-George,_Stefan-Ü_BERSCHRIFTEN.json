{"dta.poem.963": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "\u00dc BERSCHRIFTEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich zog ein vogel frei aus goldnem bauer               ", "tokens": ["Als", "ich", "zog", "ein", "vo\u00b7gel", "frei", "aus", "gold\u00b7nem", "bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ward der segen mir in reichem maasse               ", "tokens": ["Ward", "der", "se\u00b7gen", "mir", "in", "rei\u00b7chem", "maas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frauen warfen von der mauer               ", "tokens": ["Frau\u00b7en", "war\u00b7fen", "von", "der", "mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosen auf die strasse.             ", "tokens": ["Ro\u00b7sen", "auf", "die", "stras\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Durch der l&#228;nder wunder marmor der pal&#228;ste               ", "tokens": ["Durch", "der", "l", "&#228;", "nder", "wun\u00b7der", "mar\u00b7mor", "der", "pal", "&#228;", "ste"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "NN", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "ADJA"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Grauen in den heiligen gezelten               ", "tokens": ["Grau\u00b7en", "in", "den", "hei\u00b7li\u00b7gen", "ge\u00b7zel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zog ich fern vom schwarm der g&#228;ste               ", "tokens": ["Zog", "ich", "fern", "vom", "schwarm", "der", "g", "&#228;", "ste"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "ADJD", "ART", "NN", "$(", "ADJA"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ich sang nur selten             ", "tokens": ["Und", "ich", "sang", "nur", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jahre flossen &#183; von den heimatlichen essen               ", "tokens": ["Jah\u00b7re", "flos\u00b7sen", "&#183;", "von", "den", "hei\u00b7mat\u00b7li\u00b7chen", "es\u00b7sen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "APPR", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wirbelt rauch zum grauen wolkenraum               ", "tokens": ["Wir\u00b7belt", "rauch", "zum", "grau\u00b7en", "wol\u00b7ken\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich erhoffe nur vergessen               ", "tokens": ["Ich", "er\u00b7hof\u00b7fe", "nur", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruh und blassen traum.             ", "tokens": ["Ruh", "und", "blas\u00b7sen", "traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}