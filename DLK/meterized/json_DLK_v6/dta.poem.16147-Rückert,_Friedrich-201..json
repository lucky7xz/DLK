{"dta.poem.16147": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "201.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bild von Gro\u00dfmuth ist der L\u00f6w' und Tapferkeit,", "tokens": ["Ein", "Bild", "von", "Gro\u00df\u00b7muth", "ist", "der", "L\u00f6\u00b7w'", "und", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es ist ihm angestammt der Ruhm aus alter Zeit.", "tokens": ["Es", "ist", "ihm", "an\u00b7ge\u00b7stammt", "der", "Ruhm", "aus", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar sagen M\u00e4nner, die auf L\u00e4nderkunde reisen,", "tokens": ["Zwar", "sa\u00b7gen", "M\u00e4n\u00b7ner", ",", "die", "auf", "L\u00e4n\u00b7der\u00b7kun\u00b7de", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allbeides sei an ihm nicht unbedingt zu preisen.", "tokens": ["All\u00b7bei\u00b7des", "sei", "an", "ihm", "nicht", "un\u00b7be\u00b7dingt", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein wir glaubens nicht, und glauben sonst doch gern,", "tokens": ["Al\u00b7lein", "wir", "glau\u00b7bens", "nicht", ",", "und", "glau\u00b7ben", "sonst", "doch", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PTKNEG", "$,", "KON", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was zur Verkleinerung gereichet gro\u00dfen Herrn.", "tokens": ["Was", "zur", "Ver\u00b7klei\u00b7ne\u00b7rung", "ge\u00b7rei\u00b7chet", "gro\u00b7\u00dfen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von k\u00f6niglichem Muth wo w\u00fcrde denn gefunden", "tokens": ["Von", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "Muth", "wo", "w\u00fcr\u00b7de", "denn", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PWAV", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Vorbild, wenn es w\u00e4r' am L\u00f6wen auch verschwunden!", "tokens": ["Ein", "Vor\u00b7bild", ",", "wenn", "es", "w\u00e4r'", "am", "L\u00f6\u00b7wen", "auch", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}