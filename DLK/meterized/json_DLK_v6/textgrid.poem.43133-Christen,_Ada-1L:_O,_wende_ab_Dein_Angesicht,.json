{"textgrid.poem.43133": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, wende ab Dein Angesicht,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, wende ab Dein Angesicht,", "tokens": ["O", ",", "wen\u00b7de", "ab", "Dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das thr\u00e4nenfeuchte, schmerzenbleiche,", "tokens": ["Das", "thr\u00e4\u00b7nen\u00b7feuch\u00b7te", ",", "schmer\u00b7zen\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Thr\u00e4nen wecken Todte nicht,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "we\u00b7cken", "Tod\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Du knieest hier vor einer Leiche.", "tokens": ["Und", "Du", "kni\u00b7eest", "hier", "vor", "ei\u00b7ner", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Fleh' nicht mit gellem Jammerschrei:", "tokens": ["Fleh'", "nicht", "mit", "gel\u00b7lem", "Jam\u00b7mer\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "\u00bbnur eine Stunde soll sie leben!\u00ab", "tokens": ["\u00bb", "nur", "ei\u00b7ne", "Stun\u00b7de", "soll", "sie", "le\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist vorbei, \u2013 es ist vorbei \u2013", "tokens": ["Es", "ist", "vor\u00b7bei", ",", "\u2013", "es", "ist", "vor\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "$(", "PPER", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das f\u00fchlst Du durch die Seele beben.", "tokens": ["Das", "f\u00fchlst", "Du", "durch", "die", "See\u00b7le", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du suchtest Freude hier und Lust,", "tokens": ["Du", "such\u00b7test", "Freu\u00b7de", "hier", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der todten Jugend s\u00fc\u00dfe Namen;", "tokens": ["Der", "tod\u00b7ten", "Ju\u00b7gend", "s\u00fc\u00b7\u00dfe", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "O Mann! \u2013 schau' in die \u00f6de Brust \u2013", "tokens": ["O", "Mann", "!", "\u2013", "schau'", "in", "die", "\u00f6\u00b7de", "Brust", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und Du verstehst mein \u00bbNein,\u00ab mein \u00bbAmen!\u00ab", "tokens": ["Und", "Du", "ver\u00b7stehst", "mein", "\u00bb", "Nein", ",", "\u00ab", "mein", "\u00bb", "A\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "$(", "PTKANT", "$,", "$(", "PPOSAT", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}