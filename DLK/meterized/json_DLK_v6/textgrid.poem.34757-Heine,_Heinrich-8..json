{"textgrid.poem.34757": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graue Nacht liegt auf dem Meere,", "tokens": ["Grau\u00b7e", "Nacht", "liegt", "auf", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die kleinen Sterne glimmen.", "tokens": ["Und", "die", "klei\u00b7nen", "Ster\u00b7ne", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchmal t\u00f6nen in dem Wasser", "tokens": ["Manch\u00b7mal", "t\u00f6\u00b7nen", "in", "dem", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange hingezogne Stimmen.", "tokens": ["Lan\u00b7ge", "hin\u00b7ge\u00b7zog\u00b7ne", "Stim\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dorten spielt der alte Nordwind", "tokens": ["Dor\u00b7ten", "spielt", "der", "al\u00b7te", "Nord\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den blanken Meereswellen,", "tokens": ["Mit", "den", "blan\u00b7ken", "Mee\u00b7res\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die wie Orgelpfeifen h\u00fcpfen,", "tokens": ["Die", "wie", "Or\u00b7gel\u00b7pfei\u00b7fen", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die wie Orgelpfeifen schwellen.", "tokens": ["Die", "wie", "Or\u00b7gel\u00b7pfei\u00b7fen", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heidnisch halb und halb auch kirchlich", "tokens": ["Heid\u00b7nisch", "halb", "und", "halb", "auch", "kirch\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingen diese Melodeien,", "tokens": ["Klin\u00b7gen", "die\u00b7se", "Me\u00b7lo\u00b7dei\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigen mutig in die H\u00f6he,", "tokens": ["Stei\u00b7gen", "mu\u00b7tig", "in", "die", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sich drob die Sterne freuen.", "tokens": ["Da\u00df", "sich", "drob", "die", "Ster\u00b7ne", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Sterne, immer gr\u00f6\u00dfer,", "tokens": ["Und", "die", "Ster\u00b7ne", ",", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fchen auf mit Lustgewimmel,", "tokens": ["Gl\u00fc\u00b7hen", "auf", "mit", "Lust\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am Ende gro\u00df wie Sonnen", "tokens": ["Und", "am", "En\u00b7de", "gro\u00df", "wie", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJD", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweifen sie umher am Himmel.", "tokens": ["Schwei\u00b7fen", "sie", "um\u00b7her", "am", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zur Musik, die unten t\u00f6net,", "tokens": ["Zur", "Mu\u00b7sik", ",", "die", "un\u00b7ten", "t\u00f6\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wirbeln sie die tollsten Weisen;", "tokens": ["Wir\u00b7beln", "sie", "die", "tolls\u00b7ten", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sonnennachtigallen sind es,", "tokens": ["Son\u00b7nen\u00b7nach\u00b7ti\u00b7gal\u00b7len", "sind", "es", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dort oben strahlend kreisen.", "tokens": ["Die", "dort", "o\u00b7ben", "strah\u00b7lend", "krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und das braust und schmettert m\u00e4chtig,", "tokens": ["Und", "das", "braust", "und", "schmet\u00b7tert", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meer und Himmel h\u00f6r ich singen,", "tokens": ["Meer", "und", "Him\u00b7mel", "h\u00f6r", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich f\u00fchle Riesenwollust", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "Rie\u00b7sen\u00b7wol\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrmisch in mein Herze dringen.", "tokens": ["St\u00fcr\u00b7misch", "in", "mein", "Her\u00b7ze", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}