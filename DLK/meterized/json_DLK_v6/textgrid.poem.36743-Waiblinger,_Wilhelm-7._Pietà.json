{"textgrid.poem.36743": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "7. Piet\u00e0", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine treffliche That des edeln f\u00fchlenden Herzens,", "tokens": ["Ei\u00b7ne", "treff\u00b7li\u00b7che", "That", "des", "e\u00b7deln", "f\u00fch\u00b7len\u00b7den", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wenn sie auf blumigem Weg schon in den Himmel uns f\u00fchrt,", "tokens": ["Wenn", "sie", "auf", "blu\u00b7mi\u00b7gem", "Weg", "schon", "in", "den", "Him\u00b7mel", "uns", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann, Canova, hat dich dies einz'ge unsterbliche Bildwerk", "tokens": ["Dann", ",", "Ca\u00b7no\u00b7va", ",", "hat", "dich", "dies", "einz'\u00b7ge", "uns\u00b7terb\u00b7li\u00b7che", "Bild\u00b7werk"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$,", "VAFIN", "PPER", "PDS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch aus des Irrthums Gewalt in den Olympus gef\u00fchrt.", "tokens": ["Auch", "aus", "des", "Irr\u00b7thums", "Ge\u00b7walt", "in", "den", "O\u00b7lym\u00b7pus", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "APPR", "ART", "NE", "VVPP", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}