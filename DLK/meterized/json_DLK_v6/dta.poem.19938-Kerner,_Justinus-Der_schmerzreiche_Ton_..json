{"dta.poem.19938": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der schmerzreiche Ton .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wehlaut aus dem Todtenzimmer,", "tokens": ["Wehl\u00b7aut", "aus", "dem", "Tod\u00b7ten\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Glockenklang, der Sch\u00fcler Chor,", "tokens": ["Glo\u00b7cken\u00b7klang", ",", "der", "Sch\u00fc\u00b7ler", "Chor", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das sind T\u00f6ne wohl, die immer", "tokens": ["Das", "sind", "T\u00f6\u00b7ne", "wohl", ",", "die", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "ADV", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmerzreich dringen mir in's Ohr.", "tokens": ["Schmer\u00b7zreich", "drin\u00b7gen", "mir", "in's", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ein Ton im Haus der Leiche", "tokens": ["Doch", "ein", "Ton", "im", "Haus", "der", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringet mir vor allen Schmerz,", "tokens": ["Brin\u00b7get", "mir", "vor", "al\u00b7len", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ton, bei dem ich stets erbleiche,", "tokens": ["Ton", ",", "bei", "dem", "ich", "stets", "er\u00b7blei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ton, der mir zerrei\u00dft das Herz.", "tokens": ["Ton", ",", "der", "mir", "zer\u00b7rei\u00dft", "das", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ton aus stiller Todtenkammer", "tokens": ["Ton", "aus", "stil\u00b7ler", "Tod\u00b7ten\u00b7kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Mensch im Leichenschrein \u2014", "tokens": ["Wo", "der", "Mensch", "im", "Lei\u00b7chen\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann der Tischler mit dem Hammer,", "tokens": ["Wann", "der", "Tischler", "mit", "dem", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schl\u00e4gt den ersten Nagel ein.", "tokens": ["Schl\u00e4gt", "den", "ers\u00b7ten", "Na\u00b7gel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}