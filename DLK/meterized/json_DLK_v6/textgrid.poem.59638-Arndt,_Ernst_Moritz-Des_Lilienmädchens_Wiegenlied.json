{"textgrid.poem.59638": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Des Lilienm\u00e4dchens Wiegenlied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlafe, Kindlein hold und wei\u00df,", "tokens": ["Schla\u00b7fe", ",", "Kin\u00b7dlein", "hold", "und", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das noch nichts von Sorgen wei\u00df,", "tokens": ["Das", "noch", "nichts", "von", "Sor\u00b7gen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlaf in stiller s\u00fc\u00dfer Ruh',", "tokens": ["Schlaf", "in", "stil\u00b7ler", "s\u00fc\u00b7\u00dfer", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tu die kleinen \u00c4uglein zu.", "tokens": ["Tu", "die", "klei\u00b7nen", "\u00c4ug\u00b7lein", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen stehn die Lilien wei\u00df,", "tokens": ["Drau\u00b7\u00dfen", "stehn", "die", "Li\u00b7li\u00b7en", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Haben allersch\u00f6nsten Preis,", "tokens": ["Ha\u00b7ben", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Preis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Droben in der lichten H\u00f6h'", "tokens": ["Dro\u00b7ben", "in", "der", "lich\u00b7ten", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Stehn die Englein wei\u00df wie Schnee.", "tokens": ["Stehn", "die", "En\u00b7glein", "wei\u00df", "wie", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kommt, ihr Englein wei\u00df und fein,", "tokens": ["Kommt", ",", "ihr", "En\u00b7glein", "wei\u00df", "und", "fein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiegt mir sch\u00f6n mein Kindelein,", "tokens": ["Wiegt", "mir", "sch\u00f6n", "mein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiegt sein Herzchen fromm und gut,", "tokens": ["Wiegt", "sein", "Herz\u00b7chen", "fromm", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Wind der Lilie tut.", "tokens": ["Wie", "der", "Wind", "der", "Li\u00b7lie", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlafe, Kindlein, schlafe nun!", "tokens": ["Schla\u00b7fe", ",", "Kin\u00b7dlein", ",", "schla\u00b7fe", "nun", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollst in Gottes Frieden ruhn;", "tokens": ["Sollst", "in", "Got\u00b7tes", "Frie\u00b7den", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die frommen Engelein", "tokens": ["Denn", "die", "from\u00b7men", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen deine W\u00e4chter sein.", "tokens": ["Wol\u00b7len", "dei\u00b7ne", "W\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}