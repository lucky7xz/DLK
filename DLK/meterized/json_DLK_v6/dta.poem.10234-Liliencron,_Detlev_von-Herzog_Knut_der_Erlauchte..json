{"dta.poem.10234": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Knut der Erlauchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "In Schleswig h\u00e4lt seinen Hof Herzog Knut,", "tokens": ["In", "Schles\u00b7wig", "h\u00e4lt", "sei\u00b7nen", "Hof", "Her\u00b7zog", "Knut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "NE", "NE", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Sein Gelbhaar quillt aus dem Eisenhut,", "tokens": ["Sein", "Gelb\u00b7haar", "quillt", "aus", "dem", "Ei\u00b7sen\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Den Frieden gab er, da\u00df Jeder schlief", "tokens": ["Den", "Frie\u00b7den", "gab", "er", ",", "da\u00df", "Je\u00b7der", "schlief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der \u00c4rmste selbst hatte Siegel und Brief,", "tokens": ["Der", "\u00c4rms\u00b7te", "selbst", "hat\u00b7te", "Sie\u00b7gel", "und", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Zart lag in seinem Arm stahlhart", "tokens": ["Zart", "lag", "in", "sei\u00b7nem", "Arm", "stahl\u00b7hart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jubelnd patscht nach dem langen Bart", "tokens": ["Und", "ju\u00b7belnd", "patscht", "nach", "dem", "lan\u00b7gen", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Im Winter elf Hundert drei\u00dfig und ein,", "tokens": ["Im", "Win\u00b7ter", "elf", "Hun\u00b7dert", "drei\u00b7\u00dfig", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "CARD", "CARD", "CARD", "KON", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Ritter sprengt ins Thor herein,", "tokens": ["Ein", "Rit\u00b7ter", "sprengt", "ins", "Thor", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "K\u00f6nig Magnus schrieb: Es treibt mich fort,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", "schrieb", ":", "Es", "treibt", "mich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Herzog Knut gieb mir dein F\u00fcrstenwort,", "tokens": ["Her\u00b7zog", "Knut", "gieb", "mir", "dein", "F\u00fcrs\u00b7ten\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Herzog nahm Abschied. Sein Auge blau", "tokens": ["Der", "Her\u00b7zog", "nahm", "Ab\u00b7schied", ".", "Sein", "Au\u00b7ge", "blau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "PPOSAT", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jens Wohnsfleth und Iven Reventlow", "tokens": ["Jens", "Wohns\u00b7fleth", "und", "I\u00b7ven", "Re\u00b7vent\u00b7low"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und als er kam in Roeskilde Ort,", "tokens": ["Und", "als", "er", "kam", "in", "Ro\u00b7es\u00b7kil\u00b7de", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die B\u00e4ume raunen von Frevel und Mord,", "tokens": ["Die", "B\u00e4u\u00b7me", "rau\u00b7nen", "von", "Fre\u00b7vel", "und", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Acht Tage war Jagd und Trinken und Tanz,", "tokens": ["Acht", "Ta\u00b7ge", "war", "Jagd", "und", "Trin\u00b7ken", "und", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und als genug der Firlefanz,", "tokens": ["Und", "als", "ge\u00b7nug", "der", "Fir\u00b7le\u00b7fanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201edie Weiber horchen an Vorhang und Spalt,", "tokens": ["\u201e", "die", "Wei\u00b7ber", "hor\u00b7chen", "an", "Vor\u00b7hang", "und", "Spalt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00df gehen uns in den dunklen Wald,", "tokens": ["La\u00df", "ge\u00b7hen", "uns", "in", "den", "dunk\u00b7len", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Wie war der Wald so wei\u00df und still,", "tokens": ["Wie", "war", "der", "Wald", "so", "wei\u00df", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern von der Weltesche Yggdrasil", "tokens": ["Fern", "von", "der", "Wel\u00b7te\u00b7sche", "Ygg\u00b7dra\u00b7sil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Tuk Ebbson, der Bote, sang vor sich hin,", "tokens": ["Tuk", "Eb\u00b7bson", ",", "der", "Bo\u00b7te", ",", "sang", "vor", "sich", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und leise sang er vor sich hin,", "tokens": ["Und", "lei\u00b7se", "sang", "er", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Herzog h\u00f6rt nicht. Mit fr\u00f6hlichem Sinn", "tokens": ["Der", "Her\u00b7zog", "h\u00f6rt", "nicht", ".", "Mit", "fr\u00f6h\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PTKNEG", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tuk Ebbson, der Bote, singt vor sich hin,", "tokens": ["Tuk", "Eb\u00b7bson", ",", "der", "Bo\u00b7te", ",", "singt", "vor", "sich", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "K\u00f6nig Magnus sitzt auf dem Eichenstumpf,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", "sitzt", "auf", "dem", "Ei\u00b7chen\u00b7stumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Unterm B\u00e4renpelz und Wolffellstrumpf", "tokens": ["Un\u00b7term", "B\u00e4\u00b7ren\u00b7pelz", "und", "Wolf\u00b7fell\u00b7strumpf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Auf springt er, als er den Herzog erschaut,", "tokens": ["Auf", "springt", "er", ",", "als", "er", "den", "Her\u00b7zog", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er k\u00fc\u00dft ihn auf die Lippen traut,", "tokens": ["Er", "k\u00fc\u00dft", "ihn", "auf", "die", "Lip\u00b7pen", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Dann tritt er zur\u00fcck und klatscht in die Hand,", "tokens": ["Dann", "tritt", "er", "zu\u00b7r\u00fcck", "und", "klatscht", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und an der Waldbl\u00f6\u00dfe lichten Rand", "tokens": ["Und", "an", "der", "Wald\u00b7bl\u00f6\u00b7\u00dfe", "lich\u00b7ten", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "\u201enun soll es sich zeigen, beim heiligen Christ,", "tokens": ["\u201e", "nun", "soll", "es", "sich", "zei\u00b7gen", ",", "beim", "hei\u00b7li\u00b7gen", "Christ", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dem Herzog lie\u00df er keine Frist,", "tokens": ["Dem", "Her\u00b7zog", "lie\u00df", "er", "kei\u00b7ne", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und schlug ihn tot. Der Herzog fiel", "tokens": ["Und", "schlug", "ihn", "tot", ".", "Der", "Her\u00b7zog", "fiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der K\u00f6nig trocknet Axt und Stiel,", "tokens": ["Der", "K\u00f6\u00b7nig", "trock\u00b7net", "Axt", "und", "Stiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Wie war der Wald so wei\u00df und still,", "tokens": ["Wie", "war", "der", "Wald", "so", "wei\u00df", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern von der Weltesche Yggdrasil", "tokens": ["Fern", "von", "der", "Wel\u00b7te\u00b7sche", "Ygg\u00b7dra\u00b7sil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}