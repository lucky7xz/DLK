{"textgrid.poem.40187": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein silbern Herze,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein silbern Herze,", "tokens": ["Ein", "sil\u00b7bern", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "von Golde ein'n Ring", "tokens": ["von", "Gol\u00b7de", "ein'n", "Ring"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "die Liebste mir gab,", "tokens": ["die", "Liebs\u00b7te", "mir", "gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "als zum Scheiden es ging;", "tokens": ["als", "zum", "Schei\u00b7den", "es", "ging", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "und that in das Herze", "tokens": ["und", "that", "in", "das", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "ihr Bild hinein, \u2013", "tokens": ["ihr", "Bild", "hin\u00b7ein", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "so einsam der Morgen,", "tokens": ["so", "ein\u00b7sam", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "bin ", "tokens": ["bin"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Arm Fr\u00f6schlein im Gleise,", "tokens": ["Arm", "Fr\u00f6schlein", "im", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "zermalmt liegst du!", "tokens": ["zer\u00b7malmt", "liegst", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich wandre meine Stra\u00dfe", "tokens": ["Ich", "wand\u00b7re", "mei\u00b7ne", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und wandre immer zu ...", "tokens": ["und", "wand\u00b7re", "im\u00b7mer", "zu", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon teilt sich der Nebel,", "tokens": ["Schon", "teilt", "sich", "der", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "ergl\u00e4nzet die Welt,", "tokens": ["er\u00b7gl\u00e4n\u00b7zet", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "im Sonnenschein glitzert", "tokens": ["im", "Son\u00b7nen\u00b7schein", "glit\u00b7zert"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "das Aehrenfeld;", "tokens": ["das", "A\u00b7eh\u00b7ren\u00b7feld", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "die Hummeln summen,", "tokens": ["die", "Hum\u00b7meln", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "die Lerchen singen;", "tokens": ["die", "Ler\u00b7chen", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "die Birken wehend", "tokens": ["die", "Bir\u00b7ken", "we\u00b7hend"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "die Zweige schwingen;", "tokens": ["die", "Zwei\u00b7ge", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "die Pappeln, die sch\u00fctteln,", "tokens": ["die", "Pap\u00b7peln", ",", "die", "sch\u00fct\u00b7teln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "die Bl\u00e4tter im Wind;", "tokens": ["die", "Bl\u00e4t\u00b7ter", "im", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "sie raunen, sie lispeln", "tokens": ["sie", "rau\u00b7nen", ",", "sie", "lis\u00b7peln"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "von meinem fernen Kind.", "tokens": ["von", "mei\u00b7nem", "fer\u00b7nen", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Will nehmen das Herzlein", "tokens": ["Will", "neh\u00b7men", "das", "Herz\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "vom seidenen Band \u2013", "tokens": ["vom", "sei\u00b7de\u00b7nen", "Band", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "und leg's in das Ringlein", "tokens": ["und", "leg's", "in", "das", "Rin\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "in meiner Hand, \u2013", "tokens": ["in", "mei\u00b7ner", "Hand", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "so schreit' ich und schau'", "tokens": ["so", "schreit'", "ich", "und", "schau'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "als ein ", "tokens": ["als", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "so halt' ich in Treuen", "tokens": ["so", "halt'", "ich", "in", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "ohn' Ende Dich umfahn ...", "tokens": ["ohn'", "En\u00b7de", "Dich", "um\u00b7fahn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Bleib sitzen nur, H\u00e4slein!", "tokens": ["Bleib", "sit\u00b7zen", "nur", ",", "H\u00e4s\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "heut jag' ich dich nicht ...", "tokens": ["heut", "jag'", "ich", "dich", "nicht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich wandre, ich schreite;", "tokens": ["Ich", "wand\u00b7re", ",", "ich", "schrei\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "die Sonne sticht ...", "tokens": ["die", "Son\u00b7ne", "sticht", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "In Dorfes Mitten", "tokens": ["In", "Dor\u00b7fes", "Mit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "der Friedhof sich hebt;", "tokens": ["der", "Fried\u00b7hof", "sich", "hebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "wie wird's gar k\u00fchl sich ruhen,", "tokens": ["wie", "wird's", "gar", "k\u00fchl", "sich", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wenn man mich einst begr\u00e4bt!", "tokens": ["wenn", "man", "mich", "einst", "be\u00b7gr\u00e4bt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "zwei wei\u00dfe Rosen biegen", "tokens": ["zwei", "wei\u00b7\u00dfe", "Ro\u00b7sen", "bie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ums Grabkreuz die Aest',", "tokens": ["ums", "Grab\u00b7kreuz", "die", "A\u00b7est'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "drauf steht mein Nam' geschrieben, \u2013", "tokens": ["drauf", "steht", "mein", "Nam'", "ge\u00b7schrie\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "bis der Regen ihn l\u00f6scht ...", "tokens": ["bis", "der", "Re\u00b7gen", "ihn", "l\u00f6scht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Hinterm Kirchlein die Schenke", "tokens": ["Hin\u00b7term", "Kirch\u00b7lein", "die", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "hei\u00dft \u00bbZu den drei Linden\u00ab;", "tokens": ["hei\u00dft", "\u00bb", "Zu", "den", "drei", "Lin\u00b7den", "\u00ab", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$(", "APPR", "ART", "CARD", "NN", "$(", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "da wird zum ", "tokens": ["da", "wird", "zum"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "wohl ", "tokens": ["wohl"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}, "stanza.13": {"line.1": {"text": "Ei Tausend, Frau Wirtin,", "tokens": ["Ei", "Tau\u00b7send", ",", "Frau", "Wir\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Euer T\u00f6chterlein das?!", "tokens": ["Eu\u00b7er", "T\u00f6ch\u00b7ter\u00b7lein", "das", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ei, f\u00fcllt mir geschwinde", "tokens": ["Ei", ",", "f\u00fcllt", "mir", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJA"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "noch Einmal das Glas! \u2013 \u2013", "tokens": ["noch", "Ein\u00b7mal", "das", "Glas", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Was wackelt der Pfahl da?", "tokens": ["Was", "wa\u00b7ckelt", "der", "Pfahl", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "der ist wohl betrunken \u2013!", "tokens": ["der", "ist", "wohl", "be\u00b7trun\u00b7ken", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "VAFIN", "ADV", "VVINF", "$(", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich wandre, ich schreite,", "tokens": ["Ich", "wand\u00b7re", ",", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "in Sinnen versunken.", "tokens": ["in", "Sin\u00b7nen", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Wir war'n ja so alleine,", "tokens": ["Wir", "wa\u00b7r'n", "ja", "so", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "und sie \u2013 war so weit!", "tokens": ["und", "sie", "\u2013", "war", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "ich will ihr ", "tokens": ["ich", "will", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "und ach \u2013 wenn ", "tokens": ["und", "ach", "\u2013", "wenn"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "XY", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.16": {"line.1": {"text": "Und am ", "tokens": ["Und", "am"], "token_info": ["word", "word"], "pos": ["KON", "APPRART"], "meter": "--", "measure": "unknown.measure.zero"}, "line.2": {"text": "steht mein elterlich Haus,", "tokens": ["steht", "mein", "el\u00b7ter\u00b7lich", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "da schaut mein lieb M\u00fctterchen", "tokens": ["da", "schaut", "mein", "lieb", "M\u00fct\u00b7ter\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.4": {"text": "zum Fenster nach mir aus;", "tokens": ["zum", "Fens\u00b7ter", "nach", "mir", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "und drinnen sitzt mein Vater,", "tokens": ["und", "drin\u00b7nen", "sitzt", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wie'n K\u00f6nig auf sein'm Thron,", "tokens": ["wie'n", "K\u00f6\u00b7nig", "auf", "sein'm", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und will's nicht verraten,", "tokens": ["und", "will's", "nicht", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "da\u00df er wart't auf sein'n Sohn ...", "tokens": ["da\u00df", "er", "wart't", "auf", "sein'n", "Sohn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Nun will ich nicht sinnen,", "tokens": ["Nun", "will", "ich", "nicht", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "ob ich gl\u00fccklich kann werden:", "tokens": ["ob", "ich", "gl\u00fcck\u00b7lich", "kann", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "ich hab' ja die Liebe", "tokens": ["ich", "hab'", "ja", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "und 'ne Heimat auf Erden!", "tokens": ["und", "'ne", "Hei\u00b7mat", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}