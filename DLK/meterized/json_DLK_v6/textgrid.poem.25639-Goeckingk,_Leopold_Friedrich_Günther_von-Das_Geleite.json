{"textgrid.poem.25639": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Das Geleite", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, Kinder! sprach ein F\u00fcrst zum edlen Rath,", "tokens": ["Nun", ",", "Kin\u00b7der", "!", "sprach", "ein", "F\u00fcrst", "zum", "ed\u00b7len", "Rath", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ihn begleitete, nicht weiter sollt ihr reiten!", "tokens": ["Der", "ihn", "be\u00b7glei\u00b7te\u00b7te", ",", "nicht", "wei\u00b7ter", "sollt", "ihr", "rei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Worauf der Magistrat", "tokens": ["Wo\u00b7rauf", "der", "Ma\u00b7gist\u00b7rat"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur noch um die Erlaubni\u00df bat,", "tokens": ["Nur", "noch", "um", "die", "Er\u00b7laub\u00b7ni\u00df", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn bis zum Rabensteine zu begleiten.", "tokens": ["Ihn", "bis", "zum", "Ra\u00b7ben\u00b7stei\u00b7ne", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}