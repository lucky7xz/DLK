{"textgrid.poem.38751": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "8. Sabbath", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel lacht in seiner heitern Bl\u00e4ue,", "tokens": ["Der", "Him\u00b7mel", "lacht", "in", "sei\u00b7ner", "hei\u00b7tern", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde gr\u00fcnt in allen ihren Lichten,", "tokens": ["Die", "Er\u00b7de", "gr\u00fcnt", "in", "al\u00b7len", "ih\u00b7ren", "Lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Adler schw\u00e4rmt in der azurnen Freye,", "tokens": ["Der", "Ad\u00b7ler", "schw\u00e4rmt", "in", "der", "a\u00b7zur\u00b7nen", "Frey\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und will den Fittig nach der Sonne richten;", "tokens": ["Und", "will", "den", "Fit\u00b7tig", "nach", "der", "Son\u00b7ne", "rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Mensch empf\u00e4ngt von oben seine Weihe,", "tokens": ["Der", "Mensch", "emp\u00b7f\u00e4ngt", "von", "o\u00b7ben", "sei\u00b7ne", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vom Kreuze nieder will die Seele fl\u00fcchten,", "tokens": ["Vom", "Kreu\u00b7ze", "nie\u00b7der", "will", "die", "See\u00b7le", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der heil'ge Leichnam steigt aus den Gewanden,", "tokens": ["Der", "heil'\u00b7ge", "Leich\u00b7nam", "steigt", "aus", "den", "Ge\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Lieb' ist nun vom Grabe auferstanden.", "tokens": ["Die", "Lieb'", "ist", "nun", "vom", "Gra\u00b7be", "auf\u00b7er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das neue Herz besucht die lichten H\u00f6hen,", "tokens": ["Das", "neu\u00b7e", "Herz", "be\u00b7sucht", "die", "lich\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und findet dorten seine J\u00fcnger wieder;", "tokens": ["Und", "fin\u00b7det", "dor\u00b7ten", "sei\u00b7ne", "J\u00fcn\u00b7ger", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Propheten lassen sich von oben sehen,", "tokens": ["Pro\u00b7phe\u00b7ten", "las\u00b7sen", "sich", "von", "o\u00b7ben", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit Tr\u00f6sten l\u00e4chelnd schauen sie hernieder.", "tokens": ["Mit", "Tr\u00f6s\u00b7ten", "l\u00e4\u00b7chelnd", "schau\u00b7en", "sie", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da sieht man das Panier des Friedens wehen,", "tokens": ["Da", "sieht", "man", "das", "Pa\u00b7nier", "des", "Frie\u00b7dens", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Es singen Cherubim die heil'gen Lieder,", "tokens": ["Es", "sin\u00b7gen", "Che\u00b7ru\u00b7bim", "die", "heil'\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das Kreuz, die Dornenkrone sind verschwunden,", "tokens": ["Das", "Kreuz", ",", "die", "Dor\u00b7nen\u00b7kro\u00b7ne", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Das Morgenroth entstr\u00f6mt den s\u00fcssen Wunden.", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "ent\u00b7str\u00f6mt", "den", "s\u00fcs\u00b7sen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}