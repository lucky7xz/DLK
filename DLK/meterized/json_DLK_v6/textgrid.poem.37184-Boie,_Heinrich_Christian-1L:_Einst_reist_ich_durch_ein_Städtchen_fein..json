{"textgrid.poem.37184": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst reist ich durch ein St\u00e4dtchen fein.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst reist ich durch ein St\u00e4dtchen fein.", "tokens": ["Einst", "reist", "ich", "durch", "ein", "St\u00e4dt\u00b7chen", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00f6ner Morgen. Die Uhr schlug neun.", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Mor\u00b7gen", ".", "Die", "Uhr", "schlug", "neun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "CARD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das St\u00e4dtchen fein wolt ich besehn,", "tokens": ["Das", "St\u00e4dt\u00b7chen", "fein", "wolt", "ich", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hub an Stra\u00df auf Stra\u00df ab zu gehn.", "tokens": ["Hub", "an", "Stra\u00df", "auf", "Stra\u00df", "ab", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Arbeitsam l\u00e4rmt der ganze Ort,", "tokens": ["Ar\u00b7beit\u00b7sam", "l\u00e4rmt", "der", "gan\u00b7ze", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Es h\u00e4mmert hier, es klopfet dort,", "tokens": ["Es", "h\u00e4m\u00b7mert", "hier", ",", "es", "klop\u00b7fet", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der tr\u00e4gt das her, der schleppt das hin:", "tokens": ["Der", "tr\u00e4gt", "das", "her", ",", "der", "schleppt", "das", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "PTKVZ", "$,", "PRELS", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie wohl ward mir dabei zu Sinn!", "tokens": ["Wie", "wohl", "ward", "mir", "da\u00b7bei", "zu", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf gr\u00fcnem Kirchhof blieb ich stehn", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Kirch\u00b7hof", "blieb", "ich", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah ein Haus fast gro\u00df und sch\u00f6n,", "tokens": ["Und", "sah", "ein", "Haus", "fast", "gro\u00df", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Th\u00fcr und Fensterladen zu", "tokens": ["Doch", "Th\u00fcr", "und", "Fens\u00b7ter\u00b7la\u00b7den", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles still in todter Ruh.", "tokens": ["Und", "al\u00b7les", "still", "in", "tod\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich ruf ein M\u00e4del zu mir hin:", "tokens": ["Ich", "ruf", "ein", "M\u00e4\u00b7del", "zu", "mir", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbsolch sch\u00f6nes Haus und niemand drin?\u00ab", "tokens": ["\u00bb", "solch", "sch\u00f6\u00b7nes", "Haus", "und", "nie\u00b7mand", "drin", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "KON", "PIS", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja wohl! was rechts, beim Element!", "tokens": ["Ja", "wohl", "!", "was", "rechts", ",", "beim", "E\u00b7le\u00b7ment", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "PWS", "ADV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwer wohnt denn da?\u00ab \u2013 Der Supperndent.", "tokens": ["\u00bb", "wer", "wohnt", "denn", "da", "?", "\u00ab", "\u2013", "Der", "Sup\u00b7pern\u00b7dent", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ADV", "$.", "$(", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}