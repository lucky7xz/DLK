{"textgrid.poem.58138": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "176. Wie die Speise ist, so ist das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen irdisch sind, sie hungern nach der Erden,", "tokens": ["Die", "Men\u00b7schen", "ir\u00b7disch", "sind", ",", "sie", "hun\u00b7gern", "nach", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Seelengrund wird nur mit finstrer Qual gef\u00fcllt;", "tokens": ["Ihr", "See\u00b7len\u00b7grund", "wird", "nur", "mit", "finst\u00b7rer", "Qual", "ge\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach hungre nur nach Gott, so wirst du g\u00f6ttlich werden,", "tokens": ["Ach", "hung\u00b7re", "nur", "nach", "Gott", ",", "so", "wirst", "du", "g\u00f6tt\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirst du froh und licht, so wird dein Durst gestillt!", "tokens": ["So", "wirst", "du", "froh", "und", "licht", ",", "so", "wird", "dein", "Durst", "ge\u00b7stillt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}