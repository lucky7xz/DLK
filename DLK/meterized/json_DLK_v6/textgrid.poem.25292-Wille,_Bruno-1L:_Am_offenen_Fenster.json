{"textgrid.poem.25292": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am offenen Fenster", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am offenen Fenster", "tokens": ["Am", "of\u00b7fe\u00b7nen", "Fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein Fl\u00e4mmchen wacht,", "tokens": ["Ein", "Fl\u00e4mm\u00b7chen", "wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es flirrt und flackert", "tokens": ["Es", "flirrt", "und", "fla\u00b7ckert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In wehender Nacht.", "tokens": ["In", "we\u00b7hen\u00b7der", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Windsto\u00df w\u00fcrgt es;", "tokens": ["Ein", "Wind\u00b7sto\u00df", "w\u00fcrgt", "es", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da beugt es sich m\u00fcd,", "tokens": ["Da", "beugt", "es", "sich", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als ob ein blaues", "tokens": ["Als", "ob", "ein", "blau\u00b7es"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bl\u00fcmchen verbl\u00fcht.", "tokens": ["Bl\u00fcm\u00b7chen", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Aus lischt sein Auge;", "tokens": ["Aus", "lischt", "sein", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein letzter Strahl", "tokens": ["Ein", "letz\u00b7ter", "Strahl"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hinan zum heiligen", "tokens": ["Hi\u00b7nan", "zum", "hei\u00b7li\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sternensaal./", "tokens": ["Ster\u00b7nen\u00b7saal", ".", "/"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Arm Flackerseelchen,", "tokens": ["Arm", "Fla\u00b7cker\u00b7seel\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du Bettelkind,", "tokens": ["Du", "Bet\u00b7tel\u00b7kind", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gern w\u00e4rst du worden,", "tokens": ["Gern", "w\u00e4rst", "du", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VAPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was Sterne sind.", "tokens": ["Was", "Ster\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Mu\u00dft nun verspr\u00fchen", "tokens": ["Mu\u00dft", "nun", "ver\u00b7spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In Nacht und Tod.", "tokens": ["In", "Nacht", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jedoch getrost:", "tokens": ["Je\u00b7doch", "ge\u00b7trost", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der Lichtborn loht!", "tokens": ["Der", "Licht\u00b7born", "loht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dein Lichtborn droben,", "tokens": ["Dein", "Licht\u00b7born", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die gl\u00fchenden Sonnen,", "tokens": ["Die", "gl\u00fc\u00b7hen\u00b7den", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dran heilige Sehnsucht", "tokens": ["Dran", "hei\u00b7li\u00b7ge", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dir ist entbronnen.", "tokens": ["Dir", "ist", "ent\u00b7bron\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Und was du liebtest", "tokens": ["Und", "was", "du", "lieb\u00b7test"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In armer Zeit,", "tokens": ["In", "ar\u00b7mer", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dein Reichtum ist es", "tokens": ["Dein", "Reich\u00b7tum", "ist", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In Ewigkeit.", "tokens": ["In", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Der Sternenliebe", "tokens": ["Der", "Ster\u00b7nen\u00b7lie\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ergib dich ganz!", "tokens": ["Er\u00b7gib", "dich", "ganz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So wirst du selber", "tokens": ["So", "wirst", "du", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zu Sternenglanz.", "tokens": ["Zu", "Ster\u00b7nen\u00b7glanz", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}