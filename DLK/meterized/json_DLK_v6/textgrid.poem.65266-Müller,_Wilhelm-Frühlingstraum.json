{"textgrid.poem.65266": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingstraum", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte von bunten Blumen,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "bun\u00b7ten", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wie sie wohl bl\u00fchen im Mai,", "tokens": ["So", "wie", "sie", "wohl", "bl\u00fc\u00b7hen", "im", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich tr\u00e4umte von gr\u00fcnen Wiesen,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "gr\u00fc\u00b7nen", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von lustigem Vogelgeschrei.", "tokens": ["Von", "lus\u00b7ti\u00b7gem", "Vo\u00b7gel\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und als die H\u00e4hne kr\u00e4hten,", "tokens": ["Und", "als", "die", "H\u00e4h\u00b7ne", "kr\u00e4h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ward mein Auge wach;", "tokens": ["Da", "ward", "mein", "Au\u00b7ge", "wach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war es kalt und finster,", "tokens": ["Da", "war", "es", "kalt", "und", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schrieen die Raben vom Dach.", "tokens": ["Es", "schri\u00b7een", "die", "Ra\u00b7ben", "vom", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Doch an den Fensterscheiben", "tokens": ["Doch", "an", "den", "Fens\u00b7ter\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer malte die Bl\u00e4tter da?", "tokens": ["Wer", "mal\u00b7te", "die", "Bl\u00e4t\u00b7ter", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr lacht wohl \u00fcber den Tr\u00e4umer,", "tokens": ["Ihr", "lacht", "wohl", "\u00fc\u00b7ber", "den", "Tr\u00e4u\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Blumen im Winter sah?", "tokens": ["Der", "Blu\u00b7men", "im", "Win\u00b7ter", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich tr\u00e4umte von Lieb' um Liebe,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "von", "Lieb'", "um", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von einer sch\u00f6nen Maid,", "tokens": ["Von", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Herzen und von K\u00fcssen,", "tokens": ["Von", "Her\u00b7zen", "und", "von", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Wonn' und Seligkeit.", "tokens": ["Von", "Wonn'", "und", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als die H\u00e4hne kr\u00e4hten,", "tokens": ["Und", "als", "die", "H\u00e4h\u00b7ne", "kr\u00e4h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ward mein Herze wach;", "tokens": ["Da", "ward", "mein", "Her\u00b7ze", "wach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun sitz' ich hier alleine", "tokens": ["Nun", "sitz'", "ich", "hier", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und denke dem Traume nach.", "tokens": ["Und", "den\u00b7ke", "dem", "Trau\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Augen schlie\u00df' ich wieder,", "tokens": ["Die", "Au\u00b7gen", "schlie\u00df'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch schl\u00e4gt das Herz so warm.", "tokens": ["Noch", "schl\u00e4gt", "das", "Herz", "so", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann gr\u00fcnt ihr Bl\u00e4tter am Fenster?", "tokens": ["Wann", "gr\u00fcnt", "ihr", "Bl\u00e4t\u00b7ter", "am", "Fens\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wann halt' ich dich, Liebchen, im Arm?", "tokens": ["Wann", "halt'", "ich", "dich", ",", "Lieb\u00b7chen", ",", "im", "Arm", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}