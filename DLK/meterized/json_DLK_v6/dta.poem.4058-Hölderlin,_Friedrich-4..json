{"dta.poem.4058": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aber wir, zufrieden gesellt, wie die liebenden", "tokens": ["A\u00b7ber", "wir", ",", "zu\u00b7frie\u00b7den", "ge\u00b7sellt", ",", "wie", "die", "lie\u00b7ben\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJD", "VVPP", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schw\u00e4ne,", "tokens": ["Schw\u00e4\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wenn sie ruhen am See, oder auf Wellen gewiegt,", "tokens": ["Wenn", "sie", "ru\u00b7hen", "am", "See", ",", "o\u00b7der", "auf", "Wel\u00b7len", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Niedersehn in die Wasser, wo silberne Wolken sich", "tokens": ["Nie\u00b7der\u00b7sehn", "in", "die", "Was\u00b7ser", ",", "wo", "sil\u00b7ber\u00b7ne", "Wol\u00b7ken", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "PRF"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "spiegeln,", "tokens": ["spie\u00b7geln", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und \u00e4therisches Blau unter den Schiffenden wallt,", "tokens": ["Und", "\u00e4t\u00b7he\u00b7ri\u00b7sches", "Blau", "un\u00b7ter", "den", "Schif\u00b7fen\u00b7den", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "So auf Erden wandelten wir. Und drohte der", "tokens": ["So", "auf", "Er\u00b7den", "wan\u00b7del\u00b7ten", "wir", ".", "Und", "droh\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "$.", "KON", "VVFIN", "ART"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nord auch,", "tokens": ["Nord", "auch", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Er, der Liebenden Feind, klagenbereitend, und", "tokens": ["Er", ",", "der", "Lie\u00b7ben\u00b7den", "Feind", ",", "kla\u00b7gen\u00b7be\u00b7rei\u00b7tend", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$,", "VVPP", "$,", "KON"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "fiel", "tokens": ["fiel"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Von den Aesten das Laub, und flog im Winde der", "tokens": ["Von", "den", "A\u00b7es\u00b7ten", "das", "Laub", ",", "und", "flog", "im", "Win\u00b7de", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "ART"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.12": {"text": "Regen,", "tokens": ["Re\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ruhig l\u00e4chelten wir, f\u00fchlten den eigenen Gott", "tokens": ["Ru\u00b7hig", "l\u00e4\u00b7chel\u00b7ten", "wir", ",", "f\u00fchl\u00b7ten", "den", "ei\u00b7ge\u00b7nen", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Unter trautem Gespr\u00e4ch, in Einem Seelengesange,", "tokens": ["Un\u00b7ter", "trau\u00b7tem", "Ge\u00b7spr\u00e4ch", ",", "in", "Ei\u00b7nem", "See\u00b7len\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Ganz in Frieden mit uns kindlich und freudig", "tokens": ["Ganz", "in", "Frie\u00b7den", "mit", "uns", "kind\u00b7lich", "und", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "allein.", "tokens": ["al\u00b7lein", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Aber das Haus ist \u00f6de mir nun, und sie haben", "tokens": ["A\u00b7ber", "das", "Haus", "ist", "\u00f6\u00b7de", "mir", "nun", ",", "und", "sie", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "PPER", "ADV", "$,", "KON", "PPER", "VAFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.18": {"text": "mein Auge", "tokens": ["mein", "Au\u00b7ge"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Mir genommen, auch mich hab' ich verloren mit", "tokens": ["Mir", "ge\u00b7nom\u00b7men", ",", "auch", "mich", "hab'", "ich", "ver\u00b7lo\u00b7ren", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "ADV", "PPER", "VAFIN", "PPER", "VVPP", "APPR"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "ihr.", "tokens": ["ihr", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.21": {"text": "Darum irr' ich umher und wohl, wie die Schatten,", "tokens": ["Da\u00b7rum", "irr'", "ich", "um\u00b7her", "und", "wohl", ",", "wie", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "so mu\u00df ich", "tokens": ["so", "mu\u00df", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Leben, und sinnlos d\u00fcnkt lange das Uebrige mir.", "tokens": ["Le\u00b7ben", ",", "und", "sinn\u00b7los", "d\u00fcnkt", "lan\u00b7ge", "das", "Ueb\u00b7ri\u00b7ge", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}