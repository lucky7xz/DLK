{"textgrid.poem.67183": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wird des Himmels Vater schauen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird des Himmels Vater schauen", "tokens": ["Wie", "wird", "des", "Him\u00b7mels", "Va\u00b7ter", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Freude das erwachsne Kind,", "tokens": ["Mit", "Freu\u00b7de", "das", "er\u00b7wachs\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gehend auf blumenreichen Auen,", "tokens": ["Ge\u00b7hend", "auf", "blu\u00b7men\u00b7rei\u00b7chen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit andern, welche lieb ihm sind.", "tokens": ["Mit", "an\u00b7dern", ",", "wel\u00b7che", "lieb", "ihm", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Indessen freue dich des Lebens,", "tokens": ["In\u00b7des\u00b7sen", "freu\u00b7e", "dich", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus einer guten Seele kommt", "tokens": ["Aus", "ei\u00b7ner", "gu\u00b7ten", "See\u00b7le", "kommt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sch\u00f6nheit herrlichen Bestrebens,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "herr\u00b7li\u00b7chen", "Be\u00b7stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6ttlicher Grund dir mehr noch frommt.", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Grund", "dir", "mehr", "noch", "frommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}