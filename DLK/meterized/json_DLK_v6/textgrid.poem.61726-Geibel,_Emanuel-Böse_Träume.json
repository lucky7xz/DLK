{"textgrid.poem.61726": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "B\u00f6se Tr\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lie\u00df mein R\u00f6\u00dflein grasen", "tokens": ["Ich", "lie\u00df", "mein", "R\u00f6\u00df\u00b7lein", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wald an Baches Rand", "tokens": ["Im", "Wald", "an", "Ba\u00b7ches", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lag auf k\u00fchlem Rasen", "tokens": ["Und", "lag", "auf", "k\u00fch\u00b7lem", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dacht' ans Vaterland.", "tokens": ["Und", "dacht'", "ans", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bei des Baches Rinnen", "tokens": ["Und", "bei", "des", "Ba\u00b7ches", "Rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Entschlief ich unterm Baum;", "tokens": ["Ent\u00b7schlief", "ich", "un\u00b7term", "Baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da wob vor meinen Sinnen", "tokens": ["Da", "wob", "vor", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein dreifach Bild der Traum.", "tokens": ["Ein", "drei\u00b7fach", "Bild", "der", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sah ein Volk von Immen,", "tokens": ["Ich", "sah", "ein", "Volk", "von", "Im\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ohne Weisel fuhr", "tokens": ["Das", "oh\u00b7ne", "Wei\u00b7sel", "fuhr"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit verworrnen Stimmen", "tokens": ["Und", "mit", "ver\u00b7worr\u00b7nen", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinschw\u00e4rmte durch die Flur.", "tokens": ["Hin\u00b7schw\u00e4rm\u00b7te", "durch", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach allen Winden zogen", "tokens": ["Nach", "al\u00b7len", "Win\u00b7den", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie ziellos kreuz und quer", "tokens": ["Sie", "ziel\u00b7los", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hatten sich bald verflogen", "tokens": ["Und", "hat\u00b7ten", "sich", "bald", "ver\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und fanden sich nimmermehr.", "tokens": ["Und", "fan\u00b7den", "sich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich sah ein B\u00fcndel Pfeile", "tokens": ["Ich", "sah", "ein", "B\u00fcn\u00b7del", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In bl\u00f6der Knaben Hand,", "tokens": ["In", "bl\u00f6\u00b7der", "Kna\u00b7ben", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die trieben kurze Weile", "tokens": ["Die", "trie\u00b7ben", "kur\u00b7ze", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00f6sten Ring und Band.", "tokens": ["Und", "l\u00f6s\u00b7ten", "Ring", "und", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie spielten mit den Rohren", "tokens": ["Sie", "spiel\u00b7ten", "mit", "den", "Roh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Uneins und ungeschickt;", "tokens": ["Un\u00b7eins", "und", "un\u00b7ge\u00b7schickt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die H\u00e4lfte ging verloren,", "tokens": ["Die", "H\u00e4lf\u00b7te", "ging", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die H\u00e4lfte ward zerknickt.", "tokens": ["Die", "H\u00e4lf\u00b7te", "ward", "zer\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sah, wie ein Karfunkel", "tokens": ["Ich", "sah", ",", "wie", "ein", "Kar\u00b7fun\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschm\u00e4ht am Kreuzweg lag;", "tokens": ["Ver\u00b7schm\u00e4ht", "am", "Kreuz\u00b7weg", "lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Staube war er dunkel,", "tokens": ["Von", "Stau\u00b7be", "war", "er", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerspellt von Sto\u00df und Schlag.", "tokens": ["Zer\u00b7spellt", "von", "Sto\u00df", "und", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Krone der Welt zu schm\u00fccken", "tokens": ["Die", "Kro\u00b7ne", "der", "Welt", "zu", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Geschaffen deucht' er mir;", "tokens": ["Ge\u00b7schaf\u00b7fen", "deucht'", "er", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun haschte nach den St\u00fccken", "tokens": ["Nun", "haschte", "nach", "den", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der fremden Raben Gier.", "tokens": ["Der", "frem\u00b7den", "Ra\u00b7ben", "Gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da wacht' ich auf beklommen", "tokens": ["Da", "wacht'", "ich", "auf", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stieg zu Ro\u00df in Hast;", "tokens": ["Und", "stieg", "zu", "Ro\u00df", "in", "Hast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne war verglommen,", "tokens": ["Die", "Son\u00b7ne", "war", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Sp\u00e4trot war verbla\u00dft,", "tokens": ["Das", "Sp\u00e4t\u00b7rot", "war", "ver\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im k\u00fchlen Abendschauer", "tokens": ["Im", "k\u00fch\u00b7len", "A\u00b7bend\u00b7schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dannen ritt ich stumm.", "tokens": ["Von", "dan\u00b7nen", "ritt", "ich", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Herz verging in Trauer", "tokens": ["Mein", "Herz", "ver\u00b7ging", "in", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wu\u00dfte wohl, warum.", "tokens": ["Und", "wu\u00df\u00b7te", "wohl", ",", "wa\u00b7rum", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}