{"textgrid.poem.65093": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fern vom heimathlichen Strand", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern vom heimathlichen Strand", "tokens": ["Fern", "vom", "hei\u00b7math\u00b7li\u00b7chen", "Strand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Fand ich dich auf meinem Pfade \u2013", "tokens": ["Fand", "ich", "dich", "auf", "mei\u00b7nem", "Pfa\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott hat dich zu mir gesandt", "tokens": ["Gott", "hat", "dich", "zu", "mir", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich preise seine Gnade!", "tokens": ["Und", "ich", "prei\u00b7se", "sei\u00b7ne", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meiner Wunden Schmerzenbrand", "tokens": ["Mei\u00b7ner", "Wun\u00b7den", "Schmer\u00b7zen\u00b7brand"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchlt' ich in Bethesda's Bade \u2013", "tokens": ["K\u00fchlt'", "ich", "in", "Be\u00b7thes\u00b7da's", "Ba\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott hat dich zu mir gesandt", "tokens": ["Gott", "hat", "dich", "zu", "mir", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich preise seine Gnade!", "tokens": ["Und", "ich", "prei\u00b7se", "sei\u00b7ne", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Es erschlo\u00df mir deine Hand", "tokens": ["Es", "er\u00b7schlo\u00df", "mir", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neuen Glaubens Bundeslade \u2013", "tokens": ["Neu\u00b7en", "Glau\u00b7bens", "Bun\u00b7des\u00b7la\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott hat dich zu mir gesandt", "tokens": ["Gott", "hat", "dich", "zu", "mir", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich preise seine Gnade!", "tokens": ["Und", "ich", "prei\u00b7se", "sei\u00b7ne", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}