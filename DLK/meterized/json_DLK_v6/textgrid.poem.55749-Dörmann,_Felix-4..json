{"textgrid.poem.55749": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Name klingt so s\u00fc\u00df und weich,", "tokens": ["Dein", "Na\u00b7me", "klingt", "so", "s\u00fc\u00df", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ganz und gar Dir selber gleich,", "tokens": ["Ist", "ganz", "und", "gar", "Dir", "sel\u00b7ber", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du blasse, zarte Lilie.", "tokens": ["Du", "blas\u00b7se", ",", "zar\u00b7te", "Li\u00b7lie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Ich will ihn nicht entweihen,", "tokens": ["Ich", "will", "ihn", "nicht", "ent\u00b7wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht an die andern reihen", "tokens": ["Nicht", "an", "die", "an\u00b7dern", "rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus alter, tr\u00fcber Zeit,", "tokens": ["Aus", "al\u00b7ter", ",", "tr\u00fc\u00b7ber", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo Sinnengier und Leidenschaft", "tokens": ["Wo", "Sin\u00b7nen\u00b7gier", "und", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit trotzig ungest\u00fcmer Kraft", "tokens": ["Mit", "trot\u00b7zig", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinem Herzen w\u00fchlte;", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "w\u00fchl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ich vertr\u00e4umt die Tage", "tokens": ["Wo", "ich", "ver\u00b7tr\u00e4umt", "die", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und abends beim Gelage", "tokens": ["Und", "a\u00b7bends", "beim", "Ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der tollste Zecher war.", "tokens": ["Der", "tolls\u00b7te", "Ze\u00b7cher", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Deiner Liebe milder Kraft", "tokens": ["Mit", "Dei\u00b7ner", "Lie\u00b7be", "mil\u00b7der", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast Du den Bann der Leidenschaft", "tokens": ["Hast", "Du", "den", "Bann", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerschmettert und gebrochen.", "tokens": ["Zer\u00b7schmet\u00b7tert", "und", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein tiefes, keusches Lieben", "tokens": ["Ein", "tie\u00b7fes", ",", "keu\u00b7sches", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist alles, was geblieben,", "tokens": ["Ist", "al\u00b7les", ",", "was", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und das gilt Dir allein.", "tokens": ["Und", "das", "gilt", "Dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}