{"textgrid.poem.60493": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Frau von Reinbeck", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab es lange schon gewu\u00dft:", "tokens": ["Ich", "hab", "es", "lan\u00b7ge", "schon", "ge\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein tiefer Zauber wohnt in dir.", "tokens": ["Ein", "tie\u00b7fer", "Zau\u00b7ber", "wohnt", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hast du s\u00fc\u00dfe M\u00e4rchenlust", "tokens": ["Wie", "hast", "du", "s\u00fc\u00b7\u00dfe", "M\u00e4r\u00b7chen\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch eben erst erschlossen mir!", "tokens": ["Noch", "e\u00b7ben", "erst", "er\u00b7schlos\u00b7sen", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast mich gewiegt in Fr\u00fchlingstraum,", "tokens": ["Hast", "mich", "ge\u00b7wiegt", "in", "Fr\u00fch\u00b7lings\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz Schnee und strenger Winterszeit;", "tokens": ["Trotz", "Schnee", "und", "stren\u00b7ger", "Win\u00b7ter\u00b7szeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Bl\u00e4tterschmucke Baum an Baum \u2013", "tokens": ["Im", "Bl\u00e4t\u00b7ter\u00b7schmu\u00b7cke", "Baum", "an", "Baum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lieblich lockt Waldeinsamkeit! \u2013", "tokens": ["Wie", "lieb\u00b7lich", "lockt", "Wal\u00b7de\u00b7in\u00b7sam\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du stiller Liebling der Natur!", "tokens": ["Du", "stil\u00b7ler", "Lieb\u00b7ling", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr R\u00e4tsel hast du tief geschaut,", "tokens": ["Ihr", "R\u00e4t\u00b7sel", "hast", "du", "tief", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was sie schafft auf Berg und Flur \u2013", "tokens": ["Und", "was", "sie", "schafft", "auf", "Berg", "und", "Flur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdie Heimwehkranken heile ", "tokens": ["\u00bb", "die", "Heim\u00b7weh\u00b7kran\u00b7ken", "hei\u00b7le"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lehrte dir die Wunderschrift!\u00ab \u2013", "tokens": ["Ich", "lehr\u00b7te", "dir", "die", "Wun\u00b7der\u00b7schrift", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ruft sie dir im Traume zu,", "tokens": ["So", "ruft", "sie", "dir", "im", "Trau\u00b7me", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ahnend greifst du nach dem Stift. \u2013", "tokens": ["Und", "ah\u00b7nend", "greifst", "du", "nach", "dem", "Stift", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gehorsam deinem Genius,", "tokens": ["Ge\u00b7hor\u00b7sam", "dei\u00b7nem", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dir senden Farbe, Licht und Luft", "tokens": ["Dir", "sen\u00b7den", "Far\u00b7be", ",", "Licht", "und", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Durch Schnee den leisen Maienku\u00df", "tokens": ["Durch", "Schnee", "den", "lei\u00b7sen", "Mai\u00b7en\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im schwesterlichen Bl\u00fctenduft.", "tokens": ["Im", "schwes\u00b7ter\u00b7li\u00b7chen", "Bl\u00fc\u00b7ten\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hesperien! ", "tokens": ["Hes\u00b7pe\u00b7ri\u00b7en", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Wenn wir erstarrt im Lebensfrost,", "tokens": ["Wenn", "wir", "er\u00b7starrt", "im", "Le\u00b7bens\u00b7frost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So winkst doch ", "tokens": ["So", "winkst", "doch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "So hast ", "tokens": ["So", "hast"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}}}}