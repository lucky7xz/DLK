{"dta.poem.5430": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja w\u00fcrdigt man so viele Gaben, die er allein uns doch", "tokens": ["Ja", "w\u00fcr\u00b7digt", "man", "so", "vie\u00b7le", "Ga\u00b7ben", ",", "die", "er", "al\u00b7lein", "uns", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "PIS", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "geschencket,", "tokens": ["ge\u00b7schen\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da\u00df, im bedachtsamen Genu\u00df, man ihr sich freut und sein", "tokens": ["Da\u00df", ",", "im", "be\u00b7dacht\u00b7sa\u00b7men", "Ge\u00b7nu\u00df", ",", "man", "ihr", "sich", "freut", "und", "sein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPRART", "ADJA", "NN", "$,", "PIS", "PPER", "PRF", "VVFIN", "KON", "PPOSAT"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gedencket?", "tokens": ["ge\u00b7den\u00b7cket", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Treibt uns noch die Gewohnheit einst, wie oder die Religion", "tokens": ["Treibt", "uns", "noch", "die", "Ge\u00b7wohn\u00b7heit", "einst", ",", "wie", "o\u00b7der", "die", "Re\u00b7li\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$,", "PWAV", "KON", "ART", "NN"], "meter": "+-+--+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Zum Dancken, ist es kalt und kurtz; man h\u00f6ret keinen", "tokens": ["Zum", "Dan\u00b7cken", ",", "ist", "es", "kalt", "und", "kurtz", ";", "man", "h\u00f6\u00b7ret", "kei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "PIS", "VVFIN", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "andern Thon,", "tokens": ["an\u00b7dern", "Thon", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Als etwan: H\u00f6chster, dir sey danck! da wir doch, wenn", "tokens": ["Als", "et\u00b7wan", ":", "H\u00f6chs\u00b7ter", ",", "dir", "sey", "danck", "!", "da", "wir", "doch", ",", "wenn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "$.", "NN", "$,", "PPER", "VAFIN", "ADV", "$.", "KOUS", "PPER", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "wir etwan beten,", "tokens": ["wir", "et\u00b7wan", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Mit vielen ausgesuchten Worten beredt genug zur GOtt-", "tokens": ["Mit", "vie\u00b7len", "aus\u00b7ge\u00b7such\u00b7ten", "Wor\u00b7ten", "be\u00b7redt", "ge\u00b7nug", "zur", "Got\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "heit treten,", "tokens": ["heit", "tre\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Mit vielen Wiederhohlungen, was GOtt doch wei\u00df, von", "tokens": ["Mit", "vie\u00b7len", "Wie\u00b7der\u00b7hoh\u00b7lun\u00b7gen", ",", "was", "Gott", "doch", "wei\u00df", ",", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PWS", "NN", "ADV", "VVFIN", "$,", "APPR"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "ihm verlangen,", "tokens": ["ihm", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Wovon man bald des Dancks vergi\u00dft, so bald wir es von", "tokens": ["Wo\u00b7von", "man", "bald", "des", "Dancks", "ver\u00b7gi\u00dft", ",", "so", "bald", "wir", "es", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER", "PPER", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "ihm empfangen.", "tokens": ["ihm", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.16": {"text": "Dadurch nun da\u00df wir GOttes Gaben nicht w\u00fcrdigen, sie", "tokens": ["Da\u00b7durch", "nun", "da\u00df", "wir", "Got\u00b7tes", "Ga\u00b7ben", "nicht", "w\u00fcr\u00b7di\u00b7gen", ",", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "ADV", "KOUS", "PPER", "NN", "NN", "PTKNEG", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "zu ermessen,", "tokens": ["zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Einfolglich, nie darob erfreuet, des mehr als schuld\u2019gen", "tokens": ["Ein\u00b7folg\u00b7lich", ",", "nie", "da\u00b7rob", "er\u00b7freu\u00b7et", ",", "des", "mehr", "als", "schuld'\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "PAV", "VVFIN", "$,", "ART", "PIAT", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Dancks vergessen,", "tokens": ["Dancks", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Entsteht in unserm gantzen Wesen durch unser\u2019 Unem-", "tokens": ["Ent\u00b7steht", "in", "un\u00b7serm", "gant\u00b7zen", "We\u00b7sen", "durch", "un\u00b7ser'", "Un\u00b7em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "pfindlichkeit", "tokens": ["pfind\u00b7lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Ein murrisch, unvergn\u00fcgt Gem\u00fcthe, Verdru\u00df und Unzu-", "tokens": ["Ein", "mur\u00b7risch", ",", "un\u00b7ver\u00b7gn\u00fcgt", "Ge\u00b7m\u00fc\u00b7the", ",", "Ver\u00b7dru\u00df", "und", "Un\u00b7zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJD", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "friedenheit,", "tokens": ["frie\u00b7den\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Auch dann, wann wir im Gl\u00fccke sitzen, und die\u00df entstehet", "tokens": ["Auch", "dann", ",", "wann", "wir", "im", "Gl\u00fc\u00b7cke", "sit\u00b7zen", ",", "und", "die\u00df", "ent\u00b7ste\u00b7het"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$,", "KON", "PDS", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "blos allein", "tokens": ["blos", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Daraus, da\u00df wir auf GOttes Wercke so gar unbillig-", "tokens": ["Da\u00b7raus", ",", "da\u00df", "wir", "auf", "Got\u00b7tes", "Wer\u00b7cke", "so", "gar", "un\u00b7bil\u00b7lig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "NN", "NN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "achtlos seyn.", "tokens": ["acht\u00b7los", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Der Sch\u00f6pfer wird im Werck verachtet, man ehrt nicht", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "wird", "im", "Werck", "ver\u00b7ach\u00b7tet", ",", "man", "ehrt", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVFIN", "$,", "PIS", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Gott, sich selber nur,", "tokens": ["Gott", ",", "sich", "sel\u00b7ber", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}