{"textgrid.poem.38328": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "9. Erziehung durch Langeweile", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo flieh ich hin? wo soll ich bleiben?", "tokens": ["Wo", "flieh", "ich", "hin", "?", "wo", "soll", "ich", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wird die s\u00fc\u00dfe Stille seyn?", "tokens": ["Wo", "wird", "die", "s\u00fc\u00b7\u00dfe", "Stil\u00b7le", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ich mich k\u00f6nnte schliessen ein,", "tokens": ["Da", "ich", "mich", "k\u00f6nn\u00b7te", "schlies\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich nicht lassen mehr umtreiben", "tokens": ["Und", "mich", "nicht", "las\u00b7sen", "mehr", "um\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Unruh dieser \u00e4ussern Dinge.", "tokens": ["In", "Un\u00b7ruh", "die\u00b7ser", "\u00e4us\u00b7sern", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist keine Einsamkeit bereit,", "tokens": ["Ist", "kei\u00b7ne", "Ein\u00b7sam\u00b7keit", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darin ich Gott ein Loblied singe,", "tokens": ["Da\u00b7rin", "ich", "Gott", "ein", "Lob\u00b7lied", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der von Zerstreuung mich befreit?", "tokens": ["Der", "von", "Zer\u00b7streu\u00b7ung", "mich", "be\u00b7freit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Geist will in die W\u00fcste ziehen,", "tokens": ["Mein", "Geist", "will", "in", "die", "W\u00fcs\u00b7te", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fcnscht sich Taubenfl\u00fcgel an;", "tokens": ["Und", "w\u00fcnscht", "sich", "Tau\u00b7ben\u00b7fl\u00fc\u00b7gel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil er vor Angst nicht bleiben kann,", "tokens": ["Weil", "er", "vor", "Angst", "nicht", "blei\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wo die Menschen sich bem\u00fchen,", "tokens": ["Da", "wo", "die", "Men\u00b7schen", "sich", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von Gott noch weiter wegzugehen", "tokens": ["Von", "Gott", "noch", "wei\u00b7ter", "weg\u00b7zu\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und niemals bei sich selbst zu seyn;", "tokens": ["Und", "nie\u00b7mals", "bei", "sich", "selbst", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PRF", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich kann den Jammer nicht mehr sehen,", "tokens": ["Ich", "kann", "den", "Jam\u00b7mer", "nicht", "mehr", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bleibe selbst dabei nicht rein.", "tokens": ["Und", "blei\u00b7be", "selbst", "da\u00b7bei", "nicht", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum fort o Seel! entzeuch geschwinde", "tokens": ["Drum", "fort", "o", "Seel", "!", "ent\u00b7zeuch", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "FM", "FM", "NN", "$.", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich der Gesellschaft dieser Welt!", "tokens": ["Dich", "der", "Ge\u00b7sell\u00b7schaft", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PDAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zerrei\u00df, was dich gefangen h\u00e4lt,", "tokens": ["Zer\u00b7rei\u00df", ",", "was", "dich", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit dein Fu\u00df die Ruhe finde,", "tokens": ["Da\u00b7mit", "dein", "Fu\u00df", "die", "Ru\u00b7he", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo kein Ger\u00e4usche dich verst\u00f6ret;", "tokens": ["Wo", "kein", "Ge\u00b7r\u00e4u\u00b7sche", "dich", "ver\u00b7st\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Zuspruch, Sorgen und Verdru\u00df", "tokens": ["Kein", "Zu\u00b7spruch", ",", "Sor\u00b7gen", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Umgang dir mit Gott verwehret,", "tokens": ["Den", "Um\u00b7gang", "dir", "mit", "Gott", "ver\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der hier oft unterbleiben mu\u00df.", "tokens": ["Der", "hier", "oft", "un\u00b7ter\u00b7blei\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich freu mich schon auf eine Kammer,", "tokens": ["Ich", "freu", "mich", "schon", "auf", "ei\u00b7ne", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich in sich verschliessen wird;", "tokens": ["Die", "mich", "in", "sich", "ver\u00b7schlies\u00b7sen", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch den engen Raum abf\u00fchrt,", "tokens": ["Und", "durch", "den", "en\u00b7gen", "Raum", "ab\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Von aller Unruh, Streit und Jammer,", "tokens": ["Von", "al\u00b7ler", "Un\u00b7ruh", ",", "Streit", "und", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den gro\u00dfe St\u00e4dt und Schl\u00f6sser haben;", "tokens": ["Den", "gro\u00b7\u00dfe", "St\u00e4dt", "und", "Schl\u00f6s\u00b7ser", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier soll nur meine Ruhst\u00e4tt seyn,", "tokens": ["Hier", "soll", "nur", "mei\u00b7ne", "Ruh\u00b7st\u00e4tt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da Sicherheit und Fried mich laben,", "tokens": ["Da", "Si\u00b7cher\u00b7heit", "und", "Fried", "mich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kein Unfriede bricht herein.", "tokens": ["Und", "kein", "Un\u00b7frie\u00b7de", "bricht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun will ich erst recht singen, beten,", "tokens": ["Nun", "will", "ich", "erst", "recht", "sin\u00b7gen", ",", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der Andacht kommen weit;", "tokens": ["Und", "in", "der", "An\u00b7dacht", "kom\u00b7men", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich nicht durch so viel zerstreut,", "tokens": ["Weil", "ich", "nicht", "durch", "so", "viel", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Gott mit stillem Geist darf treten.", "tokens": ["Vor", "Gott", "mit", "stil\u00b7lem", "Geist", "darf", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da soll kein Feind mich hindern k\u00f6nnen,", "tokens": ["Da", "soll", "kein", "Feind", "mich", "hin\u00b7dern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich geh in Canaan schon ein,", "tokens": ["Ich", "geh", "in", "Ca\u00b7naan", "schon", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mein Paradies soll man es nennen,", "tokens": ["Mein", "Pa\u00b7ra\u00b7dies", "soll", "man", "es", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Hier will ich auch begraben seyn.", "tokens": ["Hier", "will", "ich", "auch", "be\u00b7gra\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}