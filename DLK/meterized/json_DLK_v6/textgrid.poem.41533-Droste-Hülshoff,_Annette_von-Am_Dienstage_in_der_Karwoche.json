{"textgrid.poem.41533": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Dienstage in der Karwoche", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgleich deiner eignen Seelen", "tokens": ["\u00bb", "gleich", "dei\u00b7ner", "eig\u00b7nen", "See\u00b7len"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sollst du den N\u00e4chsten lieben!\u00ab", "tokens": ["Sollst", "du", "den", "N\u00e4chs\u00b7ten", "lie\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Herr, was wird noch fehlen,", "tokens": ["O", "Herr", ",", "was", "wird", "noch", "feh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bevor dein Wort erf\u00fcllt!", "tokens": ["Be\u00b7vor", "dein", "Wort", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So mu\u00df denn all mein Denken", "tokens": ["So", "mu\u00df", "denn", "all", "mein", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich rettungslos betr\u00fcben;", "tokens": ["Mich", "ret\u00b7tungs\u00b7los", "be\u00b7tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie sich die Augen lenken,", "tokens": ["Wie", "sich", "die", "Au\u00b7gen", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steht nur der Torheit Bild.", "tokens": ["Steht", "nur", "der", "Tor\u00b7heit", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herr, ich mu\u00df bekennen,", "tokens": ["Mein", "Herr", ",", "ich", "mu\u00df", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df wenn in tiefsten Gr\u00fcnden", "tokens": ["Da\u00df", "wenn", "in", "tiefs\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Oft meine S\u00fcnden brennen,", "tokens": ["Oft", "mei\u00b7ne", "S\u00fcn\u00b7den", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich diese nie gequ\u00e4lt.", "tokens": ["Mich", "die\u00b7se", "nie", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ist denn all den Flecken,", "tokens": ["So", "ist", "denn", "all", "den", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die meine Brust entz\u00fcnden,", "tokens": ["Die", "mei\u00b7ne", "Brust", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Des \u00dcbermutes Schrecken", "tokens": ["Des", "\u00dc\u00b7ber\u00b7mu\u00b7tes", "Schre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch t\u00f6tend beigez\u00e4hlt!", "tokens": ["Noch", "t\u00f6\u00b7tend", "bei\u00b7ge\u00b7z\u00e4hlt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und hast du mich verlassen,", "tokens": ["Und", "hast", "du", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein r\u00fcgendes Gewissen,", "tokens": ["Mein", "r\u00fc\u00b7gen\u00b7des", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil ich dich wie zu hassen", "tokens": ["Weil", "ich", "dich", "wie", "zu", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "KOKOM", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinen \u00c4ngsten schien?", "tokens": ["In", "mei\u00b7nen", "\u00c4ngs\u00b7ten", "schien", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O sch\u00e4rfe deine Qualen!", "tokens": ["O", "sch\u00e4r\u00b7fe", "dei\u00b7ne", "Qua\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und la\u00df mich ganz zerrissen,", "tokens": ["Und", "la\u00df", "mich", "ganz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bedeckt mit blut'gen Malen,", "tokens": ["Be\u00b7deckt", "mit", "blut'\u00b7gen", "Ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor Gottes Augen gl\u00fchn!", "tokens": ["Vor", "Got\u00b7tes", "Au\u00b7gen", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sprich! wolltest du mich tr\u00fcgen?", "tokens": ["Sprich", "!", "woll\u00b7test", "du", "mich", "tr\u00fc\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kann der Heller Klingen", "tokens": ["Und", "kann", "der", "Hel\u00b7ler", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein feiles Wort besiegen?", "tokens": ["Dein", "fei\u00b7les", "Wort", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich der Armut bot.", "tokens": ["Die", "ich", "der", "Ar\u00b7mut", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Gold, o schn\u00f6de Gabe!", "tokens": ["O", "Gold", ",", "o", "schn\u00f6\u00b7de", "Ga\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die alles soll erringen,", "tokens": ["Die", "al\u00b7les", "soll", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So tr\u00e4gst du mir zu Grabe", "tokens": ["So", "tr\u00e4gst", "du", "mir", "zu", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Letztes in der Not!", "tokens": ["Mein", "Letz\u00b7tes", "in", "der", "Not", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie oft drang die Versteckte,", "tokens": ["Wie", "oft", "drang", "die", "Ver\u00b7steck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Sinnlichkeit, zu spenden,", "tokens": ["Die", "Sinn\u00b7lich\u00b7keit", ",", "zu", "spen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mich ein Antlitz schreckte,", "tokens": ["Wenn", "mich", "ein", "Ant\u00b7litz", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Elend ganz verzerrt,", "tokens": ["Vom", "E\u00b7lend", "ganz", "ver\u00b7zerrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mu\u00dft' es bald entrinnen", "tokens": ["Und", "mu\u00dft'", "es", "bald", "ent\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den arbeitlosen H\u00e4nden,", "tokens": ["Den", "ar\u00b7beit\u00b7lo\u00b7sen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den ratlos irren Sinnen,", "tokens": ["Den", "rat\u00b7los", "ir\u00b7ren", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Jammer ausged\u00f6rrt.", "tokens": ["In", "Jam\u00b7mer", "aus\u00b7ge\u00b7d\u00f6rrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Gold, o schn\u00f6de Gabe!", "tokens": ["O", "Gold", ",", "o", "schn\u00f6\u00b7de", "Ga\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wenig magst du frommen,", "tokens": ["Wie", "we\u00b7nig", "magst", "du", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Magst l\u00e4uten nur zu Grabe", "tokens": ["Magst", "l\u00e4u\u00b7ten", "nur", "zu", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das letzte Gnadenwehn.", "tokens": ["Das", "letz\u00b7te", "Gna\u00b7den\u00b7wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So hast du sondergleichen", "tokens": ["So", "hast", "du", "son\u00b7derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Liebe mir genommen,", "tokens": ["Die", "Lie\u00b7be", "mir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich kann l\u00e4chelnd reichen,", "tokens": ["Da\u00df", "ich", "kann", "l\u00e4\u00b7chelnd", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo Gottes Kinder sehn.", "tokens": ["Wo", "Got\u00b7tes", "Kin\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr Sinne, sprecht ihr scheuen,", "tokens": ["Ihr", "Sin\u00b7ne", ",", "sprecht", "ihr", "scheu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was habt ihr euch entzogen?", "tokens": ["Was", "habt", "ihr", "euch", "ent\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df euch nicht alles freuen?", "tokens": ["Mu\u00df", "euch", "nicht", "al\u00b7les", "freu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mich nur freuen mag!", "tokens": ["Was", "mich", "nur", "freu\u00b7en", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In flatterndem Verlangen", "tokens": ["In", "flat\u00b7tern\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Habt ihr die Lust gesogen,", "tokens": ["Habt", "ihr", "die", "Lust", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Indes die Not vergangen", "tokens": ["In\u00b7des", "die", "Not", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An eurem Jubeltag!", "tokens": ["An", "eu\u00b7rem", "Ju\u00b7bel\u00b7tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So hab' ich deine Pfunde", "tokens": ["So", "hab'", "ich", "dei\u00b7ne", "Pfun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Frevelmut vergeudet,", "tokens": ["In", "Fre\u00b7vel\u00b7mut", "ver\u00b7geu\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcr der Armut Wunde", "tokens": ["Und", "f\u00fcr", "der", "Ar\u00b7mut", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War mir ein Heller gut!", "tokens": ["War", "mir", "ein", "Hel\u00b7ler", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das wird an mir noch zehren,", "tokens": ["Das", "wird", "an", "mir", "noch", "zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn Leib und Seele scheidet,", "tokens": ["Wenn", "Leib", "und", "See\u00b7le", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird k\u00e4mpfen, mir zu wehren", "tokens": ["Wird", "k\u00e4mp\u00b7fen", ",", "mir", "zu", "weh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVINF", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den letzten Todesmut.", "tok