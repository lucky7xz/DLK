{"textgrid.poem.46825": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[immer sah ich, und es h\u00fcpfte]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer sah ich, und es h\u00fcpfte", "tokens": ["Im\u00b7mer", "sah", "ich", ",", "und", "es", "h\u00fcpf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir das Herz, wie neben dir,", "tokens": ["Mir", "das", "Herz", ",", "wie", "ne\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Am Gewand sich haltend, schl\u00fcpfte,", "tokens": ["Am", "Ge\u00b7wand", "sich", "hal\u00b7tend", ",", "schl\u00fcpf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du gingest dort und hier,", "tokens": ["Wo", "du", "gin\u00b7gest", "dort", "und", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald dich vorw\u00e4rts rei\u00dfend,", "tokens": ["Bald", "dich", "vor\u00b7w\u00e4rts", "rei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Bald dich warten hei\u00dfend,", "tokens": ["Bald", "dich", "war\u00b7ten", "hei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Unser Engelchen.", "tokens": ["Un\u00b7ser", "En\u00b7gel\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Immer seh' ich, und es bebet", "tokens": ["Im\u00b7mer", "seh'", "ich", ",", "und", "es", "be\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir das Herz noch jeder Frist,", "tokens": ["Mir", "das", "Herz", "noch", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie es dir zur Seite schwebet,", "tokens": ["Wie", "es", "dir", "zur", "Sei\u00b7te", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht von dir zu trennen ist,", "tokens": ["Nicht", "von", "dir", "zu", "tren\u00b7nen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald zur Eile dringend,", "tokens": ["Bald", "zur", "Ei\u00b7le", "drin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Bald ein Hemmni\u00df bringend,", "tokens": ["Bald", "ein", "Hemm\u00b7ni\u00df", "brin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Unser Engelchen.", "tokens": ["Un\u00b7ser", "En\u00b7gel\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Glaub', ob es dir nicht erschiene,", "tokens": ["Glaub'", ",", "ob", "es", "dir", "nicht", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glaub' es mir, ich seh' es gehn;", "tokens": ["Glaub'", "es", "mir", ",", "ich", "seh'", "es", "gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dir zur Best\u00e4t'gung diene", "tokens": ["Und", "dir", "zur", "Be\u00b7st\u00e4t'\u00b7gung", "die\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "PDAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bald ein Zupfen, bald ein Wehn:", "tokens": ["Bald", "ein", "Zup\u00b7fen", ",", "bald", "ein", "Wehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dir geht begleitend,", "tokens": ["Mit", "dir", "geht", "be\u00b7glei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueberall hin leitend,", "tokens": ["Ue\u00b7be\u00b7rall", "hin", "lei\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Unser Engelchen.", "tokens": ["Un\u00b7ser", "En\u00b7gel\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}}}}