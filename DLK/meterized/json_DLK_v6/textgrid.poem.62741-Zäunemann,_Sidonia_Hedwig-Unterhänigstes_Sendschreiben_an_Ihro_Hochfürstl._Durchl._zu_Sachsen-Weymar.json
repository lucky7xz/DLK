{"textgrid.poem.62741": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Unterh\u00e4nigstes Sendschreiben an Ihro Hochf\u00fcrstl. Durchl. zu Sachsen-Weymar", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hat mir deine Hand", "tokens": ["So", "hat", "mir", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die voller Gnade ist; die\u00df heute zugesand,", "tokens": ["Die", "vol\u00b7ler", "Gna\u00b7de", "ist", ";", "die\u00df", "heu\u00b7te", "zu\u00b7ge\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "PDS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ich von dir gew\u00fcnscht. Mit Ehrfurcht vollem Munde", "tokens": ["Was", "ich", "von", "dir", "ge\u00b7w\u00fcnscht", ".", "Mit", "Ehr\u00b7furcht", "vol\u00b7lem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$.", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bedank ich mich davor jetzt und zu jeder Stunde.", "tokens": ["Be\u00b7dank", "ich", "mich", "da\u00b7vor", "jetzt", "und", "zu", "je\u00b7der", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "ADV", "KON", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil es dein gn\u00e4digster Befehl, und Wille ist,", "tokens": ["Weil", "es", "dein", "gn\u00e4\u00b7digs\u00b7ter", "Be\u00b7fehl", ",", "und", "Wil\u00b7le", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da\u00df ich aufs Weyhnacht-Fest ", "tokens": ["Da\u00df", "ich", "aufs", "Wey\u00b7hnacht\u00b7Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ist die\u00df ein Befehl, den ich vergn\u00fcgt vernommen.", "tokens": ["So", "ist", "die\u00df", "ein", "Be\u00b7fehl", ",", "den", "ich", "ver\u00b7gn\u00fcgt", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich werde, so Gott will; gehorsamst zu dir gehn,", "tokens": ["Ich", "wer\u00b7de", ",", "so", "Gott", "will", ";", "ge\u00b7hor\u00b7samst", "zu", "dir", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "NN", "VMFIN", "$.", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Und Freud, doch Ehrfurchtsvoll, vor deinen Augen stehn.", "tokens": ["Und", "Freud", ",", "doch", "Ehr\u00b7furchts\u00b7voll", ",", "vor", "dei\u00b7nen", "Au\u00b7gen", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bitte Demuthsvoll, erhalt mir deine Gnade", "tokens": ["Ich", "bit\u00b7te", "De\u00b7muths\u00b7voll", ",", "er\u00b7halt", "mir", "dei\u00b7ne", "Gna\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und bleib ", "tokens": ["Und", "bleib"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}}}}}