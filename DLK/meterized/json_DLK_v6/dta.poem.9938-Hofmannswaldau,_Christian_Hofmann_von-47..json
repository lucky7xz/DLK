{"dta.poem.9938": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dem winter/ wie im sommer/ nichts als arbeit uns erfreut/", "tokens": ["In", "dem", "win\u00b7ter", "/", "wie", "im", "som\u00b7mer", "/", "nichts", "als", "ar\u00b7beit", "uns", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KOKOM", "APPRART", "NN", "$(", "PIS", "KOKOM", "VVFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So bald als das licht sich zeiget un\u0303 es warm/ so sucht man beut;", "tokens": ["So", "bald", "als", "das", "licht", "sich", "zei\u00b7get", "u\u00f1", "es", "warm", "/", "so", "sucht", "man", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ART", "NN", "PRF", "VVFIN", "KON", "PPER", "ADJD", "$(", "ADV", "VVFIN", "PIS", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Offentlich tr\u00e4gt man den raub/ den wir uns geholt/ zusammen/", "tokens": ["Of\u00b7fent\u00b7lich", "tr\u00e4gt", "man", "den", "raub", "/", "den", "wir", "uns", "ge\u00b7holt", "/", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ART", "NN", "$(", "ART", "PPER", "PPER", "VVPP", "$(", "PTKVZ", "$("], "meter": "-+-+-++--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und kein richter mag bi\u00dfher diesen sch\u00f6nen raub verdammen.", "tokens": ["Und", "kein", "rich\u00b7ter", "mag", "bi\u00df\u00b7her", "die\u00b7sen", "sch\u00f6\u00b7nen", "raub", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ADV", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}