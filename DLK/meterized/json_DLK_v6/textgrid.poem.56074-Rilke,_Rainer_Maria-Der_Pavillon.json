{"textgrid.poem.56074": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Pavillon", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber selbst noch durch die Fl\u00fcgelt\u00fcren", "tokens": ["A\u00b7ber", "selbst", "noch", "durch", "die", "Fl\u00fc\u00b7gel\u00b7t\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "mit dem gr\u00fcnen regentr\u00fcben Glas", "tokens": ["mit", "dem", "gr\u00fc\u00b7nen", "re\u00b7gen\u00b7tr\u00fc\u00b7ben", "Glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ist ein Spiegeln l\u00e4chelnder All\u00fcren", "tokens": ["ist", "ein", "Spie\u00b7geln", "l\u00e4\u00b7cheln\u00b7der", "Al\u00b7l\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und ein Glanz von jenem Gl\u00fcck zu sp\u00fcren,", "tokens": ["und", "ein", "Glanz", "von", "je\u00b7nem", "Gl\u00fcck", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "das sich dort, wohin sie nicht mehr f\u00fchren,", "tokens": ["das", "sich", "dort", ",", "wo\u00b7hin", "sie", "nicht", "mehr", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "$,", "PWAV", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "einst verbarg, verkl\u00e4rte und verga\u00df.", "tokens": ["einst", "ver\u00b7barg", ",", "ver\u00b7kl\u00e4r\u00b7te", "und", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber selbst noch in den Stein-Guirlanden", "tokens": ["A\u00b7ber", "selbst", "noch", "in", "den", "Stein\u00b7Guir\u00b7lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcber der nicht mehr ber\u00fchrten T\u00fcr", "tokens": ["\u00fc\u00b7ber", "der", "nicht", "mehr", "be\u00b7r\u00fchr\u00b7ten", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ist ein Hang zur Heimlichkeit vorhanden", "tokens": ["ist", "ein", "Hang", "zur", "Heim\u00b7lich\u00b7keit", "vor\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und ein stilles Mitgef\u00fchl daf\u00fcr \u2013,", "tokens": ["und", "ein", "stil\u00b7les", "Mit\u00b7ge\u00b7f\u00fchl", "da\u00b7f\u00fcr", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "und sie schauern manchmal, wie gespiegelt,", "tokens": ["und", "sie", "schau\u00b7ern", "manch\u00b7mal", ",", "wie", "ge\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wenn ein Wind sie schattig \u00fcberlief;", "tokens": ["wenn", "ein", "Wind", "sie", "schat\u00b7tig", "\u00fc\u00b7berl\u00b7ief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "auch das Wappen, wie auf einem Brief", "tokens": ["auch", "das", "Wap\u00b7pen", ",", "wie", "auf", "ei\u00b7nem", "Brief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "viel zu gl\u00fccklich, \u00fcberst\u00fcrzt gesiegelt,", "tokens": ["viel", "zu", "gl\u00fcck\u00b7lich", ",", "\u00fc\u00b7bers\u00b7t\u00fcrzt", "ge\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "redet noch. Wie wenig man verscheuchte:", "tokens": ["re\u00b7det", "noch", ".", "Wie", "we\u00b7nig", "man", "ver\u00b7scheuch\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PWAV", "PIS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "alles wei\u00df noch, weint noch, tut noch weh \u2013.", "tokens": ["al\u00b7les", "wei\u00df", "noch", ",", "weint", "noch", ",", "tut", "noch", "weh", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "ADV", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und im Fortgehn durch die tr\u00e4nenfeuchte", "tokens": ["Und", "im", "Fort\u00b7gehn", "durch", "die", "tr\u00e4\u00b7nen\u00b7feuch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "abgelegene Allee", "tokens": ["ab\u00b7ge\u00b7le\u00b7ge\u00b7ne", "Al\u00b7lee"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "f\u00fchlt man lang noch auf dem Rand des Dachs", "tokens": ["f\u00fchlt", "man", "lang", "noch", "auf", "dem", "Rand", "des", "Dachs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "jene Urnen stehen, kalt, zerspalten:", "tokens": ["je\u00b7ne", "Ur\u00b7nen", "ste\u00b7hen", ",", "kalt", ",", "zer\u00b7spal\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "doch entschlossen, noch zusammzuhalten", "tokens": ["doch", "ent\u00b7schlos\u00b7sen", ",", "noch", "zu\u00b7samm\u00b7zu\u00b7hal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "um die Asche alter Achs.", "tokens": ["um", "die", "A\u00b7sche", "al\u00b7ter", "Achs", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}