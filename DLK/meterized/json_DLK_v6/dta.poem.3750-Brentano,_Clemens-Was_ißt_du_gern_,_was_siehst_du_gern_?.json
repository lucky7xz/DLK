{"dta.poem.3750": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Was i\u00dft du gern ,  was siehst du gern ?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geschnittne Nudele e\u00df ich gern,               ", "tokens": ["Ge\u00b7schnitt\u00b7ne", "Nu\u00b7de\u00b7le", "e\u00df", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aber nur die feine,", "tokens": ["A\u00b7ber", "nur", "die", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6ne M\u00e4dele seh ich gern,", "tokens": ["Sch\u00f6\u00b7ne", "M\u00e4\u00b7de\u00b7le", "seh", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Aber nur die kleine.", "tokens": ["A\u00b7ber", "nur", "die", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}