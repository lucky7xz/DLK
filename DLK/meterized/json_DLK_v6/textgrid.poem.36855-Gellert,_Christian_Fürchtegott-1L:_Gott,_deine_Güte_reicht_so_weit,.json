{"textgrid.poem.36855": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, deine G\u00fcte reicht so weit,", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, deine G\u00fcte reicht so weit,", "tokens": ["Gott", ",", "dei\u00b7ne", "G\u00fc\u00b7te", "reicht", "so", "weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So weit die Wolken gehen;", "tokens": ["So", "weit", "die", "Wol\u00b7ken", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du kr\u00f6nst uns mit Barmherzigkeit,", "tokens": ["Du", "kr\u00f6nst", "uns", "mit", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eilst, uns beizustehen.", "tokens": ["Und", "eilst", ",", "uns", "bei\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr, meine Burg, mein Fels, mein Hort,", "tokens": ["Herr", ",", "mei\u00b7ne", "Burg", ",", "mein", "Fels", ",", "mein", "Hort", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vernimm mein Flehn, merk auf mein Wort;", "tokens": ["Ver\u00b7nimm", "mein", "Flehn", ",", "merk", "auf", "mein", "Wort", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn ich will vor dir beten!", "tokens": ["Denn", "ich", "will", "vor", "dir", "be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich bitte nicht um \u00dcberflu\u00df", "tokens": ["Ich", "bit\u00b7te", "nicht", "um", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Sch\u00e4tze dieser Erden.", "tokens": ["Und", "Sch\u00e4t\u00b7ze", "die\u00b7ser", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mir, so viel ich haben mu\u00df,", "tokens": ["La\u00df", "mir", ",", "so", "viel", "ich", "ha\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADV", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deiner Gnade werden.", "tokens": ["Nach", "dei\u00b7ner", "Gna\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gib mir nur Weisheit und Verstand,", "tokens": ["Gib", "mir", "nur", "Weis\u00b7heit", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich, Gott, und den, den du gesandt,", "tokens": ["Dich", ",", "Gott", ",", "und", "den", ",", "den", "du", "ge\u00b7sandt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KON", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mich selbst zu erkennen.", "tokens": ["Und", "mich", "selbst", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bitte nicht um Ehr und Ruhm,", "tokens": ["Ich", "bit\u00b7te", "nicht", "um", "Ehr", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sehr sie Menschen r\u00fchren;", "tokens": ["So", "sehr", "sie", "Men\u00b7schen", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des guten Namens Eigentum", "tokens": ["Des", "gu\u00b7ten", "Na\u00b7mens", "Ei\u00b7gen\u00b7tum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df mich nur nicht verlieren.", "tokens": ["La\u00df", "mich", "nur", "nicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein wahrer Ruhm sei meine Pflicht,", "tokens": ["Mein", "wah\u00b7rer", "Ruhm", "sei", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Ruhm vor deinem Angesicht,", "tokens": ["Der", "Ruhm", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und frommer Freunde Liebe.", "tokens": ["Und", "from\u00b7mer", "Freun\u00b7de", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So bitt ich dich, Herr Zebaoth,", "tokens": ["So", "bitt", "ich", "dich", ",", "Herr", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch nicht um langes Leben.", "tokens": ["Auch", "nicht", "um", "lan\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Gl\u00fccke Demut, Mut in Not,", "tokens": ["Im", "Gl\u00fc\u00b7cke", "De\u00b7mut", ",", "Mut", "in", "Not", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wolltest du mir geben.", "tokens": ["Das", "woll\u00b7test", "du", "mir", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deiner Hand steht meine Zeit;", "tokens": ["In", "dei\u00b7ner", "Hand", "steht", "mei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df du mich nur Barmherzigkeit", "tokens": ["La\u00df", "du", "mich", "nur", "Barm\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor dir im Tode finden.", "tokens": ["Vor", "dir", "im", "To\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}