{"textgrid.poem.57165": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du machst es dir noch leicht.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du machst es dir noch leicht.", "tokens": ["Du", "machst", "es", "dir", "noch", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du tust Verzicht", "tokens": ["Du", "tust", "Ver\u00b7zicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und gibst der Welt", "tokens": ["und", "gibst", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "dann Trauer zum Entgelt:", "tokens": ["dann", "Trau\u00b7er", "zum", "Ent\u00b7gelt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "sie mu\u00df es b\u00fc\u00dfen,", "tokens": ["sie", "mu\u00df", "es", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "was du nicht erreicht.", "tokens": ["was", "du", "nicht", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Verzicht' und traure,", "tokens": ["Ver\u00b7zicht'", "und", "trau\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "aber klage nicht;", "tokens": ["a\u00b7ber", "kla\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "dein Schmerz durchschaure", "tokens": ["dein", "Schmerz", "durch\u00b7schau\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Leben und Gedicht, \u2013", "tokens": ["Le\u00b7ben", "und", "Ge\u00b7dicht", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "doch bau er sie nicht auf!", "tokens": ["doch", "bau", "er", "sie", "nicht", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das fall' ", "tokens": ["Das", "fall'"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "der \u00fcber Schmerz und Lust", "tokens": ["der", "\u00fc\u00b7ber", "Schmerz", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dein tiefstes Du.", "tokens": ["Dein", "tiefs\u00b7tes", "Du", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}