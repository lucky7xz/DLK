{"dta.poem.691": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der dicke Nebel bricht in welchen sich verh\u00fcllet", "tokens": ["Der", "di\u00b7cke", "Ne\u00b7bel", "bricht", "in", "wel\u00b7chen", "sich", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PWAT", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alles hebt vnd h\u00e4lt;", "tokens": ["Der", "al\u00b7les", "hebt", "vnd", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aller scharffe pein vnd herbe thr\u00e4nen stillet/", "tokens": ["Der", "al\u00b7ler", "scharf\u00b7fe", "pein", "vnd", "her\u00b7be", "thr\u00e4\u00b7nen", "stil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Sch\u00f6pffer dieser welt. ", "tokens": ["Der", "Sch\u00f6pf\u00b7fer", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wendet sich vnd h\u00f6rt nach meinem wimmern.", "tokens": ["Er", "wen\u00b7det", "sich", "vnd", "h\u00f6rt", "nach", "mei\u00b7nem", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vnd bl\u00e4\u00dft mein erstarte Leichen mit ernewtem Leben an:", "tokens": ["Vnd", "bl\u00e4\u00dft", "mein", "er\u00b7star\u00b7te", "Lei\u00b7chen", "mit", "er\u00b7new\u00b7tem", "Le\u00b7ben", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Da\u00df ich/ der ich schon erstummet/ jhm mit jauchtzen dancken", "tokens": ["Da\u00df", "ich", "/", "der", "ich", "schon", "er\u00b7stum\u00b7met", "/", "jhm", "mit", "jaucht\u00b7zen", "dan\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$(", "PPER", "APPR", "VVINF", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "kan/ ", "tokens": ["kan", "/"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Ich sp\u00fcr\u2019 vmb mich sein edle W\u00e4chter schimmern.", "tokens": ["Ich", "sp\u00fcr'", "vmb", "mich", "sein", "ed\u00b7le", "W\u00e4ch\u00b7ter", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Versteckt mich in de\u00df Abgrunds gr\u00fcnde;", "tokens": ["Ver\u00b7steckt", "mich", "in", "de\u00df", "Ab\u00b7grunds", "gr\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd wo ich kaum mich selber finde/", "tokens": ["Vnd", "wo", "ich", "kaum", "mich", "sel\u00b7ber", "fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ja in mittelpunct der Erden.", "tokens": ["Ja", "in", "mit\u00b7tel\u00b7punct", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Er wird mich au\u00df dieser Tieffen/ au\u00df der vners", "tokens": ["Er", "wird", "mich", "au\u00df", "die\u00b7ser", "Tief\u00b7fen", "/", "au\u00df", "der", "vners"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$(", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Au\u00df der Hellen hell\u2019 erretten; mir sol aller gr\u00fcffte grufft", "tokens": ["Au\u00df", "der", "Hel\u00b7len", "hell'", "er\u00b7ret\u00b7ten", ";", "mir", "sol", "al\u00b7ler", "gr\u00fcff\u00b7te", "grufft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$.", "PPER", "VMFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Noch zum ehren Schawplatz werden.", "tokens": ["Noch", "zum", "eh\u00b7ren", "Schaw\u00b7platz", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Jagt mich wo die welt aufh\u00f6rt/ wie die kalten l\u00fcffte ringen:", "tokens": ["Jagt", "mich", "wo", "die", "welt", "auf\u00b7h\u00f6rt", "/", "wie", "die", "kal\u00b7ten", "l\u00fcff\u00b7te", "rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "ART", "NN", "VVPP", "$(", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Wo das heisse Landt verbrennt; GOt der wird mich wider", "tokens": ["Wo", "das", "heis\u00b7se", "Landt", "ver\u00b7brennt", ";", "Got", "der", "wird", "mich", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$.", "NN", "ART", "VAFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "bringen. ", "tokens": ["brin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}