{"textgrid.poem.58354": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "392. Der Liebe Fegfeuer", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe ist ein Feu'r, das keine Schlacken leidet,", "tokens": ["Die", "Lie\u00b7be", "ist", "ein", "Feu'r", ",", "das", "kei\u00b7ne", "Schla\u00b7cken", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das alle Eigenheit von unsrer Seele scheidet;", "tokens": ["Das", "al\u00b7le", "Ei\u00b7gen\u00b7heit", "von", "uns\u00b7rer", "See\u00b7le", "schei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gib ihren Flammen Raum und achte keine Pein,", "tokens": ["Gib", "ih\u00b7ren", "Flam\u00b7men", "Raum", "und", "ach\u00b7te", "kei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "KON", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie wird zuletzt in dir ein Freudenleben sein!", "tokens": ["Sie", "wird", "zu\u00b7letzt", "in", "dir", "ein", "Freu\u00b7den\u00b7le\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}