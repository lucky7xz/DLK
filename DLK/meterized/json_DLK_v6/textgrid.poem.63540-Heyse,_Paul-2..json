{"textgrid.poem.63540": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Nacht und fr\u00fchem Tag", "tokens": ["Zwi\u00b7schen", "Nacht", "und", "fr\u00fc\u00b7hem", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu mir kamen die b\u00f6sen Tr\u00e4ume,", "tokens": ["Zu", "mir", "ka\u00b7men", "die", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "B\u00f6se Tr\u00e4ume, s\u00fc\u00dfe Tr\u00e4ume,", "tokens": ["B\u00f6\u00b7se", "Tr\u00e4u\u00b7me", ",", "s\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da ich wach und wehrlos lag.", "tokens": ["Da", "ich", "wach", "und", "wehr\u00b7los", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Rissen der Liebe wild und zag", "tokens": ["Ris\u00b7sen", "der", "Lie\u00b7be", "wild", "und", "zag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Allen Schleiertrug herunter.", "tokens": ["Al\u00b7len", "Schlei\u00b7er\u00b7trug", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glut ging auf \u2013 ach, Ruh' ging unter", "tokens": ["Glut", "ging", "auf", "\u2013", "ach", ",", "Ruh'", "ging", "un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "$(", "XY", "$,", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Nacht und fr\u00fchem Tag!", "tokens": ["Zwi\u00b7schen", "Nacht", "und", "fr\u00fc\u00b7hem", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}