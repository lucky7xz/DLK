{"textgrid.poem.60684": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des G\u00e4rtners Esel klagte bei dem Schicksal sehr,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des G\u00e4rtners Esel klagte bei dem Schicksal sehr,", "tokens": ["Des", "G\u00e4rt\u00b7ners", "E\u00b7sel", "klag\u00b7te", "bei", "dem", "Schick\u00b7sal", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil man vor Sonnenaufgang ihn schon weckt.", "tokens": ["Weil", "man", "vor", "Son\u00b7nen\u00b7auf\u00b7gang", "ihn", "schon", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwas noch des Hahnes Morgenruf bezweckt?\u00ab", "tokens": ["\u00bb", "was", "noch", "des", "Hah\u00b7nes", "Mor\u00b7gen\u00b7ruf", "be\u00b7zweckt", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "ADV", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So sprach er. \u00bbIch bin fr\u00fcher auf als er.", "tokens": ["So", "sprach", "er", ".", "\u00bb", "Ich", "bin", "fr\u00fc\u00b7her", "auf", "als", "er", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "ADJD", "APPR", "KOUS", "PPER", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wozu? Um Kraut zu Markt zu tragen!", "tokens": ["Wo\u00b7zu", "?", "Um", "Kraut", "zu", "Markt", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUI", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist's n\u00f6tig, darum mich aus meinem Schlaf zu jagen?\u00ab", "tokens": ["Ist's", "n\u00f6\u00b7tig", ",", "da\u00b7rum", "mich", "aus", "mei\u00b7nem", "Schlaf", "zu", "ja\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "$,", "PAV", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Schicksal h\u00f6rt voll Mitgef\u00fchl des Esels Klagen", "tokens": ["Das", "Schick\u00b7sal", "h\u00f6rt", "voll", "Mit\u00b7ge\u00b7f\u00fchl", "des", "E\u00b7sels", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und gibt ihn, da\u00df er Ruhe f\u00e4nde,", "tokens": ["Und", "gibt", "ihn", ",", "da\u00df", "er", "Ru\u00b7he", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vom G\u00e4rtner fort in Gerbers H\u00e4nde.", "tokens": ["Vom", "G\u00e4rt\u00b7ner", "fort", "in", "Ger\u00b7bers", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die schweren H\u00e4ute, ihr Gestank \u2013 wie unerh\u00f6rt! \u2013", "tokens": ["Die", "schwe\u00b7ren", "H\u00e4u\u00b7te", ",", "ihr", "Ge\u00b7stank", "\u2013", "wie", "un\u00b7er\u00b7h\u00f6rt", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die haben bald das unzufriedne Vieh emp\u00f6rt.", "tokens": ["Die", "ha\u00b7ben", "bald", "das", "un\u00b7zu\u00b7fried\u00b7ne", "Vieh", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u00bbo w\u00e4r ich wieder,\u00ab sprach's, \u00bbbei meinem ersten Herrn!", "tokens": ["\u00bb", "o", "w\u00e4r", "ich", "wie\u00b7der", ",", "\u00ab", "sprach's", ",", "\u00bb", "bei", "mei\u00b7nem", "ers\u00b7ten", "Herrn", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VAFIN", "PPER", "ADV", "$,", "$(", "NE", "$,", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auch hab ich, sah er's nicht, manch K\u00f6pfchen Kohl erwischt \u2013", "tokens": ["Auch", "hab", "ich", ",", "sah", "er's", "nicht", ",", "manch", "K\u00f6pf\u00b7chen", "Kohl", "er\u00b7wischt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "PIS", "PTKNEG", "$,", "PIAT", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hier gibt es nebenbei nicht einen Zwetschenkern.", "tokens": ["Hier", "gibt", "es", "ne\u00b7ben\u00b7bei", "nicht", "ei\u00b7nen", "Zwet\u00b7schen\u00b7kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Fast nichts als Hiebe werden mir hier aufgetischt.\u00ab", "tokens": ["Fast", "nichts", "als", "Hie\u00b7be", "wer\u00b7den", "mir", "hier", "auf\u00b7ge\u00b7tischt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIS", "KOKOM", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er w\u00fcnscht Ver\u00e4nderung. So kam der graue Renner", "tokens": ["Er", "w\u00fcnscht", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", ".", "So", "kam", "der", "grau\u00b7e", "Ren\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Schlie\u00dflich herunter bis zu einem Kohlenbrenner.", "tokens": ["Schlie\u00df\u00b7lich", "her\u00b7un\u00b7ter", "bis", "zu", "ei\u00b7nem", "Koh\u00b7len\u00b7bren\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Er klagte wieder. \u00bbWie, noch immer?\u00ab", "tokens": ["Er", "klag\u00b7te", "wie\u00b7der", ".", "\u00bb", "Wie", ",", "noch", "im\u00b7mer", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "$(", "PWAV", "$,", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Sprach zornig das Geschick. \u00bbDer Esel qu\u00e4lt mich schlimmer,", "tokens": ["Sprach", "zor\u00b7nig", "das", "Ge\u00b7schick", ".", "\u00bb", "Der", "E\u00b7sel", "qu\u00e4lt", "mich", "schlim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als hundert K\u00f6nige es machen.", "tokens": ["Als", "hun\u00b7dert", "K\u00f6\u00b7ni\u00b7ge", "es", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Glaubt er, er sei der einzige blo\u00df,", "tokens": ["Glaubt", "er", ",", "er", "sei", "der", "ein\u00b7zi\u00b7ge", "blo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "ADJA", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Der nicht zufrieden ist mit seinem Los?", "tokens": ["Der", "nicht", "zu\u00b7frie\u00b7den", "ist", "mit", "sei\u00b7nem", "Los", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Hab ich nichts anderes im Kopf als seine Sachen?\u00ab", "tokens": ["Hab", "ich", "nichts", "an\u00b7de\u00b7res", "im", "Kopf", "als", "sei\u00b7ne", "Sa\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PIS", "PIS", "APPRART", "NN", "KOKOM", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Schicksal hatte recht.", "tokens": ["Das", "Schick\u00b7sal", "hat\u00b7te", "recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So machen's aber alle Leute.", "tokens": ["So", "ma\u00b7chen's", "a\u00b7ber", "al\u00b7le", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stets scheint uns unsre Lage schlecht,", "tokens": ["Stets", "scheint", "uns", "uns\u00b7re", "La\u00b7ge", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am schlechtsten immer die von heute.", "tokens": ["Am", "schlechts\u00b7ten", "im\u00b7mer", "die", "von", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADV", "ART", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit unsern Petitionen.", "tokens": ["Mit", "un\u00b7sern", "Pe\u00b7ti\u00b7ti\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ob Jupiter auch jedem seinen Wunsch erf\u00fcllt,", "tokens": ["Ob", "Ju\u00b7pi\u00b7ter", "auch", "je\u00b7dem", "sei\u00b7nen", "Wunsch", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er wird nur immer lauter angebr\u00fcllt.", "tokens": ["Er", "wird", "nur", "im\u00b7mer", "lau\u00b7ter", "an\u00b7ge\u00b7br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}