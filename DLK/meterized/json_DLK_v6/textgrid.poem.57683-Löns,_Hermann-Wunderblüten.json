{"textgrid.poem.57683": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Wunderbl\u00fcten", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen hohen Ginsterb\u00fcschen", "tokens": ["Zwi\u00b7schen", "ho\u00b7hen", "Gins\u00b7ter\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhst du jetzt vom weiten Weg,", "tokens": ["Ruhst", "du", "jetzt", "vom", "wei\u00b7ten", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gelbe Wunderbl\u00fcten bauen", "tokens": ["Gel\u00b7be", "Wun\u00b7der\u00b7bl\u00fc\u00b7ten", "bau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um dich her ein Goldgeheg.", "tokens": ["Um", "dich", "her", "ein", "Gold\u00b7ge\u00b7heg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APZR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gelbe Wunderbl\u00fcten senken", "tokens": ["Gel\u00b7be", "Wun\u00b7der\u00b7bl\u00fc\u00b7ten", "sen\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldigend sich auf dein Haar,", "tokens": ["Hul\u00b7di\u00b7gend", "sich", "auf", "dein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und das schimmert in der Sonne", "tokens": ["Und", "das", "schim\u00b7mert", "in", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4rchenhaft und wunderbar.", "tokens": ["M\u00e4r\u00b7chen\u00b7haft", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich lieg zu deinen F\u00fc\u00dfen,", "tokens": ["Und", "ich", "lieg", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh bewundernd auf dich hin,", "tokens": ["Seh", "be\u00b7wun\u00b7dernd", "auf", "dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf mein Lieb im goldnen Ginster,", "tokens": ["Auf", "mein", "Lieb", "im", "gold\u00b7nen", "Gins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine goldene K\u00f6nigin.", "tokens": ["Mei\u00b7ne", "gol\u00b7de\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}