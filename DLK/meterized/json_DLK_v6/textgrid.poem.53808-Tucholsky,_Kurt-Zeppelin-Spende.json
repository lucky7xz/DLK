{"textgrid.poem.53808": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zeppelin-Spende", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist ein wahrhaft deutscher Mann.", "tokens": ["Hier", "ist", "ein", "wahr\u00b7haft", "deut\u00b7scher", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Spannt den Ballon f\u00fcr den Ozean an,", "tokens": ["Spannt", "den", "Bal\u00b7lon", "f\u00fcr", "den", "O\u00b7ze\u00b7an", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "und alle Leute m\u00f6gen ihn leiden:", "tokens": ["und", "al\u00b7le", "Leu\u00b7te", "m\u00f6\u00b7gen", "ihn", "lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "so aufdringlich schlicht und so laut bescheiden . . .", "tokens": ["so", "auf\u00b7dring\u00b7lich", "schlicht", "und", "so", "laut", "be\u00b7schei\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "ADV", "ADJD", "VVPP", "$.", "$.", "$."], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es steigt sein Ruhm in die H\u00f6he und weiter \u2013", "tokens": ["Es", "steigt", "sein", "Ruhm", "in", "die", "H\u00f6\u00b7he", "und", "wei\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "KON", "ADV", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "niemand gedenkt der Mitarbeiter.", "tokens": ["nie\u00b7mand", "ge\u00b7denkt", "der", "Mit\u00b7ar\u00b7bei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Es steigen die nationalsten Faxen \u2013", "tokens": ["Es", "stei\u00b7gen", "die", "na\u00b7ti\u00b7o\u00b7nals\u00b7ten", "Fa\u00b7xen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "niemand gedenkt der Angelsachsen,", "tokens": ["nie\u00b7mand", "ge\u00b7denkt", "der", "An\u00b7gel\u00b7sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "die den Flug immerhin zuerst unternommen.", "tokens": ["die", "den", "Flug", "im\u00b7mer\u00b7hin", "zu\u00b7erst", "un\u00b7ter\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und als er dr\u00fcben angekommen,", "tokens": ["Und", "als", "er", "dr\u00fc\u00b7ben", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "br\u00fcllt auf ein Volk: Es ist erreicht!", "tokens": ["br\u00fcllt", "auf", "ein", "Volk", ":", "Es", "ist", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Stammtischgehirne sind sanft erweicht.", "tokens": ["Die", "Stamm\u00b7tischge\u00b7hir\u00b7ne", "sind", "sanft", "er\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ehrendoktor. Geschrei. Baldachin:", "tokens": ["Eh\u00b7ren\u00b7dok\u00b7tor", ".", "Ge\u00b7schrei", ".", "Bal\u00b7da\u00b7ch\u00b7in", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+--+-++-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "\u00bbzeppelin \u2013! Zeppelin \u2013!\u00ab", "tokens": ["\u00bb", "zep\u00b7pe\u00b7lin", "\u2013", "!", "Zep\u00b7pe\u00b7lin", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "FM.la", "$(", "$.", "NE", "$(", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das l\u00e4\u00dft den wahrhaften Deutschen nicht schlafen.", "tokens": ["Das", "l\u00e4\u00dft", "den", "wahr\u00b7haf\u00b7ten", "Deut\u00b7schen", "nicht", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus dem nationalen Luftschiffhafen", "tokens": ["Aus", "dem", "na\u00b7ti\u00b7o\u00b7na\u00b7len", "Luft\u00b7schiff\u00b7ha\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "bittet er um milde Gaben.", "tokens": ["bit\u00b7tet", "er", "um", "mil\u00b7de", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil wir sonst keine Sorgen haben:", "tokens": ["Weil", "wir", "sonst", "kei\u00b7ne", "Sor\u00b7gen", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nordpol! Er mu\u00df zum Nordpol fliegen!", "tokens": ["Der", "Nord\u00b7pol", "!", "Er", "mu\u00df", "zum", "Nord\u00b7pol", "flie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Deutschland liegt vorn! Und Deutschland mu\u00df siegen!", "tokens": ["Deutschland", "liegt", "vorn", "!", "Und", "Deutschland", "mu\u00df", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "KON", "NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer darf das bezahlen? Arbeiter. Kinder.", "tokens": ["Wer", "darf", "das", "be\u00b7zah\u00b7len", "?", "Ar\u00b7bei\u00b7ter", ".", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VVINF", "$.", "NN", "$.", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Auch schwere Kriegsbesch\u00e4digte nicht minder,", "tokens": ["Auch", "schwe\u00b7re", "Kriegs\u00b7be\u00b7sch\u00e4\u00b7dig\u00b7te", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "kurz: die Geld haben. Er wird f\u00fchren,", "tokens": ["kurz", ":", "die", "Geld", "ha\u00b7ben", ".", "Er", "wird", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ART", "NN", "VAFIN", "$.", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "15% Verwaltungsgeb\u00fchren,", "tokens": ["%", "Ver\u00b7wal\u00b7tungs\u00b7ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["punct", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Reklame auf Postkarten und Plakat,", "tokens": ["Re\u00b7kla\u00b7me", "auf", "Post\u00b7kar\u00b7ten", "und", "Pla\u00b7kat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Direktoren, Briefbogen, Apparat . . .", "tokens": ["Di\u00b7rek\u00b7to\u00b7ren", ",", "Brief\u00b7bo\u00b7gen", ",", "Ap\u00b7pa\u00b7rat", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$.", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Stummbleiben die Massen, auf dem Land, in Berlin:", "tokens": ["Stumm\u00b7blei\u00b7ben", "die", "Mas\u00b7sen", ",", "auf", "dem", "Land", ",", "in", "Ber\u00b7lin", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "NE", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "\u00bbzeppelin . . . ? Zeppelin . . . ?\u00ab", "tokens": ["\u00bb", "zep\u00b7pe\u00b7lin", ".", ".", ".", "?", "Zep\u00b7pe\u00b7lin", ".", ".", ".", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$.", "$.", "$.", "NE", "$.", "$.", "$.", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Keinen Pfennig.", "tokens": ["Kei\u00b7nen", "Pfen\u00b7nig", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Eine Million", "tokens": ["Ei\u00b7ne", "Mil\u00b7li\u00b7on"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "M\u00e4nner hungern seit Monaten schon,", "tokens": ["M\u00e4n\u00b7ner", "hun\u00b7gern", "seit", "Mo\u00b7na\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "haben kein Geld f\u00fcr deine Taten,", "tokens": ["ha\u00b7ben", "kein", "Geld", "f\u00fcr", "dei\u00b7ne", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "pfeifen auf Nordpol und Luftakrobaten;", "tokens": ["pfei\u00b7fen", "auf", "Nord\u00b7pol", "und", "Luf\u00b7ta\u00b7kro\u00b7ba\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "suchen sich hier das kleine St\u00fcck", "tokens": ["su\u00b7chen", "sich", "hier", "das", "klei\u00b7ne", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "trocken Brot und Arbeit, ihr bi\u00dfchen Gl\u00fcck . . .", "tokens": ["tro\u00b7cken", "Brot", "und", "Ar\u00b7beit", ",", "ihr", "bi\u00df\u00b7chen", "Gl\u00fcck", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Fahr immerzu!", "tokens": ["Fahr", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Pack ein! Pack ein", "tokens": ["Pack", "ein", "!", "Pack", "ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "NN", "ART"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "in deinen Ballon den ganzen Verein", "tokens": ["in", "dei\u00b7nen", "Bal\u00b7lon", "den", "gan\u00b7zen", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "der gro\u00dfm\u00e4uligen Militaristen,", "tokens": ["der", "gro\u00df\u00b7m\u00e4u\u00b7li\u00b7gen", "Mi\u00b7li\u00b7ta\u00b7ris\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "der Femem\u00f6rder, Gerichtssadisten \u2013", "tokens": ["der", "Fe\u00b7me\u00b7m\u00f6r\u00b7der", ",", "Ge\u00b7richts\u00b7sa\u00b7dis\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Singt bei der Abfahrt brausende Lieder!", "tokens": ["Singt", "bei", "der", "Ab\u00b7fahrt", "brau\u00b7sen\u00b7de", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Nimm sie mit und komm nie mehr wieder!", "tokens": ["Nimm", "sie", "mit", "und", "komm", "nie", "mehr", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "KON", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Begl\u00fccke die Eskimos! Laufe Ski", "tokens": ["Be\u00b7gl\u00fc\u00b7cke", "die", "Es\u00b7ki\u00b7mos", "!", "Lau\u00b7fe", "Ski"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und begr\u00fcnde da eine Monarchie!", "tokens": ["und", "be\u00b7gr\u00fcn\u00b7de", "da", "ei\u00b7ne", "Mon\u00b7ar\u00b7chie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Du bist nicht Deutschland. Du bist nicht der Staat.", "tokens": ["Du", "bist", "nicht", "Deutschland", ".", "Du", "bist", "nicht", "der", "Staat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NE", "$.", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Das ausgehungerte Proletariat", "tokens": ["Das", "aus\u00b7ge\u00b7hun\u00b7ger\u00b7te", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7at"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sieht dich ohne Bedauern ziehn \u2013", "tokens": ["sieht", "dich", "oh\u00b7ne", "Be\u00b7dau\u00b7ern", "ziehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Zeppelin \u2013! Zeppelin \u2013!", "tokens": ["Zep\u00b7pe\u00b7lin", "\u2013", "!", "Zep\u00b7pe\u00b7lin", "\u2013", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "punct"], "pos": ["NE", "$(", "$.", "NE", "$(", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}