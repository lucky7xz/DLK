{"textgrid.poem.24848": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Regen und Sturmgebrause", "tokens": ["Mit", "Re\u00b7gen", "und", "Sturm\u00b7ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sei mir willkommen, Dezembermond,", "tokens": ["Sei", "mir", "will\u00b7kom\u00b7men", ",", "De\u00b7zem\u00b7ber\u00b7mond", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und f\u00fchr mich den Weg zum traulichen Hause,", "tokens": ["Und", "f\u00fchr", "mich", "den", "Weg", "zum", "trau\u00b7li\u00b7chen", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo meine geliebte Herrin wohnt!", "tokens": ["Wo", "mei\u00b7ne", "ge\u00b7lieb\u00b7te", "Her\u00b7rin", "wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nie hab' ich die Bl\u00fcte des Maien,", "tokens": ["Nie", "hab'", "ich", "die", "Bl\u00fc\u00b7te", "des", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den blauenden Himmel, den blitzenden Tau", "tokens": ["Den", "blau\u00b7en\u00b7den", "Him\u00b7mel", ",", "den", "blit\u00b7zen\u00b7den", "Tau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So fr\u00f6hlich gegr\u00fc\u00dft wie heute dein Schneien,", "tokens": ["So", "fr\u00f6h\u00b7lich", "ge\u00b7gr\u00fc\u00dft", "wie", "heu\u00b7te", "dein", "Schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KOKOM", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Nebelgebr\u00e4u und Wolkengrau.", "tokens": ["Dein", "Ne\u00b7bel\u00b7ge\u00b7br\u00e4u", "und", "Wol\u00b7ken\u00b7grau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn durch das Flockengetriebe,", "tokens": ["Denn", "durch", "das", "Flo\u00b7cken\u00b7ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sch\u00f6ner, als je der Lenz gelacht,", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "je", "der", "Lenz", "ge\u00b7lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Leuchtet und bl\u00fcht der Fr\u00fchling der Liebe", "tokens": ["Leuch\u00b7tet", "und", "bl\u00fcht", "der", "Fr\u00fch\u00b7ling", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mir heimlich nun in der Winternacht.", "tokens": ["Mir", "heim\u00b7lich", "nun", "in", "der", "Win\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}