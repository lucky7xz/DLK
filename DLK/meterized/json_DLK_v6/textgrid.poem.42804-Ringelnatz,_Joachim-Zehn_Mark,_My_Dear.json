{"textgrid.poem.42804": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Zehn Mark, My Dear", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heusinger war heute bei mir.", "tokens": ["Heu\u00b7sin\u00b7ger", "war", "heu\u00b7te", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ob ich morgen mit zum Rennen k\u00e4me,", "tokens": ["Ob", "ich", "mor\u00b7gen", "mit", "zum", "Ren\u00b7nen", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weil doch wieder mal sein Pferd My Dear", "tokens": ["Weil", "doch", "wie\u00b7der", "mal", "sein", "Pferd", "My", "De\u00b7ar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "PPOSAT", "NN", "NE", "NE"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "An dem Derby teilnehme.", "tokens": ["An", "dem", "Der\u00b7by", "teil\u00b7neh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "FM", "FM", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das dumme Tier My Dear", "tokens": ["Das", "dum\u00b7me", "Tier", "My", "De\u00b7ar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ist noch gar nicht hier.", "tokens": ["Ist", "noch", "gar", "nicht", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aber es kommt vielleicht,", "tokens": ["A\u00b7ber", "es", "kommt", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Abgeschickt ist es;", "tokens": ["Ab\u00b7ge\u00b7schickt", "ist", "es", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Hat aber noch nie ein Ziel erreicht.", "tokens": ["Hat", "a\u00b7ber", "noch", "nie", "ein", "Ziel", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Den ganzen Tag fri\u00dft es.", "tokens": ["Den", "gan\u00b7zen", "Tag", "fri\u00dft", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Selten steht es.", "tokens": ["Sel\u00b7ten", "steht", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meistens liegt es.", "tokens": ["Meis\u00b7tens", "liegt", "es", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ganz langsam geht es,", "tokens": ["Ganz", "lang\u00b7sam", "geht", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Es sei denn: Man schiebt es,", "tokens": ["Es", "sei", "denn", ":", "Man", "schiebt", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Oder wenn es Hafer sieht, dann fliegt es.", "tokens": ["O\u00b7der", "wenn", "es", "Ha\u00b7fer", "sieht", ",", "dann", "fliegt", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Niemals aber, niemals siegt es.", "tokens": ["Nie\u00b7mals", "a\u00b7ber", ",", "nie\u00b7mals", "siegt", "es", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So ein Pferd! Und so was gibt es!", "tokens": ["So", "ein", "Pferd", "!", "Und", "so", "was", "gibt", "es", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "KON", "ADV", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heusinger nat\u00fcrlich liebt es.", "tokens": ["Heu\u00b7sin\u00b7ger", "na\u00b7t\u00fcr\u00b7lich", "liebt", "es", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "X-Beine hat's", "tokens": ["X\u00b7\u00b7Bei\u00b7ne", "hat's"], "token_info": ["word", "word"], "pos": ["NE", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und sieht aus wie ungeboren.", "tokens": ["Und", "sieht", "aus", "wie", "un\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KOKOM", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcnf Mark Sieg und f\u00fcnf Mark Platz", "tokens": ["F\u00fcnf", "Mark", "Sieg", "und", "f\u00fcnf", "Mark", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "NN", "KON", "CARD", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Hab' ich Rindvieh an dem Ro\u00df verloren.", "tokens": ["Hab'", "ich", "Rind\u00b7vieh", "an", "dem", "Ro\u00df", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Niemals wieder werde", "tokens": ["Nie\u00b7mals", "wie\u00b7der", "wer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich bei einem Rennen", "tokens": ["Ich", "bei", "ei\u00b7nem", "Ren\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wetten, ohne Pferde", "tokens": ["Wet\u00b7ten", ",", "oh\u00b7ne", "Pfer\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUI", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vorher ganz genau zu kennen.", "tokens": ["Vor\u00b7her", "ganz", "ge\u00b7nau", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Stelle dir doch einmal vor:", "tokens": ["Stel\u00b7le", "dir", "doch", "ein\u00b7mal", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zehn Mark Leberk\u00e4se! Zehn Mark Bier!", "tokens": ["Zehn", "Mark", "Le\u00b7ber\u00b7k\u00e4\u00b7se", "!", "Zehn", "Mark", "Bier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$.", "CARD", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Oder sonstwas, was ich an My Dear", "tokens": ["O\u00b7der", "sonst\u00b7was", ",", "was", "ich", "an", "My", "De\u00b7ar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWS", "PPER", "APPR", "NE", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sozusagen Knall und Fall verlor.", "tokens": ["So\u00b7zu\u00b7sa\u00b7gen", "Knall", "und", "Fall", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Nein, man soll nicht aufs Geratewohl riskieren.", "tokens": ["Nein", ",", "man", "soll", "nicht", "aufs", "Ge\u00b7ra\u00b7te\u00b7wohl", "ris\u00b7kie\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VMFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Dann schon lieber in der Lotterie", "tokens": ["Dann", "schon", "lie\u00b7ber", "in", "der", "Lot\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was gewinnen, als um solch ein Vieh", "tokens": ["Was", "ge\u00b7win\u00b7nen", ",", "als", "um", "solch", "ein", "Vieh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "$,", "KOUS", "APPR", "PIAT", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auf betr\u00fcgerische Art sein Geld verlieren.", "tokens": ["Auf", "be\u00b7tr\u00fc\u00b7ge\u00b7ri\u00b7sche", "Art", "sein", "Geld", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}