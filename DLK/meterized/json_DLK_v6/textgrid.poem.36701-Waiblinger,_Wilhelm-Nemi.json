{"textgrid.poem.36701": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nemi", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Suchest du schattiges Gr\u00fcn, so lockt dich der Hain der Diana,", "tokens": ["Su\u00b7chest", "du", "schat\u00b7ti\u00b7ges", "Gr\u00fcn", ",", "so", "lockt", "dich", "der", "Hain", "der", "Di\u00b7a\u00b7na", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NE", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Willst du dich k\u00fchlen, so lockt hier dich zum Bade der See.", "tokens": ["Willst", "du", "dich", "k\u00fch\u00b7len", ",", "so", "lockt", "hier", "dich", "zum", "Ba\u00b7de", "der", "See", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Suchst du Weisheit, Egeria lehrt! Am Ruder des Staates", "tokens": ["Suchst", "du", "Weis\u00b7heit", ",", "E\u00b7ge\u00b7ria", "lehrt", "!", "Am", "Ru\u00b7der", "des", "Staa\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "NE", "VVFIN", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sa\u00df sie mit Numa, und jetzt treibt sie die M\u00fchle des Dorfs.", "tokens": ["Sa\u00df", "sie", "mit", "Nu\u00b7ma", ",", "und", "jetzt", "treibt", "sie", "die", "M\u00fch\u00b7le", "des", "Dorfs", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}}}}}