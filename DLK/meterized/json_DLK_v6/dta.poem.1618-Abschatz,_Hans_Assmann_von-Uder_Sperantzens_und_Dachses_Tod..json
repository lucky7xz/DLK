{"dta.poem.1618": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uder Sperantzens und Dachses Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es will der stoltze Mensch ein Grab von Ertz und Marmol", "tokens": ["Es", "will", "der", "stolt\u00b7ze", "Mensch", "ein", "Grab", "von", "Ertz", "und", "Mar\u00b7mol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "haben;", "tokens": ["ha\u00b7ben", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Man sieht wie Zinn und Kupffer ihm die lezte Wohnung ist.", "tokens": ["Man", "sieht", "wie", "Zinn", "und", "Kupf\u00b7fer", "ihm", "die", "lez\u00b7te", "Woh\u00b7nung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "NN", "KON", "NN", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der treuen Hunde Paar hat sich weit pr\u00e4chtiger begraben/", "tokens": ["Der", "treu\u00b7en", "Hun\u00b7de", "Paar", "hat", "sich", "weit", "pr\u00e4ch\u00b7ti\u00b7ger", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PRF", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Ihm eine Grabst\u00e4tt in Crystall und Spiegeln auserkist.", "tokens": ["Ihm", "ei\u00b7ne", "Grab\u00b7st\u00e4tt", "in", "Crys\u00b7tall", "und", "Spie\u00b7geln", "aus\u00b7er\u00b7kist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Der gro\u00dfe K\u00fcnstler/ die Natur hat solche selbst gebauet.", "tokens": ["Der", "gro\u00b7\u00dfe", "K\u00fcnst\u00b7ler", "/", "die", "Na\u00b7tur", "hat", "sol\u00b7che", "selbst", "ge\u00b7bau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Der gr\u00fcnen Flutt Crystall grub die ber\u00fchmten Br\u00fcder ein/", "tokens": ["Der", "gr\u00fc\u00b7nen", "Flutt", "Crys\u00b7tall", "grub", "die", "be\u00b7r\u00fchm\u00b7ten", "Br\u00fc\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Worauff man ihr gepaartes Licht vom Himmel gl\u00e4ntzend", "tokens": ["Wo\u00b7rauff", "man", "ihr", "ge\u00b7paar\u00b7tes", "Licht", "vom", "Him\u00b7mel", "gl\u00e4nt\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "schauet.", "tokens": ["schau\u00b7et", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Ich wette/ Dachs und Hoffnung wird ein neuer Hunds-", "tokens": ["Ich", "wet\u00b7te", "/", "Dachs", "und", "Hoff\u00b7nung", "wird", "ein", "neu\u00b7er", "Hunds"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "NN", "KON", "NN", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Stern seyn.", "tokens": ["Stern", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}