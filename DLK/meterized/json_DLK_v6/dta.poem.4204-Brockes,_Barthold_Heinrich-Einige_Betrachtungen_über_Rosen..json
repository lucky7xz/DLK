{"dta.poem.4204": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige Betrachtungen \u00fcber Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Rosen Monat war nunmehr aufs neue wiederum", "tokens": ["Der", "Ro\u00b7sen", "Mo\u00b7nat", "war", "nun\u00b7mehr", "aufs", "neu\u00b7e", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "APPRART", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "erschienen,", "tokens": ["er\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Man sah, mit einer neuen Lust, auf dem so holden Dunkel-", "tokens": ["Man", "sah", ",", "mit", "ei\u00b7ner", "neu\u00b7en", "Lust", ",", "auf", "dem", "so", "hol\u00b7den", "Dun\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "gr\u00fcnen", "tokens": ["gr\u00fc\u00b7nen"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der Rosen-B\u00fcsch\u2019 ein hell Gepr\u00e4nge,", "tokens": ["Der", "Ro\u00b7sen\u00b7B\u00fcsch'", "ein", "hell", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In einer kaum zu zehl\u2019nden Menge,", "tokens": ["In", "ei\u00b7ner", "kaum", "zu", "zehl'n\u00b7den", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von aufgebrochnen Rosen bl\u00fchn,", "tokens": ["Von", "auf\u00b7ge\u00b7broch\u00b7nen", "Ro\u00b7sen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und im verschiednen Feur, doch meist im rohten, gl\u00fchn.", "tokens": ["Und", "im", "ver\u00b7schied\u00b7nen", "Feur", ",", "doch", "meist", "im", "roh\u00b7ten", ",", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "ADV", "ADV", "APPRART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Um nun durch sie noch mehr den Augen liebzukosen,", "tokens": ["Um", "nun", "durch", "sie", "noch", "mehr", "den", "Au\u00b7gen", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df ich roht- gelbe, wei\u00df\u2019 auch Wein- und E\u00dfig-Rosen,", "tokens": ["Lie\u00df", "ich", "roht", "gel\u00b7be", ",", "wei\u00df'", "auch", "Wein", "und", "E\u00b7\u00dfig\u00b7Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "TRUNC", "ADJA", "$,", "VVFIN", "ADV", "TRUNC", "KON", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die so verschiedne Farben schm\u00fccken,", "tokens": ["Die", "so", "ver\u00b7schied\u00b7ne", "Far\u00b7ben", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von unterschiednen Arten, pfl\u00fccken,", "tokens": ["Von", "un\u00b7ter\u00b7schied\u00b7nen", "Ar\u00b7ten", ",", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einem Spiegel von Krystallen", "tokens": ["Auf", "ei\u00b7nem", "Spie\u00b7gel", "von", "Krys\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihren gr\u00fcnen Bl\u00e4ttern legen,", "tokens": ["Mit", "ih\u00b7ren", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7tern", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sah, mit fr\u00f6hlichem Erwegen,", "tokens": ["Und", "sah", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den sch\u00f6nen Schmuck, der Farben Pracht von allen,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Schmuck", ",", "der", "Far\u00b7ben", "Pracht", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ins glatte Glas gedoppelt fallen.", "tokens": ["Ins", "glat\u00b7te", "Glas", "ge\u00b7dop\u00b7pelt", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hiedurch verdoppelte sich mein Vergn\u00fcgen", "tokens": ["Hie\u00b7durch", "ver\u00b7dop\u00b7pel\u00b7te", "sich", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In der vermehrten Pracht nicht nur;", "tokens": ["In", "der", "ver\u00b7mehr\u00b7ten", "Pracht", "nicht", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es schien der Farben Zier, und jegliche Figur,", "tokens": ["Es", "schien", "der", "Far\u00b7ben", "Zier", ",", "und", "jeg\u00b7li\u00b7che", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In dem gefulgten Glas, und seinem klaren Dunkeln,", "tokens": ["In", "dem", "ge\u00b7fulg\u00b7ten", "Glas", ",", "und", "sei\u00b7nem", "kla\u00b7ren", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Noch angenehmer fast zu funkeln,", "tokens": ["Noch", "an\u00b7ge\u00b7neh\u00b7mer", "fast", "zu", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ja in noch hell- und klarerm Strahl,", "tokens": ["Ja", "in", "noch", "hell", "und", "kla\u00b7rerm", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ADV", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Als selber das Original.", "tokens": ["Als", "sel\u00b7ber", "das", "O\u00b7rig\u00b7i\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wie ich nun bald den hellen Wiederschein", "tokens": ["Wie", "ich", "nun", "bald", "den", "hel\u00b7len", "Wie\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von dieser gleichsam hohlen Tiefe,", "tokens": ["Von", "die\u00b7ser", "gleich\u00b7sam", "hoh\u00b7len", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit frohen Blicken, \u00fcberliefe;", "tokens": ["Mit", "fro\u00b7hen", "Bli\u00b7cken", ",", "\u00fc\u00b7berl\u00b7ie\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nahm ein besondrer Reiz mich ein.", "tokens": ["Nahm", "ein", "be\u00b7sond\u00b7rer", "Reiz", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der vielen Rosen Zierlichkeit,", "tokens": ["Der", "vie\u00b7len", "Ro\u00b7sen", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der vielen Farben Unterscheid,", "tokens": ["Der", "vie\u00b7len", "Far\u00b7ben", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewegten mich aufs neu, wie wundersch\u00f6n", "tokens": ["Be\u00b7weg\u00b7ten", "mich", "aufs", "neu", ",", "wie", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So Farb\u2019 als Form in ihnen, anzusehn.", "tokens": ["So", "Fa\u00b7rb'", "als", "Form", "in", "ih\u00b7nen", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "APPR", "PPER", "$,", "VVIZU", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Der an der ", "tokens": ["Der", "an", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Den insbesondere die ", "tokens": ["Den", "ins\u00b7be\u00b7son\u00b7de\u00b7re", "die"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ART"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "So kaum an Glanz dem Silber wichen,", "tokens": ["So", "kaum", "an", "Glanz", "dem", "Sil\u00b7ber", "wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die frey- und fl\u00fccht\u2019gen Bl\u00e4tter schm\u00fccken,", "tokens": ["Die", "frey", "und", "fl\u00fccht'\u00b7gen", "Bl\u00e4t\u00b7ter", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Worinn ein Gelb, wie Gold, vergn\u00fcglich zu erblicken;", "tokens": ["Wo\u00b7rinn", "ein", "Gelb", ",", "wie", "Gold", ",", "ver\u00b7gn\u00fcg\u00b7lich", "zu", "er\u00b7bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "So, durch des Laubes Dunkelgr\u00fcn,", "tokens": ["So", ",", "durch", "des", "Lau\u00b7bes", "Dun\u00b7kel\u00b7gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das sie noch angenehmer macht,", "tokens": ["Das", "sie", "noch", "an\u00b7ge\u00b7neh\u00b7mer", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Noch desto mehr erh\u00f6ht und sch\u00f6ner schien;", "tokens": ["Noch", "des\u00b7to", "mehr", "er\u00b7h\u00f6ht", "und", "sch\u00f6\u00b7ner", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Und an der Farb\u2019 ihm wenigstens kaum weichet;", "tokens": ["Und", "an", "der", "Fa\u00b7rb'", "ihm", "we\u00b7nigs\u00b7tens", "kaum", "wei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und endlich die aus ", "tokens": ["Und", "end\u00b7lich", "die", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "In lieblich zarter Gluht und s\u00fcssen Flammen,", "tokens": ["In", "lieb\u00b7lich", "zar\u00b7ter", "Gluht", "und", "s\u00fcs\u00b7sen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Noch reizender, als alle fast zusammen,", "tokens": ["Noch", "rei\u00b7zen\u00b7der", ",", "als", "al\u00b7le", "fast", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Am allermeisten noch das Aug\u2019 erfrischt.", "tokens": ["Am", "al\u00b7ler\u00b7meis\u00b7ten", "noch", "das", "Aug'", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die, sag\u2019 ich, lagen all\u2019 in einem bunten Schein,", "tokens": ["Die", ",", "sag'", "ich", ",", "la\u00b7gen", "all'", "in", "ei\u00b7nem", "bun\u00b7ten", "Schein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und drungen einzeln bald, bald insgesammt,", "tokens": ["Und", "drun\u00b7gen", "ein\u00b7zeln", "bald", ",", "bald", "ins\u00b7ge\u00b7sammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da jegliche f\u00fcr sich, auch in Gemeinschaft, flammt,", "tokens": ["Da", "jeg\u00b7li\u00b7che", "f\u00fcr", "sich", ",", "auch", "in", "Ge\u00b7mein\u00b7schaft", ",", "flammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "PRF", "$,", "ADV", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit solchem Reiz in aller Augen ein,", "tokens": ["Mit", "sol\u00b7chem", "Reiz", "in", "al\u00b7ler", "Au\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR",