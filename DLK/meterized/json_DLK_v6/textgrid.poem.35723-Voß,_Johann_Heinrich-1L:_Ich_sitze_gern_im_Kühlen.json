{"textgrid.poem.35723": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sitze gern im K\u00fchlen", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sitze gern im K\u00fchlen", "tokens": ["Ich", "sit\u00b7ze", "gern", "im", "K\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf meiner Kn\u00fcppelbank,", "tokens": ["Auf", "mei\u00b7ner", "Kn\u00fcp\u00b7pel\u00b7bank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seh' im Winde w\u00fchlen", "tokens": ["Und", "seh'", "im", "Win\u00b7de", "w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Roggenfeld entlang.", "tokens": ["Das", "Rog\u00b7gen\u00b7feld", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann flecht' ich St\u00fchl' und K\u00f6rbe,", "tokens": ["Dann", "flecht'", "ich", "St\u00fchl'", "und", "K\u00f6r\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sing', und denke wohl:", "tokens": ["Und", "sing'", ",", "und", "den\u00b7ke", "wohl", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bald sagt des Holzes Kerbe,", "tokens": ["Bald", "sagt", "des", "Hol\u00b7zes", "Ker\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die vierte Stieg' ist voll.", "tokens": ["Die", "vier\u00b7te", "Stieg'", "ist", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie unvermerkt doch schlendert", "tokens": ["Wie", "un\u00b7ver\u00b7merkt", "doch", "schlen\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die liebe Zeit dahin!", "tokens": ["Die", "lie\u00b7be", "Zeit", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar viel hat sich ver\u00e4ndert,", "tokens": ["Gar", "viel", "hat", "sich", "ver\u00b7\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seit ich im Dorfe bin.", "tokens": ["Seit", "ich", "im", "Dor\u00b7fe", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So manches Jugendspielers", "tokens": ["So", "man\u00b7ches", "Ju\u00b7gend\u00b7spie\u00b7lers"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gedenk' ich: Ach der war!", "tokens": ["Ge\u00b7denk'", "ich", ":", "Ach", "der", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "ART", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Sohn des Nebensch\u00fclers", "tokens": ["Der", "Sohn", "des", "Ne\u00b7ben\u00b7sch\u00fc\u00b7lers"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat auch schon graues Haar.", "tokens": ["Hat", "auch", "schon", "grau\u00b7es", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer h\u00f6ren mag, der h\u00f6ret", "tokens": ["Wer", "h\u00f6\u00b7ren", "mag", ",", "der", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich oft von alter Zeit:", "tokens": ["Mich", "oft", "von", "al\u00b7ter", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer da und dort verkehret,", "tokens": ["Wer", "da", "und", "dort", "ver\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer dies und das verneut.", "tokens": ["Wer", "dies", "und", "das", "ver\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df des Krams nicht minder,", "tokens": ["Ich", "wei\u00df", "des", "Krams", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als unsers Kirchturms Knopf;", "tokens": ["Als", "un\u00b7sers", "Kirch\u00b7turms", "Knopf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Neue nur, ihr Kinder,", "tokens": ["Das", "Neu\u00b7e", "nur", ",", "ihr", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Behalt' ich nicht im Kopf.", "tokens": ["Be\u00b7halt'", "ich", "nicht", "im", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich mag's auch nicht behalten,", "tokens": ["Ich", "mag's", "auch", "nicht", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob's abschreckt oder k\u00f6rnt;", "tokens": ["Ob's", "ab\u00b7schreckt", "o\u00b7der", "k\u00f6rnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe l\u00e4ngst am Alten", "tokens": ["Ich", "ha\u00b7be", "l\u00e4ngst", "am", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Spr\u00fcchlein ausgelernt:", "tokens": ["Mein", "Spr\u00fcch\u00b7lein", "aus\u00b7ge\u00b7lernt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Mensch im Anfang launet,", "tokens": ["Der", "Mensch", "im", "An\u00b7fang", "lau\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und findet manches hart;", "tokens": ["Und", "fin\u00b7det", "man\u00b7ches", "hart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er wird's gewohnt, und staunet,", "tokens": ["Er", "wird's", "ge\u00b7wohnt", ",", "und", "stau\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie gut es endlich ward.", "tokens": ["Wie", "gut", "es", "end\u00b7lich", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du wirk', ohn' umzugaffen,", "tokens": ["Du", "wirk'", ",", "ohn'", "um\u00b7zu\u00b7gaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und \u00fcbe deine Pflicht.", "tokens": ["Und", "\u00fc\u00b7be", "dei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will Gott was Neues schaffen,", "tokens": ["Will", "Gott", "was", "Neu\u00b7es", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So widerstrebe nicht.", "tokens": ["So", "wi\u00b7der\u00b7stre\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie seltsam er oft bessert,", "tokens": ["Wie", "selt\u00b7sam", "er", "oft", "bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Er \u00fcbersieht uns weit:", "tokens": ["Er", "\u00fc\u00b7ber\u00b7sieht", "uns", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was klein war, wird vergr\u00f6\u00dfert,", "tokens": ["Was", "klein", "war", ",", "wird", "ver\u00b7gr\u00f6\u00b7\u00dfert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Gro\u00dfe wird zerstreut.", "tokens": ["Das", "Gro\u00b7\u00dfe", "wird", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "F\u00fcrwahr im Himmel waltet,", "tokens": ["F\u00fcr\u00b7wahr", "im", "Him\u00b7mel", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der wohl zu walten wei\u00df;", "tokens": ["Der", "wohl", "zu", "wal\u00b7ten", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Alte, der nie altet,", "tokens": ["Der", "Al\u00b7te", ",", "der", "nie", "al\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der lenkt der Dinge Gleis.", "tokens": ["Der", "lenkt", "der", "Din\u00b7ge", "Gleis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gewitter, Sturm und Regen", "tokens": ["Ge\u00b7wit\u00b7ter", ",", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erheitern Luft und Flur.", "tokens": ["Er\u00b7hei\u00b7tern", "Luft", "und", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bebt nicht vor Donnerschl\u00e4gen;", "tokens": ["Bebt", "nicht", "vor", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Alte bessert nur.", "tokens": ["Der", "Al\u00b7te", "bes\u00b7sert", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Jetzt naht er manchem Volke", "tokens": ["Jetzt", "naht", "er", "man\u00b7chem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Strafgericht und Graus,", "tokens": ["Mit", "Straf\u00b7ge\u00b7richt", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und donnert aus der Wolke;", "tokens": ["Und", "don\u00b7nert", "aus", "der", "Wol\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Getrost! er bessert aus.", "tokens": ["Ge\u00b7trost", "!", "er", "bes\u00b7sert", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum la\u00df ich ohne Kummer", "tokens": ["Drum", "la\u00df", "ich", "oh\u00b7ne", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es gehen, wie es geht:", "tokens": ["Es", "ge\u00b7hen", ",", "wie", "es", "geht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als ob in halbem Schlummer", "tokens": ["Als", "ob", "in", "hal\u00b7bem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um mich der Schatten weht.", "tokens": ["Um", "mich", "der", "Schat\u00b7ten", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}