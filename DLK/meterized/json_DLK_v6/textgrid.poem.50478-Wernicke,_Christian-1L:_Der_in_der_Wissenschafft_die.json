{"textgrid.poem.50478": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der in der Wissenschafft die ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der in der Wissenschafft die ", "tokens": ["Der", "in", "der", "Wis\u00b7sen\u00b7schafft", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Friedrich die ", "tokens": ["In", "Fried\u00b7rich", "die"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Als d\u00e4cht' er nur auf das, ", "tokens": ["Als", "d\u00e4cht'", "er", "nur", "auf", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "APPR", "ART", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der die ", "tokens": ["Der", "die"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "So woll als ", "tokens": ["So", "woll", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Der nichts als ", "tokens": ["Der", "nichts", "als"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Und in der ", "tokens": ["Und", "in", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Der es mit allen redlich meint,", "tokens": ["Der", "es", "mit", "al\u00b7len", "red\u00b7lich", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der ist es den der Nord mit recht ", "tokens": ["Der", "ist", "es", "den", "der", "Nord", "mit", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ART", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Doch den man itzt im ", "tokens": ["Doch", "den", "man", "itzt", "im"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "ADV", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Mehr als ", "tokens": ["Mehr", "als"], "token_info": ["word", "word"], "pos": ["PIAT", "KOKOM"], "meter": "+-", "measure": "trochaic.single"}}}}}