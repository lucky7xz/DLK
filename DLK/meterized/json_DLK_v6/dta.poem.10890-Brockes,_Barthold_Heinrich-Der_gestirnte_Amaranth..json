{"dta.poem.10890": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gestirnte Amaranth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welche neue Zierlichkeit, so an Farb, als an Figur,", "tokens": ["Wel\u00b7che", "neu\u00b7e", "Zier\u00b7lich\u00b7keit", ",", "so", "an", "Farb", ",", "als", "an", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-++-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Zeugt, in zubewundernder Pracht und Sch\u00f6nheit, die", "tokens": ["Zeugt", ",", "in", "zu\u00b7be\u00b7wun\u00b7dern\u00b7der", "Pracht", "und", "Sch\u00f6n\u00b7heit", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,", "PRELS"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jm gestirnten Amaranth! welch ein Purpur! welch ein Glanz!", "tokens": ["Jm", "ge\u00b7stirn\u00b7ten", "A\u00b7ma\u00b7ran\u00b7th", "!", "welch", "ein", "Pur\u00b7pur", "!", "welch", "ein", "Glanz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PWAT", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "+-+-+--++-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Welche nett geformte R\u00fcnde! die dennoch aus lauter Spitzen,", "tokens": ["Wel\u00b7che", "nett", "ge\u00b7form\u00b7te", "R\u00fcn\u00b7de", "!", "die", "den\u00b7noch", "aus", "lau\u00b7ter", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJA", "NN", "$.", "ART", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "So nach einem klugen Rang, und besondrer Ordnung, sitzen,", "tokens": ["So", "nach", "ei\u00b7nem", "klu\u00b7gen", "Rang", ",", "und", "be\u00b7sond\u00b7rer", "Ord\u00b7nung", ",", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Recht bewunderns-werth bestehn!", "tokens": ["Recht", "be\u00b7wun\u00b7derns\u00b7werth", "be\u00b7stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber seht ihr, zwischen ihnen, in dem Purpur, der so sch\u00f6n,", "tokens": ["A\u00b7ber", "seht", "ihr", ",", "zwi\u00b7schen", "ih\u00b7nen", ",", "in", "dem", "Pur\u00b7pur", ",", "der", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "In ganz unverhofftem Schim\u0303er, Silber-weisse Sternchen blitzen?", "tokens": ["In", "ganz", "un\u00b7ver\u00b7hoff\u00b7tem", "Schim\u0303er", ",", "Sil\u00b7ber\u00b7weis\u00b7se", "Stern\u00b7chen", "blit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Ist es m\u00f6glich? das ist rar! aber la\u00dft uns weiter gehn,", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", "?", "das", "ist", "rar", "!", "a\u00b7ber", "la\u00dft", "uns", "wei\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "PDS", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und die weisse Blume dort, ob auch die gestirnt, besehn,", "tokens": ["Und", "die", "weis\u00b7se", "Blu\u00b7me", "dort", ",", "ob", "auch", "die", "ge\u00b7stirnt", ",", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$,", "KOUS", "ADV", "ART", "VVPP", "$,", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ja wahrhaftig, ebenfalls. Und noch mehr, die sind verg\u00fcldet.", "tokens": ["Ja", "wahr\u00b7haf\u00b7tig", ",", "e\u00b7ben\u00b7falls", ".", "Und", "noch", "mehr", ",", "die", "sind", "ver\u00b7g\u00fcl\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,", "ADV", "$.", "KON", "ADV", "ADV", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Lieber Gott! so Farb als Formen zeigen, in dem Bl\u00fcm-", "tokens": ["Lie\u00b7ber", "Gott", "!", "so", "Farb", "als", "For\u00b7men", "zei\u00b7gen", ",", "in", "dem", "Bl\u00fcm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "ADV", "NN", "KOUS", "NN", "VVINF", "$,", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "Dir zu deinen heilgen Ehren, eine neue Wunder-Zier.", "tokens": ["Dir", "zu", "dei\u00b7nen", "heil\u00b7gen", "Eh\u00b7ren", ",", "ei\u00b7ne", "neu\u00b7e", "Wun\u00b7der\u00b7Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Ich bewunderte den Rang, so der Bl\u00e4tter, als der Sternen,", "tokens": ["Ich", "be\u00b7wun\u00b7der\u00b7te", "den", "Rang", ",", "so", "der", "Bl\u00e4t\u00b7ter", ",", "als", "der", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Und betrachtete sie n\u00e4her, um, wie sie formirt, zu lernen.", "tokens": ["Und", "be\u00b7trach\u00b7te\u00b7te", "sie", "n\u00e4\u00b7her", ",", "um", ",", "wie", "sie", "for\u00b7mirt", ",", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPER", "ADJD", "$,", "KOUI", "$,", "PWAV", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+----+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da ich denn, nicht ohn ein billig Stutzen und Erstaunen, fand,", "tokens": ["Da", "ich", "denn", ",", "nicht", "ohn", "ein", "bil\u00b7lig", "Stut\u00b7zen", "und", "Er\u00b7stau\u00b7nen", ",", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PTKNEG", "APPR", "ART", "ADJD", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "Da\u00df ein jedes Sternchen immer, mitten in zwey Bl\u00e4ttern, stand,", "tokens": ["Da\u00df", "ein", "je\u00b7des", "Stern\u00b7chen", "im\u00b7mer", ",", "mit\u00b7ten", "in", "zwey", "Bl\u00e4t\u00b7tern", ",", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "ADV", "$,", "ADV", "APPR", "CARD", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Welche eine nette kleine H\u00fclse dergestalt vereint,", "tokens": ["Wel\u00b7che", "ei\u00b7ne", "net\u00b7te", "klei\u00b7ne", "H\u00fcl\u00b7se", "der\u00b7ge\u00b7stalt", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Da\u00df es nur ein einzigs Blatt, so sich oben theilet, scheint,", "tokens": ["Da\u00df", "es", "nur", "ein", "ein\u00b7zigs", "Blatt", ",", "so", "sich", "o\u00b7ben", "thei\u00b7let", ",", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.20": {"text": "Ob nun gleich die ganze H\u00fclse, wie an einer Aehre, platt,", "tokens": ["Ob", "nun", "gleich", "die", "gan\u00b7ze", "H\u00fcl\u00b7se", ",", "wie", "an", "ei\u00b7ner", "A\u00b7eh\u00b7re", ",", "platt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.21": {"text": "Ist dennoch, wenn mans betrachtet, jedes kleine schmale Blatt", "tokens": ["Ist", "den\u00b7noch", ",", "wenn", "mans", "be\u00b7trach\u00b7tet", ",", "je\u00b7des", "klei\u00b7ne", "schma\u00b7le", "Blatt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PIS", "VVPP", "$,", "PIAT", "ADJA", "ADJA", "NN"], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.22": {"text": "In sich an den Seiten hohl, und verschr\u00e4nkt ein wolligt", "tokens": ["In", "sich", "an", "den", "Sei\u00b7ten", "hohl", ",", "und", "ver\u00b7schr\u00e4nkt", "ein", "wol\u00b7ligt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ART", "NN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Dieses, sechs gesteifte Spitzen,", "tokens": ["Die\u00b7ses", ",", "sechs", "ge\u00b7steif\u00b7te", "Spit\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Welche das so nette Sternchen rings um gleichsam unterst\u00fctzen.", "tokens": ["Wel\u00b7che", "das", "so", "net\u00b7te", "Stern\u00b7chen", "rings", "um", "gleich\u00b7sam", "un\u00b7ter\u00b7st\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "ADJA", "NN", "ADV", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Letzlich ist das Sternchen selbst, in der Mitten, hell und sch\u00f6n,", "tokens": ["Letz\u00b7lich", "ist", "das", "Stern\u00b7chen", "selbst", ",", "in", "der", "Mit\u00b7ten", ",", "hell", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "In dem Purpur, weis, wie Silber, und so gelb, als Gold, zu sehn,", "tokens": ["In", "dem", "Pur\u00b7pur", ",", "weis", ",", "wie", "Sil\u00b7ber", ",", "und", "so", "gelb", ",", "als", "Gold", ",", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PTKVZ", "$,", "PWAV", "NN", "$,", "KON", "ADV", "ADJD", "$,", "KOUS", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.27": {"text": "In dem weissen Amaranth. Was man ferner an ihr sieht,", "tokens": ["In", "dem", "weis\u00b7sen", "A\u00b7ma\u00b7ran\u00b7th", ".", "Was", "man", "fer\u00b7ner", "an", "ihr", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PWS", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Ist, da\u00df sie, wie andre Blumen, nicht verwelkt; nein, lange", "tokens": ["Ist", ",", "da\u00df", "sie", ",", "wie", "and\u00b7re", "Blu\u00b7men", ",", "nicht", "ver\u00b7welkt", ";", "nein", ",", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "$,", "PWAV", "ADJA", "NN", "$,", "PTKNEG", "VVPP", "$.", "PTKANT", "$,", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "Durch der Bl\u00e4tter Festigkeit, die in ihren engen R\u00f6hren,", "tokens": ["Durch", "der", "Bl\u00e4t\u00b7ter", "Fes\u00b7tig\u00b7keit", ",", "die", "in", "ih\u00b7ren", "en\u00b7gen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Deren Theilchen dicht- und fester, weil sie wenig S\u00e4fte n\u00e4hren,", "tokens": ["De\u00b7ren", "Theil\u00b7chen", "dicht", "und", "fes\u00b7ter", ",", "weil", "sie", "we\u00b7nig", "S\u00e4f\u00b7te", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "TRUNC", "KON", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.31": {"text": "Dauren sie, auch sonder Wasser. Eben dieser Unterscheid", "tokens": ["Dau\u00b7ren", "sie", ",", "auch", "son\u00b7der", "Was\u00b7ser", ".", "E\u00b7ben", "die\u00b7ser", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$.", "ADV", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Zeigt aufs neu uns offenbar, auch in der Verschiedenheit,", "tokens": ["Zeigt", "aufs", "neu", "uns", "of\u00b7fen\u00b7bar", ",", "auch", "in", "der", "Ver\u00b7schie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJD", "PPER", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Da\u00df es Gott nicht minder m\u00f6glich, auch von festerem Ver-", "tokens": ["Da\u00df", "es", "Gott", "nicht", "min\u00b7der", "m\u00f6g\u00b7lich", ",", "auch", "von", "fes\u00b7te\u00b7rem", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.34": {"text": "Blumen uns hervor zu bringen.", "tokens": ["Blu\u00b7men", "uns", "her\u00b7vor", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Machst demnach du, liebste Blume, auch durch deinen Bau", "tokens": ["Machst", "dem\u00b7nach", "du", ",", "liebs\u00b7te", "Blu\u00b7me", ",", "auch", "durch", "dei\u00b7nen", "Bau"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.36": {"text": "Da auch du, nach Maa\u00df und Ordnung, eingerichtet und ge-", "tokens": ["Da", "auch", "du", ",", "nach", "Maa\u00df", "und", "Ord\u00b7nung", ",", "ein\u00b7ge\u00b7rich\u00b7tet", "und", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,", "VVPP", "KON", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.37": {"text": "Da\u00df ein weises Wesen dich wunderbar hervorgebracht,", "tokens": ["Da\u00df", "ein", "wei\u00b7ses", "We\u00b7sen", "dich", "wun\u00