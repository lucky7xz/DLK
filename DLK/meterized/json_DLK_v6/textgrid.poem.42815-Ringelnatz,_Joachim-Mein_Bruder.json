{"textgrid.poem.42815": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Mein Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Bruder l\u00f6st immer Probleme.", "tokens": ["Mein", "Bru\u00b7der", "l\u00f6st", "im\u00b7mer", "Prob\u00b7le\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Bruder verfolgt ein Ziel.", "tokens": ["Mein", "Bru\u00b7der", "ver\u00b7folgt", "ein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich nennt er eine bequeme", "tokens": ["Mich", "nennt", "er", "ei\u00b7ne", "be\u00b7que\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlawinernatur ohne Stil.", "tokens": ["Schla\u00b7wi\u00b7ner\u00b7na\u00b7tur", "oh\u00b7ne", "Stil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Bruder wohnt \u2013 Ehrensache \u2013", "tokens": ["Mein", "Bru\u00b7der", "wohnt", "\u2013", "Eh\u00b7ren\u00b7sa\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sagt, er habe Niveau.", "tokens": ["Und", "sagt", ",", "er", "ha\u00b7be", "Ni\u00b7vea\u00b7u", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wenn ich dar\u00fcber lache,", "tokens": ["Doch", "wenn", "ich", "da\u00b7r\u00fc\u00b7ber", "la\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beschimpft er mich: Ich sei roh.", "tokens": ["Be\u00b7schimpft", "er", "mich", ":", "Ich", "sei", "roh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Bruder mu\u00df Rechnung tragen", "tokens": ["Mein", "Bru\u00b7der", "mu\u00df", "Rech\u00b7nung", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und spricht gern \u00fcber Kultur.", "tokens": ["Und", "spricht", "gern", "\u00fc\u00b7ber", "Kul\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mich hat er einmal geschlagen,", "tokens": ["Mich", "hat", "er", "ein\u00b7mal", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil mir dabei was entfuhr.", "tokens": ["Weil", "mir", "da\u00b7bei", "was", "ent\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PWS", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Mein Bruder haut mich sehr h\u00e4ufig.", "tokens": ["Mein", "Bru\u00b7der", "haut", "mich", "sehr", "h\u00e4u\u00b7fig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er nennt das dann \u00bbaus Prinzip\u00ab.", "tokens": ["Er", "nennt", "das", "dann", "\u00bb", "aus", "Prin\u00b7zip", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "$(", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solche Worte sind ihm gel\u00e4ufig.", "tokens": ["Sol\u00b7che", "Wor\u00b7te", "sind", "ihm", "ge\u00b7l\u00e4u\u00b7fig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ich habe ihn deshalb so lieb.", "tokens": ["Ich", "ha\u00b7be", "ihn", "des\u00b7halb", "so", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich w\u00fcrde ihn auch gern mal hauen.", "tokens": ["Ich", "w\u00fcr\u00b7de", "ihn", "auch", "gern", "mal", "hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch er ist leider sehr stark.", "tokens": ["Doch", "er", "ist", "lei\u00b7der", "sehr", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur wenn er Gl\u00fcck hat bei Frauen,", "tokens": ["Nur", "wenn", "er", "Gl\u00fcck", "hat", "bei", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann schenkt er mir immer zwei Mark.", "tokens": ["Dann", "schenkt", "er", "mir", "im\u00b7mer", "zwei", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "CARD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich bin zwar ein saudummes Luder,", "tokens": ["Ich", "bin", "zwar", "ein", "sau\u00b7dum\u00b7mes", "Lu\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Meine beiden Beine sind schief.", "tokens": ["Mei\u00b7ne", "bei\u00b7den", "Bei\u00b7ne", "sind", "schief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Im \u00fcbrigen ist mein Bruder", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "ist", "mein", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gar nicht verwandt, sondern stief.", "tokens": ["Gar", "nicht", "ver\u00b7wandt", ",", "son\u00b7dern", "stief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Doch wenn ich \u00bbgestiefelter Kater\u00ab", "tokens": ["Doch", "wenn", "ich", "\u00bb", "ge\u00b7stie\u00b7fel\u00b7ter", "Ka\u00b7ter", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihn nenne, dann sch\u00e4umt er wie Most", "tokens": ["Ihn", "nen\u00b7ne", ",", "dann", "sch\u00e4umt", "er", "wie", "Most"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "KOKOM", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und schreibt Beschwerden an Vater,", "tokens": ["Und", "schreibt", "Be\u00b7schwer\u00b7den", "an", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und die trage ich dann zur Post.", "tokens": ["Und", "die", "tra\u00b7ge", "ich", "dann", "zur", "Post", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Ich trage ihm alle Pakete,", "tokens": ["Ich", "tra\u00b7ge", "ihm", "al\u00b7le", "Pa\u00b7ke\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die gr\u00f6\u00dfer sind, als er denkt.", "tokens": ["Die", "gr\u00f6\u00b7\u00dfer", "sind", ",", "als", "er", "denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzt hat er meine Trompete", "tokens": ["Jetzt", "hat", "er", "mei\u00b7ne", "Trom\u00b7pe\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hinter meinem R\u00fccken verschenkt.", "tokens": ["Hin\u00b7ter", "mei\u00b7nem", "R\u00fc\u00b7cken", "ver\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Ein Bischof hat einen braunen", "tokens": ["Ein", "Bi\u00b7schof", "hat", "ei\u00b7nen", "brau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frack meinem Bruder verehrt.", "tokens": ["Frack", "mei\u00b7nem", "Bru\u00b7der", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie w\u00fcrden \u00fcberhaupt staunen,", "tokens": ["Sie", "w\u00fcr\u00b7den", "\u00fc\u00b7ber\u00b7haupt", "stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "mit wem mein Bruder verkehrt.", "tokens": ["mit", "wem", "mein", "Bru\u00b7der", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Dagegen lebe ich \u2013 meint er \u2013", "tokens": ["Da\u00b7ge\u00b7gen", "le\u00b7be", "ich", "\u2013", "meint", "er", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ganz stur wie ein Vieh in den Tag.", "tokens": ["Ganz", "stur", "wie", "ein", "Vieh", "in", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Manchmal, wo Damen sind, weint er;", "tokens": ["Manch\u00b7mal", ",", "wo", "Da\u00b7men", "sind", ",", "weint", "er", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So einer stirbt mal am Schlag.", "tokens": ["So", "ei\u00b7ner", "stirbt", "mal", "am", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}