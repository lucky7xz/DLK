{"textgrid.poem.50877": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch lustiger Wald um das graue Schlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch lustiger Wald um das graue Schlo\u00df", "tokens": ["Welch", "lus\u00b7ti\u00b7ger", "Wald", "um", "das", "grau\u00b7e", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hat sich zusammengefunden,", "tokens": ["Hat", "sich", "zu\u00b7sam\u00b7men\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein gr\u00fcnes bewegliches Nadelgeh\u00f6lz,", "tokens": ["Ein", "gr\u00fc\u00b7nes", "be\u00b7weg\u00b7li\u00b7ches", "Na\u00b7del\u00b7ge\u00b7h\u00f6lz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von keiner Wurzel gebunden!", "tokens": ["Von", "kei\u00b7ner", "Wur\u00b7zel", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Anstatt der warmen Sonne scheint", "tokens": ["An\u00b7statt", "der", "war\u00b7men", "Son\u00b7ne", "scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Rauschgold durch die Wipfel;", "tokens": ["Das", "Rauschgold", "durch", "die", "Wip\u00b7fel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hier backt man Kuchen, dort br\u00e4t man Wurst,", "tokens": ["Hier", "backt", "man", "Ku\u00b7chen", ",", "dort", "br\u00e4t", "man", "Wurst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "ADV", "VVFIN", "PIS", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das R\u00e4uchlein zieht um die Gipfel.", "tokens": ["Das", "R\u00e4uch\u00b7lein", "zieht", "um", "die", "Gip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist ein fr\u00f6hliches Leben im Wald,", "tokens": ["Es", "ist", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Le\u00b7ben", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Volk erf\u00fcllet die R\u00e4ume;", "tokens": ["Das", "Volk", "er\u00b7f\u00fcl\u00b7let", "die", "R\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die nie mit Tr\u00e4nen ein Reis gepflanzt,", "tokens": ["Die", "nie", "mit", "Tr\u00e4\u00b7nen", "ein", "Reis", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die f\u00e4llen am frohsten die B\u00e4ume.", "tokens": ["Die", "f\u00e4l\u00b7len", "am", "frohs\u00b7ten", "die", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der eine kauft ein bescheidnes Gew\u00e4chs", "tokens": ["Der", "ei\u00b7ne", "kauft", "ein", "be\u00b7scheid\u00b7nes", "Ge\u00b7w\u00e4chs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu \u00fcberreichen Geschenken,", "tokens": ["Zu", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "Ge\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der andre einen gewaltigen Strauch,", "tokens": ["Der", "and\u00b7re", "ei\u00b7nen", "ge\u00b7wal\u00b7ti\u00b7gen", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drei N\u00fcsse daran zu henken.", "tokens": ["Drei", "N\u00fcs\u00b7se", "da\u00b7ran", "zu", "hen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort feilscht um ein verkr\u00fcppeltes Reis", "tokens": ["Dort", "feilscht", "um", "ein", "ver\u00b7kr\u00fcp\u00b7pel\u00b7tes", "Reis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Weib mit scharfen Waffen:", "tokens": ["Ein", "Weib", "mit", "schar\u00b7fen", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der d\u00fcnne Silberling soll zugleich", "tokens": ["Der", "d\u00fcn\u00b7ne", "Sil\u00b7ber\u00b7ling", "soll", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Baum und die Fr\u00fcchte verschaffen!", "tokens": ["Den", "Baum", "und", "die", "Fr\u00fcch\u00b7te", "ver\u00b7schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Mit gl\u00fchender Nase schleppt der Lakai", "tokens": ["Mit", "gl\u00fc\u00b7hen\u00b7der", "Na\u00b7se", "schleppt", "der", "La\u00b7kai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die schwere Tanne von hinnen,", "tokens": ["Die", "schwe\u00b7re", "Tan\u00b7ne", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Z\u00f6fchen tr\u00e4gt ein Leiterchen nach,", "tokens": ["Das", "Z\u00f6f\u00b7chen", "tr\u00e4gt", "ein", "Lei\u00b7ter\u00b7chen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu ersteigen die gr\u00fcnen Zinnen.", "tokens": ["Zu", "er\u00b7stei\u00b7gen", "die", "gr\u00fc\u00b7nen", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Und kommt die Nacht, so singt der Wald", "tokens": ["Und", "kommt", "die", "Nacht", ",", "so", "singt", "der", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wiegt sich im Gaslichtscheine;", "tokens": ["Und", "wiegt", "sich", "im", "Gas\u00b7licht\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bang f\u00fchrt die arme Mutter ihr Kind", "tokens": ["Bang", "f\u00fchrt", "die", "ar\u00b7me", "Mut\u00b7ter", "ihr", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vor\u00fcber dem Zauberhaine.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "dem", "Zau\u00b7ber\u00b7hai\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Einst sah ich einen Weihnachtsbaum:", "tokens": ["Einst", "sah", "ich", "ei\u00b7nen", "Weih\u00b7nachts\u00b7baum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im d\u00fcstern Bergesbanne", "tokens": ["Im", "d\u00fcs\u00b7tern", "Ber\u00b7ges\u00b7ban\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stand eisbezuckert auf dem Granit", "tokens": ["Stand", "eis\u00b7be\u00b7zu\u00b7ckert", "auf", "dem", "Gra\u00b7nit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die alte Wettertanne.", "tokens": ["Die", "al\u00b7te", "Wet\u00b7ter\u00b7tan\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und zwischen den \u00c4sten waren sch\u00f6n", "tokens": ["Und", "zwi\u00b7schen", "den", "\u00c4s\u00b7ten", "wa\u00b7ren", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sterne aufgegangen,", "tokens": ["Die", "Ster\u00b7ne", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am untersten Ast sah ich entsetzt", "tokens": ["Am", "un\u00b7ters\u00b7ten", "Ast", "sah", "ich", "ent\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die alte Schmidtin hangen.", "tokens": ["Die", "al\u00b7te", "Schmid\u00b7tin", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hell schien der Mond ihr ins Gesicht,", "tokens": ["Hell", "schien", "der", "Mond", "ihr", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das festlich still verkl\u00e4ret;", "tokens": ["Das", "fest\u00b7lich", "still", "ver\u00b7kl\u00e4\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil sie auf der Welt sonst nichts besa\u00df,", "tokens": ["Weil", "sie", "auf", "der", "Welt", "sonst", "nichts", "be\u00b7sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hatte sie sich selbst bescheret.", "tokens": ["Hat\u00b7te", "sie", "sich", "selbst", "be\u00b7sche\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}