{"textgrid.poem.41338": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Papefiguier und Ver\u00e4chter der sch\u00f6nsten Stellen im Milton", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blasse Chaerilus wird oft, aus Eifer, roth,", "tokens": ["Der", "blas\u00b7se", "Chae\u00b7ri\u00b7lus", "wird", "oft", ",", "aus", "Ei\u00b7fer", ",", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "ADV", "$,", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ich das erste Paar im Milton reizend finde.", "tokens": ["Wann", "ich", "das", "ers\u00b7te", "Paar", "im", "Mil\u00b7ton", "rei\u00b7zend", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er bleibe, was er ist: so d\u00fcrr als Miltons Tod.", "tokens": ["Er", "blei\u00b7be", ",", "was", "er", "ist", ":", "so", "d\u00fcrr", "als", "Mil\u00b7tons", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$.", "ADV", "ADJD", "KOKOM", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bosheitvoll, wie Miltons S\u00fcnde!", "tokens": ["Und", "bos\u00b7heit\u00b7voll", ",", "wie", "Mil\u00b7tons", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}