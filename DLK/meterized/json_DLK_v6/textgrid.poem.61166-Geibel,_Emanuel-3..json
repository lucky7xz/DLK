{"textgrid.poem.61166": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Schmied, Herr Schmied, beschlagt mir mein R\u00f6\u00dflein,", "tokens": ["Herr", "Schmied", ",", "Herr", "Schmied", ",", "be\u00b7schlagt", "mir", "mein", "R\u00f6\u00df\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und habt ihr's beschlagen, so macht mir ein Schl\u00f6\u00dflein,", "tokens": ["Und", "habt", "ih\u00b7r's", "be\u00b7schla\u00b7gen", ",", "so", "macht", "mir", "ein", "Schl\u00f6\u00df\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-++-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Schl\u00f6\u00dflein so fest und ein Schl\u00f6\u00dflein so fein,", "tokens": ["Ein", "Schl\u00f6\u00df\u00b7lein", "so", "fest", "und", "ein", "Schl\u00f6\u00df\u00b7lein", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und mu\u00df bei dem Schl\u00f6\u00dflein ein Schl\u00fcssel auch sein.", "tokens": ["Und", "mu\u00df", "bei", "dem", "Schl\u00f6\u00df\u00b7lein", "ein", "Schl\u00fcs\u00b7sel", "auch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Das Schl\u00f6\u00dflein, das will ich vors Herze mir legen,", "tokens": ["Das", "Schl\u00f6\u00df\u00b7lein", ",", "das", "will", "ich", "vors", "Her\u00b7ze", "mir", "le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und hab' ich's verschlossen mit Kreuz und mit Segen,", "tokens": ["Und", "hab'", "ich's", "ver\u00b7schlos\u00b7sen", "mit", "Kreuz", "und", "mit", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "So werf' in den See ich den Schl\u00fcssel hinein,", "tokens": ["So", "wer\u00b7f'", "in", "den", "See", "ich", "den", "Schl\u00fcs\u00b7sel", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Darf nimmer ein Wort mehr heraus noch herein.", "tokens": ["Darf", "nim\u00b7mer", "ein", "Wort", "mehr", "he\u00b7raus", "noch", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn wer eine selige Liebe will tragen,", "tokens": ["Denn", "wer", "ei\u00b7ne", "se\u00b7li\u00b7ge", "Lie\u00b7be", "will", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der darf es den alten Jungfern nicht sagen;", "tokens": ["Der", "darf", "es", "den", "al\u00b7ten", "Jung\u00b7fern", "nicht", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Dornen, die Disteln, die stechen gar sehr,", "tokens": ["Die", "Dor\u00b7nen", ",", "die", "Dis\u00b7teln", ",", "die", "ste\u00b7chen", "gar", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch stechen die Altjungfernzungen noch mehr.", "tokens": ["Doch", "ste\u00b7chen", "die", "Alt\u00b7jung\u00b7fern\u00b7zun\u00b7gen", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Sie tragen's zur Bas' hin und zur Frau Gevattern,", "tokens": ["Sie", "tra\u00b7gen's", "zur", "Bas'", "hin", "und", "zur", "Frau", "Ge\u00b7vat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "APPRART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bis da\u00df es die G\u00e4ns' auf dem Markte beschnattern,", "tokens": ["Bis", "da\u00df", "es", "die", "G\u00e4ns'", "auf", "dem", "Mark\u00b7te", "be\u00b7schnat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Bis da\u00df es der Entrich beredt auf dem See", "tokens": ["Bis", "da\u00df", "es", "der", "Ent\u00b7rich", "be\u00b7redt", "auf", "dem", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und der Kuckuck im Walde, und das tut noch weh.", "tokens": ["Und", "der", "Ku\u00b7ckuck", "im", "Wal\u00b7de", ",", "und", "das", "tut", "noch", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,", "KON", "PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und w\u00e4r' ich der Herrgott, so lie\u00df' ich auf Erden", "tokens": ["Und", "w\u00e4r'", "ich", "der", "Herr\u00b7gott", ",", "so", "lie\u00df'", "ich", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Zu Dornen und Disteln die Klatschzungen werden,", "tokens": ["Zu", "Dor\u00b7nen", "und", "Dis\u00b7teln", "die", "Klatschzun\u00b7gen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da fr\u00e4\u00df' sie der Esel, und h\u00e4tt's keine Not,", "tokens": ["Da", "fr\u00e4\u00df'", "sie", "der", "E\u00b7sel", ",", "und", "h\u00e4tt's", "kei\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und weinte mein Schatz sich die Augen nicht rot.", "tokens": ["Und", "wein\u00b7te", "mein", "Schatz", "sich", "die", "Au\u00b7gen", "nicht", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PRF", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}