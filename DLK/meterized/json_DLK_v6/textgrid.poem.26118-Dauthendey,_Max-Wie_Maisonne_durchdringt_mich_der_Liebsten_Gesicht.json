{"textgrid.poem.26118": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Wie Maisonne durchdringt mich der Liebsten Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Pappel am Berg steht im Nachmittagslicht,", "tokens": ["Ei\u00b7ne", "Pap\u00b7pel", "am", "Berg", "steht", "im", "Nach\u00b7mit\u00b7tags\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Maischein durch jedes Bl\u00e4ttlein geht,", "tokens": ["Mai\u00b7schein", "durch", "je\u00b7des", "Bl\u00e4tt\u00b7lein", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kein Blatt der Sonne widersteht.", "tokens": ["Kein", "Blatt", "der", "Son\u00b7ne", "wi\u00b7der\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Maisonne allein hat jetzt \u00fcberall Raum.", "tokens": ["Mai\u00b7son\u00b7ne", "al\u00b7lein", "hat", "jetzt", "\u00fc\u00b7be\u00b7rall", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Der Flu\u00df samt H\u00e4user und der Stadt", "tokens": ["Der", "Flu\u00df", "samt", "H\u00e4u\u00b7ser", "und", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Sonne jetzt unterzugehen hat.", "tokens": ["In", "Son\u00b7ne", "jetzt", "un\u00b7ter\u00b7zu\u00b7ge\u00b7hen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Vor Sonne seh' ich die Ferne nicht,", "tokens": ["Vor", "Son\u00b7ne", "seh'", "ich", "die", "Fer\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor Sonne seh' ich die N\u00e4he kaum,", "tokens": ["Vor", "Son\u00b7ne", "seh'", "ich", "die", "N\u00e4\u00b7he", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Maisonne durchdringt mich der Liebsten Gesicht.", "tokens": ["Wie", "Mai\u00b7son\u00b7ne", "durch\u00b7dringt", "mich", "der", "Liebs\u00b7ten", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}}}}}