{"dta.poem.2850": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Statt vieler gebe Gott dir Einen Freund, getreuen,", "tokens": ["Statt", "vie\u00b7ler", "ge\u00b7be", "Gott", "dir", "Ei\u00b7nen", "Freund", ",", "ge\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "PPER", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jeder Lage dich, und sich mit dir, zu freuen;", "tokens": ["In", "je\u00b7der", "La\u00b7ge", "dich", ",", "und", "sich", "mit", "dir", ",", "zu", "freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "$,", "KON", "PRF", "APPR", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der dein Gef\u00e4hrte sei zu Fahrt und zu Gefahr,", "tokens": ["Der", "dein", "Ge\u00b7f\u00e4hr\u00b7te", "sei", "zu", "Fahrt", "und", "zu", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dein Geselle, wo du siedelst, immerdar;", "tokens": ["Und", "dein", "Ge\u00b7sel\u00b7le", ",", "wo", "du", "sie\u00b7delst", ",", "im\u00b7mer\u00b7dar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann aber dein Geno\u00df in jeglichem Genu\u00df,", "tokens": ["Dann", "a\u00b7ber", "dein", "Ge\u00b7no\u00df", "in", "jeg\u00b7li\u00b7chem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und niemals sei der Tro\u00df der Welt dir zum Verdru\u00df.", "tokens": ["Und", "nie\u00b7mals", "sei", "der", "Tro\u00df", "der", "Welt", "dir", "zum", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}