{"textgrid.poem.38117": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Traure nicht, traure nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritt ein Herr und auch sein Knecht,", "tokens": ["Es", "ritt", "ein", "Herr", "und", "auch", "sein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ritten miteinander einen Winter weiten Weg.", "tokens": ["Sie", "rit\u00b7ten", "mi\u00b7tein\u00b7an\u00b7der", "ei\u00b7nen", "Win\u00b7ter", "wei\u00b7ten", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Sie kamen an einen Feigenbaum,", "tokens": ["Sie", "ka\u00b7men", "an", "ei\u00b7nen", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lieb Knecht steig, schau dich ume auf dem d\u00fcrren Feigenbaum.", "tokens": ["Lieb", "Knecht", "steig", ",", "schau", "dich", "u\u00b7me", "auf", "dem", "d\u00fcr\u00b7ren", "Fei\u00b7gen\u00b7baum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Es ist, lieb Herr, es ist zu viel,", "tokens": ["Es", "ist", ",", "lieb", "Herr", ",", "es", "ist", "zu", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "NN", "$,", "PPER", "VAFIN", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kraft ist mir entschwunden, die Aestlein sind auch d\u00fcrr.", "tokens": ["Mein", "Kraft", "ist", "mir", "ent\u00b7schwun\u00b7den", ",", "die", "A\u00b7est\u00b7lein", "sind", "auch", "d\u00fcrr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Lieb Knecht so halt mein Ro\u00df am Zaum,", "tokens": ["Lieb", "Knecht", "so", "halt", "mein", "Ro\u00df", "am", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will wohl selber steigen auf den d\u00fcrren Feigenbaum.", "tokens": ["Ich", "will", "wohl", "sel\u00b7ber", "stei\u00b7gen", "auf", "den", "d\u00fcr\u00b7ren", "Fei\u00b7gen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Und da er auf den Baum nauf trat,", "tokens": ["Und", "da", "er", "auf", "den", "Baum", "nauf", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aestlein waren d\u00fcrre, er fiel ins gr\u00fcne Gras.", "tokens": ["Die", "A\u00b7est\u00b7lein", "wa\u00b7ren", "d\u00fcr\u00b7re", ",", "er", "fiel", "ins", "gr\u00fc\u00b7ne", "Gras", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Lieb Herr, nun liegst du halber tod", "tokens": ["Lieb", "Herr", ",", "nun", "liegst", "du", "hal\u00b7ber", "tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo soll ich mir nun ausnehmen, mein schwer verdienten Lohn?", "tokens": ["Wo", "soll", "ich", "mir", "nun", "aus\u00b7neh\u00b7men", ",", "mein", "schwer", "ver\u00b7dien\u00b7ten", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Lieb Knecht, f\u00fcr deinen Lohn und Werth,", "tokens": ["Lieb", "Knecht", ",", "f\u00fcr", "dei\u00b7nen", "Lohn", "und", "Werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr sollst du wohl nehmen mein Rappelbraunes Pferd.", "tokens": ["Da\u00b7f\u00fcr", "sollst", "du", "wohl", "neh\u00b7men", "mein", "Rap\u00b7pel\u00b7brau\u00b7nes", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Dein Rappelbraun Pferd, das mag ich nit,", "tokens": ["Dein", "Rap\u00b7pel\u00b7braun", "Pferd", ",", "das", "mag", "ich", "nit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich wei\u00df mir noch was Anders, das mir lieber lieber ist.", "tokens": ["Ich", "wei\u00df", "mir", "noch", "was", "An\u00b7ders", ",", "das", "mir", "lie\u00b7ber", "lie\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PWS", "ADV", "$,", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Lieb Knecht, f\u00fcr deinen Lohn und Werth,", "tokens": ["Lieb", "Knecht", ",", "f\u00fcr", "dei\u00b7nen", "Lohn", "und", "Werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr sollst du wohl nehmen mein Silberreiches Schwerdt.", "tokens": ["Da\u00b7f\u00fcr", "sollst", "du", "wohl", "neh\u00b7men", "mein", "Sil\u00b7ber\u00b7rei\u00b7ches", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.10": {"line.1": {"text": "Dein Silberreiches Schwerdt das mag ich nit,", "tokens": ["Dein", "Sil\u00b7ber\u00b7rei\u00b7ches", "Schwerdt", "das", "mag", "ich", "nit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PDS", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df mir noch was Anders, das mir lieber lieber ist.", "tokens": ["Ich", "wei\u00df", "mir", "noch", "was", "An\u00b7ders", ",", "das", "mir", "lie\u00b7ber", "lie\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PWS", "ADV", "$,", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "Lieb Knecht, so nimm mein wundersch\u00f6nes Weib,", "tokens": ["Lieb", "Knecht", ",", "so", "nimm", "mein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dazu den jungen Markgraf, der in der Wickelwiege leit.", "tokens": ["Da\u00b7zu", "den", "jun\u00b7gen", "Mark\u00b7graf", ",", "der", "in", "der", "Wi\u00b7ckel\u00b7wie\u00b7ge", "leit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.12": {"line.1": {"text": "Lieb Herr, jetz reit ich, schau um ein Grab,", "tokens": ["Lieb", "Herr", ",", "jetz", "reit", "ich", ",", "schau", "um", "ein", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df man euch mit den Sch\u00fclern zur Kirche eintrag.", "tokens": ["Da\u00df", "man", "euch", "mit", "den", "Sch\u00fc\u00b7lern", "zur", "Kir\u00b7che", "ein\u00b7trag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.13": {"line.1": {"text": "Und da sie an die Kirche kamen,", "tokens": ["Und", "da", "sie", "an", "die", "Kir\u00b7che", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da fiengen alle Gl\u00f6ckelein zu l\u00e4uten l\u00e4uten an.", "tokens": ["Da", "fi\u00b7en\u00b7gen", "al\u00b7le", "Gl\u00f6\u00b7ckel\u00b7ein", "zu", "l\u00e4u\u00b7ten", "l\u00e4u\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.14": {"line.1": {"text": "Sie l\u00e4uten so h\u00fcbsch, sie l\u00e4uten so fein,", "tokens": ["Sie", "l\u00e4u\u00b7ten", "so", "h\u00fcbsch", ",", "sie", "l\u00e4u\u00b7ten", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie l\u00e4uten dem Markgrafen ins Himmels Reich hinein", "tokens": ["Sie", "l\u00e4u\u00b7ten", "dem", "Mark\u00b7gra\u00b7fen", "ins", "Him\u00b7mels", "Reich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "NN", "APZR"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Ins Paradeis, ins Himmelreich,", "tokens": ["Ins", "Pa\u00b7ra\u00b7deis", ",", "ins", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sitzen die Markgrafen den Engelein zugleich.", "tokens": ["Da", "sit\u00b7zen", "die", "Mark\u00b7gra\u00b7fen", "den", "En\u00b7ge\u00b7lein", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}