{"textgrid.poem.53644": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Lied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer an der Wand lang. Immer an der Wand lang!", "tokens": ["Im\u00b7mer", "an", "der", "Wand", "lang", ".", "Im\u00b7mer", "an", "der", "Wand", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$.", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Einer taumelt. Einer baumelt.", "tokens": ["Ei\u00b7ner", "tau\u00b7melt", ".", "Ei\u00b7ner", "bau\u00b7melt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schieber lacht sich krank.", "tokens": ["Und", "der", "Schie\u00b7ber", "lacht", "sich", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Schon revoltieren", "tokens": ["Schon", "re\u00b7vol\u00b7tie\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die ersten Lebensmittel der Entente", "tokens": ["Die", "ers\u00b7ten", "Le\u00b7bens\u00b7mit\u00b7tel", "der", "En\u00b7ten\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Magen des Kapitalisten!", "tokens": ["Im", "Ma\u00b7gen", "des", "Ka\u00b7pi\u00b7ta\u00b7lis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Berlin, Dem T\u00e4nzer ist der Tod \u2013", "tokens": ["Ber\u00b7lin", ",", "Dem", "T\u00e4n\u00b7zer", "ist", "der", "Tod", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Foxtrott und Jazz \u2013", "tokens": ["Fox\u00b7trott", "und", "Jazz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Republik am\u00fcsiert sich k\u00f6niglich \u2013", "tokens": ["Die", "Re\u00b7pub\u00b7lik", "a\u00b7m\u00fc\u00b7siert", "sich", "k\u00f6\u00b7nig\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Vergi\u00df mein nicht zum ersten Mai", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "zum", "ers\u00b7ten", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als alle Knospen sprangen!", "tokens": ["Als", "al\u00b7le", "Knos\u00b7pen", "spran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr Doktor, da is was von Noske drin,", "tokens": ["Herr", "Dok\u00b7tor", ",", "da", "is", "was", "von", "Nos\u00b7ke", "drin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KOUS", "FM", "PWS", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und was vom Lieben.", "tokens": ["Und", "was", "vom", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Teure Heimat, Jott befohl'n.", "tokens": ["Teu\u00b7re", "Hei\u00b7mat", ",", "Jott", "be\u00b7fohl'", "n."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "$,", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch bei dir is nischt zu holen.", "tokens": ["Doch", "bei", "dir", "is", "nischt", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "FM", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du bist", "tokens": ["Denn", "du", "bist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ausjemist", "tokens": ["Aus\u00b7je\u00b7mist"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Bis uffs Hemd!", "tokens": ["Bis", "uffs", "Hemd", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und 'ck find' mir wo 'ne Zille.", "tokens": ["Und", "'", "ck", "find", "'", "mir", "wo", "'ne", "Zil\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "VVFIN", "$(", "PPER", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jondle los.", "tokens": ["Jond\u00b7le", "los", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Pacht mir dr\u00fcben 'ne Destille.", "tokens": ["Pacht", "mir", "dr\u00fc\u00b7ben", "'ne", "Des\u00b7til\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In Los Angelos.", "tokens": ["In", "Los", "An\u00b7ge\u00b7los", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Und:", "tokens": ["Und", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Da drinnen, i Gott bewahre,", "tokens": ["Da", "drin\u00b7nen", ",", "i", "Gott", "be\u00b7wah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NE", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Da gibts keine Gitarre!", "tokens": ["Da", "gibts", "kei\u00b7ne", "Gi\u00b7tar\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Da gehts schwapp, schwapp,", "tokens": ["Da", "gehts", "schwapp", ",", "schwapp", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ach lecke, lecke ab!", "tokens": ["Ach", "le\u00b7cke", ",", "le\u00b7cke", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "As de Levone", "tokens": ["As", "de", "Le\u00b7vo\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "De treifne Melone", "tokens": ["De", "treif\u00b7ne", "Me\u00b7lo\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schaint in de jiddische Schweiz!", "tokens": ["Schaint", "in", "de", "jid\u00b7di\u00b7sche", "Schweiz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}