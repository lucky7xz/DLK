{"textgrid.poem.42603": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Katharina", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Muse, die von Recht und Freiheit singet,", "tokens": ["Die", "Mu\u00b7se", ",", "die", "von", "Recht", "und", "Frei\u00b7heit", "sin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wandelt einsam, ferne den Pal\u00e4sten;", "tokens": ["Sie", "wan\u00b7delt", "ein\u00b7sam", ",", "fer\u00b7ne", "den", "Pa\u00b7l\u00e4s\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Lustgesang und Reigen dort erklinget,", "tokens": ["Wenn", "Lust\u00b7ge\u00b7sang", "und", "Rei\u00b7gen", "dort", "er\u00b7klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie hat nicht Anteil an des Hofes Festen:", "tokens": ["Sie", "hat", "nicht", "An\u00b7teil", "an", "des", "Ho\u00b7fes", "Fes\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch nun der laute Schmerz die Fl\u00fcgel schwinget,", "tokens": ["Doch", "nun", "der", "lau\u00b7te", "Schmerz", "die", "Fl\u00fc\u00b7gel", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da kommt auch sie mit andern Trauerg\u00e4sten,", "tokens": ["Da", "kommt", "auch", "sie", "mit", "an\u00b7dern", "Trau\u00b7er\u00b7g\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hat sie nicht die Lebenden erhoben,", "tokens": ["Und", "hat", "sie", "nicht", "die", "Le\u00b7ben\u00b7den", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Toten, die nicht h\u00f6ren, darf sie loben.", "tokens": ["Die", "To\u00b7ten", ",", "die", "nicht", "h\u00f6\u00b7ren", ",", "darf", "sie", "lo\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Stadt erdr\u00f6hnt vom Schall der Totenglocken,", "tokens": ["Die", "Stadt", "er\u00b7dr\u00f6hnt", "vom", "Schall", "der", "To\u00b7ten\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Menge br\u00fcstet sich im schwarzen Kleide,", "tokens": ["Die", "Men\u00b7ge", "br\u00fcs\u00b7tet", "sich", "im", "schwar\u00b7zen", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Antlitz l\u00e4chelt, und kein Aug ist trocken,", "tokens": ["Kein", "Ant\u00b7litz", "l\u00e4\u00b7chelt", ",", "und", "kein", "Aug", "ist", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wettkampf ist im ungeme\u00dfnen Leide:", "tokens": ["Ein", "Wett\u00b7kampf", "ist", "im", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Lei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch all dies kann die Muse nicht verlocken,", "tokens": ["Doch", "all", "dies", "kann", "die", "Mu\u00b7se", "nicht", "ver\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PDS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie das Falsche nicht vom Echten scheide;", "tokens": ["Da\u00df", "sie", "das", "Fal\u00b7sche", "nicht", "vom", "Ech\u00b7ten", "schei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Glocke t\u00f6net, wenn man sie geschwungen,", "tokens": ["Die", "Glo\u00b7cke", "t\u00f6\u00b7net", ",", "wenn", "man", "sie", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Tr\u00e4nen gibt es, die nicht tief entsprungen.", "tokens": ["Und", "Tr\u00e4\u00b7nen", "gibt", "es", ",", "die", "nicht", "tief", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PRELS", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der reiche Sarg, von K\u00fcnstlerhand gezimmert,", "tokens": ["Der", "rei\u00b7che", "Sarg", ",", "von", "K\u00fcnst\u00b7ler\u00b7hand", "ge\u00b7zim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit einer F\u00fcrstin purpurnem Gewande,", "tokens": ["Mit", "ei\u00b7ner", "F\u00fcrs\u00b7tin", "pur\u00b7pur\u00b7nem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit einer Krone, die von Steinen flimmert,", "tokens": ["Mit", "ei\u00b7ner", "Kro\u00b7ne", ",", "die", "von", "Stei\u00b7nen", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bedeutet er nicht gro\u00dfes Weh dem Lande?", "tokens": ["Be\u00b7deu\u00b7tet", "er", "nicht", "gro\u00b7\u00dfes", "Weh", "dem", "Lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, wie der Purpur, wie die Krone schimmert,", "tokens": ["Doch", ",", "wie", "der", "Pur\u00b7pur", ",", "wie", "die", "Kro\u00b7ne", "schim\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Muse huldigt nimmermehr dem Tande;", "tokens": ["Die", "Mu\u00b7se", "hul\u00b7digt", "nim\u00b7mer\u00b7mehr", "dem", "Tan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der ird'sche Glanz, kann er die Augen blenden,", "tokens": ["Der", "ird'\u00b7sche", "Glanz", ",", "kann", "er", "die", "Au\u00b7gen", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die sich zum Licht der ew'gen Sterne wenden?", "tokens": ["Die", "sich", "zum", "Licht", "der", "ew'\u00b7gen", "Ster\u00b7ne", "wen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie blickt zum Himmel, blickt zur Erde wieder,", "tokens": ["Sie", "blickt", "zum", "Him\u00b7mel", ",", "blickt", "zur", "Er\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie schaut in alle Zeiten der Geschichte:", "tokens": ["Sie", "schaut", "in", "al\u00b7le", "Zei\u00b7ten", "der", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da steigen K\u00f6niginnen auf und nieder,", "tokens": ["Da", "stei\u00b7gen", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und viele schwinden hin wie Traumgesichte", "tokens": ["Und", "vie\u00b7le", "schwin\u00b7den", "hin", "wie", "Traum\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sind verschollen in dem Mund der Lieder", "tokens": ["Und", "sind", "ver\u00b7schol\u00b7len", "in", "dem", "Mund", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sind erloschen in des Ruhmes Lichte,", "tokens": ["Und", "sind", "er\u00b7lo\u00b7schen", "in", "des", "Ruh\u00b7mes", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Indes in frischem, unverbl\u00fchtem Leben", "tokens": ["In\u00b7des", "in", "fri\u00b7schem", ",", "un\u00b7ver\u00b7bl\u00fch\u00b7tem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Namen edler B\u00fcrgerinnen schweben.", "tokens": ["Die", "Na\u00b7men", "ed\u00b7ler", "B\u00fcr\u00b7ge\u00b7rin\u00b7nen", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Drum darf die Muse wohl, die ernste, fragen:", "tokens": ["Drum", "darf", "die", "Mu\u00b7se", "wohl", ",", "die", "erns\u00b7te", ",", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbhat dieser goldne Schmuck ein Haupt umfangen,", "tokens": ["\u00bb", "hat", "die\u00b7ser", "gold\u00b7ne", "Schmuck", "ein", "Haupt", "um\u00b7fan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das w\u00fcrdig und erleuchtet ihn getragen?", "tokens": ["Das", "w\u00fcr\u00b7dig", "und", "er\u00b7leuch\u00b7tet", "ihn", "ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat unter dieses Purpurmantels Prangen", "tokens": ["Hat", "un\u00b7ter", "die\u00b7ses", "Pur\u00b7pur\u00b7man\u00b7tels", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein hohes, k\u00f6nigliches Herz geschlagen?", "tokens": ["Ein", "ho\u00b7hes", ",", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Herz", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Herz, erf\u00fcllt von heiligem Verlangen,", "tokens": ["Ein", "Herz", ",", "er\u00b7f\u00fcllt", "von", "hei\u00b7li\u00b7gem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von reger Kraft, in weitesten Bezirken", "tokens": ["Von", "re\u00b7ger", "Kraft", ",", "in", "wei\u00b7tes\u00b7ten", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Belebend, h\u00fclfreich, menschlich gro\u00df zu wirken?\u00ab", "tokens": ["Be\u00b7le\u00b7bend", ",", "h\u00fclf\u00b7reich", ",", "menschlich", "gro\u00df", "zu", "wir\u00b7ken", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "So fr\u00e4gt die Muse, doch im innern Geiste", "tokens": ["So", "fr\u00e4gt", "die", "Mu\u00b7se", ",", "doch", "im", "in\u00b7nern", "Geis\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward ihr voraus der rechten Antwort Kunde,", "tokens": ["Ward", "ihr", "vo\u00b7raus", "der", "rech\u00b7ten", "Ant\u00b7wort", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da spricht sie manches Schmerzliche, das meiste", "tokens": ["Da", "spricht", "sie", "man\u00b7ches", "Schmerz\u00b7li\u00b7che", ",", "das", "meis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschlie\u00dft sie bitter in des Busens Grunde;", "tokens": ["Ver\u00b7schlie\u00dft", "sie", "bit\u00b7ter", "in", "des", "Bu\u00b7sens", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und da\u00df auch sie ihr Totenopfer leiste,", "tokens": ["Und", "da\u00df", "auch", "sie", "ihr", "To\u00b7ten\u00b7op\u00b7fer", "leis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr Zeichen stifte dieser Trauerstunde,", "tokens": ["Ihr", "Zei\u00b7chen", "stif\u00b7te", "die\u00b7ser", "Trau\u00b7er\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Legt sie zur Krone hin, der goldesschweren,", "tokens": ["Legt", "sie", "zu