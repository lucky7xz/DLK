{"textgrid.poem.65856": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "130", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Sonn' ist nichts in meines Liebchens Blicken:", "tokens": ["Von", "Sonn'", "ist", "nichts", "in", "mei\u00b7nes", "Lieb\u00b7chens", "Bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Schnee wei\u00df, ist ihr Busen graulich gar:", "tokens": ["Wenn", "Schnee", "wei\u00df", ",", "ist", "ihr", "Bu\u00b7sen", "grau\u00b7lich", "gar", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weit r\u00f6ter gl\u00fcht Rubin als ihre Lippen:", "tokens": ["Weit", "r\u00f6\u00b7ter", "gl\u00fcht", "Ru\u00b7bin", "als", "ih\u00b7re", "Lip\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "NE", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Haare Draht sind, hat sie drahtnes Haar.", "tokens": ["Wenn", "Haa\u00b7re", "Draht", "sind", ",", "hat", "sie", "draht\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "$,", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Damaskusrosen wei\u00df und rot erblickt' ich;", "tokens": ["Da\u00b7mas\u00b7kus\u00b7ro\u00b7sen", "wei\u00df", "und", "rot", "er\u00b7blickt'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch nicht auf Liebchens Wangen solchen Flor:", "tokens": ["Doch", "nicht", "auf", "Lieb\u00b7chens", "Wan\u00b7gen", "sol\u00b7chen", "Flor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mancher Wohlgeruch ist mehr erquicklich,", "tokens": ["Und", "man\u00b7cher", "Wohl\u00b7ge\u00b7ruch", "ist", "mehr", "er\u00b7quick\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als der aus ihrem Munde geht hervor.", "tokens": ["Als", "der", "aus", "ih\u00b7rem", "Mun\u00b7de", "geht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gern h\u00f6r' ich, wenn sie spricht; doch zu gestehen", "tokens": ["Gern", "h\u00f6r'", "ich", ",", "wenn", "sie", "spricht", ";", "doch", "zu", "ge\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$.", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Bleibt, da\u00df Musik mir weit ein s\u00fc\u00df'rer Gru\u00df.", "tokens": ["Bleibt", ",", "da\u00df", "Mu\u00b7sik", "mir", "weit", "ein", "s\u00fc\u00df'\u00b7rer", "Gru\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Zwar keine G\u00f6ttin hab' ich schreiten sehen:", "tokens": ["Zwar", "kei\u00b7ne", "G\u00f6t\u00b7tin", "hab'", "ich", "schrei\u00b7ten", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mein Liebchen, wenn es wandelt, geht zu Fu\u00df.", "tokens": ["Mein", "Lieb\u00b7chen", ",", "wenn", "es", "wan\u00b7delt", ",", "geht", "zu", "Fu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und doch, gewi\u00df, so hoch begl\u00fcckt sie mich", "tokens": ["Und", "doch", ",", "ge\u00b7wi\u00df", ",", "so", "hoch", "be\u00b7gl\u00fcckt", "sie", "mich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "ADV", "ADJD", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Als irgendeine, die man schlecht verglich.", "tokens": ["Als", "ir\u00b7gend\u00b7ei\u00b7ne", ",", "die", "man", "schlecht", "ver\u00b7glich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "PIS", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}