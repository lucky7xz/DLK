{"textgrid.poem.46003": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Im Wetter auf der Heimfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du lieber wilder Regen", "tokens": ["O", "du", "lie\u00b7ber", "wil\u00b7der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du lieber Sturm der Nacht", "tokens": ["O", "du", "lie\u00b7ber", "Sturm", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da der Finsternis entgegen", "tokens": ["Da", "der", "Fins\u00b7ter\u00b7nis", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich mein Licht nach Haus gebracht.", "tokens": ["Ich", "mein", "Licht", "nach", "Haus", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sturm du warst ein Bild des Lebens", "tokens": ["Sturm", "du", "warst", "ein", "Bild", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Licht du warst der Liebe Bild", "tokens": ["Licht", "du", "warst", "der", "Lie\u00b7be", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das im Drang des Widerstrebens", "tokens": ["Das", "im", "Drang", "des", "Wi\u00b7der\u00b7stre\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchtet unter Jesu Schild.", "tokens": ["Leuch\u00b7tet", "un\u00b7ter", "Je\u00b7su", "Schild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ich bebe, zieht so brausend", "tokens": ["Doch", "ich", "be\u00b7be", ",", "zieht", "so", "brau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sp\u00e4t der Sturm mir noch durchs Haar", "tokens": ["Sp\u00e4t", "der", "Sturm", "mir", "noch", "durchs", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt das welke Laub mir sausend", "tokens": ["Treibt", "das", "wel\u00b7ke", "Laub", "mir", "sau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach im Kreis um den Altar.", "tokens": ["Nach", "im", "Kreis", "um", "den", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine Lampe flackert, lecket,", "tokens": ["Mei\u00b7ne", "Lam\u00b7pe", "fla\u00b7ckert", ",", "le\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ru\u00dft die blanke Leuchte an.", "tokens": ["Ru\u00dft", "die", "blan\u00b7ke", "Leuch\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zuckend hin und her geschrecket", "tokens": ["Zu\u00b7ckend", "hin", "und", "her", "ge\u00b7schre\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "PTKVZ", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt ihr Schein mir irre Bahn.", "tokens": ["Zeigt", "ihr", "Schein", "mir", "ir\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleich' ich doch dem armen Schwimmer", "tokens": ["Gleich'", "ich", "doch", "dem", "ar\u00b7men", "Schwim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der zum teuren Ziele ringt", "tokens": ["Der", "zum", "teu\u00b7ren", "Zie\u00b7le", "ringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den verf\u00fchrt vom falschen Schimmer", "tokens": ["Den", "ver\u00b7f\u00fchrt", "vom", "fal\u00b7schen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVPP", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald das wilde Meer verschlingt.", "tokens": ["Bald", "das", "wil\u00b7de", "Meer", "ver\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles hab' ich sinken lassen", "tokens": ["Al\u00b7les", "hab'", "ich", "sin\u00b7ken", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinken alle Lust der Welt", "tokens": ["Sin\u00b7ken", "al\u00b7le", "Lust", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines treu ans Herz zu fassen", "tokens": ["Ei\u00b7nes", "treu", "ans", "Herz", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADJD", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mich \u00fcber Meer erh\u00e4lt.", "tokens": ["Was", "mich", "\u00fc\u00b7ber", "Meer", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eine Gott gefallne Bl\u00fcte", "tokens": ["Ei\u00b7ne", "Gott", "ge\u00b7fall\u00b7ne", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt und hebt mein brennend Herz,", "tokens": ["Tr\u00e4gt", "und", "hebt", "mein", "bren\u00b7nend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treib o Woge die vergl\u00fchte", "tokens": ["Treib", "o", "Wo\u00b7ge", "die", "ver\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "FM", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Asche endlich heimatw\u00e4rts.", "tokens": ["A\u00b7sche", "end\u00b7lich", "hei\u00b7mat\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Aber diese Bl\u00fcte k\u00fchlet", "tokens": ["A\u00b7ber", "die\u00b7se", "Bl\u00fc\u00b7te", "k\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig mir die hei\u00dfe Glut", "tokens": ["E\u00b7wig", "mir", "die", "hei\u00b7\u00dfe", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie verzehrt, die in mir w\u00fchlet", "tokens": ["Nie", "ver\u00b7zehrt", ",", "die", "in", "mir", "w\u00fch\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich der Flamme irre Wut.", "tokens": ["Mich", "der", "Flam\u00b7me", "ir\u00b7re", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O ertr\u00e4nk' mich wilder Regen", "tokens": ["O", "er\u00b7tr\u00e4nk'", "mich", "wil\u00b7der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleudre mich du Sturm der Nacht", "tokens": ["Schleud\u00b7re", "mich", "du", "Sturm", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einem scharfen Fels entgegen,", "tokens": ["Ei\u00b7nem", "schar\u00b7fen", "Fels", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein schwerer Traum erwacht.", "tokens": ["Da\u00df", "mein", "schwe\u00b7rer", "Traum", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wind und Wasser um mich zanken", "tokens": ["Wind", "und", "Was\u00b7ser", "um", "mich", "zan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Bahnen wankt das Licht,", "tokens": ["Auf", "den", "Bah\u00b7nen", "wankt", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarze Wolken der Gedanken", "tokens": ["Schwar\u00b7ze", "Wol\u00b7ken", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzen vor das Weltgericht.", "tokens": ["St\u00fcr\u00b7zen", "vor", "das", "Welt\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Soll ich fliehen soll ich bleiben", "tokens": ["Soll", "ich", "flie\u00b7hen", "soll", "ich", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O unnennbar liebes Gut!", "tokens": ["O", "un\u00b7nenn\u00b7bar", "lie\u00b7bes", "Gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolle mich zum Ziele treiben", "tokens": ["Wol\u00b7le", "mich", "zum", "Zie\u00b7le", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die ganze Hoffnung ruht.", "tokens": ["Wo", "die", "gan\u00b7ze", "Hoff\u00b7nung", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Alles, was im Sturm zu schiffen", "tokens": ["Al\u00b7les", ",", "was", "im", "Sturm", "zu", "schif\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst mein banger Arm umfa\u00dft", "tokens": ["Einst", "mein", "ban\u00b7ger", "Arm", "um\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt um mich, der selbst ergriffen", "tokens": ["Treibt", "um", "mich", ",", "der", "selbst", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PRELS", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt ohn' Steuer und ohn' Mast.", "tokens": ["Schwebt", "ohn'", "Steu\u00b7er", "und", "ohn'", "Mast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Eines ist mir nur geblieben", "tokens": ["Ei\u00b7nes", "ist", "mir", "nur", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines, das ich nie verlor", "tokens": ["Ei\u00b7nes", ",", "das", "ich", "nie", "ver\u00b7lor"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein unsterblich treues Lieben", "tokens": ["Ein", "uns\u00b7terb\u00b7lich", "treu\u00b7es", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00dft mich \u00fcberm Meer empor.", "tokens": ["Rei\u00dft", "mich", "\u00fc\u00b7berm", "Meer", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Heil dir, die des Sturmes Z\u00fcgel", "tokens": ["Heil", "dir", ",", "die", "des", "Stur\u00b7mes", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PRELS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit Kinderh\u00e4nden lenkt", "tokens": ["Wie", "mit", "Kin\u00b7der\u00b7h\u00e4n\u00b7den", "lenkt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die reinen Himmelsfl\u00fcgel", "tokens": ["Und", "die", "rei\u00b7nen", "Him\u00b7mels\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig durch die Nacht hin schwenkt.", "tokens": ["Se\u00b7lig", "durch", "die", "Nacht", "hin", "schwenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Immergr\u00fcne Dornenkrone", "tokens": ["Im\u00b7mer\u00b7gr\u00fc\u00b7ne", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die die Rosen seelw\u00e4rts flicht", "tokens": ["Die", "die", "Ro\u00b7sen", "seel\u00b7w\u00e4rts", "flicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Leib aufschreit, o schone,", "tokens": ["Da\u00df", "der", "Leib", "auf\u00b7schreit", ",", "o", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Geist in Wonne bricht.", "tokens": ["Und", "der", "Geist", "in", "Won\u00b7ne", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ja ich trag' dich dicht am Herzen,", "tokens": ["Ja", "ich", "trag'", "dich", "dicht", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du zerrei\u00dfest mir die Brust", "tokens": ["Du", "zer\u00b7rei\u00b7\u00dfest", "mir", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Nesselglut der Schmerzen", "tokens": ["Doch", "die", "Nes\u00b7sel\u00b7glut", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckt mir eine heil'ge Lust.", "tokens": ["Deckt", "mir", "ei\u00b7ne", "heil'\u00b7ge", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Selig, gehst du treu zur Seiten,", "tokens": ["Se\u00b7lig", ",", "gehst", "du", "treu", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweb' ich durch die Wetternacht", "tokens": ["Schweb'", "ich", "durch", "die", "Wet\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es doch ein s\u00fc\u00dfes 