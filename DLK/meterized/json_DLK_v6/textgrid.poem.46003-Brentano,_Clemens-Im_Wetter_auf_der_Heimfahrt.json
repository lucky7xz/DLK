ster", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "ha\u00b7ben", "Nes\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Herz hat eine Brust.", "tokens": ["Und", "mein", "Herz", "hat", "ei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "An der T\u00fcre angekommen", "tokens": ["An", "der", "T\u00fc\u00b7re", "an\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprachst du mir ein freundlich Wort", "tokens": ["Sprachst", "du", "mir", "ein", "freund\u00b7lich", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ttst mich gerne aufgenommen", "tokens": ["H\u00e4ttst", "mich", "ger\u00b7ne", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch mein Richter trieb mich fort.", "tokens": ["Doch", "mein", "Rich\u00b7ter", "trieb", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Wenn ich einstens kann verdienen", "tokens": ["Wenn", "ich", "eins\u00b7tens", "kann", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.te