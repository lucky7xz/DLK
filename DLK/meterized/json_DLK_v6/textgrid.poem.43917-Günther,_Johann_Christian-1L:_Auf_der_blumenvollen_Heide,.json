{"textgrid.poem.43917": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf der blumenvollen Heide,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der blumenvollen Heide,", "tokens": ["Auf", "der", "blu\u00b7men\u00b7vol\u00b7len", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der schattenreichen Bach", "tokens": ["An", "der", "schat\u00b7ten\u00b7rei\u00b7chen", "Bach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sann ich jezt der Augenweide", "tokens": ["Sann", "ich", "jezt", "der", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des vergangnen Traumes nach,", "tokens": ["Des", "ver\u00b7gang\u00b7nen", "Trau\u00b7mes", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der mich darum dr\u00fcckt und qu\u00e4lt,", "tokens": ["Der", "mich", "da\u00b7rum", "dr\u00fcckt", "und", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PAV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil mir nunmehr wachend fehlt,", "tokens": ["Weil", "mir", "nun\u00b7mehr", "wa\u00b7chend", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mir deine Lust vermehlt.", "tokens": ["Was", "mir", "dei\u00b7ne", "Lust", "ver\u00b7mehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O was waren das vor Glieder!", "tokens": ["O", "was", "wa\u00b7ren", "das", "vor", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "ART", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O welch sch\u00f6ner Selbstbetrug", "tokens": ["O", "welch", "sch\u00f6\u00b7ner", "Selbst\u00b7be\u00b7trug"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ri\u00df mich vor Entz\u00fcckung nieder!", "tokens": ["Ri\u00df", "mich", "vor", "Ent\u00b7z\u00fc\u00b7ckung", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O da k\u00fcst ich kaum genug,", "tokens": ["O", "da", "k\u00fcst", "ich", "kaum", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis die Morgenr\u00f6the kam", "tokens": ["Bis", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und aus Misgunst oder Scham", "tokens": ["Und", "aus", "Mis\u00b7gunst", "o\u00b7der", "Scham"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bildn\u00fc\u00df, Lust und Schlummer nahm.", "tokens": ["Bild\u00b7n\u00fc\u00df", ",", "Lust", "und", "Schlum\u00b7mer", "nahm", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unaussprechliches Erg\u00f6zen,", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7ches", "Er\u00b7g\u00f6\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich dich nicht wiedersehn?", "tokens": ["Soll", "ich", "dich", "nicht", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, nach solchen theuren Sch\u00e4zen", "tokens": ["Nein", ",", "nach", "sol\u00b7chen", "theu\u00b7ren", "Sch\u00e4\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf ich wohl nicht wachend flehn.", "tokens": ["Darf", "ich", "wohl", "nicht", "wa\u00b7chend", "flehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sezt dein Schatten meiner Ruh,", "tokens": ["Sezt", "dein", "Schat\u00b7ten", "mei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6nste Doris, schon so zu,", "tokens": ["Sch\u00f6ns\u00b7te", "Do\u00b7ris", ",", "schon", "so", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NE", "$,", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dencke, was dein Antliz thu.", "tokens": ["Den\u00b7cke", ",", "was", "dein", "Ant\u00b7liz", "thu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deiner Augen scharfes Blicken", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "schar\u00b7fes", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigt mir einen hohen Geist,", "tokens": ["Zeigt", "mir", "ei\u00b7nen", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der zum Herrschen und Entz\u00fccken", "tokens": ["Der", "zum", "Herr\u00b7schen", "und", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiche Kraft und Anmuth weist;", "tokens": ["Glei\u00b7che", "Kraft", "und", "An\u00b7muth", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses ward ich mit Gefahr", "tokens": ["Die\u00b7ses", "ward", "ich", "mit", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Freyheit nechst gewahr,", "tokens": ["Mei\u00b7ner", "Frey\u00b7heit", "nechst", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als dein Strahl die Glut gebahr.", "tokens": ["Als", "dein", "Strahl", "die", "Glut", "ge\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doris, halt es nicht vor Scherzen;", "tokens": ["Do\u00b7ris", ",", "halt", "es", "nicht", "vor", "Scher\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verachte Spott und Neid,", "tokens": ["Ich", "ver\u00b7ach\u00b7te", "Spott", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tt ich auch noch tausend Herzen,", "tokens": ["H\u00e4tt", "ich", "auch", "noch", "tau\u00b7send", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieben alle dir geweiht.", "tokens": ["Blie\u00b7ben", "al\u00b7le", "dir", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trag ich einen Tropfen Blut,", "tokens": ["Trag", "ich", "ei\u00b7nen", "Trop\u00b7fen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher dir kein Opfer thut,", "tokens": ["Wel\u00b7cher", "dir", "kein", "Op\u00b7fer", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So verzehr ihn Gift und Glut.", "tokens": ["So", "ver\u00b7zehr", "ihn", "Gift", "und", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Die Vergn\u00fcgung wahrer Liebe", "tokens": ["Die", "Ver\u00b7gn\u00fc\u00b7gung", "wah\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nicht eben so gemein,", "tokens": ["Ist", "nicht", "e\u00b7ben", "so", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Gem\u00fcther gleiche Triebe", "tokens": ["Der", "Ge\u00b7m\u00fc\u00b7ther", "glei\u00b7che", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fc\u00dfen ihre Quellen seyn;", "tokens": ["M\u00fc\u00b7\u00dfen", "ih\u00b7re", "Quel\u00b7len", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pr\u00fcfe mich und sey vergn\u00fcgt,", "tokens": ["Pr\u00fc\u00b7fe", "mich", "und", "sey", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ein Herz, so du besiegt,", "tokens": ["Da\u00df", "ein", "Herz", ",", "so", "du", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch mit Ehrfurcht vor dir liegt.", "tokens": ["Auch", "mit", "Ehr\u00b7furcht", "vor", "dir", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Von der Wiege bis zur Baare", "tokens": ["Von", "der", "Wie\u00b7ge", "bis", "zur", "Baa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist gar oft ein kurzer Schritt.", "tokens": ["Ist", "gar", "oft", "ein", "kur\u00b7zer", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doris, nimm die besten Jahre", "tokens": ["Do\u00b7ris", ",", "nimm", "die", "bes\u00b7ten", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Lust der Jugend mit,", "tokens": ["Und", "die", "Lust", "der", "Ju\u00b7gend", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh der Lippen May verbl\u00fcht", "tokens": ["Eh", "der", "Lip\u00b7pen", "May", "ver\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Zeit, so pl\u00f6zlich flieht,", "tokens": ["Und", "die", "Zeit", ",", "so", "pl\u00f6z\u00b7lich", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Farbe, Muth und Lust entzieht.", "tokens": ["Far\u00b7be", ",", "Muth", "und", "Lust", "ent\u00b7zieht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Kommt mein Ziel an Lebensschrancken,", "tokens": ["Kommt", "mein", "Ziel", "an", "Le\u00b7bens\u00b7schran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcntsch ich von der Phantasie,", "tokens": ["W\u00fcnt\u00b7sch", "ich", "von", "der", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dein Bildn\u00fc\u00df in Gedancken", "tokens": ["Da\u00df", "dein", "Bild\u00b7n\u00fc\u00df", "in", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich der Welt vergn\u00fcgt entzieh;", "tokens": ["Mich", "der", "Welt", "ver\u00b7gn\u00fcgt", "ent\u00b7zieh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses w\u00fcntsch ich und dabey,", "tokens": ["Die\u00b7ses", "w\u00fcnt\u00b7sch", "ich", "und", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PAV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Da\u00df der Spruch der Grabschrift sey:", "tokens": ["Da\u00df", "der", "Spruch", "der", "Grab\u00b7schrift", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Klug, verschwiegen und getreu.", "tokens": ["Klug", ",", "ver\u00b7schwie\u00b7gen", "und", "ge\u00b7treu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}