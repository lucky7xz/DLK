, "line.3": {"text": "Im Gewand von Seide \u2014", "tokens": ["Im", "Ge\u00b7wand", "von", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geh und lieb' und leide!", "tokens": ["Geh", "und", "lieb'", "und", "lei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Frommer Augen helle Lust", "tokens": ["From\u00b7mer", "Au\u00b7gen", "hel\u00b7le", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberstrahlt an voller Brust", "tokens": ["Ue\u00b7ber\u00b7strahlt", "an", "vol\u00b7ler", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzendes Geschmeide \u2014", "tokens": ["Blit\u00b7zen\u00b7des", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geh und lieb' und leide!", "tokens": ["Geh", "und", "lieb'", "und", "lei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Merke dir's, du blondes Haar:", "tokens": ["Mer\u00b7ke", "dir's", ",", "du", "blon\u00b7des", "Haar", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmerz und Lust Geschwisterpaar,", "tokens": ["Schmerz", "und", "Lust", "Ge\u00b7schwis\u00b7ter\