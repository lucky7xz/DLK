{"textgrid.poem.62494": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die hellen Lichter scheinen!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die hellen Lichter scheinen!", "tokens": ["Wie", "die", "hel\u00b7len", "Lich\u00b7ter", "schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Kinder sind gekommen,", "tokens": ["Und", "die", "Kin\u00b7der", "sind", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All die gro\u00dfen, all die kleinen,", "tokens": ["All", "die", "gro\u00b7\u00dfen", ",", "all", "die", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "PIAT", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben ihr Geschenk genommen.", "tokens": ["Ha\u00b7ben", "ihr", "Ge\u00b7schenk", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Spielwerk bringt es uns zum Spielen,", "tokens": ["Spiel\u00b7werk", "bringt", "es", "uns", "zum", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das geliebte Wunderkind.", "tokens": ["Das", "ge\u00b7lieb\u00b7te", "Wun\u00b7der\u00b7kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spielen m\u00f6gen wir und f\u00fchlen,", "tokens": ["Spie\u00b7len", "m\u00f6\u00b7gen", "wir", "und", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir wieder Kinder sind.", "tokens": ["Da\u00df", "wir", "wie\u00b7der", "Kin\u00b7der", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfe Fr\u00fcchte, fremde Bl\u00fcten", "tokens": ["S\u00fc\u00b7\u00dfe", "Fr\u00fcch\u00b7te", ",", "frem\u00b7de", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt es in der zarten Hand,", "tokens": ["Tr\u00e4gt", "es", "in", "der", "zar\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie Engel ziehn und h\u00fcten", "tokens": ["Wie", "sie", "En\u00b7gel", "ziehn", "und", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem sel'gen Himmelsland.", "tokens": ["In", "dem", "sel'\u00b7gen", "Him\u00b7mels\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so hat es tausend Gaben", "tokens": ["Und", "so", "hat", "es", "tau\u00b7send", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen Menschen mitgebracht,", "tokens": ["Al\u00b7len", "Men\u00b7schen", "mit\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Herzen zu erlaben", "tokens": ["Al\u00b7le", "Her\u00b7zen", "zu", "er\u00b7la\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der hochgelobten Nacht.", "tokens": ["In", "der", "hoch\u00b7ge\u00b7lob\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch Vers\u00f6hnung, ew'ges Leben,", "tokens": ["Auch", "Ver\u00b7s\u00f6h\u00b7nung", ",", "ew'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trost und Freiheit, Gnadenf\u00fcll',", "tokens": ["Trost", "und", "Frei\u00b7heit", ",", "Gna\u00b7den\u00b7f\u00fcll'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Wort, umsonst gegeben", "tokens": ["Got\u00b7tes", "Wort", ",", "um\u00b7sonst", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem, welcher h\u00f6ren will.", "tokens": ["Je\u00b7dem", ",", "wel\u00b7cher", "h\u00f6\u00b7ren", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nimmer kann ich euch vergessen,", "tokens": ["Nim\u00b7mer", "kann", "ich", "euch", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All ihr sch\u00f6nen Christgeschenke!", "tokens": ["All", "ihr", "sch\u00f6\u00b7nen", "Christ\u00b7ge\u00b7schen\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Abgrund, reich und unermessen,", "tokens": ["Ab\u00b7grund", ",", "reich", "und", "un\u00b7er\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drein ich liebend mich versenke.", "tokens": ["Drein", "ich", "lie\u00b7bend", "mich", "ver\u00b7sen\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}