{"textgrid.poem.57126": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wachsende Unsicherheit", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geh' wie auf dem Meer in Dunkels Scho\u00df,", "tokens": ["Ich", "geh'", "wie", "auf", "dem", "Meer", "in", "Dun\u00b7kels", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein jeder Schritt ist schwankend wie auf Tod ...", "tokens": ["ein", "je\u00b7der", "Schritt", "ist", "schwan\u00b7kend", "wie", "auf", "Tod", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch immer wieder h\u00e4lt in h\u00f6chster Not", "tokens": ["Doch", "im\u00b7mer", "wie\u00b7der", "h\u00e4lt", "in", "h\u00f6chs\u00b7ter", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "den zagen Fu\u00df ein unsichtbares Flo\u00df.", "tokens": ["den", "za\u00b7gen", "Fu\u00df", "ein", "un\u00b7sicht\u00b7ba\u00b7res", "Flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}