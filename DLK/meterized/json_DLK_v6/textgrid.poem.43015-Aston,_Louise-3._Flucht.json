{"textgrid.poem.43015": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3. Flucht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich nicht brechen die engen Schranken?", "tokens": ["Soll", "ich", "nicht", "bre\u00b7chen", "die", "en\u00b7gen", "Schran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kurz ist der Jugend bl\u00fchende Lust!", "tokens": ["Kurz", "ist", "der", "Ju\u00b7gend", "bl\u00fc\u00b7hen\u00b7de", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "L\u00e4nger ertrag' ich den Sturm der Gedanken,", "tokens": ["L\u00e4n\u00b7ger", "er\u00b7trag'", "ich", "den", "Sturm", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Trage den Kampf nicht der sehnenden Brust!", "tokens": ["Tra\u00b7ge", "den", "Kampf", "nicht", "der", "seh\u00b7nen\u00b7den", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wild wie B\u00e4che von Felsen sch\u00e4umen,", "tokens": ["Wild", "wie", "B\u00e4\u00b7che", "von", "Fel\u00b7sen", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Jauchzend hernieder in's sch\u00f6ne Thal:", "tokens": ["Jauch\u00b7zend", "her\u00b7nie\u00b7der", "in's", "sch\u00f6\u00b7ne", "Thal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Sehnt sich mein Geist aus den engen R\u00e4umen", "tokens": ["Sehnt", "sich", "mein", "Geist", "aus", "den", "en\u00b7gen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Hin zu der Freiheit belebendem Strahl!", "tokens": ["Hin", "zu", "der", "Frei\u00b7heit", "be\u00b7le\u00b7ben\u00b7dem", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Muthig, mein Renner mit flatternden M\u00e4hnen!", "tokens": ["Mut\u00b7hig", ",", "mein", "Ren\u00b7ner", "mit", "flat\u00b7tern\u00b7den", "M\u00e4h\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Muthig hinein in die weite Welt!", "tokens": ["Mut\u00b7hig", "hin\u00b7ein", "in", "die", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Rasch, wie des Geistes gefl\u00fcgeltes Sehnen;", "tokens": ["Rasch", ",", "wie", "des", "Geis\u00b7tes", "ge\u00b7fl\u00fc\u00b7gel\u00b7tes", "Seh\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Wie das Gew\u00f6lk am Himmelszelt!", "tokens": ["Wie", "das", "Ge\u00b7w\u00f6lk", "am", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fliehen der Landschaft Bilder vor\u00fcber,", "tokens": ["Flie\u00b7hen", "der", "Land\u00b7schaft", "Bil\u00b7der", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "D\u00f6rfer und W\u00e4lder, H\u00fcgel und Thal:", "tokens": ["D\u00f6r\u00b7fer", "und", "W\u00e4l\u00b7der", ",", "H\u00fc\u00b7gel", "und", "Thal", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Tob' ich aus des Gedankens Fieber;", "tokens": ["Tob'", "ich", "aus", "des", "Ge\u00b7dan\u00b7kens", "Fie\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Flieht mich der eigenen Seele Qual!", "tokens": ["Flieht", "mich", "der", "ei\u00b7ge\u00b7nen", "See\u00b7le", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "M\u00f6gen die Funken durchs Dunkel stieben;", "tokens": ["M\u00f6\u00b7gen", "die", "Fun\u00b7ken", "durchs", "Dun\u00b7kel", "stie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Einsamer Hufschlag verhallen in Nacht!", "tokens": ["Ein\u00b7sa\u00b7mer", "Huf\u00b7schlag", "ver\u00b7hal\u00b7len", "in", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "So zerstob mein Hoffen, mein Lieben;", "tokens": ["So", "zer\u00b7stob", "mein", "Hof\u00b7fen", ",", "mein", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sinkende Sterne in wilder Pracht.", "tokens": ["Sin\u00b7ken\u00b7de", "Ster\u00b7ne", "in", "wil\u00b7der", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "G\u00e4hnende Schl\u00fcnde zur Rechten, zur Linken;", "tokens": ["G\u00e4h\u00b7nen\u00b7de", "Schl\u00fcn\u00b7de", "zur", "Rech\u00b7ten", ",", "zur", "Lin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Tanzende Lichter, dem Abgrund entsandt!", "tokens": ["Tan\u00b7zen\u00b7de", "Lich\u00b7ter", ",", "dem", "Ab\u00b7grund", "ent\u00b7sandt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "M\u00f6gen die Geister der Tiefe winken:", "tokens": ["M\u00f6\u00b7gen", "die", "Geis\u00b7ter", "der", "Tie\u00b7fe", "win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Meiner Seele sind sie verwandt!", "tokens": ["Mei\u00b7ner", "See\u00b7le", "sind", "sie", "ver\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Wettergew\u00f6lk mag die Felsen umth\u00fcrmen,", "tokens": ["Wet\u00b7ter\u00b7ge\u00b7w\u00f6lk", "mag", "die", "Fel\u00b7sen", "umt\u00b7h\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Blitze zucken von H\u00f6h' zu H\u00f6h'!", "tokens": ["Blit\u00b7ze", "zu\u00b7cken", "von", "H\u00f6h'", "zu", "H\u00f6h'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Frei ausathmet nur in St\u00fcrmen", "tokens": ["Frei", "au\u00b7sath\u00b7met", "nur", "in", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Brust ihr lastendes Weh!", "tokens": ["Mei\u00b7ne", "Brust", "ihr", "las\u00b7ten\u00b7des", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wenn der Donner m\u00e4chtig die Felsen r\u00fcttelt;", "tokens": ["Wenn", "der", "Don\u00b7ner", "m\u00e4ch\u00b7tig", "die", "Fel\u00b7sen", "r\u00fct\u00b7telt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn die Gipfel gei\u00dfelt des Sturms Gebraus:", "tokens": ["Wenn", "die", "Gip\u00b7fel", "gei\u00b7\u00dfelt", "des", "Sturms", "Ge\u00b7braus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Fl\u00fcchtet der D\u00e4mon, der mich sch\u00fcttelt,", "tokens": ["Fl\u00fcch\u00b7tet", "der", "D\u00e4\u00b7mon", ",", "der", "mich", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Zu den Br\u00fcdern in's All hinaus.", "tokens": ["Zu", "den", "Br\u00fc\u00b7dern", "in's", "All", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Muthig, mein Renner mit flatternden M\u00e4hnen!", "tokens": ["Mut\u00b7hig", ",", "mein", "Ren\u00b7ner", "mit", "flat\u00b7tern\u00b7den", "M\u00e4h\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Bote des Sturms in gefl\u00fcgeltem Lauf!", "tokens": ["Bo\u00b7te", "des", "Sturms", "in", "ge\u00b7fl\u00fc\u00b7gel\u00b7tem", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Trocknet der Himmel die Wolkenthr\u00e4nen;", "tokens": ["Trock\u00b7net", "der", "Him\u00b7mel", "die", "Wol\u00b7ken\u00b7thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Steigen die ewigen Sterne auf!", "tokens": ["Stei\u00b7gen", "die", "e\u00b7wi\u00b7gen", "Ster\u00b7ne", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Weh'! wenn die finstern Schleier schwinden,", "tokens": ["Weh'", "!", "wenn", "die", "fins\u00b7tern", "Schlei\u00b7er", "schwin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Richt' ich die Blicke himmelw\u00e4rts;", "tokens": ["Richt'", "ich", "die", "Bli\u00b7cke", "him\u00b7mel\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Winkt nur aus endlos tiefen Gr\u00fcnden", "tokens": ["Winkt", "nur", "aus", "end\u00b7los", "tie\u00b7fen", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mir mein unendlicher, tiefer Schmerz.", "tokens": ["Mir", "mein", "un\u00b7end\u00b7li\u00b7cher", ",", "tie\u00b7fer", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}