{"textgrid.poem.49590": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber k\u00fcrzlich erst ged\u00fcngte", "tokens": ["\u00dc\u00b7ber", "k\u00fcrz\u00b7lich", "erst", "ge\u00b7d\u00fcng\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiesen zieht der Blumenflor,", "tokens": ["Wie\u00b7sen", "zieht", "der", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Natur, die sich verj\u00fcngte,", "tokens": ["Und", "Na\u00b7tur", ",", "die", "sich", "ver\u00b7j\u00fcng\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt uns sch\u00f6n und lieblich vor.", "tokens": ["Kommt", "uns", "sch\u00f6n", "und", "lieb\u00b7lich", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4mmer springen, Ziegen h\u00fcpfen,", "tokens": ["L\u00e4m\u00b7mer", "sprin\u00b7gen", ",", "Zie\u00b7gen", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Tiere d\u00fcnkt es recht,", "tokens": ["Al\u00b7le", "Tie\u00b7re", "d\u00fcnkt", "es", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebedurstig anzukn\u00fcpfen", "tokens": ["Lie\u00b7be\u00b7durs\u00b7tig", "an\u00b7zu\u00b7kn\u00fcp\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem anderen Geschlecht.", "tokens": ["Mit", "dem", "an\u00b7de\u00b7ren", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spatzen, Tauben, Stare, Schwalben", "tokens": ["Spat\u00b7zen", ",", "Tau\u00b7ben", ",", "Sta\u00b7re", ",", "Schwal\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Paaren sich, und auch das Huhn", "tokens": ["Paa\u00b7ren", "sich", ",", "und", "auch", "das", "Huhn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PRF", "$,", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will im Feld und allenthalben", "tokens": ["Will", "im", "Feld", "und", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Hahn dasselbe tun.", "tokens": ["Mit", "dem", "Hahn", "das\u00b7sel\u00b7be", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stolz erf\u00fcllt den muntern Gockel,", "tokens": ["Stolz", "er\u00b7f\u00fcllt", "den", "mun\u00b7tern", "Go\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Weibchen schwimmt im Gl\u00fcck.", "tokens": ["Und", "das", "Weib\u00b7chen", "schwimmt", "im", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der arme Pfarrerzwockel", "tokens": ["Nur", "der", "ar\u00b7me", "Pfar\u00b7rer\u00b7zwo\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieht sich in sich selbst zur\u00fcck.", "tokens": ["Zieht", "sich", "in", "sich", "selbst", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebesgl\u00fcck und Liebesschmerzen", "tokens": ["Lie\u00b7bes\u00b7gl\u00fcck", "und", "Lie\u00b7bes\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind ihm fremd et cetera.", "tokens": ["Sind", "ihm", "fremd", "et", "ce\u00b7te\u00b7ra", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er kennt nur Frauenherzen,", "tokens": ["Denn", "er", "kennt", "nur", "Frau\u00b7en\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er durch ein Astloch sah.", "tokens": ["Die", "er", "durch", "ein", "Ast\u00b7loch", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}