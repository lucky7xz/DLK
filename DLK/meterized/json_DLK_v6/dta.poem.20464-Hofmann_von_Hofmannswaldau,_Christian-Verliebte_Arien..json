{"dta.poem.20464": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Niemand wei\u00df wie schwer mirs f\u00e4llt/", "tokens": ["Nie\u00b7mand", "wei\u00df", "wie", "schwer", "mirs", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ADJD", "NE", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flammen in der brust zu hegen;", "tokens": ["Flam\u00b7men", "in", "der", "brust", "zu", "he\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie dennoch f\u00fcr der welt/", "tokens": ["Und", "sie", "den\u00b7noch", "f\u00fcr", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ans freye licht zu legen.", "tokens": ["Nicht", "ans", "frey\u00b7e", "licht", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feuer l\u00e4st sich nicht verhelen;", "tokens": ["Feu\u00b7er", "l\u00e4st", "sich", "nicht", "ver\u00b7he\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn sein glantz ist allzuklar/", "tokens": ["Denn", "sein", "glantz", "ist", "all\u00b7zu\u00b7klar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die glut verliebter seelen/", "tokens": ["Und", "die", "glut", "ver\u00b7lieb\u00b7ter", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht sich selber offenbar.", "tokens": ["Macht", "sich", "sel\u00b7ber", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hundert augen die von neid", "tokens": ["Hun\u00b7dert", "au\u00b7gen", "die", "von", "neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ART", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von lauter argwohn brennen/", "tokens": ["Und", "von", "lau\u00b7ter", "arg\u00b7wohn", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind auff mich zu sehn bereit/", "tokens": ["Sind", "auff", "mich", "zu", "sehn", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "ADJD", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Ob sie was vermercken k\u00f6nnen.", "tokens": ["Ob", "sie", "was", "ver\u00b7mer\u00b7cken", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch verberg ich meine schmertzen/", "tokens": ["Noch", "ver\u00b7berg", "ich", "mei\u00b7ne", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man keine funcken sieht/", "tokens": ["Da\u00df", "man", "kei\u00b7ne", "fun\u00b7cken", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da die liebe doch im hertzen", "tokens": ["Da", "die", "lie\u00b7be", "doch", "im", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein andrer Aethna gl\u00fcht.", "tokens": ["Wie", "ein", "an\u00b7drer", "A\u00b7e\u00b7th\u00b7na", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dieses ist der liebe kunst/", "tokens": ["Die\u00b7ses", "ist", "der", "lie\u00b7be", "kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Amor suchet finsternissen/", "tokens": ["A\u00b7mor", "su\u00b7chet", "fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seiner stillen brunst/", "tokens": ["Und", "von", "sei\u00b7ner", "stil\u00b7len", "brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df der helle tag nichts wissen.", "tokens": ["Mu\u00df", "der", "hel\u00b7le", "tag", "nichts", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Venus bricht mit ihrem sterne", "tokens": ["Ve\u00b7nus", "bricht", "mit", "ih\u00b7rem", "ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erst bey dunckler nacht herein/", "tokens": ["Erst", "bey", "dunck\u00b7ler", "nacht", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die zarte jugend lerne", "tokens": ["Da\u00df", "die", "zar\u00b7te", "ju\u00b7gend", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der liebe heimlich seyn.", "tokens": ["In", "der", "lie\u00b7be", "heim\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum gewehne dich mein muth/", "tokens": ["Drum", "ge\u00b7weh\u00b7ne", "dich", "mein", "muth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine flammen zuverschweigen;", "tokens": ["Dei\u00b7ne", "flam\u00b7men", "zu\u00b7ver\u00b7schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df von der verborgnen glut", "tokens": ["La\u00df", "von", "der", "ver\u00b7borg\u00b7nen", "glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weder mund noch auge zeugen.", "tokens": ["We\u00b7der", "mund", "noch", "au\u00b7ge", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mustu dich gleich etwas zwingen/", "tokens": ["Mus\u00b7tu", "dich", "gleich", "et\u00b7was", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist gleich die verstellung schwer;", "tokens": ["Ist", "gleich", "die", "ver\u00b7stel\u00b7lung", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Aus den allerschwersten dingen", "tokens": ["Aus", "den", "al\u00b7ler\u00b7schwers\u00b7ten", "din\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kommt die gr\u00f6ste lust offt her.", "tokens": ["Kommt", "die", "gr\u00f6s\u00b7te", "lust", "offt", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Perlen liegen eingeschrenckt", "tokens": ["Per\u00b7len", "lie\u00b7gen", "ein\u00b7ge\u00b7schrenckt"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den harten muschel-h\u00e4usern.", "tokens": ["In", "den", "har\u00b7ten", "mu\u00b7schel\u00b7h\u00e4u\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer auff frische rosen denckt/", "tokens": ["Wer", "auff", "fri\u00b7sche", "ro\u00b7sen", "denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sucht sie in den dornen-reisern.", "tokens": ["Sucht", "sie", "in", "den", "dor\u00b7nen\u00b7rei\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Honig ist nicht ohne bienen.", "tokens": ["Ho\u00b7nig", "ist", "nicht", "oh\u00b7ne", "bie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer in Canaan will stehn/", "tokens": ["Wer", "in", "Ca\u00b7naan", "will", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VMFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df erst in Egypten dienen/", "tokens": ["Mu\u00df", "erst", "in", "E\u00b7gyp\u00b7ten", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NE", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und durch meer und w\u00fcsten gehn.", "tokens": ["Und", "durch", "meer", "und", "w\u00fcs\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Vielleicht wird des himmels gunst", "tokens": ["Viel\u00b7leicht", "wird", "des", "him\u00b7mels", "gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mir das gl\u00fcck noch k\u00fcnfftig g\u00f6nnen/", "tokens": ["Mir", "das", "gl\u00fcck", "noch", "k\u00fcnff\u00b7tig", "g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die kohlen meiner brunst", "tokens": ["Da\u00df", "die", "koh\u00b7len", "mei\u00b7ner", "brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Offenbarlich brennen k\u00f6nnen.", "tokens": ["Of\u00b7fen\u00b7bar\u00b7lich", "bren\u00b7nen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Itzo schreib ich meinem hertzen", "tokens": ["It\u00b7zo", "schreib", "ich", "mei\u00b7nem", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen wahren denck-spruch ein:", "tokens": ["Die\u00b7sen", "wah\u00b7ren", "den\u00b7ck\u00b7spruch", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.7": {"text": "Feuers-glut und liebes-schmertzen", "tokens": ["Feu\u00b7er\u00b7sglut", "und", "lie\u00b7bes\u00b7schmert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fcssen wohl bewahret seyn.", "tokens": ["M\u00fcs\u00b7sen", "wohl", "be\u00b7wah\u00b7ret", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}