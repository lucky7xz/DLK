{"textgrid.poem.59671": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Friedensgebet", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib Frieden, Herr, gib Frieden,", "tokens": ["Gib", "Frie\u00b7den", ",", "Herr", ",", "gib", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$,", "VVIMP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du milder Liebeshort!", "tokens": ["Du", "mil\u00b7der", "Lie\u00b7bes\u00b7hort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einst bist du abgeschieden", "tokens": ["Einst", "bist", "du", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit s\u00fc\u00dfem Freudenwort:", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Freu\u00b7den\u00b7wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gib Frieden, Herr, gib Frieden!", "tokens": ["Gib", "Frie\u00b7den", ",", "Herr", ",", "gib", "Frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$,", "VVIMP", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt will Streit und Krieg,", "tokens": ["Die", "Welt", "will", "Streit", "und", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Stille wird gemieden,", "tokens": ["Der", "Stil\u00b7le", "wird", "ge\u00b7mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wilde hat den Sieg,", "tokens": ["Der", "Wil\u00b7de", "hat", "den", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Unruh' herrscht auf Erden", "tokens": ["Und", "Un\u00b7ruh'", "herrscht", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Lug und Trug und List \u2013", "tokens": ["Und", "Lug", "und", "Trug", "und", "List", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach! La\u00df es stille werden,", "tokens": ["Ach", "!", "La\u00df", "es", "stil\u00b7le", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du stiller Jesus Christ!", "tokens": ["Du", "stil\u00b7ler", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gib Frieden, Herr, gib Frieden,", "tokens": ["Gib", "Frie\u00b7den", ",", "Herr", ",", "gib", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$,", "VVIMP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du milder Liebeshort!", "tokens": ["Du", "mil\u00b7der", "Lie\u00b7bes\u00b7hort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird es schon hienieden", "tokens": ["Dann", "wird", "es", "schon", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Paradiesesort,", "tokens": ["Ein", "Pa\u00b7ra\u00b7die\u00b7se\u00b7sort", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Sorgen fliehn und Schmerzen", "tokens": ["Und", "Sor\u00b7gen", "fliehn", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus jeder schweren Brust,", "tokens": ["Aus", "je\u00b7der", "schwe\u00b7ren", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In Freuden gl\u00fchn die Herzen,", "tokens": ["In", "Freu\u00b7den", "gl\u00fchn", "die", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Lieb' und Himmelslust.", "tokens": ["In", "Lieb'", "und", "Him\u00b7mels\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}