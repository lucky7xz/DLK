{"textgrid.poem.47857": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Garten eingestiegen", "tokens": ["In", "den", "Gar\u00b7ten", "ein\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r ich nun mit gutem Gl\u00fcck \u2013", "tokens": ["W\u00e4r", "ich", "nun", "mit", "gu\u00b7tem", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Flederm\u00e4use fliegen!", "tokens": ["Wie", "die", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Langsam weicht die Nacht zur\u00fcck.", "tokens": ["Lang\u00b7sam", "weicht", "die", "Nacht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch indes am Feldessaume", "tokens": ["Doch", "in\u00b7des", "am", "Fel\u00b7des\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcben kaum Aurora glimmt,", "tokens": ["Dr\u00fc\u00b7ben", "kaum", "Au\u00b7ro\u00b7ra", "glimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab ich unterm Lindenbaume", "tokens": ["Hab", "ich", "un\u00b7term", "Lin\u00b7den\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier die Fiedel schon gestimmt.", "tokens": ["Hier", "die", "Fie\u00b7del", "schon", "ge\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, dein Kammerfenster blinket", "tokens": ["Sieh", ",", "dein", "Kam\u00b7mer\u00b7fens\u00b7ter", "blin\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem ersten Morgenstrahl;", "tokens": ["In", "dem", "ers\u00b7ten", "Mor\u00b7gen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heller wird's, die Nacht versinket;", "tokens": ["Hel\u00b7ler", "wird's", ",", "die", "Nacht", "ver\u00b7sin\u00b7ket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horch! Da schlug die Nachtigall!", "tokens": ["Horch", "!", "Da", "schlug", "die", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlaf nicht mehr! Die Morgenl\u00fcfte", "tokens": ["Schlaf", "nicht", "mehr", "!", "Die", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fctteln schon an deiner T\u00fcr;", "tokens": ["R\u00fct\u00b7teln", "schon", "an", "dei\u00b7ner", "T\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings erwacht sind Klang und D\u00fcfte,", "tokens": ["Rings", "er\u00b7wacht", "sind", "Klang", "und", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Herz verlangt nach dir.", "tokens": ["Und", "mein", "Herz", "ver\u00b7langt", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu des Gartens Schattend\u00fcster", "tokens": ["Zu", "des", "Gar\u00b7tens", "Schat\u00b7ten\u00b7d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm herab, geliebtes Kind!", "tokens": ["Komm", "her\u00b7ab", ",", "ge\u00b7lieb\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Laub ein leis Gefl\u00fcster \u2013", "tokens": ["Nur", "im", "Laub", "ein", "leis", "Ge\u00b7fl\u00fcs\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschwiegen ist der Wind.", "tokens": ["Und", "ver\u00b7schwie\u00b7gen", "ist", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}