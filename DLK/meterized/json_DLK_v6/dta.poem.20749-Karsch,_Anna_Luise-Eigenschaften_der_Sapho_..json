{"dta.poem.20749": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Eigenschaften der Sapho .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht immer will ich so, wie andre Leute wollen,                 ", "tokens": ["Nicht", "im\u00b7mer", "will", "ich", "so", ",", "wie", "and\u00b7re", "Leu\u00b7te", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "ADV", "$,", "PWAV", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nicht Gesetze geben sollen", "tokens": ["Die", "nicht", "Ge\u00b7set\u00b7ze", "ge\u00b7ben", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sapho, der Empfindungsvollen,", "tokens": ["Der", "Sa\u00b7pho", ",", "der", "Emp\u00b7fin\u00b7dungs\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Die um den sch\u00f6nen Geist nicht tr\u00e4gt ein sch\u00f6nes Kleid,", "tokens": ["Die", "um", "den", "sch\u00f6\u00b7nen", "Geist", "nicht", "tr\u00e4gt", "ein", "sch\u00f6\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der in den Adern ist ein Dichter-Quell gequollen", "tokens": ["Der", "in", "den", "A\u00b7dern", "ist", "ein", "Dich\u00b7ter\u00b7Quell", "ge\u00b7quol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu aller Lieder M\u00f6glichkeit,", "tokens": ["Zu", "al\u00b7ler", "Lie\u00b7der", "M\u00f6g\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der hoch von Z\u00e4rtlichkeit der Busen aufgeschwollen,", "tokens": ["Der", "hoch", "von", "Z\u00e4rt\u00b7lich\u00b7keit", "der", "Bu\u00b7sen", "auf\u00b7ge\u00b7schwol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die aus den Augen oft l\u00e4\u00dft Thr\u00e4nen nieder rollen,", "tokens": ["Die", "aus", "den", "Au\u00b7gen", "oft", "l\u00e4\u00dft", "Thr\u00e4\u00b7nen", "nie\u00b7der", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "NN", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem Himmel ihren Dank zu zollen", "tokens": ["Dem", "Him\u00b7mel", "ih\u00b7ren", "Dank", "zu", "zol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr diesen goldnen Theil in ihrer Lebenszeit.", "tokens": ["F\u00fcr", "die\u00b7sen", "gold\u00b7nen", "Theil", "in", "ih\u00b7rer", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}