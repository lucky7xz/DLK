{"textgrid.poem.67688": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Wunderk\u00f6nigs Jesu Rath", "tokens": ["Des", "Wun\u00b7der\u00b7k\u00f6\u00b7nigs", "Je\u00b7su", "Rath"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sollst Du verschweigen,", "tokens": ["Sollst", "Du", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Des gro\u00dfen Gottes laute That,", "tokens": ["Des", "gro\u00b7\u00dfen", "Got\u00b7tes", "lau\u00b7te", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zeigen!", "tokens": ["Die", "zei\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Des gro\u00dfen Gottes laute That", "tokens": ["Des", "gro\u00b7\u00dfen", "Got\u00b7tes", "lau\u00b7te", "That"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann Wild und Vogel finden,", "tokens": ["Kann", "Wild", "und", "Vo\u00b7gel", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Des Wunders Jesu stillen Rath", "tokens": ["Des", "Wun\u00b7ders", "Je\u00b7su", "stil\u00b7len", "Rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lie\u00df Gnade Dir verk\u00fcnden.", "tokens": ["Lie\u00df", "Gna\u00b7de", "Dir", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Natur ist heller Zeitungsstaat", "tokens": ["Na\u00b7tur", "ist", "hel\u00b7ler", "Zei\u00b7tungs\u00b7staat"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcrs Volk auf allen Gassen;", "tokens": ["F\u00fcrs", "Volk", "auf", "al\u00b7len", "Gas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das Wort ist Freund- und Vaterrath,", "tokens": ["Das", "Wort", "ist", "Freun\u00b7d", "und", "Va\u00b7ter\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nur Kinder k\u00f6nnen's fassen.", "tokens": ["Nur", "Kin\u00b7der", "k\u00f6n\u00b7nen's", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}