{"textgrid.poem.33807": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebl\u00fcht hat einst der Pol im Tropenlichte,", "tokens": ["Ge\u00b7bl\u00fcht", "hat", "einst", "der", "Pol", "im", "Tro\u00b7pen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die W\u00fcste trug den Scho\u00df voll Sommerbl\u00fcten,", "tokens": ["Die", "W\u00fcs\u00b7te", "trug", "den", "Scho\u00df", "voll", "Som\u00b7mer\u00b7bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Steppe sang, die Heidequellen spr\u00fchten,", "tokens": ["Die", "Step\u00b7pe", "sang", ",", "die", "Hei\u00b7de\u00b7quel\u00b7len", "spr\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo jetzt das Meer, stand einst die Bernsteinfichte.", "tokens": ["Wo", "jetzt", "das", "Meer", ",", "stand", "einst", "die", "Bern\u00b7stein\u00b7fich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erinnrung lebt noch; oft wie Traumgesichte", "tokens": ["E\u00b7rinn\u00b7rung", "lebt", "noch", ";", "oft", "wie", "Traum\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$.", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Malt seiner Vorzeit Bild das Mittagsbr\u00fcten", "tokens": ["Malt", "sei\u00b7ner", "Vor\u00b7zeit", "Bild", "das", "Mit\u00b7tags\u00b7br\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der W\u00fcstenluft; die Blumen der vergl\u00fchten", "tokens": ["Der", "W\u00fcs\u00b7ten\u00b7luft", ";", "die", "Blu\u00b7men", "der", "ver\u00b7gl\u00fch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Polsonne stehn auf dunkler Kohlenschichte.", "tokens": ["Pol\u00b7son\u00b7ne", "stehn", "auf", "dunk\u00b7ler", "Koh\u00b7len\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch lebt ein Baum seit fr\u00fchen Erdenaltern,", "tokens": ["Auch", "lebt", "ein", "Baum", "seit", "fr\u00fc\u00b7hen", "Er\u00b7de\u00b7nal\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der oft, umrankt von lauschenden Lianen,", "tokens": ["Der", "oft", ",", "um\u00b7rankt", "von", "lau\u00b7schen\u00b7den", "Li\u00b7a\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Urwald noch erz\u00e4hlt von seinen Ahnen.", "tokens": ["Dem", "Ur\u00b7wald", "noch", "er\u00b7z\u00e4hlt", "von", "sei\u00b7nen", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann lauscht um ihn ein Kreis von blauen Faltern,", "tokens": ["Dann", "lauscht", "um", "ihn", "ein", "Kreis", "von", "blau\u00b7en", "Fal\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann horcht der Papagei mit offnem Schnabel,", "tokens": ["Dann", "horcht", "der", "Pa\u00b7pa\u00b7gei", "mit", "off\u00b7nem", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieses ist des Baums uralte Fabel:", "tokens": ["Und", "die\u00b7ses", "ist", "des", "Baums", "ur\u00b7al\u00b7te", "Fa\u00b7bel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}