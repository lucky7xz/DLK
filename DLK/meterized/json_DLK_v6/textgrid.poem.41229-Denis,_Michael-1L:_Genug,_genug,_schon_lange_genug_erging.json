{"textgrid.poem.41229": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Genug, genug, schon lange genug erging", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Genug, genug, schon lange genug erging", "tokens": ["Ge\u00b7nug", ",", "ge\u00b7nug", ",", "schon", "lan\u00b7ge", "ge\u00b7nug", "er\u00b7ging"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Tokays H\u00f6hen, edelster Traubensohn!", "tokens": ["Von", "To\u00b7kays", "H\u00f6\u00b7hen", ",", "e\u00b7dels\u00b7ter", "Trau\u00b7ben\u00b7sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Goldflu\u00df und von Tarzals H\u00f6hen,", "tokens": ["Dein", "Gold\u00b7flu\u00df", "und", "von", "Tar\u00b7zals", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Greisenverj\u00fcng'ter, Herzenerfreuer!", "tokens": ["Grei\u00b7sen\u00b7ver\u00b7j\u00fcng'\u00b7ter", ",", "Her\u00b7ze\u00b7ner\u00b7freu\u00b7er", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Des Herbstes Ersterzeugter und Stolz der Thei\u00df!", "tokens": ["Des", "Herbs\u00b7tes", "E\u00b7rster\u00b7zeug\u00b7ter", "und", "Stolz", "der", "Thei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du warst genug, schon lange genug der F\u00fcrst,", "tokens": ["Du", "warst", "ge\u00b7nug", ",", "schon", "lan\u00b7ge", "ge\u00b7nug", "der", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bei vaterl\u00e4ndischen Gelagen,", "tokens": ["Bei", "va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7schen", "Ge\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den \u00fcbrigen Traubens\u00f6hnen.", "tokens": ["Un\u00b7ter", "den", "\u00fcb\u00b7ri\u00b7gen", "Trau\u00b7ben\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Nun sieht den Irrthum langer Jahrhunderte", "tokens": ["Nun", "sieht", "den", "Irr\u00b7thum", "lan\u00b7ger", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der weise Gaumen eckeler Gro\u00dfen ein,", "tokens": ["Der", "wei\u00b7se", "Gau\u00b7men", "ec\u00b7ke\u00b7ler", "Gro\u00b7\u00dfen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erkl\u00e4rt dich angema\u00dfter Herrschaft", "tokens": ["Er\u00b7kl\u00e4rt", "dich", "an\u00b7ge\u00b7ma\u00df\u00b7ter", "Herr\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber die Rebengetr\u00e4nke schuldig,", "tokens": ["Ue\u00b7ber", "die", "Re\u00b7ben\u00b7ge\u00b7tr\u00e4n\u00b7ke", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und wittert nach den Trauben der Ufer hin,", "tokens": ["Und", "wit\u00b7tert", "nach", "den", "Trau\u00b7ben", "der", "U\u00b7fer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo Gothen einst geboten, verarteten,", "tokens": ["Wo", "Go\u00b7then", "einst", "ge\u00b7bo\u00b7ten", ",", "ver\u00b7ar\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von S\u00fcden hergeschiffter, schwarzer", "tokens": ["Von", "S\u00fc\u00b7den", "her\u00b7ge\u00b7schiff\u00b7ter", ",", "schwar\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fremdlinge leidende Knechte wurden.", "tokens": ["Fremd\u00b7lin\u00b7ge", "lei\u00b7den\u00b7de", "Knech\u00b7te", "wur\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Schimpf ist gro\u00df. Doch tr\u00f6ste dich Heldentrank!", "tokens": ["Der", "Schimpf", "ist", "gro\u00df", ".", "Doch", "tr\u00f6s\u00b7te", "dich", "Hel\u00b7den\u00b7trank", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du hast Gef\u00e4hrten. Siehe, den h\u00f6flichen,", "tokens": ["Du", "hast", "Ge\u00b7f\u00e4hr\u00b7ten", ".", "Sie\u00b7he", ",", "den", "h\u00f6f\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "VVIMP", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geschminkten, kalten Auslandliedern", "tokens": ["Ge\u00b7schmink\u00b7ten", ",", "kal\u00b7ten", "Aus\u00b7land\u00b7lie\u00b7dern"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hangen mit hastigem Ohr' entgegen", "tokens": ["Han\u00b7gen", "mit", "has\u00b7ti\u00b7gem", "Ohr'", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Thuiskons Zwitterenkel, verachten ihn,", "tokens": ["Thu\u00b7is\u00b7kons", "Zwit\u00b7te\u00b7ren\u00b7kel", ",", "ver\u00b7ach\u00b7ten", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verstehn ihn nicht den heimischen Herzgesang,", "tokens": ["Ver\u00b7stehn", "ihn", "nicht", "den", "hei\u00b7mi\u00b7schen", "Herz\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den k\u00fchnen hei\u00dfen vollgedr\u00e4ngten", "tokens": ["Den", "k\u00fch\u00b7nen", "hei\u00b7\u00dfen", "voll\u00b7ge\u00b7dr\u00e4ng\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Thatenverewiger, Seelenheber.", "tokens": ["Tha\u00b7ten\u00b7ve\u00b7re\u00b7wi\u00b7ger", ",", "See\u00b7len\u00b7he\u00b7ber", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ist er darum nun weniger, was er ist?", "tokens": ["Ist", "er", "da\u00b7rum", "nun", "we\u00b7ni\u00b7ger", ",", "was", "er", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "ADV", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "O nein! Von seinem blaulichen Wege sieht,", "tokens": ["O", "nein", "!", "Von", "sei\u00b7nem", "blau\u00b7li\u00b7chen", "We\u00b7ge", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll seines Werthes, dieser, Adler", "tokens": ["Voll", "sei\u00b7nes", "Wert\u00b7hes", ",", "die\u00b7ser", ",", "Ad\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "PDS", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruhig auf qu\u00e4kende S\u00fcmpfe nieder.", "tokens": ["Ru\u00b7hig", "auf", "qu\u00e4\u00b7ken\u00b7de", "S\u00fcmp\u00b7fe", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "So du auch, Sohn der Traube! Wann Friederichs", "tokens": ["So", "du", "auch", ",", "Sohn", "der", "Trau\u00b7be", "!", "Wann", "Frie\u00b7de\u00b7richs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "ADV", "$,", "NN", "ART", "NN", "$.", "PWAV", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Herrmanns Barde", "tokens": ["Und", "Herr\u00b7manns", "Bar\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der jetzt auf Tarzal und Tokay", "tokens": ["Der", "jetzt", "auf", "Tar\u00b7zal", "und", "To\u00b7kay"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zeitiget, andre Gebirge g\u00f6lden.", "tokens": ["Zei\u00b7ti\u00b7get", ",", "and\u00b7re", "Ge\u00b7bir\u00b7ge", "g\u00f6l\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}