{"textgrid.poem.24916": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Normannenverm\u00e4chtnis", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig winkt \u2013 es reihen im Kreis,", "tokens": ["Der", "K\u00f6\u00b7nig", "winkt", "\u2013", "es", "rei\u00b7hen", "im", "Kreis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Knechte sich ehrfurchtsvoll;", "tokens": ["Die", "Knech\u00b7te", "sich", "ehr\u00b7furchts\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie tragen hinweg auf der S\u00e4nfte den Greis,", "tokens": ["Sie", "tra\u00b7gen", "hin\u00b7weg", "auf", "der", "S\u00e4nf\u00b7te", "den", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Strand ihn entlang, wo um Klippen von Eis", "tokens": ["Den", "Strand", "ihn", "ent\u00b7lang", ",", "wo", "um", "Klip\u00b7pen", "von", "Eis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PWAV", "APPR", "NN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Erdr\u00f6hnt der Wogen Geroll.", "tokens": ["Er\u00b7dr\u00f6hnt", "der", "Wo\u00b7gen", "Ge\u00b7roll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ihm folgte von hundert Rossen ein Zug,", "tokens": ["Ihm", "folg\u00b7te", "von", "hun\u00b7dert", "Ros\u00b7sen", "ein", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Krone, Scepter und Thron", "tokens": ["Der", "Kro\u00b7ne", ",", "Scep\u00b7ter", "und", "Thron"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und die Sch\u00e4tze, die er erbeutet, trug;", "tokens": ["Und", "die", "Sch\u00e4t\u00b7ze", ",", "die", "er", "er\u00b7beu\u00b7tet", ",", "trug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beim Vater ging und zur Erde schlug", "tokens": ["Beim", "Va\u00b7ter", "ging", "und", "zur", "Er\u00b7de", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Voll Trauer die Augen der Sohn.", "tokens": ["Voll", "Trau\u00b7er", "die", "Au\u00b7gen", "der", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und als sie kamen zum tosenden Fjord,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "zum", "to\u00b7sen\u00b7den", "Fjord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo geankert das Heerschiff lag", "tokens": ["Wo", "ge\u00b7an\u00b7kert", "das", "Heer\u00b7schiff", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Wellen hoch aufpeitschte der Nord,", "tokens": ["Und", "die", "Wel\u00b7len", "hoch", "auf\u00b7peitschte", "der", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Neu winkte der K\u00f6nig: \u00bbDas ist der Ort,", "tokens": ["Neu", "wink\u00b7te", "der", "K\u00f6\u00b7nig", ":", "\u00bb", "Das", "ist", "der", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$(", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und heute ist es der Tag.", "tokens": ["Und", "heu\u00b7te", "ist", "es", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Alt ward ich; die Sehnen sind mir erschlafft;", "tokens": ["Alt", "ward", "ich", ";", "die", "Seh\u00b7nen", "sind", "mir", "er\u00b7schlafft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Knochen Mark ist verdorrt;", "tokens": ["Der", "Kno\u00b7chen", "Mark", "ist", "ver\u00b7dorrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nicht kann ich mehr schaffen, wie sonst ich geschafft;", "tokens": ["Nicht", "kann", "ich", "mehr", "schaf\u00b7fen", ",", "wie", "sonst", "ich", "ge\u00b7schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PWAV", "ADV", "PPER", "VVPP", "$."], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nicht blitzt, geschleudert mit alter Kraft,", "tokens": ["Nicht", "blitzt", ",", "ge\u00b7schleu\u00b7dert", "mit", "al\u00b7ter", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mein Beil in der Schlacht hinfort.", "tokens": ["Mein", "Beil", "in", "der", "Schlacht", "hin\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und sollt' ich nun, statt zu schl\u00fcrfen den Hauch", "tokens": ["Und", "sollt'", "ich", "nun", ",", "statt", "zu", "schl\u00fcr\u00b7fen", "den", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "$,", "KOUI", "PTKZU", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der eisigen Meeresluft,", "tokens": ["Der", "ei\u00b7si\u00b7gen", "Mee\u00b7res\u00b7luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im dumpfen Gemach ersticken am Rauch?", "tokens": ["Im", "dum\u00b7pfen", "Ge\u00b7mach", "er\u00b7sti\u00b7cken", "am", "Rauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf dem Holzsto\u00df lieber nach Nordmannsbrauch", "tokens": ["Auf", "dem", "Holz\u00b7sto\u00df", "lie\u00b7ber", "nach", "Nord\u00b7manns\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Erw\u00e4hl' ich die lodernde Gruft.", "tokens": ["Er\u00b7w\u00e4hl'", "ich", "die", "lo\u00b7dern\u00b7de", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Mit mir verzehre die Flammenglut", "tokens": ["Mit", "mir", "ver\u00b7zeh\u00b7re", "die", "Flam\u00b7men\u00b7glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "All meine Habe zugleich!", "tokens": ["All", "mei\u00b7ne", "Ha\u00b7be", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mein Rolf, nicht lass' ich dir Thron noch Gut,", "tokens": ["Mein", "Rolf", ",", "nicht", "lass'", "ich", "dir", "Thron", "noch", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PTKNEG", "VVFIN", "PPER", "PPER", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dir nur meinen Ruhm und die sch\u00e4umende Flut;", "tokens": ["Dir", "nur", "mei\u00b7nen", "Ruhm", "und", "die", "sch\u00e4u\u00b7men\u00b7de", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sie sei dein K\u00f6nigreich!", "tokens": ["Sie", "sei", "dein", "K\u00f6\u00b7nig\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nichts fruchtet dem Sohn ein Schatz, am Herd", "tokens": ["Nichts", "fruch\u00b7tet", "dem", "Sohn", "ein", "Schatz", ",", "am", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Vater und Ahnen ererbt;", "tokens": ["Von", "Va\u00b7ter", "und", "Ah\u00b7nen", "er\u00b7erbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "F\u00fcr den Thron nur, den er erk\u00e4mpft mit dem Schwert,", "tokens": ["F\u00fcr", "den", "Thron", "nur", ",", "den", "er", "er\u00b7k\u00e4mpft", "mit", "dem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur f\u00fcr ", "tokens": ["Nur", "f\u00fcr"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Den in Feindesblut er gef\u00e4rbt.", "tokens": ["Den", "in", "Fein\u00b7des\u00b7blut", "er", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Dein Reich ist weit, ist weit wie die Welt;", "tokens": ["Dein", "Reich", "ist", "weit", ",", "ist", "weit", "wie", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schau hin! Was w\u00e4hlst du, mein Rolf?", "tokens": ["Schau", "hin", "!", "Was", "w\u00e4hlst", "du", ",", "mein", "Rolf", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Das Klippengestad', wo das Kriegshorn gellt", "tokens": ["Das", "Klip\u00b7pen\u00b7ge\u00b7stad'", ",", "wo", "das", "Kriegs\u00b7horn", "gellt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und der Nordschein flammend die Wogen erhellt?", "tokens": ["Und", "der", "Nord\u00b7schein", "flam\u00b7mend", "die", "Wo\u00b7gen", "er\u00b7hellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Im S\u00fcden den blauenden Golf?", "tokens": ["Im", "S\u00fc\u00b7den", "den", "blau\u00b7en\u00b7den", "Golf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Dort leuchten goldene Fr\u00fcchte am Strand", "tokens": ["Dort", "leuch\u00b7ten", "gol\u00b7de\u00b7ne", "Fr\u00fcch\u00b7te", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Schl\u00f6sser aus G\u00e4rten hervor;", "tokens": ["Und", "Schl\u00f6s\u00b7ser", "aus", "G\u00e4r\u00b7ten", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nur gewagt! und der lieblichen Herrin Hand,", "tokens": ["Nur", "ge\u00b7wagt", "!", "und", "der", "lieb\u00b7li\u00b7chen", "Her\u00b7rin", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Du wirst sie gewinnen mit Schlo\u00df und Land,", "tokens": ["Du", "wirst", "sie", "ge\u00b7win\u00b7nen", "mit", "Schlo\u00df", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wenn du schreitest als Sieger durchs Thor.", "tokens": ["Wenn", "du", "schrei\u00b7test", "als", "Sie\u00b7ger", "durchs", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}}, "stanza.10": {"line.1": {"text": "Einst stritt auch ich dort im Kampfgew\u00fchl,", "tokens": ["Einst", "stritt", "auch", "ich", "dort", "im", "Kampf\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir trieften die Locken von Blut;", "tokens": ["Mir", "trief\u00b7ten", "die", "Lo\u00b7cken", "von", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch die Nacht dann hab' ich auf duftendem Pf\u00fchl", "tokens": ["Doch", "die", "Nacht", "dann", "hab'", "ich", "auf", "duf\u00b7ten\u00b7dem", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Myrtenhaine bei Saitenspiel", "tokens": ["Im", "Myr\u00b7ten\u00b7hai\u00b7ne", "bei", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "An weichem Busen geruht.", "tokens": ["An", "wei\u00b7chem", "Bu\u00b7sen", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Genug, genug! so lange das her!", "tokens": ["Ge\u00b7nug", ",", "ge\u00b7nug", "!", "so", "lan\u00b7ge", "das", "her", "!"], "token_info": ["word", "punct", "word", "punct", "w