{"textgrid.poem.52161": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Abendwolke", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So stille ruht im Hafen", "tokens": ["So", "stil\u00b7le", "ruht", "im", "Ha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das tiefe Wasser dort,", "tokens": ["Das", "tie\u00b7fe", "Was\u00b7ser", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Ruder sind entschlafen,", "tokens": ["Die", "Ru\u00b7der", "sind", "ent\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schifflein sind im Port.", "tokens": ["Die", "Schif\u00b7flein", "sind", "im", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur oben in dem \u00c4ther", "tokens": ["Nur", "o\u00b7ben", "in", "dem", "\u00c4\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der lauen Maiennacht,", "tokens": ["Der", "lau\u00b7en", "Mai\u00b7en\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort segelt noch ein sp\u00e4ter", "tokens": ["Dort", "se\u00b7gelt", "noch", "ein", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Friedfert'ger Ferge sacht.", "tokens": ["Fried\u00b7fert'\u00b7ger", "Fer\u00b7ge", "sacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Barke still und dunkel", "tokens": ["Die", "Bar\u00b7ke", "still", "und", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4hrt hin in D\u00e4mmerschein", "tokens": ["F\u00e4hrt", "hin", "in", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und leisem Sterngefunkel", "tokens": ["Und", "lei\u00b7sem", "Stern\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Himmel und hinein.", "tokens": ["Am", "Him\u00b7mel", "und", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}