{"textgrid.poem.60067": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die alte Finsterni\u00df entwich;", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alte Finsterni\u00df entwich;", "tokens": ["Die", "al\u00b7te", "Fins\u00b7ter\u00b7ni\u00df", "ent\u00b7wich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00fcste ward erhellt:", "tokens": ["Die", "W\u00fcs\u00b7te", "ward", "er\u00b7hellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da baute Gott, der Sch\u00f6pfer, sich", "tokens": ["Da", "bau\u00b7te", "Gott", ",", "der", "Sch\u00f6p\u00b7fer", ",", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "ART", "NN", "$,", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Tempel diese Welt.", "tokens": ["Zum", "Tem\u00b7pel", "die\u00b7se", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Eintracht wandelte die Schaar", "tokens": ["In", "Ein\u00b7tracht", "wan\u00b7del\u00b7te", "die", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der lichten Sterne fort;", "tokens": ["Der", "lich\u00b7ten", "Ster\u00b7ne", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Liebe, lauter Liebe war", "tokens": ["Und", "Lie\u00b7be", ",", "lau\u00b7ter", "Lie\u00b7be", "war"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das gro\u00dfe Sch\u00f6pfungs-Wort.", "tokens": ["Das", "gro\u00b7\u00dfe", "Sch\u00f6p\u00b7fungs\u00b7Wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf Erden mu\u00dft' ein Paradies,", "tokens": ["Auf", "Er\u00b7den", "mu\u00dft'", "ein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Liebes-Tempel bl\u00fchn,", "tokens": ["Ein", "Lie\u00b7bes\u00b7Tem\u00b7pel", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo jedes L\u00fcftchen ruhig blies", "tokens": ["Wo", "je\u00b7des", "L\u00fcft\u00b7chen", "ru\u00b7hig", "blies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs friedenvolle Gr\u00fcn;", "tokens": ["Durchs", "frie\u00b7den\u00b7vol\u00b7le", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo in der Unschuld Heiligthum", "tokens": ["Wo", "in", "der", "Un\u00b7schuld", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lamm bey Tigern ging,", "tokens": ["Das", "Lamm", "bey", "Ti\u00b7gern", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Zweig an Zweig, und Blum' an Blum'", "tokens": ["Wo", "Zweig", "an", "Zweig", ",", "und", "Blum'", "an", "Blum'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Liebes-Knoten hing.", "tokens": ["In", "Lie\u00b7bes\u00b7Kno\u00b7ten", "hing", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier sollten, gleich dem Sonnenstrahl,", "tokens": ["Hier", "soll\u00b7ten", ",", "gleich", "dem", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seelen alle rein,", "tokens": ["Die", "See\u00b7len", "al\u00b7le", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf jedem Berg, in jedem Thal", "tokens": ["Auf", "je\u00b7dem", "Berg", ",", "in", "je\u00b7dem", "Thal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschen Br\u00fcder seyn.", "tokens": ["Die", "Men\u00b7schen", "Br\u00fc\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vergebens, ach! es floh zu bald,", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "ach", "!", "es", "floh", "zu", "bald", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ITJ", "$.", "PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es floh die goldne Zeit;", "tokens": ["Es", "floh", "die", "gold\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins Reich der Liebe trat Gewalt;", "tokens": ["Ins", "Reich", "der", "Lie\u00b7be", "trat", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tempel war entweiht.", "tokens": ["Der", "Tem\u00b7pel", "war", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn aber seliges Vertraun", "tokens": ["Wenn", "a\u00b7ber", "se\u00b7li\u00b7ges", "Ver\u00b7traun"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ganz die Erde lie\u00df,", "tokens": ["Nicht", "ganz", "die", "Er\u00b7de", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00dft uns wieder auferbaun", "tokens": ["So", "la\u00dft", "uns", "wie\u00b7der", "au\u00b7fer\u00b7baun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wonne-Paradies.", "tokens": ["Ein", "Won\u00b7ne\u00b7Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O selig, drey Mahl selig ist", "tokens": ["O", "se\u00b7lig", ",", "drey", "Mahl", "se\u00b7lig", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "CARD", "NN", "ADJD", "VAFIN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das Pl\u00e4tzchen unter'm Mond,", "tokens": ["Das", "Pl\u00e4tz\u00b7chen", "un\u00b7ter'm", "Mond", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo sich mit Einfalt Wahrheit k\u00fc\u00dft,", "tokens": ["Wo", "sich", "mit", "Ein\u00b7falt", "Wahr\u00b7heit", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Liebe Treue wohnt;", "tokens": ["Bey", "Lie\u00b7be", "Treu\u00b7e", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Gro\u00dfe mit dem Niedern geht,", "tokens": ["Der", "Gro\u00b7\u00dfe", "mit", "dem", "Nie\u00b7dern", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn br\u00fcderlich umarmt,", "tokens": ["Ihn", "br\u00fc\u00b7der\u00b7lich", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Schw\u00e4chern, der um Beystand fleht,", "tokens": ["Des", "Schw\u00e4\u00b7chern", ",", "der", "um", "Beys\u00b7tand", "fleht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein St\u00e4rkrer sich erbarmt;", "tokens": ["Ein", "St\u00e4r\u00b7krer", "sich", "er\u00b7barmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Am Morgen, wenn des Landmanns Lied", "tokens": ["Am", "Mor\u00b7gen", ",", "wenn", "des", "Land\u00b7manns", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus voller Scheun' erklingt,", "tokens": ["Aus", "vol\u00b7ler", "Scheun'", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wittwe nicht gen Himmel sieht,", "tokens": ["Die", "Witt\u00b7we", "nicht", "gen", "Him\u00b7mel", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und matt die H\u00e4nde ringt;", "tokens": ["Und", "matt", "die", "H\u00e4n\u00b7de", "ringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Am Abend, wer sein graues Haar", "tokens": ["Am", "A\u00b7bend", ",", "wer", "sein", "grau\u00b7es", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ehr' im Stillen tr\u00e4gt,", "tokens": ["Mit", "Ehr'", "im", "Stil\u00b7len", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich nach so manchem sauren Jahr", "tokens": ["Sich", "nach", "so", "man\u00b7chem", "sau\u00b7ren", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht trostlos niederlegt!", "tokens": ["Nicht", "trost\u00b7los", "nie\u00b7der\u00b7legt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wohlauf, ihr Br\u00fcder! la\u00dft uns so,", "tokens": ["Wohl\u00b7auf", ",", "ihr", "Br\u00fc\u00b7der", "!", "la\u00dft", "uns", "so", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beharrlich im Vertraun,", "tokens": ["Be\u00b7harr\u00b7lich", "im", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In unserm Paradiese froh", "tokens": ["In", "un\u00b7serm", "Pa\u00b7ra\u00b7die\u00b7se", "froh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Liebes-Tempel baun.", "tokens": ["Den", "Lie\u00b7bes\u00b7Tem\u00b7pel", "baun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mag er im Erdenschatten hier", "tokens": ["Mag", "er", "im", "Er\u00b7den\u00b7schat\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur unvollendet stehn!", "tokens": ["Nur", "un\u00b7voll\u00b7en\u00b7det", "stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einst \u00fcber Sternen werden wir", "tokens": ["Einst", "\u00fc\u00b7ber", "Ster\u00b7nen", "wer\u00b7den", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den bessern Tempel sehn.", "tokens": ["Den", "bes\u00b7sern", "Tem\u00b7pel", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}