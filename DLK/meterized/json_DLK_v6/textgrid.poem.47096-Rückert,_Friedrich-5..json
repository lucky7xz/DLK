{"textgrid.poem.47096": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaub' nur, weil ich von dir gehe,", "tokens": ["Glaub'", "nur", ",", "weil", "ich", "von", "dir", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht, da\u00df darum es geschehe,", "tokens": ["Nicht", ",", "da\u00df", "da\u00b7rum", "es", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PAV", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Weil ich such' ein sch\u00f6nres Gl\u00fcck als hier!", "tokens": ["Weil", "ich", "such'", "ein", "sch\u00f6n\u00b7res", "Gl\u00fcck", "als", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "KOKOM", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eben darum, weil ich keines", "tokens": ["E\u00b7ben", "da\u00b7rum", ",", "weil", "ich", "kei\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PAV", "$,", "KOUS", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Such' im Strahl des Sonnenscheines,", "tokens": ["Such'", "im", "Strahl", "des", "Son\u00b7nen\u00b7schei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben darum geh' ich fort von dir.", "tokens": ["E\u00b7ben", "da\u00b7rum", "geh'", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}