{"textgrid.poem.57917": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Neuphilologie", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den Schutzmann frage:", "tokens": ["Wenn", "ich", "den", "Schutz\u00b7mann", "fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo willst du hin?", "tokens": ["Wo", "willst", "du", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbzur englischen Stunde!\u00ab", "tokens": ["\u00bb", "zur", "eng\u00b7li\u00b7schen", "Stun\u00b7de", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Spricht er mit tr\u00fcbem Sinn.", "tokens": ["Spricht", "er", "mit", "tr\u00fc\u00b7bem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich den Schutzmann frage:", "tokens": ["Wenn", "ich", "den", "Schutz\u00b7mann", "fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo kommst du her?", "tokens": ["Wo", "kommst", "du", "her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbich ochse Franz\u00f6sisch!\u00ab", "tokens": ["\u00bb", "ich", "oc\u00b7hse", "Fran\u00b7z\u00f6\u00b7sisch", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Spricht er und seufzet schwer.", "tokens": ["Spricht", "er", "und", "seuf\u00b7zet", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er hat es doch begriffen,", "tokens": ["Er", "hat", "es", "doch", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ging's anfangs auch nicht leicht,", "tokens": ["Ging's", "an\u00b7fangs", "auch", "nicht", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kriegte es doch binnen", "tokens": ["Er", "krieg\u00b7te", "es", "doch", "bin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach: \u00bbEs ist erreicht!\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Es", "ist", "er\u00b7reicht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als ich ihn dann fragte:", "tokens": ["Und", "als", "ich", "ihn", "dann", "frag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verstand er meine Rede", "tokens": ["Ver\u00b7stand", "er", "mei\u00b7ne", "Re\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sagte fr\u00f6hlich: \u00bbNee!\u00ab", "tokens": ["Und", "sag\u00b7te", "fr\u00f6h\u00b7lich", ":", "\u00bb", "Nee", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}