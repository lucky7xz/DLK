{"dta.poem.2305": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonne! alle Menschenzungen", "tokens": ["Son\u00b7ne", "!", "al\u00b7le", "Men\u00b7schen\u00b7zun\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Loben deine goldne Stralen.", "tokens": ["Lo\u00b7ben", "dei\u00b7ne", "gold\u00b7ne", "Stra\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4che, wo sich Nimsen baden,", "tokens": ["B\u00e4\u00b7che", ",", "wo", "sich", "Nim\u00b7sen", "ba\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PRF", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie sich am Ufer troknen;", "tokens": ["Wo", "sie", "sich", "am", "U\u00b7fer", "trok\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Th\u00e4ler, wo sich Hirt\u2019 und Heerde", "tokens": ["Th\u00e4\u00b7ler", ",", "wo", "sich", "Hirt'", "und", "Heer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Glut entgegen lagert;", "tokens": ["Dei\u00b7ner", "Glut", "ent\u00b7ge\u00b7gen", "la\u00b7gert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Berge, wo von dir erw\u00e4rmet", "tokens": ["Ber\u00b7ge", ",", "wo", "von", "dir", "er\u00b7w\u00e4r\u00b7met"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ei\u00df und Schnee in Th\u00e4ler rinnet;", "tokens": ["Ei\u00df", "und", "Schnee", "in", "Th\u00e4\u00b7ler", "rin\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Klippen, wo an kalten Eichen", "tokens": ["Klip\u00b7pen", ",", "wo", "an", "kal\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ziegen hangen, Gemsen klettern;", "tokens": ["Zie\u00b7gen", "han\u00b7gen", ",", "Gem\u00b7sen", "klet\u00b7tern", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Fluren, wo Narzissen bl\u00fchen", "tokens": ["Flu\u00b7ren", ",", "wo", "Nar\u00b7zis\u00b7sen", "bl\u00fc\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo dein Stral Violen w\u00e4rmet,", "tokens": ["Wo", "dein", "Stral", "Vi\u00b7o\u00b7len", "w\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Danken dir f\u00fcr deine Stralen:", "tokens": ["Dan\u00b7ken", "dir", "f\u00fcr", "dei\u00b7ne", "Stra\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Aber ich kann dir nicht danken;", "tokens": ["A\u00b7ber", "ich", "kann", "dir", "nicht", "dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.15": {"text": "Denn du straltest gar zu helle,", "tokens": ["Denn", "du", "stral\u00b7test", "gar", "zu", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als mich in der Sommerlaube", "tokens": ["Als", "mich", "in", "der", "Som\u00b7mer\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Keine Mutter schen sollte.", "tokens": ["Kei\u00b7ne", "Mut\u00b7ter", "schen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}