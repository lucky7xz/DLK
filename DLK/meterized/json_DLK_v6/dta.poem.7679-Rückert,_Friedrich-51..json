{"dta.poem.7679": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel besser, da\u00df ein Volk nur einen Irrthum habe,", "tokens": ["Viel", "bes\u00b7ser", ",", "da\u00df", "ein", "Volk", "nur", "ei\u00b7nen", "Irr\u00b7thum", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als eine Wahrheit f\u00fcr sich selber jeder Knabe.", "tokens": ["Als", "ei\u00b7ne", "Wahr\u00b7heit", "f\u00fcr", "sich", "sel\u00b7ber", "je\u00b7der", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PRF", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel besser, da\u00df den Dienst ein gro\u00dfer G\u00f6tz besitze,", "tokens": ["Viel", "bes\u00b7ser", ",", "da\u00df", "den", "Dienst", "ein", "gro\u00b7\u00dfer", "G\u00f6tz", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als jeder f\u00fcr sein Haus sich einen kleinen schnitze.", "tokens": ["Als", "je\u00b7der", "f\u00fcr", "sein", "Haus", "sich", "ei\u00b7nen", "klei\u00b7nen", "schnit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "PRF", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Unsinn machte mir nicht die Erbitterung,", "tokens": ["Der", "Un\u00b7sinn", "mach\u00b7te", "mir", "nicht", "die", "Er\u00b7bit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als der Gesinnungszwist, die Sinnzersplitterung.", "tokens": ["Als", "der", "Ge\u00b7sin\u00b7nungs\u00b7zwist", ",", "die", "Sinn\u00b7zer\u00b7split\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}