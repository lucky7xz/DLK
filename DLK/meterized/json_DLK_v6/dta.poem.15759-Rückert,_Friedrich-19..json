{"dta.poem.15759": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum mit Reimen euch, und schweren Reimen, qu\u00e4len?", "tokens": ["Wa\u00b7rum", "mit", "Rei\u00b7men", "euch", ",", "und", "schwe\u00b7ren", "Rei\u00b7men", ",", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "$,", "KON", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' es, ihr Dichter, nicht genug die Silben z\u00e4hlen?", "tokens": ["W\u00e4r'", "es", ",", "ihr", "Dich\u00b7ter", ",", "nicht", "ge\u00b7nug", "die", "Sil\u00b7ben", "z\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Griechen wars genug, warum w\u00e4rs uns nicht auch?", "tokens": ["Den", "Grie\u00b7chen", "wars", "ge\u00b7nug", ",", "wa\u00b7rum", "w\u00e4rs", "uns", "nicht", "auch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Silbenz\u00e4hlung selbst ist zeitlicher Gebrauch:", "tokens": ["Doch", "Sil\u00b7ben\u00b7z\u00e4h\u00b7lung", "selbst", "ist", "zeit\u00b7li\u00b7cher", "Ge\u00b7brauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Psalter Davids rauscht noch ohne Silbenzahl;", "tokens": ["Der", "Psal\u00b7ter", "Da\u00b7vids", "rauscht", "noch", "oh\u00b7ne", "Sil\u00b7ben\u00b7zahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so aus Zeit in Zeit wuchs mit der Kunst die Qual;", "tokens": ["Und", "so", "aus", "Zeit", "in", "Zeit", "wuchs", "mit", "der", "Kunst", "die", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wuchs mit der Genu\u00df, dem H\u00f6rer nicht allein,", "tokens": ["Und", "wuchs", "mit", "der", "Ge\u00b7nu\u00df", ",", "dem", "H\u00f6\u00b7rer", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Dichter allermeist, der gern geplagt will seyn.", "tokens": ["Dem", "Dich\u00b7ter", "al\u00b7ler\u00b7meist", ",", "der", "gern", "ge\u00b7plagt", "will", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer will nun jeder Zeit bestimmen gleiches Ma\u00df,", "tokens": ["Wer", "will", "nun", "je\u00b7der", "Zeit", "be\u00b7stim\u00b7men", "glei\u00b7ches", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da jede nach Bedarf ihr eignes stets besa\u00df?", "tokens": ["Da", "je\u00b7de", "nach", "Be\u00b7darf", "ihr", "eig\u00b7nes", "stets", "be\u00b7sa\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "NN", "PPOSAT", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der K\u00fcnstler aber sei gelobt, der f\u00fchlt und w\u00e4gt,", "tokens": ["Der", "K\u00fcnst\u00b7ler", "a\u00b7ber", "sei", "ge\u00b7lobt", ",", "der", "f\u00fchlt", "und", "w\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was seine Zeit von Kunst bedarf und was vertr\u00e4gt;", "tokens": ["Was", "sei\u00b7ne", "Zeit", "von", "Kunst", "be\u00b7darf", "und", "was", "ver\u00b7tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der ihr nichts bietet, was sie nicht vertr\u00e4gt, nichts weigert,", "tokens": ["Der", "ihr", "nichts", "bie\u00b7tet", ",", "was", "sie", "nicht", "ver\u00b7tr\u00e4gt", ",", "nichts", "wei\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie bedarf, und nicht ihr falsch Bed\u00fcrfnis steigert.", "tokens": ["Was", "sie", "be\u00b7darf", ",", "und", "nicht", "ihr", "falsch", "Be\u00b7d\u00fcrf\u00b7nis", "stei\u00b7gert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "PTKNEG", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}