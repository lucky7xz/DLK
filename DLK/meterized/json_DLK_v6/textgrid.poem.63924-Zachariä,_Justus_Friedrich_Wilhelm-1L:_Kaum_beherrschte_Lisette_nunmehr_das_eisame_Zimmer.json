{"textgrid.poem.63924": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaum beherrschte Lisette nunmehr das eisame Zimmer", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum beherrschte Lisette nunmehr das eisame Zimmer", "tokens": ["Kaum", "be\u00b7herrschte", "Li\u00b7set\u00b7te", "nun\u00b7mehr", "das", "ei\u00b7sa\u00b7me", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Unumschr\u00e4nkt und allein, so nahm sie die Maske der Trauer", "tokens": ["Un\u00b7um\u00b7schr\u00e4nkt", "und", "al\u00b7lein", ",", "so", "nahm", "sie", "die", "Mas\u00b7ke", "der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Von dem Gesicht, und war nicht mehr der Seufzer Rosaurens", "tokens": ["Von", "dem", "Ge\u00b7sicht", ",", "und", "war", "nicht", "mehr", "der", "Seuf\u00b7zer", "Ro\u00b7sau\u00b7rens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Stets gef\u00e4lliges Echo. Sie warf auf den Leichnam des Katers,", "tokens": ["Stets", "ge\u00b7f\u00e4l\u00b7li\u00b7ges", "E\u00b7cho", ".", "Sie", "warf", "auf", "den", "Leich\u00b7nam", "des", "Ka\u00b7ters", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Den sie so sehr im Leben geha\u00dft, zufriedene Blicke.", "tokens": ["Den", "sie", "so", "sehr", "im", "Le\u00b7ben", "ge\u00b7ha\u00dft", ",", "zu\u00b7frie\u00b7de\u00b7ne", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Also schaut der w\u00fcrgende Sieger zufrieden in's Schlachtfeld;", "tokens": ["Al\u00b7so", "schaut", "der", "w\u00fcr\u00b7gen\u00b7de", "Sie\u00b7ger", "zu\u00b7frie\u00b7den", "in's", "Schlacht\u00b7feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Weidet die Augen am Blut der Erschlagenen; die wiehernden Rosse", "tokens": ["Wei\u00b7det", "die", "Au\u00b7gen", "am", "Blut", "der", "Er\u00b7schla\u00b7ge\u00b7nen", ";", "die", "wie\u00b7hern\u00b7den", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-+-+-", "measure": "dactylic.tetra.plus"}, "line.8": {"text": "Tragen ihn hoch auf Leichnamen her. \u2013 Indem die Posaune", "tokens": ["Tra\u00b7gen", "ihn", "hoch", "auf", "Leich\u00b7na\u00b7men", "her", ".", "\u2013", "In\u00b7dem", "die", "Po\u00b7sau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$.", "$(", "KOUS", "ART", "NN"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.9": {"text": "Siegender Heerschaaren um ihn ert\u00f6nt, so d\u00fcnkt er ein Gott sich.", "tokens": ["Sie\u00b7gen\u00b7der", "Heer\u00b7schaa\u00b7ren", "um", "ihn", "er\u00b7t\u00f6nt", ",", "so", "d\u00fcnkt", "er", "ein", "Gott", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "$."], "meter": "-+-++--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "H\u00f6hnisch stie\u00df die erbitterte Zofe den blutigen Leichnam", "tokens": ["H\u00f6h\u00b7nisch", "stie\u00df", "die", "er\u00b7bit\u00b7ter\u00b7te", "Zo\u00b7fe", "den", "blu\u00b7ti\u00b7gen", "Leich\u00b7nam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--++", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Mit dem Fu\u00df, doch ri\u00df sie vorher mit entweihenden H\u00e4nden", "tokens": ["Mit", "dem", "Fu\u00df", ",", "doch", "ri\u00df", "sie", "vor\u00b7her", "mit", "ent\u00b7wei\u00b7hen\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Von dem Halse den blendenden Purpur, mit silbernen Blumen,", "tokens": ["Von", "dem", "Hal\u00b7se", "den", "blen\u00b7den\u00b7den", "Pur\u00b7pur", ",", "mit", "sil\u00b7ber\u00b7nen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Und mit Laubwerk gestickt; besah ihn mit geizigen Blicken,", "tokens": ["Und", "mit", "Laub\u00b7werk", "ge\u00b7stickt", ";", "be\u00b7sah", "ihn", "mit", "gei\u00b7zi\u00b7gen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Rollt ihn zusammen und sprach: Dem Himmel sey Dank, da\u00df du endlich", "tokens": ["Rollt", "ihn", "zu\u00b7sam\u00b7men", "und", "sprach", ":", "Dem", "Him\u00b7mel", "sey", "Dank", ",", "da\u00df", "du", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Deinen verr\u00e4th'rischen Hals gebrochen, verworf'nes Gesch\u00f6pfe!", "tokens": ["Dei\u00b7nen", "ver\u00b7r\u00e4th'\u00b7ri\u00b7schen", "Hals", "ge\u00b7bro\u00b7chen", ",", "ver\u00b7wor\u00b7f'\u00b7nes", "Ge\u00b7sch\u00f6p\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "Wohl mir, da\u00df ich dich todt, du falsche Bestie, sehe!", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "dich", "todt", ",", "du", "fal\u00b7sche", "Be\u00b7stie", ",", "se\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "O, wie bin ich so sicher nunmehr, da\u00df k\u00fcnftig mein Fr\u00e4ulein", "tokens": ["O", ",", "wie", "bin", "ich", "so", "si\u00b7cher", "nun\u00b7mehr", ",", "da\u00df", "k\u00fcnf\u00b7tig", "mein", "Fr\u00e4u\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "$,", "KOUS", "ADJD", "PPOSAT", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "In dem Schoo\u00dfe dich wiegt und dich aus Z\u00e4rtlichkeit k\u00fcsset.", "tokens": ["In", "dem", "Schoo\u00b7\u00dfe", "dich", "wiegt", "und", "dich", "aus", "Z\u00e4rt\u00b7lich\u00b7keit", "k\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "KON", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Pfui! wie konnten die sch\u00f6nsten Lippen so z\u00e4rtlich dich k\u00fcssen,", "tokens": ["Pfui", "!", "wie", "konn\u00b7ten", "die", "sch\u00f6ns\u00b7ten", "Lip\u00b7pen", "so", "z\u00e4rt\u00b7lich", "dich", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Und wie konnte die weicheste Hand dein Fuchshaar so streicheln!", "tokens": ["Und", "wie", "konn\u00b7te", "die", "wei\u00b7ches\u00b7te", "Hand", "dein", "Fuchs\u00b7haar", "so", "strei\u00b7cheln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+--+-++-+-", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Geh' nun hin, du hungriger R\u00e4uber, und fri\u00df mir den Braten,", "tokens": ["Geh'", "nun", "hin", ",", "du", "hung\u00b7ri\u00b7ger", "R\u00e4u\u00b7ber", ",", "und", "fri\u00df", "mir", "den", "Bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "KON", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Oder das braune Ragout, das ich vom Munde mir sparte!", "tokens": ["O\u00b7der", "das", "brau\u00b7ne", "Ra\u00b7gout", ",", "das", "ich", "vom", "Mun\u00b7de", "mir", "spar\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Geh' nun hin und w\u00fcrge dir Tauben, und hole dir ferner", "tokens": ["Geh'", "nun", "hin", "und", "w\u00fcr\u00b7ge", "dir", "Tau\u00b7ben", ",", "und", "ho\u00b7le", "dir", "fer\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Papageyen zum leckeren Fra\u00df! es sey dir erlaubet!", "tokens": ["Pa\u00b7pa\u00b7ge\u00b7yen", "zum", "le\u00b7cke\u00b7ren", "Fra\u00df", "!", "es", "sey", "dir", "er\u00b7lau\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "---+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Also spottete sie des armen get\u00f6dteten Murners.", "tokens": ["Al\u00b7so", "spot\u00b7te\u00b7te", "sie", "des", "ar\u00b7men", "ge\u00b7t\u00f6d\u00b7te\u00b7ten", "Mur\u00b7ners", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "O, wie pl\u00f6tzlich \u00e4ndern sich nicht die glei\u00dfenden Reden", "tokens": ["O", ",", "wie", "pl\u00f6tz\u00b7lich", "\u00e4n\u00b7dern", "sich", "nicht", "die", "glei\u00b7\u00dfen\u00b7den", "Re\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PRF", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Eines ver\u00e4nderten Hofs, der nichts mehr f\u00fcrchtet und hoffet!", "tokens": ["Ei\u00b7nes", "ver\u00b7\u00e4n\u00b7der\u00b7ten", "Hofs", ",", "der", "nichts", "mehr", "f\u00fcrch\u00b7tet", "und", "hof\u00b7fet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Jetzt er\u00f6ffnete Lisette das Fenster; sie fasset den K\u00f6rper", "tokens": ["Jetzt", "er\u00b7\u00f6ff\u00b7ne\u00b7te", "Li\u00b7set\u00b7te", "das", "Fens\u00b7ter", ";", "sie", "fas\u00b7set", "den", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "--+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bei dem hintersten Bein, und wirft ihn zum Fenster herunter", "tokens": ["Bei", "dem", "hin\u00b7ters\u00b7ten", "Bein", ",", "und", "wirft", "ihn", "zum", "Fens\u00b7ter", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Auf den schimpflichen Mist. So st\u00fcrzten die Statuen eh'mals", "tokens": ["Auf", "den", "schimpf\u00b7li\u00b7chen", "Mist", ".", "So", "st\u00fcrz\u00b7ten", "die", "Sta\u00b7tu\u00b7en", "eh'\u00b7mals"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "KOUS"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Eines Tyrannen herab; so ward das Schrecken der R\u00f6mer,", "tokens": ["Ei\u00b7nes", "Ty\u00b7ran\u00b7nen", "her\u00b7ab", ";", "so", "ward", "das", "Schre\u00b7cken", "der", "R\u00f6\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Nun ein verst\u00fcmmelter Rumpf, in faule Kan\u00e4le geschmissen.", "tokens": ["Nun", "ein", "ver\u00b7st\u00fcm\u00b7mel\u00b7ter", "Rumpf", ",", "in", "fau\u00b7le", "Ka\u00b7n\u00e4\u00b7le", "ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Fern vom traurigen Zimmer befand sich indessen Rosaura", "tokens": ["Fern", "vom", "trau\u00b7ri\u00b7gen", "Zim\u00b7mer", "be\u00b7fand", "sich", "in\u00b7des\u00b7sen", "Ro\u00b7sau\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "NE"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bei dem g\u00fctigen Alten, der sie mit holden Gespr\u00e4chen,", "tokens": ["Bei", "dem", "g\u00fc\u00b7ti\u00b7gen", "Al\u00b7ten", ",", "der", "sie", "mit", "hol\u00b7den", "Ge\u00b7spr\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von anmuthigen Reisen in's Bad, zu tr\u00f6sten bem\u00fcht war,", "tokens": ["Von", "an\u00b7mut\u00b7hi\u00b7gen", "Rei\u00b7sen", "in's", "Bad", ",", "zu", "tr\u00f6s\u00b7ten", "be\u00b7m\u00fcht", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NE", "$,", "PTKZU", "VVINF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihr Geschenke versprach von neuen, modischen Stoffen,", "tokens": ["Ihr", "Ge\u00b7schen\u00b7ke", "ver\u00b7sprach", "von", "neu\u00b7en", ",", "mo\u00b7di\u00b7schen", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und mit Soucis, und Lila, und Dauphine sie erfreute.", "tokens": ["Und", "mit", "Sou\u00b7cis", ",", "und", "Li\u00b7la", ",", "und", "Dau\u00b7phi\u00b7ne", "sie", "er\u00b7freu\u00b7te", "