{"textgrid.poem.60645": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Adler horstete auf einem hohlen Baum,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Adler horstete auf einem hohlen Baum,", "tokens": ["Ein", "Ad\u00b7ler", "hors\u00b7te\u00b7te", "auf", "ei\u00b7nem", "hoh\u00b7len", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dessen Wurzelwerk das Haus der Bache stand,", "tokens": ["In", "des\u00b7sen", "Wur\u00b7zel\u00b7werk", "das", "Haus", "der", "Ba\u00b7che", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mitten zwischen beiden fand", "tokens": ["Und", "mit\u00b7ten", "zwi\u00b7schen", "bei\u00b7den", "fand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wilde Katze ein Quartier.", "tokens": ["Die", "wil\u00b7de", "Kat\u00b7ze", "ein", "Quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "F\u00fcr alle war gen\u00fcgend Raum,", "tokens": ["F\u00fcr", "al\u00b7le", "war", "ge\u00b7n\u00fc\u00b7gend", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Friede herrschte dort wie hier.", "tokens": ["Und", "Frie\u00b7de", "herrschte", "dort", "wie", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die Katze aber sollte bald", "tokens": ["Die", "Kat\u00b7ze", "a\u00b7ber", "soll\u00b7te", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die sch\u00f6ne Eintracht untergraben.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ein\u00b7tracht", "un\u00b7ter\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zum Adler klomm sie auf. \u00bbWir haben\u00ab,", "tokens": ["Zum", "Ad\u00b7ler", "klomm", "sie", "auf", ".", "\u00bb", "Wir", "ha\u00b7ben", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So sprach sie, \u00bbhier den schlimmsten Aufenthalt.", "tokens": ["So", "sprach", "sie", ",", "\u00bb", "hier", "den", "schlimms\u00b7ten", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Bache bringt uns arge Not:", "tokens": ["Die", "Ba\u00b7che", "bringt", "uns", "ar\u00b7ge", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Um unser Leben abzuk\u00fcrzen,", "tokens": ["Um", "un\u00b7ser", "Le\u00b7ben", "ab\u00b7zu\u00b7k\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sucht sie die Eiche umzust\u00fcrzen,", "tokens": ["Sucht", "sie", "die", "Ei\u00b7che", "um\u00b7zus\u00b7t\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Seht nur, sie w\u00fchlt uns in den Tod! \u2013", "tokens": ["Seht", "nur", ",", "sie", "w\u00fchlt", "uns", "in", "den", "Tod", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Auf unsre Kinder hat sie's abgesehen,\u00ab", "tokens": ["Auf", "uns\u00b7re", "Kin\u00b7der", "hat", "sie's", "ab\u00b7ge\u00b7se\u00b7hen", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "NE", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "So sprach die Warnerin im Gehen,", "tokens": ["So", "sprach", "die", "War\u00b7ne\u00b7rin", "im", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "\u00bbsie will sie t\u00f6ten und verzehren.\u00ab", "tokens": ["\u00bb", "sie", "will", "sie", "t\u00f6\u00b7ten", "und", "ver\u00b7zeh\u00b7ren", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und scheidend von dem hohen Ort", "tokens": ["Und", "schei\u00b7dend", "von", "dem", "ho\u00b7hen", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Lief sie in gro\u00dfer Eile fort,", "tokens": ["Lief", "sie", "in", "gro\u00b7\u00dfer", "Ei\u00b7le", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Um auch die Bache zu belehren,", "tokens": ["Um", "auch", "die", "Ba\u00b7che", "zu", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die drunten still in Wochen lag.", "tokens": ["Die", "drun\u00b7ten", "still", "in", "Wo\u00b7chen", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "\u00bbach, Nachbarin, sch\u00f6n guten Tag!\u00ab", "tokens": ["\u00bb", "ach", ",", "Nach\u00b7ba\u00b7rin", ",", "sch\u00f6n", "gu\u00b7ten", "Tag", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So sprach sie leise, \u00bbh\u00f6ret meinen Rat:", "tokens": ["So", "sprach", "sie", "lei\u00b7se", ",", "\u00bb", "h\u00f6\u00b7ret", "mei\u00b7nen", "Rat", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Von Hause fortzugehn, w\u00e4r keine kluge Tat;", "tokens": ["Von", "Hau\u00b7se", "fort\u00b7zu\u00b7gehn", ",", "w\u00e4r", "kei\u00b7ne", "klu\u00b7ge", "Tat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Adler ist auf Eure Jungen sehr erpicht!", "tokens": ["Der", "Ad\u00b7ler", "ist", "auf", "Eu\u00b7re", "Jun\u00b7gen", "sehr", "er\u00b7picht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich bitte nur, verratet nicht,", "tokens": ["Ich", "bit\u00b7te", "nur", ",", "ver\u00b7ra\u00b7tet", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Da\u00df ich es war, die Euch gewarnt.\u00ab", "tokens": ["Da\u00df", "ich", "es", "war", ",", "die", "Euch", "ge\u00b7warnt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nachdem sie beide Teile so umgarnt,", "tokens": ["Nach\u00b7dem", "sie", "bei\u00b7de", "Tei\u00b7le", "so", "um\u00b7garnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Zog sie sich in ihr Loch zur\u00fcck.", "tokens": ["Zog", "sie", "sich", "in", "ihr", "Loch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der Adler aber wagte nicht mehr auszufliegen,", "tokens": ["Der", "Ad\u00b7ler", "a\u00b7ber", "wag\u00b7te", "nicht", "mehr", "aus\u00b7zu\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und auch die Bache blieb bei ihren Jungen liegen.", "tokens": ["Und", "auch", "die", "Ba\u00b7che", "blieb", "bei", "ih\u00b7ren", "Jun\u00b7gen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Dummheit dieser beiden war der Katze Gl\u00fcck.", "tokens": ["Die", "Dumm\u00b7heit", "die\u00b7ser", "bei\u00b7den", "war", "der", "Kat\u00b7ze", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "PIAT", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie zogen vor, daheim zu bleiben,", "tokens": ["Sie", "zo\u00b7gen", "vor", ",", "da\u00b7heim", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Statt Nahrungssuche zu betreiben,", "tokens": ["Statt", "Nah\u00b7rungs\u00b7su\u00b7che", "zu", "be\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Um bei Gefahr zur Hand zu sein:", "tokens": ["Um", "bei", "Ge\u00b7fahr", "zur", "Hand", "zu", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "APPRART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der Adler f\u00fcrchtend, da\u00df die Eiche falle,", "tokens": ["Der", "Ad\u00b7ler", "f\u00fcrch\u00b7tend", ",", "da\u00df", "die", "Ei\u00b7che", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Die Bache \u00e4ngstlich vor der Adlerkralle.", "tokens": ["Die", "Ba\u00b7che", "\u00e4ngst\u00b7lich", "vor", "der", "Ad\u00b7ler\u00b7kral\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Und so verreckte Gro\u00df und Klein", "tokens": ["Und", "so", "ver\u00b7reck\u00b7te", "Gro\u00df", "und", "Klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Im Adler- und im Schweinenest.", "tokens": ["Im", "Ad\u00b7ler", "und", "im", "Schwei\u00b7ne\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Die Katze aber hielt ein reiches Schlemmerfest.", "tokens": ["Die", "Kat\u00b7ze", "a\u00b7ber", "hielt", "ein", "rei\u00b7ches", "Schlem\u00b7mer\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Arglistige Rede ist der schlimmste Feind!", "tokens": ["Arg\u00b7lis\u00b7ti\u00b7ge", "Re\u00b7de", "ist", "der", "schlimms\u00b7te", "Feind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie schmeichelt dir und klingt so wohlgemeint.", "tokens": ["Sie", "schmei\u00b7chelt", "dir", "und", "klingt", "so", "wohl\u00b7ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von allen \u00dcbeln aber, wie mir scheint,", "tokens": ["Von", "al\u00b7len", "\u00dc\u00b7beln", "a\u00b7ber", ",", "wie", "mir", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die je die B\u00fcchse der Pandora barg,", "tokens": ["Die", "je", "die", "B\u00fcch\u00b7se", "der", "Pan\u00b7do\u00b7ra", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gibt's keines, das wie sie so arg.", "tokens": ["Gibt's", "kei\u00b7nes", ",", "das", "wie", "sie", "so", "arg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "PRELS", "KOKOM", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}