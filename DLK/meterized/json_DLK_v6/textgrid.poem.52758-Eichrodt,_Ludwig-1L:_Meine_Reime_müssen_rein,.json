{"textgrid.poem.52758": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Reime m\u00fcssen rein,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Reime m\u00fcssen rein,", "tokens": ["Mei\u00b7ne", "Rei\u00b7me", "m\u00fcs\u00b7sen", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunder \u2013 lich intakt sein,", "tokens": ["Wun\u00b7der", "\u2013", "lich", "in\u00b7takt", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "ADJD", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Strophen, wenn nicht fein,", "tokens": ["Mei\u00b7ne", "Stro\u00b7phen", ",", "wenn", "nicht", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scrupul\u00f6s exact sein,", "tokens": ["Scru\u00b7pu\u00b7l\u00f6s", "e\u00b7xact", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lieber, wenn ", "tokens": ["Lie\u00b7ber", ",", "wenn"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sicher halbcontract sein;", "tokens": ["Si\u00b7cher", "halb\u00b7con\u00b7tract", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Lieber aber L\u00fcckenb\u00fc\u00dfer,", "tokens": ["Lie\u00b7ber", "a\u00b7ber", "L\u00fc\u00b7cken\u00b7b\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als zu reimen Glockengie\u00dfer;", "tokens": ["Als", "zu", "rei\u00b7men", "Glo\u00b7cken\u00b7gie\u00b7\u00dfer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieber einen Eisentbinder,", "tokens": ["Lie\u00b7ber", "ei\u00b7nen", "Ei\u00b7sent\u00b7bin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als gereimet Winter Kinder;", "tokens": ["Als", "ge\u00b7rei\u00b7met", "Win\u00b7ter", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Lieber etwa herzbeschiedlich,", "tokens": ["Lie\u00b7ber", "et\u00b7wa", "herz\u00b7be\u00b7schied\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Als gem\u00fcthlich je auf friedlich;", "tokens": ["Als", "ge\u00b7m\u00fcth\u00b7lich", "je", "auf", "fried\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Lieber manchmal Reissereimer,", "tokens": ["Lie\u00b7ber", "manch\u00b7mal", "Reis\u00b7se\u00b7rei\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gluthensch\u00e4umer,", "tokens": ["Glu\u00b7then\u00b7sch\u00e4u\u00b7mer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Dichted\u00e4umer,", "tokens": ["Dich\u00b7te\u00b7d\u00e4u\u00b7mer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Als geradaus Verseleimer;", "tokens": ["Als", "ge\u00b7ra\u00b7daus", "Ver\u00b7se\u00b7lei\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.17": {"text": "Lieber noch der leere Schein,", "tokens": ["Lie\u00b7ber", "noch", "der", "lee\u00b7re", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "L\u00e4cherlich befrackt sein,", "tokens": ["L\u00e4\u00b7cher\u00b7lich", "be\u00b7frackt", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Als verzeihlich nackt sein;", "tokens": ["Als", "ver\u00b7zeih\u00b7lich", "nackt", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Lieber zu der H\u00f6rer Pein", "tokens": ["Lie\u00b7ber", "zu", "der", "H\u00f6\u00b7rer", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Auf der Staunestelzebein-", "tokens": ["Auf", "der", "Stau\u00b7ne\u00b7stel\u00b7ze\u00b7bein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Schneiderw\u00f6rterjagd sein;", "tokens": ["Schnei\u00b7der\u00b7w\u00f6r\u00b7ter\u00b7jagd", "sein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Lieber regenwasserrein,", "tokens": ["Lie\u00b7ber", "re\u00b7gen\u00b7was\u00b7ser\u00b7rein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}