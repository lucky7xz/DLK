, "PPOSAT", "NN", "VVFIN", "$,", "KON", "KOUS", "PPER", "APPR", "ADJA", "KON", "ADJA", "TRUNC", "VVFIN", "ADV", "ADJD", "VAFIN", "$,", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,", "PWAV", "PPER", "ART", "PIAT", "APPR", "PDAT", "NN", "APPR", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,", "KON", "PAV", "ART", "TRUNC", "VVFIN", "ART", "NN", "VVINF", "VMFIN", "$.", "KON", "PWAV", "VAFIN", "APPR", "PDAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PIS", "PDAT", "ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "TRUNC", "ADJA", "NN", "VVFIN", "VAFIN", "$,", "VVINF", "VMFIN", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+-+-+-+-+-+-+-+-+-+-+--+-+-+-+-+-+-+-+-+--+--+-+---+--+-+--+-+-+-+-+-+-+-+--+-+-+-+-+-+-+-+-+-+---+-+-+--+-+-+-+-+-+--+--+-+-+-+--+--+-+-+-+-+--+--+--+--+--+--+-+-+--+--+--+-+---+-+-+-+--+-+-+-+--+-+--+-+-+--+-+-+--+--+--+-+--+-+-+-+--+--+-+-+-+--+-+--+-+--+-+-+-+--+--+-+----+-+-+-+-+--+--+-+-+--+--+-+-+-+-+-+-+--+-+--+--+--+-+--+--+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der Juda bracht die Kron, besingen und vermelden.", "tokens": ["Der", "Ju\u00b7da", "bracht", "die", "Kron", ",", "be\u00b7sin\u00b7gen", "und", "ver\u00b7mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des K\u00f6nigs, dessen Thron man nicht gnug preisen kan,", "tokens": ["Des", "K\u00f6\u00b7nigs", ",", "des\u00b7sen", "Thron", "man", "nicht", "gnug", "prei\u00b7sen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil alle Welt von dem schaut ihren Heyland an.", "tokens": ["Weil", "al\u00b7le", "Welt", "von", "dem", "schaut", "ih\u00b7ren", "Hey\u00b7land", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Aus seinem Stamme ist der Heyden Licht entstanden,", "tokens": ["Aus", "sei\u00b7nem", "Stam\u00b7me", "ist", "der", "Hey\u00b7den", "Licht", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein ewig-w\u00e4hrend Lob verbleibt in allen Landen,", "tokens": ["Sein", "e\u00b7wig\u00b7w\u00e4h\u00b7rend", "Lob", "ver\u00b7bleibt", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Nachwelt spricht von ihm, da\u00df f\u00fcr den Hirtenstab", "tokens": ["Die", "Nach\u00b7welt", "spricht", "von", "ihm", ",", "da\u00df", "f\u00fcr", "den", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der H\u00f6chste ihm den Thron und K\u00f6nigsscepter gab.", "tokens": ["Der", "H\u00f6chs\u00b7te", "ihm", "den", "Thron", "und", "K\u00f6\u00b7nigs\u00b7scep\u00b7ter", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er lie\u00df in Gottesfurcht, Geduld, Muth, G\u00fcte, blicken,", "tokens": ["Er", "lie\u00df", "in", "Got\u00b7tes\u00b7furcht", ",", "Ge\u00b7duld", ",", "Muth", ",", "G\u00fc\u00b7te", ",", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und kont in jedem Stand, sich wie er solte schicken, ", "tokens": ["Und", "kont", "in", "je\u00b7dem", "Stand", ",", "sich", "wie", "er", "sol\u00b7te", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "$,", "PRF", "PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jm Lehramt war er gro\u00df, und im Regentenstand,", "tokens": ["Jm", "Le\u00b7hramt", "war", "er", "gro\u00df", ",", "und", "im", "Re\u00b7gen\u00b7ten\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Auch Haus-Stand ordentlich, di\u00df n\u00e4hrte Volck und Land.", "tokens": ["Auch", "Haus\u00b7Stand", "or\u00b7dent\u00b7lich", ",", "di\u00df", "n\u00e4hr\u00b7te", "Volck", "und", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "$,", "PDS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.13": {"text": "Sein hoher Wundergeist macht ihn bald zum Propheten,", "tokens": ["Sein", "ho\u00b7her", "Wun\u00b7der\u00b7geist", "macht", "ihn", "bald", "zum", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und seine Lieb zu Gott zum Lehrer der Poeten.", "tokens": ["Und", "sei\u00b7ne", "Lieb", "zu", "Gott", "zum", "Leh\u00b7rer", "der", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er f\u00fchrte so sein Volck gleich als er selbst sich f\u00fchrt,", "tokens": ["Er", "f\u00fchr\u00b7te", "so", "sein", "Volck", "gleich", "als", "er", "selbst", "sich", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "KOUS", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er war des Willens Herr, drum ward auch wol regiert.", "tokens": ["Er", "war", "des", "Wil\u00b7lens", "Herr", ",", "drum", "ward", "auch", "wol", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PAV", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was s\u00e4um ich aber noch, den Nahmen f\u00fcrzutragen?", "tokens": ["Was", "s\u00e4um", "ich", "a\u00b7ber", "noch", ",", "den", "Nah\u00b7men", "f\u00fcr\u00b7zu\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der David ist es, merckt, von dem ich wollen sagen,", "tokens": ["Der", "Da\u00b7vid", "ist", "es", ",", "merckt", ",", "von", "dem", "ich", "wol\u00b7len", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "$,", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ja David ist mein Held, von dem mein schwacher Kiel,", "tokens": ["Ja", "Da\u00b7vid", "ist", "mein", "Held", ",", "von", "dem", "mein", "schwa\u00b7cher", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch aus zu stolzem Muth, anietzo schreiben will.", "tokens": ["Doch", "aus", "zu", "stol\u00b7zem", "Muth", ",", "an\u00b7iet\u00b7zo", "schrei\u00b7ben", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "ADJA", "NN", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ja wahrlich ist mein Muth hierinnen stoltz zu nennen,", "tokens": ["Ja", "wahr\u00b7lich", "ist", "mein", "Muth", "hie\u00b7rin\u00b7nen", "stoltz", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VAFIN", "PPOSAT", "NN", "PAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dieweil ich mich verge\u00df hierinn selbst zu erkennen:", "tokens": ["Die\u00b7weil", "ich", "mich", "ver\u00b7ge\u00df", "hie\u00b7rinn", "selbst", "zu", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch David macht mich k\u00fchn. Mein David war ein Knab,", "tokens": ["Doch", "Da\u00b7vid", "macht", "mich", "k\u00fchn", ".", "Mein", "Da\u00b7vid", "war", "ein", "Knab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als er dem Goliath den Sieg gewonnen ab.", "tokens": ["Als", "er", "dem", "Go\u00b7li\u00b7ath", "den", "Sieg", "ge\u00b7won\u00b7nen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was ich mich unternehm, vergleich ich mit dem Riesen,", "tokens": ["Was", "ich", "mich", "un\u00b7ter\u00b7nehm", ",", "ver\u00b7gleich", "ich", "mit", "dem", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADJD", "$,", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den zu bezwingen man zwar andre solt\u2019 erkiesen,", "tokens": ["Den", "zu", "be\u00b7zwin\u00b7gen", "man", "zwar", "and\u00b7re", "solt'", "er\u00b7kie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "PIS", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Von einer kl\u00fcgern Hand, von einem sch\u00e4rffern Sinn,", "tokens": ["Von", "ei\u00b7ner", "kl\u00fc\u00b7gern", "Hand", ",", "von", "ei\u00b7nem", "sch\u00e4rf\u00b7fern", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch unternehm ich mirs, so schwach ich immer bin.", "tokens": ["Doch", "un\u00b7ter\u00b7nehm", "ich", "mirs", ",", "so", "schwach", "ich", "im\u00b7mer", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und wann ich gleich damit nicht allen werd behagen,", "tokens": ["Und", "wann", "ich", "gleich", "da\u00b7mit", "nicht", "al\u00b7len", "werd", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PAV", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So wird in dieser Furcht mein Will doch nicht verzagen,", "tokens": ["So", "wird", "in", "die\u00b7ser", "Furcht", "mein", "Will", "doch", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN", "PPOSAT", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich singe gleichwol fort. Mein David l\u00e4\u00dft mir zu,", "tokens": ["Ich", "sin\u00b7ge", "gleich\u00b7wol", "fort", ".", "Mein", "Da\u00b7vid", "l\u00e4\u00dft", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$.", "PPOSAT", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df ich zu seinem Lob, so viel ich kan, nur thu.", "tokens": ["Da\u00df", "ich", "zu", "sei\u00b7nem", "Lob", ",", "so", "viel", "ich", "kan", ",", "nur", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich suche keine Ehr in meinen Reim-Gedichten,", "tokens": ["Ich", "su\u00b7che", "kei\u00b7ne", "Ehr", "in", "mei\u00b7nen", "Reim\u00b7Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich achte nicht, wann mich die Welt hierinn will richten,", "tokens": ["Ich", "ach\u00b7te", "nicht", ",", "wann", "mich", "die", "Welt", "hie\u00b7rinn", "will", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich sing aus freyer Lust, und nicht aus hohem Geist,", "tokens": ["Ich", "sing", "aus", "frey\u00b7er", "Lust", ",", "und", "nicht", "aus", "ho\u00b7hem", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Von David, dessen Thun mit mir ein jeder preist.", "tokens": ["Von", "Da\u00b7vid", ",", "des\u00b7sen", "Thun", "mit", "mir", "ein", "je\u00b7der", "preist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELAT", "NN", "APPR", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}