{"textgrid.poem.47779": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[lieb' und Rausch' ist ein Geheimni\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb' und Rausch' ist ein Geheimni\u00df;", "tokens": ["Lieb'", "und", "Rausch'", "ist", "ein", "Ge\u00b7heim\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille mit Erl\u00e4uterungen!", "tokens": ["Stil\u00b7le", "mit", "Er\u00b7l\u00e4u\u00b7te\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es der Menge klar zu machen,", "tokens": ["Es", "der", "Men\u00b7ge", "klar", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht gen\u00fcgten Engelzungen.", "tokens": ["Nicht", "ge\u00b7n\u00fcg\u00b7ten", "En\u00b7gel\u00b7zun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}