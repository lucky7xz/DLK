{"textgrid.poem.26784": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimmer, das glaubt mir,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer, das glaubt mir,", "tokens": ["Nim\u00b7mer", ",", "das", "glaubt", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erscheinen die G\u00f6tter,", "tokens": ["Er\u00b7schei\u00b7nen", "die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nimmer allein.", "tokens": ["Nim\u00b7mer", "al\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kaum da\u00df ich Bacchus, den lustigen, habe,", "tokens": ["Kaum", "da\u00df", "ich", "Bac\u00b7chus", ",", "den", "lus\u00b7ti\u00b7gen", ",", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NE", "$,", "ART", "ADJA", "$,", "VAFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kommt auch schon Amor, der l\u00e4chelnde Knabe,", "tokens": ["Kommt", "auch", "schon", "A\u00b7mor", ",", "der", "l\u00e4\u00b7cheln\u00b7de", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ph\u00f6bus der Herrliche findet sich ein.", "tokens": ["Ph\u00f6\u00b7bus", "der", "Herr\u00b7li\u00b7che", "fin\u00b7det", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Sie nahen, sie kommen", "tokens": ["Sie", "na\u00b7hen", ",", "sie", "kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Himmlischen alle,", "tokens": ["Die", "Himm\u00b7li\u00b7schen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mit G\u00f6ttern erf\u00fcllt sich", "tokens": ["Mit", "G\u00f6t\u00b7tern", "er\u00b7f\u00fcllt", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die irdische Halle.", "tokens": ["Die", "ir\u00b7di\u00b7sche", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sagt, wie bewirt ich,", "tokens": ["Sagt", ",", "wie", "be\u00b7wirt", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVPP", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Erdegeborne,", "tokens": ["Der", "Er\u00b7de\u00b7ge\u00b7bor\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Himmlischen Chor?", "tokens": ["Himm\u00b7li\u00b7schen", "Chor", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schenket mir euer unsterbliches Leben,", "tokens": ["Schen\u00b7ket", "mir", "eu\u00b7er", "uns\u00b7terb\u00b7li\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "G\u00f6tter! Was kann euch der Sterbliche geben?", "tokens": ["G\u00f6t\u00b7ter", "!", "Was", "kann", "euch", "der", "Sterb\u00b7li\u00b7che", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Hebet zu eurem Olymp mich empor!", "tokens": ["He\u00b7bet", "zu", "eu\u00b7rem", "O\u00b7lymp", "mich", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.7": {"text": "Die Freude, sie wohnt nur", "tokens": ["Die", "Freu\u00b7de", ",", "sie", "wohnt", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In Jupiters Saale,", "tokens": ["In", "Ju\u00b7pi\u00b7ters", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "O f\u00fcllet mit Nektar,", "tokens": ["O", "f\u00fcl\u00b7let", "mit", "Nek\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "O reicht mir die Schale!", "tokens": ["O", "reicht", "mir", "die", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Reich ihm die Schale!", "tokens": ["Reich", "ihm", "die", "Scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schenke dem Dichter,", "tokens": ["Schen\u00b7ke", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hebe, nur ein.", "tokens": ["He\u00b7be", ",", "nur", "ein", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Netz ihm die Augen mit himmlischem Taue,", "tokens": ["Netz", "ihm", "die", "Au\u00b7gen", "mit", "himm\u00b7li\u00b7schem", "Tau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Da\u00df er den Styx, den verha\u00dften, nicht schaue,", "tokens": ["Da\u00df", "er", "den", "Styx", ",", "den", "ver\u00b7ha\u00df\u00b7ten", ",", "nicht", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Einer der Unsern sich d\u00fcnke zu sein.", "tokens": ["Ei\u00b7ner", "der", "Un\u00b7sern", "sich", "d\u00fcn\u00b7ke", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PRF", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Sie rauschet, sie perlet,", "tokens": ["Sie", "rau\u00b7schet", ",", "sie", "per\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die himmlische Quelle,", "tokens": ["Die", "himm\u00b7li\u00b7sche", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der Busen wird ruhig,", "tokens": ["Der", "Bu\u00b7sen", "wird", "ru\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Das Auge wird helle.", "tokens": ["Das", "Au\u00b7ge", "wird", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}