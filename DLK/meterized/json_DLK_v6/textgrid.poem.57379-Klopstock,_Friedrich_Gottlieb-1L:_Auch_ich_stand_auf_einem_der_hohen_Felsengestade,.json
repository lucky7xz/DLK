{"textgrid.poem.57379": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch ich stand auf einem der hohen Felsengestade,", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich stand auf einem der hohen Felsengestade,", "tokens": ["Auch", "ich", "stand", "auf", "ei\u00b7nem", "der", "ho\u00b7hen", "Fel\u00b7sen\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Schauete heisstheilnehmend hinab", "tokens": ["Schau\u00b7e\u00b7te", "heisst\u00b7heil\u00b7neh\u00b7mend", "hin\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Auf die emp\u00f6rten Wogen, des donnernden Ozeans Berge,", "tokens": ["Auf", "die", "em\u00b7p\u00f6r\u00b7ten", "Wo\u00b7gen", ",", "des", "don\u00b7nern\u00b7den", "O\u00b7ze\u00b7ans", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Alle sie Spiele des Sturms,", "tokens": ["Al\u00b7le", "sie", "Spie\u00b7le", "des", "Sturms", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "In die Nacht hinab der Staatsumschaffung! Die Segler", "tokens": ["In", "die", "Nacht", "hin\u00b7ab", "der", "Staat\u00b7sum\u00b7schaf\u00b7fung", "!", "Die", "Seg\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Schwebeten, schwankten, krachten einher.", "tokens": ["Schwe\u00b7be\u00b7ten", ",", "schwank\u00b7ten", ",", "krach\u00b7ten", "ein\u00b7her", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "K\u00fchn gesteuert einher, und ohne Steuer; es wurd' oft", "tokens": ["K\u00fchn", "ge\u00b7steu\u00b7ert", "ein\u00b7her", ",", "und", "oh\u00b7ne", "Steu\u00b7er", ";", "es", "wurd'", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVPP", "PTKVZ", "$,", "KON", "APPR", "NN", "$.", "PPER", "VAFIN", "ADV"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Bleich von gestrandeten Leichen der Fels.", "tokens": ["Bleich", "von", "ge\u00b7stran\u00b7de\u00b7ten", "Lei\u00b7chen", "der", "Fels", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Selten nur wandt' ich mich um, und blickte nieder ins stille", "tokens": ["Sel\u00b7ten", "nur", "wandt'", "ich", "mich", "um", ",", "und", "blick\u00b7te", "nie\u00b7der", "ins", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "APPRART", "ADJA"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Thal, wo die Saite der Alten mir t\u00f6nt,", "tokens": ["Thal", ",", "wo", "die", "Sai\u00b7te", "der", "Al\u00b7ten", "mir", "t\u00f6nt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.11": {"text": "Und auch ich wohl den Klang der eigenen Saite behorche,", "tokens": ["Und", "auch", "ich", "wohl", "den", "Klang", "der", "ei\u00b7ge\u00b7nen", "Sai\u00b7te", "be\u00b7hor\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wenn des Griechen mich weniger schreckt.", "tokens": ["Wenn", "des", "Grie\u00b7chen", "mich", "we\u00b7ni\u00b7ger", "schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Aber m\u00fcde zu schauen den Sturm, und die scheiternden Segler,", "tokens": ["A\u00b7ber", "m\u00fc\u00b7de", "zu", "schau\u00b7en", "den", "Sturm", ",", "und", "die", "schei\u00b7tern\u00b7den", "Seg\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Kehret' ich endlich zur\u00fcck", "tokens": ["Keh\u00b7ret'", "ich", "end\u00b7lich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "In mein Thal. Tief barg ich mich nun in den Hainen Ach\u00e4a's,", "tokens": ["In", "mein", "Thal", ".", "Tief", "barg", "ich", "mich", "nun", "in", "den", "Hai\u00b7nen", "A\u00b7ch\u00e4\u00b7a's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Dass mein Ohr nicht vernahm", "tokens": ["Dass", "mein", "Ohr", "nicht", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Jenen Orkan: und verlor bis zu mir ein sterbender Laut sich;", "tokens": ["Je\u00b7nen", "Or\u00b7kan", ":", "und", "ver\u00b7lor", "bis", "zu", "mir", "ein", "ster\u00b7ben\u00b7der", "Laut", "sich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "KON", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "APPR", "PRF", "$."], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "\u00dcbersang ich ihn leicht.", "tokens": ["\u00dc\u00b7bers\u00b7ang", "ich", "ihn", "leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Oft ward dann Thuiskone von mir gerufen zum Wettstreit", "tokens": ["Oft", "ward", "dann", "Thu\u00b7is\u00b7ko\u00b7ne", "von", "mir", "ge\u00b7ru\u00b7fen", "zum", "Wett\u00b7streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "APPR", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Mit den gestorbenen, und", "tokens": ["Mit", "den", "ge\u00b7stor\u00b7be\u00b7nen", ",", "und"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "KON"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Doch unsterblichen, mit Romana, und selbst mit Hell\u00e4nis!", "tokens": ["Doch", "uns\u00b7terb\u00b7li\u00b7chen", ",", "mit", "Ro\u00b7ma\u00b7na", ",", "und", "selbst", "mit", "Hel\u00b7l\u00e4\u00b7nis", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "APPR", "NE", "$,", "KON", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Wenn Thuiskon' ich beschwor, bey der Kraft", "tokens": ["Wenn", "Thuis\u00b7kon'", "ich", "be\u00b7schwor", ",", "bey", "der", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Ihrer K\u00fcrze; dann erhub sie sich, folgte mir: andre", "tokens": ["Ih\u00b7rer", "K\u00fcr\u00b7ze", ";", "dann", "er\u00b7hub", "sie", "sich", ",", "folg\u00b7te", "mir", ":", "and\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "$.", "PIS"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Winke noch machten ihr froher den Blick.", "tokens": ["Win\u00b7ke", "noch", "mach\u00b7ten", "ihr", "fro\u00b7her", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.25": {"text": "Wirst du ihr, Galliette, des Kampfes K\u00fchnheit, und wirst du", "tokens": ["Wirst", "du", "ihr", ",", "Gal\u00b7liet\u00b7te", ",", "des", "Kamp\u00b7fes", "K\u00fchn\u00b7heit", ",", "und", "wirst", "du"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "$,", "ART", "NN", "NN", "$,", "KON", "VAFIN", "PPER"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Sie, o Ingles, verzeihn?", "tokens": ["Sie", ",", "o", "Ing\u00b7les", ",", "ver\u00b7zeihn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "FM", "$,", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Sie vielleicht; doch Eins ist au blutige Wunde, und das wird", "tokens": ["Sie", "viel\u00b7leicht", ";", "doch", "Eins", "ist", "au", "blu\u00b7ti\u00b7ge", "Wun\u00b7de", ",", "und", "das", "wird"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$.", "ADV", "NN", "VAFIN", "NE", "ADJA", "NN", "$,", "KON", "PDS", "VAFIN"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.28": {"text": "Nie vergessen, der Sieg.", "tokens": ["Nie", "ver\u00b7ges\u00b7sen", ",", "der", "Sieg", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Lasst euch bes\u00e4nftigen durch ihr Gest\u00e4ndniss, dass sie nur Keime", "tokens": ["Lasst", "euch", "be\u00b7s\u00e4nf\u00b7ti\u00b7gen", "durch", "ihr", "Ge\u00b7st\u00e4nd\u00b7niss", ",", "dass", "sie", "nur", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Weniger Lorberbl\u00e4tter errang.", "tokens": ["We\u00b7ni\u00b7ger", "Lor\u00b7ber\u00b7bl\u00e4t\u00b7ter", "er\u00b7rang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}