{"textgrid.poem.56046": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4therbst in Venedig", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun treibt die Stadt schon nicht mehr wie ein K\u00f6der,", "tokens": ["Nun", "treibt", "die", "Stadt", "schon", "nicht", "mehr", "wie", "ein", "K\u00f6\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der alle aufgetauchten Tage f\u00e4ngt.", "tokens": ["der", "al\u00b7le", "auf\u00b7ge\u00b7tauch\u00b7ten", "Ta\u00b7ge", "f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die gl\u00e4sernen Pal\u00e4ste klingen spr\u00f6der", "tokens": ["Die", "gl\u00e4\u00b7ser\u00b7nen", "Pa\u00b7l\u00e4s\u00b7te", "klin\u00b7gen", "spr\u00f6\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "an deinen Blick. Und aus den G\u00e4rten h\u00e4ngt", "tokens": ["an", "dei\u00b7nen", "Blick", ".", "Und", "aus", "den", "G\u00e4r\u00b7ten", "h\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "der Sommer wie ein Haufen Marionetten", "tokens": ["der", "Som\u00b7mer", "wie", "ein", "Hau\u00b7fen", "Ma\u00b7ri\u00b7o\u00b7net\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+----+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "kopf\u00fcber, m\u00fcde, umgebracht.", "tokens": ["kopf\u00b7\u00fc\u00b7ber", ",", "m\u00fc\u00b7de", ",", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aber vom Grund aus alten Waldskeletten", "tokens": ["A\u00b7ber", "vom", "Grund", "aus", "al\u00b7ten", "Walds\u00b7ke\u00b7let\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "steigt Willen auf: als sollte \u00fcber Nacht", "tokens": ["steigt", "Wil\u00b7len", "auf", ":", "als", "soll\u00b7te", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "$.", "KOUS", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "der General des Meeres die Galeeren", "tokens": ["der", "Ge\u00b7ne\u00b7ral", "des", "Mee\u00b7res", "die", "Ga\u00b7lee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "verdoppeln in dem wachen Arsenal,", "tokens": ["ver\u00b7dop\u00b7peln", "in", "dem", "wa\u00b7chen", "Ar\u00b7se\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "um schon die n\u00e4chste Morgenluft zu teeren", "tokens": ["um", "schon", "die", "n\u00e4chs\u00b7te", "Mor\u00b7gen\u00b7luft", "zu", "tee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "mit einer Flotte, welche ruderschlagend", "tokens": ["mit", "ei\u00b7ner", "Flot\u00b7te", ",", "wel\u00b7che", "ru\u00b7der\u00b7schla\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sich dr\u00e4ngt und j\u00e4h, mit allen Flaggen tagend,", "tokens": ["sich", "dr\u00e4ngt", "und", "j\u00e4h", ",", "mit", "al\u00b7len", "Flag\u00b7gen", "ta\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "ADJD", "$,", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den gro\u00dfen Wind hat, strahlend und fatal.", "tokens": ["den", "gro\u00b7\u00dfen", "Wind", "hat", ",", "strah\u00b7lend", "und", "fa\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}