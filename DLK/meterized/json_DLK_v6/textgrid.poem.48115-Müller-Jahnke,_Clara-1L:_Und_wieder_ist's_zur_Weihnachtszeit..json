{"textgrid.poem.48115": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wieder ist's zur Weihnachtszeit.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder ist's zur Weihnachtszeit.", "tokens": ["Und", "wie\u00b7der", "ist's", "zur", "Weih\u00b7nachts\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch meine Seele schleicht ein Traum", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "schleicht", "ein", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von wundersamer Herrlichkeit,", "tokens": ["von", "wun\u00b7der\u00b7sa\u00b7mer", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vom goldumstrahlten Lichterbaum.", "tokens": ["vom", "gold\u00b7ums\u00b7trahl\u00b7ten", "Lich\u00b7ter\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kindheit Zauber spinnt mich ein,", "tokens": ["Der", "Kind\u00b7heit", "Zau\u00b7ber", "spinnt", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit seiner T\u00f6ne s\u00fc\u00dfer Pracht", "tokens": ["mit", "sei\u00b7ner", "T\u00f6\u00b7ne", "s\u00fc\u00b7\u00dfer", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "umflutet mich das Jubellied:", "tokens": ["um\u00b7flu\u00b7tet", "mich", "das", "Ju\u00b7bel\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdies ist der Tag, den Gott gemacht!\u00ab", "tokens": ["\u00bb", "dies", "ist", "der", "Tag", ",", "den", "Gott", "ge\u00b7macht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbden Gott gemacht\u00ab \u2013: ein Glockenton", "tokens": ["\u00bb", "den", "Gott", "ge\u00b7macht", "\u00ab", "\u2013", ":", "ein", "Glo\u00b7cken\u00b7ton"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "VVPP", "$(", "$(", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durchirrt die L\u00fcfte weich und lieb \u2013", "tokens": ["durch\u00b7irrt", "die", "L\u00fcf\u00b7te", "weich", "und", "lieb", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbden Gott gemacht\u00ab \u2013: ein Sturmwind pfeift", "tokens": ["\u00bb", "den", "Gott", "ge\u00b7macht", "\u00ab", "\u2013", ":", "ein", "Sturm\u00b7wind", "pfeift"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVPP", "$(", "$(", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch Winternacht wie Schwerterhieb.", "tokens": ["durch", "Win\u00b7ter\u00b7nacht", "wie", "Schwer\u00b7ter\u00b7hieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbden Gott gemacht\u00ab \u2013: ein Lachen gellt", "tokens": ["\u00bb", "den", "Gott", "ge\u00b7macht", "\u00ab", "\u2013", ":", "ein", "La\u00b7chen", "gellt"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVPP", "$(", "$(", "$.", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "durch all die Lust und schluchzt und weint . . .", "tokens": ["durch", "all", "die", "Lust", "und", "schluchzt", "und", "weint", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "aus einer H\u00fctte komm ich her,", "tokens": ["aus", "ei\u00b7ner", "H\u00fct\u00b7te", "komm", "ich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "in die kein Strahl der Gnade scheint.", "tokens": ["in", "die", "kein", "Strahl", "der", "Gna\u00b7de", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Eisblumen bl\u00fchn am Fensterglas,", "tokens": ["Eis\u00b7blu\u00b7men", "bl\u00fchn", "am", "Fens\u00b7ter\u00b7glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die W\u00e4nde glitzernd, feucht, beru\u00dft;", "tokens": ["die", "W\u00e4n\u00b7de", "glit\u00b7zernd", ",", "feucht", ",", "be\u00b7ru\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auf d\u00fcrftigem Stroh ein sieches Weib,", "tokens": ["auf", "d\u00fcrf\u00b7ti\u00b7gem", "Stroh", "ein", "sie\u00b7ches", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "das Kind an abgezehrter Brust;", "tokens": ["das", "Kind", "an", "ab\u00b7ge\u00b7zehr\u00b7ter", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der Mann auf harter Ofenbank", "tokens": ["der", "Mann", "auf", "har\u00b7ter", "O\u00b7fen\u00b7bank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit stierem Blick, mit dumpfen Sinn . . .", "tokens": ["mit", "stie\u00b7rem", "Blick", ",", "mit", "dum\u00b7pfen", "Sinn", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die Liebe, die sie einst verband,", "tokens": ["die", "Lie\u00b7be", ",", "die", "sie", "einst", "ver\u00b7band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "im Elend starb sie l\u00e4ngst dahin.", "tokens": ["im", "E\u00b7lend", "starb", "sie", "l\u00e4ngst", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Elend starb sie, wie das Paar", "tokens": ["Im", "E\u00b7lend", "starb", "sie", ",", "wie", "das", "Paar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von Menschenknospen, jung und frisch,", "tokens": ["von", "Men\u00b7schen\u00b7knos\u00b7pen", ",", "jung", "und", "frisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "das M\u00e4del, braun und tannenschlank,", "tokens": ["das", "M\u00e4\u00b7del", ",", "braun", "und", "tan\u00b7nen\u00b7schlank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Knabe, blond und tr\u00e4umerisch,", "tokens": ["der", "Kna\u00b7be", ",", "blond", "und", "tr\u00e4u\u00b7me\u00b7risch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wie jauchzten sie zur Sommerzeit!", "tokens": ["wie", "jauchz\u00b7ten", "sie", "zur", "Som\u00b7mer\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie senkten sie die K\u00f6pflein m\u00fcd,", "tokens": ["Wie", "senk\u00b7ten", "sie", "die", "K\u00f6\u00b7pflein", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "als in des Winters harter Not", "tokens": ["als", "in", "des", "Win\u00b7ters", "har\u00b7ter", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ihr Lebensfl\u00e4mmchen matt vergl\u00fcht . . .", "tokens": ["ihr", "Le\u00b7bens\u00b7fl\u00e4mm\u00b7chen", "matt", "ver\u00b7gl\u00fcht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und gestern trug man sie hinaus", "tokens": ["Und", "ge\u00b7stern", "trug", "man", "sie", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im schwarzen Sarg aufs wei\u00dfe Feld;", "tokens": ["im", "schwar\u00b7zen", "Sarg", "aufs", "wei\u00b7\u00dfe", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "kein Strahl der Liebe leuchtet mehr", "tokens": ["kein", "Strahl", "der", "Lie\u00b7be", "leuch\u00b7tet", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in dieses Jammers enge Welt.", "tokens": ["in", "die\u00b7ses", "Jam\u00b7mers", "en\u00b7ge", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 Und dr\u00fcben blitzt im Herrenschlo\u00df", "tokens": ["\u2013", "Und", "dr\u00fc\u00b7ben", "blitzt", "im", "Her\u00b7ren\u00b7schlo\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das Lichtmeer auf, die Weihnachtspracht,", "tokens": ["das", "Licht\u00b7meer", "auf", ",", "die", "Weih\u00b7nachts\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und brausend klingt das Jubellied:", "tokens": ["und", "brau\u00b7send", "klingt", "das", "Ju\u00b7bel\u00b7lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbdies ist der Tag, den Gott gemacht!\u00ab", "tokens": ["\u00bb", "dies", "ist", "der", "Tag", ",", "den", "Gott", "ge\u00b7macht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zwei Rosenknospen welkten hin", "tokens": ["Zwei", "Ro\u00b7sen\u00b7knos\u00b7pen", "welk\u00b7ten", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und starben in des Winters Bann,", "tokens": ["und", "star\u00b7ben", "in", "des", "Win\u00b7ters", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die dritte seufzt nach Lust und Licht,", "tokens": ["die", "drit\u00b7te", "seufzt", "nach", "Lust", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df sie zum Lenz erbl\u00fchen kann.", "tokens": ["da\u00df", "sie", "zum", "Lenz", "er\u00b7bl\u00fc\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der neue Heiland geht und weint", "tokens": ["Der", "neu\u00b7e", "Hei\u00b7land", "geht", "und", "weint"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und findet keiner Krippe Raum . . .", "tokens": ["und", "fin\u00b7det", "kei\u00b7ner", "Krip\u00b7pe", "Raum", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann graust du, Tag, den Gott gemacht?", "tokens": ["Wann", "graust", "du", ",", "Tag", ",", "den", "Gott", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann wirst du, Wahrheit, Weihnachtstraum?", "tokens": ["Wann", "wirst", "du", ",", "Wahr\u00b7heit", ",", "Weih\u00b7nachts\u00b7traum", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}