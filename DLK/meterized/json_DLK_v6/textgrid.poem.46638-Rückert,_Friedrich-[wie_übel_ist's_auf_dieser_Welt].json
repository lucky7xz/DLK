{"textgrid.poem.46638": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie \u00fcbel ist's auf dieser Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00fcbel ist's auf dieser Welt", "tokens": ["Wie", "\u00fc\u00b7bel", "ist's", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu leben und zu sterben!", "tokens": ["Zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du kannst, wie es dir wohlgef\u00e4llt,", "tokens": ["Du", "kannst", ",", "wie", "es", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht dies noch das erwerben.", "tokens": ["Nicht", "dies", "noch", "das", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "ADV", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie Abraham in Mamres Hain", "tokens": ["Wie", "Ab\u00b7ra\u00b7ham", "in", "Mam\u00b7res", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begraben seine Todten,", "tokens": ["Be\u00b7gra\u00b7ben", "sei\u00b7ne", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So solltest du mir begraben sein,", "tokens": ["So", "soll\u00b7test", "du", "mir", "be\u00b7gra\u00b7ben", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch das ist mir verboten.", "tokens": ["Doch", "das", "ist", "mir", "ver\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie nur sie wittern, da\u00df im Haus", "tokens": ["Wie", "nur", "sie", "wit\u00b7tern", ",", "da\u00df", "im", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir einen Todten haben,", "tokens": ["Wir", "ei\u00b7nen", "Tod\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich st\u00fcrzen sich als wie zum Schmaus", "tokens": ["Gleich", "st\u00fcr\u00b7zen", "sich", "als", "wie", "zum", "Schmaus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "KOUS", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcber die schwarzen Raben.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "die", "schwar\u00b7zen", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "O da\u00df du h\u00e4ttest den sch\u00f6nen Leib", "tokens": ["O", "da\u00df", "du", "h\u00e4t\u00b7test", "den", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Auf deiner Seele Fl\u00fcgeln", "tokens": ["Auf", "dei\u00b7ner", "See\u00b7le", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mitnehmen k\u00f6nnen, da\u00df Mann noch Weib", "tokens": ["Mit\u00b7neh\u00b7men", "k\u00f6n\u00b7nen", ",", "da\u00df", "Mann", "noch", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$,", "KOUS", "NN", "ADV", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht d\u00fcrft' an dir mir kl\u00fcgeln.", "tokens": ["Nicht", "d\u00fcrft'", "an", "dir", "mir", "kl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein Todtenantlitz, das mich gelabt,", "tokens": ["Dein", "Tod\u00b7ten\u00b7ant\u00b7litz", ",", "das", "mich", "ge\u00b7labt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mag ich nun nicht mehr schauen,", "tokens": ["Mag", "ich", "nun", "nicht", "mehr", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit dich unter den H\u00e4nden gehabt", "tokens": ["Seit", "dich", "un\u00b7ter", "den", "H\u00e4n\u00b7den", "ge\u00b7habt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ART", "NN", "VAPP"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gespenstige Leichenfrauen.", "tokens": ["Ge\u00b7spens\u00b7ti\u00b7ge", "Lei\u00b7chen\u00b7frau\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}