{"textgrid.poem.66662": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Basta! Die literarischen Gespenster", "tokens": ["Bas\u00b7ta", "!", "Die", "li\u00b7te\u00b7ra\u00b7ri\u00b7schen", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Blas' ich hinweg mit einem leichten Hauch.", "tokens": ["Blas'", "ich", "hin\u00b7weg", "mit", "ei\u00b7nem", "leich\u00b7ten", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die erste Rose gr\u00fc\u00dft mich heut vom Fenster,", "tokens": ["Die", "ers\u00b7te", "Ro\u00b7se", "gr\u00fc\u00dft", "mich", "heut", "vom", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit hundert Knospen rankt empor der Strauch.", "tokens": ["Mit", "hun\u00b7dert", "Knos\u00b7pen", "rankt", "em\u00b7por", "der", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald bl\u00fctenvoll in Herrlichkeit umkr\u00e4nzt er", "tokens": ["Bald", "bl\u00fc\u00b7ten\u00b7voll", "in", "Herr\u00b7lich\u00b7keit", "um\u00b7kr\u00e4nzt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir die Veranda ... Eintagsruhm ist Rauch ...", "tokens": ["Mir", "die", "Ver\u00b7an\u00b7da", "...", "Ein\u00b7tags\u00b7ruhm", "ist", "Rauch", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$(", "NN", "VAFIN", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Ich habe nachgedacht. Es ist zum Lachen.", "tokens": ["Ich", "ha\u00b7be", "nach\u00b7ge\u00b7dacht", ".", "Es", "ist", "zum", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich kann mir nichts mehr aus der Posse machen.", "tokens": ["Ich", "kann", "mir", "nichts", "mehr", "aus", "der", "Pos\u00b7se", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und w\u00fcrden tausend Zeitungen mich feiern,", "tokens": ["Und", "w\u00fcr\u00b7den", "tau\u00b7send", "Zei\u00b7tun\u00b7gen", "mich", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "CARD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und winkte mir der rauschendste Gewinn,", "tokens": ["Und", "wink\u00b7te", "mir", "der", "rau\u00b7schends\u00b7te", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ich gebe tausend hohle Ruhmesleiern", "tokens": ["Ich", "ge\u00b7be", "tau\u00b7send", "hoh\u00b7le", "Ruh\u00b7mes\u00b7lei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr diese eine dunkle Rose hin.", "tokens": ["F\u00fcr", "die\u00b7se", "ei\u00b7ne", "dunk\u00b7le", "Ro\u00b7se", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Steckt denn im Kopf von M\u00fcllern oder Meiern", "tokens": ["Steckt", "denn", "im", "Kopf", "von", "M\u00fcl\u00b7lern", "o\u00b7der", "Mei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur der geringste Unterscheidungssinn?", "tokens": ["Nur", "der", "ge\u00b7rings\u00b7te", "Un\u00b7ter\u00b7schei\u00b7dungs\u00b7sinn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich zweifle, seh ich \u00fcberall im Leben", "tokens": ["Ich", "zweif\u00b7le", ",", "seh", "ich", "\u00fc\u00b7be\u00b7rall", "im", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Blick doch an der Etikette kleben.", "tokens": ["Den", "Blick", "doch", "an", "der", "E\u00b7ti\u00b7ket\u00b7te", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum sei gescheit und freue dich der Rosen!", "tokens": ["Drum", "sei", "ge\u00b7scheit", "und", "freu\u00b7e", "dich", "der", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Rosenheim auch deine Seele stammt.", "tokens": ["Aus", "Ro\u00b7sen\u00b7heim", "auch", "dei\u00b7ne", "See\u00b7le", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die frischlebendigen Morgenl\u00fcfte kosen", "tokens": ["Die", "frischle\u00b7ben\u00b7di\u00b7gen", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihrer jungen Sch\u00f6nheit Duft und Samt.", "tokens": ["Um", "ih\u00b7rer", "jun\u00b7gen", "Sch\u00f6n\u00b7heit", "Duft", "und", "Samt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier ist dein Reich. Hier gibt es keine Posen,", "tokens": ["Hier", "ist", "dein", "Reich", ".", "Hier", "gibt", "es", "kei\u00b7ne", "Po\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hier kannst du feiern, was dein Herz entflammt,", "tokens": ["Hier", "kannst", "du", "fei\u00b7ern", ",", "was", "dein", "Herz", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier bist du frei von Kameraderien", "tokens": ["Hier", "bist", "du", "frei", "von", "Ka\u00b7me\u00b7ra\u00b7de\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und kannst an deinen Hausalt\u00e4ren knien.", "tokens": ["Und", "kannst", "an", "dei\u00b7nen", "Haus\u00b7al\u00b7t\u00e4\u00b7ren", "kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die R\u00f6mer nannten solch ein Ding Penaten,", "tokens": ["Die", "R\u00f6\u00b7mer", "nann\u00b7ten", "solch", "ein", "Ding", "Pe\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was keine G\u00f6tter der Boheme sind;", "tokens": ["Was", "kei\u00b7ne", "G\u00f6t\u00b7ter", "der", "Bo\u00b7he\u00b7me", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es waren des Logis intimste Paten", "tokens": ["Es", "wa\u00b7ren", "des", "Lo\u00b7gis", "in\u00b7tims\u00b7te", "Pa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und steckten eigentlich in Stuhl und Spind.", "tokens": ["Und", "steck\u00b7ten", "ei\u00b7gent\u00b7lich", "in", "Stuhl", "und", "Spind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn nach den \u00f6ffentlichen Heldentaten", "tokens": ["Wenn", "nach", "den", "\u00f6f\u00b7fent\u00b7li\u00b7chen", "Hel\u00b7den\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+--++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sich die Quiriten Haus und Ingesind", "tokens": ["Sich", "die", "Qui\u00b7ri\u00b7ten", "Haus", "und", "In\u00b7ge\u00b7sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Recht ", "tokens": ["Recht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Dann sp\u00fcrten sie, was sie Penaten hie\u00dfen.", "tokens": ["Dann", "sp\u00fcr\u00b7ten", "sie", ",", "was", "sie", "Pe\u00b7na\u00b7ten", "hie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So bet' auch ich zu meines Hauses G\u00f6ttern,", "tokens": ["So", "bet'", "auch", "ich", "zu", "mei\u00b7nes", "Hau\u00b7ses", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und frische Blumen in die Schalen f\u00fcllt", "tokens": ["Und", "fri\u00b7sche", "Blu\u00b7men", "in", "die", "Scha\u00b7len", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Weib zum Opfer t\u00e4glich. Giftigen Sp\u00f6ttern", "tokens": ["Mein", "Weib", "zum", "Op\u00b7fer", "t\u00e4g\u00b7lich", ".", "Gif\u00b7ti\u00b7gen", "Sp\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ADJD", "$.", "NN", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bleibt diese Welt verschlossen und verh\u00fcllt.", "tokens": ["Bleibt", "die\u00b7se", "Welt", "ver\u00b7schlos\u00b7sen", "und", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gebell von Gassenhunden und Hundsf\u00f6ttern", "tokens": ["Ge\u00b7bell", "von", "Gas\u00b7sen\u00b7hun\u00b7den", "und", "Hunds\u00b7f\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "St\u00f6rt nicht mein Ohr, und ob der P\u00f6bel br\u00fcllt", "tokens": ["St\u00f6rt", "nicht", "mein", "Ohr", ",", "und", "ob", "der", "P\u00f6\u00b7bel", "br\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beifall den G\u00f6tzen, lebenden und toten,", "tokens": ["Bei\u00b7fall", "den", "G\u00f6t\u00b7zen", ",", "le\u00b7ben\u00b7den", "und", "to\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Hier ist das Br\u00fcllen wenigstens verboten.", "tokens": ["Hier", "ist", "das", "Br\u00fcl\u00b7len", "we\u00b7nigs\u00b7tens", "ver\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Denn Helden w\u00e4hl' ich mir aus anderm Schnitte,", "tokens": ["Denn", "Hel\u00b7den", "w\u00e4hl'", "ich", "mir", "aus", "an\u00b7derm", "Schnit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sie die \u00f6ffentliche Meinung sch\u00e4tzt,", "tokens": ["Als", "sie", "die", "\u00f6f\u00b7fent\u00b7li\u00b7che", "Mei\u00b7nung", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "(\u00bbDie \u00f6ffentliche Meinung, aber bitte!", "tokens": ["(", "\u00bb", "Die", "\u00f6f\u00b7fent\u00b7li\u00b7che", "Mei\u00b7nung", ",", "a\u00b7ber", "bit\u00b7te", "!"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "ART", "ADJA", "NN", "$,", "ADV", "PTKANT", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Was sagen Sie? Wer dieses Weib verletzt!\u00ab)", "tokens": ["Was", "sa\u00b7gen", "Sie", "?", "Wer", "die\u00b7ses", "Weib", "ver\u00b7letzt", "!", "\u00ab", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PWS", "PDAT", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein Sinn hat seine eigene Kultussitte,", "tokens": ["Mein", "Sinn", "hat", "sei\u00b7ne", "ei\u00b7ge\u00b7ne", "Kul\u00b7tus\u00b7sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die nicht nach jedermanns Geschmacke schw\u00e4tzt.", "tokens": ["Die", "nicht", "nach", "je\u00b7der\u00b7manns", "Ge\u00b7schma\u00b7cke", "schw\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So zieh ich vor den plumpen Faustpanduren", "tokens": ["So", "zieh", "ich", "vor", "den", "plum\u00b7pen", "Faust\u00b7pan\u00b7du\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die feineren, die faustischen Naturen.", "tokens": ["Die", "fei\u00b7ne\u00b7ren", ",", "die", "faus\u00b7ti\u00b7schen", "Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich schw\u00e4rme nicht f\u00fcr jede Machtentfaltung", "tokens": ["Ich", "schw\u00e4r\u00b7me", "nicht", "f\u00fcr", "je\u00b7de", "Mach\u00b7tent\u00b7fal\u00b7tung"], "token_info": ["word", "word", "word", "word