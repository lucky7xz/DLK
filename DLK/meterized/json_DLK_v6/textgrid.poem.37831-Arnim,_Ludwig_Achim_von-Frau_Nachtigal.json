{"textgrid.poem.37831": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Frau Nachtigal", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachtigal ich h\u00f6r dich singen,", "tokens": ["Nach\u00b7ti\u00b7gal", "ich", "h\u00f6r", "dich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Herz m\u00f6cht mir im Leib zerspringen,", "tokens": ["Das", "Herz", "m\u00f6cht", "mir", "im", "Leib", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komme doch und sag mir bald,", "tokens": ["Kom\u00b7me", "doch", "und", "sag", "mir", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich mich verhalten soll.", "tokens": ["Wie", "ich", "mich", "ver\u00b7hal\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nachtigal ich seh dich laufen,", "tokens": ["Nach\u00b7ti\u00b7gal", "ich", "seh", "dich", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem B\u00e4chlein thust du saufen,", "tokens": ["An", "dem", "B\u00e4ch\u00b7lein", "thust", "du", "sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du tunkst dein klein Schn\u00e4blein ein,", "tokens": ["Du", "tunkst", "dein", "klein", "Schn\u00e4b\u00b7lein", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinst es w\u00e4r der beste Wein.", "tokens": ["Meinst", "es", "w\u00e4r", "der", "bes\u00b7te", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nachtigal wo ist gut wohnen,", "tokens": ["Nach\u00b7ti\u00b7gal", "wo", "ist", "gut", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Linden, in den Kronen,", "tokens": ["Auf", "den", "Lin\u00b7den", ",", "in", "den", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei der sch\u00f6n Frau Nachtigal,", "tokens": ["Bei", "der", "sch\u00f6n", "Frau", "Nach\u00b7ti\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df mein Sch\u00e4tzchen tausendmal.", "tokens": ["Gr\u00fc\u00df", "mein", "Sch\u00e4tz\u00b7chen", "tau\u00b7send\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}