{"textgrid.poem.49344": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Tauflied", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welch ein unvergleichlichs Gut", "tokens": ["O", "welch", "ein", "un\u00b7ver\u00b7gleich\u00b7lichs", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibst du, Herr, deinen Kindern:", "tokens": ["Gibst", "du", ",", "Herr", ",", "dei\u00b7nen", "Kin\u00b7dern", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser und zugleich dein Blut", "tokens": ["Das", "Was\u00b7ser", "und", "zu\u00b7gleich", "dein", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verehrest du den S\u00fcndern!", "tokens": ["Ver\u00b7eh\u00b7rest", "du", "den", "S\u00fcn\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drei Dinge sind, welch' allermeist", "tokens": ["Drei", "Din\u00b7ge", "sind", ",", "welch'", "al\u00b7ler\u00b7meist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Erden Zeugnis geben:", "tokens": ["Auf", "Er\u00b7den", "Zeug\u00b7nis", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Blut, das Wasser und der Geist.", "tokens": ["Das", "Blut", ",", "das", "Was\u00b7ser", "und", "der", "Geist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die k\u00f6nnen uns erheben", "tokens": ["Die", "k\u00f6n\u00b7nen", "uns", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu deinem Freudenleben.", "tokens": ["Zu", "dei\u00b7nem", "Freu\u00b7den\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die\u00df Sacrament ist selbst durch dich", "tokens": ["Die\u00df", "Sa\u00b7cra\u00b7ment", "ist", "selbst", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Geheiligt und beschlossen,", "tokens": ["Ge\u00b7hei\u00b7ligt", "und", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df wie du, Herr, bist sichtbarlich", "tokens": ["Da\u00df", "wie", "du", ",", "Herr", ",", "bist", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "KOKOM", "PPER", "$,", "NN", "$,", "VAFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit Wasser ganz begossen", "tokens": ["Mit", "Was\u00b7ser", "ganz", "be\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Im Jordan durch Johannes Hand:", "tokens": ["Im", "Jor\u00b7dan", "durch", "Jo\u00b7han\u00b7nes", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So sol auch uns rein machen", "tokens": ["So", "sol", "auch", "uns", "rein", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Dein heiligs Blut, das theure Pfand,", "tokens": ["Dein", "hei\u00b7ligs", "Blut", ",", "das", "theu\u00b7re", "Pfand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das lauter Himmelssachen", "tokens": ["Das", "lau\u00b7ter", "Him\u00b7mels\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Kan w\u00fcrken in uns Schwachen.", "tokens": ["Kan", "w\u00fcr\u00b7ken", "in", "uns", "Schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Du hast uns durch die\u00df Sacrament", "tokens": ["Du", "hast", "uns", "durch", "die\u00df", "Sa\u00b7cra\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Kirchen inverleibet,", "tokens": ["Der", "Kir\u00b7chen", "in\u00b7ver\u00b7lei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Also, da\u00df man uns Christen nennt", "tokens": ["Al\u00b7so", ",", "da\u00df", "man", "uns", "Chris\u00b7ten", "nennt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "PPER", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Und in dein Buch itz schreibet;", "tokens": ["Und", "in", "dein", "Buch", "itz", "schrei\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die\u00df Wasserbad hat uns im Wort", "tokens": ["Die\u00df", "Was\u00b7ser\u00b7bad", "hat", "uns", "im", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Auch rein gemacht von S\u00fcnden.", "tokens": ["Auch", "rein", "ge\u00b7macht", "von", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Dein guter Geist der wol' hinfort", "tokens": ["Dein", "gu\u00b7ter", "Geist", "der", "wol'", "hin\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Herzen recht entz\u00fcnden", "tokens": ["Die", "Her\u00b7zen", "recht", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und Lieb' in ihnen gr\u00fcnden!", "tokens": ["Und", "Lieb'", "in", "ih\u00b7nen", "gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wir sind, Herr, in dein Gnadenreich", "tokens": ["Wir", "sind", ",", "Herr", ",", "in", "dein", "Gna\u00b7den\u00b7reich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Durch diesen Bund gesetzet,", "tokens": ["Durch", "die\u00b7sen", "Bund", "ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Der uns an Leib und Seel' zugleich", "tokens": ["Der", "uns", "an", "Leib", "und", "Seel'", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Recht inniglich ergetzet;", "tokens": ["Recht", "in\u00b7nig\u00b7lich", "er\u00b7get\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.32": {"text": "Du hast uns durch die\u00df reine Bad", "tokens": ["Du", "hast", "uns", "durch", "die\u00df", "rei\u00b7ne", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDS", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "So trefflich sch\u00f6n bekleidet,", "tokens": ["So", "treff\u00b7lich", "sch\u00f6n", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Da\u00df auch hinfort von deiner Gnad'", "tokens": ["Da\u00df", "auch", "hin\u00b7fort", "von", "dei\u00b7ner", "Gnad'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Uns selbst der Tod nicht scheidet", "tokens": ["Uns", "selbst", "der", "Tod", "nicht", "schei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Noch alles, was uns neidet.", "tokens": ["Noch", "al\u00b7les", ",", "was", "uns", "nei\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Aus H\u00f6llenkindern sind wir schon", "tokens": ["Aus", "H\u00f6l\u00b7len\u00b7kin\u00b7dern", "sind", "wir", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Der Gnaden Kinder worden;", "tokens": ["Der", "Gna\u00b7den", "Kin\u00b7der", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Die\u00df ist der Christen sch\u00f6nste Kron'", "tokens": ["Die\u00df", "ist", "der", "Chris\u00b7ten", "sch\u00f6ns\u00b7te", "Kron'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und Schmuck in ihrem Orden.", "tokens": ["Und", "Schmuck", "in", "ih\u00b7rem", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Ja, Christus selber und sein Blut,", "tokens": ["Ja", ",", "Chris\u00b7tus", "sel\u00b7ber", "und", "sein", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "ADV", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Sein Tod und Sieg daneben,", "tokens": ["Sein", "Tod", "und", "Sieg", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Ist nunmehr unser eignes Gut,", "tokens": ["Ist", "nun\u00b7mehr", "un\u00b7ser", "eig\u00b7nes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Das er uns hat gegeben,", "tokens": ["Das", "er", "uns", "hat", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Mit ihm dadurch zu leben.", "tokens": ["Mit", "ihm", "da\u00b7durch", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Er hat uns auch das Kindesrecht", "tokens": ["Er", "hat", "uns", "auch", "das", "Kin\u00b7des\u00b7recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Der Seligkeit geschenket;", "tokens": ["Der", "Se\u00b7lig\u00b7keit", "ge\u00b7schen\u00b7ket", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Durch solches ist die S\u00fcnde schlecht", "tokens": ["Durch", "sol\u00b7ches", "ist", "die", "S\u00fcn\u00b7de", "schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ins tiefe Meer versenket.", "tokens": ["Ins", "tie\u00b7fe", "Meer", "ver\u00b7sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Was k\u00f6nnen Teufel, H\u00f6lle, Tod,", "tokens": ["Was", "k\u00f6n\u00b7nen", "Teu\u00b7fel", ",", "H\u00f6l\u00b7le", ",", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Welch' uns stets widerstunden,", "tokens": ["Welch'", "uns", "stets", "wi\u00b7der\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Weil Jesus Christus alle Not", "tokens": ["Weil", "Je\u00b7sus", "Chris\u00b7tus", "al\u00b7le", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Samt ihnen \u00fcberwunden?", "tokens": ["Samt", "ih\u00b7nen", "\u00fc\u00b7berw\u00b7un\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Nun ist das Heil gefunden!", "tokens": ["Nun", "ist", "das", "Heil", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Herr, la\u00df uns doch, den Reben gleich,", "tokens": ["Herr", ",", "la\u00df", "uns", "doch", ",", "den", "Re\u00b7ben", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Auch gute Fr\u00fcchte bringen", "tokens": ["Auch", "gu\u00b7te", "Fr\u00fcch\u00b7te", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Und aus der Welt nach deinem Reich'", "tokens": ["Und", "aus", "der", "Welt", "nach", "dei\u00b7nem", "Reich'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Im Glauben eifrig ringen;", "tokens": ["Im", "Glau\u00b7ben", "eif\u00b7rig", "rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "La\u00df uns durch wahre Reu' und Bu\u00df'", "tokens": ["La\u00df", "uns", "durch", "wah\u00b7re", "Reu'", "und", "Bu\u00df'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Auch t\u00e4glich mit dir sterben,", "tokens": ["Auch", "t\u00e4g\u00b7lich", "mit", "dir", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Demnach der alte Adam mu\u00df", "tokens": ["Dem\u00b7nach", "der", "al\u00b7te", "A\u00b7dam", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Bis auf den Grund verderben,", "tokens": ["Bis", "auf", "den", "Grund", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Sol man dein Reich erwerben.", "tokens": ["Sol", "man", "dein", "Reich", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Hilf, da\u00df wir diesen Gnadenbund", "tokens": ["Hilf", ",", "da\u00df", "wir", "die\u00b7sen", "Gna\u00b7den\u00b7bund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.65": {"text": "Der Taufe nie vergessen,", "tokens": ["Der", "Tau\u00b7fe", "nie", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "Und sich kein freches Herz noch Mund", "tokens": ["Und", "sich", "kein", "fre\u00b7ches", "Herz", "noch", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "PIAT", "ADJA", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Zu schm\u00e4hen ihn vermessen;", "tokens": ["Zu", "schm\u00e4\u00b7hen", "ihn", "ver\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Die Taufe mu\u00df in Angst und Pein,", "tokens": ["Die", "Tau\u00b7fe", "mu\u00df", "in", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.69": {"text": "Ja, wenn wir gehn von hinnen,", "tokens": ["Ja", ",", "wenn", "wir", "gehn", "von", "hin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "Herr, unser Trost und Freude sein;", "tokens": ["Herr", ",", "un\u00b7ser", "Trost", "und", "Freu\u00b7de", "sein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.71": {"text": "Das hei\u00dft der Welt entrinnen,", "tokens": ["Das", "hei\u00dft", "der", "Welt", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "Den Himmel zu gewinnen.", "tokens": ["Den", "Him\u00b7mel", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}