{"textgrid.poem.66913": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gen Himmel reckt das Kanonenrohr", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gen Himmel reckt das Kanonenrohr", "tokens": ["Gen", "Him\u00b7mel", "reckt", "das", "Ka\u00b7no\u00b7nen\u00b7rohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den uners\u00e4ttlichen Rachen.", "tokens": ["Den", "un\u00b7er\u00b7s\u00e4tt\u00b7li\u00b7chen", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das eiserne Untier horcht. Sein Ohr", "tokens": ["Das", "ei\u00b7ser\u00b7ne", "Un\u00b7tier", "horcht", ".", "Sein", "Ohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verfolgt die Schritte der Wachen.", "tokens": ["Ver\u00b7folgt", "die", "Schrit\u00b7te", "der", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Gefressen hat es den ganzen Tag,", "tokens": ["Ge\u00b7fres\u00b7sen", "hat", "es", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun kann es ruhn und verdauen.", "tokens": ["Nun", "kann", "es", "ruhn", "und", "ver\u00b7dau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch eh im Osten es d\u00e4mmern mag,", "tokens": ["Doch", "eh", "im", "Os\u00b7ten", "es", "d\u00e4m\u00b7mern", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gibt's frische Knochen zu kauen.", "tokens": ["Gibt's", "fri\u00b7sche", "Kno\u00b7chen", "zu", "kau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Musik, Musik in Blut und Kot", "tokens": ["Mu\u00b7sik", ",", "Mu\u00b7sik", "in", "Blut", "und", "Kot"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist ihm das Wimmern und St\u00f6hnen.", "tokens": ["Ist", "ihm", "das", "Wim\u00b7mern", "und", "St\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was w\u00e4chst auf Erdenfeldern Brot?!", "tokens": ["Was", "w\u00e4chst", "auf", "Er\u00b7den\u00b7fel\u00b7dern", "Brot", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fleisch! Schlachtfeld! Heulen und Dr\u00f6hnen! \u2013", "tokens": ["Fleisch", "!", "Schlacht\u00b7feld", "!", "Heu\u00b7len", "und", "Dr\u00f6h\u00b7nen", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gen Himmel reckt die Faust geballt", "tokens": ["Gen", "Him\u00b7mel", "reckt", "die", "Faust", "ge\u00b7ballt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sterbender. Stumm bleibt die Ferne.", "tokens": ["Ein", "Ster\u00b7ben\u00b7der", ".", "Stumm", "bleibt", "die", "Fer\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Verdorrt seine Zunge. Klar und kalt", "tokens": ["Ver\u00b7dorrt", "sei\u00b7ne", "Zun\u00b7ge", ".", "Klar", "und", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "NN", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Funkeln die himmlischen Sterne.", "tokens": ["Fun\u00b7keln", "die", "himm\u00b7li\u00b7schen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}