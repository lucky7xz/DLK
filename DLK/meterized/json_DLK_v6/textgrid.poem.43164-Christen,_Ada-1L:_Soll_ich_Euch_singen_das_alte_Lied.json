{"textgrid.poem.43164": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll ich Euch singen das alte Lied", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich Euch singen das alte Lied", "tokens": ["Soll", "ich", "Euch", "sin\u00b7gen", "das", "al\u00b7te", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von Jugend, Fr\u00fchling und Rosen?", "tokens": ["Von", "Ju\u00b7gend", ",", "Fr\u00fch\u00b7ling", "und", "Ro\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Soll ich Euch schildern mit s\u00fc\u00dfem Wort", "tokens": ["Soll", "ich", "Euch", "schil\u00b7dern", "mit", "s\u00fc\u00b7\u00dfem", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das Sprie\u00dfen, Knospen und Kosen?", "tokens": ["Das", "Sprie\u00b7\u00dfen", ",", "Knos\u00b7pen", "und", "Ko\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr h\u00f6ret, sehet und f\u00fchlt es nicht,", "tokens": ["Ihr", "h\u00f6\u00b7ret", ",", "se\u00b7het", "und", "f\u00fchlt", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wenn Dichter auch r\u00fchrend leiern,", "tokens": ["Wenn", "Dich\u00b7ter", "auch", "r\u00fch\u00b7rend", "lei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da\u00df wieder einmal die Wiese gr\u00fcnt,", "tokens": ["Da\u00df", "wie\u00b7der", "ein\u00b7mal", "die", "Wie\u00b7se", "gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Winterst\u00fcrme nun feiern.", "tokens": ["Die", "Win\u00b7ter\u00b7st\u00fcr\u00b7me", "nun", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Als Gottesfriede und Fr\u00fchlingsluft", "tokens": ["Als", "Got\u00b7tes\u00b7frie\u00b7de", "und", "Fr\u00fch\u00b7lings\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Durch alle Welten gezogen,", "tokens": ["Durch", "al\u00b7le", "Wel\u00b7ten", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Habt Ihr, wie am schmutzigsten Wintertag,", "tokens": ["Habt", "Ihr", ",", "wie", "am", "schmut\u00b7zigs\u00b7ten", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Geschachert doch nur und betrogen!", "tokens": ["Ge\u00b7scha\u00b7chert", "doch", "nur", "und", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ADV", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}