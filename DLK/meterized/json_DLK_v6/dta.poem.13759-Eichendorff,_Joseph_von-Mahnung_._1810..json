{"dta.poem.13759": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da mocht' ich l\u00e4nger nicht nach Euch mehr fragen,", "tokens": ["Da", "mocht'", "ich", "l\u00e4n\u00b7ger", "nicht", "nach", "Euch", "mehr", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKNEG", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wald empfing, wie rauschend! den Entfloh'nen,", "tokens": ["Der", "Wald", "emp\u00b7fing", ",", "wie", "rau\u00b7schend", "!", "den", "Ent\u00b7floh'\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "VVPP", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Burgen alt, an Stromesk\u00fchle wohnen,", "tokens": ["In", "Bur\u00b7gen", "alt", ",", "an", "Stro\u00b7mes\u00b7k\u00fch\u00b7le", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wollt' ich auf Bergen bei den alten Sagen.", "tokens": ["Wollt'", "ich", "auf", "Ber\u00b7gen", "bei", "den", "al\u00b7ten", "Sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}