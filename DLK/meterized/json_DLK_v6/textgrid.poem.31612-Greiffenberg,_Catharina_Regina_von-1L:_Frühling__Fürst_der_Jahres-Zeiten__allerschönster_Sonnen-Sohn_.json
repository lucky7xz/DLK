{"textgrid.poem.31612": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchling / F\u00fcrst der Jahres-Zeiten / allersch\u00f6nster Sonnen-Sohn /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling / F\u00fcrst der Jahres-Zeiten / allersch\u00f6nster Sonnen-Sohn /", "tokens": ["Fr\u00fch\u00b7ling", "/", "F\u00fcrst", "der", "Jah\u00b7res\u00b7Zei\u00b7ten", "/", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ter", "Son\u00b7nen\u00b7Sohn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "ART", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Rosen-Vatter / komm herzu! schau / die neubekleidten Wiesen", "tokens": ["Ro\u00b7sen\u00b7Vat\u00b7ter", "/", "komm", "her\u00b7zu", "!", "schau", "/", "die", "neu\u00b7be\u00b7kleid\u00b7ten", "Wie\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "ADV", "$.", "PTKVZ", "$(", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "prangen in der Hoffnungs-Farb: Ach erhebe n\u00e4chst bey diesen", "tokens": ["pran\u00b7gen", "in", "der", "Hoff\u00b7nungs\u00b7Fa\u00b7rb", ":", "Ach", "er\u00b7he\u00b7be", "n\u00e4chst", "bey", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "ITJ", "VVFIN", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "die Gr\u00fcn-weissen Lauberh\u00fctten / deinen Bl\u00fchgestickten Thron.", "tokens": ["die", "Gr\u00fcn\u00b7weis\u00b7sen", "Lau\u00b7ber\u00b7h\u00fct\u00b7ten", "/", "dei\u00b7nen", "Bl\u00fch\u00b7ge\u00b7stick\u00b7ten", "Thron", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Aus den bundten Tulipanen / machdie gl\u00e4nzend Lenzen-Kron.", "tokens": ["Aus", "den", "bund\u00b7ten", "Tu\u00b7li\u00b7pa\u00b7nen", "/", "mach\u00b7die", "gl\u00e4n\u00b7zend", "Len\u00b7zen\u00b7Kron", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "VVFIN", "ADJD", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Ach da\u00df die verliebten West von sich Blumen-Biesem bliesen!", "tokens": ["Ach", "da\u00df", "die", "ver\u00b7lieb\u00b7ten", "West", "von", "sich", "Blu\u00b7men\u00b7Bie\u00b7sem", "blie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ART", "ADJA", "NN", "APPR", "PRF", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Billich / vor dem Steinern Pracht / G\u00e4rten-Schmelzwerk wird gepriesen /", "tokens": ["Bil\u00b7lich", "/", "vor", "dem", "Stei\u00b7nern", "Pracht", "/", "G\u00e4r\u00b7ten\u00b7Schmelz\u00b7werk", "wird", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "APPR", "ART", "NN", "NN", "$(", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.8": {"text": "komme / Fr\u00fcling / fr\u00f6lichs Wesen! dir rufft schon mein willkomms-Thon.", "tokens": ["kom\u00b7me", "/", "Fr\u00fc\u00b7ling", "/", "fr\u00f6\u00b7lichs", "We\u00b7sen", "!", "dir", "rufft", "schon", "mein", "will\u00b7komms\u00b7Thon", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-++--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Mein' in Ruh verliebte Sinnen / w\u00fcnschen keinen Hoheit-schein /", "tokens": ["Mein'", "in", "Ruh", "ver\u00b7lieb\u00b7te", "Sin\u00b7nen", "/", "w\u00fcn\u00b7schen", "kei\u00b7nen", "Ho\u00b7heit\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "VVFIN", "NN", "$(", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "als der Sonnen Klarheits-Glanz / seyn in und mit sich vergn\u00fcget.", "tokens": ["als", "der", "Son\u00b7nen", "Kla\u00b7rheits\u00b7Glanz", "/", "seyn", "in", "und", "mit", "sich", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$(", "VAINF", "APPR", "KON", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "In der Rosen Purpur-Tracht / all ihr Pracht und Hoffart liget.", "tokens": ["In", "der", "Ro\u00b7sen", "Pur\u00b7pur\u00b7Tracht", "/", "all", "ihr", "Pracht", "und", "Hof\u00b7fart", "li\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "PIAT", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "B\u00e4sser ist / als Welt-beherrschen / Tugend ihr Leibeigne seyn", "tokens": ["B\u00e4s\u00b7ser", "ist", "/", "als", "Welt\u00b7be\u00b7herr\u00b7schen", "/", "Tu\u00b7gend", "ihr", "Leib\u00b7eig\u00b7ne", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$(", "KOUS", "NN", "$(", "NN", "PPOSAT", "NN", "VAINF"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Sorgen frey und Freyheit voll seyn die frischen Kr\u00e4nz der Erden /", "tokens": ["Sor\u00b7gen", "frey", "und", "Frey\u00b7heit", "voll", "seyn", "die", "fri\u00b7schen", "Kr\u00e4nz", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "NN", "ADJD", "VAINF", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "hegen s\u00fcsse Tugend-Ruh: G\u00fcldne Regen / Angst-Beschwerden.", "tokens": ["he\u00b7gen", "s\u00fcs\u00b7se", "Tu\u00b7gen\u00b7dRuh", ":", "G\u00fcld\u00b7ne", "Re\u00b7gen", "/", "Angst\u00b7Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$.", "ADJA", "NN", "$(", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}