{"textgrid.poem.57737": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist einer, der Judith Josephi sah", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist einer, der Judith Josephi sah", "tokens": ["Ist", "ei\u00b7ner", ",", "der", "Ju\u00b7dith", "Jo\u00b7se\u00b7phi", "sah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "ART", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sein Herz blieb stumm und lau?", "tokens": ["Und", "sein", "Herz", "blieb", "stumm", "und", "lau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ner war einst in Samaria", "tokens": ["Sch\u00f6\u00b7ner", "war", "einst", "in", "Sa\u00b7ma\u00b7ria"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Keine Hammoriterfrau;", "tokens": ["Kei\u00b7ne", "Ham\u00b7mo\u00b7ri\u00b7ter\u00b7frau", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr Angesicht ist rot und wei\u00df", "tokens": ["Ihr", "An\u00b7ge\u00b7sicht", "ist", "rot", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Linnen, getr\u00e4nkt in Bordeaux,", "tokens": ["Wie", "Lin\u00b7nen", ",", "ge\u00b7tr\u00e4nkt", "in", "Bor\u00b7dea\u00b7ux", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "VVPP", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihre Augen, blau wie Ehrenpreis,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", ",", "blau", "wie", "Eh\u00b7ren\u00b7preis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Sind liebessiegesfroh.", "tokens": ["Sind", "lie\u00b7bes\u00b7sie\u00b7ges\u00b7froh", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im V\u00f6lkermuseum ein Bildnis starrt", "tokens": ["Im", "V\u00f6l\u00b7ker\u00b7mu\u00b7se\u00b7um", "ein", "Bild\u00b7nis", "starrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geformt aus grauem Stein,", "tokens": ["Ge\u00b7formt", "aus", "grau\u00b7em", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seine kalten Z\u00fcge stehen hart", "tokens": ["Sei\u00b7ne", "kal\u00b7ten", "Z\u00fc\u00b7ge", "ste\u00b7hen", "hart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Im warmen Sonnenschein;", "tokens": ["Im", "war\u00b7men", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D\u00fcster droht das hehre", "tokens": ["D\u00fcs\u00b7ter", "droht", "das", "heh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Uralte K\u00f6nigsbild,", "tokens": ["Ur\u00b7al\u00b7te", "K\u00f6\u00b7nigs\u00b7bild", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seine Augen sind mit Leere,", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "sind", "mit", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Lippen mit Schweigen gef\u00fcllt.", "tokens": ["Sei\u00b7ne", "Lip\u00b7pen", "mit", "Schwei\u00b7gen", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Judith Josephi steht vor dem Stein", "tokens": ["Ju\u00b7dith", "Jo\u00b7se\u00b7phi", "steht", "vor", "dem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Des gro\u00dfen Pharao:", "tokens": ["Des", "gro\u00b7\u00dfen", "Pha\u00b7rao", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u00bbdas soll \u00e4gyptische Bildkunst sein?", "tokens": ["\u00bb", "das", "soll", "\u00e4\u00b7gypt\u00b7i\u00b7sche", "Bild\u00b7kunst", "sein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich finde es plump und roh!\u00ab", "tokens": ["Ich", "fin\u00b7de", "es", "plump", "und", "roh", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Einen Nasenst\u00fcber", "tokens": ["Ei\u00b7nen", "Na\u00b7sen\u00b7st\u00fc\u00b7ber"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Versetzt sie dem grauen Gesicht:", "tokens": ["Ver\u00b7setzt", "sie", "dem", "grau\u00b7en", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "\u00bbwissen Sie was, mein Lieber?", "tokens": ["\u00bb", "wis\u00b7sen", "Sie", "was", ",", "mein", "Lie\u00b7ber", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PIS", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Sie imponieren mir nicht!\u00ab", "tokens": ["Sie", "im\u00b7po\u00b7nie\u00b7ren", "mir", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Das graue Bildnis kraust die Stirn,", "tokens": ["Das", "grau\u00b7e", "Bild\u00b7nis", "kraust", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schauder sie erschreckt;", "tokens": ["Ein", "Schau\u00b7der", "sie", "er\u00b7schreckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbes braust mir wohl noch im Gehirn", "tokens": ["\u00bb", "es", "braust", "mir", "wohl", "noch", "im", "Ge\u00b7hirn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von heute Nacht der Sekt.\u00ab", "tokens": ["Von", "heu\u00b7te", "Nacht", "der", "Sekt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aber dann rauscht sie weiter,", "tokens": ["A\u00b7ber", "dann", "rauscht", "sie", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ihre seidenen R\u00f6cke weh'n,", "tokens": ["Ih\u00b7re", "sei\u00b7de\u00b7nen", "R\u00f6\u00b7cke", "weh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Wo hell und hold und heiter", "tokens": ["Wo", "hell", "und", "hold", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die G\u00f6tter der Griechen steh'n.", "tokens": ["Die", "G\u00f6t\u00b7ter", "der", "Grie\u00b7chen", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbach ja, die Nacht war toll und lang!\u00ab", "tokens": ["\u00bb", "ach", "ja", ",", "die", "Nacht", "war", "toll", "und", "lang", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Augen werden ihr schwer;", "tokens": ["Die", "Au\u00b7gen", "wer\u00b7den", "ihr", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie gleitet auf die Ruhebank,", "tokens": ["Sie", "glei\u00b7tet", "auf", "die", "Ru\u00b7he\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still ist es um sie her.", "tokens": ["Still", "ist", "es", "um", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sinken ihre Lider,", "tokens": ["Es", "sin\u00b7ken", "ih\u00b7re", "Li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lauter ihr Atem weht,", "tokens": ["Lau\u00b7ter", "ihr", "A\u00b7tem", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Langsam auf und nieder", "tokens": ["Lang\u00b7sam", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ihre Spitzenbluse geht.", "tokens": ["Ih\u00b7re", "Spit\u00b7zen\u00b7blu\u00b7se", "geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "L\u00e4ngst erlosch an der roten Wand", "tokens": ["L\u00e4ngst", "er\u00b7losch", "an", "der", "ro\u00b7ten", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der gelbe Sonnenschein,", "tokens": ["Der", "gel\u00b7be", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im grauen Spinnewebegewand", "tokens": ["Im", "grau\u00b7en", "Spin\u00b7ne\u00b7we\u00b7be\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Tritt der Abend ein;", "tokens": ["Tritt", "der", "A\u00b7bend", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die T\u00f6ne der Glocke verklangen,", "tokens": ["Die", "T\u00f6\u00b7ne", "der", "Glo\u00b7cke", "ver\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Schweigen bricht ringsum hervor,", "tokens": ["Schwei\u00b7gen", "bricht", "ring\u00b7sum", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der W\u00e4rter ist gegangen,", "tokens": ["Der", "W\u00e4r\u00b7ter", "ist", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es schlo\u00df sich T\u00fcr und Tor.", "tokens": ["Es", "schlo\u00df", "sich", "T\u00fcr", "und", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Eine eherne Stimme das Schweigen st\u00f6rt:", "tokens": ["Ei\u00b7ne", "e\u00b7her\u00b7ne", "Stim\u00b7me", "das", "Schwei\u00b7gen", "st\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00bbjudith Josephi, tritt her!\u00ab", "tokens": ["\u00bb", "ju\u00b7dith", "Jo\u00b7se\u00b7phi", ",", "tritt", "her", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "NE", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus ihrem Schlummer empor sie f\u00e4hrt,", "tokens": ["Aus", "ih\u00b7rem", "Schlum\u00b7mer", "em\u00b7por", "sie", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ringsum ist's stille und leer;", "tokens": ["Ring\u00b7sum", "ist's", "stil\u00b7le", "und", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Rundum ihre Augen sp\u00e4hen,", "tokens": ["Run\u00b7dum", "ih\u00b7re", "Au\u00b7gen", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Schweigen atmet tief,", "tokens": ["Das", "Schwei\u00b7gen", "at\u00b7met", "tief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Niemand ist zu sehen,", "tokens": ["Nie\u00b7mand", "ist", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Der sie mit Namen rief.", "tokens": ["Der", "sie", "mit", "Na\u00b7men", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Des Pharao Bildnis starrt sie an", "tokens": ["Des", "Pha\u00b7rao", "Bild\u00b7nis", "starrt", "sie", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit leerem Gespensterblick,", "tokens": ["Mit", "lee\u00b7rem", "Ge\u00b7spens\u00b7ter\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zieht langsam sie in seinen Bann,", "tokens": ["Zieht", "lang\u00b7sam", "sie", "in", "sei\u00b7nen", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Herzblut tritt zur\u00fcck;", "tokens": ["Ihr", "Herz\u00b7blut", "tritt", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["P