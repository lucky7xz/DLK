{"textgrid.poem.47511": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in des Geliebten Haare w\u00fchlt ein wilder Wind -]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Geliebten Haare w\u00fchlt ein wilder Wind \u2013", "tokens": ["In", "des", "Ge\u00b7lieb\u00b7ten", "Haa\u00b7re", "w\u00fchlt", "ein", "wil\u00b7der", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unw\u00fcrdiges Schicksal, dem wir unterworfen sind!", "tokens": ["Un\u00b7w\u00fcr\u00b7di\u00b7ges", "Schick\u00b7sal", ",", "dem", "wir", "un\u00b7ter\u00b7wor\u00b7fen", "sind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Zu gleichen nicht vermag ich einem Hauch der Luft,", "tokens": ["Zu", "glei\u00b7chen", "nicht", "ver\u00b7mag", "ich", "ei\u00b7nem", "Hauch", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu spielen nicht an jene Wange leis und lind!", "tokens": ["Zu", "spie\u00b7len", "nicht", "an", "je\u00b7ne", "Wan\u00b7ge", "leis", "und", "lind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "APPR", "PDAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}