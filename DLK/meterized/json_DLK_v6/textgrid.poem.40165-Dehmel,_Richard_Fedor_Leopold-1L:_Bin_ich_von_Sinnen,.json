{"textgrid.poem.40165": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bin ich von Sinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich von Sinnen,", "tokens": ["Bin", "ich", "von", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "bin ich beth\u00f6rt?", "tokens": ["bin", "ich", "be\u00b7th\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.3": {"text": "Alles tiefinnen", "tokens": ["Al\u00b7les", "tie\u00b7fin\u00b7nen"], "token_info": ["word", "word"], "pos": ["PIS", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "ist mir verst\u00f6rt!", "tokens": ["ist", "mir", "ver\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Kann mich nicht fassen, kann nicht zur\u00fcck zu mir,", "tokens": ["Kann", "mich", "nicht", "fas\u00b7sen", ",", "kann", "nicht", "zu\u00b7r\u00fcck", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "PTKVZ", "APPR", "PPER", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "mein ganzes Leben lebt ", "tokens": ["mein", "gan\u00b7zes", "Le\u00b7ben", "lebt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bin deine Treue,", "tokens": ["Bin", "dei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "mein Treuer du!", "tokens": ["mein", "Treu\u00b7er", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "brachtest mir neue.", "tokens": ["brach\u00b7test", "mir", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "s\u00fc\u00dfere Ruh'!", "tokens": ["s\u00fc\u00b7\u00dfe\u00b7re", "Ruh'", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "ich f\u00fchl's und schaure \u2013 sind ganz dahin.", "tokens": ["ich", "f\u00fchl's", "und", "schau\u00b7re", "\u2013", "sind", "ganz", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "VAFIN", "ADV", "PAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schwach bin ich nimmer,", "tokens": ["Schwach", "bin", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "bin's doch f\u00fcr dich!", "tokens": ["bin's", "doch", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.3": {"text": "fordre du immer:", "tokens": ["ford\u00b7re", "du", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "ich gebe mich!", "tokens": ["ich", "ge\u00b7be", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Kann nichts mehr wollen, als nur dein eigen sein:", "tokens": ["Kann", "nichts", "mehr", "wol\u00b7len", ",", "als", "nur", "dein", "ei\u00b7gen", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VMFIN", "$,", "KOUS", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "nimm mich, o nimm mich! ich bin ja dein!", "tokens": ["nimm", "mich", ",", "o", "nimm", "mich", "!", "ich", "bin", "ja", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "VVIMP", "PPER", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Bin ja so fr\u00f6hlich.", "tokens": ["Bin", "ja", "so", "fr\u00f6h\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "da\u00df du mich liebst!", "tokens": ["da\u00df", "du", "mich", "liebst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "macht doch so selig,", "tokens": ["macht", "doch", "so", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "was du mir giebst!", "tokens": ["was", "du", "mir", "giebst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "will ", "tokens": ["will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}}}}}