{"textgrid.poem.60920": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Cecco di Giorgio in Urbino", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich dem erlauchten Geschlecht, f\u00fcr das ich gebaut in Urbino,", "tokens": ["Gleich", "dem", "er\u00b7lauch\u00b7ten", "Ge\u00b7schlecht", ",", "f\u00fcr", "das", "ich", "ge\u00b7baut", "in", "Ur\u00b7bi\u00b7no", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "APPR", "NE", "$,"], "meter": "-+-+-+---+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Schnell, fr\u00fchzeitig verfiel meiner Pal\u00e4ste Palast;", "tokens": ["Schnell", ",", "fr\u00fch\u00b7zei\u00b7tig", "ver\u00b7fiel", "mei\u00b7ner", "Pa\u00b7l\u00e4s\u00b7te", "Pa\u00b7last", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber der G\u00e4nge, des Hofs und der Treppen Geschmack und der S\u00e4le", "tokens": ["A\u00b7ber", "der", "G\u00e4n\u00b7ge", ",", "des", "Hofs", "und", "der", "Trep\u00b7pen", "Ge\u00b7schmack", "und", "der", "S\u00e4\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "NN", "KON", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nennt im Verfall mich noch Lehrer des zierlichen Stils.", "tokens": ["Nennt", "im", "Ver\u00b7fall", "mich", "noch", "Leh\u00b7rer", "des", "zier\u00b7li\u00b7chen", "Stils", "."], "token_info": ["wo