{"textgrid.poem.59528": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Kindlein im wei\u00dfen Kleid,", "tokens": ["Es", "sa\u00df", "ein", "Kin\u00b7dlein", "im", "wei\u00b7\u00dfen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Kr\u00e4nzlein trug es der Herrlichkeit", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "trug", "es", "der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von Rosen und Lilien sch\u00f6n gewunden,", "tokens": ["Von", "Ro\u00b7sen", "und", "Li\u00b7li\u00b7en", "sch\u00f6n", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VAPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Solche Blumen sind nicht auf Erden erfunden;", "tokens": ["Sol\u00b7che", "Blu\u00b7men", "sind", "nicht", "auf", "Er\u00b7den", "er\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "+-+-++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Auch war das Kindlein sch\u00f6n und s\u00fc\u00df,", "tokens": ["Auch", "war", "das", "Kin\u00b7dlein", "sch\u00f6n", "und", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als k\u00e4m' es aus dem Paradies.", "tokens": ["Als", "k\u00e4m'", "es", "aus", "dem", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wer das liebliche Kindlein sah,", "tokens": ["Und", "wer", "das", "lieb\u00b7li\u00b7che", "Kin\u00b7dlein", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem wunderbarliche Lust geschah,", "tokens": ["Dem", "wun\u00b7der\u00b7bar\u00b7li\u00b7che", "Lust", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als w\u00e4r' er zum Himmel schon hoch erhoben", "tokens": ["Als", "w\u00e4r'", "er", "zum", "Him\u00b7mel", "schon", "hoch", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und h\u00f6rte Gott Vater von Engeln loben", "tokens": ["Und", "h\u00f6r\u00b7te", "Gott", "Va\u00b7ter", "von", "En\u00b7geln", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "NN", "APPR", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und s\u00e4h' die Stern' im Jubelring", "tokens": ["Und", "s\u00e4h'", "die", "Stern'", "im", "Ju\u00b7bel\u00b7ring"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lobpreisen den Sch\u00f6pfer aller Ding'.", "tokens": ["Lob\u00b7prei\u00b7sen", "den", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Ding'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wohin das liebliche Kindlein kam,", "tokens": ["Wo\u00b7hin", "das", "lieb\u00b7li\u00b7che", "Kin\u00b7dlein", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Alle Zwietracht pl\u00f6tzlichen Abschied nahm,", "tokens": ["Al\u00b7le", "Zwiet\u00b7racht", "pl\u00f6tz\u00b7li\u00b7chen", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Liebe und Friede und stille Freude,", "tokens": ["Und", "Lie\u00b7be", "und", "Frie\u00b7de", "und", "stil\u00b7le", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als w\u00e4r' es schon Himmel, erfreut' die Leute.", "tokens": ["Als", "w\u00e4r'", "es", "schon", "Him\u00b7mel", ",", "er\u00b7freut'", "die", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Das Kindlein lieb, das dies getan,", "tokens": ["Das", "Kin\u00b7dlein", "lieb", ",", "das", "dies", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich Gottes Engel all' empfahn.", "tokens": ["Gleich", "Got\u00b7tes", "En\u00b7gel", "all'", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Kind auf Erden die Unschuld hei\u00dft,", "tokens": ["Das", "Kind", "auf", "Er\u00b7den", "die", "Un\u00b7schuld", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Himmel auch ist es hoch gepreist", "tokens": ["Im", "Him\u00b7mel", "auch", "ist", "es", "hoch", "ge\u00b7preist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor heiligen M\u00e4chten und hohen Thronen,", "tokens": ["Vor", "hei\u00b7li\u00b7gen", "M\u00e4ch\u00b7ten", "und", "ho\u00b7hen", "Thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die rings um den H\u00f6chsten im Lichte wohnen,", "tokens": ["Die", "rings", "um", "den", "H\u00f6chs\u00b7ten", "im", "Lich\u00b7te", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Steht Gott zun\u00e4chst zur rechten Hand", "tokens": ["Steht", "Gott", "zu\u00b7n\u00e4chst", "zur", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird sein Liebling dort genannt.", "tokens": ["Und", "wird", "sein", "Lieb\u00b7ling", "dort", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn alles Sch\u00f6ne geworden ist", "tokens": ["Denn", "al\u00b7les", "Sch\u00f6\u00b7ne", "ge\u00b7wor\u00b7den", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAPP", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch Kindereinfalt zu jeder Frist,", "tokens": ["Durch", "Kin\u00b7der\u00b7e\u00b7in\u00b7falt", "zu", "je\u00b7der", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonnen und Monden und hellen Sterne,", "tokens": ["Die", "Son\u00b7nen", "und", "Mon\u00b7den", "und", "hel\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die leuchten und winken aus weiter Ferne,", "tokens": ["Die", "leuch\u00b7ten", "und", "win\u00b7ken", "aus", "wei\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "APPR", "ADV", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der Blumenkeim, das Menschenherz:", "tokens": ["Der", "Blu\u00b7men\u00b7keim", ",", "das", "Men\u00b7schen\u00b7herz", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum will es alles himmelw\u00e4rts.", "tokens": ["Drum", "will", "es", "al\u00b7les", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Das Kindlein hab' ich gekonterfeit", "tokens": ["Das", "Kin\u00b7dlein", "hab'", "ich", "ge\u00b7kon\u00b7ter\u00b7feit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit seinem Kr\u00e4nzlein und wei\u00dfen Kleid,", "tokens": ["Mit", "sei\u00b7nem", "Kr\u00e4nz\u00b7lein", "und", "wei\u00b7\u00dfen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df Glaube und Sehnsucht der ewigen Liebe", "tokens": ["Da\u00df", "Glau\u00b7be", "und", "Sehn\u00b7sucht", "der", "e\u00b7wi\u00b7gen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Uns br\u00fcnstig zum Himmel der Freuden h\u00fcbe:", "tokens": ["Uns", "br\u00fcns\u00b7tig", "zum", "Him\u00b7mel", "der", "Freu\u00b7den", "h\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Denn wer das Kindlein zu sich h\u00e4lt,", "tokens": ["Denn", "wer", "das", "Kin\u00b7dlein", "zu", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem ist das Herz gar wohl bestellt.", "tokens": ["Dem", "ist", "das", "Herz", "gar", "wohl", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Besonders Kindelein fromm und zart", "tokens": ["Be\u00b7son\u00b7ders", "Kin\u00b7de\u00b7lein", "fromm", "und", "zart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und holden M\u00e4gdlein von stiller Art,", "tokens": ["Und", "hol\u00b7den", "M\u00e4gd\u00b7lein", "von", "stil\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch helles Gespiegel den reinen Frauen", "tokens": ["Auch", "hel\u00b7les", "Ge\u00b7spie\u00b7gel", "den", "rei\u00b7nen", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich habe dies Bildchen gestellt zu schauen,", "tokens": ["Ich", "ha\u00b7be", "dies", "Bild\u00b7chen", "ge\u00b7stellt", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da\u00df drin sie spat und fr\u00fche sehn", "tokens": ["Da\u00df", "drin", "sie", "spat", "und", "fr\u00fc\u00b7he", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "KON", "ADJA", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und werden gleich der Unschuld sch\u00f6n.", "tokens": ["Und", "wer\u00b7den", "gleich", "der", "Un\u00b7schuld", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}