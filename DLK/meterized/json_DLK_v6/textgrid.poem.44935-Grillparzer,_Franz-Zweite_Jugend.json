{"textgrid.poem.44935": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zweite Jugend", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann erh\u00e4lt sich wunderbar,", "tokens": ["Der", "Mann", "er\u00b7h\u00e4lt", "sich", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird nicht alt, wie wir Philister,", "tokens": ["Er", "wird", "nicht", "alt", ",", "wie", "wir", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PWAV", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein J\u00fcngling scheint er ganz und gar,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "scheint", "er", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn nicht als Mann, doch als Minister.", "tokens": ["Wenn", "nicht", "als", "Mann", ",", "doch", "als", "Mi\u00b7nis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "KOUS", "NN", "$,", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}