{"textgrid.poem.60592": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Erde, wo j\u00fcngst Blut geflossen,", "tokens": ["O", "Er\u00b7de", ",", "wo", "j\u00fcngst", "Blut", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df Bluhmen sprossen,", "tokens": ["La\u00df", "Bluh\u00b7men", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Noch vor der Bluhmenzeit,", "tokens": ["Noch", "vor", "der", "Bluh\u00b7men\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Den holden Frieden zu bekr\u00e4nzen,", "tokens": ["Den", "hol\u00b7den", "Frie\u00b7den", "zu", "be\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der wieder k\u00f6mmt nach langem Streit,", "tokens": ["Der", "wie\u00b7der", "k\u00f6mmt", "nach", "lan\u00b7gem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und vor ihm her zu gl\u00e4nzen", "tokens": ["Und", "vor", "ihm", "her", "zu", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APZR", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Fr\u00fchlingskleid!", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7kleid", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er k\u00f6mmt zur\u00fcck durch \u00f6de Fluren,", "tokens": ["Er", "k\u00f6mmt", "zu\u00b7r\u00fcck", "durch", "\u00f6\u00b7de", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll frischer Spuren", "tokens": ["Voll", "fri\u00b7scher", "Spu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der kriegerischen Wuth.", "tokens": ["Der", "krie\u00b7ge\u00b7ri\u00b7schen", "Wuth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er eilt aus r\u00e4ubervollen Str\u00e4uchen,", "tokens": ["Er", "eilt", "aus", "r\u00e4u\u00b7ber\u00b7vol\u00b7len", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wandelt schaudernd \u00fcber Blut", "tokens": ["Und", "wan\u00b7delt", "schau\u00b7dernd", "\u00fc\u00b7ber", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und halbverweste Leichen", "tokens": ["Und", "halb\u00b7ver\u00b7wes\u00b7te", "Lei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Asch und Glut.", "tokens": ["Und", "Asch", "und", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Freude jauchzt auf allen Wegen", "tokens": ["Die", "Freu\u00b7de", "jauchzt", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm wild entgegen,", "tokens": ["Ihm", "wild", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch s\u00fc\u00dfen Weihrauchduft.", "tokens": ["Durch", "s\u00fc\u00b7\u00dfen", "Weih\u00b7rauch\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Anblick tr\u00f6stet die Geplagten,", "tokens": ["Sein", "An\u00b7blick", "tr\u00f6s\u00b7tet", "die", "Ge\u00b7plag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und seine sanfte Stimme ruft", "tokens": ["Und", "sei\u00b7ne", "sanf\u00b7te", "Stim\u00b7me", "ruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die hoffenden Verjagten", "tokens": ["Die", "hof\u00b7fen\u00b7den", "Ver\u00b7jag\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus fremder Luft.", "tokens": ["Aus", "frem\u00b7der", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sie taumeln jetzt mit scheuen Schritten", "tokens": ["Sie", "tau\u00b7meln", "jetzt", "mit", "scheu\u00b7en", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihren H\u00fctten,", "tokens": ["Zu", "ih\u00b7ren", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zu Wohnungen der Noth.", "tokens": ["Zu", "Woh\u00b7nun\u00b7gen", "der", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie finden rauchende Ruinen,", "tokens": ["Sie", "fin\u00b7den", "rau\u00b7chen\u00b7de", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Blut erschlagner Freunde roth;", "tokens": ["Vom", "Blut", "er\u00b7schlag\u00b7ner", "Freun\u00b7de", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Hunger nagt in ihnen", "tokens": ["Und", "Hun\u00b7ger", "nagt", "in", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Am letzten Brod.", "tokens": ["Am", "letz\u00b7ten", "Brod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Gekr\u00f6nte H\u00e4upter gro\u00dfer Staaten,", "tokens": ["Ge\u00b7kr\u00f6n\u00b7te", "H\u00e4up\u00b7ter", "gro\u00b7\u00dfer", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seht eure Thaten,", "tokens": ["Seht", "eu\u00b7re", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und wie ihr uns begl\u00fcckt!", "tokens": ["Und", "wie", "ihr", "uns", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Z\u00e4hlt die erschlagnen Unterthanen,", "tokens": ["Z\u00e4hlt", "die", "er\u00b7schlag\u00b7nen", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann ihr, von Heldenlust entz\u00fcckt,", "tokens": ["Wann", "ihr", ",", "von", "Hel\u00b7den\u00b7lust", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf die ersiegten Fahnen", "tokens": ["Auf", "die", "er\u00b7sieg\u00b7ten", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Stolz l\u00e4chelnd blickt!", "tokens": ["Stolz", "l\u00e4\u00b7chelnd", "blickt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wie lange werden doch die F\u00fcrsten", "tokens": ["Wie", "lan\u00b7ge", "wer\u00b7den", "doch", "die", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Lorbeern d\u00fcrsten,", "tokens": ["Nach", "Lor\u00b7beern", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie Mars nach Blute schnaubt!", "tokens": ["Wie", "Mars", "nach", "Blu\u00b7te", "schnaubt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Schande, nicht mit Lorbeerkr\u00e4nzen,", "tokens": ["Mit", "Schan\u00b7de", ",", "nicht", "mit", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verh\u00e4ngni\u00df, kr\u00f6ne dessen Haupt,", "tokens": ["Ver\u00b7h\u00e4ng\u00b7ni\u00df", ",", "kr\u00f6\u00b7ne", "des\u00b7sen", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wieder unsern Gr\u00e4nzen", "tokens": ["Der", "wie\u00b7der", "un\u00b7sern", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Frieden raubt!", "tokens": ["Den", "Frie\u00b7den", "raubt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Der nicht sein Volk mit Huld erquicket,", "tokens": ["Der", "nicht", "sein", "Volk", "mit", "Huld", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Noth erblicket,", "tokens": ["Die", "Noth", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und Hungrige nicht speist,", "tokens": ["Und", "Hung\u00b7ri\u00b7ge", "nicht", "speist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht mit wohlth\u00e4tigen Erbarmen", "tokens": ["Nicht", "mit", "wohlt\u00b7h\u00e4\u00b7ti\u00b7gen", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als einen Vater sich erweist,", "tokens": ["Als", "ei\u00b7nen", "Va\u00b7ter", "sich", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wann ihn ein Schwarm von Armen", "tokens": ["Wann", "ihn", "ein", "Schwarm", "von", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lautjauchzend preist:", "tokens": ["Lau\u00b7tjauch\u00b7zend", "preist", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Damit, nach unerh\u00f6rten Plagen,", "tokens": ["Da\u00b7mit", ",", "nach", "un\u00b7er\u00b7h\u00f6r\u00b7ten", "Pla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In heitrern Tagen,", "tokens": ["In", "hei\u00b7trern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Landmann sich erfreu,", "tokens": ["Der", "Land\u00b7mann", "sich", "er\u00b7freu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt seine w\u00fcsten Felder baue,", "tokens": ["Jetzt", "sei\u00b7ne", "w\u00fcs\u00b7ten", "Fel\u00b7der", "bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, sicher vor der Tyranney,", "tokens": ["Und", ",", "si\u00b7cher", "vor", "der", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Heerdenvoller Aue", "tokens": ["Auf", "Heer\u00b7den\u00b7vol\u00b7ler", "Au\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Selbst gl\u00fccklich sey!", "tokens": ["Selbst", "gl\u00fcck\u00b7lich", "sey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}