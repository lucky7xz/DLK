{"textgrid.poem.64456": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "78. Der Mahler", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich mahl' oft, und nehme nichts daf\u00fcr,", "tokens": ["Auch", "ich", "mahl'", "oft", ",", "und", "neh\u00b7me", "nichts", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Der, den ich gemahlt, z\u00fcrnt noch dazu mit mir;", "tokens": ["Und", "Der", ",", "den", "ich", "ge\u00b7mahlt", ",", "z\u00fcrnt", "noch", "da\u00b7zu", "mit", "mir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "ADV", "PAV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich gebe ganz getreu die Z\u00fcge der Natur:", "tokens": ["Ich", "ge\u00b7be", "ganz", "ge\u00b7treu", "die", "Z\u00fc\u00b7ge", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weswegen ist der Mann denn selbst Caricatur?", "tokens": ["Wes\u00b7we\u00b7gen", "ist", "der", "Mann", "denn", "selbst", "Ca\u00b7ri\u00b7ca\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}