["KON", "ART", "NN", "$(", "VVFIN", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jahr f\u00fcr Jahr, am Aschermittwoch.", "tokens": ["Jahr", "f\u00fcr", "Jahr", ",", "am", "A\u00b7scher\u00b7mitt\u00b7woch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}