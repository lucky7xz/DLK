{"textgrid.poem.34531": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1. Die W\u00fcrfel", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahr f\u00fcr Jahr, am Aschermittwoch,", "tokens": ["Jahr", "f\u00fcr", "Jahr", ",", "am", "A\u00b7scher\u00b7mitt\u00b7woch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn der Carneval durchtobt ist,", "tokens": ["wenn", "der", "Car\u00b7ne\u00b7val", "durch\u00b7tobt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "qu\u00e4lt Pierrot sich mit Gedanken,", "tokens": ["qu\u00e4lt", "Pier\u00b7rot", "sich", "mit", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die nach Tod und Heirath schielen.", "tokens": ["die", "nach", "Tod", "und", "Hei\u00b7rath", "schie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus der braunen Stummelpfeife", "tokens": ["Aus", "der", "brau\u00b7nen", "Stum\u00b7mel\u00b7pfei\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "steigen graue Nachtgebilde \u2013", "tokens": ["stei\u00b7gen", "grau\u00b7e", "Nacht\u00b7ge\u00b7bil\u00b7de", "\u2013"],