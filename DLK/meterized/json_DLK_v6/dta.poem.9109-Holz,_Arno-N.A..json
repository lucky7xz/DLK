{"dta.poem.9109": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich trat in mein Zimmer.", "tokens": ["Ich", "trat", "in", "mein", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Fenster standen weit auf,", "tokens": ["Die", "Fens\u00b7ter", "stan\u00b7den", "weit", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "draussen", "tokens": ["draus\u00b7sen"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "schien die Sonne.", "tokens": ["schien", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wie wunderbar,", "tokens": ["Wie", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Rosen?", "tokens": ["Ro\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein ganzer Strauss!", "tokens": ["Ein", "gan\u00b7zer", "Strauss", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Weisse, gelbe und dunkelrote . . .", "tokens": ["Weis\u00b7se", ",", "gel\u00b7be", "und", "dun\u00b7kel\u00b7ro\u00b7te", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "ADJA", "$.", "$.", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ah, wie das duftete! Wie das wohl that!", "tokens": ["Ah", ",", "wie", "das", "duf\u00b7te\u00b7te", "!", "Wie", "das", "wohl", "that", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PDS", "VVFIN", "$.", "PWAV", "PDS", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und ich stellte das Glas wieder auf meinen Schreibtisch.", "tokens": ["Und", "ich", "stell\u00b7te", "das", "Glas", "wie\u00b7der", "auf", "mei\u00b7nen", "Schreib\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Dort steht es und schimmert nun,", "tokens": ["Dort", "steht", "es", "und", "schim\u00b7mert", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und in Alles, was ich schreibe, f\u00e4llt sein sch\u00f6ner Schein.", "tokens": ["und", "in", "Al\u00b7les", ",", "was", "ich", "schrei\u00b7be", ",", "f\u00e4llt", "sein", "sch\u00f6\u00b7ner", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.7": {"line.1": {"text": "Du Liebe, du Gute!", "tokens": 