{"dta.poem.12876": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie schlief, \u00fcber ihre geschlos-  \n sene augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr sternen! die ihr sonst auch sternen \u00fcberwindet,", "tokens": ["Ihr", "ster\u00b7nen", "!", "die", "ihr", "sonst", "auch", "ster\u00b7nen", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PRELS", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr schlaft und zeigt gleichwol den blitz der grausamkeit.", "tokens": ["Ihr", "schlaft", "und", "zeigt", "gleich\u00b7wol", "den", "blitz", "der", "grau\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach augen! thut ihr das, nun ihr geschlossen seyd;", "tokens": ["Ach", "au\u00b7gen", "!", "thut", "ihr", "das", ",", "nun", "ihr", "ge\u00b7schlos\u00b7sen", "seyd", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "PDS", "$,", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was w\u00fcrdet ihr nicht thun, wenn ihr noch offen st\u00fcndet?", "tokens": ["Was", "w\u00fcr\u00b7det", "ihr", "nicht", "thun", ",", "wenn", "ihr", "noch", "of\u00b7fen", "st\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "