{"textgrid.poem.60738": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Gambacorti und Gualandi", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Alfons, der m\u00e4chtige K\u00f6nig,", "tokens": ["Als", "Al\u00b7fons", ",", "der", "m\u00e4ch\u00b7ti\u00b7ge", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seine Scharen ausgeschickt,", "tokens": ["Sei\u00b7ne", "Scha\u00b7ren", "aus\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Anzufeinden jene weise", "tokens": ["An\u00b7zu\u00b7fein\u00b7den", "je\u00b7ne", "wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Florentinische Republik,", "tokens": ["Flo\u00b7ren\u00b7ti\u00b7ni\u00b7sche", "Re\u00b7pub\u00b7lik", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die verwaltete wohlbed\u00e4chtig", "tokens": ["Die", "ver\u00b7wal\u00b7te\u00b7te", "wohl\u00b7be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Cosimo von Medicis,", "tokens": ["Co\u00b7si\u00b7mo", "von", "Me\u00b7di\u00b7cis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hatte Gerhard Gambacorti,", "tokens": ["Hat\u00b7te", "Ger\u00b7hard", "Gam\u00b7ba\u00b7cor\u00b7ti", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tief im Scho\u00df des Apennins,", "tokens": ["Tief", "im", "Scho\u00df", "des", "A\u00b7pen\u00b7nins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Als ein Lehn der Florentiner", "tokens": ["Als", "ein", "Lehn", "der", "Flo\u00b7ren\u00b7ti\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eine Herrschaft im Besitz.", "tokens": ["Ei\u00b7ne", "Herr\u00b7schaft", "im", "Be\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Durch Verschw\u00e4grung war verkn\u00fcpft er", "tokens": ["Durch", "Ver\u00b7schw\u00e4g\u00b7rung", "war", "ver\u00b7kn\u00fcpft", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jenem gro\u00dfen Albizi,", "tokens": ["Je\u00b7nem", "gro\u00b7\u00dfen", "Al\u00b7bi\u00b7zi", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Welcher aus Florenz vertrieben", "tokens": ["Wel\u00b7cher", "aus", "Flo\u00b7renz", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "APPR", "NE", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Nach dem heiligen Grabe ging,", "tokens": ["Nach", "dem", "hei\u00b7li\u00b7gen", "Gra\u00b7be", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Bis zuletzt er, heimgewandert,", "tokens": ["Bis", "zu\u00b7letzt", "er", ",", "heim\u00b7ge\u00b7wan\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Seltner Schicksalslaune Spiel,", "tokens": ["Selt\u00b7ner", "Schick\u00b7sals\u00b7lau\u00b7ne", "Spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "An dem Hochzeittag der Tochter", "tokens": ["An", "dem", "Hoch\u00b7zeit\u00b7tag", "der", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "War gestorben im Exil.", "tokens": ["War", "ge\u00b7stor\u00b7ben", "im", "E\u00b7xil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Des gedenkt nun Gambacorti,", "tokens": ["Des", "ge\u00b7denkt", "nun", "Gam\u00b7ba\u00b7cor\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Der Verrat und T\u00fccke spinnt,", "tokens": ["Der", "Ver\u00b7rat", "und", "T\u00fc\u00b7cke", "spinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Als ein Feind der Mediceer", "tokens": ["Als", "ein", "Feind", "der", "Me\u00b7di\u00b7ceer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Abgeneigt der Republik,", "tokens": ["Ab\u00b7ge\u00b7neigt", "der", "Re\u00b7pub\u00b7lik", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Welcher Gleichwohl seinen Sohn er", "tokens": ["Wel\u00b7cher", "Gleich\u00b7wohl", "sei\u00b7nen", "Sohn", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Hat als Geisel \u00fcberschickt,", "tokens": ["Hat", "als", "Gei\u00b7sel", "\u00fc\u00b7bersc\u00b7hickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Sicherheit ihr einzufl\u00f6\u00dfen,", "tokens": ["Si\u00b7cher\u00b7heit", "ihr", "ein\u00b7zu\u00b7fl\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Die bereits Verrat umstrickt.", "tokens": ["Die", "be\u00b7reits", "Ver\u00b7rat", "um\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Als vor seinem Schlo\u00df Corzano,", "tokens": ["Als", "vor", "sei\u00b7nem", "Schlo\u00df", "Cor\u00b7za\u00b7no", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Wo den kleinen Hof er hielt,", "tokens": ["Wo", "den", "klei\u00b7nen", "Hof", "er", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Mit dem Feldhauptmann des K\u00f6nigs", "tokens": ["Mit", "dem", "Feld\u00b7haupt\u00b7mann", "des", "K\u00f6\u00b7nigs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nun des K\u00f6nigs Heer erschien,", "tokens": ["Nun", "des", "K\u00f6\u00b7nigs", "Heer", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "L\u00e4\u00dft die Br\u00fccke Gambacorti", "tokens": ["L\u00e4\u00dft", "die", "Br\u00fc\u00b7cke", "Gam\u00b7ba\u00b7cor\u00b7ti"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Nieder, tritt entgegen ihm,", "tokens": ["Nie\u00b7der", ",", "tritt", "ent\u00b7ge\u00b7gen", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Dem die Burg er f\u00fcr den K\u00f6nig", "tokens": ["Dem", "die", "Burg", "er", "f\u00fcr", "den", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "T\u00fcckisch \u00fcberliefern will.", "tokens": ["T\u00fc\u00b7ckisch", "\u00fc\u00b7berl\u00b7ie\u00b7fern", "will", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Ihn umgeben seine Ritter,", "tokens": ["Ihn", "um\u00b7ge\u00b7ben", "sei\u00b7ne", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "M\u00e4nner vielgewandt im Krieg:", "tokens": ["M\u00e4n\u00b7ner", "viel\u00b7ge\u00b7wandt", "im", "Krieg", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Unter ihnen war Gualandi,", "tokens": ["Un\u00b7ter", "ih\u00b7nen", "war", "Gu\u00b7a\u00b7lan\u00b7di", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.38": {"text": "Dem der Hochverrat mi\u00dffiel.", "tokens": ["Dem", "der", "Hoch\u00b7ver\u00b7rat", "mi\u00df\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Der ergreift den Gambacorti,", "tokens": ["Der", "er\u00b7greift", "den", "Gam\u00b7ba\u00b7cor\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "\u00dcber die Br\u00fccke st\u00f6\u00dft er ihn;", "tokens": ["\u00dc\u00b7ber", "die", "Br\u00fc\u00b7cke", "st\u00f6\u00dft", "er", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Diese wird, auf sein Verlangen,", "tokens": ["Die\u00b7se", "wird", ",", "auf", "sein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Aufgezogen augenblicks,", "tokens": ["Auf\u00b7ge\u00b7zo\u00b7gen", "au\u00b7gen\u00b7blicks", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "W\u00e4hrend aufgepflanzt die freie", "tokens": ["W\u00e4h\u00b7rend", "auf\u00b7ge\u00b7pflanzt", "die", "frei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Florentinische Fahne wird,", "tokens": ["Flo\u00b7ren\u00b7ti\u00b7ni\u00b7sche", "Fah\u00b7ne", "wird", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "W\u00e4hrend innerhalb die Mannschaft", "tokens": ["W\u00e4h\u00b7rend", "in\u00b7ner\u00b7halb", "die", "Mann\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Ruft: Es lebe die Republik!", "tokens": ["Ruft", ":", "Es", "le\u00b7be", "die", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.47": {"text": "Gambacorti steht verlassen", "tokens": ["Gam\u00b7ba\u00b7cor\u00b7ti", "steht", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Au\u00dferhalb, im Angesicht", "tokens": ["Au\u00b7\u00dfer\u00b7halb", ",", "im", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word"], "pos": ["PAV", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Seiner nun verlornen Feste,", "tokens": ["Sei\u00b7ner", "nun", "ver\u00b7lor\u00b7nen", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Die Gualandi treu verficht.", "tokens": ["Die", "Gu\u00b7a\u00b7lan\u00b7di", "treu", "ver\u00b7ficht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Nach Neapel mu\u00df er wandern,", "tokens": ["Nach", "Nea\u00b7pel", "mu\u00df", "er", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Mit dem Feinde mu\u00df er ziehn;", "tokens": ["Mit", "dem", "Fein\u00b7de", "mu\u00df", "er", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Doch es schickt den Sohn zur\u00fcck ihm", "tokens": ["Doch", "es", "schickt", "den", "Sohn", "zu\u00b7r\u00fcck", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Gro\u00dfgesinnt die Republik.", "tokens": ["Gro\u00df\u00b7ge\u00b7sinnt", "die", "Re\u00b7pub\u00b7lik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}