{"textgrid.poem.42261": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Sie mein Eins und Alles", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht zum F\u00fcrsten hat mich das Geschick,", "tokens": ["Nicht", "zum", "F\u00fcrs\u00b7ten", "hat", "mich", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Nicht zum Grafen, noch zum Herrn geboren,", "tokens": ["Nicht", "zum", "Gra\u00b7fen", ",", "noch", "zum", "Herrn", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und f\u00fcrwahr nicht hellerswert verloren", "tokens": ["Und", "f\u00fcr\u00b7wahr", "nicht", "hel\u00b7lers\u00b7wert", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat an mich das goldbeschwerte Gl\u00fcck.", "tokens": ["Hat", "an", "mich", "das", "gold\u00b7be\u00b7schwer\u00b7te", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "G\u00fcnstig hat auch keines Wesirs Blick", "tokens": ["G\u00fcns\u00b7tig", "hat", "auch", "kei\u00b7nes", "We\u00b7sirs", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PIAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich im Staat zu hoher W\u00fcrd' erkoren.", "tokens": ["Mich", "im", "Staat", "zu", "ho\u00b7her", "W\u00fcrd'", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Alles st\u00f6\u00dft, wie gegen mich verschworen,", "tokens": ["Al\u00b7les", "st\u00f6\u00dft", ",", "wie", "ge\u00b7gen", "mich", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jeden Wunsch mir unerh\u00f6rt zur\u00fcck.", "tokens": ["Je\u00b7den", "Wunsch", "mir", "un\u00b7er\u00b7h\u00f6rt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Von der Wieg' an, bis zu meinem Grabe,", "tokens": ["Von", "der", "Wieg'", "an", ",", "bis", "zu", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist ein wohl ersung'nes Lorbeerreis", "tokens": ["Ist", "ein", "wohl", "er\u00b7sung'\u00b7nes", "Lor\u00b7beer\u00b7reis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Ehr' und meine ganze Habe.", "tokens": ["Mei\u00b7ne", "Ehr'", "und", "mei\u00b7ne", "gan\u00b7ze", "Ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dennoch auch dies Eine, so ich wei\u00df,", "tokens": ["Den\u00b7noch", "auch", "dies", "Ei\u00b7ne", ",", "so", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "ART", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Spendet' ich mit Lust zur Opfergabe,", "tokens": ["Spen\u00b7det'", "ich", "mit", "Lust", "zur", "Op\u00b7fer\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4r', o Molly, dein Besitz der Preis.", "tokens": ["W\u00e4r'", ",", "o", "Mol\u00b7ly", ",", "dein", "Be\u00b7sitz", "der", "Preis", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}