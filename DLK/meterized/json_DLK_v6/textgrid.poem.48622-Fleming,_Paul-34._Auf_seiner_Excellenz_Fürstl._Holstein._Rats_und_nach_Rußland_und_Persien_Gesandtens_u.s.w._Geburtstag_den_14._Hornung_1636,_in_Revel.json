{"textgrid.poem.48622": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "34. Auf seiner Excellenz F\u00fcrstl. Holstein. Rats und nach Ru\u00dfland und Persien Gesandtens u.s.w. Geburtstag den 14. Hornung 1636, in Revel", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, brich an, du liebstes Licht,", "tokens": ["Komm", ",", "brich", "an", ",", "du", "liebs\u00b7tes", "Licht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "komm, brich an f\u00fcr deinen Lieben,", "tokens": ["komm", ",", "brich", "an", "f\u00fcr", "dei\u00b7nen", "Lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dem der Himmel Heil verspricht", "tokens": ["dem", "der", "Him\u00b7mel", "Heil", "ver\u00b7spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und hat in sein Buch geschrieben!", "tokens": ["und", "hat", "in", "sein", "Buch", "ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Recht ists, da\u00df du den itzt labst,", "tokens": ["Recht", "ists", ",", "da\u00df", "du", "den", "itzt", "labst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "dem du vor das Leben gabst.", "tokens": ["dem", "du", "vor", "das", "Le\u00b7ben", "gabst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jupiters saphirner Saal", "tokens": ["Ju\u00b7pi\u00b7ters", "sa\u00b7phir\u00b7ner", "Saal"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "\u00f6ffnet seine g\u00fcldnen Pforten.", "tokens": ["\u00f6ff\u00b7net", "sei\u00b7ne", "g\u00fcld\u00b7nen", "Pfor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der demantnen L\u00fcfte Zahl", "tokens": ["Der", "de\u00b7mant\u00b7nen", "L\u00fcf\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bricht herf\u00fcr aus allen Orten.", "tokens": ["bricht", "her\u00b7f\u00fcr", "aus", "al\u00b7len", "Or\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Juno setzt ihr buntes Zelt", "tokens": ["Ju\u00b7no", "setzt", "ihr", "bun\u00b7tes", "Zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "in das g\u00fcldne Sternenfeld.", "tokens": ["in", "das", "g\u00fcld\u00b7ne", "Ster\u00b7nen\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Titan ist vor Morgen auf", "tokens": ["Ti\u00b7tan", "ist", "vor", "Mor\u00b7gen", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und hei\u00dft Memnons Mutter eilen,", "tokens": ["und", "hei\u00dft", "Mem\u00b7nons", "Mut\u00b7ter", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df er halte seinen Lauf", "tokens": ["da\u00df", "er", "hal\u00b7te", "sei\u00b7nen", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mit geschm\u00fccktern Feuer-G\u00e4ulen.", "tokens": ["mit", "ge\u00b7schm\u00fcck\u00b7tern", "Feu\u00b7er\u00b7G\u00e4u\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zynthie, die volle, steht,", "tokens": ["Zynt\u00b7hie", ",", "die", "vol\u00b7le", ",", "steht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "bis ihr ganzes Volk abgeht.", "tokens": ["bis", "ihr", "gan\u00b7zes", "Volk", "ab\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Die gefrorne Winterwelt", "tokens": ["Die", "ge\u00b7fror\u00b7ne", "Win\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "treibt zu See und Lande Scherze.", "tokens": ["treibt", "zu", "See", "und", "Lan\u00b7de", "Scher\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und der Hornung schreit zum M\u00e4rze:", "tokens": ["und", "der", "Hor\u00b7nung", "schreit", "zum", "M\u00e4r\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sei ewig ohne Pein,", "tokens": ["Der", "sei", "e\u00b7wig", "oh\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "dem wir heute fr\u00f6lich sein!", "tokens": ["dem", "wir", "heu\u00b7te", "fr\u00f6\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}