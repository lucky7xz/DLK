{"textgrid.poem.37385": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Hinten herum", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch, der etwas auf sich h\u00e4lt,", "tokens": ["Ein", "Mensch", ",", "der", "et\u00b7was", "auf", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewegt sich gern in feiner Welt;", "tokens": ["Be\u00b7wegt", "sich", "gern", "in", "fei\u00b7ner", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn erst in weltgewandten Kreisen", "tokens": ["Denn", "erst", "in", "welt\u00b7ge\u00b7wand\u00b7ten", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lernt man die rechten Redeweisen,", "tokens": ["Lernt", "man", "die", "rech\u00b7ten", "Re\u00b7de\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verbindlich, aber zugespitzt", "tokens": ["Ver\u00b7bind\u00b7lich", ",", "a\u00b7ber", "zu\u00b7ge\u00b7spitzt"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und treffend, wo die Schw\u00e4re sitzt.", "tokens": ["Und", "tref\u00b7fend", ",", "wo", "die", "Schw\u00e4\u00b7re", "sitzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist so wie mit Rektor Knaut,", "tokens": ["Es", "ist", "so", "wie", "mit", "Rek\u00b7tor", "Knaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der immer l\u00e4chelt, wenn er haut.", "tokens": ["Der", "im\u00b7mer", "l\u00e4\u00b7chelt", ",", "wenn", "er", "haut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auch ist bei Knaben weit ber\u00fcchtigt", "tokens": ["Auch", "ist", "bei", "Kna\u00b7ben", "weit", "be\u00b7r\u00fcch\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Instrument, womit er z\u00fcchtigt.", "tokens": ["Das", "Inst\u00b7ru\u00b7ment", ",", "wo\u00b7mit", "er", "z\u00fcch\u00b7tigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zu diesem Zweck bedient er n\u00e4mlich,", "tokens": ["Zu", "die\u00b7sem", "Zweck", "be\u00b7dient", "er", "n\u00e4m\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als f\u00fcr den S\u00fcnder gut bek\u00f6mmlich,", "tokens": ["Als", "f\u00fcr", "den", "S\u00fcn\u00b7der", "gut", "be\u00b7k\u00f6mm\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sich einer schlanken Haselgerte,", "tokens": ["Sich", "ei\u00b7ner", "schlan\u00b7ken", "Ha\u00b7sel\u00b7ger\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zwar biegsam, doch nicht ohne H\u00e4rte,", "tokens": ["Zwar", "bieg\u00b7sam", ",", "doch", "nicht", "oh\u00b7ne", "H\u00e4r\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die sich, von rascher Hand bewegt,", "tokens": ["Die", "sich", ",", "von", "ra\u00b7scher", "Hand", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Geschmeidig um die H\u00fcfte legt.", "tokens": ["Ge\u00b7schmei\u00b7dig", "um", "die", "H\u00fcf\u00b7te", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nur wer es f\u00fchlte, der begreift es:", "tokens": ["Nur", "wer", "es", "f\u00fchl\u00b7te", ",", "der", "be\u00b7greift", "es", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Vorn schl\u00e4gt er zu und hinten kneift es.", "tokens": ["Vorn", "schl\u00e4gt", "er", "zu", "und", "hin\u00b7ten", "kneift", "es", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}