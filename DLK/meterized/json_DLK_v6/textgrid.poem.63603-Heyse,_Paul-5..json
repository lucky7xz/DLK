{"textgrid.poem.63603": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu bitter w\u00e4r' ich? Doch wer hat's verschuldet,", "tokens": ["Zu", "bit\u00b7ter", "w\u00e4r'", "ich", "?", "Doch", "wer", "hat's", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "$.", "KON", "PWS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn meine Zunge trieft von Bitterkeiten?", "tokens": ["Wenn", "mei\u00b7ne", "Zun\u00b7ge", "trieft", "von", "Bit\u00b7ter\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein feiger Knecht, wer ohne Murren duldet!", "tokens": ["Ein", "fei\u00b7ger", "Knecht", ",", "wer", "oh\u00b7ne", "Mur\u00b7ren", "dul\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ward nicht, was S\u00fc\u00dfes ich geno\u00df vorzeiten,", "tokens": ["Ward", "nicht", ",", "was", "S\u00fc\u00b7\u00dfes", "ich", "ge\u00b7no\u00df", "vor\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "PWS", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mir \u00fcber Nacht vergiftet und verg\u00e4llt,", "tokens": ["Mir", "\u00fc\u00b7ber", "Nacht", "ver\u00b7gif\u00b7tet", "und", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Entwertet meine liebsten Kostbarkeiten,", "tokens": ["Ent\u00b7wer\u00b7tet", "mei\u00b7ne", "liebs\u00b7ten", "Kost\u00b7bar\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da, die ich \u00fcber jeden Schatz gestellt,", "tokens": ["Da", ",", "die", "ich", "\u00fc\u00b7ber", "je\u00b7den", "Schatz", "ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die goldne Freiheit, nun als schlimmste B\u00fcrde", "tokens": ["Die", "gold\u00b7ne", "Frei\u00b7heit", ",", "nun", "als", "schlimms\u00b7te", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mir unertr\u00e4glich auf die Seele f\u00e4llt?", "tokens": ["Mir", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "auf", "die", "See\u00b7le", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es wog mir kein Gewinn, noch Ehr' und W\u00fcrde", "tokens": ["Es", "wog", "mir", "kein", "Ge\u00b7winn", ",", "noch", "Ehr'", "und", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Den Adel auf: mir selber zu geh\u00f6ren,", "tokens": ["Den", "A\u00b7del", "auf", ":", "mir", "sel\u00b7ber", "zu", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nicht eingeschr\u00e4nkt in eines Dienstes H\u00fcrde;", "tokens": ["Nicht", "ein\u00b7ge\u00b7schr\u00e4nkt", "in", "ei\u00b7nes", "Diens\u00b7tes", "H\u00fcr\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nicht mit banausischem Gesch\u00e4ft zu st\u00f6ren", "tokens": ["Nicht", "mit", "ba\u00b7nau\u00b7si\u00b7schem", "Ge\u00b7sch\u00e4ft", "zu", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die stille Bildkraft, all die Schmerzenslust,", "tokens": ["Die", "stil\u00b7le", "Bild\u00b7kraft", ",", "all", "die", "Schmer\u00b7zens\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In ernstem Ringen Geister zu beschw\u00f6ren.", "tokens": ["In", "erns\u00b7tem", "Rin\u00b7gen", "Geis\u00b7ter", "zu", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie war ich mir so streng und froh bewu\u00dft,", "tokens": ["Wie", "war", "ich", "mir", "so", "streng", "und", "froh", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df meines Wirkens Ma\u00df und meiner Pflichten", "tokens": ["Da\u00df", "mei\u00b7nes", "Wir\u00b7kens", "Ma\u00df", "und", "mei\u00b7ner", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mir einzig ruht' im Grund der eignen Brust!", "tokens": ["Mir", "ein\u00b7zig", "ruht'", "im", "Grund", "der", "eig\u00b7nen", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und nun \u2013 wie m\u00f6cht' ich gern auf mich verzichten", "tokens": ["Und", "nun", "\u2013", "wie", "m\u00f6cht'", "ich", "gern", "auf", "mich", "ver\u00b7zich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und, fremdem Willen dienstbar untertan,", "tokens": ["Und", ",", "frem\u00b7dem", "Wil\u00b7len", "dienst\u00b7bar", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ein dumpfes Werk gedankenlos verrichten!", "tokens": ["Ein", "dum\u00b7pfes", "Werk", "ge\u00b7dan\u00b7ken\u00b7los", "ver\u00b7rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Den Bauer neid' ich, der in grader Bahn", "tokens": ["Den", "Bau\u00b7er", "neid'", "ich", ",", "der", "in", "gra\u00b7der", "Bahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die Furche zieht, den K\u00e4rrner, der im Staube", "tokens": ["Die", "Fur\u00b7che", "zieht", ",", "den", "K\u00e4rr\u00b7ner", ",", "der", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Des Heerwegs seinen Rossen geht voran.", "tokens": ["Des", "Heer\u00b7wegs", "sei\u00b7nen", "Ros\u00b7sen", "geht", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und jener dort in niedrer Reisiglaube,", "tokens": ["Und", "je\u00b7ner", "dort", "in", "nie\u00b7drer", "Rei\u00b7si\u00b7glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Der Steine klopft, geb\u00fcckt am hei\u00dfen Wege,", "tokens": ["Der", "Stei\u00b7ne", "klopft", ",", "ge\u00b7b\u00fcckt", "am", "hei\u00b7\u00dfen", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Nicht \u00f6dem M\u00fc\u00dfiggang ist er zum Raube.", "tokens": ["Nicht", "\u00f6\u00b7dem", "M\u00fc\u00b7\u00dfig\u00b7gang", "ist", "er", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Sein Tagwerk f\u00f6rdert jeder seiner Schl\u00e4ge,", "tokens": ["Sein", "Tag\u00b7werk", "f\u00f6r\u00b7dert", "je\u00b7der", "sei\u00b7ner", "Schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und, wacker bis zum Feierabend, letzt", "tokens": ["Und", ",", "wa\u00b7cker", "bis", "zum", "Fei\u00b7er\u00b7a\u00b7bend", ",", "letzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "ADJD", "APPR", "APPRART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ihn seine Flasche, wird der Arm ihm tr\u00e4ge.", "tokens": ["Ihn", "sei\u00b7ne", "Fla\u00b7sche", ",", "wird", "der", "Arm", "ihm", "tr\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und ich \u2013 dem selbst der Quell der Musen jetzt,", "tokens": ["Und", "ich", "\u2013", "dem", "selbst", "der", "Quell", "der", "Mu\u00b7sen", "jetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ART", "ADV", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der Himmelstrank, wie schaler Sp\u00fclicht mundet,", "tokens": ["Der", "Him\u00b7mel\u00b7strank", ",", "wie", "scha\u00b7ler", "Sp\u00fc\u00b7licht", "mun\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wo ist ein Werk, ein Ziel, das mich erg\u00f6tzt?", "tokens": ["Wo", "ist", "ein", "Werk", ",", "ein", "Ziel", ",", "das", "mich", "er\u00b7g\u00f6tzt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Denn die Gestalt, die sich dem Dichter rundet,", "tokens": ["Denn", "die", "Ge\u00b7stalt", ",", "die", "sich", "dem", "Dich\u00b7ter", "run\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Soll er beleben mit dem eignen Blut.", "tokens": ["Soll", "er", "be\u00b7le\u00b7ben", "mit", "dem", "eig\u00b7nen", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.36": {"text": "Wie? wenn er selbst nun ward zu Tod verwundet,", "tokens": ["Wie", "?", "wenn", "er", "selbst", "nun", "ward", "zu", "Tod", "ver\u00b7wun\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Da\u00df Lebens\u00fcberflu\u00df und -\u00fcbermut", "tokens": ["Da\u00df", "Le\u00b7bens\u00b7\u00fc\u00b7berf\u00b7lu\u00df", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Versiegen und die Liebe geht verloren,", "tokens": ["Ver\u00b7sie\u00b7gen", "und", "die", "Lie\u00b7be", "geht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Die auch an ", "tokens": ["Die", "auch", "an"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.40": {"text": "So hab' ich selbst mich wider mich verschworen.", "tokens": ["So", "hab'", "ich", "selbst", "mich", "wi\u00b7der", "mich", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Mir selbst geh\u00f6r' ich? Keinen schlimmern Herrn", "tokens": ["Mir", "selbst", "ge\u00b7h\u00f6r'", "ich", "?", "Kei\u00b7nen", "schlim\u00b7mern", "Herrn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und keinen \u00e4rmern h\u00e4tt' ich je erkoren.", "tokens": ["Und", "kei\u00b7nen", "\u00e4r\u00b7mern", "h\u00e4tt'", "ich", "je", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Sein schn\u00f6des Joch absch\u00fctteln m\u00f6cht' ich gern,", "tokens": ["Sein", "schn\u00f6\u00b7des", "Joch", "ab\u00b7sch\u00fct\u00b7teln", "m\u00f6cht'", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Der mich mi\u00dfhandelt, der mich darben l\u00e4\u00dft,", "tokens": ["Der", "mich", "mi\u00df\u00b7han\u00b7delt", ",", "der", "mich", "dar\u00b7ben", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PRELS", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und kann nur knirschend an der Kette zerrn.", "tokens": ["Und", "kann", "nur", "knir\u00b7schend", "an", "der", "Ket\u00b7te", "zerrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "So wird der Freiheit jammervoller Rest", "tokens": ["So", "wird", "der", "Frei\u00b7heit", "jam\u00b7mer\u00b7vol\u00b7ler", "Rest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Mir noch zum Fluch. Wenn unser Wille schwankt,", "tokens": ["Mir", "noch", "zum", "Fluch", ".", "Wenn", "un\u00b7ser", "Wil\u00b7le", "schwankt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$.", "KOUS", "PPOSAT", "NN", "VVFIN", 