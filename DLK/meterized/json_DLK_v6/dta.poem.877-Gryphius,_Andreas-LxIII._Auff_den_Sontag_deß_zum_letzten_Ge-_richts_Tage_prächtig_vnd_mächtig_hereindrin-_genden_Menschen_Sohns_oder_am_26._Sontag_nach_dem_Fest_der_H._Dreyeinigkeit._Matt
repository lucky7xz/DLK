{"dta.poem.877": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LxIII.  Auff den Sontag de\u00df zum letzten Ge-  \n richts Tage pr\u00e4chtig vnd m\u00e4chtig hereindrin-  \n genden Menschen Sohns/ oder am 26. Sontag nach  \n dem Fest der H. Dreyeinigkeit.  Matth.  25.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! was ist die\u00df? mein Hertz ist wund/ ", "tokens": ["Ach", "!", "was", "ist", "die\u00df", "?", "mein", "Hertz", "ist", "wund", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PDS", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Das schrecken schleust mir Zung vnd Mund/", "tokens": ["Das", "schre\u00b7cken", "schleust", "mir", "Zung", "vnd", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sinnen Krafft f\u00e4lt gantz darnieder/", "tokens": ["Der", "Sin\u00b7nen", "Krafft", "f\u00e4lt", "gantz", "dar\u00b7nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir zittern alle meine Glieder.", "tokens": ["Mir", "zit\u00b7tern", "al\u00b7le", "mei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Augen