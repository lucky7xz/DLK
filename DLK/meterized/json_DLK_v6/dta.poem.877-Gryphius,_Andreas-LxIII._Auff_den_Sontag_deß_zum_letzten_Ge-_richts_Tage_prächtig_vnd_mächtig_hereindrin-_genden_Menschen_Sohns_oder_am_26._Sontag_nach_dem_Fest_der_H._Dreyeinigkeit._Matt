1": {"text": "Das kein Tag/ keine Nacht wird schliessen/", "tokens": ["Das", "kein", "Tag", "/", "kei\u00b7ne", "Nacht", "wird", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "PIAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kein Stern wird zu verdecken wissen.", "tokens": ["Kein", "Stern", "wird", "zu", "ver\u00b7de\u00b7cken", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein Wort von Eisen vnd von Stein/", "tokens": ["Ein", "Wort", "von", "Ei\u00b7sen", "vnd", "von", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vnd was noch sonst mag h\u00e4rter seyn!", "tokens": ["Vnd", "was", "noch", "sonst", "mag", "h\u00e4r\u00b7ter", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ist in mein Kranckes Hertze kommen/", "tokens": ["Ist", "in", "mein", "Kran\u00b7ckes", "Hert\u00b7ze", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Hat Marck vnd Bein mir eingenommen.", "tokens": ["Hat", "Marck", "vnd", "Bein", "mir", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die\u00df donner-Wort hei\u00dft Ewigkeit/", "tokens": ["Die\u00df", "don\u00b7ner\u00b7Wort", "hei\u00dft", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "F\u00fcr dem ihr Himmel schwach noch seid/", "tokens": ["F\u00fcr", "dem", "ihr", "Him\u00b7mel", "schwach", "noch", "seid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Obschon ihr ewres Wetters Flammen", "tokens": ["Ob\u00b7schon", "ihr", "ew\u00b7res", "Wet\u00b7ters", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Schlo\u00df/ Plitz vnd Hagel rufft zusammen.", "tokens": ["Schlo\u00df", "/", "Plitz", "vnd", "Ha\u00b7gel", "rufft", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Es ist vnd heisset Ewigkeit/", "tokens": ["Es", "ist", "vnd", "heis\u00b7set", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Das gar kein ziel hat weit vnd breit/", "tokens": ["Das", "gar", "kein", "ziel", "hat", "weit", "vnd", "breit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das die Gem\u00fcther heisset stehen/", "tokens": ["Das", "die", "Ge\u00b7m\u00fc\u00b7ther", "heis\u00b7set", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nach sachen/ welche nicht vergehen.", "tokens": ["Nach", "sa\u00b7chen", "/", "wel\u00b7che", "nicht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$(", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Di\u00df Wort hat K\u00e4yser new gemacht/", "tokens": ["Di\u00df", "Wort", "hat", "K\u00e4y\u00b7ser", "new", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Vnd in geringe Kappen bracht/", "tokens": ["Vnd", "in", "ge\u00b7rin\u00b7ge", "Kap\u00b7pen", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Hat viel gelehret einsamm leben/", "tokens": ["Hat", "viel", "ge\u00b7leh\u00b7ret", "ein\u00b7samm", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "In W\u00fcste h\u00f6len sich begeben;", "tokens": ["In", "W\u00fcs\u00b7te", "h\u00f6\u00b7len", "sich", "be\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Hat tausend Lauren eingeh\u00fcllt", "tokens": ["Hat", "tau\u00b7send", "Lau\u00b7ren", "ein\u00b7ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "CARD", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Vnd tausend Wandren Lust gestillt;", "tokens": ["Vnd", "tau\u00b7send", "Wand\u00b7ren", "Lust", "ge\u00b7stillt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Gekleidet sehr viel Fromme Seelen/", "tokens": ["Ge\u00b7klei\u00b7det", "sehr", "viel", "From\u00b7me", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mit rawen Haaren von Kamelen.", "tokens": ["Mit", "ra\u00b7wen", "Haa\u00b7ren", "von", "Ka\u00b7me\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die Ewigkeit nimmt gantz mich ein/", "tokens": ["Die", "E\u00b7wig\u00b7keit", "nimmt", "gantz", "mich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Macht da\u00df Ich nicht kan ruhig seyn;", "tokens": ["Macht", "da\u00df", "Ich", "nicht", "kan", "ru\u00b7hig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PTKNEG", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.35": {"text": "Die Ewigkeit bringt meinem Hertzen/", "tokens": ["Die", "E\u00b7wig\u00b7keit", "bringt", "mei\u00b7nem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "ein stetes Sorgen/ Furcht vnd Schmertzen.", "tokens": ["ein", "ste\u00b7tes", "Sor\u00b7gen", "/", "Furcht", "vnd", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "O ferres End vnendlich weit/", "tokens": ["O", "fer\u00b7res", "End", "vnend\u00b7lich", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "ADJD", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.38": {"text": "O grose zeit/ ohn alle zeit!", "tokens": ["O", "gro\u00b7se", "zeit", "/", "ohn", "al\u00b7le", "zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "O Jahre! jare nicht zu nennen!", "tokens": ["O", "Jah\u00b7re", "!", "ja\u00b7re", "nicht", "zu", "nen\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "O Anzahl/ die kein Mensch kan kennen!", "tokens": ["O", "An\u00b7zahl", "/", "die", "kein", "Mensch", "kan", "ken\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Steig/ ab/ steig in die Helle hin/", "tokens": ["Steig", "/", "ab", "/", "steig", "in", "die", "Hel\u00b7le", "hin", "/"], "token_info": ["word", "p