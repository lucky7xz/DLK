{"dta.poem.6170": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein C\u00f6rper scheint zuweilen", "tokens": ["Ein", "C\u00f6r\u00b7per", "scheint", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bald mehr gepresst, bald st\u00e4rcker ausgespannt,", "tokens": ["Bald", "mehr", "ge\u00b7presst", ",", "bald", "st\u00e4r\u00b7cker", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da er jedoch in seinen Theilen", "tokens": ["Da", "er", "je\u00b7doch", "in", "sei\u00b7nen", "Thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--++-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nicht zugenommen, nichts verlohren. Der Verstand", "tokens": ["Nicht", "zu\u00b7ge\u00b7nom\u00b7men", ",", "nichts", "ver\u00b7loh\u00b7ren", ".", "Der", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "PIS", "VVPP", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zeigt deutlich, wenn sich offt die C\u00f6rper schnell verbreiten,", "tokens": ["Zeigt", "deut\u00b7lich", ",", "wenn", "sich", "offt", "die", "C\u00f6r\u00b7per", "schnell", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PRF", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und man, es sich verd\u00fcnnen, nennt,", "tokens": ["Und", "man", ",", "es", "sich", "ver\u00b7d\u00fcn\u00b7nen", ",", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PPER", "PRF", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df andre C\u00f6rpergen in ihre Oeffnung gleiten,", "tokens": ["Da\u00df", "and\u00b7re", "C\u00f6r\u00b7per\u00b7gen", "in", "ih\u00b7re", "Oeff\u00b7nung", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wodurch sich alles theilt, und dehnt und trennt.", "tokens": ["Wo\u00b7durch", "sich", "al\u00b7les", "theilt", ",", "und", "dehnt", "und", "trennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}