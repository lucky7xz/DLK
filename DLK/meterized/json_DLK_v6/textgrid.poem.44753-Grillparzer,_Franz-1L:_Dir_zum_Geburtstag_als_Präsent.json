{"textgrid.poem.44753": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir zum Geburtstag als Pr\u00e4sent", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir zum Geburtstag als Pr\u00e4sent", "tokens": ["Dir", "zum", "Ge\u00b7burts\u00b7tag", "als", "Pr\u00e4\u00b7sent"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "KOUS", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sag ich nun nicht mehr: ", "tokens": ["Sag", "ich", "nun", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auch: \u00bbLuder\u00ab will ich nicht mehr sagen;", "tokens": ["Auch", ":", "\u00bb", "Lu\u00b7der", "\u00ab", "will", "ich", "nicht", "mehr", "sa\u00b7gen", ";"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "NN", "$(", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Hund und nicht Verwandte schlagen,", "tokens": ["Nicht", "Hund", "und", "nicht", "Ver\u00b7wand\u00b7te", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zunge fest im Maule tragen,", "tokens": ["Die", "Zun\u00b7ge", "fest", "im", "Mau\u00b7le", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will artig sein mit F\u00fc\u00df und H\u00e4nd:", "tokens": ["Will", "ar\u00b7tig", "sein", "mit", "F\u00fc\u00df", "und", "H\u00e4nd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das ist der Tant Kathel Geburtstags-Pr\u00e4sent.", "tokens": ["Das", "ist", "der", "Tant", "Ka\u00b7thel", "Ge\u00b7burts\u00b7tags\u00b7Pr\u00e4\u00b7sent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}