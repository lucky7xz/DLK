{"dta.poem.9723": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie hatte alle hertzen in der hand.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertze fehlt dir noch/ ob schon du alle hertzen", "tokens": ["Mein", "hert\u00b7ze", "fehlt", "dir", "noch", "/", "ob", "schon", "du", "al\u00b7le", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ADV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deiner engen hand zusammen h\u00e4ltst gefa\u00dft;", "tokens": ["In", "dei\u00b7ner", "en\u00b7gen", "hand", "zu\u00b7sam\u00b7men", "h\u00e4ltst", "ge\u00b7fa\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nein! es fehlt dir nicht/ weil du es ohne schertzen/", "tokens": ["Doch", "nein", "!", "es", "fehlt", "dir", "nicht", "/", "weil", "du", "es", "oh\u00b7ne", "schert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob gleich nicht in der hand/ doch fest im hertzen hast.", "tokens": ["Ob", "gleich", "nicht", "in", "der", "hand", "/", "doch", "fest", "im", "hert\u00b7zen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word",