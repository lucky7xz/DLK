{"textgrid.poem.38929": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! wer seid ihr fremden Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! wer seid ihr fremden Wesen,", "tokens": ["Ach", "!", "wer", "seid", "ihr", "frem\u00b7den", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit Grimm mein Herz zerschneiden?", "tokens": ["Die", "mit", "Grimm", "mein", "Herz", "zer\u00b7schnei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft mich wieder neu genesen,", "tokens": ["La\u00dft", "mich", "wie\u00b7der", "neu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehmt, o nehmt zur\u00fcck die Leiden!", "tokens": ["Nehmt", ",", "o", "nehmt", "zu\u00b7r\u00fcck", "die", "Lei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich meine Zitter spiele,", "tokens": ["Wenn", "ich", "mei\u00b7ne", "Zit\u00b7ter", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kenn' ich ihre T\u00f6ne nicht,", "tokens": ["Kenn'", "ich", "ih\u00b7re", "T\u00f6\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Innre Angst und Schreckgef\u00fchle", "tokens": ["Inn\u00b7re", "Angst", "und", "Schreck\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dunkeln mir der Sonne Licht.", "tokens": ["Dun\u00b7keln", "mir", "der", "Son\u00b7ne", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Liebe scheint dazwischen,", "tokens": ["Und", "die", "Lie\u00b7be", "scheint", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie wenn sie mich nicht mehr kennt,", "tokens": ["Wie", "wenn", "sie", "mich", "nicht", "mehr", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie bei Nacht in gr\u00fcnen B\u00fcschen,", "tokens": ["Wie", "bei", "Nacht", "in", "gr\u00fc\u00b7nen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "R\u00e4thselvolles Mondlicht brennt.", "tokens": ["R\u00e4th\u00b7sel\u00b7vol\u00b7les", "Mond\u00b7licht", "brennt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}