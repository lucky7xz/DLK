{"textgrid.poem.40255": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit zehn Kanonen blank an Bord,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit zehn Kanonen blank an Bord,", "tokens": ["Mit", "zehn", "Ka\u00b7no\u00b7nen", "blank", "an", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit vollen Segeln vor dem Wind,", "tokens": ["mit", "vol\u00b7len", "Se\u00b7geln", "vor", "dem", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die flink wie M\u00f6venfl\u00fcgel sind,", "tokens": ["die", "flink", "wie", "M\u00f6\u00b7ven\u00b7fl\u00fc\u00b7gel", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "streicht eine Barke durch die Flut:", "tokens": ["streicht", "ei\u00b7ne", "Bar\u00b7ke", "durch", "die", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "die Barke des Piratenherrn,", "tokens": ["die", "Bar\u00b7ke", "des", "Pi\u00b7ra\u00b7ten\u00b7herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "auf allen Meeren er gekannt", "tokens": ["auf", "al\u00b7len", "Mee\u00b7ren", "er", "ge\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "von einem bis zum andern Strand,", "tokens": ["von", "ei\u00b7nem", "bis", "zum", "an\u00b7dern", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der \u00bbHai\u00ab getauft f\u00fcr seinen Mut.", "tokens": ["der", "\u00bb", "Hai", "\u00ab", "ge\u00b7tauft", "f\u00fcr", "sei\u00b7nen", "Mut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "$(", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im dunkeln Wasser h\u00fcpft der Mond,", "tokens": ["Im", "dun\u00b7keln", "Was\u00b7ser", "h\u00fcpft", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Tauwerk seufzt und pfeift der Wind,", "tokens": ["im", "Tau\u00b7werk", "seufzt", "und", "pfeift", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein langer Silberstreifen rinnt", "tokens": ["ein", "lan\u00b7ger", "Sil\u00b7ber\u00b7strei\u00b7fen", "rinnt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "breit durch die blaubewegte Flut.", "tokens": ["breit", "durch", "die", "blau\u00b7be\u00b7weg\u00b7te", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der Piratenkapit\u00e4n", "tokens": ["Und", "der", "Pi\u00b7ra\u00b7ten\u00b7ka\u00b7pi\u00b7t\u00e4n"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "sitzt singend hoch an Steuers Rand,", "tokens": ["sitzt", "sin\u00b7gend", "hoch", "an", "Steu\u00b7ers", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "links Asiens, rechts Europens Strand,", "tokens": ["links", "A\u00b7siens", ",", "rechts", "Eu\u00b7ro\u00b7pens", "Strand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und sitzt und singt und schwenkt den Hut:", "tokens": ["und", "sitzt", "und", "singt", "und", "schwenkt", "den", "Hut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbfliege, mein Segler du, fliege,", "tokens": ["\u00bb", "flie\u00b7ge", ",", "mein", "Seg\u00b7ler", "du", ",", "flie\u00b7ge", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "NN", "PPER", "$,", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "unverzagt;", "tokens": ["un\u00b7ver\u00b7zagt", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "fliegst und segelst zum Siege!", "tokens": ["fliegst", "und", "se\u00b7gelst", "zum", "Sie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Spottest der St\u00fcrme, der Klippen und Riffe,", "tokens": ["Spot\u00b7test", "der", "St\u00fcr\u00b7me", ",", "der", "Klip\u00b7pen", "und", "Rif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "der Himmelst\u00fccken, der feindlichen Schiffe,", "tokens": ["der", "Him\u00b7mel\u00b7st\u00fc\u00b7cken", ",", "der", "feind\u00b7li\u00b7chen", "Schif\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "weil dein Herr sein Leben wagt!", "tokens": ["weil", "dein", "Herr", "sein", "Le\u00b7ben", "wagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwanzig Prisen", "tokens": ["Zwan\u00b7zig", "Pri\u00b7sen"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "haben wir gemacht,", "tokens": ["ha\u00b7ben", "wir", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "haben die Staatsm\u00fctzen", "tokens": ["ha\u00b7ben", "die", "Staats\u00b7m\u00fct\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.10": {"text": "ausgelacht;", "tokens": ["aus\u00b7ge\u00b7lacht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "hundert Nationen", "tokens": ["hun\u00b7dert", "Na\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "liegen und gr\u00fc\u00dfen hier", "tokens": ["lie\u00b7gen", "und", "gr\u00fc\u00b7\u00dfen", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "mit ihren Flaggen", "tokens": ["mit", "ih\u00b7ren", "Flag\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "zu F\u00fc\u00dfen mir.", "tokens": ["zu", "F\u00fc\u00b7\u00dfen", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Denn meine Barke ist mein Reichtum,", "tokens": ["Denn", "mei\u00b7ne", "Bar\u00b7ke", "ist", "mein", "Reich\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "denn mein Gesetz ist mein Begehr,", "tokens": ["denn", "mein", "Ge\u00b7setz", "ist", "mein", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "mein Gott der Wind und meine Freiheit,", "tokens": ["mein", "Gott", "der", "Wind", "und", "mei\u00b7ne", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "mein einzig Vaterland das Meer.", "tokens": ["mein", "ein\u00b7zig", "Va\u00b7ter\u00b7land", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6nige streiten dadr\u00fcben", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "strei\u00b7ten", "da\u00b7dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PAV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "in blinder Gier", "tokens": ["in", "blin\u00b7der", "Gier"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "um ein paar Aecker R\u00fcben.", "tokens": ["um", "ein", "paar", "A\u00b7e\u00b7cker", "R\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sehet, ich lache! ", "tokens": ["Se\u00b7het", ",", "ich", "la\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "reichen, soweit das weite wilde", "tokens": ["rei\u00b7chen", ",", "so\u00b7weit", "das", "wei\u00b7te", "wil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Meer entrollt sein frei Pannier.", "tokens": ["Meer", "ent\u00b7rollt", "sein", "frei", "Pan\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da ist kein Wimpel,", "tokens": ["Da", "ist", "kein", "Wim\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "wie er auch gl\u00e4nze,", "tokens": ["wie", "er", "auch", "gl\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "da keine K\u00fcste,", "tokens": ["da", "kei\u00b7ne", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "wo sie auch grenze,", "tokens": ["wo", "sie", "auch", "gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "die nicht Salut gethan", "tokens": ["die", "nicht", "Sa\u00b7lut", "ge\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "meinem Geschlecht,", "tokens": ["mei\u00b7nem", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "die nicht erkannten", "tokens": ["die", "nicht", "er\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PTKNEG", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "mein Hoheitsrecht.", "tokens": ["mein", "Ho\u00b7heits\u00b7recht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Denn meine Barke ist mein Reichtum,", "tokens": ["Denn", "mei\u00b7ne", "Bar\u00b7ke", "ist", "mein", "Reich\u00b7tum", ","], "