: "hoi alle Segel breit, Fersengeldsegel,", "tokens": ["hoi", "al\u00b7le", "Se\u00b7gel", "breit", ",", "Fer\u00b7sen\u00b7geld\u00b7se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$,", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "rennt es und rennt es; denn diese Flegel", "tokens": ["rennt", "es", "und", "rennt", "es", ";", "denn", "die\u00b7se", "Fle\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "$.", "KON", "PDAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "lieben den K\u00f6nig der Meere nicht.", "tokens": ["lie\u00b7ben", "den", "K\u00f6\u00b7nig", "der", "Mee\u00b7re", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber wie Br\u00fcder", "tokens": ["A\u00b7ber", "wie", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ich und Ihr,", "tokens": ["Ich", "und", "Ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "meine Getreuen,", "tokens": ["mei\u00b7ne", "Ge\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "teilen die Beute wir.", "tokens": ["tei\u00b7len", "die", "Beu\u00b7te", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Ein einzig E