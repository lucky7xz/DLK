{"textgrid.poem.48645": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Sonnenschein im Regen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Obschon durch des Himmels Z\u00e4bren", "tokens": ["Ob\u00b7schon", "durch", "des", "Him\u00b7mels", "Z\u00e4b\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieser ganze Tag wird na\u00df,", "tokens": ["die\u00b7ser", "gan\u00b7ze", "Tag", "wird", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "doch so kan mich di\u00df beschweren,", "tokens": ["doch", "so", "kan", "mich", "di\u00df", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "diese Not nicht machen bla\u00df.", "tokens": ["die\u00b7se", "Not", "nicht", "ma\u00b7chen", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "VVINF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieb, ein steter Sonnenschein", "tokens": ["Lieb", ",", "ein", "ste\u00b7ter", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sind mir euer' \u00c4ugelein!", "tokens": ["sind", "mir", "eu\u00b7er'", "\u00c4u\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}