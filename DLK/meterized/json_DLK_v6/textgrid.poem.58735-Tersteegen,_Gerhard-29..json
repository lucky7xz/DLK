{"textgrid.poem.58735": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich beschw\u00f6re euch, da\u00df ihr meine Freundin nicht aufweckt. Hoheslied 3, 5", "tokens": ["Ich", "be\u00b7schw\u00f6\u00b7re", "euch", ",", "da\u00df", "ihr", "mei\u00b7ne", "Freun\u00b7din", "nicht", "auf\u00b7weckt", ".", "Ho\u00b7hes\u00b7lied", "3", ",", "5"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$.", "NN", "CARD", "$,", "CARD"], "meter": "--+--+-+-+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nein, nein, ich f\u00fcrcht' nicht mehr die Welt mit ihrer Macht;", "tokens": ["Nein", ",", "nein", ",", "ich", "f\u00fcrcht'", "nicht", "mehr", "die", "Welt", "mit", "ih\u00b7rer", "Macht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Teufel kann mich nicht verletzen,", "tokens": ["Der", "Teu\u00b7fel", "kann", "mich", "nicht", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch H\u00f6ll', noch Tod in Schrecken setzen,", "tokens": ["Noch", "H\u00f6ll'", ",", "noch", "Tod", "in", "Schre\u00b7cken", "set\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Qual, die Marter ich nicht acht'.", "tokens": ["Die", "Qual", ",", "die", "Mar\u00b7ter", "ich", "nicht", "acht'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich leb' in Sicherheit, in Ruhe schlaf' ich ein,", "tokens": ["Ich", "leb'", "in", "Si\u00b7cher\u00b7heit", ",", "in", "Ru\u00b7he", "schlaf'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe macht mich ohne Kummer,", "tokens": ["Die", "Lie\u00b7be", "macht", "mich", "oh\u00b7ne", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wacht f\u00fcr mich in meinem Schlummer,", "tokens": ["Sie", "wacht", "f\u00fcr", "mich", "in", "mei\u00b7nem", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie macht, da\u00df ich kann sicher sein.", "tokens": ["Sie", "macht", ",", "da\u00df", "ich", "kann", "si\u00b7cher", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df von keiner Sorg', in keiner Furcht ich schweb';", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "Sor\u00b7g'", ",", "in", "kei\u00b7ner", "Furcht", "ich", "schweb'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In unvergleichlich s\u00fc\u00dfem Frieden", "tokens": ["In", "un\u00b7ver\u00b7gleich\u00b7lich", "s\u00fc\u00b7\u00dfem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von B\u00f6s' und Gutem abgeschieden", "tokens": ["Von", "B\u00f6s'", "und", "Gu\u00b7tem", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich in der Welt wie einsam leb'.", "tokens": ["Ich", "in", "der", "Welt", "wie", "ein\u00b7sam", "leb'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Liebe seh' ich nur, ich kenne sie allein,", "tokens": ["Die", "Lie\u00b7be", "seh'", "ich", "nur", ",", "ich", "ken\u00b7ne", "sie", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin wie fremd zu allem Werke,", "tokens": ["Ich", "bin", "wie", "fremd", "zu", "al\u00b7lem", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie meine St\u00fctze ist und St\u00e4rke,", "tokens": ["Sie", "mei\u00b7ne", "St\u00fct\u00b7ze", "ist", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie 'leichtert alle Last und Pein.", "tokens": ["Sie", "'leich\u00b7tert", "al\u00b7le", "Last", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}