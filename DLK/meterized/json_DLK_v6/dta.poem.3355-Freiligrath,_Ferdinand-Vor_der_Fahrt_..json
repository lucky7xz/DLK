{"dta.poem.3355": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Vor der Fahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1846", "urn": "urn:nbn:de:kobv:b4-200905191464", "language": ["de:0.99"], "booktitle": "Freiligrath, Ferdinand: \u00c7a ira! Herisau, 1846."}, "poem": {"stanza.1": {"line.1": {"text": "Jenseits der grauen Wasserw\u00fcste", "tokens": ["Jen\u00b7seits", "der", "grau\u00b7en", "Was\u00b7ser\u00b7w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie liegt die Zukunft winkend da!", "tokens": ["Wie", "liegt", "die", "Zu\u00b7kunft", "win\u00b7kend", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eine gr\u00fcne lachende K\u00fcste,", "tokens": ["Ei\u00b7ne", "gr\u00fc\u00b7ne", "la\u00b7chen\u00b7de", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein geahndet Amerika!", "tokens": ["Ein", "ge\u00b7ahn\u00b7det", "A\u00b7me\u00b7ri\u00b7ka", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVPP", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein geahndet Amerika!", "tokens": ["Ein", "ge\u00b7ahn\u00b7det", "A\u00b7me\u00b7ri\u00b7ka", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVPP", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und ob auch hoch die Wasser springen,", "tokens": ["Und", "ob", "auch", "hoch", "die", "Was\u00b7ser", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ob auch Sandbank uns droht und Riff:", "tokens": ["Ob", "auch", "Sand\u00b7bank", "uns", "droht", "und", "Riff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ein erprobt und verwegen Schiff", "tokens": ["Ein", "er\u00b7probt", "und", "ver\u00b7we\u00b7gen", "Schiff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVPP", "KON", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Wird die Muth\u2019gen hin\u00fcberbringen!", "tokens": ["Wird", "die", "Muth'\u00b7gen", "hin\u00b7\u00fc\u00b7berb\u00b7rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Frisch auf denn, springt hinein! Frisch auf, das", "tokens": ["Frisch", "auf", "denn", ",", "springt", "hin\u00b7ein", "!", "Frisch", "auf", ",", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ADV", "$,", "VVFIN", "PTKVZ", "$.", "NN", "PTKVZ", "$,", "PRELS"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sto\u00dft ab! Sto\u00dft ab! K\u00fchn durch den Sturm!", "tokens": ["Sto\u00dft", "ab", "!", "Sto\u00dft", "ab", "!", "K\u00fchn", "durch", "den", "Sturm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O tapfer Fahrzeug! Ohne Schwanken", "tokens": ["O", "tap\u00b7fer", "Fahr\u00b7zeug", "!", "Oh\u00b7ne", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bef\u00e4hrt es dreist die zorn\u2019ge Fluth!", "tokens": ["Be\u00b7f\u00e4hrt", "es", "dreist", "die", "zorn'\u00b7ge", "Fluth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwarz die Masten und schwarz die Planken,", "tokens": ["Schwarz", "die", "Mas\u00b7ten", "und", "schwarz", "die", "Plan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und die Wimpel sind roth wie Blut!", "tokens": ["Und", "die", "Wim\u00b7pel", "sind", "roth", "wie", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und die Wimpel sind roth wie Blut!", "tokens": ["Und", "die", "Wim\u00b7pel", "sind", "roth", "wie", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die Segel braun von Dampf und Feuer;", "tokens": ["Die", "Se\u00b7gel", "braun", "von", "Dampf", "und", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Verdeck herab ihren Blitz", "tokens": ["Vom", "Ver\u00b7deck", "her\u00b7ab", "ih\u00b7ren", "Blitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PPOSAT", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Spr\u00fchn Gewehre, spr\u00fcht das Gesch\u00fctz,", "tokens": ["Spr\u00fchn", "Ge\u00b7weh\u00b7re", ",", "spr\u00fcht", "das", "Ge\u00b7sch\u00fctz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und das blanke Schwert ist sein Steuer!", "tokens": ["Und", "das", "blan\u00b7ke", "Schwert", "ist", "sein", "Steu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Frisch auf denn, springt hinein! Frisch auf, das", "tokens": ["Frisch", "auf", "denn", ",", "springt", "hin\u00b7ein", "!", "Frisch", "auf", ",", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ADV", "$,", "VVFIN", "PTKVZ", "$.", "NN", "PTKVZ", "$,", "PRELS"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sto\u00dft ab! Sto\u00dft ab! K\u00fchn durch den Sturm!", "tokens": ["Sto\u00dft", "ab", "!", "Sto\u00dft", "ab", "!", "K\u00fchn", "durch", "den", "Sturm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So f\u00e4hrt es aus zu seinen Reisen,", "tokens": ["So", "f\u00e4hrt", "es", "aus", "zu", "sei\u00b7nen", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So tr\u00e4gt es M\u00e4nner in den Streit: \u2014", "tokens": ["So", "tr\u00e4gt", "es", "M\u00e4n\u00b7ner", "in", "den", "Streit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit den Helden haben die Weisen", "tokens": ["Mit", "den", "Hel\u00b7den", "ha\u00b7ben", "die", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Seine dunkeln Borde geweiht!", "tokens": ["Sei\u00b7ne", "dun\u00b7keln", "Bor\u00b7de", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Seine dunkeln Borde geweiht!", "tokens": ["Sei\u00b7ne", "dun\u00b7keln", "Bor\u00b7de", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ha, wie Kosciuszko dreist es f\u00fchrte!", "tokens": ["Ha", ",", "wie", "Ko\u00b7sciusz\u00b7ko", "dreist", "es", "f\u00fchr\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "NE", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ha, wie Washington es gelenkt!", "tokens": ["Ha", ",", "wie", "Was\u00b7hing\u00b7ton", "es", "ge\u00b7lenkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "NE", "PPER", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Lafayette\u2019s und Franklin\u2019s denkt,", "tokens": ["Lafa\u00b7yet\u00b7te's", "und", "Fran\u00b7klin's", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wer sonst seine Flammen sch\u00fcrte!", "tokens": ["Und", "wer", "sonst", "sei\u00b7ne", "Flam\u00b7men", "sch\u00fcr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Frisch auf denn, springt hinein! Frisch auf, das", "tokens": ["Frisch", "auf", "denn", ",", "springt", "hin\u00b7ein", "!", "Frisch", "auf", ",", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ADV", "$,", "VVFIN", "PTKVZ", "$.", "NN", "PTKVZ", "$,", "PRELS"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sto\u00dft ab! Sto\u00dft ab! K\u00fchn durch den Sturm!", "tokens": ["Sto\u00dft", "ab", "!", "Sto\u00dft", "ab", "!", "K\u00fchn", "durch", "den", "Sturm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr fragt erstaunt: Wie mag es hei\u00dfen?", "tokens": ["Ihr", "fragt", "er\u00b7staunt", ":", "Wie", "mag", "es", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Antwort ist mit festem Ton:", "tokens": ["Die", "Ant\u00b7wort", "ist", "mit", "fes\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie in Oesterreich so in Preu\u00dfen", "tokens": ["Wie", "in", "O\u00b7es\u00b7ter\u00b7reich", "so", "in", "Preu\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADV", "APPR", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Hei\u00dft das Schiff: \u201eRevolution!\u201c", "tokens": ["Hei\u00dft", "das", "Schiff", ":", "\u201e", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Hei\u00dft das Schiff: \u201eRevolution!\u201c", "tokens": ["Hei\u00dft", "das", "Schiff", ":", "\u201e", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Es ist die einz\u2019ge richt\u2019ge F\u00e4hre \u2014", "tokens": ["Es", "ist", "die", "einz'\u00b7ge", "richt'\u00b7ge", "F\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum in See, du kecker Pirat!", "tokens": ["Drum", "in", "See", ",", "du", "ke\u00b7cker", "Pi\u00b7rat", "!"], "token_info": ["word