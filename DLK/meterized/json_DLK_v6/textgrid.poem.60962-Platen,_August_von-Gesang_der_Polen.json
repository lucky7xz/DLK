{"textgrid.poem.60962": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Polen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4chtiger, der du als Emp\u00f6rer", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", ",", "der", "du", "als", "Em\u00b7p\u00f6\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "KOUS", "NN"], "meter": "+---++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Uns verdammst, und weit und breit", "tokens": ["Uns", "ver\u00b7dammst", ",", "und", "weit", "und", "breit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrger sammelst und Zerst\u00f6rer,", "tokens": ["W\u00fcr\u00b7ger", "sam\u00b7melst", "und", "Zer\u00b7st\u00f6\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Heischend Unterw\u00fcrfigkeit:", "tokens": ["Hei\u00b7schend", "Un\u00b7ter\u00b7w\u00fcr\u00b7fig\u00b7keit", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine heiligen Herrscherrechte,", "tokens": ["Dei\u00b7ne", "hei\u00b7li\u00b7gen", "Herr\u00b7scher\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Legst du nicht zuvor sie dar?", "tokens": ["Legst", "du", "nicht", "zu\u00b7vor", "sie", "dar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind wir wirklich deine Knechte,", "tokens": ["Sind", "wir", "wirk\u00b7lich", "dei\u00b7ne", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind wir deine Sklaven, Zar?", "tokens": ["Sind", "wir", "dei\u00b7ne", "Skla\u00b7ven", ",", "Zar", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4hnst du so die Schuld zu s\u00fchnen,", "tokens": ["W\u00e4hnst", "du", "so", "die", "Schuld", "zu", "s\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die an uns, o Autokrat,", "tokens": ["Die", "an", "uns", ",", "o", "Au\u00b7to\u00b7krat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den, ach! vergeblich K\u00fchnen", "tokens": ["An", "den", ",", "ach", "!", "ver\u00b7geb\u00b7lich", "K\u00fch\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "ITJ", "$.", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Frau begangen hat?", "tokens": ["Je\u00b7ne", "Frau", "be\u00b7gan\u00b7gen", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil wir innern Streit gefristet,", "tokens": ["Weil", "wir", "in\u00b7nern", "Streit", "ge\u00b7fris\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher stets Verderben kocht,", "tokens": ["Wel\u00b7cher", "stets", "Ver\u00b7der\u00b7ben", "kocht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat ein Weib uns \u00fcberlistet,", "tokens": ["Hat", "ein", "Weib", "uns", "\u00fc\u00b7berl\u00b7is\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat ein Weib uns unterjocht.", "tokens": ["Hat", "ein", "Weib", "uns", "un\u00b7ter\u00b7jocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sendend ihre Mordgesellen,", "tokens": ["Sen\u00b7dend", "ih\u00b7re", "Mord\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die geschlachtet Alt und Jung,", "tokens": ["Die", "ge\u00b7schlach\u00b7tet", "Alt", "und", "Jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df sie mit Gesch\u00fctz umstellen", "tokens": ["Lie\u00df", "sie", "mit", "Ge\u00b7sch\u00fctz", "um\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Reichsversammelung!", "tokens": ["Uns\u00b7re", "Reichs\u00b7ver\u00b7sam\u00b7me\u00b7lung", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Schweigend sa\u00dfen unsre V\u00e4ter", "tokens": ["Schwei\u00b7gend", "sa\u00b7\u00dfen", "uns\u00b7re", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem ringsbedrohten Haus:", "tokens": ["In", "dem", "rings\u00b7be\u00b7droh\u00b7ten", "Haus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sei es fr\u00fcher, sei es sp\u00e4ter,", "tokens": ["Sei", "es", "fr\u00fc\u00b7her", ",", "sei", "es", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rache sann sich jeder aus!", "tokens": ["Ra\u00b7che", "sann", "sich", "je\u00b7der", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Br\u00fcder, kommt, es sei versammelt", "tokens": ["Br\u00fc\u00b7der", ",", "kommt", ",", "es", "sei", "ver\u00b7sam\u00b7melt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Alter, jeder Stand,", "tokens": ["Je\u00b7des", "Al\u00b7ter", ",", "je\u00b7der", "Stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder, dessen Lippe stammelt", "tokens": ["Je\u00b7der", ",", "des\u00b7sen", "Lip\u00b7pe", "stam\u00b7melt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PRELAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Namen, Vaterland!", "tokens": ["Dei\u00b7nen", "Na\u00b7men", ",", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei's, da\u00df unsres Rechts Verp\u00f6ner", "tokens": ["Sei's", ",", "da\u00df", "uns\u00b7res", "Rechts", "Ver\u00b7p\u00f6\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tausend \u00fcber Tausend wirbt:", "tokens": ["Tau\u00b7send", "\u00fc\u00b7ber", "Tau\u00b7send", "wirbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "CARD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stirbt sich's nicht in Waffen sch\u00f6ner,", "tokens": ["Stirbt", "sich's", "nicht", "in", "Waf\u00b7fen", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als sich's auf der Folter stirbt?", "tokens": ["Als", "sich's", "auf", "der", "Fol\u00b7ter", "stirbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weil der Docht nicht ewig lodert,", "tokens": ["Weil", "der", "Docht", "nicht", "e\u00b7wig", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ein Gott dem Menschen gab,", "tokens": ["Den", "ein", "Gott", "dem", "Men\u00b7schen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ja Poniatowski modert,", "tokens": ["Weil", "ja", "Po\u00b7ni\u00b7a\u00b7tows\u00b7ki", "mo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil Kosciusko liegt im Grab,", "tokens": ["Weil", "Ko\u00b7scius\u00b7ko", "liegt", "im", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "M\u00f6chten wir, wie jene, trinken", "tokens": ["M\u00f6ch\u00b7ten", "wir", ",", "wie", "je\u00b7ne", ",", "trin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PDS", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhm noch aus dem Kelch der Not:", "tokens": ["Ruhm", "noch", "aus", "dem", "Kelch", "der", "Not", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll der Freiheit Sonne sinken,", "tokens": ["Soll", "der", "Frei\u00b7heit", "Son\u00b7ne", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Folgt ein langes Abendrot!", "tokens": ["Folgt", "ein", "lan\u00b7ges", "A\u00b7ben\u00b7drot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Deutsches Volk, das kalt und m\u00fc\u00dfig", "tokens": ["Deut\u00b7sches", "Volk", ",", "das", "kalt", "und", "m\u00fc\u00b7\u00dfig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsern Untergang beschaut,", "tokens": ["Un\u00b7sern", "Un\u00b7ter\u00b7gang", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mache deine Seele fl\u00fcssig,", "tokens": ["Ma\u00b7che", "dei\u00b7ne", "See\u00b7le", "fl\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Eis noch nie getaut!", "tokens": ["De\u00b7ren", "Eis", "noch", "nie", "ge\u00b7taut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines eignen Reichs Ruine", "tokens": ["Dei\u00b7nes", "eig\u00b7nen", "Reichs", "Ru\u00b7i\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcrzte bald dem unsern nach;", "tokens": ["St\u00fcrz\u00b7te", "bald", "dem", "un\u00b7sern", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine k\u00fcnftige Katharine", "tokens": ["Ei\u00b7ne", "k\u00fcnf\u00b7ti\u00b7ge", "Ka\u00b7tha\u00b7ri\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wird vollenden deine Schmach!", "tokens": ["Wird", "voll\u00b7en\u00b7den", "dei\u00b7ne", "Schmach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sei dem Leben hold der Feige;", "tokens": ["Sei", "dem", "Le\u00b7ben", "hold", "der", "Fei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wer den Tod begehrt,", "tokens": ["A\u00b7ber", "wer", "den", "Tod", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flicht mit Mut Zypressenzweige", "tokens": ["Flicht", "mit", "Mut", "Zyp\u00b7res\u00b7sen\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "NE"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Um das vaterl\u00e4ndische Schwert.", "tokens": ["Um", "das", "va\u00b7ter\u00b7l\u00e4n\u00b7di\u00b7sche", "Schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "W\u00e4hrend unsre Feinde schie\u00dfen,", "tokens": ["W\u00e4h\u00b7rend", "uns\u00b7re", "Fein\u00b7de", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werden froh wir sein und frei;", "tokens": ["Wer\u00b7den", "froh", "wir", "sein", "und", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VAINF", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hrend unsre Wunden flie\u00dfen,", "tokens": ["W\u00e4h\u00b7rend", "uns\u00b7re", "Wun\u00b7den", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jubelt unser Schlachtgeschrei!", "tokens": ["Ju\u00b7belt", "un\u00b7ser", "Schlacht\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber als vor jenem Throne", "tokens": ["A\u00b7ber", "als", "vor", "je\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefgeb\u00fcckt wir uns gestr\u00e4ubt,", "tokens": ["Tief\u00b7ge\u00b7b\u00fcckt", "wir", "uns", "ge\u00b7str\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets belauert durch Spione,", "tokens": ["Stets", "be\u00b7lau\u00b7ert", "durch", "Spi\u00b7o\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+++-", "measur