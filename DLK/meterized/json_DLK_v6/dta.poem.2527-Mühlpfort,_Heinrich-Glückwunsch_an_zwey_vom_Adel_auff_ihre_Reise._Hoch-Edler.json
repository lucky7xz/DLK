{"dta.poem.2527": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckwunsch an zwey vom Adel auff ihre  \n Reise.  \n Hoch-Edler/", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deinen Brief hab ich zurecht empfangrnempfangen/", "tokens": ["Dei\u00b7nen", "Brief", "hab", "ich", "zu\u00b7recht", "emp\u00b7fan\u00b7grnem\u00b7pfan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKVZ", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Es hat das reine Blatt dein reines Hertz entdeckt/", "tokens": ["Es", "hat", "das", "rei\u00b7ne", "Blatt", "dein", "rei\u00b7nes", "Hertz", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie unser Freundschafft-Licht im finstrem nicht ver-", "tokens": ["Wie", "un\u00b7ser", "Freund\u00b7schafft\u00b7Licht", "im", "fins\u00b7trem", "nicht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "ADJA", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie dein beflammter Sinn der Liebe Feur erweckt.", "tokens": ["Wie", "dein", "be\u00b7flamm\u00b7ter", "Sinn", "der", "Lie\u00b7be", "Feur", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der einig-liebe Brief war voller Liebligkeiten/", "tokens": ["Der", "ei\u00b7nig\u00b7lie\u00b7be", "Brief", "war", "vol\u00b7ler", "Lieb\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man sah die Gratien auff jeder Seiten stehn/", "tokens": ["Man", "sah", "die", "Gra\u00b7ti\u00b7en", "auff", "je\u00b7der", "Sei\u00b7ten", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der R\u00f6mer darff nicht mehr mit unsren Deutschen streiten/", "tokens": ["Der", "R\u00f6\u00b7mer", "darff", "nicht", "mehr", "mit", "un\u00b7sren", "Deut\u00b7schen", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dein Red-Art zeugte schon da\u00df du ihm vor-solst gehen.", "tokens": ["Dein", "Re\u00b7dArt", "zeug\u00b7te", "schon", "da\u00df", "du", "ihm", "vor\u00b7solst", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur eines lob\u2019 ich nicht; da\u00df du mich loben wollen;", "tokens": ["Nur", "ei\u00b7nes", "lob'", "ich", "nicht", ";", "da\u00df", "du", "mich", "lo\u00b7ben", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Ruhm besch\u00e4met mich; ich wei\u00df wie schlecht ich bin;", "tokens": ["Dein", "Ruhm", "be\u00b7sch\u00e4\u00b7met", "mich", ";", "ich", "wei\u00df", "wie", "schlecht", "ich", "bin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "KOKOM", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich h\u00e4tte freylich mehr au\u00df Pflicht-Schuld leisten sollen/", "tokens": ["Ich", "h\u00e4t\u00b7te", "frey\u00b7lich", "mehr", "au\u00df", "Pflicht\u00b7Schuld", "leis\u00b7ten", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Kr\u00e4ffte mangeln mir/ doch aber nicht der Sinn.", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "man\u00b7geln", "mir", "/", "doch", "a\u00b7ber", "nicht", "der", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ADV", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du sihst au\u00df diesem Brieff/ wie ich so alber fchreibe/", "tokens": ["Du", "sihst", "au\u00df", "die\u00b7sem", "Brieff", "/", "wie", "ich", "so", "al\u00b7ber", "fchrei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$(", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie alle Zierlichkeit von meinen Reimen fleucht/", "tokens": ["Wie", "al\u00b7le", "Zier\u00b7lich\u00b7keit", "von", "mei\u00b7nen", "Rei\u00b7men", "fleucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil ich die Poesie nicht mehr so br\u00fcnstig treibe/", "tokens": ["Weil", "ich", "die", "Poe\u00b7sie", "nicht", "mehr", "so", "br\u00fcns\u00b7tig", "trei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "So sp\u00fchrt man Sonnenklar/ da\u00df Ph\u00f6bus von mir weicht.", "tokens": ["So", "sp\u00fchrt", "man", "Son\u00b7nen\u00b7klar", "/", "da\u00df", "Ph\u00f6\u00b7bus", "von", "mir", "weicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$(", "KOUS", "NE", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wolte sonst ein Lied zum Angedencken setzen/", "tokens": ["Ich", "wol\u00b7te", "sonst", "ein", "Lied", "zum", "An\u00b7ge\u00b7den\u00b7cken", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und beyder Br\u00fcder Ruhm den Sternen machen kund;", "tokens": ["Und", "bey\u00b7der", "Br\u00fc\u00b7der", "Ruhm", "den", "Ster\u00b7nen", "ma\u00b7chen", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So/ weil die Musen nicht mich wollen mehr ergetzen.", "tokens": ["So", "/", "weil", "die", "Mu\u00b7sen", "nicht", "mich", "wol\u00b7len", "mehr", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "PTKNEG", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So nimm das Hertze hin/ nicht den beredten Mund.", "tokens": ["So", "nimm", "das", "Hert\u00b7ze", "hin", "/", "nicht", "den", "be\u00b7red\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "VVFIN", "PTKVZ", "$(", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Rei\u00df\u2019 immer wohlbegl\u00fcckt du Zwey Gestirn im Adel/", "tokens": ["Rei\u00df'", "im\u00b7mer", "wohl\u00b7be\u00b7gl\u00fcckt", "du", "Zwey", "Ge\u00b7stirn", "im", "A\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "CARD", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ziere deinen Stamm/ dem nichts an W\u00fcrden gleich/", "tokens": ["Und", "zie\u00b7re", "dei\u00b7nen", "Stamm", "/", "dem", "nichts", "an", "W\u00fcr\u00b7den", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "ART", "PIS", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Ahnen ewig Lob und Leben sonder tadel", "tokens": ["Der", "Ah\u00b7nen", "e\u00b7wig", "Lob", "und", "Le\u00b7ben", "son\u00b7der", "ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Schl\u00e4gt alle Mi\u00dfgunst weg/ und macht den Neider bleich.", "tokens": ["Schl\u00e4gt", "al\u00b7le", "Mi\u00df\u00b7gunst", "weg", "/", "und", "macht", "den", "Nei\u00b7der", "bleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Zunder wird dich auch du werthes Paar anz\u00fcnden/", "tokens": ["Der", "Zun\u00b7der", "wird", "dich", "auch", "du", "wert\u00b7hes", "Paar", "an\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PPER", "ADJA", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der angebohrne Muth zu Thaten muntern auff/", "tokens": ["Der", "an\u00b7ge\u00b7bohr\u00b7ne", "Muth", "zu", "Tha\u00b7ten", "mun\u00b7tern", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du wirst Gelegenheit zu grossen Dingen finden/", "tokens": ["Du", "wirst", "Ge\u00b7le\u00b7gen\u00b7heit", "zu", "gros\u00b7sen", "Din\u00b7gen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Je sch\u00f6ner ist der Lohn/ je weiter ist der Lauff/", "tokens": ["Je", "sch\u00f6\u00b7ner", "ist", "der", "Lohn", "/", "je", "wei\u00b7ter", "ist", "der", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$(", "ADV", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir k\u00f6nnen nichts von dir als gute Fr\u00fcchte hoffen/", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nichts", "von", "dir", "als", "gu\u00b7te", "Fr\u00fcch\u00b7te", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "PPER", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil sich die Bl\u00fcthe schon in solcher Anmuth wie\u00df/", "tokens": ["Weil", "sich", "die", "Bl\u00fc\u00b7the", "schon", "in", "sol\u00b7cher", "An\u00b7muth", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weil Adel/ Witz und Kunst bey dir zusammen troffen/", "tokens": ["Weil", "A\u00b7del", "/", "Witz", "und", "Kunst", "bey", "dir", "zu\u00b7sam\u00b7men", "trof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und die Vollkommenheit sich scheinbar sehen lie\u00df.", "tokens": ["Und", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "sich", "schein\u00b7bar", "se\u00b7hen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Morgenr\u00f6the wil den Tag/ der folget/ zeigen/", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "wil", "den", "Tag", "/", "der", "fol\u00b7get", "/", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$(", "ART", "VVFIN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Wiegen sagen offt den Baur und F\u00fcrsten an/", "tokens": ["Die", "Wie\u00b7gen", "sa\u00b7gen", "offt", "den", "Baur", "und", "F\u00fcrs\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wird nur die Jugend nicht nach Ruhm und Ehren steigen/", "tokens": ["Wird", "nur", "die", "Ju\u00b7gend", "nicht", "nach", "Ruhm", "und", "Eh\u00b7ren", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So ists im Alter wol umb W\u00fcrd und Pracht gethan.", "tokens": ["So", "ists", "im", "Al\u00b7ter", "wol", "umb", "W\u00fcrd", "und", "Pracht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zu dem so darffstu nicht erst Ehren-Bilder suchen", "tokens": ["Zu", "dem", "so", "darffs\u00b7tu", "nicht", "erst", "Eh\u00b7ren\u00b7Bil\u00b7der", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "PAV", "PTKNEG", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Schau Vat- und Vettern an/ und ihren hohen Stand/", "tokens": ["Schau", "Vat", "und", "Vet\u00b7tern", "an", "/", "und", "ih\u00b7ren", "ho\u00b7hen", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "KON", "NN", "PTKVZ", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie haben schon die Bahn/ die du itzt gehst/ gebrochen/", "tokens": ["Sie", "ha\u00b7ben", "schon", "die", "Bahn", "/", "die", "du", "itzt", "gehst", "/", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der Degen und der Kiel macht sie der Welt bekand.", "tokens": ["Der", "De\u00b7gen", "und", "der", "Kiel", "macht", "sie", "der", "Welt", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "l