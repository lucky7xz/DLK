, "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Was w\u00fcntsch ich endlich dann/ nichts als nur langes leben/", "tokens": ["Was", "w\u00fcnt\u00b7sch", "ich", "end\u00b7lich", "dann", "/", "nichts", "als", "nur", "lan\u00b7ges", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$(", "PIS", "KOKOM", "ADV", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.74": {"text": "Den Eltern und auch dir/ so steht schon alles wohl/", "tokens": ["Den", "El\u00b7tern", "und", "auch", "dir", "/", "so", "steht", "schon", "al\u00b7les", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "PPER", "$(", "ADV", "VVFIN", "ADV", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Du wirst noch Schlesien mit neuem Glantz umbgeben/", "tokens": ["Du", "wirst", "noch", "Schle\u00b7si\u00b7en", "mit", "neu\u00b7em", "Glantz", "umb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Das werthe Schlesien/ das schier verfallen soll.", "tokens": ["Das", "wert\u00b7he", "Schle\u00b7si\u00b7en", "/", "das", "schier", "ver\u00b7fal\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$(", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Hoch-Edles Zweygestirn erschein\u2019 uns auch denn wieder/", "tokens": ["Hoch\u00b7Ed\u00b7les", "Zwey\u00b7ges\u00b7tirn", "er\u00b7sc