{"textgrid.poem.62608": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Herzog von St. Crux", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herzog stritte mit den Mohren,", "tokens": ["Der", "Her\u00b7zog", "strit\u00b7te", "mit", "den", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nahme Oran gl\u00fccklich ein;", "tokens": ["Und", "nah\u00b7me", "O\u00b7ran", "gl\u00fcck\u00b7lich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch gieng daselbst sein Lebens-Licht verlohren.", "tokens": ["Doch", "gieng", "da\u00b7selbst", "sein", "Le\u00b7bens\u00b7Licht", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein tapfrer Kriegs- und Sieges-Held,", "tokens": ["Ein", "tapf\u00b7rer", "Kriegs", "und", "Sie\u00b7ges\u00b7Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Grand von Spanien, ein k\u00fchner Ajax f\u00e4llt.", "tokens": ["Ein", "Grand", "von", "Spa\u00b7ni\u00b7en", ",", "ein", "k\u00fch\u00b7ner", "A\u00b7jax", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die mit dem Gl\u00fcck gekr\u00f6nte Nacht", "tokens": ["Die", "mit", "dem", "Gl\u00fcck", "ge\u00b7kr\u00f6n\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat diesen Sieges-F\u00fcrst ums Leben bracht.", "tokens": ["Hat", "die\u00b7sen", "Sie\u00b7ges\u00b7F\u00fcrst", "ums", "Le\u00b7ben", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schlagt Mohren! schlagt auf diesen Prinzen lo\u00df!", "tokens": ["Schlagt", "Moh\u00b7ren", "!", "schlagt", "auf", "die\u00b7sen", "Prin\u00b7zen", "lo\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Genug sein Sieg und seine Ehr ist gro\u00df.", "tokens": ["Ge\u00b7nug", "sein", "Sieg", "und", "sei\u00b7ne", "Ehr", "ist", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Man netzet zwar mit Thr\u00e4nen seine Gruft;", "tokens": ["Man", "net\u00b7zet", "zwar", "mit", "Thr\u00e4\u00b7nen", "sei\u00b7ne", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch schreibt man Ihn ins Buch der Helden:", "tokens": ["Doch", "schreibt", "man", "Ihn", "ins", "Buch", "der", "Hel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wer so wie du, o General gestorben,", "tokens": ["Wer", "so", "wie", "du", ",", "o", "Ge\u00b7ne\u00b7ral", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOKOM", "PPER", "$,", "FM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der hat sich die Unsterblichkeit erworben.", "tokens": ["Der", "hat", "sich", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}