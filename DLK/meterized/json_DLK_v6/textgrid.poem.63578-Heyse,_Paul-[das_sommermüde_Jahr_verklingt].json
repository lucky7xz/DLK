{"textgrid.poem.63578": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[das sommerm\u00fcde Jahr verklingt]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das sommerm\u00fcde Jahr verklingt.", "tokens": ["Das", "som\u00b7mer\u00b7m\u00fc\u00b7de", "Jahr", "ver\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im kahlen Wald kein Vogel singt,", "tokens": ["Im", "kah\u00b7len", "Wald", "kein", "Vo\u00b7gel", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wind saust \u00fcber die Heide.", "tokens": ["Der", "Wind", "saust", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Feuerlein ist im Kamin entfacht,", "tokens": ["Ein", "Feu\u00b7er\u00b7lein", "ist", "im", "Ka\u00b7min", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da singen wir sacht,", "tokens": ["Da", "sin\u00b7gen", "wir", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mein Herz und die Flamme, wir beide.", "tokens": ["Mein", "Herz", "und", "die", "Flam\u00b7me", ",", "wir", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "$,", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Keine Lilie mehr, keine Ros' im Beet,", "tokens": ["Kei\u00b7ne", "Li\u00b7lie", "mehr", ",", "kei\u00b7ne", "Ros'", "im", "Beet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Korb voll Trauben am Fenster steht,", "tokens": ["Ein", "Korb", "voll", "Trau\u00b7ben", "am", "Fens\u00b7ter", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "S\u00fc\u00dffeurig im purpurnen Kleide.", "tokens": ["S\u00fc\u00df\u00b7feu\u00b7rig", "im", "pur\u00b7pur\u00b7nen", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich spr\u00fche den Saft in die durstige Glut,", "tokens": ["Ich", "spr\u00fc\u00b7he", "den", "Saft", "in", "die", "durs\u00b7ti\u00b7ge", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nun flackern wir gut,", "tokens": ["Nun", "fla\u00b7ckern", "wir", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mein Herz und die Flamme, wir beide.", "tokens": ["Mein", "Herz", "und", "die", "Flam\u00b7me", ",", "wir", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "$,", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Meine Liebste kommt, zu teilen den Schmaus,", "tokens": ["Mei\u00b7ne", "Liebs\u00b7te", "kommt", ",", "zu", "tei\u00b7len", "den", "Schmaus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der Mond glimmt \u00fcber die Wipfel hinaus,", "tokens": ["Der", "Mond", "glimmt", "\u00fc\u00b7ber", "die", "Wip\u00b7fel", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sieht unsere Lust mit Neide.", "tokens": ["Sieht", "un\u00b7se\u00b7re", "Lust", "mit", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Feuer verlischt, wir schauen ihm zu,", "tokens": ["Das", "Feu\u00b7er", "ver\u00b7lischt", ",", "wir", "schau\u00b7en", "ihm", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann finden wir Ruh',", "tokens": ["Dann", "fin\u00b7den", "wir", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mein Herz und die Flamme, wir beide.", "tokens": ["Mein", "Herz", "und", "die", "Flam\u00b7me", ",", "wir", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "$,", "PPER", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}