{"dta.poem.23348": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seehandlung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht, wir wechseln, leihen, borgen;", "tokens": ["Seht", ",", "wir", "wech\u00b7seln", ",", "lei\u00b7hen", ",", "bor\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht, wir schaffen, m\u00fchn und sorgen,", "tokens": ["Seht", ",", "wir", "schaf\u00b7fen", ",", "m\u00fchn", "und", "sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df des Volkes Kraft erwache,", "tokens": ["Da\u00df", "des", "Vol\u00b7kes", "Kraft", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kunst und Flei\u00df sich geltend mache,", "tokens": ["Kunst", "und", "Flei\u00df", "sich", "gel\u00b7tend", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die Armuth werde kleiner", "tokens": ["Da\u00df", "die", "Ar\u00b7muth", "wer\u00b7de", "klei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Wohlfahrt allgemeiner.", "tokens": ["Und", "die", "Wohl\u00b7fahrt", "all\u00b7ge\u00b7mei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "So treiben wir Handlung auf der See.", "tokens": ["So", "trei\u00b7ben", "wir", "Hand\u00b7lung", "auf", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Seht, wir trachten nur und dichten,", "tokens": ["Seht", ",", "wir", "trach\u00b7ten", "nur", "und", "dich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADV", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Musterwerke zu errichten,", "tokens": ["Mus\u00b7ter\u00b7wer\u00b7ke", "zu", "er\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu beseelen alle H\u00e4nde", "tokens": ["Zu", "be\u00b7see\u00b7len", "al\u00b7le", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Gedeihen aller St\u00e4nde,", "tokens": ["Zum", "Ge\u00b7dei\u00b7hen", "al\u00b7ler", "St\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kunst und Industrie zu heben", "tokens": ["Kunst", "und", "In\u00b7dust\u00b7rie", "zu", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Marktplatz zu beleben.", "tokens": ["Und", "den", "Markt\u00b7platz", "zu", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "So treiben wir Handlung auf der See.", "tokens": ["So", "trei\u00b7ben", "wir", "Hand\u00b7lung", "auf", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht wie sich im Haus die Schnecke", "tokens": ["Nicht", "wie", "sich", "im", "Haus", "die", "Schne\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PWAV", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben wir uns nur zum Zwecke:", "tokens": ["Ha\u00b7ben", "wir", "uns", "nur", "zum", "Zwe\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, wie k\u00f6nnten wir euch hindern!", "tokens": ["Ei", ",", "wie", "k\u00f6nn\u00b7ten", "wir", "euch", "hin\u00b7dern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir, die wir den Nothstand lindern,", "tokens": ["Wir", ",", "die", "wir", "den", "Noth\u00b7stand", "lin\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bereit sind alle Zeiten", "tokens": ["Und", "be\u00b7reit", "sind", "al\u00b7le", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch nur Wohlstand zu bereiten?", "tokens": ["Euch", "nur", "Wohl\u00b7stand", "zu", "be\u00b7rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "So treiben wir Handlung auf der See.", "tokens": ["So", "trei\u00b7ben", "wir", "Hand\u00b7lung", "auf", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "La\u00dft das Kleinliche verderben!", "tokens": ["La\u00dft", "das", "Klein\u00b7li\u00b7che", "ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward nicht Freiheit den Gewerben?", "tokens": ["Ward", "nicht", "Frei\u00b7heit", "den", "Ge\u00b7wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann nicht jeder seine Sachen,", "tokens": ["Kann", "nicht", "je\u00b7der", "sei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flei\u00df und Waare geltend machen?", "tokens": ["Flei\u00df", "und", "Waa\u00b7re", "gel\u00b7tend", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00e4't wie wir die Saat der M\u00fche,", "tokens": ["S\u00e4't", "wie", "wir", "die", "Saat", "der", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df auch ", "tokens": ["Da\u00df", "auch"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "So treibet auch Handlung auf der See!", "tokens": ["So", "trei\u00b7bet", "auch", "Hand\u00b7lung", "auf", "der", "See", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Alles k\u00f6nnt auch ihr beginnen:", "tokens": ["Al\u00b7les", "k\u00f6nnt", "auch", "ihr", "be\u00b7gin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Malen, weben, hecheln, spinnen.", "tokens": ["Ma\u00b7len", ",", "we\u00b7ben", ",", "he\u00b7cheln", ",", "spin\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg mit Flotten, weg mit Schiffen!", "tokens": ["Weg", "mit", "Flot\u00b7ten", ",", "weg", "mit", "Schif\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer die Zeiten hat begriffen,", "tokens": ["Wer", "die", "Zei\u00b7ten", "hat", "be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bringet auch auf trocknem Sande", "tokens": ["Brin\u00b7get", "auch", "auf", "trock\u00b7nem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Seefahrt noch zu Stande.", "tokens": ["Ei\u00b7ne", "See\u00b7fahrt", "noch", "zu", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Juchhe! juchhe!", "tokens": ["Juch\u00b7he", "!", "juch\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Hoch lebe die Handlung auf der See!", "tokens": ["Hoch", "le\u00b7be", "die", "Hand\u00b7lung", "auf", "der", "See", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}