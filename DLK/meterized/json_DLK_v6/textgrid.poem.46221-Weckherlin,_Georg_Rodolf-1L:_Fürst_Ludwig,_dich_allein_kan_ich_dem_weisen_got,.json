{"textgrid.poem.46221": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrst Ludwig, dich allein kan ich dem weisen got,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrst Ludwig, dich allein kan ich dem weisen got,", "tokens": ["F\u00fcrst", "Lud\u00b7wig", ",", "dich", "al\u00b7lein", "kan", "ich", "dem", "wei\u00b7sen", "got", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PPER", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der die pfeil und die leir gebrauchen kan, gleich finden:", "tokens": ["der", "die", "pfeil", "und", "die", "leir", "ge\u00b7brau\u00b7chen", "kan", ",", "gleich", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN", "VVINF", "VMFIN", "$,", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "dan du mit deinem helm kanst dein haupt in kriegsnot,", "tokens": ["dan", "du", "mit", "dei\u00b7nem", "helm", "kanst", "dein", "haupt", "in", "kriegs\u00b7not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "in ruh mit einem kranz (stets k\u00fchn und klug) umbinden.", "tokens": ["in", "ruh", "mit", "ei\u00b7nem", "kranz", "(", "stets", "k\u00fchn", "und", "klug", ")", "um\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$(", "ADV", "ADJD", "KON", "ADJD", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch bist du taugenlich allein durch dein gebot", "tokens": ["Auch", "bist", "du", "tau\u00b7gen\u00b7lich", "al\u00b7lein", "durch", "dein", "ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "zu dem streit oder lieb die herzen zu entz\u00fcnden;", "tokens": ["zu", "dem", "streit", "o\u00b7der", "lieb", "die", "her\u00b7zen", "zu", "ent\u00b7z\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJD", "ART", "VVINF", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "ja, du kanst, wie du wilst, die leut, den feind, den tod", "tokens": ["ja", ",", "du", "kanst", ",", "wie", "du", "wilst", ",", "die", "leut", ",", "den", "feind", ",", "den", "tod"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PRELS", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "durch deines munds, schwerts, hand, sprach, streich, schrift \u00fcberwinden.", "tokens": ["durch", "dei\u00b7nes", "munds", ",", "schwerts", ",", "hand", ",", "sprach", ",", "streich", ",", "schrift", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "$,", "NN", "$,", "VVFIN", "$,", "ADJD", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum des himmels gnad, der deine seel und hand", "tokens": ["Da\u00b7rum", "des", "him\u00b7mels", "gnad", ",", "der", "dei\u00b7ne", "seel", "und", "hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wolt mit so gro\u00dfem schatz freigebigreichlich zieren,", "tokens": ["wolt", "mit", "so", "gro\u00b7\u00dfem", "schatz", "frei\u00b7ge\u00b7bi\u00b7greich\u00b7lich", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADV", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+---+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "will, da\u00df du stehest auch jetz f\u00fcr das vatterland;", "tokens": ["will", ",", "da\u00df", "du", "ste\u00b7hest", "auch", "jetz", "f\u00fcr", "das", "vat\u00b7ter\u00b7land", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja, er will (wie sehr auch der freiheit feind stolzieren)", "tokens": ["Ja", ",", "er", "will", "(", "wie", "sehr", "auch", "der", "frei\u00b7heit", "feind", "stol\u00b7zie\u00b7ren", ")"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "$(", "KOKOM", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "da\u00df die freiheit noch soll durch dein wort und verstand", "tokens": ["da\u00df", "die", "frei\u00b7heit", "noch", "soll", "durch", "dein", "wort", "und", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "und mit ihr auch dein ruhm f\u00fcr und f\u00fcr triumfieren.", "tokens": ["und", "mit", "ihr", "auch", "dein", "ruhm", "f\u00fcr", "und", "f\u00fcr", "tri\u00b7um\u00b7fie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "PPOSAT", "NN", "APPR", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}