{"textgrid.poem.64561": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "185. An Herrn Professor Lichtenberg", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Deutschland, wo ", "tokens": ["Durch", "Deutschland", ",", "wo"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NE", "$,", "PWAV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da reise waffenlos in voller Sicherheit.", "tokens": ["Da", "rei\u00b7se", "waf\u00b7fen\u00b7los", "in", "vol\u00b7ler", "Si\u00b7cher\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bewehrt den Reisenden um eine Gabe bitten,", "tokens": ["Be\u00b7wehrt", "den", "Rei\u00b7sen\u00b7den", "um", "ei\u00b7ne", "Ga\u00b7be", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geh\u00f6rt zur Freyheit stolzer Briten.", "tokens": ["Ge\u00b7h\u00f6rt", "zur", "Frey\u00b7heit", "stol\u00b7zer", "Bri\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}