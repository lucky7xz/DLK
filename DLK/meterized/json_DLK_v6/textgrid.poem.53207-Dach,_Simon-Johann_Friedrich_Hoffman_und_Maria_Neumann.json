{"textgrid.poem.53207": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Johann Friedrich Hoffman und Maria Neumann", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nvn Mars auch \u00fcber Pohlen", "tokens": ["Nvn", "Mars", "auch", "\u00fc\u00b7ber", "Poh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den k\u00fchnen Wallach sprengt,", "tokens": ["Den", "k\u00fch\u00b7nen", "Wal\u00b7lach", "sprengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch Preussen, Raub zu holen,", "tokens": ["Auch", "Preus\u00b7sen", ",", "Raub", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit strenger Herrschaft drengt,", "tokens": ["Mit", "stren\u00b7ger", "Herr\u00b7schaft", "drengt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun unser Gut und Leben", "tokens": ["Nun", "un\u00b7ser", "Gut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darauff geht mit der Zeit,", "tokens": ["Dar\u00b7auff", "geht", "mit", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das alles mus erbeben", "tokens": ["Das", "al\u00b7les", "mus", "er\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr seiner Grausamkeit,", "tokens": ["F\u00fcr", "sei\u00b7ner", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun die Trompetten klingen,", "tokens": ["Nun", "die", "Trom\u00b7pet\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Das wilde Feldgeschrey,", "tokens": ["Das", "wil\u00b7de", "Feld\u00b7ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Ross' und Kugeln singen,", "tokens": ["Die", "Ross'", "und", "Ku\u00b7geln", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Melpomene verzeih:", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", "ver\u00b7zeih", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Was heissest du mich geigen?", "tokens": ["Was", "heis\u00b7sest", "du", "mich", "gei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer h\u00f6rt mein schwaches Lied?", "tokens": ["Wer", "h\u00f6rt", "mein", "schwa\u00b7ches", "Lied", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hei\u00df mich viel lieber schweigen,", "tokens": ["Hei\u00df", "mich", "viel", "lie\u00b7ber", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich bin umbsonst bem\u00fcht.", "tokens": ["Ich", "bin", "um\u00b7bsonst", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo ist die Zeit geblieben", "tokens": ["Wo", "ist", "die", "Zeit", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da mich pflag jederman", "tokens": ["Da", "mich", "pflag", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu ehren und zu lieben,", "tokens": ["Zu", "eh\u00b7ren", "und", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlug ich die Seiten an,", "tokens": ["Schlug", "ich", "die", "Sei\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch die ber\u00fchmten Tichter,", "tokens": ["Auch", "die", "be\u00b7r\u00fchm\u00b7ten", "Tich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An die mein Nahm geragt,", "tokens": ["An", "die", "mein", "Nahm", "ge\u00b7ragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df auch der Erden Lichter", "tokens": ["Da\u00df", "auch", "der", "Er\u00b7den", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach meinem Spiel gefragt?", "tokens": ["Nach", "mei\u00b7nem", "Spiel", "ge\u00b7fragt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt bin ich gantz vergessen.", "tokens": ["Jetzt", "bin", "ich", "gantz", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was n\u00fctzt auch mein Geticht?", "tokens": ["Was", "n\u00fctzt", "auch", "mein", "Ge\u00b7ticht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es dienet nicht zu essen,", "tokens": ["Es", "die\u00b7net", "nicht", "zu", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es dient zu trincken nicht:", "tokens": ["Es", "dient", "zu", "trin\u00b7cken", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es taug nicht f\u00fcr Soldaten.", "tokens": ["Es", "taug", "nicht", "f\u00fcr", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir fehlt der Tyrteen Hand,", "tokens": ["Mir", "fehlt", "der", "Tyr\u00b7teen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der vor die Spartiaten", "tokens": ["Der", "vor", "die", "Spar\u00b7ti\u00b7a\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit spielen \u00fcberwand.", "tokens": ["Mit", "spie\u00b7len", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch Lesbos ward gezwungen,", "tokens": ["Auch", "Les\u00b7bos", "ward", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Alcee, durch deinen Klangk,", "tokens": ["Al\u00b7cee", ",", "durch", "dei\u00b7nen", "Klangk", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Orpheus hat gesungen", "tokens": ["Was", "Or\u00b7pheus", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War der Odrysen Zwangk,", "tokens": ["War", "der", "Od\u00b7ry\u00b7sen", "Zwangk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Auch gab Amphions Leyer", "tokens": ["Auch", "gab", "Am\u00b7phi\u00b7ons", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Den angenehmen Laut,", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df dadurch dein Gem\u00e4wer,", "tokens": ["Da\u00df", "da\u00b7durch", "dein", "Ge\u00b7m\u00e4\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Thebe, ward erbawt.", "tokens": ["O", "The\u00b7be", ",", "ward", "er\u00b7bawt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jetzt haben unsre Seiten", "tokens": ["Jetzt", "ha\u00b7ben", "uns\u00b7re", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey weiten nicht die Krafft,", "tokens": ["Bey", "wei\u00b7ten", "nicht", "die", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mars l\u00e4sst sich nicht bestreiten", "tokens": ["Mars", "l\u00e4sst", "sich", "nicht", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch alle Wissenschafft:", "tokens": ["Durch", "al\u00b7le", "Wis\u00b7sen\u00b7schafft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja k\u00f6ntt' ich auch mit streichen", "tokens": ["Ja", "k\u00f6ntt'", "ich", "auch", "mit", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VMFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit \u00fcber Phebus gehn,", "tokens": ["Weit", "\u00fc\u00b7ber", "Phe\u00b7bus", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Sebel wird mir weichen,", "tokens": ["Kein", "Se\u00b7bel", "wird", "mir", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Feind zur\u00fcckestehn:", "tokens": ["Kein", "Feind", "zu\u00b7r\u00fc\u00b7ckes\u00b7tehn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "W\u00e4r es gethan mit singen,", "tokens": ["W\u00e4r", "es", "ge\u00b7than", "mit", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "So woltt' ich \u00fcberall", "tokens": ["So", "woltt'", "ich", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Stal der Zeiten bringen", "tokens": ["Das", "Stal", "der", "Zei\u00b7ten", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stracks in Saturns Metall.", "tokens": ["Stracks", "in", "Sa\u00b7turns", "Me\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Pregel sollte werden", "tokens": ["Der", "Pre\u00b7gel", "soll\u00b7te", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein klarer Castalis,", "tokens": ["Ein", "kla\u00b7rer", "Cas\u00b7ta\u00b7lis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd Preussen aller Erden", "tokens": ["Vnd", "Preus\u00b7sen", "al\u00b7ler", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gew\u00fcnschtes Paradi\u00df.", "tokens": ["Ge\u00b7w\u00fcnschtes", "Pa\u00b7ra\u00b7di\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Weil die\u00df nicht kan geschehen,", "tokens": ["Weil", "die\u00df", "nicht", "kan", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mars mu\u00df am brete stehn,", "tokens": ["Mars", "mu\u00df", "am", "bre\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sol ich der Mi\u00dfgunst flehen,", "tokens": ["Sol", "ich", "der", "Mi\u00df\u00b7gunst", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Tugend m\u00fcssig gehn?", "tokens": ["Der", "Tu\u00b7gend", "m\u00fcs\u00b7sig", "gehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich mit dem Kriege sch\u00fctzen,", "tokens": ["Mich", "mit", "dem", "Krie\u00b7ge", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Gl\u00fcckes Sclave seyn,", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "Scla\u00b7ve", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die wehrte Zeit vern\u00fctzen", "tokens": ["Die", "wehr\u00b7te", "Zeit", "ver\u00b7n\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit m\u00fcssiggehen? nein.", "tokens": ["Mit", "m\u00fcs\u00b7sig\u00b7ge\u00b7hen", "?", "nein", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich wil mich standhafft haltten", "tokens": ["Ich", "wil", "mich", "stand\u00b7hafft", "halt\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Gott und meinen Sinn,", "tokens": ["An", "Gott", "und", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Satzung lassen waltten,", "tokens": ["Die", "Sat\u00b7zung", "las\u00b7sen", "walt\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd bleiben wer ich bin,", "tokens": ["Vnd", "blei\u00b7ben", "wer", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wil dieses Wetters lachen,", "tokens": ["Wil", "die\u00b7ses", "Wet\u00b7ters", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ein andrer Abderiht,", "tokens": ["Ein", "an\u00b7drer", "Ab\u00b7de\u00b7riht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd ferner Verse machen", "tokens": ["Vnd", "fer\u00b7ner", "Ver\u00b7se", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von allem was geschiht.", "tokens": ["Von", "al\u00b7lem", "was", "ge\u00b7schiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Streit mu\u00df sich doch enden,", "tokens": ["Der", "Streit", "mu\u00df", "sich", "doch", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man wird des wesens sat,", "tokens": ["Man", "wird", "des", "we\u00b7sens", "sat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sol sich, hoff' ich, wenden", "tokens": ["Dann", "sol", "sich", ",", "hoff'", "ich", ",", "wen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PRF", "$,", "VVFIN", "PPER", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des leichten Gl\u00fcckes Blad,", "tokens": ["Des", "leich\u00b7ten", "Gl\u00fc\u00b7ckes", "Blad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit kr\u00e4fftigen Gem\u00e4rcke", "tokens": ["Mit", "kr\u00e4ff\u00b7ti\u00b7gen", "Ge\u00b7m\u00e4r\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Gold sey und nur Schein,", "tokens": ["Was", "Gold", "sey", "und", "nur", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd wessen Tugend-wercke", "tokens": ["Vnd", "wes\u00b7sen", "Tu\u00b7gen\u00b7d\u00b7wer\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bewehrt und besser seyn.", "tokens": ["Be\u00b7wehrt", "und", "bes\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wolan mit dem bescheide,", "tokens": ["Wo\u00b7lan", "mit", "dem", "be\u00b7schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr Hoffmann, fahret fort", "tokens": ["Herr", "Hoff\u00b7mann", ",", "fah\u00b7ret", "fort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ewrer HochzeitFrewde,", "tokens": ["Mit", "ew\u00b7rer", "Hoch\u00b7zeit", "Frew\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr seht nicht an den Nord,", "tokens": ["Ihr", "seht", "nicht", "an", "den", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie ungest\u00fcm er f\u00e4hret,", "tokens": ["Wie", "un\u00b7ge\u00b7st\u00fcm", "er", "f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hebt ewren Sinn zu Gott,", "tokens": ["Hebt", "ew\u00b7ren", "Sinn", "zu", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der H\u00fclff und Raht gewehret,", "tokens": ["Der", "H\u00fclff", "und", "Raht", "ge\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie gro\u00df auch sey die Noht.", "tokens": ["Wie", "gro\u00df", "auch", "sey", "die", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Bey uns auff frembder Erden", "tokens": ["Bey", "uns", "auff", "fremb\u00b7der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Habt ihr es euch bisher", "tokens": ["Habt", "ihr", "es", "euch", "bis\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gnug sawer lassen werden,", "tokens": ["Gnug", "sa\u00b7wer", "las\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VAFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Kein' Arbeit ward euch schwer,", "tokens": ["Kein'", "Ar\u00b7beit", "ward", "euch", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nichts habt ihr untterlassen,", "tokens": ["Nichts", "habt", "ihr", "unt\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Euch r\u00fchmet selbst der Flei\u00df", "tokens": ["Euch", "r\u00fch\u00b7met", "selbst", "der", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vmb welchen bester massen", "tokens": ["Vmb", "wel\u00b7chen", "bes\u00b7ter", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Herr Peter Weger weis.", "tokens": ["Herr", "Pe\u00b7ter", "We\u00b7ger", "weis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er sah euch in den Schrancken", "tokens": ["Er", "sah", "euch", "in", "den", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Trew und Vnschuld gehn,", "tokens": ["Der", "Trew", "und", "Vn\u00b7schuld", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drumb was ihr ihm zu dancken,", "tokens": ["Drumb", "was", "ihr", "ihm", "zu", "dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6r' ich euch offt gestehn.", "tokens": ["H\u00f6r'", "ich", "euch", "offt", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja unser Haupt und Leben,", "tokens": ["Ja", "un\u00b7ser", "Haupt", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Churf\u00