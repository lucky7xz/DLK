{"textgrid.poem.31512": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der die Erde selbst erschaffen: f\u00e4llt hie auf sein Angesicht /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der die Erde selbst erschaffen: f\u00e4llt hie auf sein Angesicht /", "tokens": ["Der", "die", "Er\u00b7de", "selbst", "er\u00b7schaf\u00b7fen", ":", "f\u00e4llt", "hie", "auf", "sein", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$.", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "auf die Erd. Es betet hier / der aufs h\u00f6chste anzubeten.", "tokens": ["auf", "die", "Erd", ".", "Es", "be\u00b7tet", "hier", "/", "der", "aufs", "h\u00f6chs\u00b7te", "an\u00b7zu\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "$(", "ART", "APPRART", "ADJA", "VVIZU", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Der uns all' aus Noht erl\u00f6st / zaget hie in seinen N\u00f6ten.", "tokens": ["Der", "uns", "all'", "aus", "Noht", "er\u00b7l\u00f6st", "/", "za\u00b7get", "hie", "in", "sei\u00b7nen", "N\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "APPR", "NN", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-++-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ach sein Fu\u00dffall / uns im Himmel Ewig das Gesicht aufricht.", "tokens": ["Ach", "sein", "Fu\u00df\u00b7fall", "/", "uns", "im", "Him\u00b7mel", "E\u00b7wig", "das", "Ge\u00b7sicht", "auf\u00b7richt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$(", "PPER", "APPRART", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wie der Fall des ersten Menschen / unsern Vnschuld Thurn abbricht:", "tokens": ["Wie", "der", "Fall", "des", "ers\u00b7ten", "Men\u00b7schen", "/", "un\u00b7sern", "Vn\u00b7schuld", "Thurn", "ab\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$(", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+++", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "also f\u00e4llt der ander' / uns aus dem Schuld-Thurn zu erretten.", "tokens": ["al\u00b7so", "f\u00e4llt", "der", "an\u00b7der'", "/", "uns", "aus", "dem", "Schuld\u00b7Thurn", "zu", "er\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$(", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ein Ort / ihn in Abgrund senkt: und das ein' an dieser Ketten", "tokens": ["Ein", "Ort", "/", "ihn", "in", "Ab\u00b7grund", "senkt", ":", "und", "das", "ein'", "an", "die\u00b7ser", "Ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PPER", "APPR", "NN", "VVFIN", "$.", "KON", "PDS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "zieht / durch sein Verdtenstes-Schw\u00e4rheit / uns in Gottes Gnaden-Liecht.", "tokens": ["zieht", "/", "durch", "sein", "Verd\u00b7tens\u00b7tes\u00b7Schw\u00e4r\u00b7heit", "/", "uns", "in", "Got\u00b7tes", "Gna\u00b7den\u00b7Liecht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "PPOSAT", "NN", "$(", "PPER", "APPR", "NN", "NN", "$."], "meter": "---+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Selbst die Vnbeweglichkeit / die dem Fel\u00df zersplittert / zittert.", "tokens": ["Selbst", "die", "Vn\u00b7be\u00b7weg\u00b7lich\u00b7keit", "/", "die", "dem", "Fel\u00df", "zer\u00b7split\u00b7tert", "/", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ART", "ART", "NN", "VVPP", "$(", "VVFIN", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.10": {"text": "Der die Erden beben macht / bebet selbst vor Furcht und Angst.", "tokens": ["Der", "die", "Er\u00b7den", "be\u00b7ben", "macht", "/", "be\u00b7bet", "selbst", "vor", "Furcht", "und", "Angst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VVFIN", "$(", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Gottes Zornes- Donner ihm alle Aederlein zersplittert:", "tokens": ["Got\u00b7tes", "Zor\u00b7nes", "Don\u00b7ner", "ihm", "al\u00b7le", "A\u00b7e\u00b7der\u00b7lein", "zer\u00b7split\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "NN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+--+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "da\u00df du deren Heiles-Kr\u00e4ffte / meine Seel / dadurch erlangst;", "tokens": ["da\u00df", "du", "de\u00b7ren", "Hei\u00b7les\u00b7Kr\u00e4ff\u00b7te", "/", "mei\u00b7ne", "Seel", "/", "da\u00b7durch", "er\u00b7langst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "$(", "PPOSAT", "NN", "$(", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "und der Balsam seines Bluts / vom zerbrochnen Glas / dem Leibe /", "tokens": ["und", "der", "Bal\u00b7sam", "sei\u00b7nes", "Bluts", "/", "vom", "zer\u00b7broch\u00b7nen", "Glas", "/", "dem", "Lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$(", "APPRART", "ADJA", "NN", "$(", "ART", "NN", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "sich in dich ergie\u00df' und flie\u00df / Ruch und Ruh in dir verbleibe.", "tokens": ["sich", "in", "dich", "er\u00b7gie\u00df'", "und", "flie\u00df", "/", "Ruch", "und", "Ruh", "in", "dir", "ver\u00b7blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$(", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}}}}