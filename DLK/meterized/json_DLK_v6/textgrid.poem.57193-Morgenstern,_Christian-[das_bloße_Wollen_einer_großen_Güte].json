{"textgrid.poem.57193": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[das blo\u00dfe Wollen einer gro\u00dfen G\u00fcte]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das blo\u00dfe Wollen einer gro\u00dfen G\u00fcte", "tokens": ["Das", "blo\u00b7\u00dfe", "Wol\u00b7len", "ei\u00b7ner", "gro\u00b7\u00dfen", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ist ganz gewi\u00df ein hohes Menschentrachten.", "tokens": ["ist", "ganz", "ge\u00b7wi\u00df", "ein", "ho\u00b7hes", "Men\u00b7schen\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch es erhebt sich erst zur vollen Bl\u00fcte,", "tokens": ["Doch", "es", "er\u00b7hebt", "sich", "erst", "zur", "vol\u00b7len", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "wenn Gnaden eines seherisch Erwachten", "tokens": ["wenn", "Gna\u00b7den", "ei\u00b7nes", "se\u00b7he\u00b7risch", "Er\u00b7wach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "den Kosmos nachtentleitetem Gem\u00fcte", "tokens": ["den", "Kos\u00b7mos", "nach\u00b7tent\u00b7lei\u00b7te\u00b7tem", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als Geisterkunstwerk zum Bewu\u00dftsein brachten.", "tokens": ["als", "Geis\u00b7ter\u00b7kunst\u00b7werk", "zum", "Be\u00b7wu\u00df\u00b7tsein", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann w\u00e4chst aus Riesensch\u00f6pfungs\u00fcberblicken,", "tokens": ["Dann", "w\u00e4chst", "aus", "Rie\u00b7sen\u00b7sch\u00f6p\u00b7fungs\u00b7\u00fc\u00b7berb\u00b7li\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus Aufschau zu verborgnen Bildnersph\u00e4ren,", "tokens": ["aus", "Auf\u00b7schau", "zu", "ver\u00b7borg\u00b7nen", "Bild\u00b7ner\u00b7sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "aus Selbstmiteinbezug in deren Stufen \u2013", "tokens": ["aus", "Selbst\u00b7mi\u00b7tein\u00b7be\u00b7zug", "in", "de\u00b7ren", "Stu\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRELAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "ein Mitgef\u00fchl mit dieser Welt Geschicken,", "tokens": ["ein", "Mit\u00b7ge\u00b7f\u00fchl", "mit", "die\u00b7ser", "Welt", "Ge\u00b7schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das mehr als dunkle Herzenstriebe n\u00e4hren,", "tokens": ["das", "mehr", "als", "dunk\u00b7le", "Her\u00b7zen\u00b7strie\u00b7be", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das h\u00f6chste G\u00f6tter mit ans Werk berufen.", "tokens": ["das", "h\u00f6chs\u00b7te", "G\u00f6t\u00b7ter", "mit", "ans", "Werk", "be\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}