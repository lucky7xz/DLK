{"textgrid.poem.37285": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[er war ein grundgescheiter Mann]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war ein grundgescheiter Mann,", "tokens": ["Er", "war", "ein", "grund\u00b7ge\u00b7schei\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sehr weise und hocherfahren;", "tokens": ["Sehr", "wei\u00b7se", "und", "ho\u00b7cher\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er trug ein graumeliertes Haar,", "tokens": ["Er", "trug", "ein", "grau\u00b7me\u00b7lier\u00b7tes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil er schon ziemlich bei Jahren.", "tokens": ["Die\u00b7weil", "er", "schon", "ziem\u00b7lich", "bei", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Er war ein abgesagter Feind", "tokens": ["Er", "war", "ein", "ab\u00b7ge\u00b7sag\u00b7ter", "Feind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lachens und des Scherzens", "tokens": ["Des", "La\u00b7chens", "und", "des", "Scher\u00b7zens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und war doch der gr\u00f6\u00dfte Narr am Hof", "tokens": ["Und", "war", "doch", "der", "gr\u00f6\u00df\u00b7te", "Narr", "am", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der K\u00f6nigin seines Herzens.", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "sei\u00b7nes", "Her\u00b7zens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}