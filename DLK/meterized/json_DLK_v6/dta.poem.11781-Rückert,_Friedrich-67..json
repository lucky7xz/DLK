{"dta.poem.11781": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An jedem Morgen h\u00e4lt der sel'gen G\u00f6tter Chor", "tokens": ["An", "je\u00b7dem", "Mor\u00b7gen", "h\u00e4lt", "der", "sel'\u00b7gen", "G\u00f6t\u00b7ter", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Umfahrt um die Welt aus offnem Himmelsthor.", "tokens": ["Die", "Um\u00b7fahrt", "um", "die", "Welt", "aus", "off\u00b7nem", "Him\u00b7mel\u00b7sthor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und die verh\u00fcllte nur, die Gottheit bleibt zur\u00fcck,", "tokens": ["Und", "die", "ver\u00b7h\u00fcll\u00b7te", "nur", ",", "die", "Got\u00b7theit", "bleibt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Herde ruhend, wie der Hausfrau stilles Gl\u00fcck.", "tokens": ["Am", "Her\u00b7de", "ru\u00b7hend", ",", "wie", "der", "Haus\u00b7frau", "stil\u00b7les", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Geister aber, die vom Stamm der G\u00f6tter wohnen", "tokens": ["Die", "Geis\u00b7ter", "a\u00b7ber", ",", "die", "vom", "Stamm", "der", "G\u00f6t\u00b7ter", "woh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Erden, fahren auch empor aus allen Zonen.", "tokens": ["Auf", "Er\u00b7den", ",", "fah\u00b7ren", "auch", "em\u00b7por", "aus", "al\u00b7len", "Zo\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den G\u00f6ttern folgen sie nacheifernd Ro\u00df und Mann,", "tokens": ["Den", "G\u00f6t\u00b7tern", "fol\u00b7gen", "sie", "na\u00b7ch\u00b7ei\u00b7fernd", "Ro\u00df", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch haben G\u00f6tter nicht und Menschen gleich Gespann.", "tokens": ["Doch", "ha\u00b7ben", "G\u00f6t\u00b7ter", "nicht", "und", "Men\u00b7schen", "gleich", "Ge\u00b7spann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PTKNEG", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ganz g\u00f6ttlich sind die Ross' auch die die G\u00f6tter tragen,", "tokens": ["Ganz", "g\u00f6tt\u00b7lich", "sind", "die", "Ross'", "auch", "die", "die", "G\u00f6t\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "ADV", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gemischter Art sind die am Menschenseelenwagen.", "tokens": ["Ge\u00b7mischter", "Art", "sind", "die", "am", "Men\u00b7schen\u00b7see\u00b7len\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "APPRART", "NN", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Das eine zieht hinauf, das andre zieht hinab,", "tokens": ["Das", "ei\u00b7ne", "zieht", "hin\u00b7auf", ",", "das", "and\u00b7re", "zieht", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PTKVZ", "$,", "PRELS", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df schwer der Lenker sie erh\u00e4lt in gleichem Trab.", "tokens": ["Da\u00df", "schwer", "der", "Len\u00b7ker", "sie", "er\u00b7h\u00e4lt", "in", "glei\u00b7chem", "Trab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit M\u00fche geht es schon die ebnern Himmelsbahnen,", "tokens": ["Mit", "M\u00fc\u00b7he", "geht", "es", "schon", "die", "eb\u00b7nern", "Him\u00b7mels\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch an der Steile stockt das Ro\u00df von schlechten Ahnen.", "tokens": ["Doch", "an", "der", "Stei\u00b7le", "stockt", "das", "Ro\u00df", "von", "schlech\u00b7ten", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und wen der Zuruf nicht rei\u00dft eines Gotts empor,", "tokens": ["Und", "wen", "der", "Zu\u00b7ruf", "nicht", "rei\u00dft", "ei\u00b7nes", "Gotts", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt auf der H\u00e4lft' und folgt nicht ganz dem sel'gen Chor.", "tokens": ["Bleibt", "auf", "der", "H\u00e4lft'", "und", "folgt", "nicht", "ganz", "dem", "sel'\u00b7gen", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die G\u00f6tter fahren hin am Rand von Raum und Zeit,", "tokens": ["Die", "G\u00f6t\u00b7ter", "fah\u00b7ren", "hin", "am", "Rand", "von", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und blicken froh hinaus in die Unendlichkeit.", "tokens": ["Und", "bli\u00b7cken", "froh", "hin\u00b7aus", "in", "die", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Dort wo das Ew'ge steht, das Wahre, Gute, Sch\u00f6ne,", "tokens": ["Dort", "wo", "das", "Ew'\u00b7ge", "steht", ",", "das", "Wah\u00b7re", ",", "Gu\u00b7te", ",", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "VVFIN", "$,", "ART", "ADJA", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dessen Anblick sich erquicken G\u00f6tters\u00f6hne.", "tokens": ["An", "des\u00b7sen", "An\u00b7blick", "sich", "er\u00b7qui\u00b7cken", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und wem's der Geister gl\u00fcckt zu folgen G\u00f6tterspur,", "tokens": ["Und", "wem's", "der", "Geis\u00b7ter", "gl\u00fcckt", "zu", "fol\u00b7gen", "G\u00f6t\u00b7ter\u00b7spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sieht dasselb' entz\u00fcckt, doch sieht er halb es nur.", "tokens": ["Der", "sieht", "das\u00b7selb'", "ent\u00b7z\u00fcckt", ",", "doch", "sieht", "er", "halb", "es", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Dem einen, wenn ers sieht, so schwindeln ihm die Sinnen,", "tokens": ["Dem", "ei\u00b7nen", ",", "wenn", "ers", "sieht", ",", "so", "schwin\u00b7deln", "ihm", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den andern tr\u00e4gt zu schnell der Rosse Braus von hinnen.", "tokens": ["Den", "an\u00b7dern", "tr\u00e4gt", "zu", "schnell", "der", "Ros\u00b7se", "Braus", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKA", "ADJD", "ART", "NN", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Dem dritten b\u00e4umen sich die Rosse so und str\u00e4uben,", "tokens": ["Dem", "drit\u00b7ten", "b\u00e4u\u00b7men", "sich", "die", "Ros\u00b7se", "so", "und", "str\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "ART", "NN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er das Wahre nicht gewahret vor Bet\u00e4uben.", "tokens": ["Da\u00df", "er", "das", "Wah\u00b7re", "nicht", "ge\u00b7wah\u00b7ret", "vor", "Be\u00b7t\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Was aber jeder dort der Geister hat gesehn,", "tokens": ["Was", "a\u00b7ber", "je\u00b7der", "dort", "der", "Geis\u00b7ter", "hat", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das tragen sie mit fort, wann sie zur Erde gehn.", "tokens": ["Das", "tra\u00b7gen", "sie", "mit", "fort", ",", "wann", "sie", "zur", "Er\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PTKVZ", "$,", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Dem wahren Seyn, das sie geschaut in jenen R\u00e4umen,", "tokens": ["Dem", "wah\u00b7ren", "Seyn", ",", "das", "sie", "ge\u00b7schaut", "in", "je\u00b7nen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sinnen sie unten nach, und scheinen euch zu tr\u00e4umen;", "tokens": ["Sin\u00b7nen", "sie", "un\u00b7ten", "nach", ",", "und", "schei\u00b7nen", "euch", "zu", "tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.16": {"line.1": {"text": "Euch andern, die zum Licht empor nicht mochtet dringen,", "tokens": ["Euch", "an\u00b7dern", ",", "die", "zum", "Licht", "em\u00b7por", "nicht", "moch\u00b7tet", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "APPRART", "NN", "PTKVZ", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil euern Rossen nicht gewachsen so die Schwingen.", "tokens": ["Weil", "eu\u00b7ern", "Ros\u00b7sen", "nicht", "ge\u00b7wach\u00b7sen", "so", "die", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVPP", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Ihr habt indessen euch, vom Steigen angeregt", "tokens": ["Ihr", "habt", "in\u00b7des\u00b7sen", "euch", ",", "vom", "Stei\u00b7gen", "an\u00b7ge\u00b7regt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der G\u00f6tter auch, doch nur im niedern Kreis bewegt;", "tokens": ["Der", "G\u00f6t\u00b7ter", "auch", ",", "doch", "nur", "im", "nie\u00b7dern", "Kreis", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Wo ein Get\u00fcmmel ward, ein l\u00e4rmendes Gedr\u00e4nge,", "tokens": ["Wo", "ein", "Ge\u00b7t\u00fcm\u00b7mel", "ward", ",", "ein", "l\u00e4r\u00b7men\u00b7des", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sinnverwirrendes verwirrtes Schaugepr\u00e4nge;", "tokens": ["Ein", "sinn\u00b7ver\u00b7wir\u00b7ren\u00b7des", "ver\u00b7wirr\u00b7tes", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.19": {"line.1": {"text": "Wo jeder andres sucht, und alle gleiches Ziel", "tokens": ["Wo", "je\u00b7der", "and\u00b7res", "sucht", ",", "und", "al\u00b7le", "glei\u00b7ches", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im unaufh\u00f6rlichen Weltwettlaufrennespiel.", "tokens": ["Im", "un\u00b7auf\u00b7h\u00f6r\u00b7li\u00b7chen", "Welt\u00b7wett\u00b7lauf\u00b7ren\u00b7nes\u00b7piel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.20": {"line.1": {"text": "Wo jeder jedem vor sich dr\u00e4ngt auf engen Pfaden,", "tokens": ["Wo", "je\u00b7der", "je\u00b7dem", "vor", "sich", "dr\u00e4ngt", "auf", "en\u00b7gen", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PIS", "APPR", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimmt mancher bald am Ro\u00df und bald am Wagen Schaden.", "tokens": ["Nimmt", "man\u00b7cher", "bald", "am", "Ro\u00df", "und", "bald", "am", "Wa\u00b7gen", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPRART", "NN", "KO