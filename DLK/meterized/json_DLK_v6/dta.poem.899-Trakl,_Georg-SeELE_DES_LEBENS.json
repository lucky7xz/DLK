{"dta.poem.899": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "SeELE DES LEBENS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Verfall, der weich das Laub umd\u00fcstert,", "tokens": ["Ver\u00b7fall", ",", "der", "weich", "das", "Laub", "um\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wohnt im Wald sein weites Schweigen.", "tokens": ["Es", "wohnt", "im", "Wald", "sein", "wei\u00b7tes", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald scheint ein Dorf sich geisterhaft zu neigen.", "tokens": ["Bald", "scheint", "ein", "Dorf", "sich", "geis\u00b7ter\u00b7haft", "zu", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schwester Mund in schwarzen Zweigen fl\u00fcstert.", "tokens": ["Der", "Schwes\u00b7ter", "Mund", "in", "schwar\u00b7zen", "Zwei\u00b7gen", "fl\u00fcs\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Einsame wird bald entgleiten,", "tokens": ["Der", "Ein\u00b7sa\u00b7me", "wird", "bald", "ent\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht ein Hirt auf dunklen Pfaden.", "tokens": ["Viel\u00b7leicht", "ein", "Hirt", "auf", "dunk\u00b7len", "Pfa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tier tritt leise aus den Baumarkaden,", "tokens": ["Ein", "Tier", "tritt", "lei\u00b7se", "aus", "den", "Bau\u00b7mar\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indes die Lider sich vor Gottheit weiten.", "tokens": ["In\u00b7des", "die", "Li\u00b7der", "sich", "vor", "Got\u00b7theit", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der blaue Flu\u00df rinnt sch\u00f6n hinunter,", "tokens": ["Der", "blau\u00b7e", "Flu\u00df", "rinnt", "sch\u00f6n", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00f6lke sich am Abend zeigen;", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "sich", "am", "A\u00b7bend", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele auch in engelhaftem Schweigen.", "tokens": ["Die", "See\u00b7le", "auch", "in", "en\u00b7gel\u00b7haf\u00b7tem", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verg\u00e4ngliche Gebilde gehen unter.", "tokens": ["Ver\u00b7g\u00e4ng\u00b7li\u00b7che", "Ge\u00b7bil\u00b7de", "ge\u00b7hen", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}