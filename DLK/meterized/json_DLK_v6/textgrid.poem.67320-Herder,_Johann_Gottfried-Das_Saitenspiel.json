{"textgrid.poem.67320": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Saitenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was singt in Euch, Ihr Saiten?", "tokens": ["Was", "singt", "in", "Euch", ",", "Ihr", "Sai\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was t\u00f6nt in Eurem Schall?", "tokens": ["Was", "t\u00f6nt", "in", "Eu\u00b7rem", "Schall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bist Du es, klagenreiche,", "tokens": ["Bist", "Du", "es", ",", "kla\u00b7gen\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geliebte Nachtigall?", "tokens": ["Ge\u00b7lieb\u00b7te", "Nach\u00b7ti\u00b7gall", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die, als sie meinem Herzen", "tokens": ["Die", ",", "als", "sie", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wehklagete so zart,", "tokens": ["Weh\u00b7kla\u00b7ge\u00b7te", "so", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vielleicht im letzten Seufzer", "tokens": ["Viel\u00b7leicht", "im", "letz\u00b7ten", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Silberlaute ward.", "tokens": ["Zum", "Sil\u00b7ber\u00b7lau\u00b7te", "ward", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was spricht in Euch, Ihr Saiten?", "tokens": ["Was", "spricht", "in", "Euch", ",", "Ihr", "Sai\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was singt in Eurem Schall?", "tokens": ["Was", "singt", "in", "Eu\u00b7rem", "Schall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Betr\u00fcgst Du mich, o Liebe,", "tokens": ["Be\u00b7tr\u00fcgst", "Du", "mich", ",", "o", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit s\u00fc\u00dfem Widerhall?", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Wi\u00b7der\u00b7hall", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du T\u00e4uscherin der Herzen,", "tokens": ["Du", "T\u00e4u\u00b7sche\u00b7rin", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geliebter Lippen Tand,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Lip\u00b7pen", "Tand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bist Du vielleicht in T\u00f6ne,", "tokens": ["Bist", "Du", "viel\u00b7leicht", "in", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du Fl\u00fcchtige, verbannt?", "tokens": ["Du", "Fl\u00fcch\u00b7ti\u00b7ge", ",", "ver\u00b7bannt", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es spricht mit st\u00e4rkrer Stimme,", "tokens": ["Es", "spricht", "mit", "st\u00e4r\u00b7krer", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es dringet mir ans Herz", "tokens": ["Es", "drin\u00b7get", "mir", "ans", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weckt mit Zaubergriffen", "tokens": ["Und", "weckt", "mit", "Zau\u00b7ber\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den l\u00e4ngst entschlafnen Schmerz.", "tokens": ["Den", "l\u00e4ngst", "ent\u00b7schlaf\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bebst in mir, o Seele,", "tokens": ["Du", "bebst", "in", "mir", ",", "o", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wirst selbst ein Saitenspiel", "tokens": ["Wirst", "selbst", "ein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "(in welches Geistes H\u00e4nden?)", "tokens": ["(", "in", "wel\u00b7ches", "Geis\u00b7tes", "H\u00e4n\u00b7den", "?", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PWAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit zitterndem Gef\u00fchl.", "tokens": ["Mit", "zit\u00b7tern\u00b7dem", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es schwebet aus den Saiten,", "tokens": ["Es", "schwe\u00b7bet", "aus", "den", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es lispelt mir ins Ohr;", "tokens": ["Es", "lis\u00b7pelt", "mir", "ins", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Geist der Harmonieen,", "tokens": ["Der", "Geist", "der", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Weltgeist tritt hervor.", "tokens": ["Der", "Welt\u00b7geist", "tritt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbich bin es, der die Wesen", "tokens": ["\u00bb", "ich", "bin", "es", ",", "der", "die", "We\u00b7sen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In ihre H\u00fclle zwang", "tokens": ["In", "ih\u00b7re", "H\u00fcl\u00b7le", "zwang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sie mit Zaubereien", "tokens": ["Und", "sie", "mit", "Zau\u00b7be\u00b7rei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Sympathie durchdrang.", "tokens": ["Der", "Sym\u00b7pa\u00b7thie", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In rauher Felsenh\u00f6hle", "tokens": ["In", "rau\u00b7her", "Fel\u00b7sen\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich Dir Widerhall,", "tokens": ["Bin", "ich", "Dir", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im Ton der kleinen Kehle", "tokens": ["Im", "Ton", "der", "klei\u00b7nen", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesang der Nachtigall.", "tokens": ["Ge\u00b7sang", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin's, der in der Klage", "tokens": ["Ich", "bin's", ",", "der", "in", "der", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Herz zum Mitleid r\u00fchrt", "tokens": ["Dein", "Herz", "zum", "Mit\u00b7leid", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in der Andacht Ch\u00f6ren", "tokens": ["Und", "in", "der", "An\u00b7dacht", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es auf zum Himmel f\u00fchrt.", "tokens": ["Es", "auf", "zum", "Him\u00b7mel", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich stimmete die Welten", "tokens": ["Ich", "stim\u00b7me\u00b7te", "die", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Zu Seelen flossen Seelen,", "tokens": ["Zu", "See\u00b7len", "flos\u00b7sen", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein ew'ger Chorgesang.", "tokens": ["Ein", "ew'\u00b7ger", "Chor\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom zarten Ton beweget,", "tokens": ["Vom", "zar\u00b7ten", "Ton", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch\u00e4ngstet sich Dein Herz", "tokens": ["Dur\u00b7ch\u00e4ngs\u00b7tet", "sich", "Dein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und f\u00fchlt der Schmerzen Freude,", "tokens": ["Und", "f\u00fchlt", "der", "Schmer\u00b7zen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Freude s\u00fc\u00dfen Schmerz.\u00ab", "tokens": ["Der", "Freu\u00b7de", "s\u00fc\u00b7\u00dfen", "Schmerz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verhall', o Stimm'! Ich h\u00f6re", "tokens": ["Ver\u00b7hall'", ",", "o", "Stimm'", "!", "Ich", "h\u00f6\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKVZ", "$,", "FM", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ganzen Sch\u00f6pfung Lied,", "tokens": ["Der", "gan\u00b7zen", "Sch\u00f6p\u00b7fung", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Seelen fest an Seelen,", "tokens": ["Das", "See\u00b7len", "fest", "an", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Herzen Herzen zieht.", "tokens": ["Zu", "Her\u00b7zen", "Her\u00b7zen", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Sind wir ein ewig All,", "tokens": ["Sind", "wir", "ein", "e\u00b7wig", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Der Gottheit Widerhall.", "tokens": ["Der", "Got\u00b7theit", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}