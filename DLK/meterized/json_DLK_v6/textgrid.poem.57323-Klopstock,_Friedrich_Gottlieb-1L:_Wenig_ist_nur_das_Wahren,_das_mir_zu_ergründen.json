{"textgrid.poem.57323": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenig ist nur das Wahren, das mir zu ergr\u00fcnden", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenig ist nur das Wahren, das mir zu ergr\u00fcnden", "tokens": ["We\u00b7nig", "ist", "nur", "das", "Wah\u00b7ren", ",", "das", "mir", "zu", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gl\u00fcckte; doch ist mir es theuer, wie ein Kleinod,", "tokens": ["Gl\u00fcck\u00b7te", ";", "doch", "ist", "mir", "es", "theu\u00b7er", ",", "wie", "ein", "Klei\u00b7nod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Durch vielj\u00e4hrigen Schweiss errungen,", "tokens": ["Durch", "viel\u00b7j\u00e4h\u00b7ri\u00b7gen", "Schweiss", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Oder erk\u00e4mpfet mit Blut!", "tokens": ["O\u00b7der", "er\u00b7k\u00e4mp\u00b7fet", "mit", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ist mir ein Trunk im K\u00fchlen gesch\u00f6pft aus der Quelle;", "tokens": ["Ist", "mir", "ein", "Trunk", "im", "K\u00fch\u00b7len", "ge\u00b7sch\u00f6pft", "aus", "der", "Quel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Einer, der alt von der Kelter, im Krystall blinkt;", "tokens": ["Ei\u00b7ner", ",", "der", "alt", "von", "der", "Kel\u00b7ter", ",", "im", "Krys\u00b7tall", "blinkt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Fr\u00fchlingss\u00e4useln am Baum, der anbl\u00fcht;", "tokens": ["Fr\u00fch\u00b7lings\u00b7s\u00e4u\u00b7seln", "am", "Baum", ",", "der", "an\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wehen des fallenden Stroms;", "tokens": ["We\u00b7hen", "des", "fal\u00b7len\u00b7den", "Stroms", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Liebliche Ruh, st\u00e4ubt endlich der Fuss in des Weges", "tokens": ["Lieb\u00b7li\u00b7che", "Ruh", ",", "st\u00e4ubt", "end\u00b7lich", "der", "Fuss", "in", "des", "We\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kr\u00fcmme nicht mehr: wie durchgl\u00fchte von dem lichten", "tokens": ["Kr\u00fcm\u00b7me", "nicht", "mehr", ":", "wie", "durch\u00b7gl\u00fch\u00b7te", "von", "dem", "lich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "$.", "PWAV", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Himmel sinkend der Strahl! wie fern lag", "tokens": ["Him\u00b7mel", "sin\u00b7kend", "der", "Strahl", "!", "wie", "fern", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "$.", "PWAV", "ADJD", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lange die th\u00fcrmende Stadt!", "tokens": ["Lan\u00b7ge", "die", "th\u00fcr\u00b7men\u00b7de", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Labt, wie ein Buch, worin es im Geist der verkanten", "tokens": ["Labt", ",", "wie", "ein", "Buch", ",", "wo\u00b7rin", "es", "im", "Geist", "der", "ver\u00b7kan\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "ART", "ADJA"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Griechen sich regt, von sich selber, die Gestalten", "tokens": ["Grie\u00b7chen", "sich", "regt", ",", "von", "sich", "sel\u00b7ber", ",", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "VVFIN", "$,", "APPR", "PRF", "ADV", "$,", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht nachahmend, die auch urspr\u00fcnglich,", "tokens": ["Nicht", "nac\u00b7hah\u00b7mend", ",", "die", "auch", "ur\u00b7spr\u00fcng\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "L\u00e4chelnd auf \u00c4hnlichung sehn;", "tokens": ["L\u00e4\u00b7chelnd", "auf", "\u00c4hn\u00b7li\u00b7chung", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Heitert mich auf, wie lebender Tanz, den der J\u00fcngling", "tokens": ["Hei\u00b7tert", "mich", "auf", ",", "wie", "le\u00b7ben\u00b7der", "Tanz", ",", "den", "der", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Schleunig begann, und sein M\u00e4dchen, da die Fl\u00f6te", "tokens": ["Schleu\u00b7nig", "be\u00b7gann", ",", "und", "sein", "M\u00e4d\u00b7chen", ",", "da", "die", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wo im Schatten erscholl, der Spieler", "tokens": ["Wo", "im", "Schat\u00b7ten", "er\u00b7scholl", ",", "der", "Spie\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "$,", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gern zu den liebenden kam:", "tokens": ["Gern", "zu", "den", "lie\u00b7ben\u00b7den", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Freundesgespr\u00e4ch, das ist es mir auch, wenn in Freud' und", "tokens": ["Freun\u00b7des\u00b7ge\u00b7spr\u00e4ch", ",", "das", "ist", "es", "mir", "auch", ",", "wenn", "in", "Freud'", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "APPR", "NN", "KON"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Leide das Herz nun dahinstr\u00f6mt! O ge\u00f6fnet", "tokens": ["Lei\u00b7de", "das", "Herz", "nun", "da\u00b7hin\u00b7str\u00f6mt", "!", "O", "ge\u00b7\u00f6f\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "VVFIN", "$.", "NE", "VVPP"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wird es dann, wie vor Gott, dann rinnen", "tokens": ["Wird", "es", "dann", ",", "wie", "vor", "Gott", ",", "dann", "rin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "APPR", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beiderley Thr\u00e4nen herab!", "tokens": ["Bei\u00b7der\u00b7ley", "Thr\u00e4\u00b7nen", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}