{"textgrid.poem.43781": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Im Winter", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist der Winter die schlimmste Zeit:", "tokens": ["Wohl", "ist", "der", "Win\u00b7ter", "die", "schlimms\u00b7te", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Fr\u00fchling, er ist so weit, so weit!", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "er", "ist", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Von Gr\u00fcnen und Bl\u00fchen keine Spur,", "tokens": ["Von", "Gr\u00fc\u00b7nen", "und", "Bl\u00fc\u00b7hen", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Fenster gefrorene Blumen nur.", "tokens": ["Am", "Fens\u00b7ter", "ge\u00b7fro\u00b7re\u00b7ne", "Blu\u00b7men", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und dennoch tr\u00f6st' ich mich: mir bl\u00fcht", "tokens": ["Und", "den\u00b7noch", "tr\u00f6st'", "ich", "mich", ":", "mir", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ewiger Fr\u00fchling im Gem\u00fcth.", "tokens": ["Ein", "e\u00b7wi\u00b7ger", "Fr\u00fch\u00b7ling", "im", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich kann in Gedanken dem Winter entschweben", "tokens": ["Ich", "kann", "in", "Ge\u00b7dan\u00b7ken", "dem", "Win\u00b7ter", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und trotz dem Winter im Fr\u00fchling leben.", "tokens": ["Und", "trotz", "dem", "Win\u00b7ter", "im", "Fr\u00fch\u00b7ling", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}