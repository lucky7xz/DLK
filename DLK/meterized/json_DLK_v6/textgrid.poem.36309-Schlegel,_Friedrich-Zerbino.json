{"textgrid.poem.36309": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zerbino", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gemahlen und gewalkt mit munterm Spiele", "tokens": ["Ge\u00b7mah\u00b7len", "und", "ge\u00b7walkt", "mit", "mun\u00b7term", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schau hier des Volkes negative Dichter!", "tokens": ["Schau", "hier", "des", "Vol\u00b7kes", "ne\u00b7ga\u00b7ti\u00b7ve", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Versteh nur erst den tiefen Sinn der M\u00fchle,", "tokens": ["Ver\u00b7steh", "nur", "erst", "den", "tie\u00b7fen", "Sinn", "der", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So f\u00fchlst du Leser! bald im Haupt dich lichter.", "tokens": ["So", "f\u00fchlst", "du", "Le\u00b7ser", "!", "bald", "im", "Haupt", "dich", "lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "ADV", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Garten gleicht dies Buch im Festgew\u00fchle;", "tokens": ["Dem", "Gar\u00b7ten", "gleicht", "dies", "Buch", "im", "Fest\u00b7ge\u00b7w\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Maskiert erscheinen neu die armen Wichter,", "tokens": ["Mas\u00b7kiert", "er\u00b7schei\u00b7nen", "neu", "die", "ar\u00b7men", "Wich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warm haucht die Luft, Font\u00e4nen pl\u00e4tschern k\u00fchle,", "tokens": ["Warm", "haucht", "die", "Luft", ",", "Fon\u00b7t\u00e4\u00b7nen", "pl\u00e4t\u00b7schern", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ferne schimmern k\u00fcnstlich bunte Lichter.", "tokens": ["Und", "fer\u00b7ne", "schim\u00b7mern", "k\u00fcnst\u00b7lich", "bun\u00b7te", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verkehrt ist alles in den s\u00fc\u00dfen Possen,", "tokens": ["Ver\u00b7kehrt", "ist", "al\u00b7les", "in", "den", "s\u00fc\u00b7\u00dfen", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Statt Ya sagt das Eslein selber Ay;", "tokens": ["Statt", "Y\u00b7a", "sagt", "das", "Es\u00b7lein", "sel\u00b7ber", "Ay", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erg\u00f6tzlich spielen drein mit Narrenschw\u00e4nzen", "tokens": ["Er\u00b7g\u00f6tz\u00b7lich", "spie\u00b7len", "drein", "mit", "Nar\u00b7ren\u00b7schw\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Theater, Aufkl\u00e4rung und Nikolai.", "tokens": ["The\u00b7a\u00b7ter", ",", "Auf\u00b7kl\u00e4\u00b7rung", "und", "Ni\u00b7ko\u00b7lai", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So mahl denn Tieck! mahl ferner unverdrossen", "tokens": ["So", "mahl", "denn", "Tieck", "!", "mahl", "fer\u00b7ner", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "NE", "$.", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schriftensteller albernste Tendenzen.", "tokens": ["Der", "Schrif\u00b7ten\u00b7stel\u00b7ler", "al\u00b7berns\u00b7te", "Ten\u00b7den\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}}}}