{"textgrid.poem.39493": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du eilst nicht, leichte Lorbeer'n nur zu greifen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du eilst nicht, leichte Lorbeer'n nur zu greifen,", "tokens": ["Du", "eilst", "nicht", ",", "leich\u00b7te", "Lor\u00b7beer'n", "nur", "zu", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um m\u00fc\u00dfig dann des Lobes Trank zu schl\u00fcrfen.", "tokens": ["Um", "m\u00fc\u00b7\u00dfig", "dann", "des", "Lo\u00b7bes", "Trank", "zu", "schl\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn siebenfach Troph\u00e4en dir sich h\u00e4ufen,", "tokens": ["Wenn", "sie\u00b7ben\u00b7fach", "Trop\u00b7h\u00e4\u00b7en", "dir", "sich", "h\u00e4u\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wirst du der That, wie dein die Welt bed\u00fcrfen.", "tokens": ["Wirst", "du", "der", "That", ",", "wie", "dein", "die", "Welt", "be\u00b7d\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPOSAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So viel zu k\u00f6nnen, was man kann, zu d\u00fcrfen,", "tokens": ["So", "viel", "zu", "k\u00f6n\u00b7nen", ",", "was", "man", "kann", ",", "zu", "d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VMINF", "$,", "PRELS", "PIS", "VMFIN", "$,", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df zwar mit Gottgef\u00fchl die Brust ergreifen,", "tokens": ["Mu\u00df", "zwar", "mit", "Gott\u00b7ge\u00b7f\u00fchl", "die", "Brust", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, fliegend von Entw\u00fcrfen zu Entw\u00fcrfen,", "tokens": ["Doch", ",", "flie\u00b7gend", "von", "Ent\u00b7w\u00fcr\u00b7fen", "zu", "Ent\u00b7w\u00fcr\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft deiner Weisheit Frucht dein Muth nicht reifen.", "tokens": ["L\u00e4\u00dft", "dei\u00b7ner", "Weis\u00b7heit", "Frucht", "dein", "Muth", "nicht", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kaum hast du dich von deinem Werk geschieden,", "tokens": ["Kaum", "hast", "du", "dich", "von", "dei\u00b7nem", "Werk", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ist's dem Chaos wieder hingegeben;", "tokens": ["So", "ist's", "dem", "Chaos", "wie\u00b7der", "hin\u00b7ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zum Grabe wird die Wiege neuer Staaten.", "tokens": ["Zum", "Gra\u00b7be", "wird", "die", "Wie\u00b7ge", "neu\u00b7er", "Staa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O setze Herkulss\u00e4ulen deinen Thaten", "tokens": ["O", "set\u00b7ze", "Her\u00b7kuls\u00b7s\u00e4u\u00b7len", "dei\u00b7nen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Willst du von Nilus Haupt den Schleier heben,", "tokens": ["Willst", "du", "von", "Ni\u00b7lus", "Haupt", "den", "Schlei\u00b7er", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So st\u00fcrzen hinter dir die Pyramiden.", "tokens": ["So", "st\u00fcr\u00b7zen", "hin\u00b7ter", "dir", "die", "Py\u00b7ra\u00b7mi\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}