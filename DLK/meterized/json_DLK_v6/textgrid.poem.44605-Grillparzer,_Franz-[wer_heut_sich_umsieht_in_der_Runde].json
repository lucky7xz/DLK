{"textgrid.poem.44605": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wer heut sich umsieht in der Runde]", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer heut sich umsieht in der Runde", "tokens": ["Wer", "heut", "sich", "um\u00b7sieht", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und kehrt den Blick von Land zu Land,", "tokens": ["Und", "kehrt", "den", "Blick", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der glaubte nah die letzte Stunde,", "tokens": ["Der", "glaub\u00b7te", "nah", "die", "letz\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bosheit mit dem Ha\u00df im Bunde,", "tokens": ["Die", "Bos\u00b7heit", "mit", "dem", "Ha\u00df", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist doch nur der Unverstand.", "tokens": ["Und", "ist", "doch", "nur", "der", "Un\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar Schurken gibts, nur nicht so viele,", "tokens": ["Zwar", "Schur\u00b7ken", "gibts", ",", "nur", "nicht", "so", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "PTKNEG", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Raub begierig bei dem Brand,", "tokens": ["Nach", "Raub", "be\u00b7gie\u00b7rig", "bei", "dem", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch was uns macht zu ihrem Spiele,", "tokens": ["Doch", "was", "uns", "macht", "zu", "ih\u00b7rem", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie scheinbar n\u00e4her bringt dem Ziele,", "tokens": ["Sie", "schein\u00b7bar", "n\u00e4\u00b7her", "bringt", "dem", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist der andern Unverstand.", "tokens": ["Das", "ist", "der", "an\u00b7dern", "Un\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der M\u00f6rder selbst, mir grauts zu sagen,", "tokens": ["Der", "M\u00f6r\u00b7der", "selbst", ",", "mir", "grauts", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der jene grause Tat bestand,", "tokens": ["Der", "je\u00b7ne", "grau\u00b7se", "Tat", "be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er war nicht schlimm in fr\u00fchern Tagen;", "tokens": ["Er", "war", "nicht", "schlimm", "in", "fr\u00fc\u00b7hern", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erst als mit Unsinn er geschlagen,", "tokens": ["Erst", "als", "mit", "Un\u00b7sinn", "er", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erhob er seine Frevlerhand.", "tokens": ["Er\u00b7hob", "er", "sei\u00b7ne", "Frev\u00b7ler\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als in der Mitte von Gelichter", "tokens": ["Als", "in", "der", "Mit\u00b7te", "von", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man wog, was keine Wage wiegt,", "tokens": ["Man", "wog", ",", "was", "kei\u00b7ne", "Wa\u00b7ge", "wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hielt er erkoren sich zum Richter,", "tokens": ["Hielt", "er", "er\u00b7ko\u00b7ren", "sich", "zum", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entstanden jene Nachtgesichter,", "tokens": ["Ent\u00b7stan\u00b7den", "je\u00b7ne", "Nacht\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Gottes Strahl und Schutz besiegt.", "tokens": ["Die", "Got\u00b7tes", "Strahl", "und", "Schutz", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Unverstand in unsern Zeiten", "tokens": ["Der", "Un\u00b7ver\u00b7stand", "in", "un\u00b7sern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und als Bezeichnung unsrer Welt?", "tokens": ["Und", "als", "Be\u00b7zeich\u00b7nung", "uns\u00b7rer", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh unsre Kenntnis sich erweiten,", "tokens": ["Sieh", "uns\u00b7re", "Kennt\u00b7nis", "sich", "er\u00b7wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Fortschritt sieh nach allen Seiten,", "tokens": ["Den", "Fort\u00b7schritt", "sieh", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Wissen jedermann gestellt.", "tokens": ["Auf", "Wis\u00b7sen", "je\u00b7der\u00b7mann", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}