{"textgrid.poem.34908": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Himmelsbr\u00e4ute", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer dem Kloster geht vorbei", "tokens": ["Wer", "dem", "Klos\u00b7ter", "geht", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mittern\u00e4chtlich, sieht die Fenster", "tokens": ["Mit\u00b7ter\u00b7n\u00e4cht\u00b7lich", ",", "sieht", "die", "Fens\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell erleuchtet. Ihren Umgang", "tokens": ["Hell", "er\u00b7leuch\u00b7tet", ".", "Ih\u00b7ren", "Um\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halten dorten die Gespenster.", "tokens": ["Hal\u00b7ten", "dor\u00b7ten", "die", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine d\u00fcstre Prozession", "tokens": ["Ei\u00b7ne", "d\u00fcst\u00b7re", "Pro\u00b7zes\u00b7si\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Toter Ursulinerinnen;", "tokens": ["To\u00b7ter", "Ur\u00b7su\u00b7li\u00b7ne\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Junge, h\u00fcbsche Angesichter", "tokens": ["Jun\u00b7ge", ",", "h\u00fcb\u00b7sche", "An\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschen aus Kapuz' und Linnen.", "tokens": ["Lau\u00b7schen", "aus", "Ka\u00b7puz'", "und", "Lin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tragen Kerzen in der Hand,", "tokens": ["Tra\u00b7gen", "Ker\u00b7zen", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die unheimlich blutrot schimmern;", "tokens": ["Die", "un\u00b7heim\u00b7lich", "blut\u00b7rot", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVINF", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Seltsam widerhallt im Kreuzgang", "tokens": ["Selt\u00b7sam", "wi\u00b7der\u00b7hallt", "im", "Kreuz\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Gewisper und ein Wimmern.", "tokens": ["Ein", "Ge\u00b7wis\u00b7per", "und", "ein", "Wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach der Kirche geht der Zug,", "tokens": ["Nach", "der", "Kir\u00b7che", "geht", "der", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie setzen dort sich nieder", "tokens": ["Und", "sie", "set\u00b7zen", "dort", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PRF", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Chores Buchsbaumst\u00fchle", "tokens": ["Auf", "des", "Cho\u00b7res", "Buchs\u00b7baum\u00b7st\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und beginnen ihre Lieder.", "tokens": ["Und", "be\u00b7gin\u00b7nen", "ih\u00b7re", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Litaneienfromme Weisen,", "tokens": ["Li\u00b7ta\u00b7nei\u00b7en\u00b7from\u00b7me", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wahnsinnw\u00fcste Worte;", "tokens": ["A\u00b7ber", "wahn\u00b7sinn\u00b7w\u00fcs\u00b7te", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Arme Seelen sind es, welche", "tokens": ["Ar\u00b7me", "See\u00b7len", "sind", "es", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pochen an des Himmels Pforte.", "tokens": ["Po\u00b7chen", "an", "des", "Him\u00b7mels", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbbr\u00e4ute Christi waren wir,", "tokens": ["\u00bb", "br\u00e4u\u00b7te", "Chris\u00b7ti", "wa\u00b7ren", "wir", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Weltlust uns bet\u00f6rte,", "tokens": ["Doch", "die", "Welt\u00b7lust", "uns", "be\u00b7t\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da gaben wir dem C\u00e4sar,", "tokens": ["Und", "da", "ga\u00b7ben", "wir", "dem", "C\u00e4\u00b7sar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NE", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.4": {"text": "Was dem lieben Gott geh\u00f6rte.", "tokens": ["Was", "dem", "lie\u00b7ben", "Gott", "ge\u00b7h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Reizend ist die Uniform", "tokens": ["Rei\u00b7zend", "ist", "die", "U\u00b7ni\u00b7form"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Schnurrbarts Glanz und Gl\u00e4tte;", "tokens": ["Und", "des", "Schnurr\u00b7barts", "Glanz", "und", "Gl\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verlockend sind am meisten", "tokens": ["Doch", "ver\u00b7lo\u00b7ckend", "sind", "am", "meis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "C\u00e4sars goldne Epaulette.", "tokens": ["C\u00e4\u00b7sars", "gold\u00b7ne", "E\u00b7pau\u00b7let\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, der Stirne, welche trug", "tokens": ["Ach", ",", "der", "Stir\u00b7ne", ",", "wel\u00b7che", "trug"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Dornenkrone weiland,", "tokens": ["Ei\u00b7ne", "Dor\u00b7nen\u00b7kro\u00b7ne", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gaben wir ein Hirschgeweihe \u2013", "tokens": ["Ga\u00b7ben", "wir", "ein", "Hirschge\u00b7wei\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wir betrogen unsern Heiland.", "tokens": ["Wir", "be\u00b7tro\u00b7gen", "un\u00b7sern", "Hei\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jesus, der die G\u00fcte selbst,", "tokens": ["Je\u00b7sus", ",", "der", "die", "G\u00fc\u00b7te", "selbst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weinte sanft ob unsrer Fehle,", "tokens": ["Wein\u00b7te", "sanft", "ob", "uns\u00b7rer", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sprach: \u203aVermaledeit", "tokens": ["Und", "er", "sprach", ":", "\u203a", "Ver\u00b7ma\u00b7le\u00b7deit"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verdammt sei eure Seele!\u2039", "tokens": ["Und", "ver\u00b7dammt", "sei", "eu\u00b7re", "See\u00b7le", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Grabenstiegner Spuk der Nacht,", "tokens": ["Gra\u00b7ben\u00b7stieg\u00b7ner", "Spuk", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcssen b\u00fc\u00dfend wir nunmehre", "tokens": ["M\u00fcs\u00b7sen", "b\u00fc\u00b7\u00dfend", "wir", "nun\u00b7meh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Irregehn in diesen Mauern \u2013", "tokens": ["Ir\u00b7re\u00b7gehn", "in", "die\u00b7sen", "Mau\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Miserere! Miserere!", "tokens": ["Mi\u00b7se\u00b7re\u00b7re", "!", "Mi\u00b7se\u00b7re\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, im Grabe ist es gut,", "tokens": ["Ach", ",", "im", "Gra\u00b7be", "ist", "es", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es gleich viel besser w\u00e4re", "tokens": ["Ob", "es", "gleich", "viel", "bes\u00b7ser", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem warmen Himmelreiche \u2013", "tokens": ["In", "dem", "war\u00b7men", "Him\u00b7mel\u00b7rei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Miserere! Miserere!", "tokens": ["Mi\u00b7se\u00b7re\u00b7re", "!", "Mi\u00b7se\u00b7re\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "S\u00fc\u00dfer Jesus, o vergib", "tokens": ["S\u00fc\u00b7\u00dfer", "Je\u00b7sus", ",", "o", "ver\u00b7gib"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "FM", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Endlich uns die Schuld, die schwere,", "tokens": ["End\u00b7lich", "uns", "die", "Schuld", ",", "die", "schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlie\u00df uns auf den warmen Himmel \u2013", "tokens": ["Schlie\u00df", "uns", "auf", "den", "war\u00b7men", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Miserere! Miserere!\u00ab", "tokens": ["Mi\u00b7se\u00b7re\u00b7re", "!", "Mi\u00b7se\u00b7re\u00b7re", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Also singt die Nonnenschar,", "tokens": ["Al\u00b7so", "singt", "die", "Non\u00b7nen\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein l\u00e4ngst verstorbner K\u00fcster", "tokens": ["Und", "ein", "l\u00e4ngst", "ver\u00b7storb\u00b7ner", "K\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Spielt die Orgel. Schattenh\u00e4nde", "tokens": ["Spielt", "die", "Or\u00b7gel", ".", "Schat\u00b7ten\u00b7h\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrmen toll durch die Register.", "tokens": ["St\u00fcr\u00b7men", "toll", "durch", "die", "Re\u00b7gis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}