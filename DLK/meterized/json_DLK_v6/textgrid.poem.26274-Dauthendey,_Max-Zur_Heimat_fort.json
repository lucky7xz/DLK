{"textgrid.poem.26274": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Zur Heimat fort", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sitze in einem gro\u00dfen Baum,", "tokens": ["Ich", "sit\u00b7ze", "in", "ei\u00b7nem", "gro\u00b7\u00dfen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weit greifen die \u00c4ste zum Himmelsraum.", "tokens": ["Weit", "grei\u00b7fen", "die", "\u00c4s\u00b7te", "zum", "Him\u00b7mels\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So klammern sich meine Gedanken ins Leere", "tokens": ["So", "klam\u00b7mern", "sich", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "ins", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zur Heimat fort \u00fcber luftige Meere.", "tokens": ["Zur", "Hei\u00b7mat", "fort", "\u00fc\u00b7ber", "luf\u00b7ti\u00b7ge", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Wind nur belebt gr\u00fcner Bl\u00e4tter Schar,", "tokens": ["Der", "Wind", "nur", "be\u00b7lebt", "gr\u00fc\u00b7ner", "Bl\u00e4t\u00b7ter", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und er bewegt am Haupt mein Haar.", "tokens": ["Und", "er", "be\u00b7wegt", "am", "Haupt", "mein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Blut, das erstarrte, horcht aufgetaut,", "tokens": ["Mein", "Blut", ",", "das", "er\u00b7starr\u00b7te", ",", "horcht", "auf\u00b7ge\u00b7taut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es sucht im Wind einen Heimatlaut.", "tokens": ["Es", "sucht", "im", "Wind", "ei\u00b7nen", "Hei\u00b7mat\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}