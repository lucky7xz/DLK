{"textgrid.poem.47460": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wei\u00dft du noch, mein s\u00fc\u00dfes Herz, wie Alles sich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du noch, mein s\u00fc\u00dfes Herz, wie Alles sich", "tokens": ["Wei\u00dft", "du", "noch", ",", "mein", "s\u00fc\u00b7\u00dfes", "Herz", ",", "wie", "Al\u00b7les", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PIS", "PRF"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Hold begeben zwischen dir und mir?", "tokens": ["Hold", "be\u00b7ge\u00b7ben", "zwi\u00b7schen", "dir", "und", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie der Liebe Siegelring auf meine Stirn", "tokens": ["Wie", "der", "Lie\u00b7be", "Sie\u00b7gel\u00b7ring", "auf", "mei\u00b7ne", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dr\u00fcckte schon der erste Blick von dir?", "tokens": ["Dr\u00fcck\u00b7te", "schon", "der", "ers\u00b7te", "Blick", "von", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie zu schelten deine Lippe rang und doch", "tokens": ["Wie", "zu", "schel\u00b7ten", "dei\u00b7ne", "Lip\u00b7pe", "rang", "und", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKZU", "VVFIN", "PPOSAT", "NN", "VVFIN", "KON", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Honigk\u00fcsse tr\u00e4ufelten von ihr?", "tokens": ["Ho\u00b7nig\u00b7k\u00fcs\u00b7se", "tr\u00e4u\u00b7fel\u00b7ten", "von", "ihr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie auf uns der stille Blick des Monds geruht", "tokens": ["Wie", "auf", "uns", "der", "stil\u00b7le", "Blick", "des", "Monds", "ge\u00b7ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und in seinem stillen Blicke wir!", "tokens": ["Und", "in", "sei\u00b7nem", "stil\u00b7len", "Bli\u00b7cke", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wie, was sich kein gl\u00e4ubiges Gem\u00fcthe tr\u00e4umt,", "tokens": ["Wie", ",", "was", "sich", "kein", "gl\u00e4u\u00b7bi\u00b7ges", "Ge\u00b7m\u00fc\u00b7the", "tr\u00e4umt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PRELS", "PRF", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns die Huld des Himmels schenkte hier?", "tokens": ["Uns", "die", "Huld", "des", "Him\u00b7mels", "schenk\u00b7te", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und wie dann Hafisens Verseperlenschatz", "tokens": ["Und", "wie", "dann", "Ha\u00b7fi\u00b7sens", "Ver\u00b7se\u00b7per\u00b7len\u00b7schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tausendfach an Werth gewann und Zier?", "tokens": ["Tau\u00b7send\u00b7fach", "an", "Werth", "ge\u00b7wann", "und", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wei\u00dft du noch, mein s\u00fc\u00dfes Herz, wie Alles sich", "tokens": ["Wei\u00dft", "du", "noch", ",", "mein", "s\u00fc\u00b7\u00dfes", "Herz", ",", "wie", "Al\u00b7les", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PIS", "PRF"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Hold begeben zwischen dir und mir?", "tokens": ["Hold", "be\u00b7ge\u00b7ben", "zwi\u00b7schen", "dir", "und", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}