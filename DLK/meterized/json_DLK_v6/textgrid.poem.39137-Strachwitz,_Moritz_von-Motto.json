{"textgrid.poem.39137": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Motto", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schacher und das Memmentum,", "tokens": ["Der", "Scha\u00b7cher", "und", "das", "Mem\u00b7men\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie spreizen sich allw\u00e4rts;", "tokens": ["Sie", "sprei\u00b7zen", "sich", "all\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wo spricht f\u00fcr Ehr' und Heldenruhm", "tokens": ["Wo", "spricht", "f\u00fcr", "Ehr'", "und", "Hel\u00b7den\u00b7ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ritterliches Herz?", "tokens": ["Ein", "rit\u00b7ter\u00b7li\u00b7ches", "Herz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch, wen der junge Tatendrang", "tokens": ["Doch", ",", "wen", "der", "jun\u00b7ge", "Ta\u00b7ten\u00b7drang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu kecker Wagnis zieht,", "tokens": ["Zu", "ke\u00b7cker", "Wag\u00b7nis", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der h\u00f6rt vielleicht zum Becherklang", "tokens": ["Der", "h\u00f6rt", "viel\u00b7leicht", "zum", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein altes Heldenlied.", "tokens": ["Ein", "al\u00b7tes", "Hel\u00b7den\u00b7lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Blut ist warm, mein Herz ist jung,", "tokens": ["Mein", "Blut", "ist", "warm", ",", "mein", "Herz", "ist", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gern l\u00e4uft es fort mit mir,", "tokens": ["Gern", "l\u00e4uft", "es", "fort", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gern schwingt es der Begeisterung", "tokens": ["Gern", "schwingt", "es", "der", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Glutfarbiges Panier;", "tokens": ["Glut\u00b7far\u00b7bi\u00b7ges", "Pa\u00b7nier", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Es w\u00fchlt noch gern mit Kindessinn", "tokens": ["Es", "w\u00fchlt", "noch", "gern", "mit", "Kin\u00b7des\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im alten Sagenwust:", "tokens": ["Im", "al\u00b7ten", "Sa\u00b7gen\u00b7wust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum nehmt sie heut in Frieden hin,", "tokens": ["Drum", "nehmt", "sie", "heut", "in", "Frie\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die bunte M\u00e4rchenlust.", "tokens": ["Die", "bun\u00b7te", "M\u00e4r\u00b7chen\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}