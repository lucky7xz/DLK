{"dta.poem.2843": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond am Himmel ist der Sonne beigegeben,", "tokens": ["Der", "Mond", "am", "Him\u00b7mel", "ist", "der", "Son\u00b7ne", "bei\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit sie beid' ein Bild vorhalten unserm Leben.", "tokens": ["Da\u00b7mit", "sie", "beid'", "ein", "Bild", "vor\u00b7hal\u00b7ten", "un\u00b7serm", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Mond bedeutet, da\u00df im Wechsel alles treibt;", "tokens": ["Der", "Mond", "be\u00b7deu\u00b7tet", ",", "da\u00df", "im", "Wech\u00b7sel", "al\u00b7les", "treibt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "APPRART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonne deutet, was im Wechsel gleich sich bleibt.", "tokens": ["Die", "Son\u00b7ne", "deu\u00b7tet", ",", "was", "im", "Wech\u00b7sel", "gleich", "sich", "bleibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPRART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Am Monde tr\u00f6ste dich bei Gl\u00fcckes Unbestand,", "tokens": ["Am", "Mon\u00b7de", "tr\u00f6s\u00b7te", "dich", "bei", "Gl\u00fc\u00b7ckes", "Un\u00b7be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "PRF", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um Best\u00e4ndigkeit blick auf zum Sonnenrand.", "tokens": ["Und", "um", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "blick", "auf", "zum", "Son\u00b7nen\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nimm ab und zu an Lust, dem Mond gleich, in Geduld;", "tokens": ["Nimm", "ab", "und", "zu", "an", "Lust", ",", "dem", "Mond", "gleich", ",", "in", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "APPR", "APPR", "NN", "$,", "ART", "NN", "ADV", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie die Sonne sei unwandelbar voll Huld.", "tokens": ["Und", "wie", "die", "Son\u00b7ne", "sei", "un\u00b7wan\u00b7del\u00b7bar", "voll", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}