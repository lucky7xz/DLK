{"textgrid.poem.35611": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunkle Stille der Kindheit. Unter gr\u00fcnenden Eschen", "tokens": ["Dunk\u00b7le", "Stil\u00b7le", "der", "Kind\u00b7heit", ".", "Un\u00b7ter", "gr\u00fc\u00b7nen\u00b7den", "E\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Weidet die Sanftmut bl\u00e4ulichen Blickes; goldene Ruh.", "tokens": ["Wei\u00b7det", "die", "Sanft\u00b7mut", "bl\u00e4u\u00b7li\u00b7chen", "Bli\u00b7ckes", ";", "gol\u00b7de\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ein Dunkles entz\u00fcckt der Duft der Veilchen; schwankende \u00c4hren", "tokens": ["Ein", "Dunk\u00b7les", "ent\u00b7z\u00fcckt", "der", "Duft", "der", "Veil\u00b7chen", ";", "schwan\u00b7ken\u00b7de", "\u00c4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Abend, Samen und die goldenen Schatten der Schwermut.", "tokens": ["Im", "A\u00b7bend", ",", "Sa\u00b7men", "und", "die", "gol\u00b7de\u00b7nen", "Schat\u00b7ten", "der", "Schwer\u00b7mut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Balken behaut der Zimmermann; im d\u00e4mmernden Grund", "tokens": ["Bal\u00b7ken", "be\u00b7haut", "der", "Zim\u00b7mer\u00b7mann", ";", "im", "d\u00e4m\u00b7mern\u00b7den", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Mahlt die M\u00fchle; im Hasellaub w\u00f6lbt sich ein purpurner Mund,", "tokens": ["Mahlt", "die", "M\u00fch\u00b7le", ";", "im", "Ha\u00b7sel\u00b7laub", "w\u00f6lbt", "sich", "ein", "pur\u00b7pur\u00b7ner", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--++-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "M\u00e4nnliches rot \u00fcber schweigende Wasser geneigt.", "tokens": ["M\u00e4nn\u00b7li\u00b7ches", "rot", "\u00fc\u00b7ber", "schwei\u00b7gen\u00b7de", "Was\u00b7ser", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.8": {"text": "Leise ist der Herbst, der Geist des Waldes; goldene Wolke", "tokens": ["Lei\u00b7se", "ist", "der", "Herbst", ",", "der", "Geist", "des", "Wal\u00b7des", ";", "gol\u00b7de\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Folgt dem Einsamen, der schwarze Schatten des Enkels.", "tokens": ["Folgt", "dem", "Ein\u00b7sa\u00b7men", ",", "der", "schwar\u00b7ze", "Schat\u00b7ten", "des", "En\u00b7kels", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+---+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Neige in steinernem Zimmer; unter alten Zypressen", "tokens": ["Nei\u00b7ge", "in", "stei\u00b7ner\u00b7nem", "Zim\u00b7mer", ";", "un\u00b7ter", "al\u00b7ten", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-+--", "measure": "elegiambus"}, "line.11": {"text": "Sind der Tr\u00e4nen n\u00e4chtige Bilder zum Quell versammelt;", "tokens": ["Sind", "der", "Tr\u00e4\u00b7nen", "n\u00e4ch\u00b7ti\u00b7ge", "Bil\u00b7der", "zum", "Quell", "ver\u00b7sam\u00b7melt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Goldenes Auge des Anbeginns, dunkle Geduld des Endes.", "tokens": ["Gol\u00b7de\u00b7nes", "Au\u00b7ge", "des", "An\u00b7be\u00b7ginns", ",", "dunk\u00b7le", "Ge\u00b7duld", "des", "En\u00b7des", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}}}}}