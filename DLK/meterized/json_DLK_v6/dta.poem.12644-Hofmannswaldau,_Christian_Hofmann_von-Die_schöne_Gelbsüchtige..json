{"dta.poem.12644": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Gelbs\u00fcchtige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.71", "no:0.14", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zjeht meine sonne denn der sonnen farben an?", "tokens": ["Zjeht", "mei\u00b7ne", "son\u00b7ne", "denn", "der", "son\u00b7nen", "far\u00b7ben", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will ihr gesichte sich in kostbar gold verwandeln?", "tokens": ["Will", "ihr", "ge\u00b7sich\u00b7te", "sich", "in", "kost\u00b7bar", "gold", "ver\u00b7wan\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PRF", "APPR", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist? da\u00df sie itzund so sehr ver\u00e4ndern kan?", "tokens": ["Was", "ist", "?", "da\u00df", "sie", "it\u00b7zund", "so", "sehr", "ver\u00b7\u00e4n\u00b7dern", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie vor rosen-schmuck will sonnen-zier erhandeln.", "tokens": ["Da\u00df", "sie", "vor", "ro\u00b7sen\u00b7schmuck", "will", "son\u00b7nen\u00b7zier", "er\u00b7han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die lilien sind weg, die rosen sind dahin,", "tokens": ["Die", "li\u00b7li\u00b7en", "sind", "weg", ",", "die", "ro\u00b7sen", "sind", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der purpur, der vorher die lippen kostbar machte,", "tokens": ["Der", "pur\u00b7pur", ",", "der", "vor\u00b7her", "die", "lip\u00b7pen", "kost\u00b7bar", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weicht vor des goldes pracht und l\u00e4st ihm den gewinn,", "tokens": ["Weicht", "vor", "des", "gol\u00b7des", "pracht", "und", "l\u00e4st", "ihm", "den", "ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den ihm vorher der ort und vieler lobspruch brachte.", "tokens": ["Den", "ihm", "vor\u00b7her", "der", "ort", "und", "vie\u00b7ler", "lob\u00b7spruch", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die wange, so vorher der anmuth wohnung war,", "tokens": ["Die", "wan\u00b7ge", ",", "so", "vor\u00b7her", "der", "an\u00b7muth", "woh\u00b7nung", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ver\u00e4ndert itzt die art, und wird von golde tr\u00fcbe,", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "itzt", "die", "art", ",", "und", "wird", "von", "gol\u00b7de", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KON", "VAFIN", "APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die erste sch\u00f6nheit liegt bereits auf ihrer bahr,", "tokens": ["Die", "ers\u00b7te", "sch\u00f6n\u00b7heit", "liegt", "be\u00b7reits", "auf", "ih\u00b7rer", "bahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und, wo ichs sagen darff, auch selber meine liebe.", "tokens": ["Und", ",", "wo", "ichs", "sa\u00b7gen", "darff", ",", "auch", "sel\u00b7ber", "mei\u00b7ne", "lie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADV", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch nein, ich irre mich, ich liebe ja den geist,", "tokens": ["Doch", "nein", ",", "ich", "ir\u00b7re", "mich", ",", "ich", "lie\u00b7be", "ja", "den", "geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit nichten aber nur die \u00e4usserliche zierde,", "tokens": ["Mit", "nich\u00b7ten", "a\u00b7ber", "nur", "die", "\u00e4us\u00b7ser\u00b7li\u00b7che", "zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die uns zwar offters auch mit zarter freude speist,", "tokens": ["Die", "uns", "zwar", "off\u00b7ters", "auch", "mit", "zar\u00b7ter", "freu\u00b7de", "speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch \u00fcberwand bey mir der tugend ehr-begierde.", "tokens": ["Doch", "\u00fc\u00b7ber\u00b7wand", "bey", "mir", "der", "tu\u00b7gend", "ehr\u00b7be\u00b7gier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nein, nein, Chlorinde nein, ich bin noch so gesinnt,", "tokens": ["Nein", ",", "nein", ",", "Chlo\u00b7rin\u00b7de", "nein", ",", "ich", "bin", "noch", "so", "ge\u00b7sinnt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "NN", "PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein gold, das wird mich nicht an meiner treu verletzen;", "tokens": ["Dein", "gold", ",", "das", "wird", "mich", "nicht", "an", "mei\u00b7ner", "treu", "ver\u00b7let\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wo das gl\u00fccke mir nur mein vergn\u00fcgen spinnt,", "tokens": ["Und", "wo", "das", "gl\u00fc\u00b7cke", "mir", "nur", "mein", "ver\u00b7gn\u00fc\u00b7gen", "spinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So werd ich mich an dir, auch da du gold, erg\u00f6tzen.", "tokens": ["So", "werd", "ich", "mich", "an", "dir", ",", "auch", "da", "du", "gold", ",", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "$,", "ADV", "KOUS", "PPER", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Von allen menschen wird das gold ja sehr geliebt,", "tokens": ["Von", "al\u00b7len", "men\u00b7schen", "wird", "das", "gold", "ja", "sehr", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Warum solt ich denn auch nicht dessen werth erheben,", "tokens": ["Wa\u00b7rum", "solt", "ich", "denn", "auch", "nicht", "des\u00b7sen", "werth", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "PDS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer wei\u00df warum dich itzt der himmel so betr\u00fcbt,", "tokens": ["Wer", "wei\u00df", "wa\u00b7rum", "dich", "itzt", "der", "him\u00b7mel", "so", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PWAV", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vielleichte zeigt er mir mit dir ein goldnes leben.", "tokens": ["Viel\u00b7leich\u00b7te", "zeigt", "er", "mir", "mit", "dir", "ein", "gold\u00b7nes", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das gold verbrennet nicht, so soll auch meine treu,", "tokens": ["Das", "gold", "ver\u00b7bren\u00b7net", "nicht", ",", "so", "soll", "auch", "mei\u00b7ne", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ADV", "VMFIN", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die ich dir zugesagt, kein ungemach zerstreuen,", "tokens": ["Die", "ich", "dir", "zu\u00b7ge\u00b7sagt", ",", "kein", "un\u00b7ge\u00b7mach", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$,", "PIAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gedencke, da\u00df ich noch itzt, wie vor diesem, sey,", "tokens": ["Ge\u00b7den\u00b7cke", ",", "da\u00df", "ich", "noch", "itzt", ",", "wie", "vor", "die\u00b7sem", ",", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ADV", "$,", "PWAV", "APPR", "PDAT", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der, welcher sich gedenckt an deiner zier zu freuen.", "tokens": ["Der", ",", "wel\u00b7cher", "sich", "ge\u00b7denckt", "an", "dei\u00b7ner", "zier", "zu", "freu\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "La\u00df alles gr\u00e4men seyn, weint doch die sonne nicht,", "tokens": ["La\u00df", "al\u00b7les", "gr\u00e4\u00b7men", "seyn", ",", "weint", "doch", "die", "son\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "ADJD", "VAINF", "$,", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Ob sie gleich eben mu\u00df dergleichen farbe tragen,", "tokens": ["Ob", "sie", "gleich", "e\u00b7ben", "mu\u00df", "derg\u00b7lei\u00b7chen", "far\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was dir vielleicht itzund an deiner zier gebricht,", "tokens": ["Was", "dir", "viel\u00b7leicht", "it\u00b7zund", "an", "dei\u00b7ner", "zier", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das wird in kurtzem denn dein aug und mund erjagen.", "tokens": ["Das", "wird", "in", "kurt\u00b7zem", "denn", "dein", "aug", "und", "mund", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "KON", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mich deucht, es \u00e4ndert sich bereits die gelbe haut,", "tokens": ["Mich", "deucht", ",", "es", "\u00e4n\u00b7dert", "sich", "be\u00b7reits", "die", "gel\u00b7be", "haut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die rosen lassen sich schon, wie von weitem, schauen,", "tokens": ["Die", "ro\u00b7sen", "las\u00b7sen", "sich", "schon", ",", "wie", "von", "wei\u00b7tem", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,", "PWAV", "APPR", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die anmuth, die ihr vor bey dir ein schlo\u00df gebaut,", "tokens": ["Die", "an\u00b7muth", ",", "die", "ihr", "vor", "bey", "dir", "ein", "schlo\u00df", "ge\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "APPR", "PPER", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Die wird dir ihren schatz bald wiederum vertrauen.", "tokens": ["Die", "wird", "dir", "ih\u00b7ren", "schatz", "bald", "wie\u00b7de\u00b7rum", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Kommt, kommt ihr lilien! ihr rosen s\u00e4nmt euch nicht!", "tokens": ["Kommt", ",", "kommt", "ihr", "li\u00b7li\u00b7en", "!", "ihr", "ro\u00b7sen", "s\u00e4nmt", "euch", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der purpur m\u00fcsse sich auch bald zum lippen legen!", "tokens": ["Der", "pur\u00b7pur", "m\u00fcs\u00b7se", "sich", "auch", "bald", "zum", "lip\u00b7pen", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PRF", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es eile di\u00df, was itzt Chlorinden noch gebricht,", "tokens": ["Es", "ei\u00b7le", "di\u00df", ",", "was", "itzt", "Chlo\u00b7rin\u00b7den", "noch", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So kan ihr auge mich, so, wie vor dem, bewegen.", "tokens": ["So", "kan", "ihr", "au\u00b7ge", "mich", ",", "so", ",", "wie", "vor", "dem", ",", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "$,", "ADV", "$,", "PWAV", "APPR", "ART", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Fahr hin, du falsches gold! du bringst uns doch nur pein;", "tokens": ["Fahr", "hin", ",", "du", "fal\u00b7sches", "gold", "!", "du", "bringst", "uns", "doch", "nur", "pein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wir wollen, glaub es nur, itzt deiner gern entbehren,", "tokens": ["Wir", "wol\u00b7len", ",", "glaub", "es", "nur", ",", "itzt", "dei\u00b7ner", "gern", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn, wenn Chlorinde soll de\u00dfwegen traurig seyn,", "tokens": ["Denn", ",", "wenn", "Chlo\u00b7rin\u00b7de", "soll", "de\u00df\u00b7we\u00b7gen", "trau\u00b7rig", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "NE", "VMFIN", "PAV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So wollen wir uns nicht an deine farbe kehren.", "tokens": ["So", "wol\u00b7len", "wir", "uns", "nicht", "an", "dei\u00b7ne", "far\u00b7be", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Chlorinde! schone dich indessen etwas wohl,", "tokens": ["Chlo\u00b7rin\u00b7de", "!", "scho\u00b7ne", "dich", "in\u00b7des\u00b7sen", "et\u00b7was", "wohl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und lache nur vielmehr bey deiner neuen farbe,", "tokens": ["Und", "la\u00b7che", "nur", "viel\u00b7mehr", "bey", "dei\u00b7ner", "neu\u00b7en", "far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Du weist ja ohnedi\u00df, was es bedeuten soll,", "tokens": ["Du", "weist", "ja", "oh\u00b7ne\u00b7di\u00df", ",", "was", "es", "be\u00b7deu\u00b7ten", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die farbe bringt noch nicht der sch\u00f6nheit eine narbe.", "tokens": ["Die", "far\u00b7be", "bringt", "noch", "nicht", "der", "sch\u00f6n\u00b7heit", "ei\u00b7ne", "nar\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Es giebt ja blumen auch, die so gef\u00e4rbet sind,", "tokens": ["Es", "giebt", "ja", "blu\u00b7men", "auch", ",", "die", "so", "ge\u00b7f\u00e4r\u00b7bet", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ADV", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Und dennoch pfleget uns nach ihnen zu gel\u00fcsten,", "tokens": ["Und", "den\u00b7noch", "pfle\u00b7get", "uns", "nach", "ih\u00b7nen", "zu", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ja wo das augen-licht dergleichen eine findt,", "tokens": ["Ja", "wo", "das", "au\u00b7gen\u00b7licht", "derg\u00b7lei\u00b7chen", "ei\u00b7ne", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "ART", "NN", "PIS", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So bringt man sie, und steckt sie gar zu jenen br\u00fcsten.", "tokens": ["So", "bringt", "man", "sie", ",", "und", "steckt", "sie", "gar", "zu", "je\u00b7nen", "br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die farben sind ein spiel des lichts und der natur,", "tokens": ["Die", "far\u00b7ben", "sind", "ein", "spiel", "des", "lichts", "und", "der", "na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.54": {"text": "Und all einander gleich an w\u00fcrd und am erziehen,", "tokens": ["Und", "all", "ein\u00b7an\u00b7der", "gleich", "an", "w\u00fcrd", "und", "am", "er\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PRF", "ADV", "APPR", "VAFIN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Doch da\u00df das auge mehr liebt der, als jener spur,", "tokens": ["Doch", "da\u00df", "das", "au\u00b7ge", "mehr", "liebt", "der", ",", "als", "je\u00b7ner", "spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "ART", "$,", "KOUS", "PDAT", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.56": {"text": "Ist nur die bildung schuld, um die wir uns bem\u00fchen.", "tokens": ["Ist", "nur", "die", "bil\u00b7dung", "schuld", ",", "um", "die", "wir", "uns", "be\u00b7m\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Die hoffnung mache dir bald deine w\u00fcnsche wahr,", "tokens": ["Die", "hoff\u00b7nung", "ma\u00b7che", "dir", "bald", "dei\u00b7ne", "w\u00fcn\u00b7sche", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Das gold verwandle sich in rosen, lilien, nelcken,", "tokens": ["Das", "gold", "ver\u00b7wand\u00b7le", "sich", "in", "ro\u00b7sen", ",", "li\u00b7li\u00b7en", ",", "nel\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NE", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.59": {"text": "Die gelbsucht werffe sich auf eine todten-bahr!", "tokens": ["Die", "gelb\u00b7sucht", "werf\u00b7fe", "sich", "auf", "ei\u00b7ne", "tod\u00b7ten\u00b7bahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Denn was dich, Schone! kr\u00e4uckt, das mu\u00df wie gra\u00df verwelcken.", "tokens": ["Denn", "was", "dich", ",", "Scho\u00b7ne", "!", "kr\u00e4uckt", ",", "das", "mu\u00df", "wie", "gra\u00df", "ver\u00b7wel\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "$,", "NN", "$.", "VVFIN", "$,", "PDS", "VMFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}