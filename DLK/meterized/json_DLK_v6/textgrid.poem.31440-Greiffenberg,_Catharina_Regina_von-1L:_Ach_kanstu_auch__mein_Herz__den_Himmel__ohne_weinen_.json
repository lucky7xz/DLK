{"textgrid.poem.31440": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach kanstu auch / mein Herz / den Himmel / ohne weinen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach kanstu auch / mein Herz / den Himmel / ohne weinen /", "tokens": ["Ach", "kans\u00b7tu", "auch", "/", "mein", "Herz", "/", "den", "Him\u00b7mel", "/", "oh\u00b7ne", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ADV", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "$(", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ohn' innern Herzens-brast / und \u00e4ussern Thr\u00e4nen See /", "tokens": ["ohn'", "in\u00b7nern", "Her\u00b7zens\u00b7brast", "/", "und", "\u00e4us\u00b7sern", "Thr\u00e4\u00b7nen", "See", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "NN", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "ansehen? da\u00df ich nicht vor lauter weh vergeh /", "tokens": ["an\u00b7se\u00b7hen", "?", "da\u00df", "ich", "nicht", "vor", "lau\u00b7ter", "weh", "ver\u00b7geh", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$.", "KOUS", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dieweil er gegen mir / ganz st\u00e4hlern ist und steinen!", "tokens": ["die\u00b7weil", "er", "ge\u00b7gen", "mir", "/", "ganz", "st\u00e4h\u00b7lern", "ist", "und", "stei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$(", "ADV", "ADJD", "VAFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach mag die Sonn' auch was so Elendes bescheinen?", "tokens": ["Ach", "mag", "die", "Sonn'", "auch", "was", "so", "E\u00b7len\u00b7des", "be\u00b7schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "ART", "NN", "ADV", "PWS", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "fa\u00df dir / mein Herz / ein Herz / und Leuen m\u00fctig steh'", "tokens": ["fa\u00df", "dir", "/", "mein", "Herz", "/", "ein", "Herz", "/", "und", "Leu\u00b7en", "m\u00fc\u00b7tig", "steh'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "$(", "KON", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "im Vngl\u00fccks-mittel- punct / das jederman dann seh /", "tokens": ["im", "Vn\u00b7gl\u00fccks\u00b7mit\u00b7tel", "punct", "/", "das", "je\u00b7der\u00b7man", "dann", "seh", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "VVFIN", "$(", "ART", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wie deine Tugend sich in tr\u00fcbsal pflegt zu feinen.", "tokens": ["wie", "dei\u00b7ne", "Tu\u00b7gend", "sich", "in", "tr\u00fcb\u00b7sal", "pflegt", "zu", "fei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPR", "NE", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Halt Gottes willen still! bricht schon das Herz vor schmerz /", "tokens": ["Halt", "Got\u00b7tes", "wil\u00b7len", "still", "!", "bricht", "schon", "das", "Herz", "vor", "schmerz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "PTKVZ", "$.", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wann nur der Wille ganz / ihm treu zu dienen / bleibet.", "tokens": ["wann", "nur", "der", "Wil\u00b7le", "ganz", "/", "ihm", "treu", "zu", "die\u00b7nen", "/", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "$(", "PPER", "ADJD", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Streit' / ihm zu Lob / mit dir: da\u00df nicht nur Blut austreibet /", "tokens": ["Streit'", "/", "ihm", "zu", "Lob", "/", "mit", "dir", ":", "da\u00df", "nicht", "nur", "Blut", "aus\u00b7trei\u00b7bet", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "APPR", "NN", "$(", "APPR", "PPER", "$.", "KOUS", "PTKNEG", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "besonder Geist und Krafft / verbrenn die Lebens-Kerz", "tokens": ["be\u00b7son\u00b7der", "Geist", "und", "Krafft", "/", "ver\u00b7brenn", "die", "Le\u00b7bens\u00b7Kerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "in seiner treuen Brunst. Denk / l\u00f6blich ist der Sieg /", "tokens": ["in", "sei\u00b7ner", "treu\u00b7en", "Brunst", ".", "Denk", "/", "l\u00f6b\u00b7lich", "ist", "der", "Sieg", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "NN", "$(", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wann nur mein Gott geehrt / wann ich schon unter lieg.", "tokens": ["wann", "nur", "mein", "Gott", "ge\u00b7ehrt", "/", "wann", "ich", "schon", "un\u00b7ter", "lieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVPP", "$(", "PWAV", "PPER", "ADV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}