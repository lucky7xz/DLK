{"textgrid.poem.50954": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am sonnig edlen Gartenhaus,", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am sonnig edlen Gartenhaus,", "tokens": ["Am", "son\u00b7nig", "ed\u00b7len", "Gar\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da reifet Traub an Traube,", "tokens": ["Da", "rei\u00b7fet", "Traub", "an", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sanfte Sch\u00f6ne tritt heraus,", "tokens": ["Die", "sanf\u00b7te", "Sch\u00f6\u00b7ne", "tritt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pr\u00fcft sinnend ihre Laube;", "tokens": ["Pr\u00fcft", "sin\u00b7nend", "ih\u00b7re", "Lau\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem blauen Blick der Sch\u00f6nen gleicht", "tokens": ["Dem", "blau\u00b7en", "Blick", "der", "Sch\u00f6\u00b7nen", "gleicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Beeren dunkle Menge,", "tokens": ["Der", "Bee\u00b7ren", "dunk\u00b7le", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohin ihr freundlich Auge reicht,", "tokens": ["Wo\u00b7hin", "ihr", "freund\u00b7lich", "Au\u00b7ge", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lacht freundliches Gedr\u00e4nge.", "tokens": ["Lacht", "freund\u00b7li\u00b7ches", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rings lockt der Trauben stille Glut", "tokens": ["Rings", "lockt", "der", "Trau\u00b7ben", "stil\u00b7le", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu H\u00e4upten und zu F\u00fc\u00dfen,", "tokens": ["Zu", "H\u00e4up\u00b7ten", "und", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sie beginnt mit stillem Mut", "tokens": ["Und", "sie", "be\u00b7ginnt", "mit", "stil\u00b7lem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu schneiden all die s\u00fc\u00dfen;", "tokens": ["Zu", "schnei\u00b7den", "all", "die", "s\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie sie mit der lieben Hand", "tokens": ["Und", "wie", "sie", "mit", "der", "lie\u00b7ben", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die goldnen Bl\u00e4tter teilet,", "tokens": ["Die", "gold\u00b7nen", "Bl\u00e4t\u00b7ter", "tei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Fluge \u00fcber See und Land", "tokens": ["Im", "Flu\u00b7ge", "\u00fc\u00b7ber", "See", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schweift hin der Blick und weilet.", "tokens": ["Schweift", "hin", "der", "Blick", "und", "wei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wie eine reife Beere gl\u00e4nzt", "tokens": ["Wie", "ei\u00b7ne", "rei\u00b7fe", "Bee\u00b7re", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr feuchtes Aug hin\u00fcber,", "tokens": ["Ihr", "feuch\u00b7tes", "Aug", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo's blaut und leuchtet unbegrenzt", "tokens": ["Wo's", "blaut", "und", "leuch\u00b7tet", "un\u00b7be\u00b7grenzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fern, so fern her\u00fcber;", "tokens": ["So", "fern", ",", "so", "fern", "her\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie l\u00e4sset still und ahnungsvoll", "tokens": ["Sie", "l\u00e4s\u00b7set", "still", "und", "ah\u00b7nungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die schweren Trauben sinken,", "tokens": ["Die", "schwe\u00b7ren", "Trau\u00b7ben", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bis es in K\u00f6rben reizend schwoll", "tokens": ["Bis", "es", "in", "K\u00f6r\u00b7ben", "rei\u00b7zend", "schwoll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit tausendfachem Blinken.", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7chem", "Blin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie wandelt hin und wandelt her", "tokens": ["Sie", "wan\u00b7delt", "hin", "und", "wan\u00b7delt", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesch\u00e4ftig durch den Garten,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "durch", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis all die K\u00f6rbe, fr\u00fcchteschwer,", "tokens": ["Bis", "all", "die", "K\u00f6r\u00b7be", ",", "fr\u00fcch\u00b7te\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gereiht der Kelter warten.", "tokens": ["Ge\u00b7reiht", "der", "Kel\u00b7ter", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Kelter ist gar reich gebaut,", "tokens": ["Die", "Kel\u00b7ter", "ist", "gar", "reich", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Recht f\u00fcr der Sch\u00f6nen H\u00e4nde;", "tokens": ["Recht", "f\u00fcr", "der", "Sch\u00f6\u00b7nen", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Silber man die Spindel schaut,", "tokens": ["Von", "Sil\u00b7ber", "man", "die", "Spin\u00b7del", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Rosenholz die W\u00e4nde.", "tokens": ["Von", "Ro\u00b7sen\u00b7holz", "die", "W\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie steht auf einem Marmortisch.", "tokens": ["Sie", "steht", "auf", "ei\u00b7nem", "Mar\u00b7mor\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winzerin beginnet,", "tokens": ["Die", "Win\u00b7ze\u00b7rin", "be\u00b7gin\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df aus der Kelter s\u00fc\u00df und frisch", "tokens": ["Da\u00df", "aus", "der", "Kel\u00b7ter", "s\u00fc\u00df", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Blut der Traube rinnet;", "tokens": ["Das", "Blut", "der", "Trau\u00b7be", "rin\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie reg der wei\u00dfen Arme Zier", "tokens": ["Wie", "reg", "der", "wei\u00b7\u00dfen", "Ar\u00b7me", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit holder Kraft sich m\u00fchet!", "tokens": ["Mit", "hol\u00b7der", "Kraft", "sich", "m\u00fc\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie keltert, bis die Wange ihr", "tokens": ["Sie", "kel\u00b7tert", ",", "bis", "die", "Wan\u00b7ge", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In dunklem Purpur gl\u00fchet.", "tokens": ["In", "dunk\u00b7lem", "Pur\u00b7pur", "gl\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie keltert, da\u00df der Busen fliegt", "tokens": ["Sie", "kel\u00b7tert", ",", "da\u00df", "der", "Bu\u00b7sen", "fliegt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und woget ungemessen,", "tokens": ["Und", "wo\u00b7get", "un\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umsonst \u2013 was ihr im Sinne liegt,", "tokens": ["Um\u00b7sonst", "\u2013", "was", "ihr", "im", "Sin\u00b7ne", "liegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kann sie nicht vergessen!", "tokens": ["Das", "kann", "sie", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst \u2013 und wie die Kr\u00fcge sie", "tokens": ["Um\u00b7sonst", "\u2013", "und", "wie", "die", "Kr\u00fc\u00b7ge", "sie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KON", "PWAV", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit edlem Moste f\u00fcllet:", "tokens": ["Mit", "ed\u00b7lem", "Mos\u00b7te", "f\u00fcl\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie selber hat den Durst noch nie,", "tokens": ["Sie", "sel\u00b7ber", "hat", "den", "Durst", "noch", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Sehnen nie gestillet.", "tokens": ["Das", "Seh\u00b7nen", "nie", "ge\u00b7stil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie l\u00e4\u00dft den s\u00fc\u00dfen Feuersaft", "tokens": ["Sie", "l\u00e4\u00dft", "den", "s\u00fc\u00b7\u00dfen", "Feu\u00b7er\u00b7saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschlossen in sich g\u00e4ren,", "tokens": ["Ver\u00b7schlos\u00b7sen", "in", "sich", "g\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In k\u00fchler Nacht zu milder Kraft,", "tokens": ["In", "k\u00fch\u00b7ler", "Nacht", "zu", "mil\u00b7der", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum seltnen Wein verj\u00e4hren;", "tokens": ["Zum", "selt\u00b7nen", "Wein", "ver\u00b7j\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den tr\u00e4gt sie zu den H\u00fctten hin", "tokens": ["Den", "tr\u00e4gt", "sie", "zu", "den", "H\u00fct\u00b7ten", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl auf und ab im Tale,", "tokens": ["Wohl", "auf", "und", "ab", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie reicht der armen W\u00f6chnerin,", "tokens": ["Sie", "reicht", "der", "ar\u00b7men", "W\u00f6ch\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem kranken Greis die Schale.", "tokens": ["Dem", "kran\u00b7ken", "Greis", "die", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So keltert sie den Edelwein", "tokens": ["So", "kel\u00b7tert", "sie", "den", "E\u00b7del\u00b7wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Herbst seit manchen Jahren.", "tokens": ["Im", "Herbst", "seit", "man\u00b7chen", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Segel kommt im goldnen Schein", "tokens": ["Ein", "Se\u00b7gel", "kommt", "im", "gold\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Abends fern gefahren,", "tokens": ["Des", "A\u00b7bends", "fern", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Schifflein legt im Hafen an,", "tokens": ["Ein", "Schif\u00b7flein", "legt", "im", "Ha\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie h\u00f6rt die Schiffer singen,", "tokens": ["Sie", "h\u00f6rt", "die", "Schif\u00b7fer", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und einen hochgemuten Mann", "tokens": ["Und", "ei\u00b7nen", "hoch\u00b7ge\u00b7mu\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht sie ans Ufer springen.", "tokens": ["Sieht", "sie", "ans", "U\u00b7fer", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie kennt ihn und sie kennt ihn nicht,", "tokens": ["Sie", "kennt", "ihn", "und", "sie", "kennt", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie starrt hinaus ins Weite,", "tokens": ["Sie", "starrt", "hin\u00b7aus", "ins", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als es mit trauter Stimme spricht", "tokens": ["Als", "es", "mit", "trau\u00b7ter", "Stim\u00b7me", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dft schon ihr zur Seite.", "tokens": ["Und", "gr\u00fc\u00dft", "schon", "ihr", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die holden Kl\u00e4nge mischen sich,", "tokens": ["Die", "hol\u00b7den", "Kl\u00e4n\u00b7ge", "mi\u00b7schen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wort hier, dort die Lieder:", "tokens": ["Das", "Wort", "hier", ",", "dort", "die", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbratlos verlie\u00df der Knabe dich,", "tokens": ["\u00bb", "rat\u00b7los", "ver\u00b7lie\u00df", "der", "Kna\u00b7be", "dich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Mann kehrt dir nun wieder!", "tokens": ["Ein", "Mann", "kehrt", "dir", "nun", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O schau, wie leuchtet's weit u