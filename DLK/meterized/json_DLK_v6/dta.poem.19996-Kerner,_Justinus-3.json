{"dta.poem.19996": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du strebtest oft, ein herzlich Kind, mit Thr\u00e4nen", "tokens": ["Du", "streb\u00b7test", "oft", ",", "ein", "herz\u00b7lich", "Kind", ",", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "ADJD", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur\u00fcck zur s\u00fc\u00dfen Heimat, zu den Lieben,", "tokens": ["Zu\u00b7r\u00fcck", "zur", "s\u00fc\u00b7\u00dfen", "Hei\u00b7mat", ",", "zu", "den", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die fern in Kampf und Sturm Dich mu\u00dften w\u00e4hnen,", "tokens": ["Die", "fern", "in", "Kampf", "und", "Sturm", "Dich", "mu\u00df\u00b7ten", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indessen sie im sichern Port geblieben.", "tokens": ["In\u00b7des\u00b7sen", "sie", "im", "si\u00b7chern", "Port", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du treues Herz! nun ist erf\u00fcllt dein Sehnen,", "tokens": ["Du", "treu\u00b7es", "Herz", "!", "nun", "ist", "er\u00b7f\u00fcllt", "dein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Auge soll fortan sich nimmer tr\u00fcben;", "tokens": ["Mein", "Au\u00b7ge", "soll", "for\u00b7tan", "sich", "nim\u00b7mer", "tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast deine Heimat nun, bist nun bei jenen,", "tokens": ["Hast", "dei\u00b7ne", "Hei\u00b7mat", "nun", ",", "bist", "nun", "bei", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,", "VAFIN", "ADV", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An die du weinend Gru\u00df und Ku\u00df geschrieben.", "tokens": ["An", "die", "du", "wei\u00b7nend", "Gru\u00df", "und", "Ku\u00df", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Morgenroth seh' ich verkl\u00e4rt dich wallen,", "tokens": ["Im", "Mor\u00b7gen\u00b7roth", "seh'", "ich", "ver\u00b7kl\u00e4rt", "dich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Sterne durch den Dom des Himmels ziehen,", "tokens": ["Wo", "Ster\u00b7ne", "durch", "den", "Dom", "des", "Him\u00b7mels", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gehst mit mir durch stille Au'n und Haine.", "tokens": ["Du", "gehst", "mit", "mir", "durch", "stil\u00b7le", "Au'n", "und", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Oft h\u00f6r' ich deine liebe Stimme schallen,", "tokens": ["Oft", "h\u00f6r'", "ich", "dei\u00b7ne", "lie\u00b7be", "Stim\u00b7me", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' deinen Ku\u00df auf meinen Lippen gl\u00fchen,", "tokens": ["F\u00fchl'", "dei\u00b7nen", "Ku\u00df", "auf", "mei\u00b7nen", "Lip\u00b7pen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seh' dich mitleidig l\u00e4cheln, wenn ich weine.", "tokens": ["Seh'", "dich", "mit\u00b7lei\u00b7dig", "l\u00e4\u00b7cheln", ",", "wenn", "ich", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}