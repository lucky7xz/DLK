{"textgrid.poem.53768": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Arbeit tut not \u2013!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es raucht der Schlot. Sirene gellt.", "tokens": ["Es", "raucht", "der", "Schlot", ".", "Si\u00b7re\u00b7ne", "gellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeit tobt durch die deutsche Welt:", "tokens": ["Ar\u00b7beit", "tobt", "durch", "die", "deut\u00b7sche", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Noch mehr Tender \u2013!", "tokens": ["Noch", "mehr", "Ten\u00b7der", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Graumorgens taumelt, lungenkrank,", "tokens": ["Grau\u00b7mor\u00b7gens", "tau\u00b7melt", ",", "lun\u00b7gen\u00b7krank", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "der Mann aus seinem Menschenschrank.", "tokens": ["der", "Mann", "aus", "sei\u00b7nem", "Men\u00b7schen\u00b7schrank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Pfeife hetzt zum Eingangstor,", "tokens": ["Die", "Pfei\u00b7fe", "hetzt", "zum", "Ein\u00b7gangs\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kontrolluhr, W\u00e4chter und Hund davor . . .", "tokens": ["Kont\u00b7rol\u00b7luhr", ",", "W\u00e4ch\u00b7ter", "und", "Hund", "da\u00b7vor", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "PAV", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Noch mehr Tender! Noch mehr Automobile!", "tokens": ["Noch", "mehr", "Ten\u00b7der", "!", "Noch", "mehr", "Au\u00b7to\u00b7mo\u00b7bi\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Stumpfsinn treibt die Transmission.", "tokens": ["Der", "Stumpf\u00b7sinn", "treibt", "die", "Trans\u00b7mis\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wir haben auch einen Leitspruch schon:", "tokens": ["Wir", "ha\u00b7ben", "auch", "ei\u00b7nen", "Leits\u00b7pruch", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Arbeit tut not!", "tokens": ["Ar\u00b7beit", "tut", "not", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.4": {"text": "Die Fr\u00e4ser surren,", "tokens": ["Die", "Fr\u00e4\u00b7ser", "sur\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "H\u00e4mmer h\u00e4mmern, die S\u00e4gen schnurren . . .", "tokens": ["H\u00e4m\u00b7mer", "h\u00e4m\u00b7mern", ",", "die", "S\u00e4\u00b7gen", "schnur\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVINF", "$,", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Noch mehr Tender! Noch mehr Automobile!", "tokens": ["Noch", "mehr", "Ten\u00b7der", "!", "Noch", "mehr", "Au\u00b7to\u00b7mo\u00b7bi\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Noch mehr Zangen! Noch mehr Spatenstiele!", "tokens": ["Noch", "mehr", "Zan\u00b7gen", "!", "Noch", "mehr", "Spa\u00b7ten\u00b7stie\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Grau stickt B\u00fcroluft alle Lungen.", "tokens": ["Grau", "stickt", "B\u00fc\u00b7ro\u00b7luft", "al\u00b7le", "Lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "PIAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hier hockt die Jugend; hier sitzen die Jungen.", "tokens": ["Hier", "hockt", "die", "Ju\u00b7gend", ";", "hier", "sit\u00b7zen", "die", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Rabatte gellen durchs Telefon \u2013", "tokens": ["Ra\u00b7bat\u00b7te", "gel\u00b7len", "durchs", "Te\u00b7le\u00b7fon", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "es klappert Underwood und Cohn:", "tokens": ["es", "klap\u00b7pert", "Un\u00b7der\u00b7wood", "und", "Cohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch mehr Tender! Noch mehr Automobile!", "tokens": ["Noch", "mehr", "Ten\u00b7der", "!", "Noch", "mehr", "Au\u00b7to\u00b7mo\u00b7bi\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Noch mehr Zangen! Noch mehr Spatenstiele!", "tokens": ["Noch", "mehr", "Zan\u00b7gen", "!", "Noch", "mehr", "Spa\u00b7ten\u00b7stie\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Noch mehr Aktien! Noch mehr Industrie!", "tokens": ["Noch", "mehr", "Ak\u00b7ti\u00b7en", "!", "Noch", "mehr", "In\u00b7dust\u00b7rie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und alles made in Germany!", "tokens": ["Und", "al\u00b7les", "ma\u00b7de", "in", "Ger\u00b7ma\u00b7ny", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Waren! An Waren profitieren!", "tokens": ["Wa\u00b7ren", "!", "An", "Wa\u00b7ren", "pro\u00b7fi\u00b7tie\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$.", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Waren sinnlos produzieren!", "tokens": ["Wa\u00b7ren", "sinn\u00b7los", "pro\u00b7du\u00b7zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will einer sie haben? Kann einer kaufen?", "tokens": ["Will", "ei\u00b7ner", "sie", "ha\u00b7ben", "?", "Kann", "ei\u00b7ner", "kau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VAFIN", "$.", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Unser Land soll in Waren versaufen!", "tokens": ["Un\u00b7ser", "Land", "soll", "in", "Wa\u00b7ren", "ver\u00b7sau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Klopfen, h\u00e4mmern, schneiden und weben \u2013", "tokens": ["Klop\u00b7fen", ",", "h\u00e4m\u00b7mern", ",", "schnei\u00b7den", "und", "we\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "KON", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "eine Kleinigkeit fehlt: das Leben.", "tokens": ["ei\u00b7ne", "Klei\u00b7nig\u00b7keit", "fehlt", ":", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Kleben, kochen, f\u00e4rben und braten \u2013", "tokens": ["Kle\u00b7ben", ",", "ko\u00b7chen", ",", "f\u00e4r\u00b7ben", "und", "bra\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Kinder, macht Kinder! der Staat braucht Soldaten!", "tokens": ["Kin\u00b7der", ",", "macht", "Kin\u00b7der", "!", "der", "Staat", "braucht", "Sol\u00b7da\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Sind die Gr\u00e4ben einst voll, sinds die Gr\u00e4ber auch \u2013", "tokens": ["Sind", "die", "Gr\u00e4\u00b7ben", "einst", "voll", ",", "sinds", "die", "Gr\u00e4\u00b7ber", "auch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "das ist des Landes so der Brauch.", "tokens": ["das", "ist", "des", "Lan\u00b7des", "so", "der", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Produziert Kinder! Unentwegt!", "tokens": ["Pro\u00b7du\u00b7ziert", "Kin\u00b7der", "!", "Un\u00b7ent\u00b7wegt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie werden euch sp\u00e4ter in Kalk gelegt.", "tokens": ["Sie", "wer\u00b7den", "euch", "sp\u00e4\u00b7ter", "in", "Kalk", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Das ist Wirtschaftspolitik.", "tokens": ["Das", "ist", "Wirt\u00b7schafts\u00b7po\u00b7li\u00b7tik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und es bl\u00e4st die Milit\u00e4rmusik:", "tokens": ["Und", "es", "bl\u00e4st", "die", "Mi\u00b7li\u00b7t\u00e4r\u00b7mu\u00b7sik", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Noch mehr Granaten! Noch mehr Automobile!", "tokens": ["Noch", "mehr", "Gra\u00b7na\u00b7ten", "!", "Noch", "mehr", "Au\u00b7to\u00b7mo\u00b7bi\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-++-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Noch mehr Kinder! Noch mehr Spatenstiele!", "tokens": ["Noch", "mehr", "Kin\u00b7der", "!", "Noch", "mehr", "Spa\u00b7ten\u00b7stie\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Noch mehr Bleche! Noch mehr Krane!", "tokens": ["Noch", "mehr", "Ble\u00b7che", "!", "Noch", "mehr", "Kra\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Noch mehr Werften! Noch mehr Vulkane!", "tokens": ["Noch", "mehr", "Werf\u00b7ten", "!", "Noch", "mehr", "Vul\u00b7ka\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "In die Welt gepre\u00dft bis zum B\u00f6rsensieg \u2013", "tokens": ["In", "die", "Welt", "ge\u00b7pre\u00dft", "bis", "zum", "B\u00f6r\u00b7sen\u00b7sieg", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "APPR", "APPRART", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Und wenn sie nicht wollen, macht Deutschland Krieg!", "tokens": ["Und", "wenn", "sie", "nicht", "wol\u00b7len", ",", "macht", "Deutschland", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VMFIN", "$,", "VVFIN", "NE", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Wer wird uns den rasenden Kaufmann bezwingen \u2013?", "tokens": ["Wer", "wird", "uns", "den", "ra\u00b7sen\u00b7den", "Kauf\u00b7mann", "be\u00b7zwin\u00b7gen", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$(", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Arbeit tut not:", "tokens": ["Ar\u00b7beit", "tut", "not", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.3": {"text": "Die Masse wirds bringen.", "tokens": ["Die", "Mas\u00b7se", "wirds", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}