{"textgrid.poem.25018": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Aug' ist schwarz wie die Sturmesnacht,", "tokens": ["Dein", "Aug'", "ist", "schwarz", "wie", "die", "Stur\u00b7mes\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn Wolken den Himmel durchjagen;", "tokens": ["Wenn", "Wol\u00b7ken", "den", "Him\u00b7mel", "durch\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich blick' hinein in die wilde Pracht", "tokens": ["Ich", "blick'", "hin\u00b7ein", "in", "die", "wil\u00b7de", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und f\u00fchl' ein schwindelndes Zagen;", "tokens": ["Und", "f\u00fchl'", "ein", "schwin\u00b7deln\u00b7des", "Za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dann wieder wie aus der Unendlichkeit quillt", "tokens": ["Dann", "wie\u00b7der", "wie", "aus", "der", "Un\u00b7end\u00b7lich\u00b7keit", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--++--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein Glanz hervor, der das Bangen stillt.", "tokens": ["Ein", "Glanz", "her\u00b7vor", ",", "der", "das", "Ban\u00b7gen", "stillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Aug' ist schwarz, ist schwarz wie der Tod;", "tokens": ["Dein", "Aug'", "ist", "schwarz", ",", "ist", "schwarz", "wie", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Oft nur mit heimlichem Grauen,", "tokens": ["Oft", "nur", "mit", "heim\u00b7li\u00b7chem", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das mich in die Tiefe zu rei\u00dfen droht,", "tokens": ["Das", "mich", "in", "die", "Tie\u00b7fe", "zu", "rei\u00b7\u00dfen", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vermag ich hinein zu schauen;", "tokens": ["Ver\u00b7mag", "ich", "hin\u00b7ein", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Wonnen doch schauern aus ihm mich an,", "tokens": ["Und", "Won\u00b7nen", "doch", "schau\u00b7ern", "aus", "ihm", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "APPR", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die nie ich geahnt, noch fassen kann.", "tokens": ["Die", "nie", "ich", "ge\u00b7ahnt", ",", "noch", "fas\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVPP", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}