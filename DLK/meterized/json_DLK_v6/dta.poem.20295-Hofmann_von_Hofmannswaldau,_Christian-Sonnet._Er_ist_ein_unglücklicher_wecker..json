{"dta.poem.20295": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Er ist ein ungl\u00fccklicher wecker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich eilte Lesbien aus kurtzweil zu erwecken/", "tokens": ["Ich", "eil\u00b7te", "Les\u00b7bi\u00b7en", "aus", "kurt\u00b7zweil", "zu", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als gleich Aurorens glantz um ihr gesichte stund/", "tokens": ["Als", "gleich", "Au\u00b7ro\u00b7rens", "glantz", "um", "ihr", "ge\u00b7sich\u00b7te", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die rosen kr\u00f6nten ihr die wangen und den mund/", "tokens": ["Die", "ro\u00b7sen", "kr\u00f6n\u00b7ten", "ihr", "die", "wan\u00b7gen", "und", "den", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch weisses helffenbein lie\u00df sich der hals bedecken.", "tokens": ["Durch", "weis\u00b7ses", "helf\u00b7fen\u00b7bein", "lie\u00df", "sich", "der", "hals", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wolte meine hand auff ihre br\u00fcste strecken/", "tokens": ["Ich", "wol\u00b7te", "mei\u00b7ne", "hand", "auff", "ih\u00b7re", "br\u00fcs\u00b7te", "stre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es that ein nasser ku\u00df ihr meine geilheit kund.", "tokens": ["Es", "that", "ein", "nas\u00b7ser", "ku\u00df", "ihr", "mei\u00b7ne", "geil\u00b7heit", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ruffte Lesbie: Ist dein verstand gesund/", "tokens": ["Es", "ruff\u00b7te", "Les\u00b7bie", ":", "Ist", "dein", "ver\u00b7stand", "ge\u00b7sund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "VAFIN", "PPOSAT", "VVFIN", "ADJD", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So f\u00fchre keine brunst in meine keusche hecken.", "tokens": ["So", "f\u00fch\u00b7re", "kei\u00b7ne", "brunst", "in", "mei\u00b7ne", "keu\u00b7sche", "he\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich war darob best\u00fcrtzt/ und fluchte dem gel\u00fccke/", "tokens": ["Ich", "war", "da\u00b7rob", "be\u00b7st\u00fcrtzt", "/", "und", "fluch\u00b7te", "dem", "ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$(", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und fuhr den himmel an/ und seine reiche blicke.", "tokens": ["Und", "fuhr", "den", "him\u00b7mel", "an", "/", "und", "sei\u00b7ne", "rei\u00b7che", "bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$(", "KON", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich sprach: Wo rosen stehn/ da m\u00fcssen dornen seyn.", "tokens": ["Ich", "sprach", ":", "Wo", "ro\u00b7sen", "stehn", "/", "da", "m\u00fcs\u00b7sen", "dor\u00b7nen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "ADJD", "VVINF", "$(", "ADV", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil mich denn ihr befehl verjaget und vertrieben/", "tokens": ["Weil", "mich", "denn", "ihr", "be\u00b7fehl", "ver\u00b7ja\u00b7get", "und", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So hab ich dieses wort in ihr gemach geschrieben:", "tokens": ["So", "hab", "ich", "die\u00b7ses", "wort", "in", "ihr", "ge\u00b7mach", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auff morgenr\u00f6the folgt gar selten sonnenschein.", "tokens": ["Auff", "mor\u00b7gen\u00b7r\u00f6\u00b7the", "folgt", "gar", "sel\u00b7ten", "son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}