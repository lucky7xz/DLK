{"textgrid.poem.42279": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df Bl\u00fcmchen Wunderhold", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00fcht ein Bl\u00fcmchen irgend wo", "tokens": ["Es", "bl\u00fcht", "ein", "Bl\u00fcm\u00b7chen", "ir\u00b7gend", "wo"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem stillen Thal.", "tokens": ["In", "ei\u00b7nem", "stil\u00b7len", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das schmeichelt Aug' und Herz so froh,", "tokens": ["Das", "schmei\u00b7chelt", "Aug'", "und", "Herz", "so", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Abendsonnenstrahl.", "tokens": ["Wie", "A\u00b7bend\u00b7son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist viel k\u00f6stlicher, als Gold,", "tokens": ["Das", "ist", "viel", "k\u00f6st\u00b7li\u00b7cher", ",", "als", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Perl' und Diamant.", "tokens": ["Als", "Perl'", "und", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum wird es \u00bbBl\u00fcmchen Wunderhold\u00ab", "tokens": ["Drum", "wird", "es", "\u00bb", "Bl\u00fcm\u00b7chen", "Wun\u00b7der\u00b7hold", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit gutem Fug genannt.", "tokens": ["Mit", "gu\u00b7tem", "Fug", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl s\u00e4nge sich ein langes Lied", "tokens": ["Wohl", "s\u00e4n\u00b7ge", "sich", "ein", "lan\u00b7ges", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meines Bl\u00fcmchens Kraft:", "tokens": ["Von", "mei\u00b7nes", "Bl\u00fcm\u00b7chens", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie es am Leib' und am Gem\u00fct", "tokens": ["Wie", "es", "am", "Leib'", "und", "am", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hohe Wunder schafft.", "tokens": ["So", "ho\u00b7he", "Wun\u00b7der", "schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was kein geheimes Elixir", "tokens": ["Was", "kein", "ge\u00b7hei\u00b7mes", "E\u00b7li\u00b7xir"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir sonst gew\u00e4hren kann,", "tokens": ["Dir", "sonst", "ge\u00b7w\u00e4h\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das leistet traun! mein Bl\u00fcmchen dir.", "tokens": ["Das", "leis\u00b7tet", "traun", "!", "mein", "Bl\u00fcm\u00b7chen", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVINF", "$.", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man s\u00e4h' es ihm nicht an.", "tokens": ["Man", "s\u00e4h'", "es", "ihm", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer Wunderhold im Busen hegt,", "tokens": ["Wer", "Wun\u00b7der\u00b7hold", "im", "Bu\u00b7sen", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird wie ein Engel sch\u00f6n.", "tokens": ["Wird", "wie", "ein", "En\u00b7gel", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hab' ich, inniglich bewegt,", "tokens": ["Das", "hab'", "ich", ",", "in\u00b7nig\u00b7lich", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Mann und Weib gesehn.", "tokens": ["An", "Mann", "und", "Weib", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An Mann und Weib, alt oder jung,", "tokens": ["An", "Mann", "und", "Weib", ",", "alt", "o\u00b7der", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zieht's, wie ein Talisman,", "tokens": ["Zieht's", ",", "wie", "ein", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Der sch\u00f6nsten Seelen Huldigung", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "See\u00b7len", "Hul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Unwiderstehlich an.", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "an", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf steifem Hals ein Strotzerhaupt,", "tokens": ["Auf", "stei\u00b7fem", "Hals", "ein", "Strot\u00b7zer\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wangen hoch sich bl\u00e4h'n,", "tokens": ["Des", "Wan\u00b7gen", "hoch", "sich", "bl\u00e4h'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Nase nur nach \u00c4ther schnaubt,", "tokens": ["Des", "Na\u00b7se", "nur", "nach", "\u00c4\u00b7ther", "schnaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dft doch gewi\u00df nicht sch\u00f6n.", "tokens": ["L\u00e4\u00dft", "doch", "ge\u00b7wi\u00df", "nicht", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn irgend nun ein Rang, wenn Gold", "tokens": ["Wenn", "ir\u00b7gend", "nun", "ein", "Rang", ",", "wenn", "Gold"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu steif den Hals dir gab,", "tokens": ["Zu", "steif", "den", "Hals", "dir", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So schmeidigt ihn mein Wunderhold", "tokens": ["So", "schmei\u00b7digt", "ihn", "mein", "Wun\u00b7der\u00b7hold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und biegt dein Haupt herab.", "tokens": ["Und", "biegt", "dein", "Haupt", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es webet \u00fcber dein Gesicht", "tokens": ["Es", "we\u00b7bet", "\u00fc\u00b7ber", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Anmut Rosenflor;", "tokens": ["Der", "An\u00b7mut", "Ro\u00b7sen\u00b7flor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zieht des Auges grellem Licht", "tokens": ["Und", "zieht", "des", "Au\u00b7ges", "grel\u00b7lem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wimper mildernd vor.", "tokens": ["Die", "Wim\u00b7per", "mil\u00b7dernd", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es teilt der Fl\u00f6te weichen Klang", "tokens": ["Es", "teilt", "der", "Fl\u00f6\u00b7te", "wei\u00b7chen", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Schreiers Kehle mit,", "tokens": ["Des", "Schrei\u00b7ers", "Keh\u00b7le", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wandelt in Zephyrengang", "tokens": ["Und", "wan\u00b7delt", "in", "Ze\u00b7phy\u00b7ren\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des St\u00fcrmers Poltertritt.", "tokens": ["Des", "St\u00fcr\u00b7mers", "Pol\u00b7ter\u00b7tritt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Laute gleicht des Menschen Herz,", "tokens": ["Der", "Lau\u00b7te", "gleicht", "des", "Men\u00b7schen", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Sang und Klang gebaut,", "tokens": ["Zu", "Sang", "und", "Klang", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch spielen sie oft Lust und Schmerz", "tokens": ["Doch", "spie\u00b7len", "sie", "oft", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu st\u00fcrmisch und zu laut:", "tokens": ["Zu", "st\u00fcr\u00b7misch", "und", "zu", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Schmerz, wann Ehre, Macht und Gold", "tokens": ["Der", "Schmerz", ",", "wann", "Eh\u00b7re", ",", "Macht", "und", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor deinen W\u00fcnschen fliehn,", "tokens": ["Vor", "dei\u00b7nen", "W\u00fcn\u00b7schen", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Luft, wann sie in deinen Sold", "tokens": ["Und", "Luft", ",", "wann", "sie", "in", "dei\u00b7nen", "Sold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Siegeskr\u00e4nzen ziehn.", "tokens": ["Mit", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7zen", "ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O wie dann Wunderhold das Herz", "tokens": ["O", "wie", "dann", "Wun\u00b7der\u00b7hold", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mild und lieblich stimmt!", "tokens": ["So", "mild", "und", "lieb\u00b7lich", "stimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie allgef\u00e4llig Ernst und Scherz", "tokens": ["Wie", "all\u00b7ge\u00b7f\u00e4l\u00b7lig", "Ernst", "und", "Scherz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem Zauber schwimmt!", "tokens": ["In", "sei\u00b7nem", "Zau\u00b7ber", "schwimmt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie man alsdann nichts thut und spricht,", "tokens": ["Wie", "man", "als\u00b7dann", "nichts", "thut", "und", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drob Jemand z\u00fcrnen kann!", "tokens": ["Drob", "Je\u00b7mand", "z\u00fcr\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das macht, man trotzt und strotzet nicht", "tokens": ["Das", "macht", ",", "man", "trotzt", "und", "strot\u00b7zet", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PIS", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dr\u00e4ngt sich nicht voran.", "tokens": ["Und", "dr\u00e4ngt", "sich", "nicht", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O wie man dann so wohlgemut,", "tokens": ["O", "wie", "man", "dann", "so", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PIS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So friedlich lebt und webt!", "tokens": ["So", "fried\u00b7lich", "lebt", "und", "webt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie um das Lager, wo man ruht,", "tokens": ["Wie", "um", "das", "La\u00b7ger", ",", "wo", "man", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schlaf so segnend schwebt!", "tokens": ["Der", "Schlaf", "so", "seg\u00b7nend", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-