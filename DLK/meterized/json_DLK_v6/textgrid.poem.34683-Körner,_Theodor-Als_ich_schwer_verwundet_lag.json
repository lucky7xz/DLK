{"textgrid.poem.34683": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Als ich schwer verwundet lag", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, la\u00df mich nicht erliegen", "tokens": ["Gott", ",", "la\u00df", "mich", "nicht", "er\u00b7lie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meiner Wunde Brand!", "tokens": ["In", "mei\u00b7ner", "Wun\u00b7de", "Brand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df nicht die Marter siegen \u2013", "tokens": ["La\u00df", "nicht", "die", "Mar\u00b7ter", "sie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "'s war ja f\u00fcrs Vaterland! \u2013", "tokens": ["'s", "war", "ja", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Verla\u00df mich nicht, du Milde,", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ",", "du", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ich mich sonst bewu\u00dft,", "tokens": ["Der", "ich", "mich", "sonst", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Decke mit deinem Schilde", "tokens": ["De\u00b7cke", "mit", "dei\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die qualzerriss'ne Brust!", "tokens": ["Die", "qua\u00b7lzer\u00b7riss'\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Kopf will mir zerspalten,", "tokens": ["Der", "Kopf", "will", "mir", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wild gl\u00fcht des Auges Kreis,", "tokens": ["Wild", "gl\u00fcht", "des", "Au\u00b7ges", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch meine Glieder kalten", "tokens": ["Doch", "mei\u00b7ne", "Glie\u00b7der", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie in des Nordens Eis.", "tokens": ["Wie", "in", "des", "Nor\u00b7dens", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von wut'ger Qual zertreten", "tokens": ["Von", "wut'\u00b7ger", "Qual", "zer\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Geist im Staube schleicht.", "tokens": ["Der", "Geist", "im", "Stau\u00b7be", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich nur ", "tokens": ["La\u00df", "mich", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Mein Gott, dann wird mir leicht!", "tokens": ["Mein", "Gott", ",", "dann", "wird", "mir", "leicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein' Gnad' ist unverderblich! \u2013", "tokens": ["Dein'", "Gnad'", "ist", "un\u00b7ver\u00b7derb\u00b7lich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mut, wenn das Herz auch rei\u00dft!", "tokens": ["Mut", ",", "wenn", "das", "Herz", "auch", "rei\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Leib, der Schmerz ist sterblich,", "tokens": ["Der", "Leib", ",", "der", "Schmerz", "ist", "sterb\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unsterblich ist der Geist.", "tokens": ["U\u00b7nsterb\u00b7lich", "ist", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}}}}