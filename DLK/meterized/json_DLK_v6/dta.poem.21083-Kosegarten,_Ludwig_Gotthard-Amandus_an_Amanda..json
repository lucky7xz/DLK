"line.4": {"text": "Die Erde mir, wie Nebeld\u00e4mmerungen,", "tokens": ["Die", "Er\u00b7de", "mir", ",", "wie", "Ne\u00b7bel\u00b7d\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Himmel mir, wie Thauged\u00fcft, verschwand?", "tokens": ["Der", "Him\u00b7mel", "mir", ",", "wie", "Thau\u00b7ge\u00b7d\u00fcft", ",", "ver\u00b7schwand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wenn ich aus deiner Lippen reiner F\u00fclle", "tokens": ["Wenn", "ich", "aus", "dei\u00b7ner", "Lip\u00b7pen", "rei\u00b7ner", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das s\u00fcsse Gift begierig in mich trank,", "tokens": ["Das", "s\u00fcs\u00b7se", "Gift", "be\u00b7gie\u00b7rig", "in", "mich", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und niedertaumelte in wollusttrunkne Stille,", "tokens": ["Und", "nie\u00b7der\u00b7tau\u00b7mel\u00b7te", "in", "wol\u00b7lust\u00b7trunk\u00b7ne", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und athemlos mit Seyn und Nichtseyn rang?", "tokens": ["Und", "at\u00b7hem\u00b7los", "mit", "Seyn", "und", "Nich