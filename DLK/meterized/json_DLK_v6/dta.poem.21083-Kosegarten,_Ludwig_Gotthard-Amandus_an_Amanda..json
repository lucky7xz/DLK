{"dta.poem.21083": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Amandus an Amanda.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht lieben soll ich dich? Amanden ich nicht               ", "tokens": ["Nicht", "lie\u00b7ben", "soll", "ich", "dich", "?", "A\u00b7man\u00b7den", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PRF", "$.", "NN", "PPER", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "lieben,", "tokens": ["lie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "An die der Ewigkeiten Schwur mich band? \u2014", "tokens": ["An", "die", "der", "E\u00b7wig\u00b7kei\u00b7ten", "Schwur", "mich", "band", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mag doch der Sch\u00f6pfung Kranz, wie welkes Laub,", "tokens": ["Mag", "doch", "der", "Sch\u00f6p\u00b7fung", "Kranz", ",", "wie", "wel\u00b7kes", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "$,", "PWAV", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "zerstieben,", "tokens": ["zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Das Daseyn schwindeln an des Nichtseyns Rand,", "tokens": ["Das", "Da\u00b7seyn", "schwin\u00b7deln", "an", "des", "Nich\u00b7tseyns", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Lichtes Urquell in die alte Nacht sich tauchen,", "tokens": ["Des", "Lich\u00b7tes", "Ur\u00b7quell", "in", "die", "al\u00b7te", "Nacht", "sich", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Weltall, einge\u00e4schert von dem Zorngericht", "tokens": ["Das", "Wel\u00b7tall", ",", "ein\u00b7ge\u00b7\u00e4\u00b7schert", "von", "dem", "Zorn\u00b7ge\u00b7richt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des j\u00fcngsten Tags, hinauf zum Thron der Gottheit", "tokens": ["Des", "j\u00fcng\u00b7sten", "Tags", ",", "hin\u00b7auf", "zum", "Thron", "der", "Got\u00b7theit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "rauchen \u2014", "tokens": ["rau\u00b7chen"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Amandus l\u00e4sst Amanden nicht!", "tokens": ["A\u00b7man\u00b7dus", "l\u00e4sst", "A\u00b7man\u00b7den", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Kann ich die Elemente auseinander r\u00fctteln,", "tokens": ["Kann", "ich", "die", "E\u00b7le\u00b7men\u00b7te", "aus\u00b7ein\u00b7an\u00b7der", "r\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die das Verh\u00e4ngniss l\u00f6thete?", "tokens": ["Die", "das", "Ver\u00b7h\u00e4ng\u00b7niss", "l\u00f6\u00b7the\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann ich den alten Riesen, Schicksal, sch\u00fctteln,", "tokens": ["Kann", "ich", "den", "al\u00b7ten", "Rie\u00b7sen", ",", "Schick\u00b7sal", ",", "sch\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Herkules den Anteus sch\u00fcttelte?", "tokens": ["Wie", "Her\u00b7ku\u00b7les", "den", "An\u00b7teus", "sch\u00fct\u00b7tel\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NE", "VVFIN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Kann ich die Marmortafel niederst\u00fcrzen,", "tokens": ["Kann", "ich", "die", "Mar\u00b7mor\u00b7ta\u00b7fel", "nie\u00b7ders\u00b7t\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die fr\u00fcher, als der Sch\u00f6pfung Eckstein stand?", "tokens": ["Die", "fr\u00fc\u00b7her", ",", "als", "der", "Sch\u00f6p\u00b7fung", "E\u00b7ckstein", "stand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann ich den Knoten aus einander sch\u00fcrzen,", "tokens": ["Kann", "ich", "den", "Kno\u00b7ten", "aus", "ein\u00b7an\u00b7der", "sch\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den siebenfach um dich und mich die Gottheit", "tokens": ["Den", "sie\u00b7ben\u00b7fach", "um", "dich", "und", "mich", "die", "Got\u00b7theit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "KON", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "wand?", "tokens": ["wand", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Und k\u00f6nnt' ich es \u2014 ich ", "tokens": ["Und", "k\u00f6nnt'", "ich", "es", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "$(", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "siegen", "tokens": ["sie\u00b7gen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die Flamme, die mir Kraft und Stolz und Adel", "tokens": ["Die", "Flam\u00b7me", ",", "die", "mir", "Kraft", "und", "Stolz", "und", "A\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "giebt?", "tokens": ["giebt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Du willst es? Heuchlerin! straft nicht dein Herz", "tokens": ["Du", "willst", "es", "?", "Heuch\u00b7le\u00b7rin", "!", "straft", "nicht", "dein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "$.", "NN", "$.", "VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "dich L\u00fcgen? \u2014", "tokens": ["dich", "L\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ich weiss, dass mich Amanda liebt!", "tokens": ["Ich", "weiss", ",", "dass", "mich", "A\u00b7man\u00b7da", "liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Las ich sie nicht, die Flammenschrift, geschrieben", "tokens": ["Las", "ich", "sie", "nicht", ",", "die", "Flam\u00b7men\u00b7schrift", ",", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "PPER", "PTKNEG", "$,", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von jener Hand, die Sonnen ballt und Seelen traut:", "tokens": ["Von", "je\u00b7ner", "Hand", ",", "die", "Son\u00b7nen", "ballt", "und", "See\u00b7len", "traut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eamandus wird Amanda lieben!", "tokens": ["\u201e", "a\u00b7man\u00b7dus", "wird", "A\u00b7man\u00b7da", "lie\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VAFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eamanda sey Amandus Braut!\u201c", "tokens": ["\u201e", "a\u00b7man\u00b7da", "sey", "A\u00b7man\u00b7dus", "Braut", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "NE", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Las ich ihn nicht den sch\u00f6nsten meiner Siege?", "tokens": ["Las", "ich", "ihn", "nicht", "den", "sch\u00f6ns\u00b7ten", "mei\u00b7ner", "Sie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Las ich es nicht, dass mich Amanda liebt,", "tokens": ["Las", "ich", "es", "nicht", ",", "dass", "mich", "A\u00b7man\u00b7da", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In jedem Buchstab deiner Engelz\u00fcge,", "tokens": ["In", "je\u00b7dem", "Buch\u00b7stab", "dei\u00b7ner", "En\u00b7gel\u00b7z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In jedem Blitz, der deinem Aug' entstiebt?", "tokens": ["In", "je\u00b7dem", "Blitz", ",", "der", "dei\u00b7nem", "Aug'", "ent\u00b7stiebt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Verstand ich nicht der Blicke Funkenspr\u00fchen,", "tokens": ["Ver\u00b7stand", "ich", "nicht", "der", "Bli\u00b7cke", "Fun\u00b7ken\u00b7spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der H\u00e4nde rednerischen Druck,", "tokens": ["Der", "H\u00e4n\u00b7de", "red\u00b7ne\u00b7ri\u00b7schen", "Druck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Odems Flammenwehn, der Wangen heissres", "tokens": ["Des", "O\u00b7dems", "Flam\u00b7men\u00b7wehn", ",", "der", "Wan\u00b7gen", "heiss\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gl\u00fchen,", "tokens": ["Gl\u00fc\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der Nerven fieberhaften Zuck?", "tokens": ["Der", "Ner\u00b7ven", "fie\u00b7ber\u00b7haf\u00b7ten", "Zuck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Verstand ich nicht diess unnennbare Toben", "tokens": ["Ver\u00b7stand", "ich", "nicht", "diess", "un\u00b7nenn\u00b7ba\u00b7re", "To\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Pulse, nic