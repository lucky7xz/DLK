{"textgrid.poem.44604": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[auf Radetzkys Tod]", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du tot! Du hattest Zeit zu sterben,", "tokens": ["So", "bist", "du", "tot", "!", "Du", "hat\u00b7test", "Zeit", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn warst du n\u00f6tig noch, du starbst noch nicht.", "tokens": ["Denn", "warst", "du", "n\u00f6\u00b7tig", "noch", ",", "du", "starbst", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie wei\u00df sich l\u00e4ngst dir Bart und Haare f\u00e4rben,", "tokens": ["Wie", "wei\u00df", "sich", "l\u00e4ngst", "dir", "Bart", "und", "Haa\u00b7re", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Ma\u00df des Lebens war f\u00fcr dich die Pflicht.", "tokens": ["Das", "Ma\u00df", "des", "Le\u00b7bens", "war", "f\u00fcr", "dich", "die", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch nun, da nicht dein Land du nur gerettet,", "tokens": ["Doch", "nun", ",", "da", "nicht", "dein", "Land", "du", "nur", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch dich ermannt, der Weltteil selbst in Ruh,", "tokens": ["Durch", "dich", "er\u00b7mannt", ",", "der", "Welt\u00b7teil", "selbst", "in", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du auf deinen Nachruhm dich gebettet", "tokens": ["Hast", "du", "auf", "dei\u00b7nen", "Nach\u00b7ruhm", "dich", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schlossest sp\u00e4t die m\u00fcden Augen zu.", "tokens": ["Und", "schlos\u00b7sest", "sp\u00e4t", "die", "m\u00fc\u00b7den", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}