{"textgrid.poem.39300": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch auf! Der Seiger r\u00fchrte sich", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch auf! Der Seiger r\u00fchrte sich", "tokens": ["Horch", "auf", "!", "Der", "Sei\u00b7ger", "r\u00fchr\u00b7te", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tut sich ", "tokens": ["Und", "tut", "sich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mir ist zumut ganz ", "tokens": ["Mir", "ist", "zu\u00b7mut", "ganz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Ich mu\u00df ein bi\u00dfchen singen!", "tokens": ["Ich", "mu\u00df", "ein", "bi\u00df\u00b7chen", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar, da\u00df ich v\u00e9ritablement", "tokens": ["Zwar", ",", "da\u00df", "ich", "v\u00e9ri\u00b7ta\u00b7ble\u00b7ment"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Themis ernster Sch\u00fcler,", "tokens": ["Der", "The\u00b7mis", "erns\u00b7ter", "Sch\u00fc\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hast Du l\u00e4ngst probablement", "tokens": ["Das", "hast", "Du", "l\u00e4ngst", "pro\u00b7bab\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh\u00f6rt von Loos und M\u00fchler.", "tokens": ["Ge\u00b7h\u00f6rt", "von", "Loos", "und", "M\u00fch\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch glaub' nicht alles, was ich schrieb", "tokens": ["Doch", "glaub'", "nicht", "al\u00b7les", ",", "was", "ich", "schrieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PIS", "$,", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin trotz Recht und Weisheitstrieb", "tokens": ["Ich", "bin", "trotz", "Recht", "und", "Weis\u00b7heit\u00b7strieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch ziemlich toll bis dato!", "tokens": ["Noch", "ziem\u00b7lich", "toll", "bis", "da\u00b7to", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Noch immer mu\u00df ich in dem Netz", "tokens": ["Noch", "im\u00b7mer", "mu\u00df", "ich", "in", "dem", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Es geht mit dem verr\u00fcckten G\u00f6tz", "tokens": ["Es", "geht", "mit", "dem", "ver\u00b7r\u00fcck\u00b7ten", "G\u00f6tz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer durch der Schecken.", "tokens": ["Noch", "im\u00b7mer", "durch", "der", "Sche\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Verse, die ich lege bei", "tokens": ["Die", "Ver\u00b7se", ",", "die", "ich", "le\u00b7ge", "bei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sie schmecken nach jeder Teufelei,", "tokens": ["Sie", "schme\u00b7cken", "nach", "je\u00b7der", "Teu\u00b7fe\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur nicht nach ", "tokens": ["Nur", "nicht", "nach"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKNEG", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Du aber magst nun sehr erbost", "tokens": ["Du", "a\u00b7ber", "magst", "nun", "sehr", "er\u00b7bost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kritische Nase r\u00fcmpfen,", "tokens": ["Die", "kri\u00b7ti\u00b7sche", "Na\u00b7se", "r\u00fcmp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es bleibt mir ja der s\u00fc\u00dfe Trost:", "tokens": ["Es", "bleibt", "mir", "ja", "der", "s\u00fc\u00b7\u00dfe", "Trost", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbich h\u00f6re Dich nicht schimpfen!\u00ab", "tokens": ["\u00bb", "ich", "h\u00f6\u00b7re", "Dich", "nicht", "schimp\u00b7fen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Lied ging aus, der Tag zu Tal,", "tokens": ["Das", "Lied", "ging", "aus", ",", "der", "Tag", "zu", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun gr\u00fc\u00df' ich Euer fromm Gemahl,", "tokens": ["Nun", "gr\u00fc\u00df'", "ich", "Eu\u00b7er", "fromm", "Ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bitte dann: Vor Elf und Wolf", "tokens": ["Und", "bit\u00b7te", "dann", ":", "Vor", "Elf", "und", "Wolf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$.", "APPR", "NE", "KON", "NE"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Beh\u00fcte Gott den kleinen Rolf!", "tokens": ["Be\u00b7h\u00fc\u00b7te", "Gott", "den", "klei\u00b7nen", "Rolf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}