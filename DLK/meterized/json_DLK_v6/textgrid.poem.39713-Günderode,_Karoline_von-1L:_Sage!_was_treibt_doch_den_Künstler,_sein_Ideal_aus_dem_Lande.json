{"textgrid.poem.39713": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sage! was treibt doch den K\u00fcnstler, sein Ideal aus dem Lande", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage! was treibt doch den K\u00fcnstler, sein Ideal aus dem Lande", "tokens": ["Sa\u00b7ge", "!", "was", "treibt", "doch", "den", "K\u00fcnst\u00b7ler", ",", "sein", "I\u00b7deal", "aus", "dem", "Lan\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Der Ideen zu ziehn, und es dem Stoff zu vertraun?", "tokens": ["Der", "I\u00b7deen", "zu", "ziehn", ",", "und", "es", "dem", "Stoff", "zu", "ver\u00b7traun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "KON", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6ner wird ihm sein Bilden gelingen im Reich der Gedanken,", "tokens": ["Sch\u00f6\u00b7ner", "wird", "ihm", "sein", "Bil\u00b7den", "ge\u00b7lin\u00b7gen", "im", "Reich", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "W\u00e4re es fl\u00fcchtiger zwar, dennoch auch freier daf\u00fcr,", "tokens": ["W\u00e4\u00b7re", "es", "fl\u00fcch\u00b7ti\u00b7ger", "zwar", ",", "den\u00b7noch", "auch", "frei\u00b7er", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "ADV", "$,", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und sein Eigenthum mehr, und nicht dem Stoff unterth\u00e4nig.", "tokens": ["Und", "sein", "Ei\u00b7gen\u00b7thum", "mehr", ",", "und", "nicht", "dem", "Stoff", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "KON", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Frager! der du so fragst, du verstehst nicht des Geistes Beginnen,", "tokens": ["Fra\u00b7ger", "!", "der", "du", "so", "fragst", ",", "du", "ver\u00b7stehst", "nicht", "des", "Geis\u00b7tes", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Siehst nicht was er erstrebt, nicht was der K\u00fcnstler ersehnt.", "tokens": ["Siehst", "nicht", "was", "er", "er\u00b7strebt", ",", "nicht", "was", "der", "K\u00fcnst\u00b7ler", "er\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "PWS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Alle! sie wollen unsterbliches thun, die sterblichen Menschen.", "tokens": ["Al\u00b7le", "!", "sie", "wol\u00b7len", "uns\u00b7terb\u00b7li\u00b7ches", "thun", ",", "die", "sterb\u00b7li\u00b7chen", "Men\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "$.", "PPER", "VMFIN", "PIS", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Leben im Himmel die Frommen, in guten Thaten die Guten,", "tokens": ["Le\u00b7ben", "im", "Him\u00b7mel", "die", "From\u00b7men", ",", "in", "gu\u00b7ten", "Tha\u00b7ten", "die", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Darum in dauernder Form stellt den Gedanken er dar.", "tokens": ["Da\u00b7rum", "in", "dau\u00b7ern\u00b7der", "Form", "stellt", "den", "Ge\u00b7dan\u00b7ken", "er", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}