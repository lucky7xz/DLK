{"textgrid.poem.38809": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gr\u00fcner Wald, des Stromes klare Welle,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gr\u00fcner Wald, des Stromes klare Welle,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Wald", ",", "des", "Stro\u00b7mes", "kla\u00b7re", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Berges Hang, der reitzend sich erhebet,", "tokens": ["Des", "Ber\u00b7ges", "Hang", ",", "der", "reit\u00b7zend", "sich", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Blume, die am schlanken Stengel schwebet,", "tokens": ["Die", "Blu\u00b7me", ",", "die", "am", "schlan\u00b7ken", "Sten\u00b7gel", "schwe\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sonnenschein, des blauen Himmels Helle;", "tokens": ["Der", "Son\u00b7nen\u00b7schein", ",", "des", "blau\u00b7en", "Him\u00b7mels", "Hel\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie r\u00fchren in der Brust die goldne Quelle,", "tokens": ["Sie", "r\u00fch\u00b7ren", "in", "der", "Brust", "die", "gold\u00b7ne", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sehnsuchtsvoll nach hoher Sch\u00f6nheit strebet,", "tokens": ["Die", "sehn\u00b7suchts\u00b7voll", "nach", "ho\u00b7her", "Sch\u00f6n\u00b7heit", "stre\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich im M\u00e4dchen Sch\u00f6nheit selbst belebet,", "tokens": ["Wenn", "sich", "im", "M\u00e4d\u00b7chen", "Sch\u00f6n\u00b7heit", "selbst", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verstehn wir Wald, Berg, Strom und Blumenhelle.", "tokens": ["Ver\u00b7stehn", "wir", "Wald", ",", "Berg", ",", "Strom", "und", "Blu\u00b7me\u00b7nhel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wenn ich nun die Waldung wiederfinde,", "tokens": ["So", "wenn", "ich", "nun", "die", "Wal\u00b7dung", "wie\u00b7der\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den reinen Quell, wie ich den Berg ersteige,", "tokens": ["Den", "rei\u00b7nen", "Quell", ",", "wie", "ich", "den", "Berg", "er\u00b7stei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6mmt in die Seele mir dein Bild gelinde.", "tokens": ["K\u00f6mmt", "in", "die", "See\u00b7le", "mir", "dein", "Bild", "ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Freundschaft und Lieb' h\u00e4lt die Natur verbunden,", "tokens": ["Freund\u00b7schaft", "und", "Lieb'", "h\u00e4lt", "die", "Na\u00b7tur", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Will sich dir Wald und Quell im Bilde zeigen,", "tokens": ["Will", "sich", "dir", "Wald", "und", "Quell", "im", "Bil\u00b7de", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPER", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedenke unser in den guten Stunden.", "tokens": ["Ge\u00b7den\u00b7ke", "un\u00b7ser", "in", "den", "gu\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}