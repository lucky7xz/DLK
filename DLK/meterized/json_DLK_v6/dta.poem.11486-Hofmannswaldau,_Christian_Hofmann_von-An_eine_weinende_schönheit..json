{"dta.poem.11486": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An eine weinende sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die perlen schliesset man in g\u00f6ldne kasten ein,", "tokens": ["Die", "per\u00b7len", "schlies\u00b7set", "man", "in", "g\u00f6ld\u00b7ne", "kas\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn ihre kostbarkeit ist mehr als gold zu sch\u00e4tzen:", "tokens": ["Denn", "ih\u00b7re", "kost\u00b7bar\u00b7keit", "ist", "mehr", "als", "gold", "zu", "sch\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die perlen sollen sonst gefrorne thr\u00e4nen seyn,", "tokens": ["Die", "per\u00b7len", "sol\u00b7len", "sonst", "ge\u00b7fror\u00b7ne", "thr\u00e4\u00b7nen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "ADV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Womit aurora pflegt die muscheln zu benetzen.", "tokens": ["Wo\u00b7mit", "au\u00b7ro\u00b7ra", "pflegt", "die", "mu\u00b7scheln", "zu", "be\u00b7net\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum hemm\u2019, o Sch\u00f6nste! doch der edlen z\u00e4hren lauff!", "tokens": ["Drum", "hemm'", ",", "o", "Sch\u00f6ns\u00b7te", "!", "doch", "der", "ed\u00b7len", "z\u00e4h\u00b7ren", "lauff", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "NN", "$.", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df diesen sch\u00f6nen thau nicht so schlechthin verschiessen!", "tokens": ["La\u00df", "die\u00b7sen", "sch\u00f6\u00b7nen", "thau", "nicht", "so", "schlecht\u00b7hin", "ver\u00b7schies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Heb\u2019 ihn als perlen-safft in deinen augen auf!", "tokens": ["Heb'", "ihn", "als", "per\u00b7len\u00b7\u00b7safft", "in", "dei\u00b7nen", "au\u00b7gen", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo nicht, so la\u00df ihn nur in gold und jaspis fliessen.", "tokens": ["Wo", "nicht", ",", "so", "la\u00df", "ihn", "nur", "in", "gold", "und", "jas\u00b7pis", "flies\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}