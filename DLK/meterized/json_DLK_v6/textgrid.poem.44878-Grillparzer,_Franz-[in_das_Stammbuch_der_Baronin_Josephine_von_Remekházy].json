"iambic.tri"}, "line.7": {"text": "Die h\u00f6chsten Schicksalsgaben,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Schick\u00b7sals\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Wirklichen Gewinn;", "tokens": ["Des", "Wirk\u00b7li\u00b7chen", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und dennoch Dichterin?", "tokens": ["Und", "den\u00b7noch", "Dich\u00b7te\u00b7rin", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}