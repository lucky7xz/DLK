{"textgrid.poem.49955": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Petrefaktensammler", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal noch an eurer Seite,", "tokens": ["Ein\u00b7mal", "noch", "an", "eu\u00b7rer", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Meinen Hammer im Geleite,", "tokens": ["Mei\u00b7nen", "Ham\u00b7mer", "im", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Frickenhauser Pfade,", "tokens": ["Je\u00b7ne", "Fri\u00b7cken\u00b7hau\u00b7ser", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Links und rechts und krumm und grade,", "tokens": ["Links", "und", "rechts", "und", "krumm", "und", "gra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "KON", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An dem B\u00e4chlein hin zu scherzen,", "tokens": ["An", "dem", "B\u00e4ch\u00b7lein", "hin", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dies verlangte mich von Herzen.", "tokens": ["Dies", "ver\u00b7lang\u00b7te", "mich", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber dann mit tausend Freuden", "tokens": ["A\u00b7ber", "dann", "mit", "tau\u00b7send", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gleich den H\u00fcgel auf zu weiden,", "tokens": ["Gleich", "den", "H\u00fc\u00b7gel", "auf", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Drin die goldnen Ammoniten,", "tokens": ["Drin", "die", "gold\u00b7nen", "Am\u00b7mo\u00b7ni\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lias-Terebratuliten,", "tokens": ["Li\u00b7as\u00b7Te\u00b7re\u00b7bra\u00b7tu\u00b7li\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Pentakrinen auch, die zarten,", "tokens": ["Pen\u00b7ta\u00b7kri\u00b7nen", "auch", ",", "die", "zar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Alle sich zusammenscharten, \u2013", "tokens": ["Al\u00b7le", "sich", "zu\u00b7sam\u00b7men\u00b7schar\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PRF", "VVFIN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Den uns gar nicht ungelegen", "tokens": ["Den", "uns", "gar", "nicht", "un\u00b7ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PTKNEG", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Just ein warmer Sommerregen", "tokens": ["Just", "ein", "war\u00b7mer", "Som\u00b7mer\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ausgefurcht und abgewaschen,", "tokens": ["Aus\u00b7ge\u00b7furcht", "und", "ab\u00b7ge\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Denn so f\u00fcllt man sich die Taschen.", "tokens": ["Denn", "so", "f\u00fcllt", "man", "sich", "die", "Ta\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Auf dem Boden Hand und Knie,", "tokens": ["Auf", "dem", "Bo\u00b7den", "Hand", "und", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Kriecht man fort, o s\u00fc\u00dfe M\u00fch!", "tokens": ["Kriecht", "man", "fort", ",", "o", "s\u00fc\u00b7\u00dfe", "M\u00fch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKVZ", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und dazwischen mit Entz\u00fccken", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nach der Alb hinaufzublicken,", "tokens": ["Nach", "der", "Alb", "hin\u00b7auf\u00b7zu\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Deren burggekr\u00f6nte W\u00e4nde", "tokens": ["De\u00b7ren", "burg\u00b7ge\u00b7kr\u00f6n\u00b7te", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Unser sonnig Talgel\u00e4nde,", "tokens": ["Un\u00b7ser", "son\u00b7nig", "Tal\u00b7ge\u00b7l\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Rebengr\u00fcn und Wald und Wiesen", "tokens": ["Re\u00b7ben\u00b7gr\u00fcn", "und", "Wald", "und", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Streng mit dunkeln Schatten schlie\u00dfen!", "tokens": ["Streng", "mit", "dun\u00b7keln", "Schat\u00b7ten", "schlie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Welche liebliche Magie,", "tokens": ["Wel\u00b7che", "lieb\u00b7li\u00b7che", "Ma\u00b7gie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Uns im R\u00fccken, \u00fcbten sie!", "tokens": ["Uns", "im", "R\u00fc\u00b7cken", ",", "\u00fcb\u00b7ten", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Eben noch in Sonne glimmend", "tokens": ["E\u00b7ben", "noch", "in", "Son\u00b7ne", "glim\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und in leichtem Dufte schwimmend,", "tokens": ["Und", "in", "leich\u00b7tem", "Duf\u00b7te", "schwim\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Sieht man schwarz empor sie steigen,", "tokens": ["Sieht", "man", "schwarz", "em\u00b7por", "sie", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Wie die blaue Nacht am Tag!", "tokens": ["Wie", "die", "blau\u00b7e", "Nacht", "am", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Blau, wie nur ein Traum es zeigen,", "tokens": ["Blau", ",", "wie", "nur", "ein", "Traum", "es", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Doch kein Maler tuschen mag.", "tokens": ["Doch", "kein", "Ma\u00b7ler", "tu\u00b7schen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Seht, sie scheinen nah zu r\u00fccken,", "tokens": ["Seht", ",", "sie", "schei\u00b7nen", "nah", "zu", "r\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Immer n\u00e4her, immer dichter,", "tokens": ["Im\u00b7mer", "n\u00e4\u00b7her", ",", "im\u00b7mer", "dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und die gelben Regenlichter", "tokens": ["Und", "die", "gel\u00b7ben", "Re\u00b7gen\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "All in unser Tal zu dr\u00fccken!", "tokens": ["All", "in", "un\u00b7ser", "Tal", "zu", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Wahrlich, Sch\u00f6nres sah ich nie.", "tokens": ["Wahr\u00b7lich", ",", "Sch\u00f6n\u00b7res", "sah", "ich", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn man nur an solcher St\u00e4tte", "tokens": ["Wenn", "man", "nur", "an", "sol\u00b7cher", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeit genug zum Schauen h\u00e4tte!", "tokens": ["Zeit", "ge\u00b7nug", "zum", "Schau\u00b7en", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wi\u00dft ihr was? genie\u00dft ihr beiden", "tokens": ["Wi\u00dft", "ihr", "was", "?", "ge\u00b7nie\u00dft", "ihr", "bei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "$.", "VVFIN", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcndlich diese Herrlichkeiten,", "tokens": ["Gr\u00fcnd\u00b7lich", "die\u00b7se", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch f\u00fcr mich genie\u00dfet sie!", "tokens": ["Auch", "f\u00fcr", "mich", "ge\u00b7nie\u00b7\u00dfet", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn mich fickt' es allerdinge,", "tokens": ["Denn", "mich", "fickt'", "es", "al\u00b7ler\u00b7din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn das rein verlorenginge.", "tokens": ["Wenn", "das", "rein", "ver\u00b7lo\u00b7ren\u00b7gin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch, den Zweck nicht zu verlieren,", "tokens": ["Doch", ",", "den", "Zweck", "nicht", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Will ich jetzt auf allen vieren", "tokens": ["Will", "ich", "jetzt", "auf", "al\u00b7len", "vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nach besagten Terebrateln", "tokens": ["Nach", "be\u00b7sag\u00b7ten", "Ter\u00b7e\u00b7bra\u00b7teln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Noch ein St\u00fcckchen weiterkratteln;", "tokens": ["Noch", "ein", "St\u00fcck\u00b7chen", "wei\u00b7ter\u00b7krat\u00b7teln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das ist auch wohl Poesie.", "tokens": ["Das", "ist", "auch", "wohl", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}