{"textgrid.poem.59053": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "272. Der Kinder Werk", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abgeschieden, willenlos,", "tokens": ["Ab\u00b7ge\u00b7schie\u00b7den", ",", "wil\u00b7len\u00b7los", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverstellt, von Bildern blo\u00df", "tokens": ["Un\u00b7ver\u00b7stellt", ",", "von", "Bil\u00b7dern", "blo\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut man unsern Gott, den nahen,", "tokens": ["Schaut", "man", "un\u00b7sern", "Gott", ",", "den", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kann ihn im Grund umfahen.", "tokens": ["Und", "kann", "ihn", "im", "Grund", "um\u00b7fa\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}