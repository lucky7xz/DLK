{"textgrid.poem.42459": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4dchen mit den graden Schultern,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen mit den graden Schultern,", "tokens": ["M\u00e4d\u00b7chen", "mit", "den", "gra\u00b7den", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem engen braunen Mantel,", "tokens": ["In", "dem", "en\u00b7gen", "brau\u00b7nen", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den zieren Ellenbogen,", "tokens": ["Mit", "den", "zie\u00b7ren", "El\u00b7len\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich nah den H\u00fcften halten,", "tokens": ["Die", "sich", "nah", "den", "H\u00fcf\u00b7ten", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem marktgef\u00fcllten K\u00f6rbchen,", "tokens": ["Mit", "dem", "markt\u00b7ge\u00b7f\u00fcll\u00b7ten", "K\u00f6rb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eile nicht so rasch vor\u00fcber,", "tokens": ["Ei\u00b7le", "nicht", "so", "rasch", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bitte, dreh' dich einmal um.", "tokens": ["Bit\u00b7te", ",", "dreh'", "dich", "ein\u00b7mal", "um", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sage mir, der lange L\u00fcmmel,", "tokens": ["Sa\u00b7ge", "mir", ",", "der", "lan\u00b7ge", "L\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dir gestern ging zur Seite,", "tokens": ["Der", "dir", "ge\u00b7stern", "ging", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der so emsig mit dir schwatzte,", "tokens": ["Der", "so", "em\u00b7sig", "mit", "dir", "schwatz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Siegesfett auf dich hinabsah,", "tokens": ["Sie\u00b7ges\u00b7fett", "auf", "dich", "hin\u00b7ab\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unversch\u00e4mt mich \u00fcberglotzte,", "tokens": ["Un\u00b7ver\u00b7sch\u00e4mt", "mich", "\u00fc\u00b7berg\u00b7lotz\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sage, dieser lange L\u00fcmmel,", "tokens": ["Sa\u00b7ge", ",", "die\u00b7ser", "lan\u00b7ge", "L\u00fcm\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6cht' ich wissen, ist dein Schatz?", "tokens": ["M\u00f6cht'", "ich", "wis\u00b7sen", ",", "ist", "dein", "Schatz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erst vor kurzer Zeit entdeckt' ich", "tokens": ["Erst", "vor", "kur\u00b7zer", "Zeit", "ent\u00b7deckt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine wundersch\u00f6nen Augen,", "tokens": ["Dei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich will's mir \u00fcberlegen,", "tokens": ["Und", "ich", "will's", "mir", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie beginn' ich meinen Angriff,", "tokens": ["Wie", "be\u00b7ginn'", "ich", "mei\u00b7nen", "An\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Langsam, mit der Sonde Vorsicht,", "tokens": ["Lang\u00b7sam", ",", "mit", "der", "Son\u00b7de", "Vor\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder wie der Luchs im Sprunge;", "tokens": ["O\u00b7der", "wie", "der", "Luchs", "im", "Sprun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Br\u00e4nde flogen mir in's Herz.", "tokens": ["Br\u00e4n\u00b7de", "flo\u00b7gen", "mir", "in's", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch gleichviel \u2013 auf meine Haide,", "tokens": ["Doch", "gleich\u00b7viel", "\u2013", "auf", "mei\u00b7ne", "Hai\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der R\u00fcttelfalk am Himmel", "tokens": ["Wo", "der", "R\u00fct\u00b7tel\u00b7falk", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Todesschwinge sch\u00fcttelt,", "tokens": ["Sei\u00b7ne", "To\u00b7des\u00b7schwin\u00b7ge", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um im Sturz die Maus zu schlagen;", "tokens": ["Um", "im", "Sturz", "die", "Maus", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo auf meilenweiten G\u00e4ngen", "tokens": ["Wo", "auf", "mei\u00b7len\u00b7wei\u00b7ten", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keiner Seele ich begegne,", "tokens": ["Kei\u00b7ner", "See\u00b7le", "ich", "be\u00b7geg\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dorthin einst bestell' ich dich.", "tokens": ["Dor\u00b7thin", "einst", "be\u00b7stell'", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort auch kenn' ich ein Geheimnis:", "tokens": ["Dort", "auch", "kenn'", "ich", "ein", "Ge\u00b7heim\u00b7nis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Birkenw\u00e4ldchens Unschuld.", "tokens": ["Ei\u00b7nes", "Bir\u00b7ken\u00b7w\u00e4ld\u00b7chens", "Un\u00b7schuld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in diesem W\u00e4ldchen, wei\u00df ich,", "tokens": ["Und", "in", "die\u00b7sem", "W\u00e4ld\u00b7chen", ",", "wei\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden wir uns heimlich treffen.", "tokens": ["Wer\u00b7den", "wir", "uns", "heim\u00b7lich", "tref\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nur der alte Landbrieftr\u00e4ger", "tokens": ["Nur", "der", "al\u00b7te", "Land\u00b7brief\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmt hier abends seinen Richtweg,", "tokens": ["Nimmt", "hier", "a\u00b7bends", "sei\u00b7nen", "Richt\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und der sieht und h\u00f6rt uns nicht.", "tokens": ["Und", "der", "sieht", "und", "h\u00f6rt", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}