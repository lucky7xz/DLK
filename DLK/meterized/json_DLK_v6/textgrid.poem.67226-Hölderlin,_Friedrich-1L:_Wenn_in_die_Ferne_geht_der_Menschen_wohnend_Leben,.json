{"textgrid.poem.67226": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn in die Ferne geht der Menschen wohnend Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn in die Ferne geht der Menschen wohnend Leben,", "tokens": ["Wenn", "in", "die", "Fer\u00b7ne", "geht", "der", "Men\u00b7schen", "woh\u00b7nend", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo in die Ferne sich ergl\u00e4nzt die Zeit der Reben,", "tokens": ["Wo", "in", "die", "Fer\u00b7ne", "sich", "er\u00b7gl\u00e4nzt", "die", "Zeit", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist auch dabei des Sommers leer Gefilde,", "tokens": ["Ist", "auch", "da\u00b7bei", "des", "Som\u00b7mers", "leer", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Wald erscheint mit seinem dunklen Bilde.", "tokens": ["Der", "Wald", "er\u00b7scheint", "mit", "sei\u00b7nem", "dunk\u00b7len", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df die Natur erg\u00e4nzt das Bild der Zeiten,", "tokens": ["Da\u00df", "die", "Na\u00b7tur", "er\u00b7g\u00e4nzt", "das", "Bild", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df die verweilt, sie schnell vor\u00fcbergleiten,", "tokens": ["Da\u00df", "die", "ver\u00b7weilt", ",", "sie", "schnell", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "$,", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist aus Vollkommenheit, des Himmels H\u00f6he gl\u00e4nzet", "tokens": ["Ist", "aus", "Voll\u00b7kom\u00b7men\u00b7heit", ",", "des", "Him\u00b7mels", "H\u00f6\u00b7he", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "$,", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Menschen dann, wie B\u00e4ume Bl\u00fct umkr\u00e4nzet.", "tokens": ["Den", "Men\u00b7schen", "dann", ",", "wie", "B\u00e4u\u00b7me", "Bl\u00fct", "um\u00b7kr\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}