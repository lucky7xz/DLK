{"dta.poem.3790": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "VoGELSCHAU", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Schwalben schnee- und silberweiss", "tokens": ["Schwal\u00b7ben", "schnee", "und", "sil\u00b7ber\u00b7weiss"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "TRUNC", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah sie sich im winde wiegen", "tokens": ["Sah", "sie", "sich", "im", "win\u00b7de", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem winde hell und heiss", "tokens": ["In", "dem", "win\u00b7de", "hell", "und", "heiss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJD", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bunte h\u00e4her sah ich h\u00fcpfen", "tokens": ["Bun\u00b7te", "h\u00e4\u00b7her", "sah", "ich", "h\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Papagei und kolibri", "tokens": ["Pa\u00b7pa\u00b7gei", "und", "ko\u00b7lib\u00b7ri"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Durch die wunder-b\u00e4ume schl\u00fcpfen", "tokens": ["Durch", "die", "wun\u00b7der\u00b7b\u00e4u\u00b7me", "schl\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem wald der Tusferi", "tokens": ["In", "dem", "wald", "der", "Tus\u00b7fe\u00b7ri"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Grosse raben sah ich flattern", "tokens": ["Gros\u00b7se", "ra\u00b7ben", "sah", "ich", "flat\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dohlen schwarz und dunkelgrau", "tokens": ["Doh\u00b7len", "schwarz", "und", "dun\u00b7kel\u00b7grau"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nah am grunde \u00fcber nattern", "tokens": ["Nah", "am", "grun\u00b7de", "\u00fc\u00b7ber", "nat\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im verzauberten gehau", "tokens": ["Im", "ver\u00b7zau\u00b7ber\u00b7ten", "ge\u00b7hau"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwalben seh ich wieder fliegen", "tokens": ["Schwal\u00b7ben", "seh", "ich", "wie\u00b7der", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnee- und silberweisse schar", "tokens": ["Schnee", "und", "sil\u00b7ber\u00b7weis\u00b7se", "schar"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "FM", "FM"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie sich im winde wiegen", "tokens": ["Wie", "sie", "sich", "im", "win\u00b7de", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem winde kalt und klar", "tokens": ["In", "dem", "win\u00b7de", "kalt", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}