{"textgrid.poem.51040": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das f\u00fcr ein Schrein und Peitschenknallen?", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Schrein", "und", "Peit\u00b7schen\u00b7knal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fenster zittern von der Hufe Klang,", "tokens": ["Die", "Fens\u00b7ter", "zit\u00b7tern", "von", "der", "Hu\u00b7fe", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zw\u00f6lf Rosse keuchen an dem straffen Strang,", "tokens": ["Zw\u00f6lf", "Ros\u00b7se", "keu\u00b7chen", "an", "dem", "straf\u00b7fen", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Fuhrmannsfl\u00fcche durch die Gasse schallen.", "tokens": ["Und", "Fuhr\u00b7manns\u00b7fl\u00fc\u00b7che", "durch", "die", "Gas\u00b7se", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der auf den freien Bergen ist gefallen,", "tokens": ["Der", "auf", "den", "frei\u00b7en", "Ber\u00b7gen", "ist", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem toten Waldesk\u00f6nig gilt der Drang;", "tokens": ["Dem", "to\u00b7ten", "Wal\u00b7des\u00b7k\u00f6\u00b7nig", "gilt", "der", "Drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da schleifen sie, wohl drei\u00dfig Ellen lang,", "tokens": ["Da", "schlei\u00b7fen", "sie", ",", "wohl", "drei\u00b7\u00dfig", "El\u00b7len", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Rieseneiche durch die dumpfen Hallen.", "tokens": ["Die", "Rie\u00b7sen\u00b7ei\u00b7che", "durch", "die", "dum\u00b7pfen", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Zug h\u00e4lt unter meinem Fenster an,", "tokens": ["Der", "Zug", "h\u00e4lt", "un\u00b7ter", "mei\u00b7nem", "Fens\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn es gebricht zum Wenden ihm an Raum;", "tokens": ["Denn", "es", "ge\u00b7bricht", "zum", "Wen\u00b7den", "ihm", "an", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwundert dr\u00e4ngt sich alles Volk heran.", "tokens": ["Ver\u00b7wun\u00b7dert", "dr\u00e4ngt", "sich", "al\u00b7les", "Volk", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PRF", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie weiden sich an der gebrochnen Kraft;", "tokens": ["Sie", "wei\u00b7den", "sich", "an", "der", "ge\u00b7broch\u00b7nen", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da liegt entkr\u00f6nt der tausendj\u00e4hr'ge Baum,", "tokens": ["Da", "liegt", "ent\u00b7kr\u00f6nt", "der", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus allen Wunden quillt der edle Saft.", "tokens": ["Aus", "al\u00b7len", "Wun\u00b7den", "quillt", "der", "ed\u00b7le", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}