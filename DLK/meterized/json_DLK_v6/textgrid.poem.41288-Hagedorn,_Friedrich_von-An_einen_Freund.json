{"textgrid.poem.41288": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ist nicht klug, der vieles wagt,", "tokens": ["Der", "ist", "nicht", "klug", ",", "der", "vie\u00b7les", "wagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geringen Vortheil zu erwischen.", "tokens": ["Ge\u00b7rin\u00b7gen", "Vor\u00b7theil", "zu", "er\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies hei\u00dfet, wie August gesagt,", "tokens": ["Dies", "hei\u00b7\u00dfet", ",", "wie", "Au\u00b7gust", "ge\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit einer g\u00fcldnen Angel fischen.", "tokens": ["Mit", "ei\u00b7ner", "g\u00fcld\u00b7nen", "An\u00b7gel", "fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}