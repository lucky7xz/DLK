{"textgrid.poem.44063": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der Ruh vergn\u00fcgter Sinnen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Ruh vergn\u00fcgter Sinnen", "tokens": ["In", "der", "Ruh", "ver\u00b7gn\u00fcg\u00b7ter", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steckt das h\u00f6chste Gut der Welt,", "tokens": ["Steckt", "das", "h\u00f6chs\u00b7te", "Gut", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dies Kleinod zu gewinnen,", "tokens": ["Und", "dies", "Klei\u00b7nod", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Braucht man weder Staat noch Geld,", "tokens": ["Braucht", "man", "we\u00b7der", "Staat", "noch", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ein jeder st\u00fcndlich sieht,", "tokens": ["Weil", "ein", "je\u00b7der", "st\u00fcnd\u00b7lich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df, wer heute trozt und bl\u00fcht,", "tokens": ["Da\u00df", ",", "wer", "heu\u00b7te", "trozt", "und", "bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Morgen oft am Ruder zieht.", "tokens": ["Mor\u00b7gen", "oft", "am", "Ru\u00b7der", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eu\u00dferliche Pracht und G\u00fcter", "tokens": ["Eu\u00b7\u00dfer\u00b7li\u00b7che", "Pracht", "und", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind ein Schein verdeckter List,", "tokens": ["Sind", "ein", "Schein", "ver\u00b7deck\u00b7ter", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor niedrige Gem\u00fcther", "tokens": ["Die", "vor", "nied\u00b7ri\u00b7ge", "Ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein geschm\u00fccktes Fallbret ist.", "tokens": ["Ein", "ge\u00b7schm\u00fcck\u00b7tes", "Fall\u00b7bret", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer hier blind und sicher tritt,", "tokens": ["Wer", "hier", "blind", "und", "si\u00b7cher", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "De\u00dfen unbedachter Schritt", "tokens": ["De\u00b7\u00dfen", "un\u00b7be\u00b7dach\u00b7ter", "Schritt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmt die Reu in Abgrund mit.", "tokens": ["Nimmt", "die", "Reu", "in", "Ab\u00b7grund", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was uns bey noch gr\u00fcnen Jahren", "tokens": ["Was", "uns", "bey", "noch", "gr\u00fc\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Z\u00e4rtlich und galant entz\u00fcckt,", "tokens": ["Z\u00e4rt\u00b7lich", "und", "ga\u00b7lant", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ist ein Kram voll sch\u00f6ner Wahren,", "tokens": ["Ist", "ein", "Kram", "voll", "sch\u00f6\u00b7ner", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Zeiten Flucht entr\u00fcckt,", "tokens": ["Die", "der", "Zei\u00b7ten", "Flucht", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da denn oft der beste Ku\u00df", "tokens": ["Da", "denn", "oft", "der", "bes\u00b7te", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch des Alters \u00dcberdru\u00df", "tokens": ["Durch", "des", "Al\u00b7ters", "\u00dc\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wein zu E\u00dfig machen mu\u00df.", "tokens": ["Wein", "zu", "E\u00b7\u00dfig", "ma\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gleichwohl darf kein th\u00f6richt Gr\u00e4men", "tokens": ["Gleich\u00b7wohl", "darf", "kein", "th\u00f6\u00b7richt", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solchen Lauf verzagt beschreyn;", "tokens": ["Sol\u00b7chen", "Lauf", "ver\u00b7zagt", "be\u00b7schreyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was uns Zeit und Vorsicht nehmen,", "tokens": ["Was", "uns", "Zeit", "und", "Vor\u00b7sicht", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt die Hofnung st\u00fcndlich ein;", "tokens": ["Bringt", "die", "Hof\u00b7nung", "st\u00fcnd\u00b7lich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn ein Herze kluger Brust", "tokens": ["Denn", "ein", "Her\u00b7ze", "klu\u00b7ger", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht, so viel du leiden must,", "tokens": ["Macht", ",", "so", "viel", "du", "lei\u00b7den", "must", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sien bey allen eine Lust.", "tokens": ["Sien", "bey", "al\u00b7len", "ei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir zu Liebe f\u00e4ngt das Gl\u00fccke", "tokens": ["Mir", "zu", "Lie\u00b7be", "f\u00e4ngt", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl nicht erst was Neues an,", "tokens": ["Wohl", "nicht", "erst", "was", "Neu\u00b7es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PWS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da kein sterblich Flehn die T\u00fccke", "tokens": ["Da", "kein", "sterb\u00b7lich", "Flehn", "die", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Eifers beugen kan.", "tokens": ["Sei\u00b7nes", "Ei\u00b7fers", "beu\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob es nicht so st\u00fcrmisch thu,", "tokens": ["Ob", "es", "nicht", "so", "st\u00fcr\u00b7misch", "thu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Singt mein Geist bey stiller Ruh", "tokens": ["Singt", "mein", "Geist", "bey", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch ein h\u00f6hnisch Lied darzu.", "tokens": ["Doch", "ein", "h\u00f6h\u00b7nisch", "Lied", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Immerhin, ihr wilden Grillen!", "tokens": ["Im\u00b7mer\u00b7hin", ",", "ihr", "wil\u00b7den", "Gril\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts erwirbt euch mein Geh\u00f6r;", "tokens": ["Nichts", "er\u00b7wirbt", "euch", "mein", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr verderbt Verstand und Willen,", "tokens": ["Ihr", "ver\u00b7derbt", "Ver\u00b7stand", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber mir wohl nimmermehr.", "tokens": ["A\u00b7ber", "mir", "wohl", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter der Erg\u00f6zligkeit", "tokens": ["Un\u00b7ter", "der", "Er\u00b7g\u00f6z\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer Selbstzufriedenheit", "tokens": ["Ei\u00b7ner", "Selbst\u00b7zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00fchrt mich weder Gram noch Neid.", "tokens": ["R\u00fchrt", "mich", "we\u00b7der", "Gram", "noch", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}