{"textgrid.poem.41434": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hoffnung und die Furcht", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reisten (Wann? Vielleicht zu unsern Zeiten)", "tokens": ["Es", "reis\u00b7ten", "(", "Wann", "?", "Viel\u00b7leicht", "zu", "un\u00b7sern", "Zei\u00b7ten", ")"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "$.", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoffnung und die Furcht durchs Land.", "tokens": ["Die", "Hoff\u00b7nung", "und", "die", "Furcht", "durchs", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie jene leichtlich Freunde fand,", "tokens": ["Wie", "je\u00b7ne", "leicht\u00b7lich", "Freun\u00b7de", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wohnte diese gar bei denen, die sie scheuten.", "tokens": ["So", "wohn\u00b7te", "die\u00b7se", "gar", "bei", "de\u00b7nen", ",", "die", "sie", "scheu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "APPR", "PRELS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sogleich ver\u00e4ndert sich der Menschen Wahn und Stand.", "tokens": ["Sog\u00b7leich", "ver\u00b7\u00e4n\u00b7dert", "sich", "der", "Men\u00b7schen", "Wahn", "und", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bald f\u00e4ngt der Mangel an, sich voller Muth zu br\u00fcsten,", "tokens": ["Bald", "f\u00e4ngt", "der", "Man\u00b7gel", "an", ",", "sich", "vol\u00b7ler", "Muth", "zu", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRF", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Ueberflu\u00df, verzagt zu sein.", "tokens": ["Der", "Ue\u00b7berf\u00b7lu\u00df", ",", "ver\u00b7zagt", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Warum? Die Hoffnung kehrt beim \u00e4rmsten Alchymisten,", "tokens": ["Wa\u00b7rum", "?", "Die", "Hoff\u00b7nung", "kehrt", "beim", "\u00e4rms\u00b7ten", "Al\u00b7chy\u00b7mis\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Furcht beim reichsten Wuchrer ein.", "tokens": ["Die", "Furcht", "beim", "reichs\u00b7ten", "Wuch\u00b7rer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}