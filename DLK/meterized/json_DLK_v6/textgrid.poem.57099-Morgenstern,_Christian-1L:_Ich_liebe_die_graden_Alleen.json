{"textgrid.poem.57099": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebe die graden Alleen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe die graden Alleen", "tokens": ["Ich", "lie\u00b7be", "die", "gra\u00b7den", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "mit ihrer stolzen Flucht.", "tokens": ["mit", "ih\u00b7rer", "stol\u00b7zen", "Flucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich meine sie m\u00fcnden zu sehen", "tokens": ["Ich", "mei\u00b7ne", "sie", "m\u00fcn\u00b7den", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "in blauer Himmelsbucht.", "tokens": ["in", "blau\u00b7er", "Him\u00b7mels\u00b7bucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin sie im Flug zu Ende", "tokens": ["Ich", "bin", "sie", "im", "Flug", "zu", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und land' in der Ewigkeit.", "tokens": ["und", "land'", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie eine leise Legende", "tokens": ["Wie", "ei\u00b7ne", "lei\u00b7se", "Le\u00b7gen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "verklingt in mir die Zeit.", "tokens": ["ver\u00b7klingt", "in", "mir", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Fl\u00fcgel atmet Weiten,", "tokens": ["Mein", "Fl\u00fc\u00b7gel", "at\u00b7met", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die Menschenkraft nicht kennt:", "tokens": ["die", "Men\u00b7schen\u00b7kraft", "nicht", "kennt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gro\u00df aus Unendlichkeiten", "tokens": ["Gro\u00df", "aus", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "flammt furchtbar das Firmament.", "tokens": ["flammt", "furcht\u00b7bar", "das", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}