{"dta.poem.6943": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er, dessen Gottheit nichts als Wollust war, und Freude,", "tokens": ["Er", ",", "des\u00b7sen", "Got\u00b7theit", "nichts", "als", "Wol\u00b7lust", "war", ",", "und", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PIS", "KOKOM", "NN", "VAFIN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Errichtete sein Lehr-Geb\u00e4ude", "tokens": ["Er\u00b7rich\u00b7te\u00b7te", "sein", "Lehr\u00b7Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Vieh und Menschen gleich. Wenn alles C\u00f6rper w\u00e4re,", "tokens": ["F\u00fcr", "Vieh", "und", "Men\u00b7schen", "gleich", ".", "Wenn", "al\u00b7les", "C\u00f6r\u00b7per", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$.", "KOUS", "PIS", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie kan er doch, im Jrrthum seiner Lehre,", "tokens": ["Wie", "kan", "er", "doch", ",", "im", "Jrr\u00b7thum", "sei\u00b7ner", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des C\u00f6rpers und des Geistes Freuden", "tokens": ["Des", "C\u00f6r\u00b7pers", "und", "des", "Geis\u00b7tes", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wol voneinander unterscheiden?", "tokens": ["Wol", "von\u00b7ein\u00b7an\u00b7der", "un\u00b7ter\u00b7schei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Es scheint, da\u00df er sich selbst best\u00e4ndig widerspricht.", "tokens": ["Es", "scheint", ",", "da\u00df", "er", "sich", "selbst", "be\u00b7st\u00e4n\u00b7dig", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Welch\u2019 eine Thorheit ist es nicht,", "tokens": ["Welch'", "ei\u00b7ne", "Thor\u00b7heit", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da er die Geister selbst vernichtigt und vergr\u00e4dt,", "tokens": ["Da", "er", "die", "Geis\u00b7ter", "selbst", "ver\u00b7nich\u00b7tigt", "und", "ver\u00b7gr\u00e4dt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu der Zeit, da er sich bestrebt,", "tokens": ["Zu", "der", "Zeit", ",", "da", "er", "sich", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Der Weisheit Preis zu \u00fcberkommen,", "tokens": ["Der", "Weis\u00b7heit", "Preis", "zu", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie da\u00df ein c\u00f6rperlicher Geist", "tokens": ["Wie", "da\u00df", "ein", "c\u00f6r\u00b7per\u00b7li\u00b7cher", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Auch einen Trieb f\u00fcr Ruhm und Ehre weist,", "tokens": ["Auch", "ei\u00b7nen", "Trieb", "f\u00fcr", "Ruhm", "und", "Eh\u00b7re", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wovon der seinige so sehr doch eingenommen?", "tokens": ["Wo\u00b7von", "der", "sei\u00b7ni\u00b7ge", "so", "sehr", "doch", "ein\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PPOSS", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Betrachtet er mit der Ver\u00e4chtlichkeit", "tokens": ["Be\u00b7trach\u00b7tet", "er", "mit", "der", "Ver\u00b7\u00e4cht\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Seelen dauernde Beschaffenheit,", "tokens": ["Der", "See\u00b7len", "dau\u00b7ern\u00b7de", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "F\u00fcr wen verlangte er sich einen Ruhm zu stifften,", "tokens": ["F\u00fcr", "wen", "ver\u00b7lang\u00b7te", "er", "sich", "ei\u00b7nen", "Ruhm", "zu", "stiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den er sich sterbend doch versprach aus seinen Schrifften.", "tokens": ["Den", "er", "sich", "ster\u00b7bend", "doch", "ver\u00b7sprach", "aus", "sei\u00b7nen", "Schriff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}