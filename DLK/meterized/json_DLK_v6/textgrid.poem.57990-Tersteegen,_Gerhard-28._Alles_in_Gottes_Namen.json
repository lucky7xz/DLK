{"textgrid.poem.57990": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "28. Alles in Gottes Namen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du was nehmen vor, so leg vorher dein Herze", "tokens": ["Willst", "du", "was", "neh\u00b7men", "vor", ",", "so", "leg", "vor\u00b7her", "dein", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "VVINF", "PTKVZ", "$,", "ADV", "NE", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichg\u00fcltig, still und blo\u00df in Gottes helles Licht;", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", ",", "still", "und", "blo\u00df", "in", "Got\u00b7tes", "hel\u00b7les", "Licht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Halt ein den eignen Trieb, der bringt nur Schad' und Schmerze!", "tokens": ["Halt", "ein", "den", "eig\u00b7nen", "Trieb", ",", "der", "bringt", "nur", "Schad'", "und", "Schmer\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tust du dein Werk in Gott, so darfst du f\u00fcrchten nicht.", "tokens": ["Tust", "du", "dein", "Werk", "in", "Gott", ",", "so", "darfst", "du", "f\u00fcrch\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}