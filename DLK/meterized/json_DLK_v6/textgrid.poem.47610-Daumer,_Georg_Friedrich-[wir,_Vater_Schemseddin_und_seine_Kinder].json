{"textgrid.poem.47610": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wir, Vater Schemseddin und seine Kinder]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir, Vater Schemseddin und seine Kinder,", "tokens": ["Wir", ",", "Va\u00b7ter", "Schem\u00b7sed\u00b7din", "und", "sei\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NE", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir, Scheich Hafis und seine frommen M\u00f6nche,", "tokens": ["Wir", ",", "Scheich", "Ha\u00b7fis", "und", "sei\u00b7ne", "from\u00b7men", "M\u00f6n\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "NE", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sind ein eignes, wunderliches Volk.", "tokens": ["Wir", "sind", "ein", "eig\u00b7nes", ",", "wun\u00b7der\u00b7li\u00b7ches", "Volk", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Gram gebeugt und ewiger Klage voll,", "tokens": ["Von", "Gram", "ge\u00b7beugt", "und", "e\u00b7wi\u00b7ger", "Kla\u00b7ge", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ohn' Unterla\u00df in unserem Trauerjoch", "tokens": ["Ohn'", "Un\u00b7ter\u00b7la\u00df", "in", "un\u00b7se\u00b7rem", "Trau\u00b7er\u00b7joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Des feuchten Auges hei\u00dfe Perle streuend,", "tokens": ["Des", "feuch\u00b7ten", "Au\u00b7ges", "hei\u00b7\u00dfe", "Per\u00b7le", "streu\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und ewig hell und ewig heiter doch;", "tokens": ["Und", "e\u00b7wig", "hell", "und", "e\u00b7wig", "hei\u00b7ter", "doch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Kerze gleich hinschmelzend und vergehend,", "tokens": ["Der", "Ker\u00b7ze", "gleich", "hin\u00b7schmel\u00b7zend", "und", "ver\u00b7ge\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und doch, wie sie, in lichter Wonne lachend;", "tokens": ["Und", "doch", ",", "wie", "sie", ",", "in", "lich\u00b7ter", "Won\u00b7ne", "la\u00b7chend", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gemordet allezeit von Wimperdolchen,", "tokens": ["Ge\u00b7mor\u00b7det", "al\u00b7le\u00b7zeit", "von", "Wim\u00b7per\u00b7dol\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von grausamen, die nur nach Blute d\u00fcrsten,", "tokens": ["Von", "grau\u00b7sa\u00b7men", ",", "die", "nur", "nach", "Blu\u00b7te", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PRELS", "ADV", "APPR", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und just hierinnen unseres Seins gewi\u00df;", "tokens": ["Und", "just", "hie\u00b7rin\u00b7nen", "un\u00b7se\u00b7res", "Seins", "ge\u00b7wi\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Versunken in ein Meer von Schuld und S\u00fcnde,", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "ein", "Meer", "von", "Schuld", "und", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ganz unbekannt mit dem Gef\u00fchl der Reue,", "tokens": ["Ganz", "un\u00b7be\u00b7kannt", "mit", "dem", "Ge\u00b7f\u00fchl", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und fromm zugleich und frei von allem Argen,", "tokens": ["Und", "fromm", "zu\u00b7gleich", "und", "frei", "von", "al\u00b7lem", "Ar\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Des Lichtes S\u00f6hne, nicht der Finsterni\u00df,", "tokens": ["Des", "Lich\u00b7tes", "S\u00f6h\u00b7ne", ",", "nicht", "der", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und so der Menge v\u00f6llig unbegreiflich.", "tokens": ["Und", "so", "der", "Men\u00b7ge", "v\u00f6l\u00b7lig", "un\u00b7be\u00b7greif\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Denn diese kennt nur dreierlei Naturen,", "tokens": ["Denn", "die\u00b7se", "kennt", "nur", "drei\u00b7er\u00b7lei", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Den Fr\u00f6mler erstlich, den Fanatiker,", "tokens": ["Den", "Fr\u00f6m\u00b7ler", "erst\u00b7lich", ",", "den", "Fa\u00b7na\u00b7ti\u00b7ker", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Den finsteren, bl\u00f6dsinnigen Barbaren,", "tokens": ["Den", "fins\u00b7te\u00b7ren", ",", "bl\u00f6d\u00b7sin\u00b7ni\u00b7gen", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Den W\u00fcstling ohne Geist und Herz sodann,", "tokens": ["Den", "W\u00fcst\u00b7ling", "oh\u00b7ne", "Geist", "und", "Herz", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Den selbstischen, unedelen, gemeinen,", "tokens": ["Den", "selbs\u00b7ti\u00b7schen", ",", "un\u00b7e\u00b7de\u00b7len", ",", "ge\u00b7mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Den endlich in gewohnter Schranke dumpf", "tokens": ["Den", "end\u00b7lich", "in", "ge\u00b7wohn\u00b7ter", "Schran\u00b7ke", "dumpf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Beharrenden; f\u00fcr Leute so, wie wir,", "tokens": ["Be\u00b7har\u00b7ren\u00b7den", ";", "f\u00fcr", "Leu\u00b7te", "so", ",", "wie", "wir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "NN", "ADV", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Gebricht es ihr an Namen und Begriff.", "tokens": ["Ge\u00b7bricht", "es", "ihr", "an", "Na\u00b7men", "und", "Be\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}