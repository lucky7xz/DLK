{"textgrid.poem.42346": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: In meinem Zimmer ein paar frische Blumen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In meinem Zimmer ein paar frische Blumen,", "tokens": ["In", "mei\u00b7nem", "Zim\u00b7mer", "ein", "paar", "fri\u00b7sche", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die allen Wintermissmut mir vertreiben.", "tokens": ["Die", "al\u00b7len", "Win\u00b7ter\u00b7miss\u00b7mut", "mir", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein V\u00f6glein pickt vor meinem Fenster Krumen", "tokens": ["Ein", "V\u00f6\u00b7glein", "pickt", "vor", "mei\u00b7nem", "Fens\u00b7ter", "Kru\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und guckt dabei zutraulich durch die Scheiben.", "tokens": ["Und", "guckt", "da\u00b7bei", "zu\u00b7trau\u00b7lich", "durch", "die", "Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In Stroh und Bast die B\u00e4ume eingeschlagen,", "tokens": ["In", "Stroh", "und", "Bast", "die", "B\u00e4u\u00b7me", "ein\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit der strenge Frost sie nicht ber\u00fchre,", "tokens": ["Da\u00b7mit", "der", "stren\u00b7ge", "Frost", "sie", "nicht", "be\u00b7r\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Beete wohl verwahrt vor kalten Tagen \u2013", "tokens": ["Die", "Bee\u00b7te", "wohl", "ver\u00b7wahrt", "vor", "kal\u00b7ten", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, blossen Haupts, ein Bettler vor der T\u00fcre.", "tokens": ["Und", ",", "blos\u00b7sen", "Haupts", ",", "ein", "Bett\u00b7ler", "vor", "der", "T\u00fc\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}