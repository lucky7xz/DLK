{"textgrid.poem.43908": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweig du doch nur, du H\u00e4lfte meiner Brust;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweig du doch nur, du H\u00e4lfte meiner Brust;", "tokens": ["Schweig", "du", "doch", "nur", ",", "du", "H\u00e4lf\u00b7te", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn was du weinst, ist Blut aus meinem Herzen.", "tokens": ["Denn", "was", "du", "weinst", ",", "ist", "Blut", "aus", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich taumle so und hab an nichts mehr Lust", "tokens": ["Ich", "taum\u00b7le", "so", "und", "hab", "an", "nichts", "mehr", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VAFIN", "APPR", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als an der Angst und den getreuen Schmerzen,", "tokens": ["Als", "an", "der", "Angst", "und", "den", "ge\u00b7treu\u00b7en", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Womit der Stern, der unsre Liebe trennt,", "tokens": ["Wo\u00b7mit", "der", "Stern", ",", "der", "uns\u00b7re", "Lie\u00b7be", "trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Augen brennt.", "tokens": ["Die", "Au\u00b7gen", "brennt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Z\u00e4rtligkeit der innerlichen Qual", "tokens": ["Die", "Z\u00e4rt\u00b7lig\u00b7keit", "der", "in\u00b7ner\u00b7li\u00b7chen", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erlaubt mir kaum, ein ganzes Wort zu machen.", "tokens": ["Er\u00b7laubt", "mir", "kaum", ",", "ein", "gan\u00b7zes", "Wort", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was dem geschieht, um welchen Keil und Strahl", "tokens": ["Was", "dem", "ge\u00b7schieht", ",", "um", "wel\u00b7chen", "Keil", "und", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "VVFIN", "$,", "KOUI", "PWAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bey hei\u00dfer Luft in weitem Felde krachen,", "tokens": ["Bey", "hei\u00b7\u00dfer", "Luft", "in", "wei\u00b7tem", "Fel\u00b7de", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geschieht auch mir durch dieses Donnerwort:", "tokens": ["Ge\u00b7schieht", "auch", "mir", "durch", "die\u00b7ses", "Don\u00b7ner\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun mu\u00df ich fort.", "tokens": ["Nun", "mu\u00df", "ich", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ach harter Schlu\u00df, der unsre Musen zwingt,", "tokens": ["Ach", "har\u00b7ter", "Schlu\u00df", ",", "der", "uns\u00b7re", "Mu\u00b7sen", "zwingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Flei\u00dfes Ruhm in fremder Luft zu gr\u00fcnden", "tokens": ["Des", "Flei\u00b7\u00dfes", "Ruhm", "in", "frem\u00b7der", "Luft", "zu", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der auch mich mit Furcht und Angst umringt!", "tokens": ["Und", "der", "auch", "mich", "mit", "Furcht", "und", "Angst", "um\u00b7ringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Welch Pflaster kan den tiefen Ri\u00df verbinden,", "tokens": ["Welch", "Pflas\u00b7ter", "kan", "den", "tie\u00b7fen", "Ri\u00df", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den tiefen Ri\u00df, der mich und dich zulezt", "tokens": ["Den", "tie\u00b7fen", "Ri\u00df", ",", "der", "mich", "und", "dich", "zu\u00b7lezt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Kummer sezt?", "tokens": ["In", "Kum\u00b7mer", "sezt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Abschiedsku\u00df verschliest mein Paradies,", "tokens": ["Der", "Ab\u00b7schieds\u00b7ku\u00df", "ver\u00b7schliest", "mein", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus welchem mich Zeit und Verh\u00e4ngn\u00fc\u00df treiben;", "tokens": ["Aus", "wel\u00b7chem", "mich", "Zeit", "und", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So viel bisher dein Antliz Sonnen wies,", "tokens": ["So", "viel", "bis\u00b7her", "dein", "Ant\u00b7liz", "Son\u00b7nen", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mancher Bliz wird jezt mein Schr\u00f6cken bleiben.", "tokens": ["So", "man\u00b7cher", "Bliz", "wird", "jezt", "mein", "Schr\u00f6\u00b7cken", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Zweifel wacht und spricht von deiner Treu:", "tokens": ["Der", "Zwei\u00b7fel", "wacht", "und", "spricht", "von", "dei\u00b7ner", "Treu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie ist vorbey.", "tokens": ["Sie", "ist", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Verzeih mir doch den Argwohn gegen dich;", "tokens": ["Ver\u00b7zeih", "mir", "doch", "den", "Arg\u00b7wohn", "ge\u00b7gen", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer br\u00fcnstig liebt, dem macht die Furcht stets bange.", "tokens": ["Wer", "br\u00fcns\u00b7tig", "liebt", ",", "dem", "macht", "die", "Furcht", "stets", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschen Herz ver\u00e4ndert wunderlich;", "tokens": ["Der", "Men\u00b7schen", "Herz", "ver\u00b7\u00e4n\u00b7dert", "wun\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer weis, wie bald mein Geist die Post empfange,", "tokens": ["Wer", "weis", ",", "wie", "bald", "mein", "Geist", "die", "Post", "emp\u00b7fan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df die, so mich in Gegenwart gek\u00fcst,", "tokens": ["Da\u00df", "die", ",", "so", "mich", "in", "Ge\u00b7gen\u00b7wart", "ge\u00b7k\u00fcst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Entfernt vergi\u00dft.", "tokens": ["Ent\u00b7fernt", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Gedenck einmahl, wie sch\u00f6n wir vor gelebt", "tokens": ["Ge\u00b7denck", "ein\u00b7mahl", ",", "wie", "sch\u00f6n", "wir", "vor", "ge\u00b7lebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PWAV", "ADJD", "PPER", "APPR", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie geheim wir unsre Lust geno\u00dfen.", "tokens": ["Und", "wie", "ge\u00b7heim", "wir", "uns\u00b7re", "Lust", "ge\u00b7no\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da hat kein Neid der Reizung widerstrebt,", "tokens": ["Da", "hat", "kein", "Neid", "der", "Rei\u00b7zung", "wi\u00b7der\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Womit du mich an Hals und Brust geschlo\u00dfen,", "tokens": ["Wo\u00b7mit", "du", "mich", "an", "Hals", "und", "Brust", "ge\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da sah uns auch bey selbst erw\u00fcntschter Ruh", "tokens": ["Da", "sah", "uns", "auch", "bey", "selbst", "er\u00b7w\u00fcnt\u00b7schter", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein W\u00e4chter zu.", "tokens": ["Kein", "W\u00e4ch\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Genung! Ich mu\u00df; die Marterglocke schl\u00e4gt.", "tokens": ["Ge\u00b7nung", "!", "Ich", "mu\u00df", ";", "die", "Mar\u00b7ter\u00b7glo\u00b7cke", "schl\u00e4gt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier liegt mein Herz, da nimm es aus dem Munde", "tokens": ["Hier", "liegt", "mein", "Herz", ",", "da", "nimm", "es", "aus", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und heb es auf, die Fr\u00fcchte, so es tr\u00e4gt,", "tokens": ["Und", "heb", "es", "auf", ",", "die", "Fr\u00fcch\u00b7te", ",", "so", "es", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sind Ruh und Trost bey mancher b\u00f6sen Stunde,", "tokens": ["Sind", "Ruh", "und", "Trost", "bey", "man\u00b7cher", "b\u00f6\u00b7sen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und lis, so oft dein Gram die Leute flieht,", "tokens": ["Und", "lis", ",", "so", "oft", "dein", "Gram", "die", "Leu\u00b7te", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Abschiedslied.", "tokens": ["Mein", "Ab\u00b7schieds\u00b7lied", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Wohin ich geh, begleitet mich dein Bild,", "tokens": ["Wo\u00b7hin", "ich", "geh", ",", "be\u00b7glei\u00b7tet", "mich", "dein", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein fremder Zug wird mir den Schaz entrei\u00dfen;", "tokens": ["Kein", "frem\u00b7der", "Zug", "wird", "mir", "den", "Schaz", "ent\u00b7rei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es macht mich treu und ist ein Hofnungsschild,", "tokens": ["Es", "macht", "mich", "treu", "und", "ist", "ein", "Hof\u00b7nungs\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Neid und Noth Verfolgungssteine schmei\u00dfen,", "tokens": ["Wenn", "Neid", "und", "Noth", "Ver\u00b7fol\u00b7gungs\u00b7stei\u00b7ne", "schmei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis da\u00df die Hand, die uns hier D\u00f6rner flicht,", "tokens": ["Bis", "da\u00df", "die", "Hand", ",", "die", "uns", "hier", "D\u00f6r\u00b7ner", "flicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Myrthen bricht.", "tokens": ["Die", "Myr\u00b7then", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Erinnre dich zum \u00f6ftern meiner Huld", "tokens": ["E\u00b7rinn\u00b7re", "dich", "zum", "\u00f6f\u00b7tern", "mei\u00b7ner", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und n\u00e4hre sie mit s\u00fc\u00dfem Angedencken!", "tokens": ["Und", "n\u00e4h\u00b7re", "sie", "mit", "s\u00fc\u00b7\u00dfem", "An\u00b7ge\u00b7den\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wirst betr\u00fcbt, dies ist des Abschieds Schuld,", "tokens": ["Du", "wirst", "be\u00b7tr\u00fcbt", ",", "dies", "ist", "des", "Ab\u00b7schieds", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mu\u00df ich dich zum ersten Mahle kr\u00e4ncken,", "tokens": ["So", "mu\u00df", "ich", "dich", "zum", "ers\u00b7ten", "Mah\u00b7le", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und fordert mich der erste Gang von hier,", "tokens": ["Und", "for\u00b7dert", "mich", "der", "ers\u00b7te", "Gang", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So sterb ich dir.", "tokens": ["So", "sterb", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ich sterbe dir, und soll ein fremder Sand", "tokens": ["Ich", "ster\u00b7be", "dir", ",", "und", "soll", "ein", "frem\u00b7der", "Sand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den oft durch dich erg\u00f6zten Leib bedecken,", "tokens": ["Den", "oft", "durch", "dich", "er\u00b7g\u00f6z\u00b7ten", "Leib", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So g\u00f6nne mir das lezte Liebespfand", "tokens": ["So", "g\u00f6n\u00b7ne", "mir", "das", "lez\u00b7te", "Lie\u00b7be\u00b7spfand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und las ein Creuz mit dieser Grabschrift stecken:", "tokens": ["Und", "las", "ein", "Creuz", "mit", "die\u00b7ser", "Grab\u00b7schrift", "ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo ist ein Mensch, der treulich lieben kan?", "tokens": ["Wo", "ist", "ein", "Mensch", ",", "der", "treu\u00b7lich", "lie\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hier liegt der Mann.", "tokens": ["Hier", "liegt", "der", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iamb